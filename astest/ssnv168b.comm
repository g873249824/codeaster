#                PARABOLIQUE POUR UN CONFINEMENT DE 2 MPA
# ======================================================================
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE = LIRE_MAILLAGE( )

MODELE   = AFFE_MODELE( MAILLAGE =     MAILLAGE,
                        AFFE     = _F( TOUT         = 'OUI',
                                       PHENOMENE    = 'MECANIQUE',
                                       MODELISATION = 'AXIS')
                      )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS1   = DEFI_LIST_REEL( DEBUT      = 0.,
                           INTERVALLE = _F( JUSQU_A = 1.0, NOMBRE = 1 )
                          )

TEMPS2   = DEFI_LIST_REEL( DEBUT      = 1.,
                           INTERVALLE = _F( JUSQU_A = 2.0, NOMBRE = 100 )
                          )

#***********************************************************************
#    MATERIAU : SOL
#***********************************************************************

MATER    = DEFI_MATERIAU(  ELAS    = _F(  E  = 5800.0E6,
                                          NU = 0.3  ,),
                           DRUCK_PRAGER = _F(  ECROUISSAGE = 'PARABOLIQUE',
                                          ALPHA       =    0.33      ,
                                          P_ULTM      =    0.01      ,
                                          SY          =    2.57E6    ,
                                          SY_ULTM     =    0.57E6    ,),
                        );

CHMAT    = AFFE_MATERIAU(  MAILLAGE = MAILLAGE,
                           AFFE     = _F(  TOUT  = 'OUI',
                                           MATER = MATER )
                        );

#***********************************************************************
#    CHARGEMENT MECANIQUE : FORCE DE COMPRESSION + DDL IMPOSES
#***********************************************************************

CHAR_U1  = AFFE_CHAR_MECA( MODELE   = MODELE,
                           PRES_REP = _F( MAILLE = 'M4',
                                          PRES   = 1.    )
                         )

CHAR_U2  = AFFE_CHAR_MECA( MODELE   = MODELE,
                           PRES_REP = _F( MAILLE = 'M3',
                                          PRES   = 1.    )
                        )

DEPL_1   = AFFE_CHAR_MECA( MODELE    = MODELE,
                           FACE_IMPO = _F( MAILLE = 'M2',
                                           DY     = 0.   )
                         )

COEF1    = DEFI_FONCTION( NOM_PARA    = 'INST',
                          PROL_DROITE = 'CONSTANT',
                          VALE=( 0.0,  0.0,
                                 1.0,  2.0E6,)
                        )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************

U1=STAT_NON_LINE(
                  MODELE     = MODELE,
                  CHAM_MATER = CHMAT,
    EXCIT      = ( _F( CHARGE       = CHAR_U1,
                                     FONC_MULT    = COEF1),
     _F( CHARGE       = CHAR_U2,
                                     FONC_MULT    = COEF1),
     _F( CHARGE       = DEPL_1)),
                  COMPORTEMENT  = _F( RELATION       = 'DRUCK_PRAGER'),
                  NEWTON     = _F( MATRICE        = 'TANGENTE',
                                   REAC_ITER      = 1,),
                  CONVERGENCE= _F( RESI_GLOB_RELA = 1.E-4,
                                   ITER_GLOB_MAXI = 50,),
                  INCREMENT  = _F( LIST_INST      = TEMPS1)
                )

U1=CALC_CHAMP(reuse=U1,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=U1)


#***********************************************************************
#    NOUVEAU CHARGEMENT MECANIQUE : DEPLACEMENT UNIAXIALE UNITAIRE
#***********************************************************************


SIG_INI = CREA_CHAMP( OPERATION = 'EXTR',
                      TYPE_CHAM = 'ELGA_SIEF_R',
                      NOM_CHAM  = 'SIEF_ELGA',
        RESULTAT  = U1,
                      INST      = 1.,
                    )

VAR_INI = CREA_CHAMP( OPERATION = 'EXTR',
                      TYPE_CHAM = 'ELGA_VARI_R',
                      NOM_CHAM  = 'VARI_ELGA',
        RESULTAT  = U1,
                      INST      = 1.
                    )

DEPL_2  = AFFE_CHAR_MECA( MODELE    = MODELE,
                          FACE_IMPO = _F( MAILLE = 'M4',
                                          DY     = 1.   )
                        )

COEF2   = DEFI_FONCTION( NOM_PARA    = 'INST',
                         PROL_DROITE = 'LINEAIRE',
                         PROL_GAUCHE = 'CONSTANT',
                         VALE=( 1.0,  0.0,
                                2.0, -0.015, )
                       )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************

U2 = STAT_NON_LINE(
                    MODELE     = MODELE,
                    CHAM_MATER = CHMAT,
      EXCIT      = ( _F( CHARGE       = CHAR_U2,
                                       FONC_MULT    = COEF1),
              _F( CHARGE       = DEPL_1),
              _F( CHARGE       = DEPL_2,
                                       FONC_MULT    = COEF2)),
                    COMPORTEMENT  = _F( RELATION       = 'DRUCK_PRAGER',
                                     ITER_INTE_PAS  = -10,),
                    ETAT_INIT  = _F( SIGM           = SIG_INI,
                                     VARI           = VAR_INI),
                    INCREMENT  = _F( LIST_INST      = TEMPS2),
                    NEWTON     = _F( MATRICE        = 'TANGENTE',
                                     REAC_ITER      = 1,),
                    CONVERGENCE= _F( RESI_GLOB_RELA = 1.E-6,
                                     ITER_GLOB_MAXI = 100)
                  )

U2=CALC_CHAMP(reuse=U2,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=U2)


U2       = CALC_CHAMP( reuse    = U2,
                    RESULTAT = U2,
                    CONTRAINTE='SIGM_NOEU',VARI_INTERNE='VARI_NOEU'
                  )

TEST_RESU(RESU=(_F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='N4',
                   NOM_CMP='SIXX',
                   VALE_CALC=-2.00000000E+06,
                   VALE_REFE=-2.E6,
                   PRECISION=1.E-3,),
                _F(INST=1.0700000000000001,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='N4',
                   NOM_CMP='SIYY',
                   VALE_CALC=-8.09000000E+06,
                   VALE_REFE=-8.090000E6,
                   PRECISION=1.E-3,),
                _F(INST=1.1599999999999999,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
