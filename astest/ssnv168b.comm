                   NOEUD='N4',
                   NOM_CMP='SIYY',
                   VALE_CALC=-8.00930823E+06,
                   VALE_REFE=-8.0093082300000004E6,
                   PRECISION=1.E-3,),
                _F(INST=1.3400000000000001,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='N4',
                   NOM_CMP='SIYY',
                   VALE_CALC=-6.62756834E+06,
                   VALE_REFE=-6.6275683300000001E6,
                   PRECISION=1.E-3,),
                _F(INST=1.53,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='N4',
                   NOM_CMP='SIYY',
                   VALE_CALC=-5.80597015E+06,
                   VALE_REFE=-5.8059701490000002E6,
                   PRECISION=1.E-3,),
                _F(INST=1.0700000000000001,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='VARI_NOEU',
                   NOEUD='N4',
                   NOM_CMP='V1',
                   VALE_CALC= 0.00000000E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-3,),
                _F(INST=1.1599999999999999,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='VARI_NOEU',
                   NOEUD='N4',
                   NOM_CMP='V1',
                   VALE_CALC= 2.03569011E-03,
                   VALE_REFE=2.0356901106300001E-3,
                   PRECISION=1.E-3,),
                _F(INST=1.3400000000000001,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='VARI_NOEU',
                   NOEUD='N4',
                   NOM_CMP='V1',
                   VALE_CALC= 6.42110954E-03,
                   VALE_REFE=6.4211095370000004E-3,
                   PRECISION=1.E-3,),
                _F(INST=1.53,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='VARI_NOEU',
                   NOEUD='N4',
                   NOM_CMP='V1',
                   VALE_CALC=0.010886266,
                   VALE_REFE=0.010886266045000001,
                   PRECISION=1.E-3,),
                _F(INST=1.0700000000000001,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='DEPL',
                   NOEUD='N4',
                   NOM_CMP='DY',
                   VALE_CALC=-1.05000000E-03,
                   VALE_REFE=-1.0499999999999999E-3,
                   PRECISION=1.E-3,),
                _F(INST=1.1599999999999999,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='DEPL',
                   NOEUD='N4',
                   NOM_CMP='DY',
                   VALE_CALC=-2.40000000E-03,
                   VALE_REFE=-2.3999999999999998E-3,
                   PRECISION=1.E-3,),
                _F(INST=1.3400000000000001,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='DEPL',
                   NOEUD='N4',
                   NOM_CMP='DY',
                   VALE_CALC=-5.10000000E-03,
                   VALE_REFE=-5.1000000000000004E-3,
                   PRECISION=1.E-3,),
                _F(INST=1.53,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
