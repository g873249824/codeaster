                            9.000E+01,1.210E-05,
                            1.000E+02,1.210E-05,
                            1.100E+02,1.220E-05,
                            1.200E+02,1.220E-05,
                            1.300E+02,1.230E-05,
                            1.400E+02,1.230E-05,
                            1.500E+02,1.240E-05,
                            1.600E+02,1.250E-05,
                            1.700E+02,1.250E-05,
                            1.800E+02,1.260E-05,
                            1.900E+02,1.260E-05,
                            2.000E+02,1.270E-05,
                            2.100E+02,1.270E-05,
                            2.200E+02,1.280E-05,
                            2.300E+02,1.280E-05,
                            2.400E+02,1.290E-05,
                            2.500E+02,1.290E-05,
                            ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',);

NU_T=DEFI_CONSTANTE(NOM_RESU='TEMP',
                    VALE=0.3,);

#----------------------------------------------------------------------
# DEFINITION DE LA COURBE DE TRACTION 
#----------------------------------------------------------------------
#-- DEFINITION DE LA NAPPE (EPSI, SI, TEMP)
# ECROUISSAGE ISOTROPE NON LINEAIRE 

