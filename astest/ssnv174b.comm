                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE',
                  VALE=(0.0,0.2,1.0,0.2,)
                ),

ALPHA=DEFI_CONSTANTE(VALE=0.);

BETON=DEFI_MATERIAU( ELAS_FO =_F( E = E,
                                  NU = NU ,
                                  ALPHA=ALPHA,TEMP_DEF_ALPHA=0.,
                                  K_DESSIC = 10.0E-6,
                                  B_ENDOGE = 60.0E-6,
                                  FONC_DESORP = FONC1, ),
                     BETON_BURGER_FP =_F( K_RS = 1.2E+05,
                                        ETA_RS = 2.21E+10,
                                        KAPPA  = 3E-3,
                                        ETA_IS = 4.16E+10,
                                        K_RD   = 3.86E+04,
                                        ETA_RD = 6.19E+10,
                                        ETA_ID = 1.64e+12,
                                      ),
                  );

