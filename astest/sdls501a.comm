#---------------------------------------------------------------------
#  - CALCUL EN ANALYSE MODALE DYNAMIQUE D'UNE TOLE EN LIBRE-LIBRE
#
#  - MODELISATION AVEC DES ELEMENTS 'D_PLAN'
#---------------------------------------------------------------------
#
#
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))
#
MAILLAGE=LIRE_MAILLAGE(FORMAT='MED',);
#
MODELE=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='D_PLAN',),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
#
#


ACIER=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.3,
                            RHO=7800.0,),);
#
#
#

AFF_MAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                      AFFE=_F(TOUT='OUI',
                              MATER=ACIER,),);
#
#---------------------------------------------------------------------
#                     RESOLUTION
#---------------------------------------------------------------------
#
#
#


MAT_EL_R=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=AFF_MAT,);
#
#
#


MAT_EL_M=CALC_MATR_ELEM(OPTION='MASS_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=AFF_MAT,);
#
#
#


NUM=NUME_DDL(MATR_RIGI=MAT_EL_R,
             METHODE='MULT_FRONT',);
#
#
#


MAT_AS_R=ASSE_MATRICE(MATR_ELEM=MAT_EL_R,
                      NUME_DDL=NUM,);
#
#
#


MAT_AS_M=ASSE_MATRICE(MATR_ELEM=MAT_EL_M,
                      NUME_DDL=NUM,);
#
#
#


MODE=MODE_ITER_SIMULT(MATR_RIGI=MAT_AS_R,
                      MATR_MASS=MAT_AS_M,
                      METHODE='TRI_DIAG',
                      PREC_ORTHO=1e-12,
                      NMAX_ITER_ORTHO=50,
                      PREC_LANCZOS=1e-15,
                      NMAX_ITER_QR=30,
                      OPTION='MODE_RIGIDE',
                      CALC_FREQ=_F(OPTION='BANDE',
                                   FREQ=(500.0,5000.0,),),
                      INFO=2,);


MODE=CALC_CHAMP(reuse=MODE,RESULTAT=MODE,CONTRAINTE=('SIEF_ELGA'),ENERGIE=('ETOT_ELEM','ETOT_ELGA','ETOT_ELNO'))


DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=MODE,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

ELAS=CREA_RESU(OPERATION='AFFE',
               TYPE_RESU='EVOL_ELAS',
               NOM_CHAM='DEPL',
               AFFE=_F(
               CHAM_GD=DEPL,
               MODELE=MODELE,
               CHAM_MATER=AFF_MAT,
               INST=0.0,),);

ELAS=CALC_CHAMP(reuse=ELAS,RESULTAT=ELAS,CONTRAINTE=('SIEF_ELGA'),ENERGIE=('ETOT_ELGA','ETOT_ELNO','ETOT_ELEM'))


#
#---------------------------------------------------------------------
#                     VERIFICATIONS DES RESULTATS
#---------------------------------------------------------------------
#

TEST_RESU(RESU=(_F(PARA='FREQ',
                   NUME_MODE=4,
                   RESULTAT=MODE,
                   VALE_CALC=658.243248060,
                   VALE_REFE=658.2432,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(PARA='FREQ',
                   NUME_MODE=5,
                   RESULTAT=MODE,
                   VALE_CALC= 1.74934881E+03,
                   VALE_REFE=1749.3488,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(PARA='FREQ',
                   NUME_MODE=6,
                   RESULTAT=MODE,
                   VALE_CALC= 3.22541618E+03,
                   VALE_REFE=3225.4160999999999,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   POINT=1,
                   RESULTAT=ELAS,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   VALE_CALC=2.227508550862E5,
                   MAILLE='MA5',
                   ),
                _F(NUME_ORDRE=1,
                   POINT=1,
                   RESULTAT=ELAS,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   VALE_CALC=9.5995307118465996E8,
                   MAILLE='MA10',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=ELAS,
                   NOM_CHAM='ETOT_ELNO',
                   NOEUD='NO100',
                   NOM_CMP='TOTALE',
                   VALE_CALC=5.0435078630760002E10,
                   MAILLE='MA25',
                   ),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=MODE,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   VALE_CALC= 2.22750855E+05,
                   VALE_REFE=2.227508550862E5,
                   PRECISION=1.E-08,
                   MAILLE='MA5',),
                _F(NUME_ORDRE=1,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=MODE,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   VALE_CALC= 9.59953071E+08,
                   VALE_REFE=9.5995307118465996E8,
                   PRECISION=1.E-08,
                   MAILLE='MA10',),
                _F(NUME_ORDRE=1,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=MODE,
                   NOM_CHAM='ETOT_ELNO',
                   NOEUD='NO100',
                   NOM_CMP='TOTALE',
                   VALE_CALC= 5.04350786E+10,
                   VALE_REFE=5.0435078630760002E10,
                   PRECISION=1.E-08,
                   MAILLE='MA25',),
                ),
          )

FIN();
