#---------------------------------------------------------------------
#  - CALCUL EN ANALYSE MODALE DYNAMIQUE D'UNE TOLE EN LIBRE-LIBRE
#
#  - MODELISATION AVEC DES ELEMENTS 'D_PLAN'
#---------------------------------------------------------------------
#
#
#


DEBUT(CODE=_F(NOM='SDLS501A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))
#
#PRE_IDEAS ();
#
#
#


MAILLAGE=LIRE_MAILLAGE();
#
#
#


MODELE=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='D_PLAN',),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
#
#


ACIER=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.3,
                            RHO=7800.0,),);
#
#
#


AFF_MAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                      AFFE=_F(TOUT='OUI',
                              MATER=ACIER,),);
#
#---------------------------------------------------------------------
#                     RESOLUTION
#---------------------------------------------------------------------
#
#
#


MAT_EL_R=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=AFF_MAT,);
#
#
#


MAT_EL_M=CALC_MATR_ELEM(OPTION='MASS_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=AFF_MAT,);
#
#
#


NUM=NUME_DDL(MATR_RIGI=MAT_EL_R,
             METHODE='MULT_FRONT',);
#
#
#


MAT_AS_R=ASSE_MATRICE(MATR_ELEM=MAT_EL_R,
                      NUME_DDL=NUM,);
#
#
#


MAT_AS_M=ASSE_MATRICE(MATR_ELEM=MAT_EL_M,
                      NUME_DDL=NUM,);
#
#
#


MODE=MODE_ITER_SIMULT(MATR_A=MAT_AS_R,
                      MATR_B=MAT_AS_M,
                      METHODE='TRI_DIAG',
                      PREC_ORTHO=1e-12,
                      NMAX_ITER_ORTHO=50,
                      PREC_LANCZOS=1e-15,
                      NMAX_ITER_QR=30,
                      OPTION='MODE_RIGIDE',
                      CALC_FREQ=_F(OPTION='BANDE',
                                   FREQ=(500.0,5000.0,),),
                      INFO=2,);

MODE=CALC_ELEM(reuse=MODE,RESULTAT=MODE,MODELE=MODELE,
                        CHAM_MATER=AFF_MAT,
                OPTION=('SIEF_ELGA_DEPL','ETOT_ELEM','ETOT_ELGA','ETOT_ELNO_ELGA'),
                );

DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=MODE,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

ELAS=CREA_RESU(OPERATION='AFFE',
               TYPE_RESU='EVOL_ELAS',
               NOM_CHAM='DEPL',
               AFFE=_F(
               CHAM_GD=DEPL,
               MODELE=MODELE,
               CHAM_MATER=AFF_MAT,
               INST=0.0,),);

ELAS=CALC_ELEM(reuse =ELAS,
               RESULTAT=ELAS,
               OPTION=('SIEF_ELGA_DEPL','ETOT_ELGA','ETOT_ELNO_ELGA','ETOT_ELEM',),);

#
#---------------------------------------------------------------------
#                     VERIFICATIONS DES RESULTATS
#---------------------------------------------------------------------
#

TEST_RESU(RESU=(_F(RESULTAT=MODE,
                   NUME_MODE=4,
                   PARA='FREQ',
                   VALE=658.2432,
                   CRITERE='RELATIF',
                   PRECISION=0.01,),
                _F(RESULTAT=MODE,
                   NUME_MODE=5,
                   PARA='FREQ',
                   VALE=1749.3488,
                   CRITERE='RELATIF',
                   PRECISION=0.01,),
                _F(RESULTAT=MODE,
                   NUME_MODE=6,
                   PARA='FREQ',
                   VALE=3225.4161,
                   CRITERE='RELATIF',
                   PRECISION=0.01,),),);

TEST_RESU(RESU=(_F(RESULTAT=ELAS,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   MAILLE='MA5',
                   VALE=2.227508550862E+05,
                   POINT=1,
                   PRECISION=1e-8,
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.0.15',),
                _F(RESULTAT=ELAS,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='MA10',
                   POINT=1,
                   VALE=9.5995307118466E+08,
                   PRECISION=1e-8,
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.0.15',),
                _F(RESULTAT=ELAS,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELNO_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='MA25',
                   NOEUD='NO100',
                   VALE=5.0435078630760E+10,
                   PRECISION=1e-8,
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.0.15',),),);

TEST_RESU(RESU=(_F(RESULTAT=MODE,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   MAILLE='MA5',
                   POINT=1,
                   VALE=2.227508550862E+05,
                   PRECISION=1e-8,
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=MODE,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='MA10',
                   POINT=1,
                   VALE=9.5995307118466E+08,
                   PRECISION=1e-8,
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=MODE,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELNO_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='MA25',
                   NOEUD='NO100',
                   VALE=5.0435078630760E+10,
                   PRECISION=1e-8,
                   REFERENCE='AUTRE_ASTER',),),);

FIN();
#
