                        INTITULE='DY',
                        RESULTAT =FLEX,
                        NOM_CHAM ='DEPL',
                        NOM_CMP = 'DY',
                        GROUP_NO='P_A_A_5',),)
                        
                        


DEPLF=RECU_FONCTION(TABLE=POST1,PARA_X='INST',PARA_Y='DY',)

