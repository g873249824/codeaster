DEBUT( CODE=_F(  NOM = 'SSLV139A',NIV_PUB_WEB='INTERNET') )

PRE_GIBI();

MAIL=LIRE_MAILLAGE();

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_MA=_F(NOM='TOUT',
                                 TOUT='OUI',),);

MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(GROUP_MA='TOUT',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='AXIS_FOURIER',),);

MAIL = MODI_MAILLAGE(MAILLAGE=MAIL, reuse=MAIL,
                     ORIE_PEAU_2D=_F(GROUP_MA=('BC',),),);

MAT1=DEFI_MATERIAU(ELAS=_F(E=2.1E11,
                           NU=0.3,
                           RHO=7800,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    MODELE=MODMECA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT1,),);

PCRIT=1./(2.*pi);


CLS=AFFE_CHAR_MECA(MODELE=MODMECA,
                   DDL_IMPO=(_F(GROUP_MA='DA',
                                DX=0.0,DZ=0.),
                             _F(GROUP_MA='BC',
                                DY=0.,DZ=0.),),);

CHAR=AFFE_CHAR_MECA(MODELE=MODMECA,
                    FORCE_CONTOUR=_F(GROUP_MA='BC',
                                     FX=PCRIT,),);

ELAS=MACRO_ELAS_MULT(MODELE=MODMECA,
                     CHAM_MATER=CHMAT,
                     CHAR_MECA_GLOBAL=CLS,
                     CAS_CHARGE=_F(MODE_FOURIER=0,
                                   TYPE_MODE='SYME',
                                   CHAR_MECA=CHAR,
                                   OPTION='SIEF_ELGA_DEPL',),);

SIG=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
               OPERATION='EXTR',
               RESULTAT=ELAS,
               NOM_CHAM='SIEF_ELGA_DEPL',
               TYPE_MAXI='MINI',
               TYPE_RESU='VALE',);

MAT_RI_M=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODMECA,
                        CHAM_MATER=CHMAT,
                        MODE_FOURIER=0,
                        CHARGE=(CLS,CHAR,),);

MAT_RI_G=CALC_MATR_ELEM(OPTION='RIGI_GEOM',
                        MODELE=MODMECA,
                        SIEF_ELGA=SIG,
                        MODE_FOURIER=0,);

MAT_MAS=CALC_MATR_ELEM(OPTION='MASS_MECA',
                       MODELE=MODMECA,
                       CHAM_MATER=CHMAT,);

NUM=NUME_DDL(MATR_RIGI=MAT_RI_M,);

MAS_RI_M=ASSE_MATRICE(MATR_ELEM=MAT_RI_M,
                      NUME_DDL=NUM,);

MAS_RI_G=ASSE_MATRICE(MATR_ELEM=MAT_RI_G,
                      NUME_DDL=NUM,);

MASSE=ASSE_MATRICE(MATR_ELEM=MAT_MAS,
                   NUME_DDL=NUM,);

RESULT=MODE_ITER_SIMULT(MATR_A=MAS_RI_M,
                        MATR_B=MAS_RI_G,
                        METHODE='SORENSEN',
                        NMAX_ITER_SOREN=20,
                        TYPE_RESU='MODE_FLAMB',
                        CALC_FREQ=_F(OPTION='PLUS_PETITE',
                                     NMAX_FREQ=10,),
                        VERI_MODE=_F(SEUIL=1e-04,),);


MODE=MODE_ITER_SIMULT(MATR_A=MAS_RI_M,
                      MATR_B=MASSE,
                      METHODE='SORENSEN',
                      NMAX_ITER_SOREN=20,
                      VERI_MODE=_F(SEUIL=1e-04,),);

MODE=CALC_ELEM(reuse =MODE,
               CHAM_MATER=CHMAT,
               RESULTAT=MODE,
               MODELE=MODMECA,
               OPTION=('SIEF_ELGA_DEPL','ETOT_ELGA','ETOT_ELNO_ELGA','ETOT_ELEM',),);

DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=MODE,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

EVOLELAS=CREA_RESU(OPERATION='AFFE',
                  TYPE_RESU='EVOL_ELAS',
                  NOM_CHAM='DEPL',
                  AFFE=_F(
                  CHAM_GD=DEPL,
                  MODELE=MODMECA,
                  CHAM_MATER=CHMAT,
                  INST=0.0,),);

EVOLELAS=CALC_ELEM(reuse =EVOLELAS,
                  RESULTAT=EVOLELAS,
                  OPTION=('SIEF_ELGA_DEPL','ETOT_ELGA','ETOT_ELNO_ELGA','ETOT_ELEM',),);

RES_NORM=NORM_MODE(MODE=RESULT,
                   NORME='TRAN',);

CHA_DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                    OPERATION='EXTR',
                    RESULTAT=RESULT,
                    NOM_CHAM='DEPL',
                    NUME_MODE=1,);

MODE_1=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                   OPERATION='EXTR',
                   RESULTAT=RES_NORM,
                   NOM_CHAM='DEPL',
                   NUME_ORDRE=1,);

TEST_RESU(CHAM_NO=_F(CHAM_GD=MODE_1,
                     NOM_CMP='DY',
                     REFERENCE='ANALYTIQUE',
                     NOEUD='N4',
                     VALE=1.0),
             RESU=_F(RESULTAT=RES_NORM,
                     NUME_MODE=1,
                     PARA='CHAR_CRIT',
                     VALE=849350.9396,
                     PRECISION=2.E-3,
                     REFERENCE='ANALYTIQUE',))

TEST_RESU(RESU=(_F(RESULTAT=EVOLELAS,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   POINT=1,
                   MAILLE='M10',
                   VALE= 1.1414821265010E-01,
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.0.17',),
                _F(RESULTAT=EVOLELAS,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='M10',
                   POINT=1,
                   VALE= 1.1612386752963E+09,
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.0.17',),
                _F(RESULTAT=EVOLELAS,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELNO_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='M6',
                   NOEUD='N15',
                   VALE= 1.8163004696786E+09,
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.0.17',),),);


TEST_RESU(RESU=(_F(RESULTAT=MODE,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   POINT=1,
                   MAILLE='M10',
                   VALE= 1.1414821265010E-01,
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=MODE,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='M10',
                   POINT=1,
                   VALE= 1.1612386752963E+09,
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=MODE,
                   NUME_ORDRE=1,
                   NOM_CHAM='ETOT_ELNO_ELGA',
                   NOM_CMP='TOTALE',
                   MAILLE='M6',
                   NOEUD='N15',
                   VALE= 1.8163004696786E+09,
                   REFERENCE='AUTRE_ASTER',),),);

FIN();
