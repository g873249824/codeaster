TEST_TABLE(TABLE=ECIN,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=n,),
           NOM_PARA='TOTALE',
           VALE=9.895889,
           REFERENCE='AUTRE_ASTER',);

#=============================================================================================
# impressions
#=============================================================================================


IMPR = 0;

#    PILO = 'INTERACTIF';

PILO = 'PNG';

IMPR_FONCTION(FORMAT='XMGRACE',
              PILOTE=PILO,
              UNITE=32,
              COURBE=_F(FONCTION=DZ,
                        LEGENDE='DZ',
                        COULEUR=2,
                        FREQ_MARQUEUR=0,),
              TITRE='DEPLACEMENT',);

IMPR_FONCTION(FORMAT='XMGRACE',
              PILOTE=PILO,
              UNITE=30,
              COURBE=_F(FONCTION=FZ,
                        LEGENDE='FZ',
                        COULEUR=1,
                        FREQ_MARQUEUR=0,),
              TITRE='FORCE',);

IMPR_FONCTION(FORMAT='XMGRACE',
              PILOTE=PILO,
              UNITE=31,
              COURBE=_F(FONCTION=RZ,
                        LEGENDE='RZ',
                        COULEUR=1,
                        FREQ_MARQUEUR=0,),
              TITRE='REACTION',);

IMPR_TABLE(TABLE=INER,);

IMPR_TABLE(TABLE=DEPL,);

IMPR_TABLE(TABLE=FORC,);

IMPR_TABLE(TABLE=REAC,);

IMPR_TABLE(TABLE=EPOT,);

IMPR_TABLE(TABLE=ECIN,);


FIN();

