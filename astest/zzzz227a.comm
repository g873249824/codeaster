DEBUT(  CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',)
DEFI_GROUP(reuse=MA,MAILLAGE=MA,CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',))

MO=AFFE_MODELE(MAILLAGE=MA,AFFE=_F(TOUT='OUI',PHENOMENE='MECANIQUE',MODELISATION='3D'))

EF=DEFI_FONCTION(NOM_PARA='TEMP',VALE=(0.,5.15E8,500.,5.15E8,),PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',)
NUF=DEFI_CONSTANTE(VALE=0.2,)

ALPHAF=DEFI_FONCTION(NOM_PARA='TEMP',PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',
     VALE=(0.   , 1.E-3,
           100. , 5.E-3,
           200. , 8.E-3,
           300. , 9.E-3,
           400. , 6.E-3,
           500. , 4.E-3,),
     )

ALPHAF2=DEFI_FONCTION(NOM_PARA='TEMP',PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',
     VALE=(0.   , 1.E-3,
           100. , 9.E-3,
           200. , 8.E-3,
           300. , 4.E-3,
           400. , 6.E-3,
           500. , 9.E-3,),
     )

MAT=DEFI_MATERIAU(ELAS_FO=_F(E=EF,NU=NUF,ALPHA=ALPHAF,TEMP_DEF_ALPHA=120.))
MAT2=DEFI_MATERIAU(ELAS_FO=_F(E=EF,NU=NUF,ALPHA=ALPHAF2,TEMP_DEF_ALPHA=20.))
MAT3=DEFI_MATERIAU(ELAS_FO=_F(E=EF,NU=NUF,ALPHA=ALPHAF2,TEMP_DEF_ALPHA=220.))

MATB2=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 2.0))
MATB3=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 2.0))

CHT1=CREA_CHAMP(TYPE_CHAM='CART_TEMP_R', OPERATION='AFFE', MAILLAGE=MA,
                AFFE=_F(TOUT='OUI',NOM_CMP='TEMP',VALE=30.))


CHARCI=AFFE_CHAR_CINE(MODELE=MO,MECA_IMPO=_F(GROUP_NO='BASE1',DX=10.),)

CHARST=AFFE_CHAR_MECA(MODELE=MO,DDL_IMPO=_F(GROUP_MA='BASE1',DZ=0.,DY=0.))

L1=DEFI_LIST_REEL(VALE=(1.))


#--------------------------------------------------------------------------------------
# Validation de la prise ne compte de plusieurs temperature de reference :
# (non-regression)
#---------------------------------------------------------------------------
CHMAT2=AFFE_MATERIAU(MAILLAGE=MA,AFFE=(
           _F(TOUT='OUI', MATER=MAT),
           _F(MAILLE='M1', MATER=MAT2),
           _F(MAILLE='M8', MATER=MAT3),
           _F(MAILLE='M18',MATER=MAT),
        ),
        AFFE_VARC=(
           _F(TOUT='OUI',NOM_VARC='TEMP',CHAM_GD=CHT1,VALE_REF=0.),
           _F(MAILLE='M1',NOM_VARC='TEMP',CHAM_GD=CHT1,VALE_REF=100.),
           _F(MAILLE='M8',NOM_VARC='TEMP',CHAM_GD=CHT1,VALE_REF=200.),
           _F(MAILLE='M18',NOM_VARC='TEMP',CHAM_GD=CHT1,VALE_REF=300.),
         ),
         )


RESU2=MECA_STATIQUE(MODELE=MO,CHAM_MATER=CHMAT2,LIST_INST=L1,
         EXCIT=(_F(CHARGE=CHARST),_F(CHARGE=CHARCI)))


TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='C',
                  RESULTAT=RESU2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=-0.38209964322068002,),
          )


#------------------------------------------------------------------------------------
# Validation de la commande TEST_RESU sur des cartes (fiche 18997) :
#------------------------------------------------------------------

CH1=CREA_CHAMP(TYPE_CHAM='CART_TEMP_R', OPERATION='AFFE', MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',NOM_CMP='TEMP',VALE=30.))
TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE',
                   NOM_CMP='TEMP',
                   VALE_CALC=30.000000000,
                   VALE_REFE=30.0,
                   MAILLE='M1',
                   CHAM_GD=CH1,),
          )

CH2=CREA_CHAMP(TYPE_CHAM='CART_DEPL_C', OPERATION='AFFE', MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',NOM_CMP='DZ', VALE_C = ('RI',1.0,0.5,)))
TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE',
                   VALE_CALC_C=1.000000000+0.500000000j,
                   VALE_REFE_C=('RI', 1.0, 0.5),
                   NOM_CMP='DZ',
                   MAILLE='M1',
                   CHAM_GD=CH2,),
          )

CH3=CREA_CHAMP(TYPE_CHAM='CART_NEUT_I', OPERATION='AFFE', MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',NOM_CMP='X3', VALE_I = 17))
TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE',
                   NOM_CMP='X3',
                   VALE_CALC_I=17,
                   VALE_REFE_I=17,
                   MAILLE='M1',
                   CHAM_GD=CH3,),
          )

#-----------------------------------------------------------------------------------
# Validation de l'assemblage de 2 cartes (CREA_CHAMP / ASSE) (fiche 18995) :
#---------------------------------------------------------------------------
CART1=CREA_CHAMP(TYPE_CHAM='CART_TEMP_R', OPERATION='AFFE', MAILLAGE=MA,
                AFFE=_F(TOUT='OUI',NOM_CMP='TEMP',VALE=31.),INFO=1)

CART2=CREA_CHAMP(TYPE_CHAM='CART_TEMP_R', OPERATION='AFFE', MAILLAGE=MA,
                AFFE=_F(TOUT='OUI',NOM_CMP='TEMP',VALE=32.),INFO=1)

CART3=CREA_CHAMP(TYPE_CHAM='CART_TEMP_R', OPERATION='ASSE', MAILLAGE=MA,
                 ASSE=( _F(TOUT='OUI' , CHAM_GD=CART1, CUMUL='OUI', COEF_R=2.,),
                        _F(MAILLE='M3', CHAM_GD=CART2, CUMUL='OUI', COEF_R=3.,),
                 ),INFO=1,)
# 158=3*32 + 2*31
TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE',
                   NOM_CMP='TEMP',
                   VALE_CALC=158.000000000,
                   VALE_REFE=158.0,
                   MAILLE='M3',
                   CHAM_GD=CART3,),
          )

TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE',
                   NOM_CMP='TEMP',
                   VALE_CALC=62.000000000,
                   VALE_REFE=62.0,
                   MAILLE='M1',
                   CHAM_GD=CART3,),
          )

FIN()
