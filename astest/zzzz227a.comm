# ON VEUT VERIFIER QUE LA CARTE DES TEMPERATURE DE REFERENCE EST BIEN CREEE
# DANS LES CAS SUIVANTS :
#   - AVEC LA SYNTAXE : AFFE_MATERIAU/AFFE_VARC/VALE_REF
#   - DANS LE CAS OU L'ON AFFECTE PLUSIEURS MATERIAUX SUR LES MAILLES.

# LA SOLUTION DE "REFERENCE" EST OBTENUE AVEC 1 SEUL MATERIAU PAR MAILLE (CALCUL 0).

# ON VERIFIE ALORS QUE L'ON OBTIENT LES MEMES RESULTATS POUR LES 4 CALCULS :
# CALCUL 2 : 1 MATERIAU PAR MAILLE          + NOUVELLE SYNTAXE
# CALCUL 3 : PLUSIEURS MATERIAUX PAR MAILLE + NOUVELLE SYNTAXE
#-------------------------------------------------------------------------------


DEBUT(  CODE=_F(NOM='ZZZZ227A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE()
DEFI_GROUP(reuse=MA,MAILLAGE=MA,CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',))

MO=AFFE_MODELE(MAILLAGE=MA,AFFE=_F(TOUT='OUI',PHENOMENE='MECANIQUE',MODELISATION='3D'))

EF=DEFI_FONCTION(NOM_PARA='TEMP',VALE=(0.,5.15E8,500.,5.15E8,),PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',)
NUF=DEFI_CONSTANTE(VALE=0.2,)

ALPHAF=DEFI_FONCTION(NOM_PARA='TEMP',PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',
     VALE=(0.   , 1.E-3,
           100. , 5.E-3,
           200. , 8.E-3,
           300. , 9.E-3,
           400. , 6.E-3,
           500. , 4.E-3,),
     )

ALPHAF2=DEFI_FONCTION(NOM_PARA='TEMP',PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',
     VALE=(0.   , 1.E-3,
           100. , 9.E-3,
           200. , 8.E-3,
           300. , 4.E-3,
           400. , 6.E-3,
           500. , 9.E-3,),
     )

MAT=DEFI_MATERIAU(ELAS_FO=_F(E=EF,NU=NUF,ALPHA=ALPHAF,TEMP_DEF_ALPHA=120.))
MAT2=DEFI_MATERIAU(ELAS_FO=_F(E=EF,NU=NUF,ALPHA=ALPHAF2,TEMP_DEF_ALPHA=20.))
MAT3=DEFI_MATERIAU(ELAS_FO=_F(E=EF,NU=NUF,ALPHA=ALPHAF2,TEMP_DEF_ALPHA=220.))

MATB2=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 2.0))
MATB3=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 2.0))

CHT1=CREA_CHAMP(TYPE_CHAM='CART_TEMP_R', OPERATION='AFFE', MAILLAGE=MA,
                AFFE=_F(TOUT='OUI',NOM_CMP='TEMP',VALE=30.))


CHARCI=AFFE_CHAR_CINE(MODELE=MO,MECA_IMPO=_F(GROUP_NO='BASE1',DX=10.),)

CHARST=AFFE_CHAR_MECA(MODELE=MO,DDL_IMPO=_F(GROUP_MA='BASE1',DZ=0.,DY=0.))

L1=DEFI_LIST_REEL(VALE=(1.))


# CALCUL 2: CALCUL AVEC 1 MATERIAU PAR MAILLE:
#----------------------------------------------------------------------------
CHMAT2=AFFE_MATERIAU(MAILLAGE=MA,AFFE=(
           _F(TOUT='OUI', MATER=MAT),
           _F(MAILLE='M1', MATER=MAT2),
           _F(MAILLE='M8', MATER=MAT3),
           _F(MAILLE='M18',MATER=MAT),
        ),
        AFFE_VARC=(
           _F(TOUT='OUI',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=0.),
           _F(MAILLE='M1',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=100.),
           _F(MAILLE='M8',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=200.),
           _F(MAILLE='M18',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=300.),
         ),
         )


RESU2=MECA_STATIQUE(MODELE=MO,CHAM_MATER=CHMAT2,LIST_INST=L1,
         EXCIT=(_F(CHARGE=CHARST),_F(CHARGE=CHARCI)))


TEST_RESU(RESU=_F(NUME_ORDRE=1, NOM_CMP='DZ', GROUP_NO='C', REFERENCE='NON_REGRESSION',
          VALE=-3.8209964322068E-01, RESULTAT=RESU2, NOM_CHAM='DEPL'))


# CALCUL 3: CALCUL AVEC PLUSIEURS MATERIAUX PAR MAILLE:
#----------------------------------------------------------------------------
CHMAT3=AFFE_MATERIAU(MAILLAGE=MA,AFFE=(
           _F(TOUT='OUI', MATER=MAT),
           _F(MAILLE='M1', MATER=(MAT2,MATB2,MATB3)),
           _F(MAILLE='M8', MATER=(MAT3,MATB2,MATB3)),
           _F(MAILLE='M18',MATER=(MAT,MATB2,MATB3)),
        ),
        AFFE_VARC=(
           _F(TOUT='OUI',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=0.),
           _F(MAILLE='M1',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=100.),
           _F(MAILLE='M8',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=200.),
           _F(MAILLE='M18',NOM_VARC='TEMP',CHAMP_GD=CHT1,VALE_REF=300.),
         ),
         )

RESU3=MECA_STATIQUE(MODELE=MO,CHAM_MATER=CHMAT3,LIST_INST=L1,
         EXCIT=(_F(CHARGE=CHARST),_F(CHARGE=CHARCI)))


TEST_RESU(RESU=_F(NUME_ORDRE=1, NOM_CMP='DZ', GROUP_NO='C', REFERENCE='AUTRE_ASTER',
          VALE=-3.8209964322068E-01, RESULTAT=RESU3, NOM_CHAM='DEPL'))


FIN()

