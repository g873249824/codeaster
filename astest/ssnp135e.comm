           CRITERE='RELATIF',
           PRECISION=0.04,
           REFERENCE='AUTRE_ASTER',);

##################################
# 9. VISUALISATION DU ETA_PILOTAGE
#    EN FONCTION DE L'INSTANT
##################################

ETA_PILO=RECU_FONCTION(RESULTAT=RESU,
                       TOUT_ORDRE='OUI',
                       NOM_PARA_RESU='ETA_PILOTAGE',);

FIN();
