           NOM_PARA='DX',
           VALE=-3.4606865864745E-02,
           CRITERE='RELATIF',
           PRECISION=0.01,
           REFERENCE='AUTRE_ASTER',);

###
# B.DEPLACEMENTS DU POINT B (BORD DU TROU)
###

DY_B=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='DY_B',
                             RESULTAT=RESU,
                             NOM_CHAM='DEPL',
                             GROUP_NO='GM32',
                             NOM_CMP='DY',),);

TEST_TABLE(TABLE=DY_B,
           FILTRE=_F(NOM_PARA='INST',
