###
# C. CONTRAINTE SIXX AU POINT B
###

SXX_B2=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='SXX_B2',
                               RESULTAT=RESU,
                               NOM_CHAM='SIEF_NOEU',
                               GROUP_NO='GM32',
                               NOM_CMP='SIXX',),);

TEST_TABLE(TABLE=SXX_B2,
           FILTRE=_F(NOM_PARA='INST',
