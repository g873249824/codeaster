           CRITERE='RELATIF',
           PRECISION=1e-06,
           REFERENCE='NON_REGRESSION',
           VERSION='9.1.14',);

###
# B. CONTRAINTE SIYY AU POINT A
###

SYY_A2=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='SYY_A2',
                               RESULTAT=RESU,
                               NOM_CHAM='SIEF_NOEU',
                               GROUP_NO='GM33',
                               NOM_CMP='SIYY',),);

TEST_TABLE(TABLE=SYY_A2,
