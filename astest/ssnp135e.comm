           NOM_PARA='DY',
           VALE=7.7160730362990E-02,
           CRITERE='RELATIF',
           PRECISION=1e-08,
           REFERENCE='NON_REGRESSION',
           VERSION='9.1.14',);

TEST_TABLE(TABLE=DY_B,
