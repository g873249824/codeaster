                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=50.000000000,
                   VALE_REFE=50.0,),
                _F(NUME_ORDRE=2,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELNO',
                   NOEUD='N66',
