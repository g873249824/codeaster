# CAS TEST : SSNP152C
# ELEMENTS LINEAIRES, GRANDES ROTATIONS, CONTRAINTES PLANES, INTEGRATION PAR NOEUD,
# p=1.0E7, E1=1.0E9, E2=1.0E9, nu1=0.2, nu2=0.2, R1 = 1.0, R2 = 0.6, R3 = 0.2

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'))


MA = LIRE_MAILLAGE(FORMAT='GMSH',
                   UNITE=19,
                   )

#| noms des groupes de noeuds et mailles

MA=DEFI_GROUP(reuse=MA,MAILLAGE=MA,CREA_GROUP_MA=(_F(GROUP_MA='GM100',NOM='C1'),
                     _F(GROUP_MA='GM101',NOM='C2'),
                     _F(GROUP_MA='GM200',NOM='S1R1'),
                     _F(GROUP_MA='GM300',NOM='S1R2'),
                     _F(GROUP_MA='GM400',NOM='S2R2'),
                     _F(GROUP_MA='GM500',NOM='S2R3'),),
                                   CREA_GROUP_NO=(_F(GROUP_MA='S1R2',NOM='CONT_N_1'),
                      _F(GROUP_MA='S2R2',NOM='CONT_N_2'),),);

MA=MODI_MAILLAGE(reuse=MA,MAILLAGE=MA,ORIE_PEAU_2D=(
            _F(GROUP_MA = 'S1R1',),
            _F(GROUP_MA = 'S1R2',),
            _F(GROUP_MA = 'S2R2',),
            _F(GROUP_MA = 'S2R3',),),);



elmts   = 40
pas_tmp = 100
tour    = 2*pi/elmts

E1=1.0E9
E2=1.0E9
nu1=0.2
nu2=0.2

# False plastic material for large strains in elasticity (ticket #26286)
MAT1=DEFI_MATERIAU(ELAS = _F(E=E1,NU = nu1,),ECRO_LINE=_F(D_SIGM_EPSI=1.0,SY=1E15),)
MAT2=DEFI_MATERIAU(ELAS = _F(E=E2,NU = nu2,),ECRO_LINE=_F(D_SIGM_EPSI=1.0,SY=1E15),)

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,AFFE=(_F(GROUP_MA = 'C1',MATER = MAT1,),
                  _F(GROUP_MA = 'C2',MATER = MAT2,),),);

MO=AFFE_MODELE(MAILLAGE=MA,AFFE=_F(TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = 'C_PLAN'));


# pression externe

p=1.0E7;


R1 = 1.0;
R2 = 0.6;
R3 = 0.2;

# calcul analytique de la pression de contact

pres_cont = (2*p*(R1*R1/(R1*R1-R2*R2)))/(((1/(R1*R1-R2*R2))*(R2*R2*(1-nu1)+R1*R1*(1+nu1)))+((E1/E2)*(1/(R2*R2-R3*R3))*(R2*R2*(1-nu2)+R3*R3*(1+nu2))));


# couronne externe

a1=(pres_cont*R2*R2-p*R1*R1)/(R1*R1-R2*R2);
b1=(pres_cont-p)*(R1*R1*R2*R2)/(R1*R1-R2*R2);

eps_r_1 = (1/E1)*(a1*(1-nu1)*R1+b1*(1+nu1)/R1);

ux1 = FORMULE(VALE='eps_r_1*cos(atan2(Y,X))',
              eps_r_1=eps_r_1,
              NOM_PARA=['X', 'Y'],)

uy1 = FORMULE(VALE='eps_r_1*sin(atan2(Y,X))',
              eps_r_1=eps_r_1,
              NOM_PARA=['X', 'Y'],)


CH1=AFFE_CHAR_MECA_F(MODELE=MO, DDL_IMPO=(_F(GROUP_MA = 'S1R1', DX=ux1,DY=uy1,),),);

# couronne interne

a2=-pres_cont*R2*R2/(R2*R2-R3*R3);
b2=-pres_cont*(R2*R2*R3*R3)/(R2*R2-R3*R3);

eps_r_2 = (1/E2)*(a2*(1-nu2)*R3+b2*(1+nu2)/R3);

ux2 = FORMULE(VALE='(R3+eps_r_2)*cos(atan2(Y,X)+tour*INST)-X',
              R3=R3,
              eps_r_2=eps_r_2,
              tour=tour,
              NOM_PARA=['X', 'Y', 'INST'],)

uy2 = FORMULE(VALE='(R3+eps_r_2)*sin(atan2(Y,X)+tour*INST)-Y',
              R3=R3,
              eps_r_2=eps_r_2,
              tour=tour,
              NOM_PARA=['X', 'Y', 'INST'],)

CH2=AFFE_CHAR_MECA_F(MODELE=MO, DDL_IMPO=(_F(GROUP_MA='S2R3',DX=ux2,DY=uy2,),),);


#+----------------------------------------+
#|               contact                  |
#+----------------------------------------+

CONT_N  = DEFI_CONTACT(MODELE = MO,
                       FORMULATION    = 'CONTINUE',
                       LISSAGE        = 'OUI',
                       ALGO_RESO_CONT = 'NEWTON',
                       ALGO_RESO_GEOM = 'NEWTON',
                       ZONE =(
                           _F(GROUP_MA_ESCL = 'S2R2',
                              GROUP_MA_MAIT = 'S1R2',
                              APPARIEMENT   = 'MAIT_ESCL',
                              NORMALE       = 'MAIT',
                              INTEGRATION   = 'AUTO',
                              TOLE_PROJ_EXT = 0.0001,
                              ALGO_CONT     = 'STANDARD',
                              COEF_CONT     = E1,
                              CONTACT_INIT  = 'OUI',
                              GLISSIERE     = 'NON',),),);

CONT_G  = DEFI_CONTACT(MODELE = MO,
                       FORMULATION    = 'CONTINUE',
                       LISSAGE        = 'OUI',
                       ALGO_RESO_CONT = 'NEWTON',
                       ALGO_RESO_GEOM = 'NEWTON',
                       ZONE =(
                           _F(GROUP_MA_ESCL = 'S2R2',
                              GROUP_MA_MAIT = 'S1R2',
                              APPARIEMENT   = 'MAIT_ESCL',
                              NORMALE       = 'MAIT',
                              INTEGRATION   = 'GAUSS',
                              ORDRE_INT     =2,
                              TOLE_PROJ_EXT = 0.0001,
                              ALGO_CONT     = 'STANDARD',
                              COEF_CONT     = E1,
                              CONTACT_INIT  = 'OUI',
                              GLISSIERE     = 'NON',),),);

#+----------------------------------------+
#|               calcul                   |
#+----------------------------------------+

INSTS=DEFI_LIST_REEL(DEBUT       = 0.0,
                      INTERVALLE = _F(JUSQU_A = 1.0,
                                      NOMBRE  = pas_tmp,),);

RESU1=STAT_NON_LINE(MODELE      = MO,
                    CHAM_MATER  = CHMAT,
                    EXCIT       = (_F(CHARGE = CH1,),
                                   _F(CHARGE = CH2,),),
                    CONTACT     = CONT_N,
                    COMPORTEMENT   = (_F(RELATION     = 'VMIS_ISOT_LINE',
                                      DEFORMATION  = 'GROT_GDEP',),),
                    NEWTON      = _F(REAC_INCR=1,REAC_ITER=1),
                    INCREMENT   = _F(LIST_INST=INSTS,),
                    CONVERGENCE = _F(ITER_GLOB_MAXI = 20,),
                    SOLVEUR     = _F(METHODE     = 'MUMPS',
                                     ),
                    ARCHIVAGE   = _F(CHAM_EXCLU = 'VARI_ELGA',),
                    INFO=1,);

RESU2=STAT_NON_LINE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    EXCIT       = (_F(CHARGE = CH1,),
                                   _F(CHARGE = CH2,),),
                    CONTACT     = CONT_G,
                    COMPORTEMENT   = (_F(RELATION    = 'VMIS_ISOT_LINE',
                                     DEFORMATION = 'GROT_GDEP',),),
                    NEWTON      = _F(REAC_INCR=1,REAC_ITER=1),
                    INCREMENT   = _F(LIST_INST=INSTS,),
                    CONVERGENCE = _F(ITER_GLOB_MAXI = 20,),
                    SOLVEUR     = _F(METHODE     = 'MUMPS',
                                     ),
                    ARCHIVAGE   = _F(CHAM_EXCLU = 'VARI_ELGA',),
                    INFO=1,);
#+----------------------------------------+
#|          tests de validation           |
#+----------------------------------------+
TEST_RESU(RESU=_F(NUME_ORDRE=100,
                  VALE_ABS='OUI',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU1,
                  NOM_CHAM='DEPL',
                  NOEUD='N10',
                  NOM_CMP='LAGS_C',
                  VALE_CALC= 9.08676484E+06,
                  VALE_REFE=9.2592592592592593E6,
                  PRECISION=0.040000000000000001,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=100,
                  VALE_ABS='OUI',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU2,
                  NOM_CHAM='DEPL',
                  NOEUD='N10',
                  NOM_CMP='LAGS_C',
                  VALE_CALC= 9.10546887E+06,
                  VALE_REFE=9.2592592592592593E6,
                  PRECISION=0.040000000000000001,),
          )

FIN();
