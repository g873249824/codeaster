                                LISSAGE_THETA='LAGRANGE'),
                     )
   TEST_TABLE(       TABLE=GLOC1[i],
                     NOM_PARA='G',
                     FILTRE=_F(  NOM_PARA = 'NOEUD',
                            VALE_K = 'N403',      ),
                     VALE_CALC=11.859037296472,
                     VALE_REFE=1.158648E+01,
                     PRECISION=0.04,
                     REFERENCE='ANALYTIQUE'       )

   TEST_TABLE(       TABLE=GLOC1[i],
                     NOM_PARA='G',
                     FILTRE=_F(  NOM_PARA = 'NOEUD',
                            VALE_K = 'N2862',      ),
                     VALE_CALC=11.605181997437,
                     VALE_REFE=1.158648E+01,
                     PRECISION=0.01,
                     REFERENCE='ANALYTIQUE'       )

   TEST_TABLE(       TABLE=GLOC1[i],
