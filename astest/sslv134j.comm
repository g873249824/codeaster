   TEST_TABLE(       TABLE=GLOC2[i],
                     NOM_PARA='G',
                     FILTRE=_F(  NOM_PARA = 'NOEUD',
                            VALE_K = 'N292',      ),
                     VALE_CALC=11.745482956201, 
                     VALE_REFE=1.158648E+01,
                     PRECISION=0.03,
                     REFERENCE='ANALYTIQUE'       )

   TEST_TABLE(TABLE=GLOC2[i],
           NOM_PARA='G',
           TYPE_TEST = 'MAX',
           VALE_CALC=11.771256753319,
           VALE_REFE=1.1714999753978E+01,
           PRECISION=0.02,
           REFERENCE='ANALYTIQUE'       )

   TEST_TABLE(TABLE=GLOC2[i],
