DEBUT(CODE=_F(NOM='SSLV134J',NIV_PUB_WEB='INTERNET'))

M=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED')



M=MODI_MAILLAGE( reuse=M,   MAILLAGE=M,
                        ORIE_PEAU_3D=_F(  GROUP_MA = ( 'SINF_2', 'SINF_4', ),  )
                      )

MA=DEFI_MATERIAU(            ELAS=_F(  E = 2.E11,
                                        NU = 0.3,          ) )

CM=AFFE_MATERIAU(        MAILLAGE=M,
                                 AFFE=_F(  TOUT = 'OUI',
                                        MATER = MA,
                                        ))

M=DEFI_GROUP(reuse=M,   MAILLAGE=M,CREA_GROUP_NO=(
                _F(  GROUP_MA = 'SSUP_F',  ),
                _F(  GROUP_MA = 'SINF_F',   )) )
                
MO = [None]*2                

MO[0]=AFFE_MODELE(             MAILLAGE=M,
                                  VERIF='MAILLE',
                                 AFFE=_F(  GROUP_MA = 'MAIL',
                                        PHENOMENE = 'MECANIQUE',
                                        MODELISATION = '3D_INCO', ))
                                        
MO[1]=AFFE_MODELE(             MAILLAGE=M,
                                  VERIF='MAILLE',
                                 AFFE=_F(  GROUP_MA = 'MAIL',
                                        PHENOMENE = 'MECANIQUE',
                                        MODELISATION = '3D_INCO_UP', ))
                                        
LINST = DEFI_LIST_REEL   ( DEBUT      =  0.,
                           INTERVALLE =_F (JUSQU_A= 1.0, NOMBRE = 1   )
                                         );
FF1=DEFI_FOND_FISS(MAILLAGE=M,
                   FOND_FISS=_F(TYPE_FOND='FERME',
                                GROUP_MA = 'LFF',
                                NOEUD_ORIG = 'N49',
                                MAILLE_ORIG = 'M3219',),
                   SYME='OUI',
                   LEVRE_SUP=_F(GROUP_MA = 'LEVINF',)
                  )

FF=DEFI_FOND_FISS(MAILLAGE=M,
                  FOND_FISS=_F(TYPE_FOND='FERME',
                               GROUP_MA = 'LFF',
                               NOEUD_ORIG = 'N49',
                               MAILLE_ORIG = 'M3219',),
                   SYME='OUI',
                   LEVRE_SUP=_F(GROUP_MA = 'LEVINF',)
                 )
CH = [None]*2 
RESU = [None]*2
THET1 = [None]*2
THET = [None]*2
G01 = [None]*2
G0 = [None]*2
GLOC1 = [None]*2
GLOC2 =[None]*2
GLOC3 =[None]*2

for i in range(2) :
   CH[i]=AFFE_CHAR_MECA(       MODELE=MO[i],
                              DDL_IMPO=(
                         _F(  NOEUD = 'N9957',         DX = 0.,    ),
                         _F(  NOEUD = 'N10021',        DX = 0.,    ),
                         _F(  NOEUD = 'N9966',         DY = 0.,    ),
                         _F(  NOEUD = 'N10094',        DY = 0.,    ),
                         _F(  GROUP_NO = 'SSUP_F',     DZ = 0.,    )),PRES_REP=(
                         _F(  GROUP_MA = 'SINF_1',   PRES = -1.E6, ),
                         _F(  GROUP_MA = 'SINF_2',   PRES = -1.E6, ),
                         _F(  GROUP_MA = 'SINF_3',   PRES = -1.E6, ),
                         _F(  GROUP_MA = 'SINF_4',   PRES = -1.E6, ))
                   )

   RESU[i] = STAT_NON_LINE (MODELE     =   MO[i],
                        CHAM_MATER =   CM,
                        EXCIT      =_F ( CHARGE       = CH[i],),
                        COMP_ELAS  =_F ( RELATION     = 'ELAS',
                                         DEFORMATION  = 'PETIT',
                                         TOUT         = 'OUI'    ),
                        INCREMENT  =_F ( LIST_INST    = LINST, ),
                        CONVERGENCE = _F(
                              RESI_GLOB_MAXI=1e-07,
                              ITER_GLOB_MAXI = 15,
                  ),
                        NEWTON     =_F ( REAC_INCR    = 1 ))

   THET1[i]=CALC_THETA(MODELE=MO[i],
                 FOND_FISS=FF1,
                 THETA_3D=_F(  TOUT = 'OUI',
                               MODULE = 1.0,      )
                  )
   G01[i]=CALC_G(        RESULTAT=RESU[i],
                   OPTION='CALC_G_GLOB',
                   INST = 1,
                   THETA=_F(THETA=THET1[i],SYME='OUI'),)

   TEST_TABLE(TABLE=G01[i],
              NOM_PARA='G',
              VALE_CALC=146.74220492905,
              VALE_REFE=145.60,
              REFERENCE='NON_DEFINI',
              PRECISION=0.02)


   THET[i]=CALC_THETA(MODELE=MO[i],
                FOND_FISS=FF,
                IMPRESSION=_F(UNITE=62),
                THETA_3D=_F(  TOUT = 'OUI',
                     MODULE = 1.0,      )
                  )

