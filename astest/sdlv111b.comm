DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED')

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MA=DEFI_GROUP( reuse=MA,
                 MAILLAGE=MA,CREA_GROUP_NO=(
           _F( GROUP_MA = 'SB'),
           _F( GROUP_MA = 'SH'))
                   )

MO=AFFE_MODELE(
            MAILLAGE=MA,
               AFFE=_F(  GROUP_MA = 'V',
                      PHENOMENE = 'MECANIQUE',
                      MODELISATION = '3D_FAISCEAU')
                    )

MTB=DEFI_MATERIAU(
                ELAS=_F(      E = 1.0E+9,
                           NU = 0.3,
                           RHO = 7641.),
              FLUIDE=_F(      RHO = 1000.)
                      )

AMB=AFFE_MATERIAU(
             MAILLAGE=MA,
                AFFE=_F(  TOUT = 'OUI',
                       MATER = MTB)
                      )

AC=AFFE_CARA_ELEM(
    MODELE=MO,
    POUTRE=_F(GROUP_MA = 'V', SECTION = 'RECTANGLE',
              CARA = ('H',), VALE = ( 0.15, ),),
    ORIENTATION=_F(GROUP_MA = 'V', CARA = 'ANGL_NAUT', VALE = (0., -90., 0.,),),
    POUTRE_FLUI=_F(GROUP_MA = 'V',
                    B_T = 0.79, B_N = 0.79, A_FLUI = 2.16,
                    A_CELL = 4.41, COEF_ECHELLE = 10.)
)

CHB=AFFE_CHAR_MECA(
                  MODELE=MO,DDL_IMPO=(
               _F(  GROUP_NO = 'SB',
                          DX = 0.,  DY = 0.,  DZ = 0.,
                          DRX = 0., DRY = 0., DRZ = 0.),
               _F(  GROUP_NO = 'SH',
                                    DZ = 0.,
                          DRX = 0., DRY = 0., DRZ = 0.),
               _F(  NOEUD = 'N1',
                          PHI = 0.)),LIAISON_UNIF=(
           _F(  GROUP_NO = 'SH',
                          DDL = 'DX'),
           _F(  GROUP_NO = 'SH',
                          DDL = 'DY'))
                       )

MASSB=POST_ELEM(        MODELE=MO,
                         CHAM_MATER=AMB,
                          CARA_ELEM=AC,
                          MASS_INER=_F(  TOUT = 'OUI')
                     )


IMPR_TABLE(TABLE=MASSB)

# TEST SUR LA MASSE:
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC= 1.26950760E+04,
           VALE_REFE=1.2695075999999999E4,
           NOM_PARA='MASSE',
           TABLE=MASSB,)

# TEST SUR LE CENTRE DE GRAVITE:
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=0.420000000,
           VALE_REFE=0.41999999999999998,
           NOM_PARA='CDG_X',
           TABLE=MASSB,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=0.420000000,
           VALE_REFE=0.41999999999999998,
           NOM_PARA='CDG_Y',
           TABLE=MASSB,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=2.050000000,
           VALE_REFE=2.0499999999999998,
           NOM_PARA='CDG_Z',
           TABLE=MASSB,)

# TEST SUR LES INERTIES AU CENTRE DE GRAVITE:
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC= 1.85301561E+04,
           VALE_REFE=1.8530154999999999E4,
           NOM_PARA='IX_G',
           TABLE=MASSB,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC= 1.85301561E+04,
           VALE_REFE=1.8530154999999999E4,
           NOM_PARA='IY_G',
           TABLE=MASSB,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC= 1.49294094E+03,
           VALE_REFE=1492.941,
           NOM_PARA='IZ_G',
           TABLE=MASSB,)

MRB=CALC_MATR_ELEM(
                MODELE=MO,
                OPTION='RIGI_MECA',
            CHAM_MATER=AMB,
                CHARGE=CHB,
             CARA_ELEM=AC
                       )

MMB=CALC_MATR_ELEM(
                MODELE=MO,
                OPTION='MASS_MECA',
            CHAM_MATER=AMB,
                CHARGE=CHB,
             CARA_ELEM=AC
                       )

NUMB=NUME_DDL(
            MATR_RIGI=MRB
                 )

MASRB=ASSE_MATRICE(  MATR_ELEM=MRB,    NUME_DDL=NUMB  )

MASMB=ASSE_MATRICE(  MATR_ELEM=MMB,    NUME_DDL=NUMB  )

RESB=CALC_MODES(MATR_RIGI=MASRB,
                OPTION='BANDE',
                CALC_FREQ=_F(FREQ=( 0.40,3.8,),
                             SEUIL_FREQ=0.,
                             ),
                MATR_MASS=MASMB,
                SOLVEUR_MODAL=_F(MODE_RIGIDE='NON',
                                 METHODE='TRI_DIAG',
                                 ),
                )


TEST_RESU(RESU=(_F(PARA='FREQ',
                   NUME_MODE=1,
                   RESULTAT=RESB,
                   VALE_CALC=0.693210232,
                   VALE_REFE=0.69079999999999997,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                _F(PARA='FREQ',
                   NUME_MODE=2,
                   RESULTAT=RESB,
                   VALE_CALC=0.693210232,
                   VALE_REFE=0.69079999999999997,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                _F(PARA='FREQ',
                   NUME_MODE=19,
                   RESULTAT=RESB,
                   VALE_CALC=3.795934021,
                   VALE_REFE=3.7871000000000001,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                _F(PARA='FREQ',
                   NUME_MODE=20,
                   RESULTAT=RESB,
                   VALE_CALC=3.795934021,
                   VALE_REFE=3.7871000000000001,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                ),
          )

FIN()
