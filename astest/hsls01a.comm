# PLAQUE RECTANGULAIRE MINCE ENCASTREE SOUMISE
# A UN GRADIENT THERMIQUE DANS L'EPAISSEUR
# ET MODELISEE EN DKT- DKQ - DST - DSQ - Q4G
#====================================================================

DEBUT(CODE=_F(NOM='HSLS01A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE( )

#====================================================================
MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.E+11,  NU = 0.3,  ALPHA = 1.E-5) )


#====================================================================

MOD=AFFE_MODELE(  MAILLAGE=MA,
                  AFFE=(_F(  GROUP_MA = ('STRIA1','SQUA11'),  PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'DST'),
                        _F(  GROUP_MA = ('STRIA2','SQUA12'),  PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'DKT'),
                        _F(  GROUP_MA = 'SQUA2',  PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'Q4G'))
                 )

CARA=AFFE_CARA_ELEM(  MODELE=MOD,
                      COQUE=_F(  GROUP_MA = ('STRIA1','SQUA11','STRIA2','SQUA12','SQUA2'),
                                 EPAIS = 0.01)
                   )

TEMP=CREA_CHAMP(  OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=MA,
                  AFFE=_F( TOUT = 'OUI',
                           NOM_CMP = ('TEMP', 'TEMP_INF', 'TEMP_SUP',),
                           VALE =    ( 50.,       0.    ,   100.    ,),))

TEMPLIN=CREA_RESU(OPERATION='AFFE', TYPE_RESU='EVOL_THER',
                      NOM_CHAM='TEMP',
                       AFFE=_F(  CHAM_GD = TEMP,
                                 INST = -1.)
                                 )
TEMPER=CREA_RESU(  OPERATION='PREP_VRC2', TYPE_RESU='EVOL_THER',
    PREP_VRC2= _F(  MODELE=MOD, CARA_ELEM=CARA, EVOL_THER = TEMPLIN, ),)

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                      AFFE=_F(  TOUT='OUI',
                                MATER = MAT),
                      AFFE_VARC=  _F(TOUT='OUI',EVOL=TEMPER,NOM_VARC='TEMP',
                                     NOM_CHAM='TEMP',VALE_REF=0.0),)
CH1=AFFE_CHAR_MECA(  MODELE=MOD,
                     DDL_IMPO=_F( GROUP_NO = ('AB', 'BC', 'CD', 'DA',),
                                   DX = 0.,  DY = 0.,  DZ = 0.,
                                  DRX = 0., DRY = 0., DRZ = 0.)
                    )

MESTAT=MECA_STATIQUE(      MODELE=MOD,
                            EXCIT=_F( CHARGE = CH1),
                      CHAM_MATER=CHMAT,
                       CARA_ELEM=CARA,
                          )

MESTAT=CALC_NO( reuse=MESTAT, RESULTAT=MESTAT,
                OPTION='FORC_NODA'
                   )

MESTAT=CALC_ELEM( reuse=MESTAT, RESULTAT=MESTAT,
                  OPTION='SIGM_ELNO_DEPL'
                   )

TEST_RESU(RESU=(
   # REACTIONS
           _F(  RESULTAT = MESTAT,  NOM_CHAM = 'FORC_NODA',  NUME_ORDRE = 1,
             CRITERE = 'ABSOLU',    PRECISION = 3.E-5, REFERENCE='ANALYTIQUE',
             NOEUD = 'N104',      NOM_CMP = 'DRX',         VALE = -95.2381),
           _F(  RESULTAT = MESTAT,  NOM_CHAM = 'FORC_NODA',  NUME_ORDRE = 1,
