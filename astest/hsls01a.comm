             CRITERE = 'ABSOLU',    PRECISION = 3.E-5, REFERENCE='ANALYTIQUE',
             NOEUD = 'N260',      NOM_CMP = 'DRY',         VALE = 71.4286),
           _F(  RESULTAT = MESTAT,  NOM_CHAM = 'FORC_NODA',  NUME_ORDRE = 1,
