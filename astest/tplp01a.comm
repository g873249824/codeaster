#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ------------------------
#


MATE=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0, LAMBDA = 1.0))

#
# AFFECTATIONS: MATERIAU  MODELE  CHARGEMENT ----------------------
#


ACIER=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 1.0))

