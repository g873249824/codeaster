NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))


TFIN = 0.005;

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.001,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=0.0018,
                                    NOMBRE=140,),
                                 _F(JUSQU_A=TFIN,
                                    NOMBRE=150,),
                                    ),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=145200.0,
                            NU=0.3,),
                    MONO_VISC1=_F(N=10.0,
                                  K=40.0,
                                  C=1.0,),
                    MONO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H1=1.0,
                                  H2=1.0,
                                  H3=1.0,
                                  H4=1.0,
                                  ),
                    MONO_CINE1=_F(D=36.68,),);
                    
