                   ARCHIVAGE=_F(LIST_INST=LINST),
                   EPSI_IMPOSE=_F(EPZZ=EPSZZ)
                        );

# comparaison a la modelisation A - agregat 10 grains
TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.089999999999999997,
           VALE_CALC=301.139724363,
           VALE_REFE=279.77699999999999,
           NOM_PARA='VALEUR',
           TABLE=SOLNL2,
           FILTRE=(_F(NOM_PARA='INST',
                      VALE=5.0000000000000001E-3,),
                   _F(NOM_PARA='CMP',
                      VALE_K='SIZZ',),
                   ),
           )

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.059999999999999998,
           VALE_CALC= 2.92603509E-03,
           VALE_REFE=3.0731600000000001E-3,
           NOM_PARA='VALEUR',
           TABLE=SOLNL2,
           FILTRE=(_F(NOM_PARA='INST',
                      VALE=5.0E-3,),
                   _F(NOM_PARA='CMP',
                      VALE_K='V3',),
                   ),
           )

# non regression

TEST_TABLE(
           VALE_CALC=301.13972436315,
           NOM_PARA='VALEUR',
           TABLE=SOLNL2,
           FILTRE=(_F(NOM_PARA='INST',
                      VALE=5.0E-3,),
                   _F(NOM_PARA='CMP',
                      VALE_K='SIZZ',),
                   ),
           )

TEST_TABLE(
           VALE_CALC=2.9260350907111E-3,
           NOM_PARA='VALEUR',
           TABLE=SOLNL2,
           FILTRE=(_F(NOM_PARA='INST',
                      VALE=5.0E-3,),
                   _F(NOM_PARA='CMP',
                      VALE_K='V3',),
                   ),
           )

TBEPZZ=CALC_TABLE(TABLE = SOLNL2,
               ACTION =_F(OPERATION='FILTRE',
                          NOM_PARA='CMP',CRIT_COMP='EQ',VALE_K='EPZZ'))


FEPZZ=RECU_FONCTION(TABLE=TBEPZZ,
                   PARA_X='INST',
                   PARA_Y='VALEUR',
                   INTERPOL='LIN');

TBSIZZ=CALC_TABLE(TABLE = SOLNL2,
               ACTION =_F(OPERATION='FILTRE',
                          NOM_PARA='CMP',CRIT_COMP='EQ',VALE_K='SIZZ'))


FSIZZ=RECU_FONCTION(TABLE=TBSIZZ,
                   PARA_X='INST',
                   PARA_Y='VALEUR',
                   INTERPOL='LIN');


IMPR_FONCTION(
            COURBE=(
                    _F( FONC_X=FEPZZ,
                        FONC_Y=FSIZZ),
                        ),
            );

FIN()
