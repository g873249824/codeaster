                    MONO_VISC1=_F(N=10.0,
                                  K=40.0,
                                  C=1.0,),
                    MONO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H1=1.0,
                                  H2=1.0,
                                  H3=1.0,
                                  H4=1.0,
                                  ),
                    MONO_CINE1=_F(D=36.68,),);

