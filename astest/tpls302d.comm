M=LIRE_MAILLAGE(FORMAT='MED',  )

M=DEFI_GROUP( reuse=M,   MAILLAGE=M,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(
                       MAILLAGE=M,
                      AFFE=_F( TOUT = 'OUI',PHENOMENE = 'THERMIQUE',
                                      MODELISATION = 'COQUE'))

EPSYM=AFFE_CARA_ELEM( MODELE=MO,
                           COQUE=_F(
       GROUP_MA = 'TOUT',
EPAIS = 25.4E-3) )

MASYM=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0,LAMBDA = 25.961))

CMSYM=AFFE_MATERIAU(  MAILLAGE=M, AFFE=_F( TOUT = 'OUI',MATER = MASYM))

