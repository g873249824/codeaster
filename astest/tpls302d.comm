FS=ASSE_VECTEUR(   VECT_ELEM=VESYM, NUME_DDL=NUSYM  )

KS=FACTORISER( reuse=KS,   MATR_ASSE=KS)

TEMPS=RESOUDRE(    MATR=KS,    CHAM_NO=FS )

