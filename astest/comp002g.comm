DEBUT(CODE=_F(NOM='COMP002G',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),);

#parametres vitesse de sollicitation
vitesse = 1.e-5

t_0 = 5.e-2/(8.0*vitesse)

# liste d'archivage
temps_ar=DEFI_LIST_REEL( VALE =[t_0*i for i in range(9)],)



#parmetres elastiques
YOUNG = 200000.0;
POISSON = 0.3;


#parametres viscosite
SIGM_0=6176.
EPSI_0=3.31131121483e13
M=6.76


# def des materiaux
ACIER = [None]*2
#unite en Pa
C_Pa = 1.e+6
YOUNG_Pa = YOUNG * C_Pa

SIGM_0_Pa=SIGM_0 * C_Pa

#
# COURBE DE TRACTION a LA TEMPeRATURE 100 degres
#
A7=DEFI_FONCTION(
             NOM_PARA='EPSI',
         PROL_DROITE='EXCLU',
         PROL_GAUCHE='EXCLU',
                 VALE=(
          2.185E-3       ,437.E+6       ,
          9.185E-3       ,437.E+6       ,
          0.010235       ,447.E+6       ,
          0.015385       ,477.E+6       ,
          0.020485       ,497.E+6       ,
          0.025585       ,517.E+6       ,
          0.030635       ,527.E+6       ,
          0.03571        ,542.E+6       ,
          0.04076        ,552.E+6       ,
          0.04581        ,562.E+6       ,
          0.050835       ,567.E+6       ,
          0.05587        ,574.E+6       ,
          0.06091        ,582.E+6       ,
          0.065935       ,587.E+6       ,
          0.07097        ,594.E+6       ,
          0.075995       ,599.E+6       ,
          0.081035       ,607.E+6       ,
          0.08606        ,612.E+6       ,
          0.091085       ,617.E+6       ,
          ))


#unite en MPa
A7_MPa=CALC_FONCTION(COMB=_F(FONCTION=A7,
                             COEF=1./C_Pa,),);


ACIER[0]=DEFI_MATERIAU(ELAS=_F(E=YOUNG_Pa,
                               NU=POISSON,
                               ALPHA=11.8e-6),
                    TRACTION=_F(SIGM=A7,
                                 ),
                     VISC_SINH=_F(
                                   SIGM_0 = SIGM_0_Pa,
                                   EPSI_0 = EPSI_0,
                                   M      = M),
                                 );
#unite en MPa
ACIER[1]=DEFI_MATERIAU(ELAS=_F(E=YOUNG,
                               NU=POISSON,
                               ALPHA=11.8e-6),
                     VISC_SINH=_F(
                                   SIGM_0 = SIGM_0,
                                   EPSI_0 = EPSI_0,
                                   M      = M),
                    TRACTION=_F(SIGM=A7_MPa,),
                                 )


compor='VISC_ISOT_TRAC'

tabresu=TEST_COMPOR(OPTION='MECA',

                   COMP_INCR=_F(RELATION=compor,DEFORMATION='SIMO_MIEHE'),
                   SUPPORT='ELEMENT',
              NEWTON=_F(REAC_ITER=1),
              LIST_MATER=ACIER,
              LIST_NPAS=4*[1] + [1, 5, 25],
              VARI_TEST=('V1','VMIS','TRACE'),
              LIST_TOLE=4*[1.E-10] + [1.E-1] + 2*[1.E-2] + [1.E-5],
              YOUNG=YOUNG,POISSON=POISSON,
              )

IMPR_TABLE(TABLE=tabresu)


FIN()
