def pres(x, y):
   th=math.atan2( y - yc, x - xc )
   c=math.cos(th)
   s=math.sin(th)

   n=np.array( ( c, s) )
   t=np.array( (-s, c) )

   p=n[1]*syy*n[1]

   return p

def cisa(x, y):
   th=math.atan2( y - yc, x - xc )
   c=math.cos(th)
   s=math.sin(th)

#   n=( c, s)
#   t=(-s, c)

   n=np.array( ( c, s) )
   t=np.array( (-n[1], n[0]) )


   tau=-t[1]*syy*n[1]

   return tau

