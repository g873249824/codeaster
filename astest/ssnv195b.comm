PRE_GIBI();

MAILLE=LIRE_MAILLAGE(INFO=1,);

MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'H',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,50.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'B',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,0.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'M',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,25.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),   
                            )    
           );


MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'face_haut',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'H',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_millieu',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'M',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_bas',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'B',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
#                              _F(GROUP_MA='VOL',),                            
                            )    
           );


MAILLAGE=CREA_MAILLAGE(MAILLAGE=MAILLE,
                       );

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),   
                    );

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAGE,
                       LINE_QUAD=_F(GROUP_MA='VOL',),
                  INFO=1,);
                       
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                   AFFE=_F(GROUP_MA=('VOL',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D_XFEM',),
                     );        



LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-12.5');
LT1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-10. '); 
               

LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-37.5');
LT2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-10. ');
     

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(
                                 FONC_LT=LT1,
                                 FONC_LN=LN1,               
                                 ),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI= (2.5 ,0. ,12.5 ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(2.5,2.5,12.5 ),),
                    
                    INFO=1,
                    );

                    
FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(
                                 FONC_LT=LT2,
                                 FONC_LN=LN2,               
                                 ),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI= (2.5 ,0. ,37.5 ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(2.5,2.5,37.5 ),),
                    INFO=1,
                    );
                    

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='OUI',
                        );
                    
CTXFEM = AFFE_CHAR_MECA( CONTACT=(_F(METHODE='XFEM',
                                     INTEGRATION='GAUSS',
                                     COEF_REGU_CONT=100., 
                                     ITER_CONT_MAXI=40,   
                                     CONTACT_INIT='OUI',    
                                     COEF_ECHELLE=1.,
                                     FROTTEMENT='SANS',
                                     FISS_MAIT = FISS1,
                                    ),
                                  _F(METHODE='XFEM',
                                     INTEGRATION='GAUSS',
                                     COEF_REGU_CONT=100., 
                                     ITER_CONT_MAXI=40,   
                                     CONTACT_INIT='OUI',    
                                     COEF_ECHELLE=1.,
                                     FROTTEMENT='SANS',
                                     FISS_MAIT = FISS2,
                                    ),  
                                ),
                                MODELE=MODELEK,
                          ),
                          
                          
                          
                                                                



E=100.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL',),
                                MATER=ACIER,
                                TEMP_REF=0.0,),
                             ),);
ZERO=0.0
DEPX=1.E-3
DEPY=2.E-3
DEPZ=3.E-3

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(GROUP_NO='face_millieu',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(GROUP_NO='face_haut',
                                    DX=ZERO,
                                    DY=ZERO,   
                                    DZ=-DEPZ,),
                                 _F(GROUP_NO='face_bas',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=DEPZ,
                                    ),
                                 ),
                       INFO=1,);

        
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      CONTACT_XFEM=CTXFEM,
                      LIAISON_XFEM='OUI',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CH1,), 
                          _F(CHARGE=CTXFEM,),
                          _F(CHARGE=CHXFEM,),),                  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=250),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   );




                                
LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                            NOEUD=( 'NS55','NS56','NS57','NS58','NS63',
                         'NS64','NS68','NS69','NS73','NS160',
                         'NS161','NS162','NS163','NS168',
                         'NS169','NS173','NS174','NS178',),
                                RESULTAT=UTOT1,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2',),
                                OPERATION='EXTRACTION',),);
IMPR_TABLE(TABLE=LAG,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON',);

LAREF=-1.2E4
LA1REF=0.
LA2REF=0.

           
IMPR_RESU(
          MODELE = MODELEK,
          FORMAT = 'RESULTAT',
          RESU = (_F(
                     MAILLAGE = MAILLAG2,
                     RESULTAT = UTOT1, 
                     NOM_CHAM = ('DEPL',
                                 ) ,
           ),),); 

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MAX',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MAX',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MIN',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MAX',
           VALE=LA2REF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MIN',
           VALE=LA2REF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);           

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAGE,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAGE,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'),
                        INFO          = 2)
 
