DEBUT(CODE=_F(NOM='SSNV195B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

MAILLE=LIRE_MAILLAGE(FORMAT='MED',INFO=1);

MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'H',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,50.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'B',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,0.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'M',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,25.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                            )
           );


MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'face_hau',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'H',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_mil',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'M',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_bas',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'B',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
#                              _F(GROUP_MA='VOL'),
                            )
           );


MAILLAGE= COPIER(CONCEPT= MAILLE)

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL'),
                    );

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=_F(GROUP_MA=('VOL'),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D'),
                     );



LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-12.5');


LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-37.5');


FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN1));


FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN2));


MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='P1P1',
                        );


CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      ITER_CONT_MAXI = 40,
                      REAC_GEOM      = 'SANS',
                      ZONE=(
                            _F(
                               FISS_MAIT    = FISS1,
                               INTEGRATION  = 'GAUSS',
                               CONTACT_INIT = 'OUI',
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,
                             ),
                            _F(
                               FISS_MAIT    = FISS2,
                               INTEGRATION  = 'GAUSS',
                               CONTACT_INIT = 'OUI',
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,
                             ),
                         ),
                   );




E=100.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL'),
                                MATER=ACIER,
                                ),
                             ));
ZERO=0.0
DEPX=1.E-3
DEPY=2.E-3
DEPZ=3.E-3

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(GROUP_NO='face_mil',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(GROUP_NO='face_hau',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=-DEPZ),
                                 _F(GROUP_NO='face_bas',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=DEPZ,
                                    ),
                                 ),
                       INFO=1);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CH1),),
                   CONTACT  = CTXFEM,
                   COMPORTEMENT=_F(RELATION='ELAS',
                                GROUP_MA='VOL'),
                   INCREMENT=_F(LIST_INST=L_INST),
                   SOLVEUR=_F(METHODE='MUMPS',
                               ),
                   NEWTON=_F(REAC_ITER=1),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                   );


# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D'))

RES_XFEM=POST_CHAM_XFEM(
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,

                        INFO          = 2)

#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));


# -------------------------------------------------------------
#            TEST_TABLE
# -------------------------------------------------------------


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=LAG,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           IMPR_FONCTION='NON');

LAREF=-1.2E4
LA1REF=0.
LA2REF=0.


TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999998E-13,
           VALE_CALC=-1.200000000000E+04,
           VALE_REFE=-1.2000E4,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MAX',
           TABLE=LAG,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999998E-13,
           VALE_CALC=-1.200000000000E+04,
           VALE_REFE=-1.2000E4,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           TABLE=LAG,)

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999998E-13,
           VALE_CALC= 0.000000000000E+00,
           VALE_REFE=0.0,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MAX',
           TABLE=LAG,)

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999998E-13,
           VALE_CALC= 0.000000000000E+00,
           VALE_REFE=0.0,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MIN',
           TABLE=LAG,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999998E-13,
           VALE_CALC= 0.000000000000E+00,
           VALE_REFE=0.0,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MAX',
           TABLE=LAG,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999998E-13,
           VALE_CALC= 0.000000000000E+00,
           VALE_REFE=0.0,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MIN',
           TABLE=LAG,)

FIN();
