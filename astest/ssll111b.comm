            COOR_AXE_POUTRE = (0., 1.38889E-02,)),
        _F( GROUP_FIBRE='SEC2',
            MAILLAGE_SECT= MASEC1 , TOUT_SECT = 'OUI',
            COOR_AXE_POUTRE = (0., 1.38889E-02,)),
