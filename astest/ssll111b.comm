    GEOM_FIBRE=GF,MATER_SECT=BETON,
    MULTIFIBRE=(
        _F(GROUP_FIBRE='SEC1',MATER=BETON),
        _F(GROUP_FIBRE='SEC2',MATER=BETON),
        _F(GROUP_FIBRE='SACI',MATER=ACIER),
