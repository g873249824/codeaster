#          SOUS CHARGEMENT DOUBLEMENT SINUSOIDAL
#---------------------------------------------------------------------
#         - MODELISATION AVEC DES ELEMENTS 'DST'(QUAD4)
#---------------------------------------------------------------------
#
DEBUT(CODE=_F(NOM='SSLS504A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
#
PRE_GIBI ();
#
MA=LIRE_MAILLAGE();
#
#
#
MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='DST',),);
#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
#
#
CAR_ELE=AFFE_CARA_ELEM(MODELE=MO,
                       COQUE=( _F( GROUP_MA='TOUT_ELT',
                                   EPAIS=0.1,
                                   COQUE_NCOU=3,
                                   ANGL_REP=(0.0,0.0),),
                               _F( GROUP_MA='COTE_AB',EPAIS=0.1,),
                               _F( GROUP_MA='COTE_BC',EPAIS=0.1,),
                               _F( GROUP_MA='COTE_CD',EPAIS=0.1,),
                               _F( GROUP_MA='COTE_DA',EPAIS=0.1,),),)
#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
#
#
MAT=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=25.0,
                               E_T=1.0,
                               NU_LT=0.25,
                               G_LT=0.5,
                               G_LN=0.5,
                               G_TN=0.20000000000000001,),);
#
#
#
MULTI=DEFI_COQU_MULT(COUCHE=(_F(EPAIS=0.025000000000000001,
                                MATER=MAT,
                                ORIENTATION=0.0,),
                             _F(EPAIS=0.050000000000000003,
                                MATER=MAT,
                                ORIENTATION=90.0,),
                             _F(EPAIS=0.025000000000000001,
                                MATER=MAT,
                                ORIENTATION=0.0,),),);
#
#
#
CHAM_MAT=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MULTI,),);
#
#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------
#
#
#
CHA=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_MA='COTE_AB',
                                DZ=0.0,
                                DRY=0.0,),
                             _F(GROUP_MA='COTE_BC',
                                DX=0.0,
                                DRY=0.0,),
                             _F(GROUP_MA='COTE_CD',
                                DY=0.0,
                                DRX=0.0,),
                             _F(GROUP_MA='COTE_DA',
                                DZ=0.0,
                                DRX=0.0,),),);
PZERO = 0.01;

L = 1;


PRES = FORMULE(NOM_PARA=('X','Y'),VALE='( PZERO*sin(pi*X/L)*sin(pi*Y/L) )');

CHAOLD=AFFE_CHAR_MECA_F(MODELE=MO,
                        FORCE_COQUE=_F(GROUP_MA='TOUT_ELT',
                                       PRES=PRES,),);

#
#
#
RESU=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHAM_MAT,
                   CARA_ELEM=CAR_ELE,
                   EXCIT=(_F(CHARGE=CHA,),
                          _F(CHARGE=CHAOLD,),),);
#
# EXTRACTION DU CHAMP DE DEPLACEMENT
#
#


DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RESU,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);
#
#COUCHE 2 : CALCUL DES CONTRAINTES
#
#


C2_M=CALC_ELEM(OPTION='SIGM_ELNO_DEPL',
               RESULTAT=RESU,
               REPE_COQUE =_F( NUME_COUCHE=2,
                               NIVE_COUCHE='MOY',),);

C2_M=CALC_NO(reuse =C2_M,
             RESULTAT=C2_M,
             OPTION='SIGM_NOEU_DEPL',

             );

SIG_C2_M=CREA_CHAMP(TYPE_CHAM='NOEU_SIEF_R',
                    OPERATION='EXTR',
                    RESULTAT=C2_M,
                    NOM_CHAM='SIGM_NOEU_DEPL',
                    NUME_ORDRE=1,);


C2_S=CALC_ELEM(OPTION='SIGM_ELNO_DEPL',
                RESULTAT=RESU,
                REPE_COQUE =_F( NUME_COUCHE=2,
                                NIVE_COUCHE='SUP',),);

C2_S=CALC_NO(reuse =C2_S,
              RESULTAT=C2_S,
              OPTION='SIGM_NOEU_DEPL',

              )

SIG_C2_S=CREA_CHAMP(    RESULTAT=C2_S,
                    OPERATION='EXTR',
                     TYPE_CHAM='NOEU_SIEF_R',
                      NOM_CHAM='SIGM_NOEU_DEPL',
                    NUME_ORDRE=1
                  )

#COUCHE 3 : CALCUL DES CONTRAINTES
#
#


C3_S=CALC_ELEM(OPTION='SIGM_ELNO_DEPL',
               RESULTAT=RESU,
               REPE_COQUE =_F( NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',),);

C3_S=CALC_NO(reuse =C3_S,
             RESULTAT=C3_S,
             OPTION='SIGM_NOEU_DEPL',

             );

SIG_C3_S=CREA_CHAMP(TYPE_CHAM='NOEU_SIEF_R',
                    OPERATION='EXTR',
                    RESULTAT=C3_S,
                    NOM_CHAM='SIGM_NOEU_DEPL',
                    NUME_ORDRE=1,);





#---------------------------------------------------------------------
#                         VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
#
#
TEST_RESU(CHAM_NO=(_F(CHAM_GD=SIG_C2_M,
                      GROUP_NO='D',
                      NOM_CMP='SIXZ',
                      REFERENCE='SOURCE_EXTERNE',VALE=-0.030499999999999999,
                      PRECISION=0.02,),
                   _F(CHAM_GD=SIG_C2_M,
                      GROUP_NO='B',
                      NOM_CMP='SIYZ',
                      REFERENCE='SOURCE_EXTERNE',VALE=-0.020400000000000001,
                      PRECISION=0.03,),
                   _F(CHAM_GD=SIG_C3_S,
                      GROUP_NO='C',
                      NOM_CMP='SIXX',
                      REFERENCE='SOURCE_EXTERNE',VALE=-0.48199999999999998,
                      PRECISION=0.02,),
                   _F(CHAM_GD=SIG_C2_S,
                      GROUP_NO='C',
                      NOM_CMP='SIYY',
                      REFERENCE='SOURCE_EXTERNE',VALE=-0.40000000000000002,
                      PRECISION=0.04,),),
          RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  GROUP_NO='C',
                  REFERENCE='SOURCE_EXTERNE',VALE=-0.07417,
                  PRECISION=0.004,),);


FIN();
