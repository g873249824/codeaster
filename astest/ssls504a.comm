#          SOUS CHARGEMENT DOUBLEMENT SINUSOIDAL
#---------------------------------------------------------------------
#         - MODELISATION AVEC DES ELEMENTS 'DST'(QUAD4)
#---------------------------------------------------------------------
#
DEBUT(CODE=_F(NOM='SSLS504A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
#
MA=LIRE_MAILLAGE(FORMAT='MED',);
#
#
#
MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='DST',),);
#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
#
#
CAR_ELE=AFFE_CARA_ELEM(MODELE=MO,
                       COQUE=( _F( GROUP_MA='TOUT_ELT',
                                   EPAIS=0.1,
                                   COQUE_NCOU=3,
                                   ANGL_REP=(0.0,0.0),),
                               _F( GROUP_MA='COTE_AB',EPAIS=0.1,),
                               _F( GROUP_MA='COTE_BC',EPAIS=0.1,),
                               _F( GROUP_MA='COTE_CD',EPAIS=0.1,),
                               _F( GROUP_MA='COTE_DA',EPAIS=0.1,),),)
#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
#
#
MAT=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=25.0,
                               E_T=1.0,
                               NU_LT=0.25,
                               G_LT=0.5,
                               G_LN=0.5,
                               G_TN=0.20000000000000001,),);
#
#
#
MULTI=DEFI_COMPOSITE(COUCHE=(_F(EPAIS=0.025000000000000001,
                                MATER=MAT,
                                ORIENTATION=0.0,),
                             _F(EPAIS=0.050000000000000003,
                                MATER=MAT,
                                ORIENTATION=90.0,),
                             _F(EPAIS=0.025000000000000001,
                                MATER=MAT,
                                ORIENTATION=0.0,),),);
#
#
#
CHAM_MAT=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MULTI,),);
#
#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------
#
#
#
CHA=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_MA='COTE_AB',
                                DZ=0.0,
                                DRY=0.0,),
                             _F(GROUP_MA='COTE_BC',
                                DX=0.0,
                                DRY=0.0,),
                             _F(GROUP_MA='COTE_CD',
                                DY=0.0,
                                DRX=0.0,),
                             _F(GROUP_MA='COTE_DA',
                                DZ=0.0,
                                DRX=0.0,),),);
PZERO = 0.01;

L = 1;


PRES = FORMULE(NOM_PARA=('X','Y'),VALE='( PZERO*sin(pi*X/L)*sin(pi*Y/L) )');

CHAOLD=AFFE_CHAR_MECA_F(MODELE=MO,
                        FORCE_COQUE=_F(GROUP_MA='TOUT_ELT',
                                       PRES=PRES,),);

#
#
#
RESU=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHAM_MAT,
                   CARA_ELEM=CAR_ELE,
                   EXCIT=(_F(CHARGE=CHA,),
                          _F(CHARGE=CHAOLD,),),);
#
# EXTRACTION DU CHAMP DE DEPLACEMENT
#
#


DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RESU,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);
#
#COUCHE 2 : CALCUL DES CONTRAINTES
#
#


RESU=CALC_CHAMP(reuse=RESU,CONTRAINTE=('SIGM_ELNO'),RESULTAT=RESU)


C2_M=POST_CHAMP(RESULTAT=RESU,
                EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=2,
                                   NIVE_COUCHE='MOY',),);
C2_M=CALC_CHAMP(reuse =C2_M,
             RESULTAT=C2_M,
             CONTRAINTE='SIGM_NOEU',);

SIG_C2_M=CREA_CHAMP(TYPE_CHAM='NOEU_SIEF_R',
                    OPERATION='EXTR',
                    RESULTAT=C2_M,
                    NOM_CHAM='SIGM_NOEU',
                    NUME_ORDRE=1,);


C2_S=POST_CHAMP(RESULTAT=RESU,
                EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=2,
                                   NIVE_COUCHE='SUP',),);
C2_S=CALC_CHAMP(reuse =C2_S,
             RESULTAT=C2_S,
             CONTRAINTE='SIGM_NOEU',);

SIG_C2_S=CREA_CHAMP(RESULTAT=C2_S,
                    OPERATION='EXTR',
                    TYPE_CHAM='NOEU_SIEF_R',
                    NOM_CHAM='SIGM_NOEU',
                    NUME_ORDRE=1);

#COUCHE 3 : CALCUL DES CONTRAINTES
#
#

C3_S=POST_CHAMP(RESULTAT=RESU,
                EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=3,
                                   NIVE_COUCHE='SUP',),);
C3_S=CALC_CHAMP(reuse =C3_S,
             RESULTAT=C3_S,
             CONTRAINTE='SIGM_NOEU',);

SIG_C3_S=CREA_CHAMP(TYPE_CHAM='NOEU_SIEF_R',
                    OPERATION='EXTR',
                    RESULTAT=C3_S,
                    NOM_CHAM='SIGM_NOEU',
                    NUME_ORDRE=1,);

#---------------------------------------------------------------------
#                         VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
#
#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='C',
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=-0.074450065,
                  VALE_REFE=-0.07417,
                  PRECISION=4.0000000000000001E-3,),)

TEST_RESU(CHAM_NO=(_F(REFERENCE='SOURCE_EXTERNE',
                      NOM_CMP='SIXZ',
                      GROUP_NO='D',
                      PRECISION=0.02,
                      CHAM_GD=SIG_C2_M,
                      VALE_CALC=-0.029984866,
                      VALE_REFE=-0.030499999999999999,),
                   _F(REFERENCE='SOURCE_EXTERNE',
                      NOM_CMP='SIYZ',
                      GROUP_NO='B',
                      PRECISION=0.029999999999999999,
                      CHAM_GD=SIG_C2_M,
                      VALE_CALC=-0.020984635,
                      VALE_REFE=-0.020400000000000001,),
                   _F(REFERENCE='SOURCE_EXTERNE',
                      NOM_CMP='SIXX',
                      GROUP_NO='C',
                      PRECISION=0.02,
                      CHAM_GD=SIG_C3_S,
                      VALE_CALC=-0.473659260,
                      VALE_REFE=-0.48199999999999998,),
                   _F(REFERENCE='SOURCE_EXTERNE',
                      NOM_CMP='SIYY',
                      GROUP_NO='C',
                      PRECISION=0.040000000000000001,
                      CHAM_GD=SIG_C2_S,
                      VALE_CALC=-0.412040395,
                      VALE_REFE=-0.40000000000000002,),
                   ),
          )

FIN();
