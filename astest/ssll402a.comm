DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# nombre d'éléments par quart de cercle
nb_elem = 200


#
MA=LIRE_MAILLAGE(FORMAT = 'MED'  )



MA=DEFI_GROUP(   reuse=MA,   MAILLAGE=MA,
         CREA_GROUP_MA=_F(  NOM = 'TOUT',   TOUT = 'OUI'))

#
MO=AFFE_MODELE(    MAILLAGE=MA,
                         AFFE=_F( TOUT = 'OUI',
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = 'POU_D_T')
                 )

#
MATR=DEFI_MATERIAU(    ELAS=_F( E = 2.E11,
                               NU = 0.3)
                     )

#
CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,
                              AFFE=_F( TOUT = 'OUI',
                                    MATER = MATR)
                       )

#
CAREL=AFFE_CARA_ELEM(      MODELE=MO,
                             POUTRE=_F( GROUP_MA = 'TOUT',
                                     SECTION = 'CERCLE',
                                     CARA = ('R', 'EP',
                                               ),
                                     VALE = (0.01E0, 0.01E0,
                                               )),
                       )

#
CHARG1=AFFE_CHAR_MECA(      MODELE=MO,DDL_IMPO=(
                            _F( NOEUD = 'N2',
                                      DX = 0.,
                                      DY = 0.,
                                      DZ = 0.,
                                      DRX = 0.),
                            _F( NOEUD = 'N4',
                                      DY = 0.,
                                      DZ = 0.))
                        )

#
CHARG2=AFFE_CHAR_MECA(          MODELE=MO,FORCE_NODALE=(
                            _F( NOEUD = 'N1',
                                          FY = 1.E0),
                            _F( NOEUD = 'N3',
                                          FY = -1.E0))
                        )

#
MEST1=MECA_STATIQUE(        MODELE=MO,
                          CHAM_MATER=CHMAT,
                           CARA_ELEM=CAREL,EXCIT=(
                               _F( CHARGE = CHARG1),
                                     _F( CHARGE = CHARG2))
                      )

MEST1=CALC_CHAMP(reuse=MEST1,RESULTAT=MEST1,CONTRAINTE=('EFGE_ELNO','SIPM_ELNO', 'EFGE_NOEU',))

maille2 = "M%s"%nb_elem


