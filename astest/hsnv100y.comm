                   NOEUD='N3',
                   NOM_CMP='EPXX',
                   VALE_CALC=1.1006038136256E-03,
                   VALE_REFE=1.1E-3,
                   PRECISION=0.6E-3,
                   MAILLE='M30',),
                _F(NUME_ORDRE=3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='V1',
                   VALE_CALC=2.9999927062607E-04,
                   VALE_REFE=3.E-4,
                   PRECISION=5.E-5,
                   MAILLE='M30',),
                _F(NUME_ORDRE=3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-99.780516176357,
                   VALE_REFE=-100.0,
                   PRECISION=2.5E-3,
                   MAILLE='M30',),
                _F(NUME_ORDRE=5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPXX',
                   VALE_CALC=1.2757979741096E-03,
                   VALE_REFE=1.275E-3,
                   PRECISION=1.E-3,
                   MAILLE='M30',),
                _F(NUME_ORDRE=5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='V1',
                   VALE_CALC=5.2499209879509E-04,
                   VALE_REFE=5.25E-4,
                   PRECISION=2.E-5,
                   MAILLE='M30',),
                _F(NUME_ORDRE=5,
