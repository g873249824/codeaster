                                          DRX = 0.,  DRY = 0.,  DRZ = 0.))
                           )

PRES=AFFE_CHAR_MECA(   MODELE=MODEL,
                    FORCE_COQUE=_F(  TOUT = 'OUI', PRES = 1.)   )

CHF3=AFFE_CHAR_MECA(   MODELE=MODEL,
                    FORCE_COQUE=_F(  TOUT = 'OUI', F3 = -1.)   )

CHPE=AFFE_CHAR_MECA(   MODELE=MODEL,
                      PESANTEUR=_F(GRAVITE= 10.,
                                   DIRECTION=(0., 0., -1., ), ),)

CARELEM=AFFE_CARA_ELEM(   MODELE=MODEL,
                               COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.1) )

DEPR=MECA_STATIQUE(MODELE=MODEL,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CARELEM,
                   EXCIT=(_F(CHARGE=CINE,),
                          _F(CHARGE=PRES,),),);

