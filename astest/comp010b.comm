DEBUT(CODE=_F(NOM='COMP010B',NIV_PUB_WEB='INTERNET'),IMPR_MACRO='OUI',DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

TREF = 20.
T0   = TREF
Tmax = 500.

NCAL = 20

ZERO =DEFI_CONSTANTE(VALE=0.0);

# Parametres elastiques

YOUN=DEFI_FONCTION(  NOM_PARA='TEMP',
                     VALE=(T0,   100.E3,
                           Tmax, 200.E3,)  )

ALPH=DEFI_FONCTION(NOM_PARA='TEMP',VALE=(T0,   2.0E-5,
                                         Tmax, 2.0E-5,),);
# Parametre de la loi de Lemaitre

N_F=DEFI_FONCTION(NOM_PARA='TEMP',VALE=(T0,   10.8,
                                        Tmax,  8.0,),);

UNSK_F=DEFI_FONCTION(NOM_PARA='TEMP',VALE=(T0,   6.9E-4,
                                           Tmax, 4.0E-4,),);

UNSM_F=DEFI_FONCTION(NOM_PARA='TEMP',VALE=(T0,   0.102,
                                           Tmax, 0.05));

MATERI=DEFI_MATERIAU(ELAS_FO=_F(E=YOUN,
                                NU=ZERO,
                                TEMP_DEF_ALPHA=TREF,
                                ALPHA=ALPH,),
                     LEMAITRE_FO=_F(N=N_F,
                                    UN_SUR_K=UNSK_F,
                                    UN_SUR_M=UNSM_F,),
                      );


LMT2  = [None]*(NCAL)
time=0.
tfin=1.
for i in range(NCAL):
    timem = time
    time = timem + tfin/NCAL
    Ti = TREF + time/tfin * (Tmax - TREF)
    Tm = TREF + timem/tfin * (Tmax - TREF)
    LMT2[i]=DEFI_MATERIAU(ELAS =_F(E=YOUN(Ti),
                                        NU=0.,
                                        ALPHA=0.,),
                        LEMAITRE=_F(N=N_F(Ti),
                                    UN_SUR_K=UNSK_F(Ti),
                                    UN_SUR_M=UNSM_F(Ti),),
                                );


############################################################################
# MACRO pour test thermoplastique

compor='LEMAITRE'


tabresu=TEST_COMPOR(
              MATER=MATERI,COMP_INCR=_F(RELATION=compor,ITER_INTE_MAXI=100,PARM_THETA=1.0),
              LIST_MATER=LMT2,ALPHA=ALPH,YOUNG=YOUN,TEMP_INIT=TREF,TEMP_FIN=Tmax,
              NEWTON=_F(REAC_ITER=1),
              NB_VARI=2,
              VARI_TEST=('V1','V2'),
              )

IMPR_TABLE(TABLE=tabresu)


FIN()

