DEBUT(CODE=_F(NOM='SSNV147A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI()

MA=LIRE_MAILLAGE()

MA=DEFI_GROUP( reuse=MA,
               MAILLAGE=MA,
               CREA_GROUP_NO=_F(
                TOUT_GROUP_MA = 'OUI')
              )

MO=AFFE_MODELE(
                MAILLAGE=MA,
                AFFE=_F(
               GROUP_MA = ('BLOC', 'FIXE', 'TRAC',),
               PHENOMENE = 'MECANIQUE',
               MODELISATION = '3D')
             )

CALCAIRE=DEFI_MATERIAU(
                    ELAS=_F(
                E = 20000.,
                NU = 0.),
               ECRO_LINE=_F(
                SY = 6.,
                D_SIGM_EPSI = -10000.)
               )

CAL_FAIB=DEFI_MATERIAU(
                    ELAS=_F(
                E = 20000.,
                NU = 0.),
               ECRO_LINE=_F(
                SY = 5.,
                D_SIGM_EPSI = -10000.)
               )

CHMAT=AFFE_MATERIAU(
                MAILLAGE=MA,AFFE=(
                    _F(
                GROUP_MA = 'BLOC',
                MATER = CALCAIRE), _F(
                MAILLE = 'M1',
                MATER = CAL_FAIB))
              )

L_INST=DEFI_LIST_REEL(
               DEBUT=0.0,INTERVALLE=(

                _F(  JUSQU_A = 100.,       NOMBRE = 100),
                )
              )

LIAISON=AFFE_CHAR_MECA(
               MODELE=MO,

                FACE_IMPO=_F( GROUP_MA = 'FIXE',  DX = 0., DY = 0., DZ = 0.)
              )

TRACTION=AFFE_CHAR_MECA(
                      MODELE=MO,
               FORCE_FACE=_F( GROUP_MA = 'TRAC', FZ = 1.)
              )

RAMPE=DEFI_FONCTION(
              NOM_PARA='INST',
                  VALE=(0., 0.,
                        1., 5.,)
             )

REPONSE=STAT_NON_LINE(
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, FONC_MULT = RAMPE)),
                  COMP_INCR=_F(
                RELATION = 'ENDO_FRAGILE'),

                INCREMENT=_F(
                LIST_INST = L_INST,
                INST_FIN = 1.),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50),
                ARCHIVAGE = _F(ARCH_ETAT_INIT = 'OUI')
             )

REPONSE=STAT_NON_LINE( reuse=REPONSE,
                  ETAT_INIT=_F(
                EVOL_NOLI = REPONSE),
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, TYPE_CHARGE = 'FIXE_PILO')),
                  COMP_INCR=_F(
                RELATION = 'ENDO_FRAGILE'),

                INCREMENT=_F(
                LIST_INST = L_INST,),
               PILOTAGE=_F(
                TYPE = 'PRED_ELAS',
                COEF_MULT = 10.,
                ETA_PILO_MIN = 0.50),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 10)
             )


REPONSE = CALC_NO (reuse = REPONSE,
  RESULTAT = REPONSE,
  OPTION   = 'FORC_NODA'
   )

PILO = RECU_TABLE(
  CO = REPONSE,
  NOM_PARA = ('INST', 'ETA_PILOTAGE')
  )

TRAVAIL = POST_ELEM(
  RESULTAT = REPONSE,
  TRAV_EXT = _F()
  )


TEST_RESU(RESU=(  _F(
               RESULTAT = REPONSE,
               NUME_ORDRE = 11,
               GROUP_NO = 'E',
               NOM_CHAM = 'DEPL',
               NOM_CMP = 'DZ',
               VALE = 9.25E-5), _F(
               RESULTAT = REPONSE,
               NUME_ORDRE = 11,
               MAILLE = 'M1',
               POINT = 1,
               NOM_CHAM = 'SIEF_ELGA',
               NOM_CMP = 'SIZZ',
               VALE = 5.E-1), _F(
               RESULTAT = REPONSE,
               NUME_ORDRE = 11,
               MAILLE = 'M1',
               POINT = 1,
               NOM_CHAM = 'VARI_ELGA',
               NOM_CMP = 'V1',
               VALE = 0.964285714286))
             )


TEST_TABLE (
  TABLE = PILO,
  NOM_PARA = 'INST',
  FILTRE = _F(NOM_PARA = 'NUME_ORDRE',VALE_I=11),
  VALE = 11.,
  REFERENCE = 'NON_REGRESSION'
  )

TEST_TABLE (
  TABLE = PILO,
  NOM_PARA = 'ETA_PILOTAGE',
  FILTRE = _F(NOM_PARA = 'NUME_ORDRE',VALE_I=11),
  VALE = 0.5,
  REFERENCE = 'ANALYTIQUE'
  )

TEST_TABLE (
  TABLE = TRAVAIL,
  NOM_PARA = 'TRAV_ELAS',
  FILTRE = _F(NOM_PARA = 'NUME_ORDRE',VALE_I=11),
  VALE = 2.3125E-7,
  REFERENCE = 'ANALYTIQUE'
  )

TEST_TABLE (
  TABLE = TRAVAIL,
  NOM_PARA = 'TRAV_REEL',
  FILTRE = _F(NOM_PARA = 'NUME_ORDRE',VALE_I=11),
  VALE = 1.91875E-6,
  REFERENCE = 'ANALYTIQUE'
  )


FIN()
