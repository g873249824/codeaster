DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED', )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO1=AFFE_MODELE(  MAILLAGE=MA,
                     AFFE=_F(  GROUP_MA = 'FIBRE',  PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'POU_D_TG')  )

MO2=AFFE_MODELE(  MAILLAGE=MA,AFFE=(
                     _F(  GROUP_MA = 'FIBRE',  PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'POU_D_TG'),
                     _F(  GROUP_MA = 'RIGID',   PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'POU_D_T'))  )

#--------------------------------------------------------------------


CARELEM1=AFFE_CARA_ELEM(   MODELE=MO1,
                        POUTRE=_F(
       GROUP_MA = 'FIBRE',

                                 SECTION = 'GENERALE',
                                 CARA = ('A',     'IY',         'IZ',
                                        'AY',     'AZ',     'JX',      ),
