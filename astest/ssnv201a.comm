                                NOEUD=('N33','N34','N38','N37',
                                       'N41','N42','N46', 'N45',),
                                RESULTAT=UTOT1,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                NOM_CMP=('H1Z',),
                                OPERATION='EXTRACTION',),);

