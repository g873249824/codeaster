PRE_GIBI();

MAILLE=LIRE_MAILLAGE(INFO=1,);

MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'H',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,10.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'B',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,0.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                            )    
           );

MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'face_haut',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'H',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_bas',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'B',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
#                              _F(GROUP_MA='VOL',),                            
                            )    
           );

MAILLAGE=CREA_MAILLAGE(MAILLAGE=MAILLE,
                       );

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),   
                    );

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAGE,
                       LINE_QUAD=_F(GROUP_MA='VOL',),);
                       
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                   AFFE=_F(GROUP_MA=('VOL',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D_XFEM_CONT',),
                     );        


LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-5.');

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN1,),
                    GROUP_MA_ENRI='VOL',);
 
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS1,
                         CONTACT='OUI',
                         INFO=1,); 
 
CTXFEM = AFFE_CHAR_MECA( CONTACT=_F( FISS_MAIT=FISS1,
                                      METHODE = 'XFEM',
                                    INTEGRATION='GAUSS',
                               COEF_REGU_CONT=100., 
                               ITER_CONT_MAXI=40,   
                               CONTACT_INIT='NON',
                               GLISSIERE='OUI',    
                               COEF_ECHELLE=1.e6,
                               FROTTEMENT='SANS',
                                    ),
                          MODELE=MODELEK,
                          ), 
                                                                                          


E=100.0E6
nu=0.

ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL',),
                                MATER=ACIER,
                                ),
                             ),);
ZERO=0.0
DEPX=1.E-3
DEPY=1.E-3
DEPZ=1.E-3

ENCASTR=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(GROUP_NO='face_bas',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 ),
                       INFO=1,);

CHZ=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                  _F(GROUP_NO='face_haut', 
                                     DZ=DEPZ,
                                     DX=ZERO,
                                     ),
                                 ),
                       INFO=1,);

CHY=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                  _F(GROUP_NO='face_haut',
                                    DY=DEPY,   
                                     ),
                                 ),
                       INFO=1,);

VARZ=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.,2.,-1.0001,3.,1.0001),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

VARY=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,0.,2.,0.,3.,1.),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',)

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.0,
                                    NOMBRE=3,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=ENCASTR,),
                          _F(CHARGE=CHZ,FONC_MULT=VARZ),
                          _F(CHARGE=CHY,FONC_MULT=VARY),
                          _F(CHARGE=CTXFEM,),                             
                          _F(CHARGE=CHXFEM,),),                  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=8,
                                  RESI_GLOB_MAXI=1e-04,
                                  ),               
                   SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=250),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAGE,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAGE,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'),
                        INFO          = 2)

