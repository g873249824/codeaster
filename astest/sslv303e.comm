DEBUT( CODE=_F(  NOM = 'SSLV303E',NIV_PUB_WEB='INTERNET') )

MAIL=LIRE_MAILLAGE()

#
# DEFINITION DES GROUPES DE NOEUDS
MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,
                 CREA_GROUP_NO=(
                       _F(  GROUP_MA = ('FACE1', 'FACE2',)),
                       _F(  GROUP_MA = 'PLAN_SYM'))
                   )

#
# DEFINITION DU MODELE
MODELE=AFFE_MODELE(  MAILLAGE=MAIL,
                     AFFE=_F(  TOUT = 'OUI',
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = '3D_INCO')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 2.E11, NU = 0.3,
                                 ALPHA = 1.E-5,  RHO = 7800.)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                          AFFE=_F(  TOUT = 'OUI',
                                    MATER = ACIER)  )

#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
CLIM=AFFE_CHAR_MECA(    MODELE=MODELE,
                      DDL_IMPO=(
                            _F( GROUP_NO = ('FACE1', 'FACE2',),
                                      DX = 0.,  DY = 0.,  DZ = 0.),
# COND. DE SYM.
                            _F( GROUP_NO = 'PLAN_SYM',
                                      DY = 0.))
                        )

#   GRAVITE SUIVANT X : AVEC LES 2 MOTS-CLES
CHARG1=AFFE_CHAR_MECA(      MODELE=MODELE,
                         PESANTEUR= ( 4.905,  1.,  0.,  0. ),
                      )
CHARG2=AFFE_CHAR_MECA(      MODELE=MODELE,
                     FORCE_INTERNE=_F( TOUT = 'OUI',  FX = 38259.)
                      )

#
# RESOLUTION
INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,  0.0,
                          1.0,  1.0,),);

DEF1=STAT_NON_LINE( MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                        EXCIT=(_F(   CHARGE = CHARG1,
                                   FONC_MULT= RAMPE  ),
                               _F(  CHARGE = CLIM)),
                   COMP_INCR=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=INSTANTS,
                                SUBD_METHODE='UNIFORME', SUBD_PAS=4,
                                SUBD_PAS_MINI=0.001,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=INSTANTS,),);

DEF2=STAT_NON_LINE( MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                        EXCIT=(_F(   CHARGE = CHARG2,
                                   FONC_MULT= RAMPE  ),
                               _F(  CHARGE = CLIM)),
                   COMP_INCR=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=INSTANTS,
                                SUBD_METHODE='UNIFORME', SUBD_PAS=4,
                                SUBD_PAS_MINI=0.001,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=INSTANTS,),);

#
# RECUPERATION DU CHAMP DE DEPLACEMENTS
U1=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                NOM_CHAM='DEPL',  RESULTAT=DEF1 ,
                NUME_ORDRE=1
                      )

U2=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                NOM_CHAM='DEPL',  RESULTAT=DEF2 ,
                NUME_ORDRE=1
                      )

#
#
TEST_RESU(CHAM_NO=( _F(  CHAM_GD = U1,
                         NOM_CMP = 'DX',
                         NOEUD = 'N865',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 1.84404E-07,
                         PRECISION = 0.02),
                    _F(  CHAM_GD = U1,
                         NOM_CMP = 'DY',
                         NOEUD = 'N865',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 0.,
                         PRECISION = 1.E-07,
                         CRITERE = 'ABSOLU'),
                    _F(  CHAM_GD = U1,
                         NOM_CMP = 'DX',
                         NOEUD = 'N995',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 1.84404E-07,
                         PRECISION = 0.02),
                    _F(  CHAM_GD = U1,
                         NOM_CMP = 'DY',
                         NOEUD = 'N995',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 0.,
                         PRECISION = 1.E-07,
                         CRITERE = 'ABSOLU'))
          )

TEST_RESU(CHAM_NO=( _F(  CHAM_GD = U2,
                         NOM_CMP = 'DX',
                         NOEUD = 'N865',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 1.84404E-07,
                         PRECISION = 0.02),
                    _F(  CHAM_GD = U2,
                         NOM_CMP = 'DY',
                         NOEUD = 'N865',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 0.,
                         PRECISION = 1.E-07,
                         CRITERE = 'ABSOLU'),
                    _F(  CHAM_GD = U2,
                         NOM_CMP = 'DX',
                         NOEUD = 'N995',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 1.84404E-07,
                         PRECISION = 0.02),
                    _F(  CHAM_GD = U2,
                         NOM_CMP = 'DY',
                         NOEUD = 'N995',
                         REFERENCE = 'AUTRE_ASTER',
                         VALE = 0.,
                         PRECISION = 1.E-07,
                         CRITERE = 'ABSOLU'))
          )

#
FIN()
