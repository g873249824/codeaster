                      NOM_CMP='DX',
                      PRECISION=0.02,
                      CHAM_GD=U2,
                      VALE_CALC= 1.8642449333157E-07,
                      VALE_REFE=1.8440399999999999E-07,),
                   _F(NOEUD='N865',
                      CRITERE='ABSOLU',
                      REFERENCE='AUTRE_ASTER',
                      NOM_CMP='DY',
                      PRECISION=9.9999999999999995E-08,
                      CHAM_GD=U2,
                      VALE_CALC=-5.5430794563856E-24,
