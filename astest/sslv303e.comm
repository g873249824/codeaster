                      NOM_CMP='DX',
                      PRECISION=0.02,
                      CHAM_GD=U1,
                      VALE_CALC= 1.8615058526618E-07,
                      VALE_REFE=1.8440399999999999E-07,),
                   _F(NOEUD='N995',
                      CRITERE='ABSOLU',
