# CAS TEST TTLL01L (MUR INFINI EN 20 HEXA8_D )
#

DEBUT(CODE=_F( NOM = 'TTLL01L',NIV_PUB_WEB='INTERNET'))

TIMPVAR=DEFI_FONCTION(  
       NOM_RESU='TEMP',  NOM_PARA='INST',
       VALE=(0.0,       0.0,
            1.0E-3,  100.0,
            100.,    100.0,   )
      )

ACIER=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 1.0))

MAIL=LIRE_MAILLAGE( )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = ACIER, TEMP_REF = 20.0) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = '3D_DIAG',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(MODELE=MOTH,
          TEMP_IMPO=_F( GROUP_NO = 'FTIMP', TEMP = TIMPVAR)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,INTERVALLE=(
       _F(    JUSQU_A = 1.E-3, NOMBRE = 10),
       _F(    JUSQU_A = 1.E-2, NOMBRE = 9),
       _F(    JUSQU_A = 1.E-1, NOMBRE = 9),
       _F(    JUSQU_A = 1.E0,  NOMBRE = 9),
       _F(    JUSQU_A = 2.0,   NOMBRE = 10))
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           TEMP_INIT=_F( VALE = 0.0E0),
          INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH)
             )

TEMP_1=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=0.1 )

TEMP_2=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=0.2 )

TEMP_3=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=0.7 )

TEMP_4=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=2.0 )

TEST_RESU(CHAM_NO=(
   # VALEURS DE REFERENCE:
  _F( CHAM_GD = TEMP_1, NOEUD = 'N21',  NOM_CMP = 'TEMP',
                                            VALE = 65.48, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_2, NOEUD = 'N21',  NOM_CMP = 'TEMP',
                                            VALE = 75.58, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_3, NOEUD = 'N21',  NOM_CMP = 'TEMP',
                                            VALE = 93.01, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_4, NOEUD = 'N21',  NOM_CMP = 'TEMP',
                                            VALE = 99.72, PRECISION = 1.E-2),

  _F( CHAM_GD = TEMP_1, NOEUD = 'N69',  NOM_CMP = 'TEMP',
                                            VALE = 8.09, PRECISION = 2.E-2),
  _F( CHAM_GD = TEMP_2, NOEUD = 'N69',  NOM_CMP = 'TEMP',
                                            VALE = 26.37, PRECISION = 3.E-2),
  _F( CHAM_GD = TEMP_3, NOEUD = 'N69',  NOM_CMP = 'TEMP',
                                            VALE = 78.47, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_4, NOEUD = 'N69',  NOM_CMP = 'TEMP',
                                            VALE = 99.13, PRECISION = 1.E-2))
    )

