               _F( GROUP_NO = 'ENCAS',  DX = 0.0, DY = 0.0, DZ = 0.0,
                                      DRX = 0.0, DRY = 0., DRZ = 0.0),
                        _F( GROUP_NO = 'DEPIMP',  DX = 0.001)) )

