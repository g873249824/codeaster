DEBUT(CODE=_F(  NOM = 'ZZZZ347D',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM=('CALCULEL3_47'))

#

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED',    UNITE=20 , INFO=2)

#


MODELE=AFFE_MODELE(
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = 'BETON',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = '3D'),
                     _F(  GROUP_MA = 'CABLE',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE')) )

#

CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       BARRE=_F(  GROUP_MA = 'CABLE',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 2.5E-3, )) )

#

BETON=DEFI_MATERIAU(
                       ELAS=_F(  E = 4.5E+10,
                              NU = 0.),
                 BPEL_BETON=_F() )

#

ACIER=DEFI_MATERIAU(
                       ELAS=_F(  E = 1.85E+11,
                              NU = 0.),
                 BPEL_ACIER=_F(  F_PRG = 1.77E+9,
                              FROT_COURB = 3.e-2,
                              FROT_LINE = 1.e-2) )

#

CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAILLAGE,AFFE=(
                       _F(  GROUP_MA = 'BETON',
                              MATER = BETON),
                       _F(  GROUP_MA = 'CABLE',
                              MATER = ACIER)) )

#

CABLEPR=DEFI_CABLE_BP(            MODELE=MODELE,
                                  CHAM_MATER=CHMATER,
                                   CARA_ELEM=CARAELEM,
                              GROUP_MA_BETON='BETON',
                                  DEFI_CABLE=_F(
                               GROUP_MA = 'CABLE',
                               GROUP_NO_ANCRAGE = ( 'ANCR1',  'ANCR2', ),),
                               TYPE_ANCRAGE = ( 'PASSIF',  'ACTIF',  ),
                                TENSION_INIT=1.E6,
                               RECUL_ANCRAGE=0. )

#

CHARMECA=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                    DDL_IMPO=(_F(  GROUP_MA = ('FAC_ANC1',),
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.
                               ),),
                        INFO=1 )

CH2=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                    DDL_IMPO=(_F(  GROUP_MA = ('FAC_ANC1',),
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.
                               ),
                               _F(  GROUP_MA = ('FAC_ANC2',),
                               DY = -0.1,
                               )),
                RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'NON',
                               RELA_CINE = 'OUI'),
                        INFO=1 )

#

LINST=DEFI_LIST_REEL(    VALE=( 0.0,  1.0, 2. ) )

#


EVOLNOLI=CALC_PRECONT( CHAM_MATER=CHMATER,
                       MODELE=MODELE,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                      RESI_GLOB_MAXI=1.E-04,),
                         COMP_INCR=(_F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='CABLE'),
                                    _F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='BETON'),
                                   ),
                       INCREMENT=_F(LIST_INST=LINST,INST_FIN=1.),
                       CARA_ELEM=CARAELEM,
                       SOLVEUR=_F(METHODE='MUMPS',),
                       EXCIT=(
                              _F(CHARGE=CHARMECA,),
                                 ),
                       CABLE_BP=( CABLEPR ,),
                       INFO=1,);

EVOLNOLI=STAT_NON_LINE(reuse=EVOLNOLI,
                       ETAT_INIT=_F(EVOL_NOLI=EVOLNOLI),
                            MODELE=MODELE,
                        CHAM_MATER=CHMATER,
                         CARA_ELEM=CARAELEM,
                             EXCIT=_F(
                                    CHARGE = CH2),
                         COMP_INCR=(_F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='CABLE'),
                                    _F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='BETON'),
                                   ),
                         INCREMENT=_F(
                LIST_INST = LINST),
                          NEWTON=_F(REAC_ITER=1),
                           SOLVEUR=_F(
                METHODE = 'MUMPS',
                                      ),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 50,
                RESI_GLOB_RELA = 1.E-5),
                              INFO=1
              )

IMPR_RESU(
          RESU=_F(RESULTAT=EVOLNOLI,
                  NOM_CHAM=('DEPL','SIEF_ELGA'),GROUP_MA = ('BETON'),NUME_ORDRE=(2,3)))

IMPR_RESU(
          RESU=_F(RESULTAT=EVOLNOLI,
                  NOM_CHAM=('DEPL','SIEF_ELGA'),GROUP_MA = ('CABLE'),NUME_ORDRE=(2,3)))


# CABLE ACTIF ACTIF
CABLEP2=DEFI_CABLE_BP(            MODELE=MODELE,
                                  CHAM_MATER=CHMATER,
                                   CARA_ELEM=CARAELEM,
                                   #FROTTANT='OUI',
                              GROUP_MA_BETON='BETON',
                                  DEFI_CABLE=_F(
                               GROUP_MA = 'CABLE',
                               GROUP_NO_ANCRAGE = ( 'ANCR1',  'ANCR2', ),),
                               TYPE_ANCRAGE = ( 'ACTIF',  'ACTIF',  ),
                                TENSION_INIT=1.E6,
                               RECUL_ANCRAGE=0. )
#
EVOLNOL2=CALC_PRECONT( CHAM_MATER=CHMATER,
                       MODELE=MODELE,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                      RESI_GLOB_MAXI=1.E-04,),
                         COMP_INCR=(_F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='CABLE'),
                                    _F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='BETON'),
                                   ),
                       INCREMENT=_F(LIST_INST=LINST,),
                       CARA_ELEM=CARAELEM,
                       SOLVEUR=_F(METHODE='MUMPS',),
                       EXCIT=(
                              _F(CHARGE=CHARMECA,),
                                 ),
                       CABLE_BP=( CABLEP2 ,),
                       INFO=1,);


IMPR_RESU(
          RESU=_F(RESULTAT=EVOLNOL2,
                  NOM_CHAM=('DEPL',),GROUP_MA = ('BETON'),NUME_ORDRE=2))

IMPR_RESU(
          RESU=_F(RESULTAT=EVOLNOL2,
                  NOM_CHAM=('DEPL','SIEF_ELGA'),GROUP_MA = ('CABLE'),NUME_ORDRE=2))

# RECUL ANCRAGE
# CABLE ACTIF ACTIF
CABLEP3=DEFI_CABLE_BP(            MODELE=MODELE,
                                  CHAM_MATER=CHMATER,
                                   CARA_ELEM=CARAELEM,
                                   #FROTTANT='OUI',
                              GROUP_MA_BETON='BETON',
                                  DEFI_CABLE=_F(
                               GROUP_MA = 'CABLE',
                               GROUP_NO_ANCRAGE = ( 'ANCR1',  'ANCR2', ),),
                               TYPE_ANCRAGE = ( 'ACTIF',  'ACTIF',  ),
                                TENSION_INIT=1.E6,
                               RECUL_ANCRAGE=.5E-3 )
#
EVOLNOL3=CALC_PRECONT( CHAM_MATER=CHMATER,
                       MODELE=MODELE,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                      RESI_GLOB_MAXI=1.E-04,),
                         COMP_INCR=(_F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='CABLE'),
                                    _F( RELATION = 'ELAS',
                                        DEFORMATION = 'PETIT',
                                        GROUP_MA='BETON'),
                                   ),
                       INCREMENT=_F(LIST_INST=LINST,),
                       CARA_ELEM=CARAELEM,
                       SOLVEUR=_F(METHODE='MUMPS',),
                       EXCIT=(
                              _F(CHARGE=CHARMECA,),
                                 ),
                       CABLE_BP=( CABLEP3 ,),
                       INFO=1,);


IMPR_RESU(
          RESU=_F(RESULTAT=EVOLNOL3,
                  NOM_CHAM=('DEPL',),GROUP_MA = ('BETON'),NUME_ORDRE=2))

IMPR_RESU(
          RESU=_F(RESULTAT=EVOLNOL3,
                  NOM_CHAM=('DEPL','SIEF_ELGA'),GROUP_MA = ('CABLE'),NUME_ORDRE=2))




FIN()
#
