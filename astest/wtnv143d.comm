DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
              
#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------
# precision
n = 1000.0;

# dimensions de la structure
lx = 1.0;
ly = 1.0;
lz = 5.0;

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE  = MAILLAG1,
                       LINE_QUAD = _F(TOUT='OUI'),
                      );

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='VOL'),
                                   _F(GROUP_MA='Face_inf'),
                                   _F(GROUP_MA='Face_sup'),
                                  ),
                    INFO=2,
                   );

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MO=AFFE_MODELE(MAILLAGE=MAILLAG2,
               AFFE=(_F(TOUT ='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D_HM',),
              ));

LS_N=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-2.5');

FISS=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LS_N,),
                    INFO=1,
                   );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO,
                         FISSURE=FISS,
                         INFO=1,
                         DECOUPE_FACETTE='SOUS_ELEMENTS',
                        );

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
UNM3 = DEFI_CONSTANTE(VALE = 1.0E-3,);

ZERO = DEFI_CONSTANTE(VALE = 0.0,);

KINT = DEFI_CONSTANTE(VALE = 1.01937E-19,);

SOL  = DEFI_MATERIAU(ELAS      = _F(E           = 5800.0E6,
                                    NU          = 0.,
                                    RHO         = 2762.0,
                                    ALPHA       = 0.0,),
                     COMP_THM  =   'LIQU_SATU',
                     THM_INIT  = _F(PRE1        = 0.0,
                                    PORO        = 0.15,),
                     THM_DIFFU = _F(RHO         = 2500,
                                    BIOT_COEF   = 1.,
                                    PESA_X      = 0.0,
                                    PESA_Y      = 0.0,
                                    PESA_Z      = 0.0,
                                    PERM_IN     = KINT,),
                     THM_LIQU  = _F(RHO         = 1000,
                                    UN_SUR_K    = 5E-10,
                                    VISC        = UNM3,
                                    D_VISC_TEMP = ZERO,),
                    );

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                               MATER=SOL,
                              ),
                      );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                  DDL_IMPO=(_F(GROUP_NO='Face_inf',DX=0.,DY=0.,DZ=0.,),
                            _F(GROUP_NO='Face_sup',DX=0.,DY=0.,DZ=0.,),
                           ),
                  INFO=1,
                 );

HYDR=AFFE_CHAR_MECA(MODELE=MODELEK,
                    DDL_IMPO=(_F(TOUT='OUI',PRE1=0.0,),),
                    INFO=1,
                   );

HYDR2=AFFE_CHAR_MECA(MODELE=MODELEK,
                     DDL_IMPO=(_F(NOEUD=('N10','N11','N14','N15','N18','N19','N22','N23',),H1PRE1=0.0,),),
                     INFO=2,
                    );

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   PRES_REP=_F(FISSURE=FISS,PRES=10E6,),                            
                   );


FLUH=AFFE_CHAR_MECA(MODELE=MODELEK,
                    FLUX_THM_REP=_F(TOUT='OUI',FLUN_HYDR1=0.,),
                   );

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

MULT=DEFI_FONCTION(NOM_PARA='INST',
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='CONSTANT',
                   VALE=( 0.0,  0.0,
                          3.0,  1.0,)
                  )

L_INS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=3.0,NOMBRE=1,),
                    );

UTOT1=STAT_NON_LINE(MODELE      = MODELEK,
                    CHAM_MATER  = CHAMPMAT,
                    EXCIT       = (_F(CHARGE = CH   ),
                                   _F(CHARGE = HYDR ),
                                   _F(CHARGE = HYDR2 ),
                                   _F(CHARGE = CH1,  FONC_MULT = MULT ),
                                   _F(CHARGE = FLUH ),),
                    COMPORTEMENT   = _F(RELATION       = 'KIT_HM',
                                        RELATION_KIT   = ('ELAS',
                                                          'LIQU_SATU',
                                                          'HYDR_UTIL',),
                                        RESI_INTE_RELA = 1.E-6,
                                        ITER_INTE_PAS  = -10,
                                        ITER_INTE_MAXI = 100,
                                       ),
                    NEWTON      = _F(MATRICE   = 'TANGENTE',
                                     REAC_ITER = 1, 
                                    ),
                    CONVERGENCE = _F(RESI_GLOB_RELA = 1.E-6,
                                     ITER_GLOB_MAXI = 100,    
                                    ),
                    INCREMENT   = _F(LIST_INST = L_INS,),
                   )

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MODELE = MODELEK) 

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_HM',),),
                    );

#MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
#               AFFE=(_F(TOUT ='OUI',
#                       PHENOMENE='MECANIQUE',
#                       MODELISATION='3D_HM',),
#                    ),);  

RES_X1=POST_CHAM_XFEM(MODELE_VISU = MOD_VISU,
                      RESULTAT    = UTOT1,
                     );

# IMPR_RESU(FORMAT='GMSH',UNITE=82,RESU=_F(RESULTAT=RES_X1,));
IMPR_RESU(FORMAT='MED',RESU=_F(RESULTAT=RES_X1,));

# -------------------------------------------------------------   
#            TEST_TABLE
# -------------------------------------------------------------   

DEP1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                             GROUP_NO=('NFISSU',),
                             RESULTAT=RES_X1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',
                            ),
                  );

IMPR_TABLE(TABLE=DEP1,);

DEP1P = CALC_TABLE(TABLE = DEP1,
                   ACTION = (_F(OPERATION = 'FILTRE',
                                NOM_PARA  = 'NOEUD',
                                CRIT_COMP = 'REGEXP',
                                VALE_K    = 'N[^P]',
                               ),
                            ),
                  );


DEP1M = CALC_TABLE(TABLE = DEP1,
                   ACTION = (_F(OPERATION = 'FILTRE',
                                NOM_PARA  = 'NOEUD',
                                CRIT_COMP = 'REGEXP',
                                VALE_K    = 'N[^M]',
                               ),
                            ),
                  );

