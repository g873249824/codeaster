DEBUT(CODE=_F(NOM='SDLS03A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE( )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

#====================================================================
MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.1E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',      MATER = MAT) )

#====================================================================
# ---- PLAQUE DKT APPUYEE SUR LES BORDS

MO=AFFE_MODELE(      MAILLAGE=MA,
                      AFFE=_F(  TOUT = 'OUI',  PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'DKT')   )

CH=AFFE_CHAR_MECA(   MODELE=MO,
                       DDL_IMPO=_F(  GROUP_NO = 'BORD',  DZ = 0.)   )

CA=AFFE_CARA_ELEM(   MODELE=MO,
                       COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.01)  )

MACRO_MATR_ASSE(   MODELE=MO,            CARA_ELEM=CA,
                       CHARGE=CH,           CHAM_MATER=CHMAT,
                      NUME_DDL=CO("NUM"),MATR_ASSE=(
               _F(  MATRICE = CO("MATR"),  OPTION = 'RIGI_MECA'),
                         _F(  MATRICE = CO("MATM"),  OPTION = 'MASS_MECA'))
                   )

MODE=MODE_ITER_SIMULT(  MATR_A=MATR,   MATR_B=MATM,
             CALC_FREQ=_F(  FREQ = (5., 200., ),
                         OPTION = 'BANDE'),
                           VERI_MODE=_F(  SEUIL = 1.E-5)  )

# --------------------------------------------------------------------

TEST_RESU(RESU=(
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 0.5E-2,
           NUME_MODE = 4,       VALE = 35.63),
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 1.1E-2,
           NUME_MODE = 5,       VALE = 68.51),
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 0.9E-2,
           NUME_MODE = 6,       VALE = 109.62),
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 1.2E-2,
           NUME_MODE = 7,       VALE = 123.32),
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 1.8E-2,
           NUME_MODE = 8,      VALE = 142.51),
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 2.9E-2,
           NUME_MODE = 9,       VALE = 197.32)) )

# -------------------------------------------------------------------
# ---- MODE DE MEBRANE SUR APPUIS ELASTIQUES
#
MO2=AFFE_MODELE(  MAILLAGE=MA,AFFE=(
                   _F(  TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'DKT'),
                        _F(  NOEUD = ('N1', 'N11', 'N111', 'N121',),
                          PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'DIS_T'))
                 )

RIG=50. / 2.

CA2=AFFE_CARA_ELEM(   MODELE=MO2,
                          COQUE=_F(
       GROUP_MA = 'TOUT',
 EPAIS = 0.01),
                        DISCRET=_F(  GROUP_NO = 'BORD2',    CARA = 'K_T_D_N',
                                  VALE = ( RIG,   0.,     0., ))
                     )

CH2=AFFE_CHAR_MECA(  MODELE=MO2,DDL_IMPO=(
                       _F(  TOUT = 'OUI',  DZ = 0.),
                       _F(  TOUT = 'OUI',  DRX = 0., DRY = 0.),
                       _F(  TOUT = 'OUI',  DRZ = 0.),
                       _F(  GROUP_NO = 'BORD2',  DY = 0.))
                    )

MACRO_MATR_ASSE(   MODELE=MO2,            CARA_ELEM=CA2,
                       CHARGE=CH2,           CHAM_MATER=CHMAT,
                      NUME_DDL=CO("NUM2"),MATR_ASSE=(
               _F(  MATRICE = CO("MATR2"),  OPTION = 'RIGI_MECA'),
                         _F(  MATRICE = CO("MATM2"),  OPTION = 'MASS_MECA'))
                   )

MODE2=MODE_ITER_SIMULT(  MATR_A=MATR2,        MATR_B=MATM2,
              CALC_FREQ=_F(  NMAX_FREQ = 4),
               VERI_MODE=_F(  SEUIL = 1.E-5)  )

# --------------------------------------------------------------------

FREQ1=sqrt(100./117.) / (2. * pi)

# --------------------------------------------------------------------

TEST_RESU(
   RESU=_F(   RESULTAT = MODE2,        PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 0.1E-2,
           NUME_ORDRE = 1, REFERENCE = 'ANALYTIQUE', VALE = FREQ1)
          )

#====================================================================
# ---- PLAQUE DST APPUYEE SUR LES BORDS

MO3=AFFE_MODELE(      MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'DST')   )

CH3=AFFE_CHAR_MECA(   MODELE=MO3,
                        DDL_IMPO=_F(  GROUP_NO = 'BORD',  DZ = 0.)   )

CA3=AFFE_CARA_ELEM(   MODELE=MO3,
                       COQUE=_F(
       GROUP_MA = 'TOUT',
 EPAIS = 0.01)  )

MACRO_MATR_ASSE(   MODELE=MO3,           CARA_ELEM=CA3,
                       CHARGE=CH3,          CHAM_MATER=CHMAT,
                      NUME_DDL=CO("NUM3"),MATR_ASSE=(
               _F(  MATRICE = CO("MATR3"), OPTION = 'RIGI_MECA'),
                         _F(  MATRICE = CO("MATM3"), OPTION = 'MASS_MECA'))
                   )

MODE3=MODE_ITER_SIMULT(  MATR_A=MATR3,   MATR_B=MATM3,
             CALC_FREQ=_F(  FREQ = ( 80., ),
                         OPTION = 'CENTRE'),
                           VERI_MODE=_F(  SEUIL = 1.E-5)  )

# --------------------------------------------------------------------

TEST_RESU(RESU=(
   _F(   RESULTAT = MODE3, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 0.5E-2,
           NUME_ORDRE = 4,       VALE = 35.63),
   _F(   RESULTAT = MODE3, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.1E-2,
           NUME_ORDRE = 5,       VALE = 68.51),
   _F(   RESULTAT = MODE3, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.0E-2,
           NUME_ORDRE = 6,       VALE = 109.62),
   _F(   RESULTAT = MODE3, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.3E-2,
           NUME_ORDRE = 7,       VALE = 123.32),
   _F(   RESULTAT = MODE3, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.9E-2,
           NUME_ORDRE = 8,       VALE = 142.51),
   _F(   RESULTAT = MODE3, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 3.0E-2,
           NUME_ORDRE = 9,       VALE = 197.32)) )

# -------------------------------------------------------------------
# ---- MODE DE MEBRANE SUR APPUIS ELASTIQUES
#
MO4=AFFE_MODELE(  MAILLAGE=MA,AFFE=(
                   _F(  TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'DST'),
                        _F(  NOEUD = ('N1', 'N11', 'N111', 'N121',),
                          PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'DIS_T'))
                 )

CA4=AFFE_CARA_ELEM(   MODELE=MO4,
                          COQUE=_F(
       GROUP_MA = 'TOUT',
 EPAIS = 0.01),
                        DISCRET=_F(  GROUP_NO = 'BORD2',    CARA = 'K_T_D_N',
                                  VALE = ( RIG,   0.,     0., ))
                     )

CH4=AFFE_CHAR_MECA(  MODELE=MO4,DDL_IMPO=(
                       _F(  TOUT = 'OUI',  DZ = 0.),
                       _F(  TOUT = 'OUI',  DRX = 0., DRY = 0.),
                       _F(  TOUT = 'OUI',  DRZ = 0.),
                       _F(  GROUP_NO = 'BORD2',  DY = 0.))
                    )

MACRO_MATR_ASSE(   MODELE=MO4,            CARA_ELEM=CA4,
                       CHARGE=CH4,           CHAM_MATER=CHMAT,
                      NUME_DDL=CO("NUM4"),MATR_ASSE=(
               _F(  MATRICE = CO("MATR4"),  OPTION = 'RIGI_MECA'),
                         _F(  MATRICE = CO("MATM4"),  OPTION = 'MASS_MECA'))
                   )

MODE4=MODE_ITER_SIMULT(  MATR_A=MATR4,  MATR_B=MATM4,
              CALC_FREQ=_F(  NMAX_FREQ = 4),
               VERI_MODE=_F(  SEUIL = 1.E-5) )

# --------------------------------------------------------------------

TEST_RESU(
   RESU=_F(   RESULTAT = MODE4,        PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 0.6E-2,
           NUME_ORDRE = 1, REFERENCE = 'ANALYTIQUE', VALE = FREQ1)
          )

# --------------------------------------------------------------------
FIN()
#
