                        GROUP_NO='P_A_A_6',),)
                        
                       
DEPLF=RECU_FONCTION(TABLE=POST1,PARA_X='INST',PARA_Y='DY',)
DEPLF2=RECU_FONCTION(TABLE=POST12,PARA_X='INST',PARA_Y='DY',)

POST2=POST_RELEVE_T(
   ACTION=_F(INTITULE='FORCE',
