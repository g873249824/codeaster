             RESULTAT=FLEX,
             FORCE=('FORC_NODA',),)
FLEX2=CALC_CHAMP(reuse =FLEX2,
             RESULTAT=FLEX2,
             FORCE=('FORC_NODA',),)

POST1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                        INTITULE='DY',
