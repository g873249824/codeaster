# DATE   : 11/04/2013
# AUTEUR : F. LEBOUVIER
# TITRE  : FLAMBEMENT D'UNE ENVELOPPE CYLINDRIQUE SOUS PRESSION
#          EXTERNE
#---------------------------------------------------------------------
#  - CALCUL DE STABILITE D'UNE ENVELOPPE CYLINDRIQUE MINCE
#    CALCUL DES CHARGES CRITIQUES CONDUISANT AU FLAMBEMENT ELASTIQUE
#  - MODELISATION AVEC DES ELEMENTS 'DKT'(TRIA3)
#---------------------------------------------------------------------

DEBUT(CODE=_F(
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))
#
MA=LIRE_MAILLAGE(FORMAT='MED',);

#

MAIL=CREA_MAILLAGE(MAILLAGE=MA,
                     QUAD_LINE=_F(TOUT='OUI',),);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(NOEUD='NO7',
                                 NOM='A',),);

#

MOEL=AFFE_MODELE(MAILLAGE=MAIL,
                 AFFE=(_F(GROUP_MA='TOUT_ELT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='DKT',),
                       _F(GROUP_MA='ARETE',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='3D',),),);

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------

CAEL=AFFE_CARA_ELEM(MODELE=MOEL,
                    COQUE=_F(GROUP_MA='TOUT_ELT',
                             EPAIS=0.00045,
                             ANGL_REP=(90.,0.,),
                             A_CIS=9000.,),);

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------

MATERIAU=DEFI_MATERIAU(ELAS=_F(E=2.E+11,
                               NU=0.3,),);

#

CHAM_MAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATERIAU,),);

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------

CON_LI=AFFE_CHAR_MECA(MODELE=MOEL,
                      DDL_IMPO=(_F(GROUP_NO='O_PRIME',
                                   DX=0.,
                                   DY=0.,),
                                _F(GROUP_NO='E',
                                   DY=0.,),
                                _F(GROUP_NO='CONTOUR',
                                   DZ=0.,),),);

#

CHARGE=AFFE_CHAR_MECA(MODELE=MOEL,
                      FORCE_ARETE=_F(GROUP_MA='ARETE',
                                     FZ=-0.025,),
                      FORCE_COQUE=_F(GROUP_MA='CYLINDRE',
                                     PRES=-1.,),);

#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------

RES=MECA_STATIQUE(MODELE=MOEL,
                  CHAM_MATER=CHAM_MAT,
                  CARA_ELEM=CAEL,
                  EXCIT=(_F(CHARGE=CON_LI,),
                         _F(CHARGE=CHARGE,),),);

#

SIG=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
               OPERATION='EXTR',
               RESULTAT=RES,
               NOM_CHAM='SIEF_ELGA',
               TYPE_MAXI='MINI',
               TYPE_RESU='VALE',);

#

ASSEMBLAGE ( MODELE=MOEL,
             CHAM_MATER=CHAM_MAT,
             CARA_ELEM=CAEL,
             CHARGE = (CON_LI,),
             NUME_DDL = CO('MEL_RI_M'),
             SOLVEUR=_F(METHODE='MULT_FRONT',RENUM='METIS'),
             MATR_ASSE=(
              _F( MATRICE= CO('MAS_RI_M'), OPTION= 'RIGI_MECA'),
              _F( MATRICE= CO('MAS_RI_G'), OPTION= 'RIGI_GEOM',SIEF_ELGA=SIG),),);

#

RESULT=CALC_MODES(MATR_RIGI=MAS_RI_M,
                  VERI_MODE=_F(SEUIL=1.E-5,
                               ),
                  OPTION='PLUS_PETITE',
                  MATR_RIGI_GEOM=MAS_RI_G,
                  SOLVEUR_MODAL=_F(METHODE='SORENSEN',
                                   ),
                  TYPE_RESU='MODE_FLAMB',
                  )

TEST_RESU(RESU=_F(PARA='CHAR_CRIT',
                  NUME_MODE=10,
                  RESULTAT=RESULT,
                  VALE_CALC=-1.7055716778149E+06,
                  VALE_REFE=-1.523000E6,
                  REFERENCE='ANALYTIQUE',
                  CRITERE='RELATIF',
                  PRECISION=0.125,))

FIN();
