  LAMB_CT     = 1.81   ,
                    ),


             THM_INIT  =_F(
  TEMP      = 303.   ,
  PRE1      =  0.0   ,
  PRE2      =  0.1E6  ,
  PORO      =  0.15  ,
  PRES_VAPE    =  4.276E3 ,
                    ),
        )

#****************************************************************
# Affectation des materiaux
#****************************************************************
#
CHMAT = AFFE_MATERIAU ( MAILLAGE = CORDEC,
              AFFE = (_F( GROUP_MA  = 'JEU',
                   MATER   = MATJEU,),
                   _F( GROUP_MA  = 'ZE',
                   MATER   = MATZE,),
                   _F( GROUP_MA  = 'COX',
                   MATER   = MATCOX,),
                   ),
             );
#*************************************************************
# Affectation de l etat initial
#*************************************************************
#
PCINI = 1.E5

TPPINIT = CREA_CHAMP ( TYPE_CHAM  = 'NOEU_DEPL_R' ,
            OPERATION  = 'AFFE' ,
            MAILLAGE  = CORDEC  ,
            AFFE    = ( _F( TOUT  = 'OUI',
                     NOM_CMP = 'TEMP',
                     VALE  = 0.0,),
                    _F( GROUP_MA = 'JEU',
                      NOM_CMP  = 'PRE1',
                VALE   = 5.18E6,),
                   _F( GROUP_MA = ('ZE','COX'),
                     NOM_CMP  = 'PRE1' ,
                     VALE   = PCINI , ),
                   _F( GROUP_MA = ('ZE','COX','JEU'),
                     NOM_CMP  = 'PRE2' ,
                     VALE   = 0. , ),
                     ),
            )


#******************************************************************
# Conditions aux limites
#******************************************************************
#
CHIMP = AFFE_CHAR_MECA ( MODELE = MODELT ,
             DDL_IMPO  = (
             _F ( GROUP_MA = 'DROITE',
               PRE1   = PCINI ,),
             _F ( GROUP_MA = 'DROITE',
               PRE2   = 0.0,),
                    ),
            );

#
#***********************************************************************************************
# Flux hydraulique eau = consommation d eau appliquee au niveau de la paroi gauche du Jeu ( x 3)
# Vitesse de corrosion V1 (entre 0 et 5000 ans)
#***********************************************************************************************
#
FLUXE0 = DEFI_FONCTION ( NOM_PARA  = 'INST' ,
             VALE = (
   0.     ,  0.0   ,
   8.6400E+04 ,  2.13E-10 , # au bout de 1 jour
   3.1536E+09 ,  2.13E-10 , # au bout de 100 ans
   3.1536E+10 ,  2.13E-10 , # au bout de 1000 ans
   3.1536E+11 ,  2.13E-10 , # au bout de 10 000 ans
   3.1536E+12 ,  2.13E-10 ), # au bout de 100 000 ans
            PROL_DROITE ='EXCLU' ,
            PROL_GAUCHE ='EXCLU' , );
#
#***********************************************************************************************
# Flux hydraulique gaz = production de gaz appliquee au niveau de la paroi gauche du Jeu ( x 3)
#***********************************************************************************************
#
FLUXG0 = DEFI_FONCTION ( NOM_PARA  = 'INST' ,
           VALE = (
   0.     ,  0.0,
   8.6400E+04 ,  2.37E-11 , # au bout de 1 jour
   3.1536E+09 ,  2.37E-11 , # au bout de 100 ans
   3.1536E+10 ,  2.37E-11 , # au bout de 1000 ans
   3.1536E+11 ,  2.37E-11 , # au bout de 10 000 ans
   3.1536E+12 ,  2.37E-11 ), # au bout de 100 000 ans
            PROL_DROITE ='EXCLU' ,
            PROL_GAUCHE ='EXCLU' , );
#
#
#******************************************************************
# Chargements
#******************************************************************
# Consommation d eau
#*******************
CHE = AFFE_CHAR_MECA ( MODELE = MODELT ,
            FLUX_THM_REP = _F( GROUP_MA  = 'GAUCHE',
                      FLUN_HYDR1 = -1.0 ,),)
# Production de gaz
#******************
CHG = AFFE_CHAR_MECA ( MODELE = MODELT ,
            FLUX_THM_REP = _F( GROUP_MA  = 'GAUCHE' ,
                     FLUN_HYDR2 = 1.0 ,),)
#
#
#**************************************************************************************************
# Liste des instants de calculs
#*******************************
#
INST1 = DEFI_LIST_REEL ( DEBUT   = 0.0,
        INTERVALLE = (
 _F( JUSQU_A = 8640 , NOMBRE = 10 ,), # Au bout de 1/10 jour
 _F( JUSQU_A = 86400, NOMBRE = 20,), # Au bout de 1 jour
 _F( JUSQU_A = 172800 , NOMBRE = 10 ,), # Au bout de 2 jour
 _F( JUSQU_A = 432000 , NOMBRE = 10 ,), # Au bout de 5 jours
 _F( JUSQU_A = 864000 , NOMBRE = 5 ,), # Au bout de 10 jours
 _F( JUSQU_A = 3.1536E+07 , NOMBRE = 10 ,), # Au bout de 1 an
 _F( JUSQU_A = 6.3072E+07 , NOMBRE = 12 ,),# Au bout de 2 ans
_F( JUSQU_A = 9.4608E+07 , NOMBRE = 6 ,), # Au bout de 3 ans
