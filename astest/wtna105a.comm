                   ),

             THM_DIFFU  =_F(
  R_GAZ      = 8.315   ,
  RHO       = 2420.   ,
  CP        = 1100.   ,
  BIOT_COEF    = 1.    ,
  PESA_X      = 0.    ,
  PESA_Y      = 0.    ,
  PESA_Z      = 0.    ,
  PERMIN_X     = KINTCOX  ,
  PERMIN_Y     = KINTCOX  ,
  PERMIN_Z     = KINTCOX  ,
#
  VG_N      = NCOX   ,
  VG_PR      = PRCOX  ,
  VG_SR      = SRCOX  ,
  VG_SMAX     = SMAX  ,
  VG_SATUR=0.9999,
#
  FICKV_T     = FICKCOX  ,
  FICKV_S     = FVPS   ,
