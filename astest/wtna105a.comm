  D_VISC_TEMP   = ZERO   ,
                     ),

           THM_AIR_DISS  =_F(
  CP        = 1870.   ,
  COEF_HENRY    = KHENRY  ,
                    ),

             THM_DIFFU  =_F(
  R_GAZ      = 8.315   ,
  RHO       = 2420.   ,
  CP        = 1100.   ,
  BIOT_COEF    = 1.    ,
  PESA_X      = 0.    ,
  PESA_Y      = 0.    ,
  PESA_Z      = 0.    ,
  PERMIN_X     = KINTZE  ,
  PERMIN_Y     = KINTZE  ,
  PERMIN_Z     = KINTZE  ,
  VG_N      = NZE   ,
  VG_PR      = PRZE  ,
  VG_SR      = SRZE  ,
  VG_SMAX     = SMAX  ,
  VG_SATUR=0.9999,
#
  FICKV_T     = FICKZE  ,
  FICKV_S     = FVPS   ,
  FICKA_T     = FadZE   ,
  FICKA_S     = FAS,

  LAMB_T      = LAMBLIQ  ,
  LAMB_S      = LSZE   ,
  D_LB_S      = DLSZE   ,
  LAMB_CT     = 1.81   ,
                    ),


             THM_INIT  =_F(
