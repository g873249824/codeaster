  PRE2      =  0.1E6  ,
  PORO      =  0.15  ,
  PRES_VAPE    =  4.276E3 ,

                    ),

      )
#**************************************************************
# COX Caracteristiques du Callovo-Oxfordien
#***************************************************************
#
MATCOX = DEFI_MATERIAU (

             ELAS   =_F(
  E        = 4500.0E6  ,
  NU       = 0.3    ,
  RHO       = 2670.   ,
  ALPHA      = 1.E-5
                   ),

             COMP_THM  ='LIQU_AD_GAZ_VAPE' ,

             THM_LIQU  =_F(
  RHO       = 1000.   ,
  UN_SUR_K    = 0.500E-9  ,
  ALPHA      = 1.048E-4  ,
  CP       = 4180.   ,
  VISC      = VISCOLIQ  ,
