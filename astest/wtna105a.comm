#************************************************************************************************
# INJECTION DE GAZ DANS UN MILIEU DE TYPE ARGILE DECRIT PAR DES LOIS DE MUALEM /VAN-GENUCHTEN
#                                                                                               *
#     Tranche de 1 m d epaisseur - TYPE Colis C -  (zones de Jeu , de ZE et de COX)             *
#
#******************************************************************************************
#
#  Modele de calcul : - calcul de type hydraulique (AXIS_HH2S)
#           - comportement hydraulique "LIQU_AD_GAZ_VAPE"
#           - Modele Mualem-Van-Genuchten (parametres COUPLEX)                                  *
#           - conditions initiales
#            Pour le Cox et la ZE
#            (T = 30 C ; Pgz = 1 atm ; Pc = 0.1MPa )
#
#            Pour le JEU
#            (T = 30 c ; Pgz = 1 atm ; Pc = 5.18 MPa c.ad. S = 0.1)
#
#           - Injection de gaz applique        *
#            au niveau de la paroi externe du JEU (gauche)
#
#******************************************************************************************

#
DEBUT(CODE=_F(NOM='WTNA105A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))
#
#
# Lecture du maillage
#*********************
#
CORDEC = LIRE_MAILLAGE (FORMAT='MED', );

CORDEC = DEFI_GROUP ( reuse    = CORDEC,
            MAILLAGE  = CORDEC,
            CREA_GROUP_NO = (
              _F( GROUP_MA = 'HAUT',),
             _F( GROUP_MA = 'BAS',),
             _F( GROUP_MA = 'GAUCHE',),
             _F( GROUP_MA = 'DROITE',),
             _F( GROUP_MA = 'JEU',),
             _F( GROUP_MA = 'ZE',),
             _F( GROUP_MA = 'COX',),
             ),
           )

#*********************************************************
# Modele de calcul
#*********************************************************
#
MODELT = AFFE_MODELE ( MAILLAGE = CORDEC,
            AFFE   = _F( TOUT     = 'OUI'    ,
                    PHENOMENE  = 'MECANIQUE' ,
                    MODELISATION = 'AXIS_HH2S' ),
           ) ,
#*********************************************************
# Definition des caracteristiques fluides
#**********************************************************
UN  = DEFI_CONSTANTE ( VALE = 1.);
ZERO = DEFI_CONSTANTE ( VALE = 0.);
BIDON = DEFI_CONSTANTE ( VALE = 0.);
SMAX=0.999
#
VISCOLIQ = DEFI_CONSTANTE ( VALE = 1.0E-3 ) ;
VISCOGAZ = DEFI_CONSTANTE ( VALE = 1.8E-5 ) ;
#
DVISCOL = DEFI_CONSTANTE ( VALE = 0. ) ;
DVISCOG = DEFI_CONSTANTE ( VALE = 0. ) ;
#
#
#************************************#
# PERMEABILITE INTRINSEQUE
#************************************
KINTJEU = DEFI_CONSTANTE ( VALE = 1.019E-13 );
KINTZE  = DEFI_CONSTANTE ( VALE = 5.097E-18 );
KINTCOX = DEFI_CONSTANTE ( VALE = 5.097E-21 );
#
#
FICKJEU  = DEFI_CONSTANTE ( VALE = 0.0015 );
FICKZE   = DEFI_CONSTANTE ( VALE = 0.00075 );
FICKCOX  = DEFI_CONSTANTE ( VALE = 0.00075 );
###

#
# Fick melange liquide Couplex
#
FadJEU   = DEFI_CONSTANTE ( VALE = 4.47E-10 );
FadZE   = DEFI_CONSTANTE ( VALE = 2.23632E-10 );
FadCOX   = DEFI_CONSTANTE ( VALE = 2.23632E-10 );
#
KHENRY  = DEFI_CONSTANTE ( VALE = 1.252E05 );


# Fonction de saturation et sa derivee
#**************************************
#
# LI2  pour la variation du degre de saturation
# LIJEU pour la variation du degre de saturation
#


LI2 = DEFI_LIST_REEL ( DEBUT   = 0.019,
            INTERVALLE = (
             _F( JUSQU_A = 0.999 , PAS = 1.E-2 ,),
                  ),)


LIJEU = DEFI_LIST_REEL ( DEBUT   = 0.089,
             INTERVALLE = (
              _F( JUSQU_A = 0.999 , PAS = 1.E-2 ,),
                  ),)

#**********************
# PARAMETRE MUALEM- VAN-GENUCHTEN
#
PRJEU  = 4.91E3   ;
NJEU  = 1.55    ;
MJEU  = 1-1/NJEU ;
SRJEU  = 0.08    ;

PRZE  = 4.91E6  ;
NZE   = 1.50   ;
MZE   = 1-1/NZE ;
SRZE  = 0.00   ;

PRCOX  = 1.47E7  ;
NCOX  = 1.49   ;
MCOX  = 1-1/NCOX ;
SRCOX  = 0.01   ;

#
#
# Variation du coefficient de Fick "Fvp" en fonction de la saturation)
#**********************************************************************
# Pour le jeu (JEU)
#=======================
FVP_SR = FORMULE(NOM_PARA='SAT' ,VALE=''' ( 0.999-SAT)''');

FVPSR = CALC_FONC_INTERP ( FONCTION  = FVP_SR  ,
              LIST_PARA  = LIJEU  ,
              NOM_PARA  = 'SAT'   ,
              PROL_GAUCHE = 'CONSTANT' ,
              PROL_DROITE = 'CONSTANT' ,
              INFO    = 2     ,);

FA_SR = FORMULE(NOM_PARA='SAT' ,VALE=' SAT');

FASR = CALC_FONC_INTERP ( FONCTION  = FA_SR   ,
              LIST_PARA  = LIJEU  ,
              NOM_PARA  = 'SAT'   ,
              PROL_GAUCHE = 'CONSTANT' ,
              PROL_DROITE = 'CONSTANT' ,
              INFO    = 2     ,);
# Pour la ZE et COX
#==================
FVP_S = FORMULE(NOM_PARA='SAT' ,VALE=''' ( 0.999-SAT)''');

FVPS = CALC_FONC_INTERP ( FONCTION  = FVP_S   ,
              LIST_PARA  = LI2    ,
              NOM_PARA  = 'SAT'   ,
              PROL_GAUCHE = 'CONSTANT' ,
              PROL_DROITE = 'CONSTANT' ,
              INFO    = 2     ,);

FA_S = FORMULE(NOM_PARA='SAT' ,VALE=' SAT');

FAS = CALC_FONC_INTERP ( FONCTION  = FA_S   ,
              LIST_PARA  = LI2  ,
              NOM_PARA  = 'SAT'   ,
              PROL_GAUCHE = 'CONSTANT' ,
              PROL_DROITE = 'CONSTANT' ,
              INFO    = 2     ,);
#
# Conductivite thermique
#************************
#
LAMBLIQ = DEFI_CONSTANTE ( VALE = 0.6 ) ;         # LAMB_T (liquide)
#
# Pour le Jeu (JEU)
#***********************
#
LS_RB = FORMULE(NOM_PARA='SAT' ,VALE='''( 0.45*SAT)''');

LSRB = CALC_FONC_INTERP ( FONCTION  = LS_RB   ,
              LIST_PARA  = LIJEU  ,
              NOM_PARA  = 'SAT'   ,
              PROL_GAUCHE = 'CONSTANT' ,
              PROL_DROITE = 'CONSTANT' ,
              INFO    = 2     ,); # LAMB_S (Phi0 x S)

DLSRB  = DEFI_CONSTANTE ( VALE = 0.45 );        # D_LB_S (Phi0)

LCTRB  = DEFI_CONSTANTE ( VALE = 1.13 );        # LAMB_CT ((1 - PHI0) x LAMBSolide)
#============================================================================================
# Pour la ZE
#************
#
LS_ZE = FORMULE(NOM_PARA='SAT' ,VALE=''' ( 0.15*SAT)''');

LSZE = CALC_FONC_INTERP ( FONCTION  = LS_ZE   ,
              LIST_PARA  = LI2    ,
              NOM_PARA  = 'SAT'   ,
              PROL_GAUCHE = 'CONSTANT' ,
              PROL_DROITE = 'CONSTANT' ,
              INFO    = 2     ,); # LAMB_S (Phi0 x S)

DLSZE = DEFI_CONSTANTE ( VALE = 0.15 );         # D_LB_S (Phi0)

LCTZE = DEFI_CONSTANTE ( VALE = 1.81 );         # LAMB_CT ((1 - PHI0) x LAMBSolide)
#============================================================================================
# Pour le COX
#**************
#
LSCX  = FORMULE(NOM_PARA='SAT' ,VALE=''' ( 0.15*SAT)''');

LSCOX = CALC_FONC_INTERP ( FONCTION  = LSCX   ,
              LIST_PARA  = LI2    ,
              NOM_PARA  = 'SAT'   ,
              PROL_GAUCHE = 'CONSTANT' ,
              PROL_DROITE = 'CONSTANT' ,
              INFO    = 2     ,); # LAMB_S (Phi0 x S)

DLSCOX = DEFI_CONSTANTE ( VALE = 0.15 );         # D_LB_S (Phi0)

LCTCOX = DEFI_CONSTANTE ( VALE = 1.81 );        # LAMB_CT ((1 - PHI0) x LAMBSolide)
#

#****************************************************************
# JEU
#****************************************************************
MATJEU = DEFI_MATERIAU (

             ELAS   =_F(
  E        = 72.9E6 ,
  NU       = 0.3   ,
  RHO       = 2670.  ,
  ALPHA      = 1.E-5
                   ),

