#
#******************************************************************
# Simulation de la corrosion et du chauffage entre 0 et 5000 ans
# pour la vitesse de corrosion V1
#******************************************************************
#
cal1 = STAT_NON_LINE (
          MODELE    = MODELT     ,
          CHAM_MATER  = CHMAT    ,

          EXCIT    =(
          _F( CHARGE   = CHIMP ),
                 _F( CHARGE   = CHE ,
                   FONC_MULT = FLUXE0 ) ,
                 _F( CHARGE   = CHG ,
                   FONC_MULT = FLUXG0 ) ,
                      ),

          COMP_INCR  =_F( RELATION   = 'KIT_HH',
                   RELATION_KIT = ('LIQU_AD_GAZ_VAPE', 'HYDR_VGM'),
                   PARM_THETA = 1. , ),
          INCREMENT   =_F(
                   LIST_INST    = INST1   ,
