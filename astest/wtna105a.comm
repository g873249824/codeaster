                    ),

             THM_DIFFU  =_F(
  R_GAZ      = 8.315   ,
  RHO       = 1514.   ,
  CP        = 1500.   ,
  BIOT_COEF    = 1.    ,
  PESA_X      = 0.    ,
  PESA_Y      = 0.    ,
  PESA_Z      = 0.    ,
  PERMIN_X     = KINTJEU  ,
  PERMIN_Y     = KINTJEU  ,
  PERMIN_Z     = KINTJEU  ,
#
  VG_N      = NJEU   ,
  VG_PR      = PRJEU  ,
  VG_SR      = SRJEU  ,
  VG_SMAX     = SMAX  ,
  VG_SATUR=0.9999,

#
  FICKV_T     = FICKJEU ,
  FICKV_S     = FVPSR   ,
  FICKA_S     = FASR,
  FICKA_T     = FadJEU  ,

  LAMB_T      = LAMBLIQ  ,
  LAMB_S      = LSRB   ,
  D_LB_S      = DLSRB   ,
  LAMB_CT     = 1.13   ,
                    ),


             THM_INIT  =_F(
  TEMP      = 303.   ,
  PRE1      = 0.0   ,
  PRE2      = 0.1E6  ,
  PORO      = 0.3   ,
  PRES_VAPE    = 4.276E3 ,
                    ),

      )
#****************************************************************
# ZE Caracteristiques de la Zone Endommagee
#****************************************************************
#
MATZE = DEFI_MATERIAU (

             ELAS   =_F(
