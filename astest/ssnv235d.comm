                   NOM_CHAM='DEPL',
                   NOEUD='N7',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.001317280584141,
                   VALE_REFE=-0.001317280584141,
                   PRECISION=5.0E-5,
                   CRITERE='ABSOLU',
                   MAILLE='M1',),
                _F(INST=30240000,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOEUD='N7',
