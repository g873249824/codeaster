#CAS_TEST__: SDNV100G (idem SDNV100C avec compliance)
#METHODE DE CONTACT : CONTINUE (EN DEPLACEMENT)
#MODELE D'INTERFACE MULTI-NIVEAU AVEC COMPLIANCE

DEBUT(CODE=_F(
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))
MAILL=LIRE_MAILLAGE(FORMAT='MED',);

MAILL=DEFI_GROUP(reuse =MAILL,
                 MAILLAGE=MAILL,
                 CREA_GROUP_NO=(_F(GROUP_MA='VSOL',),
                                _F(GROUP_MA='POUTRE',),),);

MODE=AFFE_MODELE(MAILLAGE=MAILL,
                 
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='3D',),);

MAILL=MODI_MAILLAGE(reuse =MAILL,
                    MAILLAGE=MAILL,
                    ORIE_PEAU_3D=_F(GROUP_MA=('SSOL','FACEINF',),),
                    );

ACIER=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.0,
                            RHO=8000.0,),);

ACONTA=DEFI_MATERIAU(ELAS=_F(E=1e+16,
                             NU=0.0,
                             RHO=0.0,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL,
                 AFFE=(_F(GROUP_MA='POUTRE',
                          MATER=ACIER,),
                       _F(GROUP_MA='VSOL',
                          MATER=ACONTA,),),);

CH=AFFE_CHAR_MECA(MODELE=MODE,
                  DDL_IMPO=(_F(TOUT='OUI',
                               DX=0.0,
                               DY=0.0,),
                            _F(GROUP_NO='VSOL',
                               DX=0.0,
                               DY=0.0,
                               DZ=0.0,),),);

CH2     = DEFI_CONTACT(MODELE         = MODE,
                       FORMULATION    = 'DISCRETE',
                       RESI_ABSO      = 1e-08,
                       PRE_COND       = 'DIRICHLET',
                       ZONE =_F(
                             ALGO_CONT      = 'GCP',
                             GROUP_MA_ESCL  = 'FACEINF',
                             GROUP_MA_MAIT  = 'SSOL',
                               ),
                      );


# ************* CHAMP DE VITESSE INITIALE ***************

CHAMP1=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                  OPERATION='AFFE',
                  MAILLAGE=MAILL,
                  AFFE=(
                  _F(GROUP_NO='POUTRE',
                     NOM_CMP=('DX','DY','DZ',),
                     VALE=(0.0,0.0,-100.0,),),
                  _F(GROUP_NO='VSOL',
                     NOM_CMP=('DX','DY','DZ',),
                     VALE=(0.0,0.0,0.0,),),),);

# ************* CHAMP DE DEPLACEMENT INITIAL ************

CHAMP2=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                  OPERATION='AFFE',
                  MAILLAGE=MAILL,
                  AFFE=(
                  _F(GROUP_NO='POUTRE',
                     NOM_CMP=('DX','DY','DZ',),
                     VALE=(0.0,0.0,0.0,),),
                  _F(GROUP_NO='VSOL',
                     NOM_CMP=('DX','DY','DZ',),
                     VALE=(0.0,0.0,0.0,),),),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=
                    (0.0,1.0,0.0001,1.0,0.00010001,0.0,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);
#
DT = 2e-06;


L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=0.00014,
                                    PAS=DT,),);
#

L_CONT=DEFI_LIST_REEL(DEBUT=1e-05,
                      INTERVALLE=_F(JUSQU_A=0.0001,
                                    PAS=DT,),);
#
ALF = 0.3025;

#
DEL = 0.6;

#

U=DYNA_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=CH,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  = CH2,
                COMPORTEMENT=_F(RELATION='ELAS',),
                ETAT_INIT=_F(DEPL=CHAMP2,
                             VITE=CHAMP1,),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_INIT=0.0,),
                SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
                                FORMULATION='DEPLACEMENT',
                                BETA=ALF,GAMMA=DEL,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                SOLVEUR=_F(SYME='OUI',
                           METHODE='MULT_FRONT',),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),);

#############
############# CONTRAINTES SIEF_ELGA OBTENUES DU RESULTAT
#############


VITECONT=RECU_FONCTION(RESULTAT=U,
                       LIST_INST=L_CONT,
                       NOM_CHAM='VITE',
                       NOM_CMP='DZ',
                       NOEUD='N5',
                       TITRE='VITE',);

IMPR_FONCTION(FORMAT='TABLEAU',
              COURBE=_F(FONCTION=VITECONT,),
              UNITE=8,);

VC=CALC_FONCTION(COMB=_F(FONCTION=VITECONT,
                         COEF=12500.0,),);

MVC=CALC_FONCTION(INTEGRE=_F(FONCTION=VC,),);

EPOTPS=POST_ELEM(ENER_POT=_F(TOUT='OUI',),
                 RESULTAT=U,);

IMPR_TABLE(TABLE=EPOTPS,);

ECITPS=POST_ELEM(ENER_CIN=_F(TOUT='OUI',),
                 RESULTAT=U,);

IMPR_TABLE(TABLE=ECITPS,);

DE100=RECU_FONCTION(RESULTAT=U,
                    NOM_CHAM='DEPL',
                    NOM_CMP='DZ',
                    NOEUD='N100',
                    TITRE='DEPL',);

VI100=RECU_FONCTION(RESULTAT=U,
                    NOM_CHAM='VITE',
                    NOM_CMP='DZ',
                    NOEUD='N100',
                    TITRE='VITE',);

DE5=RECU_FONCTION(RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  NOEUD='N5',
                  TITRE='DEPL',);

VI5=RECU_FONCTION(RESULTAT=U,
                  NOM_CHAM='VITE',
                  NOM_CMP='DZ',
                  NOEUD='N5',
                  TITRE='VITE',);

TEST_RESU(RESU=_F(NUME_ORDRE=20,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOEUD='N5',
                  NOM_CMP='DZ',
                  VALE_CALC=-2.0E-03,
                  VALE_REFE=-2.E-3,
                  PRECISION=1.E-4,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=20,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOEUD='N100',
                  NOM_CMP='DZ',
                  VALE_CALC=-3.99999988E-03,
                  VALE_REFE=-4.0E-3,
                  PRECISION=1.E-4,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=40,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOEUD='N100',
                  NOM_CMP='DZ',
                  VALE_CALC=-3.99828876E-03,
                  VALE_REFE=-4.0E-3,
                  PRECISION=1.E-3,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=50,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOEUD='N5',
                  NOM_CMP='DZ',
                  VALE_CALC=-2.0E-03,
                  VALE_REFE=-2.E-3,
                  TOLE_MACHINE=1.E-5,
                  PRECISION=1.E-4,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=60,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOEUD='N100',
                  NOM_CMP='DZ',
                  VALE_CALC=-2.49558526E-07,
                  VALE_REFE=0.0,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-4,),
          )

#

FIN();
