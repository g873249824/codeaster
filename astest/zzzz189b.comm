                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOEUD='NO700',
                   NOM_CMP='DY',
                   VALE_CALC=3.3210448836600003E-05,
                   TOLE_MACHINE=5.E-5,),
                _F(INST=1.0,
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOEUD='NO700',
                   NOM_CMP='DZ',
                   VALE_CALC=1.51551235466E-05,
