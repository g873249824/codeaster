                   ORIE_PEAU_3D=_F(GROUP_MA=('HAUT',
                                             'BAS',),),
                  );

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(
                      _F( GROUP_MA = 'BAS'),
                      _F( GROUP_MA = 'HAUT'))
                     )

