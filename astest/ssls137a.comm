DEBUT(CODE=_F(  NOM = 'ssls137a',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM=('CALCULEL3_47'),)
# ------------------------------------------------------------------------
#                       Lecture du maillage
# ------------------------------------------------------------------------
MAIL=LIRE_MAILLAGE( FORMAT='MED', UNITE=20 )

MODELE=AFFE_MODELE(
                 MAILLAGE=MAIL,
                 AFFE=(
                       _F(  GROUP_MA = ('VOILE','FACE_SUP'),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = '3D'),
                       _F( GROUP_MA = ('CABLE1'),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE'),
                      ),
                  );

CARAELE=AFFE_CARA_ELEM( MODELE=MODELE,
                        BARRE=_F(SECTION='GENERALE',
                                 GROUP_MA=('CABLE1'),
                                 CARA=('A'),
                                 VALE=(0.00015),),
                       );

# ------------------------------------------------------------------------
#                       Definition des materiaux
# ------------------------------------------------------------------------
MBETON=DEFI_MATERIAU(ELAS=_F(E=4.E10,
#                             NU=0.20,
                             NU=0.0,
                             RHO=2500,),
                     BPEL_BETON=_F(),);

MCABLE=DEFI_MATERIAU(ELAS=_F(E=1.93000000000E11,
                             NU=0.0,
                             RHO=7850,),
                     BPEL_ACIER=_F(F_PRG=1.94000000000E11,),);

CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAIL,AFFE=(
                       _F(  GROUP_MA = ('VOILE'),
                              MATER = MBETON),
                       _F(GROUP_MA=('CABLE1'),
                             MATER=MCABLE),
                              ),);

# ------------------------------------------------------------------------
#                       Definition du cable de precontrainte
# ------------------------------------------------------------------------
CAB_BP1=DEFI_CABLE_BP(MODELE=MODELE,
                      CHAM_MATER=CHMATER,
                      CARA_ELEM=CARAELE,
                      GROUP_MA_BETON='VOILE',
                      DEFI_CABLE=_F(GROUP_NO_ANCRAGE=('CABLE1_A','CABLE1_B'),
                                    GROUP_MA='CABLE1',),
                      TYPE_ANCRAGE=('PASSIF','ACTIF',),
                      TENSION_INIT=3.750000E5,
                      RECUL_ANCRAGE=0.00,);

CMCAB1=AFFE_CHAR_MECA(MODELE=MODELE,
                  RELA_CINE_BP=_F(CABLE_BP=CAB_BP1,
                                   SIGM_BPEL='NON',
                                   RELA_CINE='OUI',),)

# ------------------------------------------------------------------------
#                       Conditions aux limites et chargement
# ------------------------------------------------------------------------
CHARMECA=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                      DDL_IMPO=(
                        _F(  GROUP_NO='ENCAST',
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,),
                               ),);

PRES =AFFE_CHAR_MECA(MODELE=MODELE,
                    PRES_REP=_F(PRES = 1.E5,GROUP_MA = 'FACE_SUP',),
                        INFO=1 )

FONCCB=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(1.0,    0,
                           2.0,  1.0,
                           ),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     );

# ------------------------------------------------------------------------
#                       Calculs
# ------------------------------------------------------------------------
LINST = DEFI_LIST_REEL (DEBUT = 0.0,
                        INTERVALLE =(_F(JUSQU_A= 2.0, PAS= 0.5,),),
                       );

# Chargement 1 : Mise en tension du cable de precontrainte
# ------------------------------------------------------------------------
RES1 =  CALC_PRECONT(MODELE=MODELE,
                     CHAM_MATER=CHMATER,
                     CARA_ELEM=CARAELE,
                     COMPORTEMENT=(_F( RELATION = 'ELAS',
                                    GROUP_MA='VOILE',),
                                _F( RELATION = 'ELAS',
                                    GROUP_MA = ('CABLE1'),),),
                     EXCIT   =(_F(CHARGE = CHARMECA,),),
                     CABLE_BP   =( CAB_BP1,),
                     ENERGIE = _F(),
                     INCREMENT=_F(LIST_INST = LINST,
                                  INST_FIN  = 1., ),);

# Chargement 2 : Pression
# ------------------------------------------------------------------------
RES1 = STAT_NON_LINE(reuse=RES1,
                     ETAT_INIT=_F(EVOL_NOLI=RES1),
                     MODELE=MODELE,
                     CHAM_MATER=CHMATER,
                     CARA_ELEM=CARAELE,
                     COMPORTEMENT=(_F( RELATION = 'ELAS',
                                    GROUP_MA='VOILE',),
                                _F( RELATION = 'ELAS',
                                    GROUP_MA = ('CABLE1',),),),
                     EXCIT   =(_F(CHARGE = CHARMECA),
                               _F(CHARGE = PRES,FONC_MULT=FONCCB),
                               _F(CHARGE = CMCAB1),),
                     INCREMENT=_F(LIST_INST = LINST,),);

# ------------------------------------------------------------------------
#                       Post-traitement
# ------------------------------------------------------------------------

ENERGIE=RECU_TABLE(CO=RES1,NOM_TABLE='PARA_CALC',);

TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=212.5031098818,
           NOM_PARA='TRAV_EXT',
           TABLE=ENERGIE,
           FILTRE=(_F(NOM_PARA='INST',
                      VALE=9.5E-01,),
                   ),
           )

#  Cable 1 - inst 1s
# ------------------------------------------------------------------------
TEST_RESU(RESU=_F(INST=1.0,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=RES1,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='N',
                  VALE_CALC= 3.75000000E+05,
                  VALE_REFE=3.75000E5,
                  CRITERE='RELATIF',
                  PRECISION=1.E-08,
                  MAILLE='M28',),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=RES1,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='N',
                  VALE_CALC= 3.75000000E+05,
                  VALE_REFE=3.75000E5,
                  CRITERE='RELATIF',
                  PRECISION=1.E-08,
                  MAILLE='M326',),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=RES1,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='N',
                  VALE_CALC= 3.75000000E+05,
                  VALE_REFE=3.75000E5,
                  CRITERE='RELATIF',
                  PRECISION=1.E-08,
                  MAILLE='M547',),
          )

#  Plaque - inst 2s
# ------------------------------------------------------------------------
TEST_RESU(RESU=_F(GROUP_NO='NODE_REF',
                  INST=2.0,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RES1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=-0.099961590,
                  VALE_REFE=-0.101677,
                  CRITERE='RELATIF',
                  PRECISION=0.02,),
          )

FIN();
