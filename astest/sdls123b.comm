DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# Carateristiques de la dalle

EP = 1.0;         # Epaisseur
R0 = 0.2          # Position du lit d'armature
young = 3.5E10
nu =0.2

MATER=DEFI_MATERIAU(
                     ELAS = _F(E          = young,
                               NU         = nu,
                               RHO        = 2500.,
                               AMOR_ALPHA = 6.69E-5,
                               AMOR_BETA  = 20.06),
                    );


MAILL=LIRE_MAILLAGE(FORMAT='MED')


CHMAT=AFFE_MATERIAU(  MAILLAGE=MAILL,
                          AFFE=_F(  TOUT = 'OUI',   MATER = MATER) )

MODEL=AFFE_MODELE(  MAILLAGE=MAILL,    
                        AFFE=_F( TOUT = 'OUI',   MODELISATION = 'Q4GG',
                              PHENOMENE = 'MECANIQUE') )

CINE=AFFE_CHAR_MECA(   MODELE=MODEL,DDL_IMPO=
              _F( GROUP_MA = 'Bords', DX = 0.,   DY  = 0.,   DZ = 0.,
                                     DRX = 0.,   DRY = 0.,  DRZ = 0.,))


PRES=AFFE_CHAR_MECA(   MODELE=MODEL,
                    FORCE_COQUE=_F(  TOUT = 'OUI',   PRES = 1.E5)   )


CARELEM=AFFE_CARA_ELEM( MODELE=MODEL,
                        COQUE=_F( GROUP_MA  = 'Poutre',
                                  INER_ROTA = 'OUI',
                                  EPAIS     = EP) )

INER=POST_ELEM(MASS_INER=_F(TOUT='OUI',),
               MODELE=MODEL,
               CHAM_MATER=CHMAT,
               CARA_ELEM=CARELEM,);

IMPR_TABLE(TABLE=INER)

F_PRES=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,    0.0,
                                           0.0001, 1.0,
                                           1.0,    1.0,
                           ),);

#-------------------------------------------------------------------------
#
# DYNA_NON_LINE: CALCUL EXPLICIT AVEC AMORTISSEMENT
#
#-------------------------------------------------------------------------

LIST=DEFI_LIST_REEL(DEBUT=0.0,
#                     INTERVALLE=_F(JUSQU_A=2.7E-2,
                     INTERVALLE=_F(JUSQU_A=1.E-3,
                                   PAS=1e-05,),);


DYNA_AMO = DYNA_NON_LINE( MODELE=MODEL,
                      CHAM_MATER= CHMAT,
                      CARA_ELEM = CARELEM,
                      MASS_DIAG='OUI',
                      COMPORTEMENT=_F(RELATION ='ELAS',),
                      INCREMENT=_F(LIST_INST = LIST,),
                      EXCIT=(_F(CHARGE=PRES,),
                             _F(CHARGE=CINE,),),
                      NEWTON=_F(MATRICE='TANGENTE',
                                REAC_ITER=1,
                                REAC_INCR=1,),
                      SCHEMA_TEMPS=_F(SCHEMA='DIFF_CENT',
                                FORMULATION='ACCELERATION',),
                      );

