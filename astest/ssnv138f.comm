                                     NOMBRE=9,),
                                  _F(JUSQU_A=6.,
                                     NOMBRE=12,)));

D_FONC=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,0.0,10.0,10.0),);


NONLI=STAT_NON_LINE(CHAM_MATER=CHMAT1,
                    MODELE=MO1,
                    COMPORTEMENT=_F(DEFORMATION='GROT_GDEP',
                                 RELATION='ELAS',),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=100,
                                   RESI_GLOB_RELA=1.E-06,),
                    INCREMENT=_F(LIST_INST=L_INST, INST_FIN=4.8,),
                    CARA_ELEM=CARA1,
                    NEWTON=_F(REAC_ITER=1,
                              MATRICE='TANGENTE'),
                    SOLVEUR=_F(NPREC=12,  ),
                    INFO=1,
                    EXCIT=(_F(CHARGE=CHA1,),
                           _F(CHARGE=CHA2,
                              FONC_MULT=D_FONC,),),);

NONLI=STAT_NON_LINE(reuse=NONLI,CHAM_MATER=CHMAT1,
                    MODELE=MO1,
                    ETAT_INIT=_F(EVOL_NOLI=NONLI),
                    COMPORTEMENT=_F(DEFORMATION='GROT_GDEP',
                                 RELATION='ELAS',),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=100,
