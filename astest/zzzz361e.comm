def ln(x,y,z) :
   if z > 0.7:           return  0.0
   if  z < 0.7 and z > 0.2:
          if y > 0.4:           return  0.0
          if  y < 0.4 and y > -0.2:
              if  x > 0.2:           return  0.0
              if  x < 0.2 and x  > -0.2:           return  0.3
   if  z < 0.2 and z > -0.2:
          if y > 0.8:           return  0.0
          if  y < 0.8 and y > 0.2:
              if  x > 0.2:           return  -0.5
              if  x < 0.2 and x > -0.2:           return  0.0
          if  y < 0.2 and y > -0.2:
              if  x > 0.8:           return  -0.2
              if  x < 0.8 and x  > 0.2:           return  0.1
              if  x < 0.2 and x > -0.2:           return  0.4

LN = FORMULE(NOM_PARA=('X','Y','Z'),VALE='ln(X,Y,Z)');

