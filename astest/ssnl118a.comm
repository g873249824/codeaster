                   RESULTAT=RSTAT1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=0.502871081,
                   VALE_REFE=0.50498960000000004,
                   CRITERE='ABSOLU',
                   PRECISION=7.0000000000000001E-3,),
                _F(NUME_ORDRE=2,
