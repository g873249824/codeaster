#
#         ..                      JWT                JWT
# K X + M X   = P    AVEC  P = P E        ET  X = X E
#       2
# (K - W M) X = P    AVEC  W = 0.2 RD/S   ET  P = 1 MPA
#
# MODELISATION : ELEMENTS MECANIQUE AXIS (TRIA3+QUAD4)
# MAILLAGE     : ALIBABA ==> ASTER
#
# ----------------------------------------------------------------------
# GROUPES DE NOEUDS ET MAILLES:
# A=GRNO1  B=GRNO2  C=GRNO3  D=GRNO4  E=GRNO5  F=GRNO6
# NOEUDS(AB)=GRNM11          SEG(EF)=GRMA12    SEG(AE)=GRMA13
# ----------------------------------------------------------------------
#

DEBUT(CODE=_F(NOM='SHLV100H',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',   )

MO=AFFE_MODELE(        MAILLAGE=M,
                                AFFE=_F(  TOUT = 'OUI',
                                       PHENOMENE = 'MECANIQUE',
                                       MODELISATION = 'AXIS'))

MA=DEFI_MATERIAU(          ELAS=_F(  E = 26.,
                                       NU = 0.3,
                                       RHO = 35.,
                                       ALPHA = 0.))

CM=AFFE_MATERIAU(      MAILLAGE=M,
                                AFFE=_F(  TOUT = 'OUI',
                                       MATER = MA))

CH=AFFE_CHAR_MECA(       MODELE=MO,
                            DDL_IMPO=_F(  GROUP_NO = 'GRNM11',  DY = 0.),
                           FACE_IMPO=_F(  GROUP_MA = 'GRMA12',  DY = 0.),
                            PRES_REP=_F(  GROUP_MA = 'GRMA13',  PRES = 1.))

KE=CALC_MATR_ELEM(       MODELE=MO,
                              CHARGE=CH,
                          CHAM_MATER=CM,
                              OPTION='RIGI_MECA'                      )

ME=CALC_MATR_ELEM(       MODELE=MO,
                              CHARGE=CH,
                          CHAM_MATER=CM,
                              OPTION='MASS_MECA'                      )

VE=CALC_VECT_ELEM(       CHARGE=CH,
                          CHAM_MATER=CM,
                              OPTION='CHAR_MECA'                      )

NU=NUME_DDL(          MATR_RIGI=KE                               )

KAS=ASSE_MATRICE(      MATR_ELEM=KE,
                            NUME_DDL=NU                               )

MAS=ASSE_MATRICE(      MATR_ELEM=ME,
                             NUME_DDL=NU                  )

VAS=ASSE_VECTEUR(      VECT_ELEM=VE,
                            NUME_DDL=NU                               )

KHA=COMB_MATR_ASSE( COMB_R=(      _F(  MATR_ASSE = KAS, COEF_R = 1.),
                              _F(  MATR_ASSE = MAS, COEF_R = -.04)))

KHA=FACTORISER(      reuse=KHA,   MATR_ASSE=KHA                              )

DEPLA=RESOUDRE(         MATR=KHA,
                             CHAM_NO=VAS                              )

DEP=CREA_RESU(OPERATION='AFFE',
              TYPE_RESU='EVOL_ELAS',
              NOM_CHAM='DEPL',
              AFFE=_F(CHAM_GD=DEPLA,
                      INST=0.,
                      MODELE=MO,
                      CHAM_MATER=CM),
                );


DEP=CALC_CHAMP(reuse=DEP,RESULTAT=DEP,CONTRAINTE=('SIGM_ELNO'),INST=0.)


