DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

LAMBDA=DEFI_CONSTANTE(VALE=10.0,);

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='BAS',),
                               _F(GROUP_MA='HAUT',),
                               _F(GROUP_MA='GAUCHE',),
                               _F(GROUP_MA='DROIT',),
                               _F(GROUP_MA='BO',),
                               _F(NOM='SOM_GAUC',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA='GAUCHE'),
                               _F(NOM='SOM_DROI',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA='DROIT',),
                               ),);

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='D_PLAN_THMD',),);

CHXN=CREA_CHAMP(TYPE_CHAM='NOEU_GEOM_R',
                OPERATION='EXTR',
                MAILLAGE=MAIL,
                NOM_CHAM='GEOMETRIE',);

FPRES = FORMULE(NOM_PARA=('X','Y','Z'),VALE='1.-X');

P1=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_F',
              OPERATION='AFFE',
              MAILLAGE=MAIL,
              AFFE=_F(TOUT='OUI',
                      NOM_CMP='X1',
                      VALE_F=FPRES,),);

P2=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_R',
              OPERATION='EVAL',
              CHAM_F=P1,
              CHAM_PARA=CHXN,);

CHNUL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                 OPERATION='AFFE',
                 MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         NOM_CMP=('DX','DY','PRE1','TEMP'),
                         VALE=(0.,0.,0.,0.),),);

PINIT=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                 OPERATION='ASSE',
                 MAILLAGE=MAIL,
                 ASSE=(_F(TOUT='OUI',
                          CHAM_GD=CHNUL),
                       _F(TOUT='OUI',
                          CHAM_GD=P2,
                          NOM_CMP='X1',
                          NOM_CMP_RESU='PRE1',),),);

LI=DEFI_LIST_REEL(DEBUT=0.0,
                  INTERVALLE=_F(JUSQU_A=800.0,
                                NOMBRE=20,),);

UN=DEFI_CONSTANTE(VALE=1.0,);

ZERO=DEFI_CONSTANTE(VALE=0.0,);

BIDON=DEFI_CONSTANTE(VALE=0.0,);

KINT=DEFI_CONSTANTE(VALE=100.0,);
EL = DEFI_FONCTION  ( NOM_PARA   = 'TEMP' ,
             VALE = ( 0.   ,  1.E7,
                      500.  ,1.E7, ),
                        PROL_DROITE  ='CONSTANT' ,
                        PROL_GAUCHE  ='CONSTANT' , );

NU=DEFI_CONSTANTE(VALE=0.,);

MATERIAU=DEFI_MATERIAU(
                    ELAS_FO=_F(E=EL,
                              NU=NU,
                              RHO=1.,
                              ALPHA=ZERO,
                              TEMP_DEF_ALPHA  =0.,
                              ),
                       COMP_THM = 'LIQU_SATU',
                       THM_LIQU=_F(RHO=1.0,
                                   UN_SUR_K=0.0,
                                   ALPHA=0.0,
                                   CP=1.0,
                                   VISC=UN,
                                   D_VISC_TEMP=ZERO,),
                       THM_GAZ=_F(MASS_MOL=0.02896,
                                  CP=1.0,
                                  VISC=BIDON,
                                  D_VISC_TEMP=BIDON,),
                       THM_VAPE_GAZ=_F(MASS_MOL=0.017999999999999999,
                                       CP=1.0,
                                       VISC=BIDON,
                                       D_VISC_TEMP=BIDON,),
                       THM_INIT=_F(TEMP=293.0,
                                   PRE1=1.E6,
                                   PRE2=1.E5,
                                   PORO=0.5,
                                   PRES_VAPE=2320.0,),
                       THM_DIFFU=_F(R_GAZ=8.3149999999999995,
                                    RHO=1.0,
                                    CP=1.0,
                                    BIOT_COEF=1.0,
                                    SATU_PRES=UN,
                                    D_SATU_PRES=ZERO,
                                    PESA_X=0.0,
                                    PESA_Y=0.0,
                                    PESA_Z=0.0,
                                    PERM_IN=KINT,
                                    PERM_LIQU=UN,
                                    D_PERM_LIQU_SATU=ZERO,
                                    PERM_GAZ=BIDON,
                                    D_PERM_SATU_GAZ=BIDON,
                                    D_PERM_PRES_GAZ=BIDON,
                                    LAMB_T=LAMBDA,),);

CHMAT0=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=_F(TOUT='OUI', MATER=MATERIAU,),
                     AFFE_VARC=_F(TOUT='OUI', NOM_VARC='TEMP',VALE_REF=0.,),
                     );

CHAR=AFFE_CHAR_MECA(MODELE=MODELE,
                    DDL_IMPO=(_F(TOUT='OUI',
                                 DX=0.0,
                                 DY=0.0,),
                              _F(GROUP_NO='SOM_GAUC',
                                 TEMP=0.0,
                                 PRE1=1.0,),
                              _F(GROUP_NO='SOM_DROI',
                                 TEMP=1.0,
                                 PRE1=0.0,),),);

RESU1=STAT_NON_LINE(MODELE=MODELE,
                    CHAM_MATER=CHMAT0,
                    EXCIT=_F(CHARGE=CHAR,),
                    COMPORTEMENT=_F(RELATION='KIT_THM',PARM_THETA=0.56999999999999995,
                                 RELATION_KIT=('ELAS','LIQU_SATU','HYDR_UTIL'),),
                    ETAT_INIT=_F(DEPL=PINIT,),
                    INCREMENT=_F(LIST_INST=LI,
                                 NUME_INST_FIN=10,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=10,),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=11,),
                    INFO=2,);

RESU1=CALC_CHAMP(reuse =RESU1,
                 CONTRAINTE=('SIGM_ELNO',),CRITERES=('SIEQ_ELGA',),
                RESULTAT=RESU1,);

TEST_RESU(RESU=(_F(NUME_ORDRE=10,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N604',
                   NOM_CMP='TEMP',
                   VALE_CALC=0.018268892,
                   VALE_REFE=0.018271068599999999,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=10,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N704',
                   NOM_CMP='TEMP',
                   VALE_CALC=0.049739477,
                   VALE_REFE=0.049743927,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=10,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N804',
                   NOM_CMP='TEMP',
                   VALE_CALC=0.135287951,
                   VALE_REFE=0.13529602600000001,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=10,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N904',
                   NOM_CMP='TEMP',
                   VALE_CALC=0.367839761,
                   VALE_REFE=0.36785074000000001,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=10,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='TEMP',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,
                   PRECISION=1.E-3,),
                ),
          )

FIN();
