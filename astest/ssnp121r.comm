DEBUT(CODE=_F(NOM='SSNP121R',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
PRE_GIBI();

MA1=LIRE_MAILLAGE();

MA = CREA_MAILLAGE(MAILLAGE=MA1,INFO=2,
                   PENTA15_18=_F(TOUT='OUI',),);

MA=DEFI_GROUP( reuse=MA,MAILLAGE=MA,CREA_GROUP_NO=(
_F(  GROUP_MA = 'SURESC',),
_F(  GROUP_MA = 'SURMAI',),
_F(  GROUP_MA = 'SURCHA',),
_F(  GROUP_MA = 'ENCAST',),
),
);

#---------------------------------------------------
MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(GROUP_MA=('V1','V2'),
                       PHENOMENE='MECANIQUE',
                    MODELISATION='3D',),),);
#--------------------------------------------------
MA=MODI_MAILLAGE(reuse =MA,
               MAILLAGE=MA,
           ORIE_PEAU_3D=_F(GROUP_MA='SURESC',),
                );

MA=MODI_MAILLAGE(reuse =MA,
               MAILLAGE=MA,
           ORIE_PEAU_3D=_F(GROUP_MA='SURMAI',),
                );

MA=MODI_MAILLAGE(reuse =MA,
               MAILLAGE=MA,
           ORIE_PEAU_3D=_F(GROUP_MA='SURCHA',),
               );

MA=MODI_MAILLAGE(reuse =MA,
               MAILLAGE=MA,
           ORIE_PEAU_3D=_F(GROUP_MA='ENCAST',),
           );
#--------------------------------------------------

MAT2=DEFI_MATERIAU(ELAS=_F(E = 20.0E+05,NU = 0.,));
#
MAT1=DEFI_MATERIAU(ELAS=_F( E = 20.0E+05,NU = 0.,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                        AFFE=(_F(GROUP_MA = ('V1'),
                                    MATER = MAT2,),
                              _F(GROUP_MA = ('V2'),
                                    MATER = MAT1,),),)
#--------------------------------------------------------
CHA1=AFFE_CHAR_MECA(MODELE=MO,
   DDL_IMPO=(_F(GROUP_NO = 'SURCHA',  DX = 0.0, DY = -0.00, DZ = -.10,),
             _F(GROUP_NO = 'ENCAST',  DX = 0.0, DY = -0.00, DZ = 0.0,),),);

CHA2 = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    ITER_GEOM_MAXI = 4,
                    ITER_CONT_MAXI = 30,
                    LISSAGE        = 'NON',
                    INFO           = 1,
                    ZONE =_F(
                             GROUP_MA_ESCL = 'SURESC',
                             GROUP_MA_MAIT = 'SURMAI',
                             INTEGRATION   = 'NOEUD',
                             ALGO_CONT     = 'STANDARD',
                             COEF_CONT     = 1000.,
                          ),
                   ); 

#-----------------------------------------------------------
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                 INTERVALLE=_F(JUSQU_A=1., NOMBRE=1,));

FONC=DEFI_FONCTION(NOM_PARA='INST',
                       VALE=(0., 0.0,1., 1.0,));
#-----------------------------------------------
RESU=STAT_NON_LINE (MODELE     = MO,
                    CHAM_MATER = CHMAT,
                    INCREMENT  =_F(LIST_INST=L_INST,
                               NUME_INST_FIN=1,),
                    EXCIT      =(_F(CHARGE=CHA1,
                                 FONC_MULT=FONC,),),
                    CONTACT    = CHA2,
                    COMP_ELAS  =_F(RELATION='ELAS'),
                         NEWTON=_F(REAC_ITER=1),
                    CONVERGENCE=_F(ARRET='OUI',
                          ITER_GLOB_MAXI=30,
                          RESI_GLOB_MAXI=1.E-8,),);


RESU=CALC_ELEM( reuse=RESU,
             RESULTAT=RESU,
               MODELE=MO,
           CHAM_MATER=CHMAT,
               OPTION=('SIEF_ELNO_ELGA','EQUI_ELNO_SIGM'));

RESU=CALC_NO( reuse=RESU,
           RESULTAT=RESU,
         CHAM_MATER=CHMAT,
             OPTION=('SIEF_NOEU_ELGA','EQUI_NOEU_SIGM'));

SIELNO=CREA_CHAMP(  OPERATION='EXTR',
                    TYPE_CHAM='ELNO_SIEF_R',
                     NOM_CHAM='SIEF_ELNO_ELGA',
                     RESULTAT=RESU,
                   NUME_ORDRE=1);

IMPR_RESU(MODELE=MO,FORMAT='RESULTAT',
            RESU=(_F( MAILLAGE=MA,
                      GROUP_NO=('SURESC','SURMAI'),
                      RESULTAT=RESU,
                      NOM_CHAM='DEPL',
                      NOM_CMP=('LAGS_C','DX','DY','DZ'),),),);

