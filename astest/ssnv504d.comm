# CAS_TEST : SSNV504D
# MODELE
# MAILLAGE : MODELISATION : AXIS
#             SOLIDE : TRIA3
#             CONTACT : SEG2
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
# CONTACT : METHODE CONTINUE
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))


#  CALCUL VALIDATION DU PASSAGE DES SINGULARITES GEOMETRIQUES


MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_MA=_F(NOM='TOUT',
                                   UNION=('LOPIN','FILIERE')),
                  CREA_GROUP_NO=(_F(GROUP_MA='LJA',
                                    NOM='LIM1'),
                                 _F(GROUP_MA='LIJ',
                                    NOM='LIM2'),
                                 _F(GROUP_MA='LHI',
                                    NOM='LIM3'),
                                 _F(GROUP_MA='LMN',
                                    NOM='LMN'),
                                 _F(GROUP_MA='LNK',
                                    NOM='LNK')));

#_____________________________________________________________________
#
#                    DEFINITION DU MODELE
#_____________________________________________________________________
#

MO=AFFE_MODELE(MAILLAGE=MAILL1,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS'));

#
# REORIENTATION DU MAILLAGE
#

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_2D=_F(GROUP_MA=('SMAI1','SESC1')));

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_2D=_F(GROUP_MA=('SMAI2','SESC2')));

#____________________________________________________________________
#
#                    DEFINITION DES MATERIAUX
#_____________________________________________________________________
#

MLOPIN=DEFI_MATERIAU(ELAS=_F(E=5000.0,
                             NU=0.45000000000000001));

MFILIERE=DEFI_MATERIAU(ELAS=_F(E=2.E5,
                               NU=0.29999999999999999));

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=(_F(GROUP_MA='LOPIN',
                          MATER=MLOPIN),
                       _F(GROUP_MA='FILIERE',
                          MATER=MFILIERE)));

#_____________________________________________________________________
#
#                    AFFECTATION DES CONDITIONS AUX LIMITES
#                       ET DU DEPLACEMENT IMPOSE
#_____________________________________________________________________
#

CH1=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_NO='LIM1',
                                DX=0.0, DY=0.0),
                             _F(GROUP_NO='LIM2',
                                DX=0.0, DY=0.0),
                             _F(GROUP_NO='LIM3',
                                DX=0.0, DY=0.0),
                             _F(GROUP_NO='LNK',
                                DX=0.0)));

CH0=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=_F(GROUP_NO='LMN', DY=1.0));


CH2  = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    ALGO_RESO_CONT = 'POINT_FIXE',
                    ALGO_RESO_GEOM = 'POINT_FIXE',
                    LISSAGE        = 'OUI',
                    ZONE =(
                           _F(
                             GROUP_MA_MAIT = 'SMAI1',
                             GROUP_MA_ESCL = 'SESC1',
                             ),
                           _F(
                             GROUP_MA_ESCL = 'SMAI2',
                             GROUP_MA_MAIT = 'SESC2',
                             ),
                          ),
                   );


#_____________________________________________________________________
#
#                    DEFINITIONS DES FONCTIONS TEMPORELLES
#_____________________________________________________________________
#

DPLY=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.0),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE');

# CONTACT RASANT: ON VA DIRECTEMENT A 10 ET NON A 5

LTPS=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=10.0,
                                     PAS=10.0,),
                                  _F(JUSQU_A=20.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=50.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=70.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=140.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=155.0,
                                     PAS=5.0,),
                                    ),);


L_INST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                  LIST_INST=LTPS,),
                     ECHEC=_F(SUBD_PAS=4,
                              SUBD_NIVEAU=5,),);

U=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=CH0,
                          FONC_MULT=DPLY),
                       _F(CHARGE=CH1)),
                CONTACT  =CH2,
                COMPORTEMENT=_F(RELATION='ELAS',
                             TOUT='OUI'),
                INCREMENT=_F(LIST_INST=L_INST),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1),
                CONVERGENCE=_F(
                ITER_GLOB_MAXI=25,
                RESI_GLOB_MAXI=9.9999999999999995E-07),
                SOLVEUR=_F(METHODE='MUMPS',),);

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='K',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=10.192587611,
                   VALE_REFE=10.192587610759,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=1,
                   PARA='ITER_GLOB',
                   VALE_CALC_I=1,
                   RESULTAT=U,
                   CRITERE='ABSOLU',
                   ),
                _F(GROUP_NO='K',
                   INST=155.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=155.074367112,
                   VALE_REFE=155.096,
                   PRECISION=1.E-3,),
                _F(PARA='ITER_GLOB',
                   VALE_CALC_I=1,
                   INST=155.0,
                   RESULTAT=U,
                   CRITERE='ABSOLU',
                   ),
                ),
          )

TEST_RESU(RESU=_F(INST=155.0,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U,
                  NOM_CHAM='CONT_NOEU',
                  NOEUD='N150',
                  NOM_CMP='CONT',
                  VALE_CALC=2.000000000,
                  VALE_REFE=2,
                  CRITERE='ABSOLU',
                  PRECISION=9.9999999999999995E-07,),
          )

FIN();
#
