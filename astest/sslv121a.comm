F1=DEFI_CONSTANTE( VALE=5.0E+11)

F2=DEFI_CONSTANTE( VALE=2.0E+11)

F3=DEFI_CONSTANTE( VALE=0.1)

F4=DEFI_CONSTANTE( VALE=0.3)

F5=DEFI_CONSTANTE( VALE=76923.1E+6)

MAT=DEFI_MATERIAU( ELAS_ISTR_FO=_F(  E_L = F1,
                                 E_N = F2,
                               NU_LT = F3,
                                NU_LN = F4,
 
  G_LN = F5,RHO = 7800.)  )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

# ------------------------------------------------------------------
#    --- STRUCTURE SUSPENDUE //Z EN A + PAS DE ROTATION

CH=AFFE_CHAR_MECA(  MODELE=MO,
       PESANTEUR=(  9.81,  0.,  0.,   -1., ),
