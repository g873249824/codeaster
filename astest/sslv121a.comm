                _F(  GROUP_NO = 'D',             DY = 0.),
                _F(  GROUP_NO = 'ABSANSA',      DX = 0.,   DY = 0.)) )

# ------------------------------------------------------------------

MELR=CALC_MATR_ELEM(  MODELE=MO,            CHARGE=CH,
                        CHAM_MATER=CHMAT,    OPTION='RIGI_MECA' )

VEL=CALC_VECT_ELEM(  CHARGE=CH,            CHAM_MATER=CHMAT,
                        OPTION='CHAR_MECA'                     )

NUM=NUME_DDL(  MATR_RIGI=MELR )

#
MATASSR=ASSE_MATRICE(  MATR_ELEM=MELR,    NUME_DDL=NUM  )

VECASS=ASSE_VECTEUR(  VECT_ELEM=VEL,     NUME_DDL=NUM  )

#
MATASSR=FACT_LDLT(reuse=MATASSR,  MATR_ASSE=MATASSR  )

VECASS=RESO_LDLT(reuse=VECASS,  MATR_FACT=MATASSR,   CHAM_NO=VECASS )

#
SIG=CALC_CHAM_ELEM(  MODELE=MO,            DEPL=VECASS,
                      CHAM_MATER=CHMAT,
                      OPTION='SIGM_ELNO_DEPL'  )

