                   PARA = 'FREQ',
                   CRITERE = 'RELATIF',
                   PRECISION = 0.011,
                   NUME_ORDRE = 5,
                   VALE = 15.3439)
          )

#
TEST_RESU(  RESU=_F( RESULTAT = MODES,
                   PARA = 'FREQ',
                   CRITERE = 'RELATIF',
                   PRECISION = 0.017,
                   NUME_ORDRE = 6,
                   VALE = 19.4766)
          )

