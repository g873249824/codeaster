#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_IDEAS()

#
MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(
         MAILLAGE=MA,
         AFFE=_F( TOUT = 'OUI',
              PHENOMENE = 'ACOUSTIQUE',
              MODELISATION = 'PLAN')
         )

#
# PROPRIETES MECANIQUES
#
AIR=DEFI_MATERIAU(
          FLUIDE=_F( RHO = 1.3,
                CELE_C = ('RI',343.0,0.,))
         )

CHM=AFFE_MATERIAU(
         MAILLAGE=MA,
         AFFE=_F( TOUT = 'OUI',
              MATER = AIR)
         )

#
# CONDITIONS AUX LIMITES
#
VELO=AFFE_CHAR_ACOU(
         MODELE=MO,
         VITE_FACE=_F( GROUP_MA = 'velo',
                   VNOR = ('RI',0.014,0.0,))
         )

IMPE=AFFE_CHAR_ACOU(
         MODELE=MO,
         IMPE_FACE=_F( GROUP_MA = 'impe',
                   IMPE = ('RI',445.9,0.0,))
         )

#
# ASSEMBLAGE
#
ASSEMBLAGE(
     MODELE=MO,
     CHARGE=IMPE,
     CHAM_MATER=CHM,
     NUME_DDL=CO("NDDL"),    SOLVEUR=_F(METHODE='MUMPS',),
     MATR_ASSE=(_F(  MATRICE = CO("MATK"), OPTION = 'RIGI_ACOU'),
                _F(  MATRICE = CO("MATM"), OPTION = 'MASS_ACOU'),
                _F(  MATRICE = CO("MATA"), OPTION = 'AMOR_ACOU')),
     VECT_ASSE=(_F(VECTEUR=CO("VECAS"),OPTION='CHAR_ACOU',CHARGE=VELO)),
                )

#
#  REPONSE HARMONIQUE
#
HARMO=DYNA_VIBRA(TYPE_CALCUL='HARM',BASE_CALCUL='PHYS',
         MATR_MASS=MATM,
         MATR_RIGI=MATK,
         MATR_AMOR=MATA,
         FREQ=500.0,
          EXCIT=_F( VECT_ASSE = VECAS,
               COEF_MULT = -1.0,
               PHAS_DEG = 90.0,
               PUIS_PULS = 1)
         )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   VALE_CALC_C=-6.195008451+-0.015674841j,
                   VALE_REFE_C=('RI', -6.2426000000000004, 0.0),
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=HARMO,
                   NOM_CHAM='PRES',
                   NOEUD='N1',
                   NOM_CMP='PRES',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=1,
                   VALE_CALC_C=-6.2444286280064+5.7609335152933E-03j,
                   VALE_REFE_C=('RI', -6.2426000000000004, 0.0),
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=HARMO,
                   NOM_CHAM='PRES',
                   NOEUD='N237',
                   NOM_CMP='PRES',
                   ),
                _F(NUME_ORDRE=1,
                   VALE_CALC_C= 5.9654562396841+2.0109538478286j,
                   VALE_REFE_C=('RI', 6.0236999999999998, 1.6387),
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=HARMO,
                   NOM_CHAM='PRES',
                   NOEUD='N2',
                   NOM_CMP='PRES',
                   PRECISION=0.07,),
                _F(NUME_ORDRE=1,
                   VALE_CALC_C=5.919857621113+1.9838491756394j,
                   VALE_REFE_C=('RI', 6.0236999999999998, 1.6387),
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=HARMO,
                   NOM_CHAM='PRES',
                   NOEUD='N205',
                   NOM_CMP='PRES',
                   PRECISION=0.06,),
                ),
          )

FIN()
