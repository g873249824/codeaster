#       ELEMENTS DE POUTRE, METHODE LAGRANGIENNE
#       REPRISE DU SSNV505F POUR VALIDER DIST_POUTRE
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

# ---------------------  MAILLAGE ----------------

MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_NO=(_F(GROUP_MA=('POU1',),
                                   NOM='GROUP1',),
                                 _F(GROUP_MA=('POU2',),
                                   NOM='GROUP2',),
                                 ));

# ---- MODELE : POUTRE DROITE D'EULER ----

MODE=AFFE_MODELE(MAILLAGE=MAILL1,
                 AFFE=(_F(GROUP_MA='POU1',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_E',),
                       _F(GROUP_MA='POU2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_E',),
                       ),);

# ------------- DEFINITON DU MATERIAU -----------

MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,NU=0.33333,RHO=7800.,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=_F(GROUP_MA=('POU1','POU2'),
                         MATER=MPOUTRE,),);

CHAMPCAR=AFFE_CARA_ELEM(MODELE=MODE,
                        POUTRE=(_F(GROUP_MA='POU1',
                                   SECTION='RECTANGLE',
                                   CARA=('HY','HZ',),
                                   VALE=(63.5,10.16,),),
                                _F(GROUP_MA='POU2',
                                   SECTION='RECTANGLE',
                                   CARA=('HY','HZ',),
                                   VALE=(63.5,10.16,),),
                                ),);

# ------------- DEFINITION D'UN CARA_ELEM POUR LE CONTACT -----------

MOD2=AFFE_MODELE(MAILLAGE=MAILL1,
                 AFFE=(_F(GROUP_MA='POU2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_E',),
                       ),);

DEFI_R=AFFE_CARA_ELEM(MODELE=MOD2,
                        POUTRE=(_F(GROUP_MA='POU2',
                                   SECTION = 'CERCLE',
                                   CARA = ('R' , 'EP',),
                                   VALE = ( 31.75 ,  1.0 ),),
                                ),);

# ---------- CONDITIONS AUX LIMITES --------------

COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO=('A','B',),
                                     DX=0.0,DY=0.0,DZ=0.0,
                                     DRX=0.0,DRY=0.0,DRZ=0.0),
                                  _F(GROUP_NO=('GROUP1','GROUP2'),
                                     DZ=0.0,DRX=0.0,DRY=0.0,),
                                  ),);

CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=_F(GROUP_NO=('C'), DY=-790.,), );

# ---- METHODE LAGRANGIENNE ----

F_DIST_M=DEFI_CONSTANTE(VALE=31.75);
F_DIST_E=DEFI_CONSTANTE(VALE=31.75);


CHLAG = DEFI_CONTACT(MODELE       = MODE,
                     FORMULATION  = 'DISCRETE',

                     ZONE=(
                          _F(
                              GROUP_MA_MAIT='POU1',
                              GROUP_MA_ESCL='POU2',
                              ALGO_CONT ='LAGRANGIEN',
                              DIST_MAIT=F_DIST_M,
                              DIST_ESCL=F_DIST_E,
                              NORMALE = 'MAIT',
                              VECT_MAIT = 'VECT_Y',
                              MAIT_VECT_Y=(0.,0.,-1.,),
                           ),

                         ),
                    )

CHPOU = DEFI_CONTACT(MODELE       = MODE,
                     FORMULATION  = 'DISCRETE',

                     ZONE=(
                          _F(
                              GROUP_MA_MAIT='POU1',
                              GROUP_MA_ESCL='POU2',
                              ALGO_CONT ='LAGRANGIEN',
                              DIST_MAIT=F_DIST_M,
                              DIST_POUTRE='OUI',
                              CARA_ELEM=DEFI_R,
                              NORMALE = 'MAIT',
                              VECT_MAIT = 'VECT_Y',
                              MAIT_VECT_Y=(0.,0.,-1.,),
                           ),

                         ),
                    )

CHPOUC = DEFI_CONTACT(MODELE       = MODE,
                      FORMULATION  = 'CONTINUE',
                      ALGO_RESO_CONT = 'NEWTON',
                      ALGO_RESO_GEOM = 'NEWTON',
                      ZONE=(
                           _F(
                               GROUP_MA_MAIT='POU1',
                               GROUP_MA_ESCL='POU2',
                               DIST_MAIT=F_DIST_M,
                               DIST_POUTRE='OUI',
                               CARA_ELEM=DEFI_R,
                               NORMALE = 'MAIT',
                               VECT_MAIT = 'VECT_Y',
                               MAIT_VECT_Y=(0.,0.,-1.,),
                            ),

                          ),
                     )

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=50,),),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=25,),),);

# ------------------------------------------------------------------------
#  CALCUL DE REFERENCE
# ------------------------------------------------------------------------

ULAG=STAT_NON_LINE(SOLVEUR=_F(),
                   MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                      ),
                CONTACT  = CHLAG,
                COMPORTEMENT=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=0.4,
                             ),
                NEWTON=_F(REAC_ITER=5,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH),
                INFO=1,);

ULAG=CALC_CHAMP( reuse =ULAG, RESULTAT=ULAG,
              FORCE='FORC_NODA', );

T_REA_LA=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=ULAG,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=T_REA_LA,);

# ------------------------------------------------------------------------
#  CALCUL VALIDANT DIST_POUTRE EN FORMULATION DISCRETE
# ------------------------------------------------------------------------

ULAG2=STAT_NON_LINE(SOLVEUR=_F(),
                    MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                      ),
                CONTACT  = CHPOU,
                COMPORTEMENT=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=0.4,
                             ),
                NEWTON=_F(REAC_ITER=5,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH),
                INFO=1,);

ULAG2=CALC_CHAMP( reuse =ULAG2, RESULTAT=ULAG2,
               FORCE='FORC_NODA', );

T_REA_L2=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=ULAG2,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=T_REA_L2,);

# ------------------------------------------------------------------------
#  CALCUL VALIDANT DIST_POUTRE EN FORMULATION CONTINUE
# ------------------------------------------------------------------------

UCONT=STAT_NON_LINE(SOLVEUR=_F(),
                    MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                      ),
                CONTACT  = CHPOU,
                COMPORTEMENT=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=0.4,
                             ),
                NEWTON=_F(REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH),
                INFO=1,);

UCONT=CALC_CHAMP( reuse =UCONT, RESULTAT=UCONT,
               FORCE='FORC_NODA', );

T_REA_C =POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=UCONT,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);


# ------------------------------------------------------------------------
TEST_TABLE(VALE_CALC=245.98717142672,
           NOM_PARA='DY',
           TABLE=T_REA_LA,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.16,),
           )

TEST_TABLE(VALE_CALC=1251.4883178278,
           NOM_PARA='DY',
           TABLE=T_REA_LA,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4,),
           )

# ------------------------------------------------------------------------
TEST_TABLE(VALE_REFE=245.98717142672,
           VALE_CALC=245.98717142672,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DY',
           TABLE=T_REA_L2,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.16,),
           )

TEST_TABLE(VALE_REFE=1251.4883178278,
           VALE_CALC=1251.4883178278,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DY',
           TABLE=T_REA_L2,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4,),
           )

# ------------------------------------------------------------------------
TEST_TABLE(VALE_REFE=245.98717142672,
           VALE_CALC=245.98717142672,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DY',
           TABLE=T_REA_C,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.16,),
           )

TEST_TABLE(VALE_REFE=1251.4883178278,
           VALE_CALC=1251.4883178278,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DY',
           TABLE=T_REA_C,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4,),
           )

# ------------------------------------------------------------------------
FIN();
