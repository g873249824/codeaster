DEBUT(CODE=_F(NOM='SSNP130C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

# DEFINITION DU MATERIAU

ACIER=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                            NU=0.3,),);

# DEFINITION DES INSTANTS POUR STAT_NON_LINE

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,NOMBRE=1,),);

F_INST=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,0.0,1.0,1.0,),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=_F(GROUP_MA='GMP34',),
                 );

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',MATER=ACIER,),);

CHP=AFFE_CHAR_MECA(MODELE=MO,
                   PRES_REP=_F(GROUP_MA='GMP34',PRES=1.0,),);

CHCL=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA='LIGA',DY=0.0,),
                              _F(GROUP_NO='GNP2',DX=0.0,),),);

DEPLA=STAT_NON_LINE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    EXCIT=(_F(CHARGE=CHP,FONC_MULT=F_INST,),
                           _F(CHARGE=CHCL,),),
                    COMP_INCR=_F(RELATION='ELAS',TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),);

DEPLA=CALC_ELEM(reuse =DEPLA,
                RESULTAT=DEPLA,
                INST=1.,
                PREC_ERR=0.8,
                OPTION=('SIEF_ELNO_ELGA','ERZ1_ELEM_SIGM','ETOT_ELEM',
                        'SING_ELEM','SING_ELNO_ELEM'),);

