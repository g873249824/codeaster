                  NOM_CMP='DRY',
                  NOEUD='NO11',
                  VALE=-0.3,
                  CRITERE='RELATIF',),);

TEST_RESU(RESU=_F(RESULTAT=NONLI,
                  INST=0.6,
                  NOM_CHAM='DEPL',REFERENCE='ANALYTIQUE',PRECISION=0.003,
                  NOM_CMP='DRY',
                  NOEUD='NO11',
                  VALE=-0.6,
                  CRITERE='RELATIF',),);
