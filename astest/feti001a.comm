                 AFFE=(_F(GROUP_MA = 'STRU',PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'D_PLAN',   ),
                       _F(GROUP_MA = 'RES',
                          MODELISATION = '2D_DIS_T',PHENOMENE='MECANIQUE'),
