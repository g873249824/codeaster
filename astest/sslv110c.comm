           REFERENCE='ANALYTIQUE',
           NOM_PARA='DG/DF',
           VALE=3.599E+00,
           PRECISION=5.E-2   )

#----------------------------------------------------------------------
#              TEST DE G(A) SUR LES 3 COURONNES
#----------------------------------------------------------------------


TEST_TABLE(      TABLE=GLOC0,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',
                            VALE_K = 'NO1099'),
                 NOM_PARA='G',
                      REFERENCE='ANALYTIQUE',
                      VALE=7.171E-5,
