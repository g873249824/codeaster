#----------------------------------------------------------------------

TEST_TABLE(TABLE=DGE2,
           SENSIBILITE = (YOUNG,),
           REFERENCE='ANALYTIQUE',
           NOM_PARA='DG/DE',
           VALE=-6.857E-09,
           PRECISION=5.E-2   )

TEST_TABLE(TABLE=DGF2,
