DEBUT(CODE=_F(NOM='SSNP15G',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='NON',),);

#......................................................................
# MODELISATION G : MODELISATION BASEE SUR LA MODELISATION A
#                  MAIS LA GESTION DE LA LISTE D'INSTANTS EST DIFFERENTE :
#                  ETUDE DU RAFFINEEMNT DU PAS DE TEMPS
#......................................................................

MA=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MA,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=195000.,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=1930.,
                                 SY=181.,),
                    PRAGER=_F(C=0.,),);

CM=AFFE_MATERIAU(MAILLAGE=MA,
                 AFFE=_F(TOUT='OUI',
                         MATER=ACIER,),);

SIGMA_F=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                            1.0,151.2,
                            2.0,257.2,
                            3.0,0.0,
                            ),PROL_DROITE='EXCLU',PROL_GAUCHE='EXCLU',);

TAU_F=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                          1.0,93.1,
                          2.0,33.1,
                          3.0,0.0,
                          ),PROL_DROITE='EXCLU',PROL_GAUCHE='EXCLU',);

TRACTION=AFFE_CHAR_MECA(MODELE=MO,
                        FORCE_FACE=_F(MAILLE='GAUCHE',
                                      FX=-1.,),);

CISAIL=AFFE_CHAR_MECA(MODELE=MO,
                      FORCE_FACE=(_F(MAILLE='GAUCHE',
                                     FY=-1.,),
                                  _F(MAILLE='DROITE',
                                     FY=1.,),
                                  _F(MAILLE='HAUT',
                                     FX=1.,),
                                  _F(MAILLE='BAS',
                                     FX=-1.,),),);

LIAISON=AFFE_CHAR_MECA(MODELE=MO,
                       DDL_IMPO=(_F(NOEUD='NO4',
                                    DX=0.,
                                    DY=0.,),
                                 _F(NOEUD='NO8',
                                    DX=0.,
                                    DY=0.,
                                    DZ=0.,),
                                 _F(NOEUD='NO2',
                                    DX=0.,),
                                 _F(NOEUD='NO6',
                                    DX=0.,),),);

# LISTE D'INSTANTS PLUS GROSSIERE QUE CELLE DE LA MODELISATION A
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=0.1,NOMBRE=1,),
                                  _F(JUSQU_A=0.9,NOMBRE=5,),
                                  _F(JUSQU_A=1.0,NOMBRE=1,),
                                  _F(JUSQU_A=2.0,NOMBRE=20,),
                                  _F(JUSQU_A=3.0,NOMBRE=1,),),);


# gestion manuelle

DEFLIST1=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                     LIST_INST=L_INST,),
                                     )

U1=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CM,
                 EXCIT=(_F(CHARGE=LIAISON,),
                        _F(CHARGE=TRACTION,
                           FONC_MULT=SIGMA_F,),
                        _F(CHARGE=CISAIL,
                           FONC_MULT=TAU_F,),),
                 COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',),
                 INCREMENT=_F(LIST_INST=DEFLIST1),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=3,),
                 RECH_LINEAIRE=_F(RHO_MAX=150),
                 )

U1=CALC_CHAMP(reuse=U1,
              RESULTAT=U1,
              DEFORMATION='EPSI_ELNO')

tole_xx = 0.2e-2
tole_xy = 0.6e-2

# TEST SUR LES DEFORMATIONS A L'INSTANT 2 :
# LA TOLERANCE EST DOUBLEE PAR RAPPORT A LA DEUXIEME LISTE D'INSTANTS
TEST_RESU(RESU=(_F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U1,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO2',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.035382410,
                   VALE_REFE=0.035264999999999998,
                   PRECISION=4.0000000000000001E-3,
                   MAILLE='CUBE',),
                _F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U1,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO2',
                   NOM_CMP='EPXY',
                   VALE_CALC=0.020234125,
                   VALE_REFE=0.020471,
                   PRECISION=0.012,
                   MAILLE='CUBE',),
                ),
          )

# ON MONTRE QUE L'ON EST ASSEZ LOIN DE LA SOLUTION DE REF -> TEST DE NON_REGRESSION
TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=U1,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO2',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.035382409884634002,
                   MAILLE='CUBE',
                   ),
                _F(INST=2.0,
                   RESULTAT=U1,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO2',
                   NOM_CMP='EPXY',
                   VALE_CALC=0.020234125161265999,
                   MAILLE='CUBE',
                   ),
                ),
          )

#------------------------------------------------------------------
#                  AVEC UNE LISTE D'INSTANTS RAFFINEE
#------------------------------------------------------------------

# LISTE D'INSTANTS REELLEMENT CALCULES DANS LE PREMIER STAT_NON_LINE (POUR INFO)
INST_C1=RECU_TABLE(CO=U1, NOM_TABLE='PARA_CALC')

#
#TUTU.EXTR_TABLE().values['INST']

# IMPRESSION DE LA LISTE D'INSTANTS REELLEMENT CALCULES DANS LE PREMIER STAT_NON_LINE (POUR INFO)
IMPR_TABLE(TABLE=INST_C1,
           NOM_PARA='INST')

# CREATION D'UNE LISTE D'INSTANTS 2 FOIS PLUS FINE
DEFLIST2 = DEFI_LIST_INST(DEFI_LIST=_F(RESULTAT=U1,
                                       SUBD_PAS=2))

U2=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CM,
                 EXCIT=(_F(CHARGE=LIAISON,),
                        _F(CHARGE=TRACTION,
                           FONC_MULT=SIGMA_F,),
                        _F(CHARGE=CISAIL,
                           FONC_MULT=TAU_F,),),
                 COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',),
                 INCREMENT=_F(LIST_INST=DEFLIST2),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=3,),
                 RECH_LINEAIRE=_F(RHO_MAX=150),
                 )

U2=CALC_CHAMP(reuse=U2,
              RESULTAT=U2,
              DEFORMATION='EPSI_ELNO')

TEST_RESU(RESU=(_F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO2',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.035324712,
                   VALE_REFE=0.035264999999999998,
                   PRECISION=2.E-3,
                   MAILLE='CUBE',),
                _F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U2,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO2',
                   NOM_CMP='EPXY',
                   VALE_CALC=0.020351416,
                   VALE_REFE=0.020471,
                   PRECISION=6.0000000000000001E-3,
                   MAILLE='CUBE',),
                ),
          )

# LISTE D'INSTANTS REELLEMENT CALCULES DANS LE DEUXIEME STAT_NON_LINE (POUR INFO)
INST_C2=RECU_TABLE(CO=U2, NOM_TABLE='PARA_CALC')

# IMPRESSION DE LA LISTE D'INSTANTS REELLEMENT CALCULES DANS LE DEUXIEME STAT_NON_LINE (POUR INFO)
IMPR_TABLE(TABLE=INST_C2,
           NOM_PARA='INST')

FIN();
