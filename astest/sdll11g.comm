DEBUT(CODE=_F( NOM = 'SDLL11G',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

ACIER=DEFI_MATERIAU(
                         ELAS=_F(  RHO = 2700.,
                           NU = 0.3,
                           E = 7.2E10))

MAYA=LIRE_MAILLAGE(FORMAT='MED', )

MATER=AFFE_MATERIAU(    MAILLAGE=MAYA,
                                 AFFE=_F(  TOUT = 'OUI',
                                        MATER = ACIER))

STRUCTUR=AFFE_MODELE(    MAILLAGE=MAYA,
                               AFFE=_F(  TOUT = 'OUI',
                                      MODELISATION = 'C_PLAN',
                                      PHENOMENE = 'MECANIQUE'))

# CALCUL DES MATRICES ELEMENTAIRES DE RIGIDITE ET DE MASSE

K_ELEM=CALC_MATR_ELEM(        MODELE=STRUCTUR,
                                CHAM_MATER=MATER,
                                   OPTION='RIGI_MECA' )

M_ELEM=CALC_MATR_ELEM(        MODELE=STRUCTUR,
                                CHAM_MATER=MATER,
                                   OPTION='MASS_MECA' )

# NUMEROTATION

NUMERO=NUME_DDL(    MATR_RIGI=K_ELEM )

# ASSEMBLAGE DES MATRICES DE RIGIDITE ET DE MASSE


K_ASSE=ASSE_MATRICE(    MATR_ELEM=K_ELEM,
                             NUME_DDL=NUMERO )

M_ASSE=ASSE_MATRICE(    MATR_ELEM=M_ELEM,
                             NUME_DDL=NUMERO )

# CALCUL DES MODES PAR SORENSEN

MODE1=MODE_ITER_SIMULT(       MATR_A=K_ASSE,
                                   MATR_B=M_ASSE,
                               CALC_FREQ=_F(  OPTION = 'BANDE',
                                           FREQ = ( 200., 3500., )))

TEST_RESU(RESU=(
