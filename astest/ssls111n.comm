#            Mailles TRIA3

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);



# Caracteristiques du beton

# Module d'Young
EB=2.E11   # module d'Young   (N/m^2)

# Coefficient de poisson vaut 0.22
NUB = 0.;

# Caracteristiques de l'acier (armatures)

# Module d'Young
E=0.E0   # module d'Young   (N/m^2)

# Coefficient de poisson
NU = 0.;


# constantes :
#------------
L=10.     # longueur (m)     (selon x)
l=5.      # largeur  (m)     (selon y)
h=0.8     # epaisseur (m)    (selon z)
e=0.4     # excentrement (m) (vers le haut z>0)
nu=0.     # coefficient de Poisson
rho=1.e3  # masse volumique  (kg/M^3)
G=EB             # car nu=0.
I=(l*h**3)/12.  # inertie de la section autour de Oy
fx=4000.  # force lineique selon x (N/m)
fz=-1000. # force lineique selon z (N/m)


# Taux de ferraillage
AX = 0.00007854;

# Taux de ferraillage
AY = 0.000007854;

# Distance grille/surface moyenne de la dalle
#EZ = 0.038;
EZ=0.0;
# Distance normalisee du ferraillage par rapport
# au feuillet moyen
R0 = (EZ / (0.5 * e));



PRE_GIBI();
MAIL=LIRE_MAILLAGE(FORMAT="ASTER",);

MAIL=DEFI_GROUP(reuse=MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='L11',),
                               _F(GROUP_MA='L12',),
                               _F(GROUP_MA='L13',),
                               _F(GROUP_MA='L14',),),);

MOD=AFFE_MODELE(MAILLAGE=MAIL,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='DKTG',),);

MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='PLAQ1',
                                      VECT_NORM=(0.0,0.0,1.0,),
                                      GROUP_NO='A1',),);
CPRAG = 0.0;
GAM = 0.0;
CPRG = CPRAG;
QP = 0.15;

MAT_B=DEFI_MATERIAU(ELAS=_F(E=EB,
                            NU=NUB,
                            RHO=2500.0,),
                    BETON_ECRO_LINE=_F(D_SIGM_EPSI=0.0,
                                       SYT=4400000.0,
                                       SYC=-52500000.0,),);

MAT_A=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=NU,),
                    ECRO_LINE=_F(D_SIGM_EPSI=0.0,
                                 SY=500000000.0,),);

MAT=DEFI_GLRC(RELATION='GLRC_DAMAGE',
              BETON=_F(MATER=MAT_B,
                       EPAIS=h,
                       GAMMA=GAM,
                       QP1=QP,
                       QP2=QP,
                       C1N1=CPRG,
                       C1N2=CPRG,
                       C1N3=CPRG,
                       C2N1=CPRG,
                       C2N2=CPRG,
                       C2N3=CPRG,
                       C1M1=CPRG,
                       C1M2=CPRG,
                       C1M3=CPRG,
                       C2M1=CPRG,
                       C2M2=CPRG,
                       C2M3=CPRG,),
              NAPPE=(_F(MATER=MAT_A,
                        OMX=AX,
                        OMY=AY,
                        RX=R0,
                        RY=R0,),
                     _F(MATER=MAT_A,
                        OMX=AX,
                        OMY=AY,
                        RX=-R0,
                        RY=-R0,),),
              INFO=2,);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

CARA_ELE=AFFE_CARA_ELEM(MODELE=MOD,
                        COQUE=_F(GROUP_MA='PLAQ1',
                                 EPAIS=h,
                                 EXCENTREMENT=e,INER_ROTA='OUI',
                                 ANGL_REP=(0.0,0.0,),),);

CL1=AFFE_CHAR_MECA(MODELE=MOD,
                     DDL_IMPO=(_F(GROUP_MA='L14',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),),
                                     );
# calcul statique :
#------------------
CHME1A=AFFE_CHAR_MECA(MODELE=MOD,
                      FORCE_ARETE=_F(GROUP_MA='L12', FX=fx, FZ=fz),);



LINST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 1));


RESU1A=STAT_NON_LINE(MODELE=MOD,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CARA_ELE,
                   EXCIT=(_F(CHARGE=CL1,),
                          _F(CHARGE=CHME1A,
                             #FONC_MULT=CH_FO,
                              ),),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=45,),
                   COMPORTEMENT=_F(RELATION='GLRC_DAMAGE',),
                   INCREMENT=_F(LIST_INST=LINST,),);



RESU1A=CALC_CHAMP(reuse=RESU1A,RESULTAT=RESU1A,CONTRAINTE=('EFGE_ELGA','EFGE_ELNO','EFGE_NOEU'))


#IMPR_RESU(FORMAT="RESULTAT",RESU=_F(MAILLAGE=MA1))
IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT=RESU1A, NOM_CHAM=('DEPL','EFGE_ELNO','EFGE_ELGA', 'EFGE_NOEU',),
             #GROUP_NO=('A1','A3'),
              MAILLE=('M1','M71', 'M180', 'M200'),
              ))


# validation de la fleche sur le bord A2 A3 :
#----------------------------------------------------------
vref1=(2*fz*L)*l*L**2/(6*EB*I)    # du au moment de la force fz
vref2=(3*fx*e)*l*L**2/(6*EB*I)    # du au moment de la force fx
vref3=fz*L/(G*h*5/6)             # du a l'effort tranchant de la force fx

TEST_RESU(RESU=_F(NUME_ORDRE=1, GROUP_NO='A3', RESULTAT=RESU1A, NOM_CHAM='DEPL', NOM_CMP='DZ',
                  REFERENCE='ANALYTIQUE', VALE_REFE=(vref1+vref2+vref3), PRECISION=7.e-2,
                  VALE_CALC=-2.96875000002E-05))


# validation de la densit√© d'effort sur le bord A1 A4
#------------------------------------------------------------
nxx=fx
TEST_RESU(RESU=_F(NUME_ORDRE=1, GROUP_NO='A1', RESULTAT=RESU1A, NOM_CHAM='EFGE_NOEU', NOM_CMP='NXX',
                  REFERENCE='ANALYTIQUE', VALE_REFE=nxx,
                  VALE_CALC=4000.0 ,))

mxx=-fz*L - e*fx
mxx=-fz*L
TEST_RESU(RESU=_F(NUME_ORDRE=1, GROUP_NO='A1', RESULTAT=RESU1A, NOM_CHAM='EFGE_NOEU', NOM_CMP='MXX',
                  REFERENCE='ANALYTIQUE', VALE_REFE=mxx,
                  VALE_CALC=9999.99999843,))




# Validation de POST_CHAMP / COQU_EXCENT
#------------------------------------------------------------
RESU1B=POST_CHAMP(RESULTAT=RESU1A, GROUP_MA='PLAQ1',
                 COQU_EXCENT=_F(NOM_CHAM='EFGE_ELNO', MODI_PLAN='OUI',))
IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT=RESU1B,  NOM_CHAM='EFGE_ELNO', MAILLE='M1'))
mxx=-fz*L
mxx=-fz*L - e*fx
TEST_RESU(RESU=_F(NUME_ORDRE=1, MAILLE='M1', NOEUD='N11', RESULTAT=RESU1B, NOM_CHAM='EFGE_ELNO', NOM_CMP='MXX',
                  REFERENCE='ANALYTIQUE', VALE_REFE=mxx,
                  VALE_CALC=8399.99999844,))


FIN();
