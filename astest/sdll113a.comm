                      MAILLAGE_1 = SQUEL ,
                      MODELE_2 = POUTRE,
                      TOUT_CHAM='OUI',
                      NUME_DDL=NUMERO,
                      INST=1.95E-2,
                      VIS_A_VIS=_F(TOUT_1='OUI',
                                   TOUT_2='OUI',),);
#
D_ssd=CREA_CHAMP(OPERATION='EXTR',
                    INST=1.95E-2,
                    NOM_CHAM = 'DEPL',
                    TYPE_CHAM = 'NOEU_DEPL_R',
                    RESULTAT = TRG_PROJ,);
#
V_ssd=CREA_CHAMP(OPERATION='EXTR',
                    INST=1.95E-2,
                    NOM_CHAM = 'VITE',
                    TYPE_CHAM = 'NOEU_DEPL_R',
                    RESULTAT = TRG_PROJ,);
#
