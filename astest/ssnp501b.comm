# CAS TEST  SSNP501B
# MODELE   : METHODE LAGRANGIENNE
# MAILLAGE : QUA4
#
#UNITES   : NEWTON , CENTIMETRE , SECONDE (N,CM,S)
#

DEBUT(CODE=_F(NOM='SSNP501B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(TOUT_GROUP_MA='OUI',),
                             _F(OPTION='NOEUD_ORDO',
                                NOM='LRESU',
                                GROUP_MA='LESC',
                                GROUP_NO_ORIG='A',
                                GROUP_NO_EXTR='D',),),);

#----- GRANDEUR PHYSIQUE ------

POLTH=DEFI_MATERIAU(ELAS=_F(E=407.,
                            NU=0.47999999999999998,),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=20700000,
                            NU=0.29999999999999999,),);

#-----   MODELE ----------------

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(GROUP_MA=('ANNEAU','PLAQUE'),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=_F(GROUP_MA=('LMAI','LESC'),),
                 INFO=2,);



CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='PLAQUE',
                             MATER=ACIER,),
                          _F(GROUP_MA='ANNEAU',
                             MATER=POLTH,),),);

#------ CONDITIONS AUX LIMITES -----------------

CL=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_NO='PLAQUE',
                               DX=0.0,),
                            _F(GROUP_NO='LCD',
                               DY=0.0,),
                            _F(GROUP_NO='LAB',
                               DX=0.0,),
                           ),
                  LIAISON_DDL=_F(GROUP_NO=('A','P1'),
                                 DDL=('DY','DY'),
                                 COEF_MULT=(1.0,-1.0),
                                 COEF_IMPO=0.0,),);

DEPL=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=_F(GROUP_NO='PLAQUE',
                                DY=-2.225,),);


CON=AFFE_CHAR_MECA(MODELE=MO,
                   CONTACT=_F(APPARIEMENT='MAIT_ESCL',
                              REAC_GEOM='CONTROLE',
                              NB_REAC_GEOM=2,
                              METHODE='LAGRANGIEN',
                              GROUP_MA_MAIT='LMAI',
                              GROUP_MA_ESCL='LESC',
                              SANS_GROUP_NO=('A','P1'),
                              ),);

#--------- DEFINITION DES FONCTIONS -----------------

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,4.0,1.0),
                    INTERPOL='LIN',);

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=4.0,
                                   NOMBRE=16,),);

#-------- CALCUL STATIQUE --------------------------

RESU=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=CON,),
                          _F(CHARGE=CL,),
                          _F(CHARGE=DEPL,
                             FONC_MULT=RAMPE,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=LINST,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=9.9999999999999995E-07,
                                  ITER_GLOB_MAXI=50,
                                  ARRET='OUI',),);


#------- POST TRAITEMENT -----------------------

RESU=CALC_ELEM(reuse =RESU,
               OPTION='SIEF_ELNO_ELGA',
               RESULTAT=RESU,);

RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION=('REAC_NODA','SIEF_NOEU_ELGA'),
             );

TAB_REAC=POST_RELEVE_T(ACTION=_F(INTITULE='MESSAGE',
                                 GROUP_NO='LRESU',
                                 RESULTAT=RESU,
                                 NOM_CHAM='REAC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);



TAB_PRES=POST_RELEVE_T(ACTION=_F(INTITULE='MESSAGE',
                                 GROUP_NO='LRESU',
                                 RESULTAT=RESU,
                                 NOM_CHAM='SIEF_NOEU_ELGA',
                                 INST=4.0,
                                 NOM_CMP='SIYY',
                                 OPERATION='EXTRACTION',),);



TAB_CONT=POST_RELEVE_T(ACTION=_F(INTITULE='MESSAGE',
                                 GROUP_NO='LRESU',
                                 RESULTAT=RESU,
                                 NOM_CHAM='VALE_CONT',
                                 INST=4.0,
                                 NOM_CMP=('CONT'),
                                 OPERATION='EXTRACTION',),);



#------- TEST DES RESULTATS --------------
# TEST REFERENCE

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.),
           VALE=-8.008331125,
           PRECISION=0.15,
           REFERENCE='SOURCE_EXTERNE',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.),
           VALE=-16.0166625,
           PRECISION=0.15,
           REFERENCE='SOURCE_EXTERNE',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.),
           VALE=-24.02499375,
           PRECISION=0.15,
           REFERENCE='SOURCE_EXTERNE',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=4.),
           VALE=-32.033325,
           PRECISION=0.15,
           REFERENCE='SOURCE_EXTERNE',);

# TEST NON REGRESSION
TEST_RESU(RESU=_F(RESULTAT=RESU,
                  GROUP_NO='D',
                  INST=2.,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE=7.3382600713111E-01,
                  PRECISION=1.E-5,
                  REFERENCE='NON_REGRESSION',
                  VERSION='6.3.04',),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  GROUP_NO='D',
                  INST=4.,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE=1.2855270869707E+00,
                  PRECISION=1.E-5,
                  REFERENCE='NON_REGRESSION',
                  VERSION='6.3.04',),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  GROUP_NO='A',
                  INST=2.,
                  NOM_CHAM='SIEF_NOEU_ELGA',
                  NOM_CMP='SIYY',
                  VALE=-1.9084000936849E+01,
                  PRECISION=1.E-5,
                  REFERENCE='NON_REGRESSION',
                  VERSION='6.3.04',),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  GROUP_NO='A',
                  INST=4.,
                  NOM_CHAM='SIEF_NOEU_ELGA',
                  NOM_CMP='SIYY',
                  VALE=-1.3732421997128E+01,
                  PRECISION=1.E-5,
                  REFERENCE='NON_REGRESSION',
                  VERSION='6.3.04',),);


FIN();
