                             VALE_I = 3)),
                  NOM_PARA='G_BILIN',
                      VALE=4.7526068735044E+01,
                 PRECISION=5.0E-8,
                 REFERENCE='NON_REGRESSION'
            )

TEST_TABLE(        TABLE=G,
                  NOM_PARA='G',
                   FILTRE=_F(  NOM_PARA = 'NUME_ORDRE',
                             VALE_I = 4),
                      VALE=2.7428584896561E+00,
                 PRECISION=5.0E-8,
                 REFERENCE='NON_REGRESSION'
            )

TEST_TABLE(        TABLE=GBIL,FILTRE=(
                    _F(  NOM_PARA = 'NUME_CMP_I',
                             VALE_I = 4),
                    _F(  NOM_PARA = 'NUME_CMP_J',
                             VALE_I = 4)),
                  NOM_PARA='G_BILIN',
                      VALE=2.7428584896561E+00,
                 PRECISION=5.0E-8,
                 REFERENCE='NON_REGRESSION'
            )

TEST_TABLE(        TABLE=GBIL,FILTRE=(
                    _F(  NOM_PARA = 'NUME_CMP_I',
                             VALE_I = 2),
                    _F(  NOM_PARA = 'NUME_CMP_J',
