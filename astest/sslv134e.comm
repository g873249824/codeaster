                  NOM_PARA='G_BILIN',
                      VALE=1.1195802561347E+01,
                 PRECISION=5.0E-8,
                 REFERENCE='NON_REGRESSION'
            )

TEST_TABLE(        TABLE=GBIL,FILTRE=(
                    _F(  NOM_PARA = 'NUME_CMP_I',
                             VALE_I = 4),
                    _F(  NOM_PARA = 'NUME_CMP_J',
