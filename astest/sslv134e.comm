                  NOM_PARA='G_BILIN',
                      VALE=1.1412041046649E+01,
                 PRECISION=5.0E-8,
                 REFERENCE='NON_REGRESSION'
            )

TEST_TABLE(        TABLE=GMAX,FILTRE=(
                    _F(  NOM_PARA = 'Q_1',
                             VALE = 1.),
                    _F(  NOM_PARA = 'Q_2',
                             VALE = 1.),
                    _F(  NOM_PARA = 'Q_3',
                             VALE = 0.),
                    _F(  NOM_PARA = 'Q_4',
                             VALE = 0.)),
                  NOM_PARA='G',
                      VALE=4.7526068735043E+01,
                 PRECISION=5.0E-8,
                 REFERENCE='NON_REGRESSION'
            )

TEST_TABLE(        TABLE=GMAX,FILTRE=(
