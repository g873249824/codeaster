DEBUT(CODE=_F(NOM='HSNV100A',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

#......................................................................
# CAS TEST THERMOPLASTICITE ELEMENTAIRE
#......................................................................
# DEFINITION DES CARACTERISTIQUES DU MATERIAU

MAIL=LIRE_MAILLAGE();

#
# DONNEES DE MODELISATION

FCT1=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(0.200E-2,400.,
                         0.400E-2,500.,
                         ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',);

#

FCT2=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(0.100E-2,200.,
                         0.300E-2,300.,
                         ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',);

#

CTRACB=DEFI_NAPPE(NOM_PARA='TEMP',
                  PARA=(0.,50.,),
                  FONCTION=(FCT1,FCT2,),
                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE',);

#
# MATERIAU ISOTROPE

MAT=DEFI_MATERIAU(ELAS=_F(E=200.E+3,
                          NU=.3,
                          ALPHA=10.0E-6,),
                  TRACTION=_F(SIGM=CTRACB,),
                  THER=_F(LAMBDA=1.0E-3,
                          RHO_CP=0.0E-3,),);

CM=AFFE_MATERIAU(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         MATER=MAT,),);

T0=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
              OPERATION='AFFE',
              MAILLAGE=MAIL,
              AFFE=_F(TOUT='OUI',
                      NOM_CMP='TEMP',
                      VALE=0.0,),);
t1 = 66.666;

t3 = 80.0;

t5 = 90.0;

t4 = 85.0;

t2 = (t1 + ((t3 - t1) / 2.0));


L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=(_F(JUSQU_A=t1,
                                     NOMBRE=1,),
                                  _F(JUSQU_A=t3,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=t5,
                                     NOMBRE=2,),),);

TIMPVAR=DEFI_FONCTION(NOM_PARA='INST',NOM_RESU='TEMP',VALE=(0.E0,0.E0,
                            100.E0,100.E0,
                            ),);

#

MOTHER=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='THERMIQUE',
                           MODELISATION='AXIS',),);

#

MOMECA=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='AXIS',),);

#

CHTHER=AFFE_CHAR_THER_F(MODELE=MOTHER,
                        TEMP_IMPO=(_F(GROUP_NO='GRNO1',
                                      TEMP=TIMPVAR,),
                                   _F(GROUP_NO='GRNO2',
                                      TEMP=TIMPVAR,),
                                   _F(GROUP_NO='GRNO3',
                                      TEMP=TIMPVAR,),
                                   _F(GROUP_NO='GRNO4',
                                      TEMP=TIMPVAR,),),);

# SOLUTION ELASTOPLASTIQUE

TEMPE=THER_LINEAIRE(MODELE=MOTHER,
                    CHAM_MATER=CM,
                    EXCIT=_F(CHARGE=CHTHER,),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    ETAT_INIT=_F(CHAM_NO=T0,),);

CHMECA=AFFE_CHAR_MECA(MODELE=MOMECA,
                      DDL_IMPO=(_F(GROUP_NO='GRNO1',
                                   DY=0.,),
                                _F(GROUP_NO='GRNO3',
                                   DY=0.,),),);

CTM=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          MATER=MAT,),
                  AFFE_VARC=_F(TOUT='OUI',
                               NOM_VARC='TEMP',
                               EVOL=TEMPE,
                               NOM_CHAM='TEMP',
                               VALE_REF=0.,),);

U=STAT_NON_LINE(MODELE=MOMECA,
                CHAM_MATER=CTM,
                EXCIT=_F(CHARGE=CHMECA,),
                COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                INCREMENT=_F(LIST_INST=L_INST,),);

U=CALC_CHAMP(reuse =U,
             RESULTAT=U,
             CONTRAINTE=('SIGM_ELNO',),
             DEFORMATION=('EPSI_ELNO',),
             VARI_INTERNE=('VARI_ELNO',),);

#

TEST_RESU(RESU=(_F(INST=90.0,
                   RESULTAT=TEMPE,
                   NOM_CHAM='TEMP',
                   NOEUD='N1',
                   NOM_CMP='TEMP',
                   VALE_CALC=90.0,),
                _F(INST=66.665999999999997,
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPXX',
                   VALE_CALC=8.66658E-4,
                   MAILLE='M1',),
                _F(INST=66.665999999999997,
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N1',
                   NOM_CMP='SIYY',
                   VALE_CALC=-133.332,
                   MAILLE='M1',),
                _F(INST=80.0,
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N2',
                   NOM_CMP='EPZZ',
                   VALE_CALC=1.1000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=80.0,
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N2',
                   NOM_CMP='V1',
                   VALE_CALC=2.9999999999999997E-4,
                   MAILLE='M1',),
                _F(INST=80.0,
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N2',
                   NOM_CMP='SIYY',
                   VALE_CALC=-100.0,
                   MAILLE='M1',),
                _F(INST=90.0,
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPZZ',
                   VALE_CALC=1.2750000000000001E-3,
                   MAILLE='M1',),
                _F(INST=90.0,
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='V1',
                   VALE_CALC=5.2499999999999997E-4,
                   MAILLE='M1',),
                _F(INST=90.0,
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-75.0,
                   MAILLE='M1',),
                ),
          )

# MATERIAU ORTHOTROPE EN FAIT ISOTROPE, EN THERMOELASTICITE

MATO=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=200000.,
                                E_T=200000.,
                                E_N=200000.,
                                NU_LT=0.3,
                                NU_LN=0.3,
                                NU_TN=0.3,
                                G_LT=75000.,
                                G_LN=75000.,
                                G_TN=75000.,
                                ALPHA_L=10.0E-6,
                                ALPHA_T=10.0E-6,
                                ALPHA_N=10.0E-6,),
                   TRACTION=_F(SIGM=CTRACB,),
                   THER=_F(LAMBDA=1.0E-3,
                           RHO_CP=0.0E-3,),);

CMO=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          MATER=MATO,),);

TEMPEO=THER_LINEAIRE(MODELE=MOTHER,
                     CHAM_MATER=CMO,
                     EXCIT=_F(CHARGE=CHTHER,),
                     INCREMENT=_F(LIST_INST=L_INST,),
                     ETAT_INIT=_F(CHAM_NO=T0,),);

CTMO=AFFE_MATERIAU(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           MATER=MATO,),
                   AFFE_VARC=_F(TOUT='OUI',
                                NOM_VARC='TEMP',
                                EVOL=TEMPEO,
                                NOM_CHAM='TEMP',
                                VALE_REF=0.,),);

VE=STAT_NON_LINE(MODELE=MOMECA,
                 CHAM_MATER=CTMO,
                 EXCIT=_F(CHARGE=CHMECA,),
                 COMP_ELAS=_F(RELATION='ELAS',),
                 INCREMENT=_F(LIST_INST=L_INST,),
                 NEWTON=_F(MATRICE='ELASTIQUE',),);

VE=CALC_CHAMP(reuse =VE,
              RESULTAT=VE,
              CONTRAINTE=('SIGM_ELNO',),
              DEFORMATION=('EPSI_ELNO',),
              VARI_INTERNE=('VARI_ELNO',),);

#
eps1 = ((1.3 * t1) * 1e-05);

eps3 = ((1.3 * t3) * 1e-05);

eps5 = ((1.3 * t5) * 1e-05);


TEST_RESU(RESU=(_F(INST=66.665999999999997,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VE,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPXX',
                   VALE_CALC= 8.66658000E-04,
                   VALE_REFE=8.6665800000000008E-4,
                   MAILLE='M1',),
                _F(INST=80.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VE,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.04E-03,
                   VALE_REFE=1.04E-3,
                   MAILLE='M1',),
                _F(INST=90.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VE,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.17000000E-03,
                   VALE_REFE=1.17E-3,
                   MAILLE='M1',),
                _F(INST=66.665999999999997,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VE,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N1',
                   NOM_CMP='SIYY',
                   VALE_CALC=-133.332000000,
                   VALE_REFE=-133.333,
                   MAILLE='M1',),
                _F(INST=80.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VE,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N2',
                   NOM_CMP='SIYY',
                   VALE_CALC=-160.000000000,
                   VALE_REFE=-160.0,
                   MAILLE='M1',),
                _F(INST=90.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VE,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-180.000000000,
                   VALE_REFE=-180.0,
                   MAILLE='M1',),
                ),
          )

VI=STAT_NON_LINE(MODELE=MOMECA,
                 CHAM_MATER=CTMO,
                 EXCIT=_F(CHARGE=CHMECA,),
                 COMP_INCR=_F(RELATION='ELAS',),
                 INCREMENT=_F(LIST_INST=L_INST,),
                 NEWTON=_F(MATRICE='ELASTIQUE',),);

VI=CALC_CHAMP(reuse =VI,
              RESULTAT=VI,
              CONTRAINTE=('SIGM_ELNO',),
              DEFORMATION=('EPSI_ELNO',),
              VARI_INTERNE=('VARI_ELNO',),);

#

TEST_RESU(RESU=(_F(INST=66.665999999999997,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VI,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPXX',
                   VALE_CALC= 8.66658000E-04,
                   VALE_REFE=8.6665800000000008E-4,
                   MAILLE='M1',),
                _F(INST=80.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VI,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.04000000E-03,
                   VALE_REFE=1.0400000000000001E-3,
                   MAILLE='M1',),
                _F(INST=90.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VI,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.17000000E-03,
                   VALE_REFE=1.17E-3,
                   MAILLE='M1',),
                _F(INST=66.665999999999997,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VI,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N1',
                   NOM_CMP='SIYY',
                   VALE_CALC=-133.332000000,
                   VALE_REFE=-133.333,
                   MAILLE='M1',),
                _F(INST=80.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VI,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N2',
                   NOM_CMP='SIYY',
                   VALE_CALC=-160.000000000,
                   VALE_REFE=-160.0,
                   MAILLE='M1',),
                _F(INST=90.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=VI,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-180.000000000,
                   VALE_REFE=-180.0,
                   MAILLE='M1',),
                ),
          )

# TEST DE REPRISE

DEP=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
               OPERATION='EXTR',
               RESULTAT=U,
               NOM_CHAM='DEPL',
               INST=t1,);

#

SI1=CREA_CHAMP(TYPE_CHAM='CART_SIEF_R',
               OPERATION='AFFE',
               MAILLAGE=MAIL,
               AFFE=_F(TOUT='OUI',
                       NOM_CMP=('SIXX','SIYY','SIZZ','SIXY','SIYZ','SIXZ',),
                       VALE=(0.,-133.332,0.,0.,0.,0.,),),);

L_INST1=DEFI_LIST_REEL(DEBUT=66.666E0,
                       INTERVALLE=(_F(JUSQU_A=80.000E0,
                                      NOMBRE=2,),
                                   _F(JUSQU_A=90.000E0,
                                      NOMBRE=2,),),);

W=STAT_NON_LINE(MODELE=MOMECA,
                CHAM_MATER=CTM,
                EXCIT=_F(CHARGE=CHMECA,),
                COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                ETAT_INIT=_F(DEPL=DEP,
                             SIGM=SI1,),
                INCREMENT=_F(LIST_INST=L_INST1,),
                CONVERGENCE=_F(RESI_GLOB_RELA=.0001,
                               ITER_GLOB_MAXI=10,),);

W=CALC_CHAMP(reuse =W,
             RESULTAT=W,
             CONTRAINTE=('SIGM_ELNO',),
             DEFORMATION=('EPSI_ELNO',),
             VARI_INTERNE=('VARI_ELNO',),);

#

TEST_RESU(RESU=(_F(INST=80.0,
                   RESULTAT=W,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N2',
                   NOM_CMP='EPZZ',
                   VALE_CALC=1.1000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=80.0,
                   RESULTAT=W,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N2',
                   NOM_CMP='V1',
                   VALE_CALC=2.9999999999999997E-4,
                   MAILLE='M1',),
                _F(INST=80.0,
                   RESULTAT=W,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N2',
                   NOM_CMP='SIYY',
                   VALE_CALC=-100.0,
                   MAILLE='M1',),
                _F(INST=90.0,
                   RESULTAT=W,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPZZ',
                   VALE_CALC=1.2750000000000001E-3,
                   MAILLE='M1',),
                _F(INST=90.0,
                   RESULTAT=W,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='V1',
                   VALE_CALC=5.2499999999999997E-4,
                   MAILLE='M1',),
                _F(INST=90.0,
                   RESULTAT=W,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-75.0,
                   MAILLE='M1',),
                ),
          )

#  ATESTS DES ENERGIES

U=CALC_CHAMP(reuse =U,
             RESULTAT=U,
             ENERGIE=('ETOT_ELGA','ENEL_ELGA',),);

TEST_RESU(RESU=_F(INST=66.665999999999997,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=U,
                  NOM_CHAM='ENEL_ELGA',
                  NOM_CMP='TOTALE',
                  VALE_CALC=0.044443556,
                  VALE_REFE=0.044443999999999997,
                  MAILLE='M1',),
          )

T_POT=POST_ELEM(ENER_POT=_F(TOUT='OUI',),
                CHARGE=CHMECA,
                RESULTAT=U,);

T_TOT=POST_ELEM(ENER_TOTALE=_F(TOUT='OUI',),
                CHARGE=CHMECA,
                RESULTAT=U,);

IMPR_TABLE(TABLE=T_POT,);

IMPR_TABLE(TABLE=T_TOT,);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=0.266661333,
           VALE_REFE=0.26666400000000001,
           NOM_PARA='TOTALE',
           TABLE=T_POT,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

# 4.4444E-2 * 3/2 * 4 = 0.266664

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=0.266661333,
           VALE_REFE=0.26666400000000001,
           NOM_PARA='TOTALE',
           TABLE=T_TOT,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

FIN();
