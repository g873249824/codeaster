DEBUT( CODE=_F(  NOM = 'SSLV04G',NIV_PUB_WEB='INTERNET') )

PRE_GIBI( )

MQ8=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),   )

M=CREA_MAILLAGE(     MAILLAGE=MQ8,
                       MODI_MAILLE=_F(  OPTION = 'QUAD8_9', TOUT = 'OUI')
                     )

M=DEFI_GROUP(reuse=M,   MAILLAGE=M,
                    CREA_GROUP_NO=_F(  GROUP_MA = 'GRNM11')   )

MO=AFFE_MODELE(            MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         PHENOMENE = 'MECANIQUE',
                                         MODELISATION = 'C_PLAN'))

MA=DEFI_MATERIAU(            ELAS=_F(  E = 200000.,
                                         NU = 0.3,
                                         ALPHA = 0.) )

CM=AFFE_MATERIAU(        MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MA))

CH=AFFE_CHAR_MECA(         MODELE=MO,
                              DDL_IMPO=_F(  GROUP_NO = 'GRNM11',  DY = 0.),
                             FACE_IMPO=_F(  GROUP_MA = 'GRMA12',  DNOR = 0.),
                              PRES_REP=_F(  GROUP_MA = 'GRMA13',  PRES = 60.)
                       )

ME=CALC_MATR_ELEM(         MODELE=MO,
                                CHARGE=CH,
                            CHAM_MATER=CM,
                               OPTION='RIGI_MECA'   )

VE=CALC_VECT_ELEM(         CHARGE=CH,
                            CHAM_MATER=CM,
                               OPTION='CHAR_MECA'   )

NU=NUME_DDL(            MATR_RIGI=ME           )

K=ASSE_MATRICE(        MATR_ELEM=ME,
                              NUME_DDL=NU )

F=ASSE_VECTEUR(        VECT_ELEM=VE,
                              NUME_DDL=NU           )

K=FACT_LDLT(      reuse=K,     MATR_ASSE=K            )

DEPLA=RESO_LDLT(           MATR_FACT=K,
                               CHAM_NO=F            )

DEFO=CALC_CHAM_ELEM(         MODELE=MO,
                           DEPL=DEPLA,
                                OPTION='EPSI_ELNO_DEPL',
                            CHAM_MATER=CM           )

SIG_NO=CALC_CHAM_ELEM(        MODELE=MO,
                                  DEPL=DEPLA,
                                 OPTION='SIGM_ELNO_DEPL',
                             CHAM_MATER=CM               )

SIG_GA=CALC_CHAM_ELEM(         MODELE=MO,
                            DEPL=DEPLA,
                                 OPTION='SIEF_ELGA_DEPL',
                             CHAM_MATER=CM           )

ENDO1=CALC_CHAM_ELEM(         MODELE=MO,
                            CHAM_ELEM=SIG_GA,
                               OPTION='ENDO_ELNO_SIGM',
                           CHAM_MATER=CM            )

ENDO2=CALC_CHAM_ELEM(         MODELE=MO,
                            CHAM_ELEM=SIG_NO,
                               OPTION='ENDO_ELNO_SIGM',
                           CHAM_MATER=CM            )

