               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

#_____________________________________________________________________
#
#                    DEFINITION DES MATERIAUX
#_____________________________________________________________________
#
#


MAT=DEFI_MATERIAU(ELAS=_F(E=2.E4,
                          NU=0.29999999999999999,
                          ALPHA=1.E-2,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);
#_____________________________________________________________________
#
#                    AFFECTATION DES CONDITIONS AUX LIMITES
#                       ET DES DEPLACEMENTS IMPOSES
#_____________________________________________________________________
#
#


CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='FACESUP',
                                 DY=-2.0,),
                              _F(GROUP_NO='LIM1',
                                 DX=0.0,),
                              _F(GROUP_NO='LIM2',
                                 DZ=0.0,),),);

CMUL=DEFI_CONSTANTE(VALE=-1.0,);

FIMP=FORMULE(NOM_PARA=('X','Y','Z',),VALE='Y-50',);

CHA2=DEFI_CONTACT(MODELE=MO,
                  FORMULATION = 'LIAISON_UNIL',
                  ZONE=_F(GROUP_NO='SMAI',
                          NOM_CMP='DY',
                          COEF_IMPO=FIMP,
                          COEF_MULT=CMUL,
                          ),);
