DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#

MATERI_1=DEFI_MATERIAU( ELAS=_F(  E = 450000.,    NU = 0.3) )

#

MAILLA_1=LIRE_MAILLAGE(FORMAT='MED',   )

MAILLA_1=DEFI_GROUP( reuse=MAILLA_1,   MAILLAGE=MAILLA_1,
                           CREA_GROUP_MA=_F(  NOM = 'TOUT',  TOUT = 'OUI') )

#

CH_MAT_1=AFFE_MATERIAU(  MAILLAGE=MAILLA_1,
                            AFFE=_F(  TOUT = 'OUI',   MATER = MATERI_1) )

#

MODELE_1=AFFE_MODELE(  MAILLAGE=MAILLA_1, 
                     AFFE=_F( TOUT = 'OUI', MODELISATION = 'COQUE_3D',
                                PHENOMENE = 'MECANIQUE') )

#

CHARGE_1=AFFE_CHAR_MECA(MODELE=MODELE_1,DDL_IMPO=(
#
                 _F( GROUP_NO = 'G_10',         DY = 0.,
                                            DRX = 0.,        DRZ = 0.),
#
                 _F( GROUP_NO = 'G_20',  DX = 0.,
                                                   DRY = 0., DRZ = 0.),
#
                 _F( GROUP_NO = 'G_30',  DX = 0., DY = 0., DZ = 0.,
                                            DRX = 0., DRY = 0., DRZ = 0.),
#
                 _F( GROUP_NO = 'G_40',  DX = 0., DY = 0., DZ = 0.,
                                            DRX = 0., DRY = 0., DRZ = 0.)),
#
               FORCE_COQUE=_F(  TOUT = 'OUI',   FZ = -1.)
#
                  )

#

CARAC_1=AFFE_CARA_ELEM(   MODELE=MODELE_1,
                               COQUE=_F(
                                       GROUP_MA = 'TOUT',
                                       EPAIS = 0.125,
                                       COEF_RIGI_DRZ = 1.E-5)
                            )

#

D_FONC=DEFI_FONCTION(
                            NOM_PARA='INST',
                                VALE=(     0.,   0.,
                                           1.,   1.,
                                     )
                         )

#
