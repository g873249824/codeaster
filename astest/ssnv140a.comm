#
                            INTERVALLE=_F(
                                         JUSQU_A = 0.4,
                                         NOMBRE = 10)
#
                         )

#
#

NONLIN=STAT_NON_LINE(
                            MODELE=MODELE_1,
                        CHAM_MATER=CH_MAT_1,
                         CARA_ELEM=CARAC_1,
                             EXCIT=_F(
                CHARGE = CHARGE_1,
                FONC_MULT = D_FONC,
                TYPE_CHARGE = 'FIXE_CSTE'),
                         COMPORTEMENT=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'GROT_GDEP'),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 25),
                            NEWTON=_F(
                REAC_ITER = 1)
              )

