                   VALE_CALC=-91.7598,
                   VALE_REFE=-91.7598,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=1.E-2,),
                _F(INST=0.8,
                   RESULTAT=VL,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-92.5802,
                   VALE_REFE=-92.5802,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=1.E-2,),
                _F(INST=1.3,
                   RESULTAT=VL,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO3',
                   NOM_CMP='SIYY',
                   VALE_CALC=70.1846,
                   VALE_REFE=70.1846,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   RESULTAT=VL,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO3',
                   NOM_CMP='SIYY',
                   VALE_CALC=84.4144,
                   VALE_REFE=84.4120,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=1.E-2,),
),)


