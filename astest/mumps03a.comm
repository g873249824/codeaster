DEBUT(CODE=_F(NOM='MUMPS03A',NIV_PUB_WEB='INTERNET')  )


MAIL=LIRE_MAILLAGE( )

AIR=DEFI_MATERIAU( FLUIDE=_F( RHO = 1.3, CELE_C = 343.+0.j))
CHAMPMAT=AFFE_MATERIAU(  MAILLAGE=MAIL, AFFE=_F( TOUT = 'OUI',  MATER = AIR) )

GUIDE=AFFE_MODELE(  MAILLAGE=MAIL,     VERIF='MAILLE',
                 AFFE=_F( TOUT = 'OUI', MODELISATION = '3D', PHENOMENE = 'ACOUSTIQUE') )

CHARACOU=AFFE_CHAR_ACOU(      MODELE=GUIDE,
                 VITE_FACE=_F( GROUP_MA = 'ENTREE', VNOR = 0.014+0.j))

IMPEACOU=AFFE_CHAR_ACOU(      MODELE=GUIDE,
                 IMPE_FACE=_F( GROUP_MA = 'SORTIE', IMPE = 445.9+0.j),
                 PRES_IMPO=_F( NOEUD = 'NO600', PRES = 3.+1.j), )
CHCINE=AFFE_CHAR_CINE(      MODELE=GUIDE,
                 ACOU_IMPO=_F( NOEUD = 'NO500', PRES = 1.+3.j), )


# 1ER CALCUL (REFERENCE) AVEC MULT_FRONT :
#-----------------------------------------
MEL1=CALC_MATR_ELEM(CHARGE=IMPEACOU, INST=0.0, OPTION='RIGI_ACOU',  MODELE=GUIDE, CHAM_MATER=CHAMPMAT, );
NUM1=NUME_DDL(METHODE='MULT_FRONT',MATR_RIGI=MEL1, );
MATAS1=ASSE_MATRICE(NUME_DDL=NUM1, MATR_ELEM=MEL1, CHAR_CINE=CHCINE  );
VCIN1=CALC_CHAR_CINE(   NUME_DDL=NUM1,  CHAR_CINE=CHCINE)

VECTELE1=CALC_VECT_ELEM(OPTION='CHAR_ACOU',  CHAM_MATER=CHAMPMAT, CHARGE=CHARACOU )
VECTAS1=ASSE_VECTEUR(  VECT_ELEM=VECTELE1,   NUME_DDL=NUM1 )

FACT_LDLT( reuse=MATAS1,  MATR_ASSE=MATAS1 )
VECTAS1=RESO_LDLT( reuse=VECTAS1,  MATR_FACT=MATAS1, CHAM_NO=VECTAS1,CHAM_CINE=VCIN1 )

TEST_RESU(CHAM_NO=_F( CHAM_GD = VECTAS1, NOEUD = 'NO1', NOM_CMP = 'PRES',
          VALE_C = 5.8076524773140E-01+0.j,   REFERENCE='NON_REGRESSION') )


# 2EME CALCUL AVEC MUMPS :
#-----------------------------------------
MEL2=CALC_MATR_ELEM(CHARGE=IMPEACOU, INST=0.0, OPTION='RIGI_ACOU',  MODELE=GUIDE, CHAM_MATER=CHAMPMAT, );
NUM2=NUME_DDL(METHODE='MUMPS',MATR_RIGI=MEL2, );
MATAS2=ASSE_MATRICE(NUME_DDL=NUM2, MATR_ELEM=MEL2, CHAR_CINE=CHCINE  );
VCIN2=CALC_CHAR_CINE(   NUME_DDL=NUM2,  CHAR_CINE=CHCINE)

VECTELE2=CALC_VECT_ELEM(OPTION='CHAR_ACOU',  CHAM_MATER=CHAMPMAT, CHARGE=CHARACOU )
VECTAS2=ASSE_VECTEUR(  VECT_ELEM=VECTELE2,   NUME_DDL=NUM2 )

FACT_LDLT( reuse=MATAS2,  MATR_ASSE=MATAS2 )
VECTAS2=RESO_LDLT( reuse=VECTAS2,  MATR_FACT=MATAS2, CHAM_NO=VECTAS2,CHAM_CINE=VCIN2 )

TEST_RESU(CHAM_NO=_F( CHAM_GD = VECTAS2, NOEUD = 'NO1', NOM_CMP = 'PRES',
          VALE_C = 5.8076524773140E-01+0.j,   REFERENCE='AUTRE_ASTER') )


FIN()
