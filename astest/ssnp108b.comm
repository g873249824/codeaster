# EFFORT NORMAL AUX NOEUDS DE L'ELEMENT DE BETON
# ----------------------------------------------
#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='B1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOLNOLI,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='NXX',
                  VALE_CALC=-9.83877533E+04,
                  VALE_REFE=-9.9912576499999996E4,
                  PRECISION=0.02,
                  MAILLE='M1',),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='B3',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOLNOLI,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='NXX',
                  VALE_CALC=-9.83877533E+04,
                  VALE_REFE=-9.9912576499999996E4,
                  PRECISION=0.02,
                  MAILLE='M6',),
          )

########################################################################
# TESTS POUR LES NOEUDS DU CABLE
########################################################################
#
# EFFORT NORMAL AUX NOEUDS DU CABLE
# ---------------------------------
#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='C1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOLNOL2,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='N',
                  VALE_CALC= 1.99822489E+05,
                  VALE_REFE=1.9982515299999999E5,
                  PRECISION=1.E-3,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='C2',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOLNOL2,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='N',
                  VALE_CALC= 1.99439325E+05,
                  VALE_REFE=1.9982515299999999E5,
                  PRECISION=2.E-3,),
          )

#
TEST_TABLE(
           VALE_CALC=1.99822E5,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='N',
           TABLE=ESSAI1,
           FILTRE=_F(NOM_PARA='NOEUD',
                     VALE_K='N5',),
           )

#
TEST_TABLE(
           VALE_CALC=1.99439E5,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='N',
           TABLE=ESSAI1,
           FILTRE=_F(NOM_PARA='NOEUD',
                     VALE_K='N6',),
           )

#--------------------------------------------------------------------------------------------
# Validation CREA_CHAMP  / OPERATION='EXTR' + TABLE  sur plusieurs types de champs :
#   CREA_TABLE / RESU
#   CALC_TABLE / OPERATION='EXTR'
#   CREA_CHAMP / OPERATION='EXTR' + TABLE
#--------------------------------------------------------------------------------------------
#IMPR_RESU(RESU=_F(RESULTAT=EVOLNOLI,NUME_ORDRE=1))


# champ DEPL :
#------------------
TAB11 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "DEPL",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('DX','DY')),)

TAB12=CALC_TABLE(TABLE=TAB11, ACTION=_F(OPERATION='EXTR', NOM_PARA=('NOEUD','DX','DY')))

CH12= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'NOEU_DEPL_R',
                 TABLE = TAB12, MAILLAGE = MAILLAGE,)
TEST_RESU(CHAM_NO=_F(CHAM_GD=CH12, NOEUD='N10', NOM_CMP='DX',
                  REFERENCE='AUTRE_ASTER', VALE_CALC=-1.3066070176316E-05, VALE_REFE=-1.3066070176316E-05,))


# champ SIEF_ELGA :
#------------------
TAB21 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "SIEF_ELGA",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('SIYY','SIXY')),)

TAB22=CALC_TABLE(TABLE=TAB21, ACTION=_F(OPERATION='EXTR', NOM_PARA=('MAILLE','POINT','SOUS_POINT','SIYY','SIXY')))

CH22= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'ELGA_SIEF_R',   OPTION = 'SIEF_ELGA' ,
                 TABLE = TAB22, MAILLAGE = MAILLAGE, MODELE = MODELE, PROL_ZERO = 'OUI',)
#    M10     |           4|          15| 4.613645846E+04|-3.207968425E+04| ...
TEST_RESU(CHAM_ELEM=_F(CHAM_GD=CH22, MAILLE='M10', POINT=4, SOUS_POINT=15,  NOM_CMP='SIYY',
                  REFERENCE='AUTRE_ASTER', VALE_CALC=-3.207968425E+04, VALE_REFE=-3.207968425E+04,))


# champ SIEF_ELNO :
#------------------
TAB31 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "SIEF_ELNO",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('SIYY','SIXY')),)

TAB32=CALC_TABLE(TABLE=TAB31, ACTION=_F(OPERATION='EXTR', NOM_PARA=('MAILLE','NOEUD','SOUS_POINT','SIYY','SIXY')))

CH32= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'ELNO_SIEF_R',   OPTION = 'SIEF_ELNO' ,
                 TABLE = TAB32, MAILLAGE = MAILLAGE, MODELE = MODELE, PROL_ZERO = 'OUI',)
#     M4      |    N10     |          15|-3.655790816E+05| 6.707396179E+03| ...
TEST_RESU(CHAM_ELEM=_F(CHAM_GD=CH32, MAILLE='M4', NOEUD='N10', SOUS_POINT=15,  NOM_CMP='SIYY',
                  REFERENCE='AUTRE_ASTER', VALE_CALC=6.707396179E+03, VALE_REFE=6.707396179E+03,))

# champ VARI_ELGA :
#------------------
TAB41 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "VARI_ELGA",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('V1',)),)

TAB42=CALC_TABLE(TABLE=TAB41, ACTION=_F(OPERATION='EXTR', NOM_PARA=('MAILLE','POINT','SOUS_POINT','V1',)))

CH42= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'ELGA_VARI_R',   OPTION = 'VARI_ELGA' ,
                 TABLE = TAB42, MAILLAGE = MAILLAGE, MODELE = MODELE, PROL_ZERO = 'OUI',)
TEST_RESU(CHAM_ELEM=_F(CHAM_GD=CH42, MAILLE='M10', POINT=4, SOUS_POINT=15,  NOM_CMP='V1',
                  REFERENCE='AUTRE_ASTER', VALE_CALC=0., VALE_REFE=0., CRITERE='ABSOLU',))


FIN()
