DEBUT(   CODE=_F(  NOM = 'SSNP108B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
PRE_GIBI( ) ;

MAILLAGE=LIRE_MAILLAGE( )

MAILLAGE=DEFI_GROUP( reuse=MAILLAGE, MAILLAGE=MAILLAGE,
                     CREA_GROUP_NO=_F(  GROUP_MA = ('CABLE','CB3', 'CB5'), ),
                    );

#
MODELE=AFFE_MODELE(  MAILLAGE=MAILLAGE,
                 AFFE=(_F(  GROUP_MA = 'BETON',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'DKT'),
                       _F(  GROUP_MA = 'CABLE',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE')) )

#
CARAELEM=AFFE_CARA_ELEM(  MODELE=MODELE,
                       COQUE=_F(  GROUP_MA = 'BETON', COQUE_NCOU=5,
                                  EPAIS = 6.0E-1 ),
                       BARRE=_F(  GROUP_MA = 'CABLE',
                                  SECTION = 'GENERALE',
                                  CARA =  'A',
                                  VALE =  1.5E-4, ), )

#
BETON=DEFI_MATERIAU(   ELAS=_F(  E = 3.0E+10,
                                 NU = 0.),
                 BPEL_BETON=_F() )

#
ACIER=DEFI_MATERIAU(  ELAS=_F(  E = 2.1E+11,
                                NU = 0.),
                     BPEL_ACIER=_F(  F_PRG = 1.77E+9,
                                     FROT_COURB = 0.,
                                     FROT_LINE = 0.) )

#
CHMATER=AFFE_MATERIAU(   MAILLAGE=MAILLAGE,
                   AFFE=(_F(  GROUP_MA = 'BETON',
                              MATER = BETON),
                         _F(  GROUP_MA = 'CABLE',
                              MATER = ACIER)) )

#
CABLEPR=DEFI_CABLE_BP(      MODELE=MODELE,
                        CHAM_MATER=CHMATER,
                         CARA_ELEM=CARAELEM,
                    GROUP_MA_BETON='BETON',
                        DEFI_CABLE=_F( GROUP_MA = 'CABLE',
                                       GROUP_NO_ANCRAGE = ( 'C1', 'C2' ),),
                      TYPE_ANCRAGE=( 'ACTIF',  'ACTIF',  ),
                      TENSION_INIT=2.0E+5,
                     RECUL_ANCRAGE=0. )

#
CHARMECA=AFFE_CHAR_MECA(  MODELE=MODELE,

                  DDL_IMPO=(_F(  GROUP_NO = ( 'CB3', 'CB5' ),
                                 DX  = 0.,
                                 DY  = 0.,
                                 DZ  = 0.,
                                 DRX = 0.,
                                 DRY = 0.,
                                 DRZ = 0.    ),
#                            _F(  GROUP_NO = 'B3',
#                                 DX  = 0.,
#                                 DZ  = 0.,  ),
                           ),
                   RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                                     SIGM_BPEL = 'OUI',
                                     RELA_CINE = 'OUI'),
                        )

#
LINST=DEFI_LIST_REEL(    VALE=( 0.0,  1.0, ) )

#
EVOLNOLI=STAT_NON_LINE(
                MODELE=MODELE,
                CHAM_MATER=CHMATER,
                CARA_ELEM=CARAELEM,
                EXCIT=      _F( CHARGE = CHARMECA),
                COMP_INCR=  _F( RELATION = 'ELAS',
                                DEFORMATION = 'PETIT',
                                TOUT = 'OUI'),
                INCREMENT=  _F( LIST_INST = LINST),
                SOLVEUR=    _F( METHODE = 'MULT_FRONT',),
                CONVERGENCE=_F( ITER_GLOB_MAXI = 10,
                                RESI_GLOB_MAXI = 1.E-5),
              )

EVOLNOLI=CALC_ELEM( reuse=EVOLNOLI,  RESULTAT=EVOLNOLI,
                            OPTION='SIEF_ELNO',
              )

EVOLNOLI=CALC_NO( reuse=EVOLNOLI,  RESULTAT=EVOLNOLI,
#                    GROUP_MA='CABLE',
#                    MAILLE=('M1','M6'),
                      OPTION='SIEF_NOEU',
              )

ESSAI1=POST_RELEVE_T(ACTION=(_F(
                              INTITULE='ESSAI1',
                              GROUP_NO = 'CABLE' ,
                              RESULTAT=EVOLNOLI,
                              NOM_CHAM='SIEF_ELNO',
                              NUME_ORDRE=1,
                              NOM_CMP='N',
                              OPERATION='EXTRACTION',),
                            ),
                    ) ;

#
########################################################################
# TESTS POUR LES NOEUDS DE L'ELEMENT DE BETON
########################################################################
#
# EFFORT NORMAL AUX NOEUDS DE L'ELEMENT DE BETON
# ----------------------------------------------
#
TEST_RESU(  RESU=_F(  RESULTAT = EVOLNOLI,
                      NUME_ORDRE = 1,
                      GROUP_NO = 'B1',
                      MAILLE = 'M1',
                      NOM_CHAM = 'SIEF_ELNO',
                      NOM_CMP = 'NXX',
                      PRECISION = 0.020,
                      REFERENCE = 'ANALYTIQUE',
                      VALE = -9.99125765E+4 ) )

#
TEST_RESU(  RESU=_F(  RESULTAT = EVOLNOLI,
                      NUME_ORDRE = 1,
                      GROUP_NO = 'B3',
                      MAILLE = 'M6',
                      NOM_CHAM = 'SIEF_ELNO',
                      NOM_CMP = 'NXX',
                      PRECISION = 0.020,
                      REFERENCE = 'ANALYTIQUE',
                      VALE = -9.99125765E+4 ) )

########################################################################
# TESTS POUR LES NOEUDS DU CABLE
########################################################################
#
# EFFORT NORMAL AUX NOEUDS DU CABLE
# ---------------------------------
#
TEST_RESU(  RESU=_F(  RESULTAT = EVOLNOLI,
                      NUME_ORDRE = 1,
                      GROUP_NO = 'C1',
                      NOM_CHAM = 'SIEF_NOEU',
                      NOM_CMP = 'N',
                      REFERENCE = 'ANALYTIQUE',
                      PRECISION = 0.001,
                      VALE = 1.99825153E+5 ) )

TEST_RESU(  RESU=_F(  RESULTAT = EVOLNOLI,
                      NUME_ORDRE = 1,
                      GROUP_NO = 'C2',
                      NOM_CHAM = 'SIEF_NOEU',
                      NOM_CMP = 'N',
                      REFERENCE = 'ANALYTIQUE',
                      PRECISION = 0.002,
                      VALE = 1.99825153E+5 ) )

#
TEST_TABLE(   TABLE=ESSAI1,
              FILTRE=_F( NOM_PARA = 'NOEUD',
                         VALE_K   = 'N5',    ),
              NOM_PARA = 'N',
                      REFERENCE = 'NON_REGRESSION',
                      VERSION   = '7.2.2',
                      VALE = 1.99822E+05 )

#
TEST_TABLE(   TABLE=ESSAI1,
              FILTRE=_F( NOM_PARA = 'NOEUD',
                         VALE_K   = 'N6',    ),
              NOM_PARA = 'N',
                      REFERENCE = 'NON_REGRESSION',
                      VERSION   = '7.2.2',
                      VALE = 1.99439E+05 )

#--------------------------------------------------------------------------------------------
# Validation CREA_CHAMP  / OPERATION='EXTR' + TABLE  sur plusieurs types de champs :
#   CREA_TABLE / RESU
#   CALC_TABLE / OPERATION='EXTR'
#   CREA_CHAMP / OPERATION='EXTR' + TABLE
#--------------------------------------------------------------------------------------------
IMPR_RESU(RESU=_F(RESULTAT=EVOLNOLI,NUME_ORDRE=1,NOM_CHAM = ("SIEF_ELNO","SIEF_ELGA",),))


# champ DEPL :
#------------------
TAB11 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "DEPL",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('DX','DY')),)

TAB12=CALC_TABLE(TABLE=TAB11, ACTION=_F(OPERATION='EXTR', NOM_PARA=('NOEUD','DX','DY')))

CH12= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'NOEU_DEPL_R',
                 TABLE = TAB12, MAILLAGE = MAILLAGE,)
TEST_RESU(CHAM_NO=_F(CHAM_GD=CH12, NOEUD='N10', NOM_CMP='DX',
                  REFERENCE='AUTRE_ASTER', VALE=-1.3066070176316E-05,))


# champ SIEF_ELGA :
#------------------
TAB21 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "SIEF_ELGA",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('SIYY','SIXY')),)

TAB22=CALC_TABLE(TABLE=TAB21, ACTION=_F(OPERATION='EXTR', NOM_PARA=('MAILLE','POINT','SOUS-POINT','SIYY','SIXY')))

CH22= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'ELGA_SIEF_R',   OPTION = 'SIEF_ELGA' ,
                 TABLE = TAB22, MAILLAGE = MAILLAGE, MODELE = MODELE, PROL_ZERO = 'OUI',)
#    M10     |           4|          15| 4.613645846E+04|-3.207968425E+04| ...
TEST_RESU(CHAM_ELEM=_F(CHAM_GD=CH22, MAILLE='M10', POINT=4, SOUS_POINT=15,  NOM_CMP='SIYY',
                  REFERENCE='AUTRE_ASTER', VALE=-3.207968425E+04,))

# champ SIEF_ELNO :
#------------------
TAB31 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "SIEF_ELNO",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('NYY')),)

TAB32=CALC_TABLE(TABLE=TAB31, ACTION=_F(OPERATION='EXTR', NOM_PARA=('MAILLE','NOEUD','NYY')))

CH32= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'ELNO_SIEF_R',   OPTION = 'SIEF_ELNO' ,
                 TABLE = TAB32, MAILLAGE = MAILLAGE, MODELE = MODELE, PROL_ZERO = 'OUI',)
#     M4      |    N10     |          15|-3.655790816E+05| 6.707396179E+03| ...
TEST_RESU(CHAM_ELEM=_F(CHAM_GD=CH32, MAILLE='M4', NOEUD='N10', NOM_CMP='NYY',
                  REFERENCE='AUTRE_ASTER', VALE=4024.4377076393,))

# champ VARI_ELGA :
#------------------
TAB41 = CREA_TABLE ( RESU = _F(RESULTAT = EVOLNOLI, NOM_CHAM = "VARI_ELGA",
                              TOUT='OUI',NUME_ORDRE = 1, NOM_CMP = ('V1',)),)

TAB42=CALC_TABLE(TABLE=TAB41, ACTION=_F(OPERATION='EXTR', NOM_PARA=('MAILLE','POINT','SOUS-POINT','V1',)))

CH42= CREA_CHAMP(OPERATION = 'EXTR', INFO=1, TYPE_CHAM = 'ELGA_VARI_R',   OPTION = 'VARI_ELGA' ,
                 TABLE = TAB42, MAILLAGE = MAILLAGE, MODELE = MODELE, PROL_ZERO = 'OUI',)
TEST_RESU(CHAM_ELEM=_F(CHAM_GD=CH42, MAILLE='M10', POINT=4, SOUS_POINT=15,  NOM_CMP='V1',
                  REFERENCE='AUTRE_ASTER', VALE=0., CRITERE='ABSOLU',))

#
FIN( )
