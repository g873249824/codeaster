                     _F( GROUP_MA = 'INT_ALV'),
                     _F(NOM='INT_ALVS',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA='INT_ALV'),
                               _F(NOM='HAUTS',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA='HAUT'),
                               _F(NOM='BASS',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA='BAS'),
                               _F(NOM='ROCHES',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA='ROCHE'),
                     _F(DIFFE=('BASS','A'),
                        NOM='BAS1',),
                     _F(DIFFE=('BAS1','B'),
