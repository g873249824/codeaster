                NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=70,),
              );


RESU0=CALC_CHAMP(reuse=RESU0,CONTRAINTE=('SIEF_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=RESU0)


RESU0=CALC_CHAMP(reuse=RESU0,
