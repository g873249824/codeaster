                       NOM_PARA='SAT',
                       PROL_DROITE='CONSTANT',
                       PROL_GAUCHE='CONSTANT',
                       INFO=2,);

DPL= FORMULE(NOM_PARA='SAT',VALE='0.999*3*SAT**2');

DPERML=CALC_FONC_INTERP(FONCTION=DPL,
                       LIST_PARA=LI1,
                       NOM_PARA='SAT',
                       PROL_DROITE='CONSTANT',
                       PROL_GAUCHE='CONSTANT',
                       INFO=2,);


