#                      _F(  JUSQU_A = 31536000.,  NOMBRE = XX1),
# 15 ans :
                        _F(  JUSQU_A = 473040000.,  NOMBRE = 2),
# 50 ans :
                      _F(  JUSQU_A = 15768.E5,  NOMBRE = 3),
# 100 ans :
                       _F(  JUSQU_A =31536.E5,  NOMBRE = 2),
# 200 ans :
                       _F(  JUSQU_A =63072.E5,  NOMBRE = 2),
# 500 ans :
                       _F(  JUSQU_A =15768.E6,  NOMBRE = 5),
# 1000 ans :
                       _F(  JUSQU_A =31536.E6,  NOMBRE = 10),
# 10000 ans :
                       _F(  JUSQU_A =31536.E7,  NOMBRE = 10),
                     ),);


############################################
#
# DEFINITION DES FONCTIONS DE COMPORTEMENT :
# VISCOSITE LIQUIDE ET GAZ : UNITE : PA.S
#
####

VISCOL=DEFI_CONSTANTE(VALE=1.,);

VISCOG=DEFI_CONSTANTE(VALE=1.8E-05,);

DVISCOL=DEFI_CONSTANTE(VALE=0.0,);

DVISCOG=DEFI_CONSTANTE(VALE=0.0,);

####
UN=DEFI_CONSTANTE(    VALE=1.,);

ZERO=DEFI_CONSTANTE(    VALE=0.,);

LI2=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=(_F(JUSQU_A=1.E6,  PAS=1.E5,),
                                 _F(JUSQU_A=1.E7,  PAS=1.E5,),
                                 _F(JUSQU_A=1.E8,  PAS=1.E6,),)
                   );

LI1=DEFI_LIST_REEL(DEBUT=1.E-5,
                   INTERVALLE=_F(JUSQU_A=1.0,
                                 PAS=0.099999,),);
#
# PRESSION CAPILLAIRE S(PC)
#
SL = FORMULE(NOM_PARA='PCAP',VALE='0.999*(0.85/(1+(6.5E-8*PCAP)**1.49)**0.33+0.15)');
SATU=CALC_FONC_INTERP(FONCTION=SL,
                      LIST_PARA=LI2,
                      NOM_PARA='PCAP',
                      PROL_DROITE='CONSTANT',
                      PROL_GAUCHE='CONSTANT',
                      INFO=2,);
DSL = FORMULE(NOM_PARA='PCAP',VALE='- 0.999*(2.71E-8*(6.5E-8*PCAP)**0.49/((1+(6.5E-8*PCAP)**1.49)**1.33))');

DSATU=CALC_FONC_INTERP(FONCTION=DSL,
                       LIST_PARA=LI2,
                       NOM_PARA='PCAP',
                       PROL_DROITE='CONSTANT',
                       PROL_GAUCHE='CONSTANT',
                       INFO=2,);
#
# PERMEABILITE INTRINSEQUE, RELATIVE DU LIQUIDE, RELATIVE DU GAZ
# UNITE INTRINSEQUE : METRES CARRES , UNITE RELATIVE : SANS
#
KINT=DEFI_CONSTANTE(    VALE=1.E-18)

PL = FORMULE(NOM_PARA='SAT',VALE='0.999*SAT**3');

PERML=CALC_FONC_INTERP(FONCTION=PL,
