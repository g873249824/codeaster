                        DDL_IMPO=_F(GROUP_NO='ORIGINE',
                                    DY=0.,),
                        FACE_IMPO=_F(GROUP_MA='BORD_DRO',
                                     DNOR=0.,),
                        PRES_REP=_F(GROUP_MA='BORD_GAU',
