                        DDL_IMPO=_F(GROUP_NO='ORIGINE',
                                    DX=0.,),
                        FACE_IMPO=_F(GROUP_MA='BORD_INF',
                                     DNOR=0.,),
                        PRES_REP=_F(GROUP_MA='BORD_SUP',
                                    PRES=10000.,),);

#
# 6.2. LES RESOLUTIONS
# 6.2.1. EN TRANSITOIRE

