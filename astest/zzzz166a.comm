# ----------------------------------------------------------------------
# CALCUL DE FLUX THERMIQUE
# MODELISATION : ELEMENTS THERMIQUE PLAN (QUAD4, TRIA3, QUAD8, QUAD9)
# ----------------------------------------------------------------------
#
DEBUT(CODE=_F(NOM='ZZZZ166A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
#
PRE_GIBI()
#
MA=LIRE_MAILLAGE( )
#
MAIL=CREA_MAILLAGE(MAILLAGE=MA,
                   MODI_MAILLE=_F(OPTION = 'QUAD8_9',
                                  GROUP_MA = 'SQU9')
                  )
#
MO=AFFE_MODELE(MAILLAGE=MAIL,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'THERMIQUE',
                       MODELISATION = 'PLAN'))
#
MAT=DEFI_MATERIAU(THER=_F(LAMBDA = 1.0,
                          RHO_CP = 0.0))
#
CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT = 'OUI',
                            MATER = MAT,
                            ))
#
#-----------------------------------------------------------------
# CHAMP DE TEMPERATURE LINEAIRE => FLUX CONSTANT
# ===============================================
FTEMPL = FORMULE(NOM_PARA=('X','Y'),VALE=' 2.*X +3.*Y')
#
CHTEMPL = CREA_CHAMP(OPERATION='AFFE',
                    TYPE_CHAM='NOEU_TEMP_F',
                    MAILLAGE=MAIL,
                    AFFE=_F(TOUT = 'OUI',
                            NOM_CMP = ('TEMP',),
                            VALE_F = (FTEMPL,))
                 )
#
RESUL = CREA_RESU(OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD = CHTEMPL,
                         INST = 1.0,
                         MODELE=MO,
                         CHAM_MATER=CHMAT)
                )
#
RESUL = CALC_ELEM(reuse=RESUL,
                 RESULTAT=RESUL,
                 OPTION=('FLUX_ELGA','FLUX_ELNO',)
                )
#
# TEST TEMPERATURE :
TEST_RESU(RESU=(_F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='A', PRECISION=1.0E-3, VALE=3.5),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='C', PRECISION=1.0E-3, VALE=11.5),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='J', PRECISION=1.0E-3, VALE=17.0),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='L', PRECISION=1.0E-3, VALE=25.0))
          )
#
# TEST FLUX_ELGA :
TEST_RESU(RESU=(_F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUX',
                   MAILLE='M1', POINT=1,
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUY',
                   MAILLE='M1', POINT=1,
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUX',
                   MAILLE='M420', POINT=1,
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUY',
                   MAILLE='M420', POINT=1,
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUX',
                   MAILLE='M1181', POINT=1,
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUY',
                   MAILLE='M1181', POINT=1,
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUX',
                   MAILLE='M2000', POINT=1,
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELGA', NOM_CMP='FLUY',
                   MAILLE='M2000', POINT=1,
                   PRECISION=1.0E-3, VALE=-3.))
          )
#
# TEST FLUX_ELNO :
TEST_RESU(RESU=(_F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M1', GROUP_NO='A',
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M1', GROUP_NO='A',
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M420', GROUP_NO='C',
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M420', GROUP_NO='C',
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M1181', GROUP_NO='J',
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M1181', GROUP_NO='J',
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M2000', GROUP_NO='L',
                   PRECISION=1.0E-3, VALE=-2.),
                _F(RESULTAT=RESUL, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M2000', GROUP_NO='L',
                   PRECISION=1.0E-3, VALE=-3.))
          )
#
#
#-----------------------------------------------------------------
# CHAMP DE TEMPERATURE QUADRATIQUE => FLUX LINEAIRE
# ==================================================
FTEMPQ = FORMULE(NOM_PARA=('X','Y'),VALE='2.*X*X +3.*Y*Y')
#
CHTEMPQ = CREA_CHAMP(OPERATION='AFFE',
                    TYPE_CHAM='NOEU_TEMP_F',
                    MAILLAGE=MAIL,
                    AFFE=_F(TOUT = 'OUI',
                            NOM_CMP = ('TEMP',),
                            VALE_F = (FTEMPQ,))
                 )
#
RESUQ = CREA_RESU(OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD = CHTEMPQ,
                         INST = 1.0,
                         MODELE=MO,
                         CHAM_MATER=CHMAT)
                )
#
RESUQ = CALC_ELEM(reuse=RESUQ,
                 RESULTAT=RESUQ,
                 OPTION=('FLUX_ELGA','FLUX_ELNO',)
                )
#
# TEST TEMPERATURE :
TEST_RESU(RESU=(_F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='A', PRECISION=1.0E-3, VALE=2.75),
#
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='C', PRECISION=1.0E-3, VALE=50.75),
#
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='J', PRECISION=1.0E-3, VALE=77.0),
#
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='TEMP', NOM_CMP='TEMP',
                   GROUP_NO='L', PRECISION=1.0E-3, VALE=125.0))
          )
#
# TEST FLUX_ELNO :
TEST_RESU(RESU=(_F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M1', GROUP_NO='A',
                   PRECISION=1.0E-3, VALE=-4.),
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M1', GROUP_NO='A',
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M420', GROUP_NO='C',
                   PRECISION=1.0E-3, VALE=-20.),
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M420', GROUP_NO='C',
                   PRECISION=1.0E-3, VALE=-3.),
#
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M1181', GROUP_NO='J',
                   PRECISION=5.5E-2, VALE=-4.),
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M1181', GROUP_NO='J',
                   PRECISION=1.5E-2, VALE=-30.),
#
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUX',
                   MAILLE='M2000', GROUP_NO='L',
                   PRECISION=1.5E-2, VALE=-20.),
                _F(RESULTAT=RESUQ, INST=1., REFERENCE='ANALYTIQUE',
                   NOM_CHAM='FLUX_ELNO', NOM_CMP='FLUY',
                   MAILLE='M2000', GROUP_NO='L',
                   PRECISION=1.5E-2, VALE=-30.))
          )
#
FIN( )
#
