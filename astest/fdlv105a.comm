#

#       RESOLUTION D UN LAPLACIEN DANS UNE LAME DE

#FLUIDE+CALCUL D'UNE MATR_ASSE_GENE DE MASSE AJOUTEE POUR

#                        DEUX CYLINDRES

#                          COAXIAUX

#                       FAIT DE D_PLAN

#                          CALCUL 2D

#                      CALCUL MODAL EN EAU

#                       CAS TEST FDLV101A

#

DEBUT(CODE=_F( NOM = 'FDLV105A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAYA=LIRE_MAILLAGE(FORMAT='MED', )

EAU=DEFI_MATERIAU( THER=_F( LAMBDA = 1., RHO_CP = 1000.))

ACIER=DEFI_MATERIAU( ELAS=_F( RHO = 7800.,  NU = 0.3, E = 2.E13))

CHAMMAT1=AFFE_MATERIAU(  MAILLAGE=MAYA,AFFE=(
                       _F( GROUP_MA = 'FLUIDE', MATER = EAU),
                       _F( GROUP_MA = 'INTERFAC', MATER = EAU))

                            )

CHAMMAT2=AFFE_MATERIAU(  MAILLAGE=MAYA,
                       AFFE=_F( GROUP_MA = 'STRUCTUR', MATER = ACIER)

                            )

#

FLUIDE=AFFE_MODELE( MAILLAGE=MAYA,AFFE=(
                     _F( GROUP_MA = 'FLUIDE',
                MODELISATION = 'AXIS',  PHENOMENE = 'THERMIQUE'),
                     _F( GROUP_MA = 'INTERFAC',
                MODELISATION = 'AXIS',  PHENOMENE = 'THERMIQUE'))

                          )

INTERF=AFFE_MODELE( MAILLAGE=MAYA,
                     AFFE=_F( GROUP_MA = 'INTERFAC',
                MODELISATION = 'AXIS',  PHENOMENE = 'THERMIQUE')
                          )

STRUCT=AFFE_MODELE( MAILLAGE=MAYA,AFFE=(
                     _F( GROUP_MA = 'STRUCTUR',
                MODELISATION = 'AXIS',  PHENOMENE = 'MECANIQUE'),
                            _F( GROUP_MA = 'RESSORTS',
                MODELISATION = '2D_DIS_T',  PHENOMENE = 'MECANIQUE'))

                          )

#

CHARGE=AFFE_CHAR_THER( MODELE=FLUIDE,
                    TEMP_IMPO=_F( GROUP_NO = 'TEMPIMPO', TEMP = 0.)
                     )

#
# MISE EN PLACE D UN RESSORT PAR RADIAN
#

CHAMPCAR=AFFE_CARA_ELEM( MODELE=STRUCT,
                 DISCRET_2D=(_F( GROUP_MA = 'RESSORTS', CARA = 'K_T_N',
                                VALE = (1., 0., 15915.5,), ),
                             _F( GROUP_MA = 'RESSORTS', CARA = 'M_T_N',
                                VALE = (0., 0., 0.,), ))
                 )

#

CHARGS=AFFE_CHAR_MECA( MODELE=STRUCT,
            DDL_IMPO=_F( GROUP_NO = 'NOSTR',   DX = 0.)
                      )

MEL_KSTR=CALC_MATR_ELEM( MODELE=STRUCT,
                               CARA_ELEM=CHAMPCAR,
                              CHAM_MATER=CHAMMAT2,  OPTION='RIGI_MECA',
                               CHARGE=(CHARGS, )
                               )

MEL_MSTR=CALC_MATR_ELEM( MODELE=STRUCT,
                               CARA_ELEM=CHAMPCAR,
                              CHAM_MATER=CHAMMAT2,  OPTION='MASS_MECA',
                              CHARGE=(CHARGS, )
                              )

NUSTR=NUME_DDL(MATR_RIGI=MEL_KSTR)

MATASKS=ASSE_MATRICE( MATR_ELEM=MEL_KSTR,  NUME_DDL=NUSTR)

MATASMS=ASSE_MATRICE( MATR_ELEM=MEL_MSTR,  NUME_DDL=NUSTR)

CHAMNO=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_DEPL_R',
                   MAILLAGE=MAYA,
                       AFFE=_F(  GROUP_NO = 'NOSTR',
                              NOM_CMP = ('DX', 'DY',),
                              VALE = (0., 1.,)),
                       NUME_DDL=NUSTR, PROL_ZERO='OUI' )

#


MODES=MODE_ITER_SIMULT( MATR_RIGI=MATASKS,  MATR_MASS=MATASMS,
                               OPTION='SANS',
                             CALC_FREQ=_F( OPTION = 'BANDE',
                                        FREQ = (1.,500.,))
                            )

NUMGEN=NUME_DDL_GENE(  BASE=MODES,
                            STOCKAGE='PLEIN'
                          )

MATRAJ=CALC_MATR_AJOU( MODELE_FLUIDE=FLUIDE,
                         MODELE_INTERFACE=INTERF,
                        OPTION='MASS_AJOU',
                         NUME_DDL_GENE=NUMGEN,
                        CHARGE=CHARGE,
                        CHAM_MATER=CHAMMAT1,
                         MODE_MECA=MODES)

MGENE=PROJ_MATR_BASE( BASE=MODES,
                         NUME_DDL_GENE=NUMGEN,
                         MATR_ASSE=MATASMS)

RIGGEN=PROJ_MATR_BASE( BASE=MODES,
                          NUME_DDL_GENE=NUMGEN,
                         MATR_ASSE=MATASKS)

MASTOT=COMB_MATR_ASSE( COMB_R=(  _F( MATR_ASSE = MGENE,
                                     COEF_R = 1.),
                                     _F( MATR_ASSE = MATRAJ,
                                      COEF_R = 1.)))

MODHUMI=MODE_ITER_SIMULT( MATR_RIGI=RIGGEN,  MATR_MASS=MASTOT,
                         SOLVEUR=_F(METHODE='LDLT'),
                             CALC_FREQ=_F( OPTION = 'BANDE',
                                        FREQ = (1.,100.,))
                            )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='FREQ',
                  RESULTAT=MODHUMI,
                  VALE_CALC=27.380409260,
                  VALE_REFE=27.25,
                  REFERENCE='NON_DEFINI',
                  CRITERE='RELATIF',
                  PRECISION=0.014999999999999999,),
          )

FIN()
#
