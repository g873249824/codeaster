DEBUT(CODE=_F(NOM='HSNV100I', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#......................................................................
# CAS TEST THERMOPLASTICITE ELEMENTAIRE
#......................................................................
# DEFINITION DES CARACTERISTIQUES DU MATERIAU
#

MAIL=LIRE_MAILLAGE( )

# -----------------------------------------------------------------------------
# DONNEES DE MODELISATION
# -----------------------------------------------------------------------------

FCT1=DEFI_FONCTION(   NOM_PARA='EPSI',  PROL_DROITE='LINEAIRE',
                                          PROL_GAUCHE='LINEAIRE',
                      VALE=(
                             0.200E-2,      400.,
                             0.400E-2,      500.,)
                                                    )

#

MAT=DEFI_MATERIAU(    ELAS=_F( E = 200.E+3, NU = .3,ALPHA = 10.0E-6),
                      TRACTION=_F( SIGM = FCT1), )

#

CM=AFFE_MATERIAU(
                      MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI',MATER = MAT), )

#

L_INST=DEFI_LIST_REEL(
                      DEBUT=0.,INTERVALLE=(
                      _F( JUSQU_A = 66.666E0,   NOMBRE = 1),
                      _F( JUSQU_A = 80.000E0,   NOMBRE = 2),
                      _F( JUSQU_A = 90.000E0,   NOMBRE = 2))
         )

#

MOMECA=AFFE_MODELE(  MAILLAGE=MAIL,
                     AFFE=_F( TOUT = 'OUI', MODELISATION = 'C_PLAN', PHENOMENE = 'MECANIQUE'))

#


CHMECA=AFFE_CHAR_MECA(
            MODELE=MOMECA,DDL_IMPO=(
           _F( NOEUD = 'N1',     DX = 0.),
           _F( GROUP_NO = 'GRNO1',  DY = 0.),
           _F( GROUP_NO = 'GRNO3',  DY = 0.)),)

# -----------------------------------------------------------------------------
# CHAMP DE CONTRAINTES IMPOSEES
# -----------------------------------------------------------------------------
EPS0=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',INFO=2,
                OPERATION='AFFE',
                MODELE=MOMECA,
                AFFE=_F(
                        #TOUT='OUI',
                        GROUP_MA='GRSD1',
                        NOM_CMP=('EPXX','EPYY','EPZZ','EPXY',),
                        VALE=(0.,0.,0.,0.),),);

EPS1=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',INFO=2,
                OPERATION='AFFE',
                MODELE=MOMECA,
                AFFE=(
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXX','EPYY','EPZZ',),
                         VALE=(6.6666E-4,6.6666E-4,6.6666E-4,)),
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY',),
                         VALE=(0.),),),);

EPS2=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',
                OPERATION='AFFE',
                MODELE=MOMECA,
                AFFE=(
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXX','EPYY','EPZZ',),
                         VALE=(7.3333E-4,7.3333E-4,7.3333E-4),),
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY',),
                         VALE=(0.),),),);

EPS3=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',
                OPERATION='AFFE',
                MODELE=MOMECA,
                AFFE=(
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXX','EPYY','EPZZ',),
                         VALE=(8.E-4,8.E-4,8.E-4,),),
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY',),
                         VALE=(0.),),),);

EPS4=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',
                OPERATION='AFFE',
                MODELE=MOMECA,
                AFFE=(
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXX','EPYY','EPZZ',),
                         VALE=(8.5E-4,8.5E-4,8.5E-4,)),
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY'),
                         VALE=(0.),),),);

EPS5=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',
                OPERATION='AFFE',
                MODELE=MOMECA,
                AFFE=(
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXX','EPYY','EPZZ',),
                         VALE=(9.E-4,9.E-4,9.E-4,),),
                      _F(#TOUT='OUI',
                        GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY',),
                         VALE=(0.),),),);

EPSA=CREA_RESU(OPERATION='AFFE',
               TYPE_RESU='EVOL_VARC',
               NOM_CHAM='EPSA_ELNO',
               AFFE=(
                     _F(CHAM_GD=EPS0,  INST=0.,),
                     _F(CHAM_GD=EPS1,  INST=6.6666E+1,),
                     _F(CHAM_GD=EPS2,  INST=7.3333E+1,),
                     _F(CHAM_GD=EPS3,  INST=80.,),
                     _F(CHAM_GD=EPS4,  INST=85.,),
                     _F(CHAM_GD=EPS5,  INST=90,),),);


CTM=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          MATER=MAT,),
                  AFFE_VARC=(_F(TOUT='OUI',
                                EVOL=EPSA,
                                NOM_VARC='EPSA',
                                NOM_CHAM='EPSA_ELNO',),
                             _F(TOUT='OUI',
                                NOM_VARC='TEMP',
                                VALE_REF=0.),),);


# -----------------------------------------------------------------------------
# RESOLUTION
# -----------------------------------------------------------------------------

U=STAT_NON_LINE(MODELE=MOMECA,
                CHAM_MATER=CTM,
                EXCIT=_F(CHARGE=CHMECA,),
                COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_RELA=1.E-5,
                               ITER_GLOB_MAXI=10,),);



# -----------------------------------------------------------------------------
# TESTS
# -----------------------------------------------------------------------------

#  TESTS DES CONTRAINTES/DEFORMATIONS

U=CALC_ELEM(reuse =U,
            RESULTAT=U,
            OPTION=(
            'SIEF_ELNO_ELGA',),);

TEST_RESU(RESU=(
          _F(  RESULTAT = U,   NOM_CHAM = 'SIEF_ELNO_ELGA',  NUME_ORDRE = 1,
            MAILLE = 'M1',NOEUD = 'N1',NOM_CMP = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -133.333),
          _F(  RESULTAT = U,   NOM_CHAM = 'SIEF_ELNO_ELGA',  NUME_ORDRE = 3,
            MAILLE = 'M1',NOEUD = 'N2',NOM_CMP = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -160.),
          _F(  RESULTAT = U,   NOM_CHAM = 'SIEF_ELNO_ELGA',  NUME_ORDRE = 5,
            MAILLE = 'M1',NOEUD = 'N3',NOM_CMP = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -180.00),
          _F(  RESULTAT = U,   NOM_CHAM = 'SIEF_ELNO_ELGA',  NUME_ORDRE = 5, CRITERE='ABSOLU',PRECISION=1.E-6,
            MAILLE = 'M1',NOEUD = 'N3',NOM_CMP = 'SIZZ',REFERENCE='ANALYTIQUE', VALE = 0.00),
            ));


           
FIN()
#
#  TESTS DES ENERGIES

U=CALC_ELEM( reuse=U, OPTION=( 'ETOT_ELGA','ENEL_ELGA'),

                          RESULTAT=U, )


TEST_RESU(RESU=(
          _F(  RESULTAT = U,  NOM_CHAM = 'ENEL_ELGA',  NUME_ORDRE = 1,REFERENCE='ANALYTIQUE',
            MAILLE = 'M1', POINT=1, NOM_CMP = 'TOTALE', VALE = 4.4444E-02),),)

T_TOT=POST_ELEM(
                    RESULTAT=U,
                      CHARGE=CHMECA,
                    ENER_TOTALE=_F( TOUT = 'OUI',)  )

TEST_TABLE ( TABLE=T_TOT,
             FILTRE=_F(NOM_PARA='NUME_ORDRE',
                       VALE_I=1),
             NOM_PARA='TOTALE',
             REFERENCE='ANALYTIQUE',
             VALE=1.7777E-01  )
