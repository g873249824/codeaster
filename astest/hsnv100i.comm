                  AFFE=_F(TOUT='OUI',
                          MATER=MAT,),
                  AFFE_VARC=(_F(TOUT='OUI',
                                EVOL=EPSA,
                                NOM_VARC='EPSA',
                                NOM_CHAM='EPSA_ELNO',),
                             ),);

