                      _F(GROUP_MA='GRSD1',
                         NOM_CMP=('EPXX','EPYY','EPZZ',),
                         VALE=(8.E-4,8.E-4,8.E-4,),),
                      _F(GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY'),
                         VALE=(0.),),),);

EPS4=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',
                OPERATION='AFFE',
                MODELE=MOMECA,
                AFFE=(
                      _F(GROUP_MA='GRSD1',
                         NOM_CMP=('EPXX','EPYY','EPZZ',),
                         VALE=(8.5E-4,8.5E-4,8.5E-4,)),
                      _F(GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY'),
                         VALE=(0.),),),);

EPS5=CREA_CHAMP(TYPE_CHAM='ELNO_EPSI_R',
                OPERATION='AFFE',
                MODELE=MOMECA,
