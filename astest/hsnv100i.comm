                      _F(GROUP_MA='GRSD1',
                         NOM_CMP=('EPXY'),
                         VALE=(0.),),),);

EPSA=CREA_RESU(OPERATION='AFFE',
               TYPE_RESU='EVOL_VARC',
               NOM_CHAM='EPSA_ELNO',
               AFFE=(
                     _F(CHAM_GD=EPS0,  INST=0.,),
                     _F(CHAM_GD=EPS1,  INST=6.6666E+1,),
                     _F(CHAM_GD=EPS2,  INST=7.3333E+1,),
                     _F(CHAM_GD=EPS3,  INST=80.,),
                     _F(CHAM_GD=EPS4,  INST=85.,),
                     _F(CHAM_GD=EPS5,  INST=90,),),);


