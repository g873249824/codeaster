#

CHAMPMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                                   AFFE=_F(  TOUT = 'OUI',
                             MATER = MAT)
                           )

#

COND_LIM=AFFE_CHAR_MECA(     MODELE=MODELE,DDL_IMPO=(
                               _F(  NOEUD = ('P1', 'P4',),
