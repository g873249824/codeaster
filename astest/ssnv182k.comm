DEBUT(CODE=_F(NOM='SSNV182K', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(_F(NOM='SURFQUAD',
                                     GROUP_MA='SURF',),
                                   _F(NOM='GN',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N141',
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=0.00001,),),
                                     );

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=(_F(GROUP_MA=('SURF',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),
                           _F(GROUP_MA=('LIG1','LIG3',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),),);


LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y-10');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='P2P1');


CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      FROTTEMENT     = 'COULOMB',
                      ITER_CONT_MAXI = 30,
                      REAC_GEOM      = 'SANS',
                      ZONE=(
                            _F(
                               FISS_MAIT    = FISS,
                               INTEGRATION  = 'FPG4',
                               CONTACT_INIT = 'OUI',
                               COEF_ECHELLE = 1.,
                               COULOMB      = 1.0,
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,    
                               ALGO_LAGR    = 'VERSION2',
                             ), 
                         ),
                   );
                     

E=100.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('SURF','LIG1','LIG3',),
                                MATER=ACIER,
                                ),);

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIG1',
                                          DX=0.0,
                                          DY=0.0,),
                                       _F(GROUP_MA='LIG3',
                                          DX=0.0,
                                          DY=-1.E-6,),),
                    INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CHXFEM,),                     
                          _F(CHARGE=CH1,),),
                   CONTACT  = CTXFEM,       
                   COMP_ELAS=_F(RELATION='ELAS',TOUT='OUI'),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=1.0,),
                   SOLVEUR=_F(METHODE='MUMPS',PCENT_PIVOT=10,
                              ),
                   NEWTON=_F(REAC_ITER=1,),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
#                             GROUP_NO='SURFQUAD',
                             GROUP_NO=('GN',),
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP=('LAGS_C','LAGS_F1',),
                             OPERATION='EXTRACTION',),);


VI=CREA_CHAMP(
                TYPE_CHAM='ELGA_VARI_R',
                OPERATION='EXTR',
                RESULTAT=UTOT1,
                NUME_ORDRE=1,
                NOM_CHAM='VARI_ELGA',);
                
IMPR_RESU(RESU=_F(CHAM_GD = VI))                


IMPR_TABLE(TABLE=LAG,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON',);

LAREF=-5.
LA1REF=0.
LA2REF=0.

# TESTS
TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MAX',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MAX',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-7,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MIN',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-7,
           REFERENCE='ANALYTIQUE',);


# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MODELE        = MODELEK,
                       INFO          = 2)

IMPR_RESU(RESU=_F(MAILLAGE=MA_XFEM))


MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        INFO          = 2)

IMPR_RESU(FORMAT='MED',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));

TEST_RESU(RESU=(_F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DX',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 0.0,
                     PRECISION=1.E-8,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.12'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DY',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 7.79E-04,
                     PRECISION=1.E-8,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.12'),
                ),
         );


FIN();

