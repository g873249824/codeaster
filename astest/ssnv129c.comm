CHA2=DEFI_CONTACT(MODELE         = MO,
                  FORMULATION    = 'DISCRETE',
                  REAC_GEOM      = 'AUTOMATIQUE',
                  ZONE=_F(
                          GROUP_MA_MAIT='Q1',
                          GROUP_MA_ESCL='Q21',
                          SANS_GROUP_NO=('DR1','DR2','DR3','DR4',
