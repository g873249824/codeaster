                           _F( NOEUD = 'O',        DX = 0.,  DY = 0.,
                                              DRX = 0., DRY = 0., DRZ = 0.)))

CHPR=AFFE_CHAR_MECA(MODELE=MODELE_1,
                   FORCE_COQUE=_F(
       GROUP_MA = 'TOUT',
      PRES = 1.) )

CHF3=AFFE_CHAR_MECA(MODELE=MODELE_1,
                   FORCE_COQUE=_F(
       GROUP_MA = 'TOUT',
        F3 = -1.) )

CHPE=AFFE_CHAR_MECA(MODELE=MODELE_1,
                      PESANTEUR=_F(GRAVITE= 10.,
                                   DIRECTION=(0., 0., -1., ), ),)

CARAC__1=AFFE_CARA_ELEM(   MODELE=MODELE_1,
                               COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.1,
                                  A_CIS = 0.83333333333333) )

DEP=MACRO_ELAS_MULT(MODELE=MODELE_1,
                    CHAM_MATER=CH_MAT_1,
                    CARA_ELEM=CARAC__1,
                    CHAR_MECA_GLOBAL=CINE,
                    CAS_CHARGE=(_F(NOM_CAS='CHPR',
                                   CHAR_MECA=CHPR,),
                                _F(NOM_CAS='CHF3',
                                   CHAR_MECA=CHF3,),
                                _F(NOM_CAS='CHPE',
                                   CHAR_MECA=CHPE,),
