DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# MODELIDATION A : FEM 2D
# Ce test correspond au test forma05a, sauf que l'on y decoupe le 
# chargement de Neumann en deux charges CLNEU2 et CLNEU2 qui vont 
# s'ajouter. On applique une FONC_MULT sur la deuxieme charge CLNEU2
# de telle sorte qu'a l'instant auquel on effectue les posts-traitements
# la combinaison de ces deux charges soit identique a la charge unique 
# de forma05a.

MA=DEFI_MATERIAU(ELAS=_F(E=210000e6,
                         NU=0.3))

MAIL=LIRE_MAILLAGE(FORMAT='MED')

MODE=AFFE_MODELE(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION= 'D_PLAN'))

MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_2D=_F(GROUP_MA='CD'))

# Barsoum
MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                  GROUP_NO_FOND='O'))

MATE=AFFE_MATERIAU(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           MATER=MA))

CLDIR=AFFE_CHAR_MECA(MODELE=MODE,
                     DDL_IMPO=(_F(GROUP_MA='OE',DY=0.),
                               _F(GROUP_NO='E', DX=0.)),)

CLNEU1=AFFE_CHAR_MECA(MODELE=MODE,
                      FORCE_CONTOUR=_F(GROUP_MA='CD',FY=5e6))

CLNEU2=AFFE_CHAR_MECA(MODELE=MODE,
                      FORCE_CONTOUR=_F(GROUP_MA='CD',FY=2.5e6))

LINST=DEFI_LIST_REEL(DEBUT=0.,
                     INTERVALLE=_F(JUSQU_A=1.,NOMBRE=1))

FMULT=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0., 0.,
                          1., 2.,),
                    PROL_DROITE='CONSTANT',);

RESU=MECA_STATIQUE(MODELE=MODE,
                   CHAM_MATER=MATE,
                   EXCIT=(
                          _F(CHARGE=CLDIR),
                          _F(CHARGE=CLNEU1),
                          _F(CHARGE=CLNEU2,FONC_MULT=FMULT),
                          ),
                   LIST_INST=LINST)

RESU=CALC_CHAMP(reuse=RESU,RESULTAT=RESU,CRITERES=('SIEQ_ELNO'),CONTRAINTE=('SIGM_ELNO'))


RESU=CALC_CHAMP(reuse =RESU,
             RESULTAT=RESU,
             CONTRAINTE='SIGM_NOEU',CRITERES='SIEQ_NOEU')

IMPR_RESU(FORMAT='MED',
          UNITE=80,
          RESU=_F(MAILLAGE=MAIL,
                  RESULTAT=RESU,
                  NOM_CHAM=('SIGM_NOEU','SIEQ_NOEU','DEPL')))

#----------------------------------------------------------------------------------
#                        fin du calcul mecanique
#----------------------------------------------------------------------------------

# post-traitements a completer
FISS=DEFI_FOND_FISS(MAILLAGE=MAIL,
                    SYME='OUI',
                    FOND_FISS=_F(GROUP_NO='O'),
                    LEVRE_SUP=_F(GROUP_MA='GO'))

RSUP=0.005 * 5
RINF=0.005 * 2


# CALC_G
G=CALC_G(OPTION='CALC_G',
         RESULTAT=RESU,
         THETA=_F(FOND_FISS=FISS,
                  R_INF=RINF,
                  R_SUP=RSUP,
                  ),
         )

IMPR_TABLE(TABLE=G)

GK=CALC_G(OPTION='CALC_K_G',
          RESULTAT=RESU,
          THETA=_F(FOND_FISS=FISS,
                   R_INF=RINF,
                   R_SUP=RSUP,
                   ),
          )

IMPR_TABLE(TABLE=GK),


# POST_K1_K2_K3
K=POST_K1_K2_K3(RESULTAT=RESU,
                FOND_FISS=FISS,)

IMPR_TABLE(TABLE=K )

# tests sur G
Gref = 192

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=3.0000000000000001E-3,
           VALE_CALC=192.511256986,
           VALE_REFE=192,
           NOM_PARA='G',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.,),
           TABLE=G,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=3.0000000000000001E-3,
           VALE_CALC=192.511256986,
           VALE_REFE=192,
           NOM_PARA='G',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.,),
           TABLE=GK,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=3.0000000000000001E-3,
           VALE_CALC=192.527315126,
           VALE_REFE=192,
           NOM_PARA='G_IRWIN',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.,),
           TABLE=GK,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.012,
           VALE_CALC=193.008786982,
           VALE_REFE=192,
           NOM_PARA='G',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.,),
           TABLE=K,)

# tests sur K1
K1ref = 6.65e6

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=3.0000000000000001E-3,
           VALE_CALC=6665536.76861,
           VALE_REFE=6.650000E6,
           NOM_PARA='K1',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.,),
           TABLE=GK,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=5.0000000000000001E-3,
           VALE_CALC=6673866.14366,
           VALE_REFE=6.650000E6,
           NOM_PARA='K1',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.,),
           TABLE=K,)

FIN()
