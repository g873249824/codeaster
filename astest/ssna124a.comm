#  PARTIE IRRADIATION ET HAUTE TEMPERATURE
#


DEBUT(CODE=_F(NOM='SSNA124A',NIV_PUB_WEB='INTERNET'),);

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=DEFI_GROUP(reuse =MAIL, MAILLAGE=MAIL, CREA_GROUP_NO=_F(GROUP_MA='GR1',),);
MAIL=DEFI_GROUP(reuse =MAIL, MAILLAGE=MAIL, CREA_GROUP_NO=_F(GROUP_MA='GR2',),);

MODEL=AFFE_MODELE(
   MAILLAGE=MAIL,
   AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='AXIS',),
);

# DEFINITION DU MATERIAU
YOUNG1 = FORMULE(NOM_PARA='TEMP', VALE='210000.0 - 30.0*TEMP', );
NU1    = FORMULE(NOM_PARA='TEMP', VALE='0.30 + 5.0E-05*TEMP', );
ALPHA1 = FORMULE(NOM_PARA='TEMP', VALE='1.0E-06*(15.0+2.0E-03*TEMP)',);
R021   = FORMULE(NOM_PARA=('TEMP','IRRA',),
    VALE='(270.0-0.65*TEMP+0.001*TEMP**2)*3.0*(2.0-exp(-IRRA/3.0))', );

RM1 = FORMULE(NOM_PARA=('TEMP','IRRA',),
    VALE='''R021(TEMP,IRRA)+((600.0-1.5*TEMP+0.010*TEMP**2)
            -(270.00-0.65*TEMP+0.001*TEMP**2))*0.50*
            (0.25-0.10*(1.0-exp(-IRRA/10.0))+exp(-IRRA/2.0))''', );

EU1 = FORMULE(NOM_PARA=('TEMP','IRRA',),
    VALE='''log(1.0+((50.0-0.15*TEMP+0.0007*TEMP**2)*0.25*exp(-IRRA/2.0))*1.0E-02)''', );


TEM=DEFI_LIST_REEL(
   DEBUT= 0.0,
   INTERVALLE=_F(JUSQU_A=380.0, PAS=20.0,),
)
TEI=DEFI_LIST_REEL(
   DEBUT= 0.0,
   INTERVALLE=_F(JUSQU_A= 10.0, PAS=0.05,),
);

# DEFINITION DU MATERIAUX : SANS GONFLEMENT
# POUR GAGNER DU TEMPS DANS LE CALCUL ON INTERPOLE LA FONCTION SUR LA LISTE D
# INSTANT DISCRETISE
YOUNG=CALC_FONC_INTERP(FONCTION=YOUNG1,LIST_PARA=TEM,
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
)
NU=CALC_FONC_INTERP(FONCTION=NU1,LIST_PARA=TEM,
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
)
ALPHA=CALC_FONC_INTERP(FONCTION=ALPHA1,LIST_PARA=TEM,
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
)

R02=CALC_FONC_INTERP(FONCTION=R021,
   LIST_PARA=TEM,NOM_PARA='TEMP',
   LIST_PARA_FONC=TEI,NOM_PARA_FONC='IRRA',
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
);
RM=CALC_FONC_INTERP(FONCTION=RM1,
   LIST_PARA=TEM,NOM_PARA='TEMP',
   LIST_PARA_FONC=TEI,NOM_PARA_FONC='IRRA',
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
);
EU=CALC_FONC_INTERP(FONCTION=EU1,
   LIST_PARA=TEM,NOM_PARA='TEMP',
   LIST_PARA_FONC=TEI,NOM_PARA_FONC='IRRA',
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
);

UN   =DEFI_CONSTANTE(VALE = 1.0)
ZERO =DEFI_CONSTANTE(VALE = 0.0)

ACIER=DEFI_MATERIAU(
   ELAS_FO=_F(E=YOUNG, NU=NU, ALPHA=ALPHA, TEMP_DEF_ALPHA=0., ),
   IRRAD3M=_F(R02=R02, RM=RM, EPSI_U=EU, KAPPA=0.8, AI0=2.2E-6,
              ETAI_S=700.0, ZETA_F=UN, RG0=0.0, ALPHA=0.0, PHI0=0.0,
              ZETA_G=ZERO,TOLER_ET=1.0E-02,),
);

IRRA_0=CREA_CHAMP(
   TYPE_CHAM='NOEU_IRRA_R', OPERATION='AFFE', MAILLAGE=MAIL,
   AFFE=_F( TOUT='OUI', NOM_CMP='IRRA', VALE=0.,)
);

IRRA_1=CREA_CHAMP(
   TYPE_CHAM='NOEU_IRRA_R', OPERATION='AFFE', MAILLAGE=MAIL,
   AFFE=_F( TOUT='OUI', NOM_CMP='IRRA', VALE=10.,),
)

IRRA=CREA_RESU(
   OPERATION='AFFE', TYPE_RESU='EVOL_VARC', NOM_CHAM='IRRA',
   AFFE=(
      _F(CHAM_GD=IRRA_0, INST=0.0,),
      _F(CHAM_GD=IRRA_0, INST=1.0,),
      _F(CHAM_GD=IRRA_0, INST=2.0,),
      _F(CHAM_GD=IRRA_1, INST=3.0,),
   ),
)

CL=AFFE_CHAR_MECA(
   MODELE=MODEL,
   DDL_IMPO=_F(GROUP_MA='AB', DX=0.,DY=0.,),
)


TEMP_0=CREA_CHAMP(
   TYPE_CHAM='NOEU_TEMP_R', OPERATION='AFFE', MAILLAGE=MAIL,
   AFFE=_F(TOUT='OUI', NOM_CMP='TEMP', VALE=20.0,),
)

TEMP_1=CREA_CHAMP(
   TYPE_CHAM='NOEU_TEMP_R', OPERATION='AFFE', MAILLAGE=MAIL,
   AFFE=_F(TOUT='OUI', NOM_CMP='TEMP', VALE=360.0,),
)

LISTE=DEFI_LIST_REEL(
   DEBUT=0.,
   INTERVALLE=(
      _F(JUSQU_A=1.0, NOMBRE=  4,),
      _F(JUSQU_A=2.0, NOMBRE=  8,),
      _F(JUSQU_A=2.1, NOMBRE= 40,),
      _F(JUSQU_A=2.3, NOMBRE= 80,),
      _F(JUSQU_A=3.0, NOMBRE= 28,),
   ),
);

TEMPE=CREA_RESU(
   OPERATION='AFFE',
   TYPE_RESU='EVOL_THER',
   NOM_CHAM='TEMP',
   AFFE=(
      _F(CHAM_GD=TEMP_0, INST=0.0,),
      _F(CHAM_GD=TEMP_0, INST=1.0,),
      _F(CHAM_GD=TEMP_1, INST=2.0,),
      _F(CHAM_GD=TEMP_1, INST=3.0,),
   ),
);


CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
   AFFE=_F(TOUT='OUI', MATER=ACIER,),
   AFFE_VARC=(
      _F(TOUT='OUI', EVOL=IRRA, NOM_VARC='IRRA',),
      _F(TOUT='OUI', EVOL=TEMPE, NOM_VARC='TEMP',VALE_REF=20.0),
   ),
)

# CONDITIONS AUX LIMITES
CL1=AFFE_CHAR_MECA(MODELE=MODEL, DDL_IMPO=_F(GROUP_MA='ED', DY=1.0,),);
CL2=AFFE_CHAR_MECA(MODELE=MODEL, FORCE_CONTOUR=_F(GROUP_MA='ED', FY=260.0,),);
CL3=AFFE_CHAR_MECA(MODELE=MODEL, LIAISON_UNIF=_F(GROUP_NO='GR1',DDL='DY',),);

FONC1=DEFI_FONCTION(
   NOM_PARA='INST',
   VALE=(0., 0., 1.,0.003771,2.,0.017253,3.,0.017253),
   PROL_DROITE='EXCLU',
   PROL_GAUCHE='EXCLU',
)
FONC2=DEFI_FONCTION(
   NOM_PARA='INST',
   VALE=(0., 0., 1.,1.,2.,1.,3.,1.),
   PROL_DROITE='EXCLU',
   PROL_GAUCHE='EXCLU',
)

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = LISTE),
                        ECHEC=_F(ACTION='DECOUPE',
                                 SUBD_METHODE='MANUEL',
                                 SUBD_PAS=4,
                                 SUBD_NIVEAU=6,),)

# PREMIER CALCUL CORRESPONDANT A UNE CHARGEMENT EN PRESSION
# REPONSE DE LA STRUCTURE EN FLUAGE
STAT1=STAT_NON_LINE(
    MODELE=MODEL,
    CHAM_MATER=CHMAT,
    EXCIT=(_F(CHARGE=CL,),
            _F(CHARGE=CL2,FONC_MULT=FONC2,),
            _F(CHARGE=CL3),),
    NEWTON=_F(REAC_INCR=1,
                MATRICE='TANGENTE',
                PAS_MINI_ELAS=0.0,
                REAC_ITER=1,
                REAC_ITER_ELAS=0),
    COMP_INCR=_F(RELATION='IRRAD3M',ITER_INTE_MAXI=120),
    CONVERGENCE=_F(ITER_GLOB_MAXI=50, RESI_GLOB_MAXI=1.0E-07),
    INCREMENT=_F(LIST_INST=DEFLIST),
)

STAT1=CALC_CHAMP(reuse=STAT1,RESULTAT=STAT1,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'))

STAT1=CALC_CHAMP(reuse=STAT1,RESULTAT=STAT1,VARI_INTERNE='VARI_NOEU')

# DEPLACEMENT ET VARIABLES INTERNES
TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.0,
    RESULTAT=STAT1,
    NOM_CHAM='DEPL',
    NOM_CMP='DY',
    VALE_CALC=0.014006796167121,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.10,
    RESULTAT=STAT1,
    NOM_CHAM='DEPL',
    NOM_CMP='DY',
    VALE_CALC=0.014015790271879,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.5,
    RESULTAT=STAT1,
    NOM_CHAM='DEPL',
    NOM_CMP='DY',
    VALE_CALC=0.017588981885686,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=3.0,
    RESULTAT=STAT1,
    NOM_CHAM='DEPL',
    NOM_CMP='DY',
    VALE_CALC=0.022286704592895,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.0,
    RESULTAT=STAT1,
    NOM_CHAM='VARI_NOEU',
    NOM_CMP='V1',
    VALE_CALC=2.821554862468E-03,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.100,
    RESULTAT=STAT1,
    NOM_CHAM='VARI_NOEU',
    NOM_CMP='V1',
    VALE_CALC=2.821554862468E-03,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.100,
    RESULTAT=STAT1,
    NOM_CHAM='VARI_NOEU',
    NOM_CMP='V2',
    VALE_CALC=570.13919478171,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.5,
    RESULTAT=STAT1,
    NOM_CHAM='VARI_NOEU',
    NOM_CMP='V3',
    VALE_CALC=5.0694485884062E-03,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=3.0,
    RESULTAT=STAT1,
    NOM_CHAM='VARI_NOEU',
    NOM_CMP='V3',
    VALE_CALC=0.012718595151131,
    ),
)

# SECOND CALCUL CORRESPONDANT A UN DEPLACEMENT IMPOSE
# REPONSE DE LA STRUCTURE EN RELAXATION
STAT2=STAT_NON_LINE(
    MODELE=MODEL,
    CHAM_MATER=CHMAT,
    EXCIT=(_F(CHARGE=CL,),
            _F(CHARGE=CL1,FONC_MULT=FONC1,),),
    NEWTON=_F(REAC_INCR=1,
                MATRICE='TANGENTE',
                PAS_MINI_ELAS=0.0,
                REAC_ITER=1,
                REAC_ITER_ELAS=0),
    COMP_INCR=_F(RELATION='IRRAD3M',ITER_INTE_MAXI=120),
    CONVERGENCE=_F(ITER_GLOB_MAXI=50, RESI_GLOB_MAXI=1.0E-07),
    INCREMENT=_F(LIST_INST=DEFLIST,INST_FIN=2.5),
)

STAT2=CALC_CHAMP(reuse=STAT2,RESULTAT=STAT2,CONTRAINTE=('SIGM_ELNO'))

STAT2=CALC_CHAMP(reuse=STAT2,RESULTAT=STAT2,CONTRAINTE='SIGM_NOEU')

# CONTRAINTES
TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=1.0,
    RESULTAT=STAT2,
    NOM_CHAM='SIGM_NOEU',
    NOM_CMP='SIYY',
    VALE_CALC=568.2288888434,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.0,
    RESULTAT=STAT2,
    NOM_CHAM='SIGM_NOEU',
    NOM_CMP='SIYY',
    VALE_CALC=505.51587273767,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.10,
    RESULTAT=STAT2,
    NOM_CHAM='SIGM_NOEU',
    NOM_CMP='SIYY',
    VALE_CALC=503.41323753799,
    ),
)

TEST_RESU(RESU=_F(GROUP_NO='E',
    INST=2.5,
    RESULTAT=STAT2,
    NOM_CHAM='SIGM_NOEU',
    NOM_CMP='SIYY',
    VALE_CALC=105.18278150757,
    ),
)

FIN();
