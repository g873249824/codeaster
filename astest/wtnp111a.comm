                                GROUP_MA=('DROITE'),
                                SANS_GROUP_NO=('MIL_DROITE'),
                                NOM_CMP = ('PRE1',),
                                COEF_IMPO=FONCT1,
                                COEF_MULT=FONCT2,),);


MESTAT=STAT_NON_LINE(MODELE=MODELE,
                     CHAM_MATER=CHMATO,
                     EXCIT=(_F(CHARGE=CHAR0,),
                            _F(CHARGE=CHAR1,),
                                                   ),
                     COMP_INCR=_F(RELATION='KIT_HM',PARM_THETA=1.0,
