DEBUT(CODE=_F(NOM='WTNP111A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAILIN=LIRE_MAILLAGE();

MAIL = CREA_MAILLAGE(
                         MAILLAGE  = MAILIN,
                         LINE_QUAD = _F(TOUT = 'OUI'),
                       )

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,INFO=2,
                CREA_GROUP_NO=(_F(GROUP_MA='GAUCHE',),
                               _F(GROUP_MA='DROITE',),
                               _F(GROUP_MA='HAUT',),
                               _F(GROUP_MA='BAS',),
                                _F(NOM = 'SOM_BAS',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA = 'BAS'),
                                _F(NOM = 'SOM_HAUT',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA = 'HAUT'),
                                _F(NOM = 'SOM_GAUCHE',
                                 CRIT_NOEUD='SOMMET',
                                 GROUP_MA = 'GAUCHE'),
                                _F(NOM = 'MIL_DROITE',
                                 CRIT_NOEUD='MILIEU',
                                 GROUP_MA = 'DROITE'),
                                                       ))

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='D_PLAN_HM',),);

LI=DEFI_LIST_REEL(DEBUT=0.0,
                  INTERVALLE=(_F(JUSQU_A=20.0,
                                 NOMBRE=20,),
                                 ),);

viscoliq=DEFI_CONSTANTE(VALE=0.001,);

dviscoli=DEFI_CONSTANTE(VALE=0.0,);

KINT=DEFI_CONSTANTE(VALE=1.0E-19,);

FONCT1=DEFI_FONCTION(   NOM_PARA='INST',
                           VALE=( 0., 2000., 20., 0., ) )
FONCT2=DEFI_FONCTION(   NOM_PARA='INST',
                           VALE=( 0., 1., 20., 1., ) )


MAT=DEFI_MATERIAU(ELAS=_F(E=51000000.0,
                          NU=0.49,
                          RHO=2600.0,
                          ALPHA=0.0,),
                  COMP_THM='LIQU_SATU',
                  THM_INIT=_F(PRE1=0,
                              PORO=0.4,
                              TEMP=20.0,),
                  THM_DIFFU=_F(RHO=1960,
                               BIOT_COEF=1.0,
                               PESA_X=0.0,
                               PESA_Y=-9.81,
                               PESA_Z=0.0,
                               PERM_IN=KINT),
                  THM_LIQU=_F(RHO=1000.0,
                              UN_SUR_K=0.0,
                              VISC=viscoliq,
                              D_VISC_TEMP=dviscoli,),);

CHMATO=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=_F(TOUT='OUI',
                             MATER=MAT,),);

CHAR0=AFFE_CHAR_MECA(MODELE=MODELE,
                     DDL_IMPO=(
                               _F(GROUP_MA=('GAUCHE','DROITE','BAS','HAUT'),
                                  DX=0.0,DY=0.0,),
                               _F(GROUP_NO=('SOM_GAUCHE','SOM_BAS','SOM_HAUT'),
                                  PRE1=1000.0,),
                                                         ))

