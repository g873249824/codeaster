DEBUT(CODE=_F(NOM='TTNP01A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'))

MAILQ8=LIRE_MAILLAGE( )

MAILQ8=DEFI_GROUP( reuse=MAILQ8,        MAILLAGE=MAILQ8,
                          CREA_GROUP_NO=_F(  TOUT_GROUP_MA = 'OUI')
                      )

MAIL=CREA_MAILLAGE(       MAILLAGE=MAILQ8,
                        MODI_MAILLE=_F(  OPTION = 'QUAD8_9',
                                      TOUT = 'OUI')
                     )

CONDUC=DEFI_FONCTION(
       NOM_PARA='TEMP',
       VALE=(  -50.0,    1.08,
                50.00,   1.08, ),
         PROL_DROITE='CONSTANT',
        PROL_GAUCHE='CONSTANT'
      )

ENTHAL=DEFI_FONCTION(
       NOM_PARA='TEMP',
       VALE=(  -50.00,       0.000,
                -0.25,      49.750,
                -0.15,     120.010,
                50.00,     169.860,
              ),
         PROL_DROITE='LINEAIRE',
        PROL_GAUCHE='LINEAIRE'
      )

ALU=DEFI_MATERIAU( THER_NL=_F(  LAMBDA = CONDUC,
                                  BETA = ENTHAL) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = ALU) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = 'PLAN_DIAG',
                                      PHENOMENE = 'THERMIQUE'))

TM45=DEFI_FONCTION(     NOM_RESU='TEMP',  NOM_PARA='INST',
                            VALE=(    0.00,    0.0,
                                      0.05,  -45.0,
                                     10.00,  -45.0, )
                        )

CHTH=AFFE_CHAR_THER_F( MODELE=MOTH,TEMP_IMPO=(
          _F( GROUP_NO = 'LBC',  TEMP = TM45),
          _F( GROUP_NO = 'LCD',  TEMP = TM45))  )

TINIT=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R',
MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI', NOM_CMP = 'TEMP',
                                  VALE = 0.)
                       )

LR8=DEFI_LIST_REEL(        DEBUT=0.E0,INTERVALLE=(
       _F(    JUSQU_A = 5.E-3, NOMBRE = 10),
       _F(    JUSQU_A = 5.E-2, NOMBRE = 9),
       _F(    JUSQU_A = 5.E-1, NOMBRE = 9),
       _F(    JUSQU_A = 4.E-0, NOMBRE = 35))
                       )

LSAVE=DEFI_LIST_REEL(VALE=(1.0, 2.0, 3.0, 4.0),);

TEMPE=THER_NON_LINE( MODELE     = MOTH,
                     ETAT_INIT  = _F(  CHAM_NO = TINIT),
                     INCREMENT  = _F(  LIST_INST = LR8),
                     CHAM_MATER = CHMAT,
                     EXCIT      = _F(  CHARGE = CHTH),
                     NEWTON     = _F(  REAC_ITER = 1),
                     PARM_THETA = 0.9,
                     ARCHIVAGE  = _F( LIST_INST = LSAVE)
                 )

T_1=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP', INST=1.0 )

T_2=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP', INST=2.0 )

T_3=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP', INST=3.0 )

T_4=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP', INST=4.0 )

TEST_RESU(CHAM_NO=(

   _F( CHAM_GD = T_1, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -11.8, PRECISION = 1.8,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'),
   _F( CHAM_GD = T_2, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -29.9, PRECISION = 1.3,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'),
   _F( CHAM_GD = T_3, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -34.7, PRECISION = 0.8,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'),
   _F( CHAM_GD = T_4, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -36.7, PRECISION = 0.1,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'),

   _F( CHAM_GD = T_1, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -0.0, PRECISION = 0.3,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'),
   _F( CHAM_GD = T_2, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -18.1, PRECISION = 1.3,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'),
   _F( CHAM_GD = T_3, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -25.2, PRECISION = 0.4,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'),
   _F( CHAM_GD = T_4, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -29.3, PRECISION = 0.2,
    CRITERE = 'ABSOLU', REFERENCE = 'ANALYTIQUE'))

    )

TEST_RESU(CHAM_NO=(

   _F( CHAM_GD = T_1, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -13.50, PRECISION = 1.E-3,
    REFERENCE = 'NON_REGRESSION'),
   _F( CHAM_GD = T_2, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -28.64, PRECISION = 1.E-3,
    REFERENCE = 'NON_REGRESSION'),
   _F( CHAM_GD = T_3, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -33.97, PRECISION = 1.E-3,
    REFERENCE = 'NON_REGRESSION'),
   _F( CHAM_GD = T_4, NOEUD = 'N157', NOM_CMP = 'TEMP', VALE = -36.68, PRECISION = 1.E-3,
    REFERENCE = 'NON_REGRESSION'),

   _F( CHAM_GD = T_1, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -0.23, PRECISION = 1.E-2,
    REFERENCE = 'NON_REGRESSION'),
   _F( CHAM_GD = T_2, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -16.87, PRECISION = 1.E-3,
    REFERENCE = 'NON_REGRESSION'),
   _F( CHAM_GD = T_3, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -24.84, PRECISION = 1.E-3,
    REFERENCE = 'NON_REGRESSION'),
   _F( CHAM_GD = T_4, NOEUD = 'N198', NOM_CMP = 'TEMP', VALE = -29.42, PRECISION = 1.E-3,
    REFERENCE = 'NON_REGRESSION'))

    )

FIN()
