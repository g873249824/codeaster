# ---------------- fonctions ----------------------

# ---- f(deplacement)

UXP=RECU_FONCTION(RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='TOUTSEUL',
                  LIST_INST=L_CAST,
                  );

