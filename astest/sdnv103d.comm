DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED', )

MA=DEFI_GROUP(reuse=MA,MAILLAGE=MA,
              CREA_GROUP_NO=_F( TOUT_GROUP_MA = 'OUI'),
              )

MODE=AFFE_MODELE(
            MAILLAGE=MA,
            
            AFFE=(
           _F( TOUT='OUI',
               PHENOMENE = 'MECANIQUE',
               MODELISATION = ('AXIS','AXIS_SI')),),);

MA=MODI_MAILLAGE( reuse=MA,
             MAILLAGE=MA,
             ORIE_PEAU_2D=_F(  GROUP_MA = ('SZZ', 'RIGID')),
             INFO=2);

ACIER=DEFI_MATERIAU(
                ELAS=_F( E = 117.E3, NU = 0.35,  RHO = 8.930E-9),
                ECRO_LINE=_F(D_SIGM_EPSI = 100., SY = 400.));

CHMAT = AFFE_MATERIAU(
            MAILLAGE=MA,
            AFFE=(
              _F( GROUP_MA = ('VOL','VOL_RIG'), MATER = ACIER),),
                        );

CH=AFFE_CHAR_MECA(
            MODELE=MODE,
            DDL_IMPO=(
             _F( GROUP_MA = 'RIGID',      DX = 0., DY = 0.,),
             _F( GROUP_MA = 'VOL_RIG',    DX = 0., DY = 0.),
             _F( GROUP_MA = 'AXE',        DX = 0.),
             ),
               );

CHCONT  = DEFI_CONTACT(MODELE         = MODE,
                       FORMULATION    = 'DISCRETE',
                       REAC_GEOM      = 'CONTROLE',
                       NB_ITER_GEOM   = 2,
                       FROTTEMENT     = 'COULOMB',

                       ZONE =_F(
                             ALGO_CONT      = 'PENALISATION',
                             ALGO_FROT      = 'PENALISATION',
                             COULOMB        = 0.25,
                             E_N            = 1.E10,
                             E_T            = 1.E5,
                             APPARIEMENT    = 'MAIT_ESCL',
                             GROUP_MA_MAIT  = 'RIGID',
                             GROUP_MA_ESCL  = 'SZZ',
                             COEF_MATR_FROT = 0.4,
                               ),
                      );

VIT_0=CREA_CHAMP( OPERATION='AFFE',
                  TYPE_CHAM='NOEU_DEPL_R',

                  MODELE=MODE,
                  AFFE=(
                     _F(TOUT='OUI',
                        NOM_CMP = ('DX', 'DY'),
                        VALE = (  0.0,  -227000.,)),
                     _F(GROUP_NO = 'VOL_RIG',
                        NOM_CMP = ('DX', 'DY'),
                        VALE = (  0.0,  0.0,   )),
                     _F(GROUP_NO = 'RIGID',
                        NOM_CMP = ('DX', 'DY'),
                        VALE = (  0.0,  0.0, )),
                    ));

L_INST=DEFI_LIST_REEL(DEBUT=-1.E-5,INTERVALLE=(
                         _F( JUSQU_A = -1.E-7, NOMBRE = 1),
                         _F( JUSQU_A =  1.E-6, NOMBRE = 4),
                         _F( JUSQU_A = 80.E-6, NOMBRE = 158))
                         )

L_SAUV=DEFI_LIST_REEL(DEBUT=-1.E-5,INTERVALLE=(
                         _F( JUSQU_A = 1.E-6, NOMBRE = 2),
                         _F( JUSQU_A = 80.E-6, NOMBRE = 79))
                         )


RESU=DYNA_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CH,
                            ),
                   CONTACT  = CHCONT,
                   ETAT_INIT=_F(VITE = VIT_0,
                                ),
                   COMPORTEMENT=(_F(RELATION='VMIS_ISOT_LINE',
                                 DEFORMATION='SIMO_MIEHE',
                                 TOUT='OUI',),
                              _F(RELATION='ELAS',
                                 DEFORMATION='PETIT',
                                 GROUP_MA='VOL_RIG',),),

                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=2.E-05,
                                ),
                  SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
                                FORMULATION='DEPLACEMENT',
                                BETA = 0.49,   # Beta,
                                GAMMA = 0.9  ), # Gamma
                  NEWTON=_F(   REAC_INCR=1,MATR_RIGI_SYME='OUI',
                                MATRICE='TANGENTE',
                                REAC_ITER=1,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                  ITER_GLOB_MAXI=20,
                                  ARRET='OUI',),
                   SOLVEUR=_F(METHODE='LDLT',),
                   ARCHIVAGE=_F(
                                LIST_INST=L_SAUV),
                   );


VITE_A2=POST_RELEVE_T(ACTION=_F(INTITULE='VITE EN A2',
                                GROUP_NO= 'A2',
                                RESULTAT=RESU,
                                NOM_CHAM='VITE',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

DEPL_A1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN A1',
                                GROUP_NO= 'A1',
                                RESULTAT=RESU,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

DEPL_B1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN B1',
                                GROUP_NO= 'B1',
                                RESULTAT=RESU,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);


#####################################################
# TESTS DE DEPLACEMENTS, VITESSES ET ACCELERATIONS
#####################################################

TEST_RESU(RESU=(_F(GROUP_NO='B1',
                   INST=2.0E-05,
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.7282211126,
                   CRITERE='RELATIF',
                   ),
                _F(GROUP_NO='A2',
                   INST=2.0E-05,
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-6.76286780248,
                   CRITERE='RELATIF',
                   ),
                _F(GROUP_NO='B1',
                   INST=2.0E-05,
                   RESULTAT=RESU,
                   NOM_CHAM='VITE',
                   NOM_CMP='DX',
                   VALE_CALC=79802.7832042,
                   CRITERE='RELATIF',
                   ),
                _F(GROUP_NO='A2',
                   INST=2.0E-05,
                   RESULTAT=RESU,
                   NOM_CHAM='VITE',
                   NOM_CMP='DY',
                   VALE_CALC=-215713.835114,
                   CRITERE='RELATIF',
                   ),
                ),
          )

#####################################################
# TESTS SUR LA VALEUR DU JEU ET DU FROTTEMENT
#####################################################

TABJEU = POST_RELEVE_T (ACTION = _F(GROUP_NO='B1',
                                 INTITULE = 'MESSAGE',
                                 RESULTAT = RESU,
                                 TOUT_CMP='OUI',
                                 NOM_CHAM   = 'CONT_NOEU',
                                 OPERATION = 'EXTRACTION'))

IMPR_TABLE(TABLE=TABJEU)

# JEU
########
TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=0.0267987246573,
           NOM_PARA='JEU',
           TABLE=TABJEU,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=10,),
           )

TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=0.0772963409467,
           NOM_PARA='JEU',
           TABLE=TABJEU,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=20,),
           )

# RT
#######
TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.,
           NOM_PARA='R',
           TABLE=TABJEU,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=10,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.,
           NOM_PARA='R',
           TABLE=TABJEU,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=20,),
           )

FIN()
