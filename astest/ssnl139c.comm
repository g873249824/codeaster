DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
# lecture maillage lineique
MAPOU=LIRE_MAILLAGE(UNITE=20,)

# definition d'un groupe de 3 mailles pour valider EXTR_RESU / RESTREINT :
DEFI_GROUP(reuse=MAPOU, MAILLAGE=MAPOU,
    CREA_GROUP_MA=_F(NOM='GM1',MAILLE=('SEG2004','SEG2005','SEG2001',)))

# lecture maillage section grossiere
MASEC=LIRE_MAILLAGE(UNITE=19,)

MOPOU=AFFE_MODELE(MAILLAGE=MAPOU,
                  AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='POU_D_EM',),)

GFF=DEFI_GEOM_FIBRE(INFO=1,
                   SECTION=_F(MAILLAGE_SECT = MASEC , TOUT_SECT = 'OUI',
                               GROUP_FIBRE = 'BETON',
                               COOR_AXE_POUTRE = (0.05,0.1)),

                 )

POUCA=AFFE_CARA_ELEM(MODELE=MOPOU,    INFO=1,
# caracteristiques des sections, orientations et rigidite de torsion
           POUTRE=_F(GROUP_MA=('POUTRE'),
                     SECTION='RECTANGLE',
                     CARA = ('HY','HZ'),
                     VALE = (0.1,0.2)),
           ORIENTATION=_F(GROUP_MA=('POUTRE'),
                          CARA='ANGL_VRIL', VALE=90.0,),
           MULTIFIBRE = _F( GROUP_MA = 'POUTRE',
                              GROUP_FIBRE= ('BETON'),
                     PREC_INERTIE=1.1E-02,
                     PREC_AIRE=1.0E-06 ,),
           GEOM_FIBRE = GFF ,
                  )

# LES MATERIAUX

# DEFINIR ICI TOUTES LES VARIATIONS FONCTION DE LA TEMPERATURE SELON EUROCODE
NUB=0.25
RHOB=2400.
EB=10.E9

BETON=DEFI_MATERIAU(ELAS=_F(E=EB, NU=NUB, RHO=RHOB),
                   )


# MATERIAU POUR LES GROUPES DE FIBRES
MATFF=DEFI_COMPOR(GEOM_FIBRE=GFF,MATER_SECT=BETON,
                 MULTIFIBRE=(_F(GROUP_FIBRE='BETON',MATER=BETON,RELATION='ELAS',),),
)


# LES CHAMPS DE MATERIAUX
CHMAT=AFFE_MATERIAU(MAILLAGE=MAPOU,
                    AFFE=_F(GROUP_MA='POUTRE', MATER=(BETON)),
                    AFFE_COMPOR=_F(GROUP_MA='POUTRE',COMPOR=MATFF),
                         )

RAC2SUR2=sqrt(2.)/2.
BLOQUAGE=AFFE_CHAR_MECA(MODELE=MOPOU,
                        DDL_IMPO=(_F(GROUP_NO='A',  DX=0.0, DY=0.0, DZ=0.0,
                                                    DRY=0.,
                                     ),
                                  _F(GROUP_NO='B',  DY=0.0, ),
                                  ),
                                  # blocage de la rotation autour de l'axe de la poutre
                                  LIAISON_DDL=_F(NOEUD=('N001','N001'),DDL=('DRX','DRZ'),COEF_MULT=(RAC2SUR2,RAC2SUR2),COEF_IMPO=0.0)
                                  )
CHARG=AFFE_CHAR_MECA(MODELE=MOPOU,
                        DDL_IMPO=(
                                  _F(GROUP_NO='B',  DX= -0.01*RAC2SUR2, DZ=-0.01*RAC2SUR2 ),
                                  ),)
PERTU=AFFE_CHAR_MECA(MODELE=MOPOU,
                        FORCE_NODALE=(
                                  _F(GROUP_NO='C',  FY=100.0 , ),
                                  ),)
FOFO=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0. , 0.,
                         1. , 1.,
                         1.1, 1.)
                         )

LINST=DEFI_LIST_REEL(DEBUT=0.0,
INTERVALLE=(_F(JUSQU_A=1.1,  NOMBRE=11,),
           ),
            );


U1=STAT_NON_LINE(MODELE=MOPOU,
                 CHAM_MATER=CHMAT,
                 CARA_ELEM=POUCA,
                 EXCIT=(_F(CHARGE=BLOQUAGE,),
                        _F(CHARGE=CHARG,FONC_MULT=FOFO),
                        _F(CHARGE=PERTU),
                         ),
                 COMPORTEMENT=(_F(RELATION='MULTIFIBRE',DEFORMATION='GROT_GDEP',),
                               ),
                 INCREMENT=_F(LIST_INST=LINST,
                              ),
                 NEWTON=_F(MATRICE='TANGENTE',REAC_ITER=1),
                 CONVERGENCE=_F(RESI_GLOB_RELA=1.E-8,
                                ITER_GLOB_MAXI=100,),
                 );

U1=CALC_CHAMP(CONTRAINTE=('SIEF_ELNO','SIPM_ELNO'),reuse=U1,RESULTAT=U1)


TEST_RESU(RESU=(_F(INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='DEPL',
                   NOEUD='N006',
                   NOM_CMP='DY',
                   VALE_CALC=0.15668437975194,
                   VALE_REFE=0.15668437975193,
                   CRITERE='RELATIF',
                   PRECISION=1.0000000000000001E-05,
                   ),
                _F(PARA='ITER_GLOB',
                   VALE_CALC_I=1,
                   INST=1.1000000000000001,
                   RESULTAT=U1,
                   CRITERE='ABSOLU',
                   ),
                ),
          )
# COMPARAISON PAR RAPPORT A POU_D_E
TEST_RESU(RESU=(_F(INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='DEPL',
                   NOEUD='N006',
                   NOM_CMP='DY',
                   VALE_CALC=0.15668437975194,
                   VALE_REFE=0.15658191432102,
                   CRITERE='RELATIF',
                   PRECISION=7.0000000000000001E-04,
                   ),
                ),
          )

TEST_RESU(RESU=(_F(INST=1.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIEF_ELNO',
                   MAILLE='SEG2001',
                   NOEUD='N002',
                   SOUS_POINT = 200,
                   NOM_CMP='SIXX',
                   VALE_CALC=-7.80155774644956E+06,
                   CRITERE='RELATIF',
                   ),
                ),
          )
TEST_RESU(RESU=(_F(INST=1.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIEF_ELNO',
                   MAILLE='SEG2001',
                   NOEUD='N002',
                   SOUS_POINT = 1,
                   NOM_CMP='SIXX',
                   VALE_CALC=1.31499192496359E+06,
                   CRITERE='RELATIF',
                   ),
                ),
          )


TEST_RESU(RESU=(_F(INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='SIPM_ELNO',
                   MAILLE='SEG2001',
                   NOEUD='N002',
                   NOM_CMP='SIXXMIN',
                   VALE_CALC=-7.80155774644956E+06,
                   VALE_REFE=-7.80155774644956E+06,
                   CRITERE='RELATIF',
                   PRECISION=1.0000000000000001E-06,
                   ),
                ),
          )
TEST_RESU(RESU=(_F(INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='SIPM_ELNO',
                   MAILLE='SEG2001',
                   NOEUD='N002',
                   NOM_CMP='SIXXMAX',
                   VALE_CALC=1.31499192496359E+06,
                   VALE_REFE=1.31499192496359E+06,
                   CRITERE='RELATIF',
                   PRECISION=1.0000000000000001E-06,
                   ),
                ),
          )


# Validation de EXTR_RESU / RESTREINT pour des cham_elem avec sous-points :
#---------------------------------------------------------------------------

MA1B=CREA_MAILLAGE(MAILLAGE=MAPOU,  RESTREINT=_F(GROUP_MA='GM1',),);

MO1B=AFFE_MODELE(MAILLAGE=MA1B,
                 AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE',  MODELISATION='POU_D_EM',),);

POUCAB=AFFE_CARA_ELEM(MODELE=MO1B,    INFO=1,
           POUTRE=_F(GROUP_MA=('GM1'), SECTION='RECTANGLE',
                     CARA = ('HY','HZ'), VALE = (0.1,0.2)),
           ORIENTATION=_F(GROUP_MA=('GM1'), CARA='ANGL_VRIL', VALE=90.0,),
           MULTIFIBRE = _F( GROUP_MA = 'GM1', GROUP_FIBRE= ('BETON'),
                            PREC_INERTIE=1.1E-02, PREC_AIRE=1.0E-06 ,),
           GEOM_FIBRE = GFF )

U1B=EXTR_RESU(RESULTAT=U1, RESTREINT=_F(MODELE=MO1B, CARA_ELEM=POUCAB, ),);

TAB1B=CREA_TABLE(
           RESU=_F(RESULTAT=U1B, NOM_CHAM='SIEF_ELNO', INST=1.0, TOUT_CMP='OUI',TOUT='OUI',))

TEST_TABLE(REFERENCE = 'AUTRE_ASTER',
               VALE_CALC = -7.80155774644956E+06,
               VALE_REFE = -7.80155774644956E+06,
               NOM_PARA  = 'SIXX',
               TABLE     = TAB1B,
               FILTRE    = (_F(NOM_PARA = 'NOM_CHAM',    VALE_K    = 'SIEF_ELNO',),
                            _F(NOM_PARA = 'INST',        VALE      = 1.0,),
                            _F(NOM_PARA = 'MAILLE',      VALE_K    = 'SEG2001',),
                            _F(NOM_PARA = 'NOEUD',       VALE_K    = 'N002',),
                            _F(NOM_PARA = 'SOUS_POINT',  VALE_I    = 200,),
                            ),)


FIN()
