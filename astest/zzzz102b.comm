DEBUT( CODE=_F(  NOM = 'ZZZZ102B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#=======================================================================
#
#          LIRE_FONCTION D'UN SPECTRE D'OSCILLATEUR FORMAT LIBRE
#
#=======================================================================

SPO=LIRE_FONCTION(     UNITE=20,
                       TYPE='NAPPE',
                       TITRE='SPECTRE OSCILLATEUR FORMAT LIBRE',
                       INTERPOL='LIN',
                       PROL_DROITE='LINEAIRE',
                       PROL_GAUCHE='CONSTANT',
                       INTERPOL_FONC='LIN',
                       PROL_DROITE_FONC='EXCLU',
                       PROL_GAUCHE_FONC='EXCLU',
                       NOM_RESU      ='ACCE',
                       NOM_PARA      ='AMOR',
                       NOM_PARA_FONC ='FREQ',
                       INDIC_PARA    =[1,1],
                       INDIC_ABSCISSE=[2,1],
                       DEFI_FONCTION =( _F(INDIC_RESU=[2,2]),
                                        _F(INDIC_RESU=[2,3]),
                                        _F(INDIC_RESU=[2,4]),
                                        _F(INDIC_RESU=[2,5]),
                                        _F(INDIC_RESU=[2,6]),),
                       )

VAL=1.09558E-02

AM1=0.01

AM2=0.05

AM3=0.1

D01=VAL/AM3

D001=VAL/AM1

D005=D01 + ((5./9.)*( D001 - D01))

TEST_FONCTION(VALEUR=(
         _F(  FONCTION = SPO,
                  NOM_PARA = ( 'FREQ',  'AMOR', ),
                  VALE_PARA = ( 5.E-1,   AM1,    ),
                  VALE_REFE = D001),
         _F(  FONCTION = SPO,
                  NOM_PARA = ( 'FREQ',  'AMOR', ),
                  VALE_PARA = ( 5.E-1,   AM2,   ),
                  VALE_REFE = D005),
         _F(  FONCTION = SPO,
                  NOM_PARA = ( 'FREQ',  'AMOR', ),
                  VALE_PARA = ( 5.E-1,   AM3,   ),
                  VALE_REFE = D01)),ATTRIBUT=(
       _F(  FONCTION = SPO,   ATTR = 'NOM_RESU',
                     PARA = AM1,     ATTR_REFE = 'ACCE'),
       _F(  FONCTION = SPO,   ATTR = 'PROL_DROITE',
                                   ATTR_REFE = 'LINEAIRE'),
       _F(  FONCTION = SPO,   ATTR = 'PROL_GAUCHE',
                                   ATTR_REFE = 'CONSTANT'),
       _F(  FONCTION = SPO,   ATTR = 'INTERPOL',
                     PARA = AM1,     ATTR_REFE = 'LIN LIN '),
       _F(  FONCTION = SPO,   ATTR = 'PROL_DROITE_FONC',
                                   ATTR_REFE = 'EXCLU'),
       _F(  FONCTION = SPO,   ATTR = 'PROL_GAUCHE_FONC',
                                   ATTR_REFE = 'EXCLU'),
       _F(  FONCTION = SPO,   ATTR = 'INTERPOL_FONC',
                     PARA = AM1,     ATTR_REFE = 'LIN LIN '))
                 )

#----------------------------------------------------------------------

FIN()
