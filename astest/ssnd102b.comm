# DISCRET 2D AVEC COMPORTEMENT CINEMATIQUE
#    K_T_D_L    K_TR_D_L
#    K_T_D_N    K_TR_D_N
#=======================================================================


DEBUT(CODE=_F(NOM='SSND102B', NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'), DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE()

MODE=AFFE_MODELE(
   MAILLAGE=MAIL,
   AFFE=(
      _F(GROUP_MA='DL_T',  PHENOMENE='MECANIQUE', MODELISATION='2D_DIS_T',),
      _F(GROUP_MA='DL_TR', PHENOMENE='MECANIQUE', MODELISATION='2D_DIS_TR',),
      _F(GROUP_MA='DN_T',  PHENOMENE='MECANIQUE', MODELISATION='2D_DIS_T',),
      _F(GROUP_MA='DN_TR', PHENOMENE='MECANIQUE', MODELISATION='2D_DIS_TR',),
   ),
)

# Caracteristiques des discrets
# KEDX : Raideur elastique
# LEDX : Limite elastique
# KCDX : Raideur cinematique
# PUDX : Puissance cinematique
# LUDX : Limite cinematique

lt_ledx= 1000.0; lt_kedx= 3400000.0; lt_ludx= 1000.0; lt_kcdx=  700000.0; lt_pudx= 2.00;
lt_ledy= 1500.0; lt_kedy= 2000000.0; lt_ludy=  800.0; lt_kcdy=  900000.0; lt_pudy= 1.50;

nt_ledx= 1000.0; nt_kedx= 3400000.0; nt_ludx= 1000.0; nt_kcdx=  700000.0; nt_pudx= 2.00;
nt_ledy= 1500.0; nt_kedy= 2000000.0; nt_ludy=  800.0; nt_kcdy=  900000.0; nt_pudy= 1.50;

lr_ledx= 1000.0; lr_kedx= 3400000.0; lr_ludx= 1000.0; lr_kcdx=  700000.0; lr_pudx= 2.00;
lr_ledy= 1500.0; lr_kedy= 2000000.0; lr_ludy=  800.0; lr_kcdy=  900000.0; lr_pudy= 1.50;
lr_lerz= 2500.0; lr_kerz= 3200000.0; lr_lurz= 1400.0; lr_kcrz=  850000.0; lr_purz= 2.25;

nr_ledx= 1000.0; nr_kedx= 3400000.0; nr_ludx= 1000.0; nr_kcdx=  700000.0; nr_pudx= 2.00;
nr_ledy= 1500.0; nr_kedy= 2000000.0; nr_ludy=  800.0; nr_kcdy=  900000.0; nr_pudy= 1.50;
nr_lerz= 2500.0; nr_kerz= 3200000.0; nr_lurz= 1400.0; nr_kcrz=  850000.0; nr_purz= 2.25;

# Definition des materiaux
MAT_LT  = DEFI_MATERIAU(
   ELAS=_F(E=1.0,NU=0.1),
   DIS_ECRO_CINE=_F(
                LIMY_DX=lt_ledx, KCIN_DX=lt_kcdx, LIMU_DX=lt_ludx, PUIS_DX=lt_pudx,
                LIMY_DY=lt_ledy, KCIN_DY=lt_kcdy, LIMU_DY=lt_ludy, PUIS_DY=lt_pudy,
   ),
)
MAT_NT  = DEFI_MATERIAU(
   ELAS=_F(E=1.0,NU=0.1),
   DIS_ECRO_CINE=_F(
                LIMY_DX=nt_ledx, KCIN_DX=nt_kcdx, LIMU_DX=nt_ludx, PUIS_DX=nt_pudx,
                LIMY_DY=nt_ledy, KCIN_DY=nt_kcdy, LIMU_DY=nt_ludy, PUIS_DY=nt_pudy,
   ),
)
MAT_LTR = DEFI_MATERIAU(
   ELAS=_F(E=1.0,NU=0.1),
   DIS_ECRO_CINE=_F(
                LIMY_DX=lr_ledx, KCIN_DX=lr_kcdx, LIMU_DX=lr_ludx, PUIS_DX=lr_pudx,
                LIMY_DY=lr_ledy, KCIN_DY=lr_kcdy, LIMU_DY=lr_ludy, PUIS_DY=lr_pudy,
                LIMY_RZ=lr_lerz, KCIN_RZ=lr_kcrz, LIMU_RZ=lr_lurz, PUIS_RZ=lr_purz,
   ),
)
MAT_NTR = DEFI_MATERIAU(
   ELAS=_F(E=1.0,NU=0.1),
   DIS_ECRO_CINE=_F(
                LIMY_DX=nr_ledx, KCIN_DX=nr_kcdx, LIMU_DX=nr_ludx, PUIS_DX=nr_pudx,
                LIMY_DY=nr_ledy, KCIN_DY=nr_kcdy, LIMU_DY=nr_ludy, PUIS_DY=nr_pudy,
                LIMY_RZ=nr_lerz, KCIN_RZ=nr_kcrz, LIMU_RZ=nr_lurz, PUIS_RZ=nr_purz,
   ),
)
CHMAT=AFFE_MATERIAU(
   MAILLAGE=MAIL,
   AFFE=(
      _F(GROUP_MA='DL_T',  MATER=MAT_LT,),
      _F(GROUP_MA='DL_TR', MATER=MAT_LTR,),
      _F(GROUP_MA='DN_T',  MATER=MAT_NT,),
      _F(GROUP_MA='DN_TR', MATER=MAT_NTR,),
   ),
)

CARELEM=AFFE_CARA_ELEM(
   MODELE=MODE,
   DISCRET_2D=(
      _F(REPERE='LOCAL',CARA='K_T_D_L', GROUP_MA='DL_T',
         VALE=(lt_kedx, lt_kedy,),),
      _F(REPERE='LOCAL',CARA='K_TR_D_L',GROUP_MA='DL_TR',
         VALE=(lr_kedx,lr_kedy,lr_kerz),),
      _F(REPERE='LOCAL',CARA='K_T_D_N', GROUP_MA='DN_T',
         VALE=(nt_kedx, nt_kedy,),),
      _F(REPERE='LOCAL',CARA='K_TR_D_N', GROUP_MA='DN_TR',
         VALE=(nr_kedx,nr_kedy,nr_kerz),),
   ),
)

# Amplitudes des chargements
lt_amdx= 5.0*lt_ledx/lt_kedx; lt_amdy= 5.0*lt_ledy/lt_kedy;
nt_amdx= 5.0*nt_ledx/nt_kedx; nt_amdy= 5.0*nt_ledy/nt_kedy;

lr_amdx= 5.0*lr_ledx/lr_kedx; lr_amdy= 5.0*lr_ledy/lr_kedy; lr_amrz= 5.0*lr_lerz/lr_kerz;

nr_amdx= 5.0*nr_ledx/nr_kedx; nr_amdy= 5.0*nr_ledy/nr_kedy; nr_amrz= 5.0*nr_lerz/nr_kerz;


CONDLIM =AFFE_CHAR_MECA(MODELE=MODE,
   DDL_IMPO = (
      _F(GROUP_NO=('PT1'), LIAISON='ENCASTRE',),
      _F(GROUP_NO=('PT2'), DX =lt_amdx, DY =lt_amdy,),
      _F(GROUP_NO=('PT3'), DX =lr_amdx, DY =lr_amdy, DRZ=lr_amrz,),
      _F(GROUP_NO=('PT4'), DX =nt_amdx, DY =nt_amdy,),
      _F(GROUP_NO=('PT5'), DX =nr_amdx, DY =nr_amdy, DRZ=nr_amrz,),
   ),
)

# Definition de la fonction de chargement
Charge = DEFI_FONCTION(NOM_PARA='INST',
   VALE =(
      0.0 , 0.000,
     30.0 , 1.000,
     90.0 ,-1.000,
    145.0 , 0.800,
    180.0 ,-0.450,
    230.0 , 1.150,
    250.0 , 0.500,
    280.0 , 1.450,
   ),
)

linstS=DEFI_LIST_REEL(DEBUT=0.0, INTERVALLE=_F(JUSQU_A = 280.0, PAS = 1.0,), )


RESU=STAT_NON_LINE(
   MODELE     = MODE,
   CHAM_MATER = CHMAT,
   CARA_ELEM  = CARELEM,
   EXCIT=_F(CHARGE=CONDLIM,FONC_MULT=Charge),
   COMP_INCR=(
      _F(RELATION='DIS_ECRO_CINE', GROUP_MA='DL_T',),
      _F(RELATION='DIS_ECRO_CINE', GROUP_MA='DL_TR',),
      _F(RELATION='DIS_ECRO_CINE', GROUP_MA='DN_T',),
      _F(RELATION='DIS_ECRO_CINE', GROUP_MA='DN_TR',),
   ),
   INCREMENT=_F(LIST_INST=linstS,),
   CONVERGENCE=(_F(ARRET='OUI', ITER_GLOB_ELAS=25, ITER_GLOB_MAXI=30,),),
   NEWTON=(_F(REAC_INCR=1, MATRICE='TANGENTE',)),
)


# IMPR_RESU(FORMAT='RESULTAT', RESU=_F(RESULTAT=RESU),)

# ==========================================================
# Recupere les reponses sur les 4 discrets pour tous les DDL
#
# Les valeurs de reference
#
LesInstants = [30.0, 90.0, 145.0, 180.0, 230.0, 250.0, 280.0]
TempsMaxi = LesInstants[-1]
#
R1000 = [1635.707253,-1635.707253,1525.492881,-1249.231871,1699.177353,-415.3810135,1789.595108]
R1500 = [2224.098875,-2224.098875,2190.040518,-2017.290415,2239.745512,-790.3976635,2258.782745]
R2000 = [2767.25258,-2767.25258,2740.932239,-2547.183658,2777.2763,-1243.236082,2787.481342]
R3000 = [3923.076923,-3923.076923,3874.157276,-3600.0,3943.599982,-2097.139481,3966.23494]
R3500 = [5022.231028,-5022.231028,4911.499221,-4443.22145,5075.752629,-2025.828992,5143.339483]
R2500 = [3773.86368,-3773.86368,3686.099695,-3236.583886,3810.233618,-1262.367026,3849.201624]

ListeDiscrets = ['DN_TR','DN_T','DL_TR','DL_T']
LesDiscrets = {}

LesDiscrets['DL_T']  = {}
LesDiscrets['DL_T']['NOEUD']  = 'N2'
LesDiscrets['DL_T']['COMPO']  = [('N','DX'),('VY','DY')]
LesDiscrets['DL_T']['MAILLE'] = 'M1'
LesDiscrets['DL_T']['ENERG']  = [9.08390,35.56339,]
LesDiscrets['DL_T']['N']   = R1000
LesDiscrets['DL_T']['VY']  = R1500

LesDiscrets['DL_TR'] = {}
LesDiscrets['DL_TR']['NOEUD']  = 'N3'
LesDiscrets['DL_TR']['COMPO']  = [('N','DX'),('VY','DY'),('MFZ','DRZ')]
LesDiscrets['DL_TR']['MAILLE'] = 'M2'
LesDiscrets['DL_TR']['ENERG']  = [9.08390,35.56339,61.54945]
LesDiscrets['DL_TR']['N']   = R1000
LesDiscrets['DL_TR']['VY']  = R1500
LesDiscrets['DL_TR']['MFZ'] = R2500

LesDiscrets['DN_T']  = {}
LesDiscrets['DN_T']['NOEUD']  = 'N4'
LesDiscrets['DN_T']['COMPO']  = [('N','DX'),('VY','DY')]
LesDiscrets['DN_T']['MAILLE'] = 'M3'
LesDiscrets['DN_T']['ENERG']  = [9.08390,35.56339,]
LesDiscrets['DN_T']['N']   = R1000
LesDiscrets['DN_T']['VY']  = R1500

LesDiscrets['DN_TR'] = {}
LesDiscrets['DN_TR']['NOEUD']  = 'N5'
LesDiscrets['DN_TR']['COMPO']  = [('N','DX'),('VY','DY'),('MFZ','DRZ')]
LesDiscrets['DN_TR']['MAILLE'] = 'M4'
LesDiscrets['DN_TR']['ENERG']  = [9.08390,35.56339,61.54945]
LesDiscrets['DN_TR']['N']   = R1000
LesDiscrets['DN_TR']['VY']  = R1500
LesDiscrets['DN_TR']['MFZ'] = R2500

# Generation des TEST_FONCTION et des TEST_RESU
for LeDiscret in ListeDiscrets:
   LeNoeud    = LesDiscrets[LeDiscret]['NOEUD']
   ListeCompo = LesDiscrets[LeDiscret]['COMPO']
   LaF = [None]*len(ListeCompo)
   LeD = [None]*len(ListeCompo)
   ii = 0
   for Force,DDL in ListeCompo:
      LaF[ii] = RECU_FONCTION(
         RESULTAT = RESU, NOM_CHAM = 'SIEF_ELGA', NOM_CMP = Force,
         GROUP_MA = LeDiscret, POINT = 1,INTERPOL='LIN',PROL_DROITE='EXCLU',PROL_GAUCHE='EXCLU',)
      LeD[ii] = RECU_FONCTION(RESULTAT=RESU, NOM_CHAM='DEPL', NOM_CMP=DDL, NOEUD= LeNoeud,
         INTERPOL='LIN',PROL_DROITE='EXCLU',PROL_GAUCHE='EXCLU',)
      # TESTS DES RESULTATS
      if ( Force in LesDiscrets[LeDiscret].keys() ):
         Lvale = LesDiscrets[LeDiscret][Force]
         motclefs = {}
         motclefs['VALEUR'] = []
         for ind in range(len(LesInstants)):
            motclefs['VALEUR'].append(_F(FONCTION=LaF[ii],NOM_PARA='INST',PRECISION=2.00E-8,
                                         VALE_PARA=LesInstants[ind], TOLE_MACHINE=2.e-8,  #TODO vale_calc
                                         VALE_CALC=Lvale[ind],VALE_REFE=Lvale[ind],
                                         REFERENCE='ANALYTIQUE',CRITERE='RELATIF'))
         TEST_FONCTION(**motclefs)
      ii += 1
   # TESTS DES RESULTATS : energie
   if ( "ENERG" in LesDiscrets[LeDiscret].keys() ):
      LaMaille = LesDiscrets[LeDiscret]['MAILLE']
      Energie  = LesDiscrets[LeDiscret]['ENERG']
      motclefs = {}
      motclefs['RESU'] = []
      for jj in range(len(Energie)):
         vale = Energie[jj]
         vari = "V%d" % (jj*3+3)
         motclefs['RESU'].append(_F(RESULTAT=RESU,INST=TempsMaxi,NOM_CHAM='VARI_ELGA',
                                    CRITERE=('RELATIF','ABSOLU'),TOLE_MACHINE=(3.00E-06,1.0E-05),
                                    MAILLE=LaMaille,POINT=1,NOM_CMP=vari,VALE_CALC=vale,))
      TEST_RESU(**motclefs)

   motclefs = {}
   motclefs['CONCEPT'] = []
   for ii in range(len(LaF)):
      motclefs['CONCEPT'].append(_F(NOM=LaF[ii]))
      motclefs['CONCEPT'].append(_F(NOM=LeD[ii]))
   DETRUIRE(INFO=1,**motclefs)

FIN()
