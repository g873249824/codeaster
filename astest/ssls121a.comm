                  RESULTAT=R2S1INF,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N1',
                  NOM_CMP='SIYY',
                  VALE_CALC=-1.5,
                  VALE_REFE=-1.5,
                  PRECISION=1.E-3,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=R2S2INF,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N1',
                  NOM_CMP='SIYY',
                  VALE_CALC=-18.75,
                  VALE_REFE=-18.76,
                  PRECISION=1.E-3,
                  MAILLE='M1',),
          )

