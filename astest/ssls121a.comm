                    EXTR_COQUE=_F(NOM_CHAM=('SIGM_ELNO','EPSI_ELNO',),
                                  NUME_COUCHE=1,
                                  NIVE_COUCHE='INF',),);

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=R1S1INF,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N1',
                  NOM_CMP='SIXX',
                  VALE_CALC=-6.818181811,
                  VALE_REFE=-6.82,
                  PRECISION=1.E-3,
                  MAILLE='M1',),
          )

R1S2INF=POST_CHAMP(RESULTAT=RESU1,
                    EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                  NUME_COUCHE=2,
                                  NIVE_COUCHE='INF',),);
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=R1S2INF,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N1',
                  NOM_CMP='SIXX',
                  VALE_CALC=-0.136363636,
                  VALE_REFE=-0.135,
                  PRECISION=0.011,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=R1S1INF,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N1',
                  NOM_CMP='EPXX',
                  VALE_CALC=-0.27204545426359,
                  MAILLE='M1',),
          )

#
#---------------------------------------------------------------------
#                         CHARGEMENT 2
#---------------------------------------------------------------------
#
