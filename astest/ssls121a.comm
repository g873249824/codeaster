CHA4=AFFE_CHAR_MECA(        MODELE=MO,
                    DDL_IMPO=(
                                _F( GROUP_MA = 'TOUT_ELT',DRY=0.),
                                          ),
            LIAISON_OBLIQUE=(
                    _F( GROUP_NO = 'COTE_DA',
                                          DX = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,),
                                    ),
                    _F( GROUP_NO = 'COTE_DA',
                                          DRY=0.,
                                    ANGL_NAUT = (0., -48.5, 90.,),
                                    ),
                    _F( GROUP_NO = 'COTE_DA',
                                          DZ=0.,
