                                    NOM_CMP    = ('DX','DY',),
                                    GROUP_NO   = ('E_HD2','E_BD2',),
                                    INTITULE   = 'DEPLACEMENT',
                                    MOYE_NOEUD = 'OUI',
                                    NOM_CHAM   = 'DEPL',
                                    RESULTAT   = RMECA,
                                    OPERATION  = 'EXTRACTION',
                                    INST       = 50.,
                                    REPERE     = 'GLOBAL',),
