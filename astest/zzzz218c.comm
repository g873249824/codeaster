                                    NOM_CHAM   = 'TEMP',
                                    RESULTAT   = RTEMPER,
                                    OPERATION  = 'EXTRACTION',
                                    INST       = 10000.,
                                    REPERE     = 'GLOBAL',),
                        INFO = 1,)



