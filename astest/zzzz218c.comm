DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),
      PAR_LOT='NON',
      IMPR_MACRO='NON',
      DEBUG=_F(HIST_ETAPE='OUI'))

pr_int   = 10.E5
pr_atmos = 1.E5
temp_ref = 20.0
temp_int = 140.0
temp_ext = temp_ref


# MESH
# -------------------------------------
MAIL = LIRE_MAILLAGE(FORMAT='MED',)

MAIL = DEFI_GROUP(
            reuse         = MAIL,
            MAILLAGE      = MAIL,
            CREA_GROUP_NO = (_F(GROUP_MA = ('BETON1','FIXE1','INTRA1','EXTRA1',
                                            'BETON2','FIXE2','INTRA2','EXTRA2',
                                            'BFISH1','BFISB1','BFISH2','BFISB2',),),),
                )

MAIL = MODI_MAILLAGE(
                  reuse        = MAIL,
                  MAILLAGE     = MAIL,
                  ORIE_PEAU_2D = (_F(GROUP_MA=('FIXE1','INTRA1','EXTRA1',
                                               'FIXE2','INTRA2','EXTRA2',
                                               'BFISH1','BFISB1',
                                               'BFISH2','BFISB2',),),),
                    )



# DEFINITION OF MODELS AND MATERIALS
# -------------------------------------
MODMECA = AFFE_MODELE(
                  MAILLAGE = MAIL,
                  AFFE     = (_F(GROUP_MA = ('BETON1','FIXE1','INTRA1','EXTRA1',
                                             'BETON2','FIXE2','INTRA2','EXTRA2',
                                             'BFISH1','BFISB1','BFISH2','BFISB2',),
                                 PHENOMENE ='MECANIQUE',
                                 MODELISATION = 'D_PLAN',),),
                    )

MODTHER = AFFE_MODELE(
                 MAILLAGE = MAIL,
                 AFFE     = (_F(GROUP_MA = ('BETON1','FIXE1','INTRA1','EXTRA1',
                                            'BETON2','FIXE2','INTRA2','EXTRA2',
                                            'BFISH1','BFISB1','BFISH2','BFISB2',),
                              PHENOMENE = 'THERMIQUE',
                              MODELISATION = 'PLAN_DIAG',),),
                    )

BETON = DEFI_MATERIAU(
                  ELAS = _F(E      = 35000.0E+6,
                            NU     = 0.25,
                            ALPHA  = 1.0E-5,),
                  THER = _F(LAMBDA = 2.3,
                            RHO_CP = 2500000.0,),
                    )

CHMAT = AFFE_MATERIAU(
                  MAILLAGE  = MAIL,
                  AFFE      = (_F(GROUP_MA = ('BETON1','FIXE1','INTRA1','EXTRA1',
                                              'BETON2','FIXE2','INTRA2','EXTRA2',
                                              'BFISH1','BFISB1','BFISH2','BFISB2',),
                                 MATER     = BETON,),),
                  AFFE_VARC = (_F(NOM_VARC = 'TEMP',
                                  VALE_REF = temp_ref),),
                    )

# BOUNDARY CONDITIONS
# ---------------------------------
# Mechanical B.C. :
CONDMECA = AFFE_CHAR_MECA(
                      MODELE   = MODMECA,
                      DDL_IMPO = (_F(GROUP_NO = 'FIXE1', DX = 0.0, DY = 0.0,),
                                  _F(GROUP_NO = 'FIXE2', DX = 0.0, DY = 0.0,),),
                        )

CONT  = DEFI_CONTACT(MODELE      = MODMECA,
                     FORMULATION = 'DISCRETE',
                     REAC_GEOM   = 'SANS',
                     ZONE        = (_F(GROUP_MA_MAIT = 'BFISH1',
                                       GROUP_MA_ESCL = 'BFISB1',
                                       SANS_GROUP_NO = ('E_HD1','E_BD1'),), # Pour tester la prise en compte de TESTXX
                                    _F(GROUP_MA_MAIT = 'BFISH2',
                                       GROUP_MA_ESCL = 'BFISB2',
                                       SANS_GROUP_NO = ('E_HD2','E_BD2'),), # Pour tester la prise en compte de TESTXX
                                   ),
                    )


# Thermal B.C. :
TEMP_INT = DEFI_CONSTANTE(VALE = temp_int,)
TEMP_EXT = DEFI_CONSTANTE(VALE = temp_ext,)
H_EXT    = DEFI_CONSTANTE(VALE = 4.0,)
H_INT    = DEFI_CONSTANTE(VALE = 8.0,)

CONDTHER = AFFE_CHAR_THER_F(
                    MODELE  = MODTHER,
                    ECHANGE = (
                        _F(GROUP_MA = 'INTRA1',
                           COEF_H   = H_INT,
                           TEMP_EXT = TEMP_INT,),
                        _F(GROUP_MA ='EXTRA1',
                           COEF_H   = H_EXT,
                           TEMP_EXT = TEMP_EXT,),
                        _F(GROUP_MA = 'INTRA2',
                           COEF_H   = H_INT,
                           TEMP_EXT = TEMP_INT,),
                        _F(GROUP_MA ='EXTRA2',
                           COEF_H   = H_EXT,
                           TEMP_EXT = TEMP_EXT,),),
                          )



# COMPUTATION
# ---------------------------------
L_INST = DEFI_LIST_REEL(
                  DEBUT      = 0.0,
                  INTERVALLE = (
                        _F(JUSQU_A = 500.,   NOMBRE = 20,),
                        _F(JUSQU_A = 10000., NOMBRE = 20,),
                              ),
                      )

