T_DEPL1 = POST_RELEVE_T(ACTION = _F(CRITERE   = 'RELATIF',
                                    NOM_CMP    = ('DX','DY',),
                                    GROUP_NO   = ('E_HD1','E_BD1',),
                                    INTITULE   = 'DEPLACEMENT',
                                    MOYE_NOEUD = 'OUI',
                                    NOM_CHAM   = 'DEPL',
                                    RESULTAT   = RMECA,
                                    OPERATION  = 'EXTRACTION',
                                    INST       = 50.,
                                    REPERE     = 'GLOBAL',),
