DEBUT(CODE=_F(NOM='SSNV185D',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'))

# MODELISATION D :X-FEM FISSURE SEMI-NON-COINCIDENTE - TRACTION (FACE SUP)

#----------------------------------------------
#                MAILLAGE : hexa_5_30_50.mgib
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='SURFSUP',),);

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

MAILLAG1=MODI_MAILLAGE(reuse =MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFSUP','SURFINF',),),
                       );

# PARAMETRES DE LA FISSURE : 0 < a < 6.5 -> influt sur le choix Rsup !
#                            b = 10.
a=5.-0.1666
b=10.
LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-15.');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='b-a-Y');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    ORIE_FOND=_F(PFON_INI= (0. ,5. ,15. ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 POINT_ORIG=(0. ,20. ,15. ),),
                    );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,FISSURE=FISS,INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,LIAISON_XFEM='OUI',INFO=1,);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=205000.0E6
nu=0.
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT = 'OUI',
                                MATER=ACIER,
                                ),
                             );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

PRES=-1.E6
CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(NOEUD='N3751',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(NOEUD='N9276',DZ=0.0,),
                             _F(NOEUD='N3760',DX=0.0,DZ=0.0,),),);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   PRES_REP=_F(GROUP_MA=('SURFSUP','SURFINF',),PRES=PRES,),);

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=3.0,NOMBRE=3,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,
                   EXCIT=(_F(CHARGE=CHXFEM,),
                          _F(CHARGE=CH1,),
                          _F(CHARGE=CH2,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INS1,
                                INST_FIN=1.0,),
#                   SOLVEUR=_F(METHODE='MUMPS',),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);

#------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS PAR LA METHODE ENERGETIQUE AVEC X-FEM
#------------------------------------------------------------------------

# TEST AVEC PLUSIEURS COURONNES
if a > 4  :
  RI=[2. , 0.666 , 1. , 1. , 1. , 2.1]
  RS=[4. , 1.666 , 2. , 3. , 4. , 3.9]
else :
  RI=[0.666 , 1.]
  RS=[1.666 , 2.]


nbc=len(RI)
SIF = [None]*nbc

for i in range(0,nbc) :

   SIF[i]=CALC_G(RESULTAT=UTOT1,
                 OPTION='CALC_K_G',
                 NUME_ORDRE=1,
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[i],
                          R_SUP=RS[i],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),);

   IMPR_TABLE(TABLE=SIF[i],);

# SOLUTION ANALYTIQUE (chaboche p.53)
coef=(pi*a)/(2.*b)
fab=sqrt(tan(coef)/coef)*(0.752+0.37*(1.-sin(coef))**3+2.02*a/b)/cos(coef)
K1REF=-PRES*sqrt(pi*a)*fab

prec = 0.03

for i in range(0,nbc) :
   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1',
              TYPE_TEST='MAX',
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=prec,
              REFERENCE='ANALYTIQUE',);

   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1',
              TYPE_TEST='MIN',
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=prec,
              REFERENCE='ANALYTIQUE',);
#------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS AVEC POST_K1_K2_K3
#------------------------------------------------------------------------

PK=POST_K1_K2_K3(MODELISATION='3D',
                     MATER=ACIER,
                     FISSURE = FISS,
                     DTAN_ORIG = (0,-1,0),
                     DTAN_EXTR = (0,-1,0),
                     RESULTAT  = UTOT1,
                     ABSC_CURV_MAXI = 1.5,
                     NB_NOEUD_COUPE = 6,
                     VECT_K1        = ( 0., 0., 1., ),
                    )

IMPR_TABLE(TABLE = PK)

TEST_TABLE( TABLE=PK,
            NOM_PARA='K1',
            FILTRE=(_F( NOM_PARA = 'PT_FOND',  
                        VALE_I = 1,  ),
                    _F( NOM_PARA = 'INST',  
                        VALE = 1.,  ),),
            VALE=K1REF,
            PRECISION=0.10,
            REFERENCE='ANALYTIQUE' )

#------------------------------------------------------------------------
# POST-TRAITEMENT : MAILLAGE FISSURE
#------------------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

TABDEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                               GROUP_NO='VOL',
                               RESULTAT=UTOT1,
                               NOM_CHAM='DEPL',
                               NUME_ORDRE=1,
                               NOM_CMP='DX',
                               OPERATION='EXTRACTION',),);

# TEST DE NON REGRESSION PAR RAPPORT A LA VERSION 8.2.14

DXREF=4.7430e+03
DYREF=4.74300e+04
DZREF=1.42289996944e+05
PREC=1.e-10

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_X',
           TYPE_TEST='SOMM_ABS',
           VALE=DXREF,
           CRITERE='RELATIF',
           PRECISION=PREC,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '8.2.14'),

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_Y',
           TYPE_TEST='SOMM_ABS',
           VALE=DYREF,
           CRITERE='RELATIF',
           PRECISION=PREC,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '8.2.14'),

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_Z',
           TYPE_TEST='SOMM_ABS',
           VALE=DZREF,
           CRITERE='RELATIF',
           PRECISION=PREC,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '8.2.14'),


#------------------------------------------------------------------------
# POST-TRAITEMENT : DEPLACEMENTS POUR LE MAILLAGE FISSURE
#------------------------------------------------------------------------


MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)

TEST_RESU(RESU=(_F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DX',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE =  4.1238378990915E-03,
                     PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.17'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DY',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 1.4804804254497,
                     PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.17'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DZ',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 1.1204378258422,
                     PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.17'),
               ),)

FIN();
