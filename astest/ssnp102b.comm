DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED',   )

M=DEFI_GROUP(  reuse=M,
               MAILLAGE=M,
               CREA_GROUP_NO= _F( GROUP_MA = 'DG'),);

FISS=DEFI_FOND_FISS(MAILLAGE=M,
                    FOND_FISS=_F(GROUP_MA='DG',),
                    SYME='OUI',
                    CONFIG_INIT='DECOLLEE',
                    NORMALE=(0.,1.,0.));


MO=AFFE_MODELE(            MAILLAGE=M,
                                 AFFE=_F(  TOUT = 'OUI',
                                        PHENOMENE = 'MECANIQUE',
                                        MODELISATION = '3D') )

SIGMF=DEFI_FONCTION(    NOM_PARA='EPSI',
                              VALE=(0.0023333,  480.2,
                                    0.01,       480.35778, ),
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='LINEAIRE')

MA=DEFI_MATERIAU(            ELAS=_F(  E = 205800.,
                                        NU = 0.3,
                                        ALPHA = 0.0),
                             TRACTION=_F(  SIGM = SIGMF),
                              PRAGER=_F(C=0.))

CM=AFFE_MATERIAU(        MAILLAGE=M,
                                 AFFE=_F(  TOUT = 'OUI',
                                        MATER = MA,
                                        ) )

#
# ****************************************************************
#            CONDITIONS AUX LIMITES EN DEPLACEMENTS :
#          CONTINUITE ET SYMETRIE SUR L'INTERFACE SDE : U2=0
#                                                 SAB : U1=0
# ****************************************************************
#


CH=AFFE_CHAR_MECA(      MODELE=MO,DDL_IMPO=(
                          _F(  GROUP_MA = 'SAB',  DX = 0.),
                          _F(  GROUP_MA = 'SDE',  DY = 0.),
                          _F(  GROUP_MA = 'EPROUVET',   DZ = 0.)),
                          PRES_REP=_F(  GROUP_MA = 'SFA',  PRES = -196.)
                      )

#
# ****************************************************************
#             DEFINITION DES INSTANTS DE CALCUL
# ****************************************************************
#


L_INST=DEFI_LIST_REEL(          DEBUT=0.0,
                              INTERVALLE=_F(  JUSQU_A = 2.0,
                                            NOMBRE = 20)
                           )

#
# ****************************************************************
#     RESOLUTION DU PROBLEME
# ****************************************************************
#


F=DEFI_FONCTION(      NOM_PARA='INST',
                        PROL_DROITE='LINEAIRE',
                       PROL_GAUCHE='LINEAIRE',
                               VALE=( 0.0,  0.0,
                                      0.5,  1.0,
                                      1.0,  0.0,
                                      1.5,  1.0,
                                      2.0,  0.0,
                                      2.5,  1.0,
                                      3.0,  0.0, )
                     )

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST),
                        ECHEC=_F(ACTION='DECOUPE',
                                 SUBD_METHODE='MANUEL',
                                 SUBD_PAS=2,
                                 SUBD_PAS_MINI = 0.001, ),)


UP=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CM,
                             EXCIT=_F(
                CHARGE = CH,
                FONC_MULT = F),
                         COMPORTEMENT=_F(
                RELATION = 'VMIS_ISOT_TRAC',
                DEFORMATION = 'PETIT'),
                         INCREMENT=_F(
                LIST_INST = DEFLIST),
                         ARCHIVAGE=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_REFE_RELA=1.,
                SIGM_REFE     =4.8E-3,
             #   RESI_GLOB_RELA = 1.E-8,
                ITER_GLOB_MAXI = 8)
              )




UP=CALC_CHAMP(reuse=UP,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=UP)


UP=CALC_CHAMP(reuse=UP,RESULTAT=UP,DEFORMATION=('EPSP_ELNO','EPSI_ELGA'))



# ****************************************************************
#     CALCUL DU TAUX DE RESTITUTION D'ENERGIE G SUR 4 COURONNES
# ****************************************************************
#





G1=CALC_G(
            RESULTAT=UP,
            THETA=_F(
                     R_INF=0.55,
                     FOND_FISS=FISS,
                     MODULE=1.0,
                     R_SUP=1.0,
                     DIRECTION=(1.,0.,0.,)),
            OPTION='CALC_G_GLOB'
             );


G2=CALC_G(
            RESULTAT=UP,
            THETA=_F(
                     R_INF=1.0,
                     FOND_FISS=FISS,
                     MODULE=1.0,
                     R_SUP=1.5,
                     DIRECTION=(1.,0.,0.,)),
            OPTION='CALC_G_GLOB'
             );

G3=CALC_G(
            RESULTAT=UP,
            THETA=_F(
                     R_INF=1.5,
                     FOND_FISS=FISS,
                     MODULE=1.0,
                     R_SUP=2.0,
                     DIRECTION=(1.,0.,0.,)),
            OPTION='CALC_G_GLOB'
             );

G4=CALC_G(
            RESULTAT=UP,
            THETA=_F(
                     R_INF=2.0,
                     FOND_FISS=FISS,
                     MODULE=1.0,
                     R_SUP=3.0,
                     DIRECTION=(1.,0.,0.,)),
            OPTION='CALC_G_GLOB'
             );

IMPR_TABLE(       TABLE=G1,)
IMPR_TABLE(       TABLE=G2,)
IMPR_TABLE(       TABLE=G3,)
IMPR_TABLE(       TABLE=G4,)

