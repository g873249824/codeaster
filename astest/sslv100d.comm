DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',)

M=DEFI_GROUP( reuse=M,   MAILLAGE=M,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(            MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         PHENOMENE = 'MECANIQUE',
                                         MODELISATION = 'D_PLAN'))

MA=DEFI_MATERIAU(            ELAS=_F(  E = 200000.,
                                         NU = 0.3,
                                         ALPHA = 0.)
                       )

MA0=DEFI_MATERIAU(          ELAS_ORTH=_F(  E_L = 200000.,
          E_T = 200000.,  E_N = 200000.,
     G_LT = 76923.0769231, G_LN = 76923.0769231, G_TN = 76923.0769231,
          NU_LT = 0.3, NU_LN = 0.3, NU_TN = 0.3,
          ALPHA_L = 0.,  ALPHA_T = 0., ALPHA_N = 0.)
                         )

CM=AFFE_MATERIAU(        MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MA))

CM0=AFFE_MATERIAU(        MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MA0))

ESSAI=AFFE_CARA_ELEM(MODELE=MO,
                MASSIF=_F(

       GROUP_MA = 'TOUT',

                ANGL_REP = (0., )))

ESSAI0=AFFE_CARA_ELEM(MODELE=MO,
                MASSIF=_F(
                GROUP_MA = 'TOUT',
                ANGL_REP = (23., )))

CH=AFFE_CHAR_MECA(       MODELE=MO,FACE_IMPO=(
          _F( GROUP_MA = 'FACEEF',  DNOR = 0.),
          _F( GROUP_MA = 'FACEAE', DNOR = -5.72E-5)),
           DDL_IMPO=_F( GROUP_NO = 'BORDAB',  DY = 0.))

DEPLA=MECA_STATIQUE(   MODELE=MO,
                           CHAM_MATER=CM, EXCIT=_F(  CHARGE = CH) )

LISTE=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=5,),);


DEPLA0=STAT_NON_LINE(   MODELE=MO,  COMPORTEMENT=_F(RELATION='ELAS'),INCREMENT=_F(LIST_INST=LISTE),
             CARA_ELEM=ESSAI0,   CHAM_MATER=CM0, EXCIT=_F(  CHARGE = CH) );

DEPLA=CALC_CHAMP( reuse=DEPLA,   RESULTAT=DEPLA,
               FORCE='REAC_NODA',
                     )

DEPLA0=CALC_CHAMP( reuse=DEPLA0,   RESULTAT=DEPLA0,
                FORCE='REAC_NODA',
                )

