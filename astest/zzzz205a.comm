DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))


#---------------------------------------------------------------------
# -- MAILLAGE LINEAIRE , MODELISATION D_PLAN
#---------------------------------------------------------------------

MA_INIT = LIRE_MAILLAGE(FORMAT='GMSH',
                        UNITE=19,
                        )

MA_INIT=DEFI_GROUP(reuse=MA_INIT,  MAILLAGE=MA_INIT,
                    CREA_GROUP_MA=_F(  NOM = 'ALL', TOUT = 'OUI'))

MA=CREA_MAILLAGE(MAILLAGE=MA_INIT,LINE_QUAD=_F(TOUT='OUI'))

ACIER=DEFI_MATERIAU(   ELAS=_F( E = 1.E+11,
                             NU = 0.3 ,RHO=7800.,))

CHMAT=AFFE_MATERIAU(   MAILLAGE=MA,
                             AFFE=_F( TOUT = 'OUI', MATER = ACIER),)

MO=AFFE_MODELE(    MAILLAGE=MA,
                         AFFE=_F(  TOUT='OUI',
                                   PHENOMENE = 'MECANIQUE',
                                   MODELISATION = 'D_PLAN'),
               )
#
#--CREATION DE LA SD RESULTAT DE TYPE EVOL_NOLI
#
VITE_1s=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_DEPL_R',
                    MAILLAGE=MA,
                    AFFE=_F(  TOUT = 'OUI',
                    NOM_CMP = ('DX','DY'),
                    VALE = (  1.5,    0.,))
                      )
VITE_2s=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_DEPL_R',
                    MAILLAGE=MA,
                    AFFE=_F(  TOUT = 'OUI',
                    NOM_CMP = ('DX','DY'),
                    VALE = (  1.5,    2.5,))
                      )
VITE=CREA_RESU(OPERATION='AFFE',
                    TYPE_RESU='EVOL_NOLI',
                    NOM_CHAM='VITE',
                    AFFE=_F(CHAM_GD=VITE_1s,INST=1.0,),);
VITE=CREA_RESU(reuse=VITE,
               RESULTAT=VITE,
               OPERATION='AFFE',
               TYPE_RESU='EVOL_NOLI',
               NOM_CHAM='VITE',
               AFFE=_F(CHAM_GD=VITE_2s,INST=2.0,),);

# CALCUL DE LA MASSE  ---------------------------------
#
MASSE=POST_ELEM(  MODELE=MO,
                        CHAM_MATER=CHMAT,
                        MASS_INER=(_F(  GROUP_MA = ( 'GM100','GM200', 'ALL' ),),
                                    _F(TOUT = 'OUI') ))

#IMPR_TABLE(TABLE=MASSE, UNITE=6)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-4,
           VALE_CALC= 2.04750000E+03,
           VALE_REFE=2047.5,
           NOM_PARA='MASSE',
           TABLE=MASSE,
           FILTRE=_F(NOM_PARA='LIEU',
                     VALE_K='MA',),
           )

