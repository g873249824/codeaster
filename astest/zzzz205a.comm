           NOM_PARA='MASSE',
           TABLE=MASSE,
           FILTRE=_F(NOM_PARA='LIEU',
                     VALE_K='ALL',),
           )

#
# CALCUL DE L'ENERGIE CINETIQUE -----------------------
#
ENER=POST_ELEM(   MODELE=MO,   CHAM_MATER=CHMAT,
                 RESULTAT=VITE,ENER_CIN=(_F(
                                           GROUP_MA=('GM100','GM200'),),
                                         _F(TOUT='OUI') ))
IMPR_TABLE(TABLE=ENER)
