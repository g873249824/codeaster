#              PILOTE='INTERACTIF',
#              COURBE=_F(FONC_X=DN,
#                        FONC_Y=FN,
#                        LEGENDE='F(U)',),
#              LEGENDE_X='\\xd\\s\\0n',
#              LEGENDE_Y='F\\sn',
#              TITRE='mode I avec charge-decharge',
#              );

#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=U_HM,NOM_CHAM='DEPL',TYPE_CHAM='VECT_2D',NOM_CMP=('DX','DY'),MAILLE = ('CUBEDEG','CUBE'))))
#IMPR_RESU(RESU=(_F(RESULTAT=U_HM,NOM_CHAM='DEPL',NOM_CMP=('PRE1','DX','DY'),MAILLE = ('CUBEDEG'))))
#IMPR_RESU(RESU=(_F(RESULTAT=U_HM,NOM_CHAM='SIEF_ELGA',NOM_CMP=('SIGN','SITX','FH11X'),MAILLE = ('CUBEDEG'))))
#IMPR_RESU(RESU=(_F(RESULTAT=U_HM,NOM_CHAM='VARI_ELGA',NOM_CMP=('V7','V8','V12'),MAILLE = ('CUBEDEG'))))
FIN()
