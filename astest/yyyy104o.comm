MA=LIRE_MAILLAGE(  
                         )

MO=AFFE_MODELE(      MAILLAGE=MA, 
                             AFFE=_F(  TOUT = 'OUI',  PHENOMENE = 'MECANIQUE',
                                                 MODELISATION = '3D')
                         )

MAT=DEFI_MATERIAU(    ELAS=_F(  E = 210000.0,  NU = 0.3)
                         )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,
                            AFFE=_F(  TOUT = 'OUI',  MATER = MAT)
                         )

CHA=AFFE_CHAR_MECA(   MODELE=MO,DDL_IMPO=(
                 _F( GROUP_NO = 'SUPY',   DY = 0.),
                          _F( GROUP_NO = 'APPUI',  DX = 0.),
                          _F( GROUP_NO = 'SUPZ',   DZ = 0.),
                          _F( GROUP_NO = 'CHARGE', DX = -1.))
                         )

MEL=CALC_MATR_ELEM(   MODELE=MO,       CHARGE=CHA,  CHAM_MATER=CHMAT,
                                           OPTION='RIGI_MECA'
                         )

#

NU=NUME_DDL(         MATR_RIGI=MEL 
                         )

#

MATAS=ASSE_MATRICE(     MATR_ELEM=MEL,  NUME_DDL=NU
                         )

VEL=CALC_VECT_ELEM(   CHARGE=CHA,     OPTION='CHAR_MECA'
                         )

DEP=ASSE_VECTEUR(     VECT_ELEM=VEL,  NUME_DDL=NU
                         )

MATAS=FACTORISER(      reuse=MATAS,  MATR_ASSE=MATAS
                         )

DEP=RESOUDRE(      reuse=DEP,  MATR=MATAS,      CHAM_NO=DEP
                         )

#

RESU=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_ELAS',
                NOM_CHAM='DEPL',
                AFFE=_F(INST=0.,
                        CHAM_GD=DEP,
                        ),
                );


RESU=CALC_ELEM(reuse=RESU, RESULTAT=RESU,
                MODELE=MO,
                INST=0.,
                CHAM_MATER=CHMAT,
