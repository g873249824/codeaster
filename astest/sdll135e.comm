# MODELISATION BARRE

DEBUT(CODE=_F(NOM='SDLL135E',
              NIV_PUB_WEB='INTERNET',),);

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='BARRE',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=200000E6,
                            NU=0.29,
                            RHO=7830.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

CARAC=AFFE_CARA_ELEM(MODELE=MODELE,
                     BARRE=_F(GROUP_MA='TOUT',
                               SECTION='CERCLE',
                               CARA=('R','EP',),
                               VALE=(0.16,0.01,),),);


# DEFINITION DES LISTES D INSTANTS
# DANS LES DEUX CAS L ONDE NE FAIT QU UN ALLER POUR
# UN MINIMUM DE TEMPS CPU CONSOMME
# LA PREMIERE CORRESPOND A L ONDRE DE TRACTION-COMPRESSION


LISTE=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=2.E-4,
                                   PAS=1e-07,),);

# ENCASTREMENT SUR UNE EXTREMITE

CL=AFFE_CHAR_MECA(MODELE=MODELE,
                  DDL_IMPO=_F(GROUP_NO='A',
                              DX=0.0,
                              DY=0.0,
                              DZ=0.0,
                              ),);

# CHARGEMENT SOUS LA FORME D UNE FORCE ( OU D UN MOMENT ) DE TYPE HEAVYSIDE

CHARFX=AFFE_CHAR_MECA(MODELE=MODELE,
                      FORCE_NODALE=_F(GROUP_NO='B',
                                      FX=0.70710678118654746,
                                      FY=0.70710678118654746),);



MACRO_MATR_ASSE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARAC,
                CHARGE=CL,
                NUME_DDL=CO('NUMEDDL'),
                MATR_ASSE=(_F(MATRICE=CO('MASSE'),
                              OPTION='MASS_MECA',),
                           _F(MATRICE=CO('RIGID'),
                              OPTION='RIGI_MECA',),),);

# LISTE DISCRETISE TRES FINEMENT POUR RESPECTER
# LE RAPPORT TAILLE DE MAILLE VITESSE DE L ONDE DE COMPRESSION

DYNATRAF=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='PHYS',
                        MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                        CARA_ELEM=CARAC,
                        MATR_MASS=MASSE,
                        MATR_RIGI=RIGID,
                        SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',),
                        EXCIT=(_F(CHARGE=CHARFX,),
                               _F(CHARGE=CL,),),
                        INCREMENT=_F(LIST_INST=LISTE,),
                        ARCHIVAGE=_F(PAS_ARCH=100,),);

DYNATRAF=CALC_CHAMP(reuse=DYNATRAF,RESULTAT=DYNATRAF,CONTRAINTE=('SIEF_ELGA','SIEF_ELNO'))


DYNATRAF=CALC_CHAMP(reuse =DYNATRAF,
                    RESULTAT=DYNATRAF,
                    FORCE='REAC_NODA',
                    MODELE=MODELE,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARAC,
                    EXCIT=(_F(CHARGE=CHARFX,),
                           _F(CHARGE=CL,),),);


DYNANOLI=DYNA_NON_LINE(
                SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
                               FORMULATION='DEPLACEMENT',),
                MODELE=MODELE,
                CARA_ELEM = CARAC,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE=CHARFX,),
                               _F(CHARGE=CL,),),
                COMP_INCR=_F(RELATION = 'ELAS'),
                INCREMENT=_F(LIST_INST=LISTE,),
                NEWTON=_F(MATRICE = 'TANGENTE',REAC_ITER = 1),
                CONVERGENCE=_F(
                                RESI_GLOB_MAXI = 1.,
                                ITER_GLOB_MAXI = 20),
                ARCHIVAGE=_F(PAS_ARCH=100,),
            );

DYNANOLI=CALC_CHAMP(reuse=DYNANOLI,RESULTAT=DYNANOLI,CONTRAINTE=('SIEF_ELNO'))

DYNANOLI=CALC_CHAMP(reuse =DYNANOLI,
                    RESULTAT=DYNANOLI,
                    FORCE='REAC_NODA',
                    MODELE=MODELE,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARAC,
                    EXCIT=(_F(CHARGE=CHARFX,),
                           _F(CHARGE=CL,),),);



