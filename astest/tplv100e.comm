DEBUT(CODE=_F(NOM='TPLV100E',
              NIV_PUB_WEB='INTERNET',),);

M=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='THERMIQUE',
                       MODELISATION='AXIS_FOURIER',),);

MA=DEFI_MATERIAU(THER=_F(LAMBDA=1.,
                         RHO_CP=0.,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=MA,),);

T0  =DEFI_CONSTANTE (VALE= 0.)
FL2 =DEFI_CONSTANTE (VALE= 2.)
CH2 =DEFI_CONSTANTE (VALE= 2.)
TE2 =DEFI_CONSTANTE (VALE= 2.)
CH=AFFE_CHAR_THER_F(MODELE=MO,
                  TEMP_IMPO=_F(NOEUD='N1',
                               TEMP=T0,),
                  FLUX_REP=_F(GROUP_MA='GRMA6',
                              FLUN=FL2,),
                  ECHANGE=_F(GROUP_MA='GRMA7',
                             COEF_H=CH2,
                             TEMP_EXT=TE2,),);

KE=CALC_MATR_ELEM(OPTION='RIGI_THER',
                  MODELE=MO,
                  CHAM_MATER=CM,
                  MODE_FOURIER=2,
                  CHARGE=CH,);

VE=CALC_VECT_ELEM(OPTION='CHAR_THER',
                  CHARGE=CH,);

NU=NUME_DDL(MATR_RIGI=KE,);

KAS=ASSE_MATRICE(MATR_ELEM=KE,
                 NUME_DDL=NU,);

VAS=ASSE_VECTEUR(VECT_ELEM=VE,
                 NUME_DDL=NU,);

KAS=FACT_LDLT(reuse =KAS,
              MATR_ASSE=KAS,);

TEMP=RESO_LDLT(MATR_FACT=KAS,
               CHAM_NO=VAS,);

FLUXNO=CALC_CHAM_ELEM(MODELE=MO,
                      CHAM_MATER=CM,
                      OPTION='FLUX_ELNO_TEMP',
                      TEMP=TEMP,
                      MODE_FOURIER=2,);

FLUXGA=CALC_CHAM_ELEM(MODELE=MO,
                      CHAM_MATER=CM,
                      OPTION='FLUX_ELGA_TEMP',
                      TEMP=TEMP,
                      MODE_FOURIER=2,);

TEST_RESU(CHAM_NO=(_F(CHAM_GD=TEMP,
                      NOEUD='N76',
                      NOM_CMP='TEMP',
                      VALE=0.25,
