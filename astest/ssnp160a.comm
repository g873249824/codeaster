def NT(epsp) :
   import numpy as NP
   lnt = a1+a2*NP.exp(a3*epsp)
   Nt=10**(lnt)
   return Nt
   
def Ctana(EPSI) :
#   Nl=5.1E29
#   Kt=exp(60000./8.3144/293)
   a=(Nl/Kt/Ctot0 + 1. + NT(EPSI)/Ctot0) 
   cta=a - sqrt (a**2 - 4*NT(EPSI)/Ctot0 )
   cta=cta/2.
   return cta
   
Ct_ana = FORMULE(VALE='Ctana(EPSI)', NOM_PARA=('EPSI'),);

