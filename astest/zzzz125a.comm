DEBUT(  CODE=_F( NOM = 'ZZZZ125A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI()

MA=LIRE_MAILLAGE( )

CHNO=CREA_CHAMP(   OPERATION='AFFE',
                       TYPE_CHAM='NOEU_TEMP_R',
                        MAILLAGE=MA,
                        AFFE=_F(  TOUT = 'OUI', NOM_CMP = 'TEMP',
                               VALE = 100.)
                    )

C1=INTE_MAIL_2D(  MAILLAGE=MA,DEFI_SEGMENT=(
                       _F(  GROUP_NO_ORIG = 'A',
                                      GROUP_NO_EXTR = 'P1'),
                       _F(  GROUP_NO_ORIG = 'P2',
                                      GROUP_NO_EXTR = 'C')),
                     DEFI_ARC=_F(  CENTRE = ( 3., 3., ),
                                PRECISION = 2.E-2,
                                GROUP_NO_ORIG = 'P1',
                                GROUP_NO_EXTR = 'P2'),
                       INFO=2
                  )

C2=INTE_MAIL_2D(    MAILLAGE=MA,
                        DEFI_ARC=_F(  GROUP_NO_CENTRE = 'O',
                                   PRECISION = 0.02,
                                   ORIGINE = ( 3., 0.3335, ),
                                   GROUP_NO_EXTR = 'P2'),
                       INFO=2
                  )

T2=POST_RELEVE_T(    ACTION=_F(  INTITULE = 'CH2',
                                  CHEMIN = C2,
                                  CHAM_GD = CHNO,
                                  NOM_CMP = 'TEMP',
                                  OPERATION = 'MOYENNE')
                                )

IMPR_TABLE(   TABLE=T2 )

C3=INTE_MAIL_2D(   MAILLAGE=MA,
                      DEFI_ARC=_F(  GROUP_NO_CENTRE = 'O',
                                RAYON = 2.,
                                SECTEUR = ( -135., -45., )),
                        INFO=2
                    )

C4=INTE_MAIL_2D(   MAILLAGE=MA,
                   DEFI_SEGMENT=(_F( EXTREMITE = (3.,2.),
                                     NOEUD_ORIG = 'N146'),
                                 _F( EXTREMITE = (3.,0.),
                                     NOEUD_ORIG = 'N146'),
                                ),
                        INFO=2
                    )

T4=POST_RELEVE_T(    ACTION=_F(  INTITULE = 'CH4',
                                  CHEMIN = C4,
                                  CHAM_GD = CHNO,
                                  NOM_CMP = 'TEMP',
                                  OPERATION = 'MOYENNE')
                                )

IMPR_TABLE(   TABLE=T4 )

TEST_TABLE(
           VALE_CALC=100.0,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='TEMP',
           TABLE=T2,
           FILTRE=_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
           )

TEST_TABLE(
           VALE_CALC=100.0,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='TEMP',
           TABLE=T4,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
                   _F(NOM_PARA='SEGMENT',
                     VALE_I=2,),),
           )

FIN( )
