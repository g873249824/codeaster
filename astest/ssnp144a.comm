                         FISSURE=FISS,
                         CONTACT='P1P1',
                         );

CTXFEM = DEFI_CONTACT(
            MODELE         = MODELEK,
            FORMULATION    = 'XFEM',
            FROTTEMENT     = 'SANS',
            ITER_CONT_MAXI=4,
            ZONE=(
                  _F(INTEGRATION='GAUSS',
                     ORDRE_INT=2,
                     ALGO_CONT='CZM',
                     FISS_MAIT = FISS,
                     ALGO_LAGR='VERSION2',
                     RELATION='CZM_EXP_REG')));

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,
                      INFO=2);



#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
nu = 0.0;
E = 500000.0;

MATE=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu),
                    RUPT_FRAG=_F(GC=0.9E3,
                                 SIGM_C=1.1E6,
                                 PENA_ADHERENCE=1.0E-3,
                                 ),
                   );
CHAMPMA=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA='SURF',MATER=MATE),
                            ),
                      );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------
L_INST=DEFI_LIST_REEL(DEBUT=0.,
