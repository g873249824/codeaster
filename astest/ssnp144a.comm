                                        DDL_2      = 'H1X',
                                        COEF_MULT_1=  1.,
                                        COEF_MULT_2=  -1.,
                                        COEF_IMPO= 0.),
                                     _F(GROUP_NO_1 = ('NINF'),
                                        GROUP_NO_2 = ('NSUP'),
                                        DDL_1      = 'H1X',
                                        DDL_2      = 'H1X',
                                        COEF_MULT_1=  1.,
                                        COEF_MULT_2=  -1.,
                                       COEF_IMPO= 0.)));


