# SDVERI='NON' car la verification est trop couteuse en CPU

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED');

MAILLAG2= COPIER(CONCEPT= MAILLAG1)

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA='SURF',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',
                              ),
                           _F(GROUP_MA=('LIG1','LIG3'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN')));

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURF',
                                      NOM='SURF'),
                                   _F(NOM='SURF_SUP',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N3',
                                      PRECISION=2.1,
                                      VECT_NORMALE=(0.0,1.0)),
                                   _F(NOM='SURF_INF',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N1',
                                      PRECISION=2.1,
                                      VECT_NORMALE=(0.0,1.0)),
                                   _F(NOM='NSUP',
                                      NOEUD=('N7','N10')),
                                   _F(NOM='NINF',
                                      NOEUD=('N6','N11')),
                                   ));

# FORME ANALYTIQUE POUR LA FISSURE

LN = FORMULE(VALE='Y-2.5',
             NOM_PARA=('X','Y'));


FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN),
                    GROUP_MA_ENRI='SURF',
                    INFO=1);

