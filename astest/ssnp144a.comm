                                  _F(JUSQU_A=3.5,
                                     NOMBRE=10),
                                  _F(JUSQU_A=4.5,
                                     NOMBRE=2),
                                  _F(JUSQU_A=5.5,
                                     NOMBRE=2),
                                  _F(JUSQU_A=7,
                                     NOMBRE=10),
                                  _F(JUSQU_A=9.5,
                                     NOMBRE=2),
                                  _F(JUSQU_A=12,
                                     NOMBRE=10),
                                  _F(JUSQU_A=15,
                                     NOMBRE=10),
