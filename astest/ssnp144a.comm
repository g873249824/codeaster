-1000. , 1000. , 1000. , -1000., 1000. , 1000. ]
nbc=len(LINST)
#----------------MODE-I------------------------

DEPY1 = 1.0;

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIG1',
                                DX=0.0,
                                DY=0.0),
                             _F(GROUP_MA='LIG3',
                                DX=0.0,
                                DY=DEPY1)));

#----------------MODE-II-----------------------

DEPX2 = 1.0;
DEPY2 = 1.0E-13;

CH21=AFFE_CHAR_MECA(MODELE=MODELEK,
