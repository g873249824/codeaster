# TESTS DE HYDR_ELNO ET HYDR_NOEU

RESUTHYD=CALC_CHAMP(reuse =RESUTHYD,
                    RESULTAT=RESUTHYD,
                    THERMIQUE='HYDR_NOEU',);

RESUDIAG=CALC_CHAMP(reuse =RESUDIAG,
                    RESULTAT=RESUDIAG,
                    THERMIQUE='HYDR_NOEU',);

HYDRELNO=CREA_CHAMP(TYPE_CHAM='ELNO_HYDR_R',
                    OPERATION='EXTR',
                    RESULTAT=RESUTHYD,
                    NOM_CHAM='HYDR_ELNO',
