ALPHA=DEFI_FONCTION(NOM_PARA='TEMP',VALE=(0.0,10.0E-6,
                          1000.0,10.0E-6,
                          ),PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',);

