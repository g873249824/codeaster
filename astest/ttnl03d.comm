CTRACB=DEFI_NAPPE(NOM_PARA='HYDR',
                  PARA=(0.,50.,),
                  FONCTION=(FCT1,FCT2,),
                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE',);

