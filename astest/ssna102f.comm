#
# COPIE DE SSNA102D POUR TESTER MELANGE ZONES CONTACT/FROTTEMENT
# REFERENCE : CALCULS AVEC ABAQUS, SYSTUS, SAMCEF
#
#########################################
# METHODE DE CONTACT : CONTINUE

DEBUT(CODE=_F(NOM='SSNA102F',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# --------------------------------------------------------------------
#       LECTURE DU MAILLAGE ET CREATION DE GROUPES DE NOEUDS
# --------------------------------------------------------------------

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

#

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(
# GROUPES DE NOEUDS POUR LES DEGRES DE LIBERTE IMPOSES
                        _F(  GROUP_MA = 'LCD1'),
                        _F(  GROUP_MA = 'LBC4'),
# GROUPES DE NOEUDS POUR LES CONDITIONS DE CONTACT
                        _F(  GROUP_MA = 'LAB1S'),
                        _F(  GROUP_MA = 'CEP2S'),
                        _F(  GROUP_MA = 'CME2S'),
                        _F(  GROUP_MA = 'CEP3S'),
                        _F(  GROUP_MA = 'CME3S'),
                        _F(  GROUP_MA = 'LCD4S'),
                        _F(  GROUP_MA = 'LCD4I'),
# LCD4P EST LCD4 SAUF NOEUD PC4 ENCASTRE (POUR LE POST-TRAITEMENT)
                        _F(  NOM = 'LCD4P',
                                        DIFFE = ('LCD4I', 'PC4',)))
                     )

#
# --------------------------------------------------------------------
#                   MODELISATION 2D AXISYMETRIQUE
# --------------------------------------------------------------------
#

MOD=AFFE_MODELE(
                       MAILLAGE=MAIL,
                       AFFE=_F(
                              GROUP_MA=('STRUCT','CEP2S','CEP3S','LCD4S'),
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'AXIS')
                    )

#
# --------------------------------------------------------------------
#         DEFINITION ET AFFECTATION D'UN MATERIAU ELASTIQUE
# --------------------------------------------------------------------
#

ELAS=DEFI_MATERIAU(
                          ELAS=_F(  E = 200000.,
                                 NU = 0.3)
                       )

#

CHMAT=AFFE_MATERIAU(
                           MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',
                                  MATER = ELAS)
                        )

#
# --------------------------------------------------------------------
#                   ENCASTREMENT DE LA LIGNE LBC4
# --------------------------------------------------------------------
#

BLOQ=AFFE_CHAR_CINE(
                           MODELE=MOD,
                              MECA_IMPO=_F(  GROUP_NO = 'LBC4',
                                         DX = 0.0,  DY = 0.0)
                        )

#
# --------------------------------------------------------------------
#               DEPLACEMENT IMPOSE SUR LA LIGNE LCD1
# --------------------------------------------------------------------
#

DEPL=AFFE_CHAR_CINE(
                           MODELE=MOD,
                              MECA_IMPO=_F(  GROUP_NO = 'LCD1',
                                                  DY = -2.0)
                       )

#
# --------------------------------------------------------------------
#     SUPPRESSION DES MOUVEMENTS DE CORPS RIGIDE DES DEUX SPHERES
# --------------------------------------------------------------------
#

LIAI=AFFE_CHAR_MECA(
                           MODELE=MOD,LIAISON_DDL=(
                           _F(  GROUP_NO = ('PA1',  'PP2', ),
                                         DDL = ('DY', 'DY',),
                                         COEF_MULT = (1., -1.,),
                                         COEF_IMPO = 0.),
                           _F(  GROUP_NO = ('PM2',  'PP3', ),
                                         DDL = ('DY', 'DY',),
                                         COEF_MULT = (1., -1.,),
                                         COEF_IMPO = 0.),
                           _F(  GROUP_NO = ('PM3',  'PD4', ),
                                         DDL = ('DY', 'DY',),
                                         COEF_MULT = (1., -1.,),
                                         COEF_IMPO = 0.))
                       )

#
# --------------------------------------------------------------------
#      CONTACT UNILATERAL : FORMULATION MAITRE-ESCLAVE (3 ZONES)
# --------------------------------------------------------------------
#
CONT = DEFI_CONTACT(MODELE         = MOD,
                    FORMULATION    = 'CONTINUE',
                    FROTTEMENT     = 'COULOMB',
                    ALGO_RESO_CONT = 'NEWTON',
                    ALGO_RESO_GEOM = 'NEWTON',
                    ALGO_RESO_FROT = 'NEWTON',
                    ADAPT_COEF     = 'OUI',
                    ZONE = (
                             _F(
                                 GROUP_MA_MAIT = 'LAB1S',
                                 GROUP_MA_ESCL = 'CEP2S',
                                 COULOMB = 0.0,
                               ),
                             _F(
                                 GROUP_MA_MAIT = 'CME2S',
                                 GROUP_MA_ESCL = 'CEP3S',
                                 COULOMB = 0.001,
                               ),
                             _F(
                                 GROUP_MA_MAIT = 'CME3S',
                                 GROUP_MA_ESCL = 'LCD4S',
                                 COULOMB = 0.0,
                               ),
                           ),
                    )

#
# --------------------------------------------------------------------
#         DECOUPAGE EN INCREMENTS DE CHARGE (1 EN ELASTICITE)
#         ET RAMPE MULTIPLICATRICE POUR LE DEPLACEMENT IMPOSE
# --------------------------------------------------------------------
#

FONC=DEFI_FONCTION(
                           NOM_PARA='INST',
                           VALE=( 0.,   0.0,
                                  1.,   1.0, )
                        )

#

LINST=DEFI_LIST_REEL(
                            DEBUT=0.0,
                            INTERVALLE=_F(  JUSQU_A = 1.,  NOMBRE = 1)
                         )

#
# --------------------------------------------------------------------
#                            CALCUL NON LINEAIRE
# --------------------------------------------------------------------
#

RESU=STAT_NON_LINE(
                          MODELE=MOD,
                          CHAM_MATER=CHMAT,EXCIT=(
                          _F(  CHARGE = BLOQ),
                                _F(  CHARGE = DEPL,   FONC_MULT = FONC),
                                _F(  CHARGE = LIAI),),
                          CONTACT  = CONT,
                          COMPORTEMENT=_F(  RELATION = 'ELAS'),
                          INCREMENT=_F(  LIST_INST = LINST),
                          NEWTON   =_F(REAC_ITER=1,) ,
                          CONVERGENCE=_F(
                                        RESI_GLOB_RELA = 1.E-6,
                                        ITER_GLOB_MAXI = 50,
                                        ARRET = 'OUI'),
                          INFO=1
                       )

#
# --------------------------------------------------------------------
#                    CALCUL DES CONTRAINTES AUX NOEUDS
# --------------------------------------------------------------------
#

RESU=CALC_CHAMP( reuse=RESU,  RESULTAT=RESU,

                        CONTRAINTE=('SIGM_ELNO'),
                  )

RESU=CALC_CHAMP( reuse=RESU,  RESULTAT=RESU,
                     CONTRAINTE='SIGM_NOEU'
                  )

#
# --------------------------------------------------------------------
#          CALCUL DES FORCES NODALES ET DES REACTIONS NODALES
# --------------------------------------------------------------------
#


RESU=CALC_CHAMP( reuse=RESU,  RESULTAT=RESU,
                     FORCE='REAC_NODA'
                  )

# --------------------------------------------------------------------
#                COMPARAISON SOURCE EXTERNE
# --------------------------------------------------------------------

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='PA4',
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.841373247,
                  VALE_REFE=-0.81,
                  CRITERE='RELATIF',
                  PRECISION=0.04,),
          )

#
# --------------------------------------------------------------------
#                CALCUL DE LA REACTION A L'ENCASTREMENT
# --------------------------------------------------------------------
#                COMPARAISON SOURCE EXTRENE
# --------------------------------------------------------------------


REACENC=POST_RELEVE_T(
                 ACTION=_F(  RESULTAT = RESU,
                          INST = 1.,
                          INTITULE = 'ENCASTREMENT',
                          NOM_CHAM = 'REAC_NODA',
                          REPERE = 'GLOBAL',
                          OPERATION = 'EXTRACTION',
                          GROUP_NO = 'LBC4',
                          RESULTANTE = ('DX', 'DY',))
              )

#

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.03,
           VALE_CALC=1.75014680E+04,
           VALE_REFE=1.7229580000000002E4,
           NOM_PARA='DY',
           TABLE=REACENC,)

FIN()
#
