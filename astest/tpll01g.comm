DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

ACIER=DEFI_MATERIAU(  THER=_F(  LAMBDA = 0.75, RHO_CP = 2.0))

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

MAIL=DEFI_GROUP(reuse=MAIL,MAILLAGE=MAIL,
                CREA_GROUP_NO=_F( TOUT_GROUP_MA = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                        AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                     AFFE=_F( TOUT = 'OUI', MODELISATION = '3D',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER(   MODELE=MOTH,
                   FLUX_REP =_F( GROUP_MA = 'G2',    FLUN = -1200.0),
                   ECHANGE  =_F( GROUP_MA = 'F1SUP', COEF_H = 30.0,
                                                     TEMP_EXT = 140.0),
                   TEMP_IMPO=_F( GROUP_NO = 'F1INF', TEMP = 100.0)
                       )

LR8=DEFI_LIST_REEL(        DEBUT=0.,
       INTERVALLE=_F(    JUSQU_A = 10.0,   NOMBRE = 2)
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
          INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH)
             )

TEMPE=CALC_CHAMP(reuse=TEMPE,RESULTAT=TEMPE,THERMIQUE=('FLUX_ELNO','FLUX_ELGA'))



TEST_RESU(RESU=(_F(NUME_ORDRE=2,
                   GROUP_NO='A',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=TEMPE,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=100.000000000,
                   VALE_REFE=100.0,),
                _F(NUME_ORDRE=2,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=TEMPE,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=20.000000000,
                   VALE_REFE=20.0,),
                _F(NUME_ORDRE=2,
                   GROUP_NO='G',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=TEMPE,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=60.000000000,
                   VALE_REFE=60.0,),
                _F(NUME_ORDRE=2,
                   REFERENCE='ANALYTIQUE',
                   POINT=3,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELNO',
                   NOM_CMP='FLUX',
                   VALE_CALC=960.000000000,
                   VALE_REFE=960.0,
                   MAILLE='M1',),
                _F(NUME_ORDRE=2,
                   REFERENCE='ANALYTIQUE',
                   POINT=2,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELNO',
                   NOM_CMP='FLUY',
                   VALE_CALC=720.000000000,
                   VALE_REFE=720.0,
                   MAILLE='M2',),
                _F(NUME_ORDRE=2,
                   REFERENCE='ANALYTIQUE',
                   POINT=3,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELGA',
                   NOM_CMP='FLUX',
                   VALE_CALC=960.000000000,
                   VALE_REFE=960.0,
                   MAILLE='M1',),
                _F(NUME_ORDRE=2,
                   REFERENCE='ANALYTIQUE',
                   POINT=2,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELGA',
                   NOM_CMP='FLUY',
                   VALE_CALC=720.000000000,
                   VALE_REFE=720.0,
                   MAILLE='M2',),
                ),
          )

FIN()
