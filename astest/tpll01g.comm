DEBUT(CODE=_F( NOM = 'TPLL01G',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

ACIER=DEFI_MATERIAU(  THER=_F(  LAMBDA = 0.75, RHO_CP = 2.0))

PRE_GIBI ( )

MAIL=LIRE_MAILLAGE( )

MAIL=DEFI_GROUP(reuse=MAIL,MAILLAGE=MAIL,
                CREA_GROUP_NO=_F( TOUT_GROUP_MA = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                        AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                     AFFE=_F( TOUT = 'OUI', MODELISATION = '3D',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER(   MODELE=MOTH,
                   FLUX_REP =_F( GROUP_MA = 'G2',    FLUN = -1200.0),
                   ECHANGE  =_F( GROUP_MA = 'F1SUP', COEF_H = 30.0,
                                                     TEMP_EXT = 140.0),
                   TEMP_IMPO=_F( GROUP_NO = 'F1INF', TEMP = 100.0)
                       )

LR8=DEFI_LIST_REEL(        DEBUT=0.,
       INTERVALLE=_F(    JUSQU_A = 10.0,   NOMBRE = 2)
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
          INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH)
             )

TEMPE=CALC_ELEM( reuse=TEMPE,
                 RESULTAT=TEMPE,
                 OPTION=('FLUX_ELNO',
                 'FLUX_ELGA',),);


TEST_RESU(RESU=(
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   GROUP_NO = 'A', REFERENCE = 'ANALYTIQUE', NOM_CMP = 'TEMP', VALE = 100.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   GROUP_NO = 'B', REFERENCE = 'ANALYTIQUE', NOM_CMP = 'TEMP', VALE = 20.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   GROUP_NO = 'G', REFERENCE = 'ANALYTIQUE', NOM_CMP = 'TEMP', VALE = 60.0),

   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELNO',
   MAILLE = 'M1',   REFERENCE = 'ANALYTIQUE', POINT = 3,
   NOM_CMP = 'FLUX', VALE = 960.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELNO',
   MAILLE = 'M2',   REFERENCE = 'ANALYTIQUE', POINT = 2,
   NOM_CMP = 'FLUY', VALE = 720.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELGA',
   MAILLE = 'M1',   REFERENCE = 'ANALYTIQUE', POINT = 3,
   NOM_CMP = 'FLUX', VALE = 960.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELGA',
   MAILLE = 'M2',   REFERENCE = 'ANALYTIQUE', POINT = 2,
   NOM_CMP = 'FLUY', VALE = 720.0))
          )

FIN()
