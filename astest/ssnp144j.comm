DEBUT(CODE=_F(NOM='SSNP144J',NIV_PUB_WEB='INTERNET',),);
#----------------------------------------------
#                MAILLAGE : ssnp144j.mgib
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE();

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       );

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA='SURF',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',                              
                              ),
                           _F(GROUP_MA=('LIG1','LIG3',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),),);
 
MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURF',
                                      NOM='SURF',),
                                   _F(NOM='SURF_SUP',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N3',
                                      PRECISION=2.1,
                                      VECT_NORMALE=(0.0,1.0),),   
                                   _F(NOM='SURF_INF',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N1',
                                      PRECISION=1.9,
                                      VECT_NORMALE=(0.0,1.0),),
                                   _F(NOM='NSUP',
                                      NOEUD=('N7','N10',),),
                                   _F(NOM='NINF',
                                      NOEUD=('N6','N11',),),
                                   _F(NOM='NSUP2',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N7',
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=0.00001,),
                                   _F(NOM='NINF2',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N6',
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=0.00001,),),);

# FORME ANALYTIQUE POUR LA FISSURE

LN = FORMULE(VALE='Y-2.0',
             NOM_PARA=('X','Y',),);


FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),
                    GROUP_MA_ENRI='SURF',
                    INFO=1,);
                                        
#-------------------------------------------------
#                   CONTACT
#-------------------------------------------------

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='P1P1',                         
                         );

CTXFEM = DEFI_CONTACT(
            MODELE         = MODELEK,
            FORMULATION    = 'XFEM',
            FROTTEMENT     = 'COULOMB',
            ITER_CONT_MAXI=4,
            ITER_FROT_MAXI=10,
            ZONE=(
                  _F(INTEGRATION='FPG2',                                   
                     COULOMB=1.0,
                     COEF_CONT=100.,
                     COEF_FROT=1000., 
                     CONTACT_INIT='NON', 
                     COEF_ECHELLE=1.,
                     FISS_MAIT = FISS,
                     ALGO_LAGR='VERSION2',
                     RELATION='CZM_EXP_REG',),),);
                       
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,
                      INFO=2,);
                      

           
#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
nu = 0.0;
E = 500000.0;

MATE=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,),
                    RUPT_FRAG=_F(GC=0.9E3,
                                 SIGM_C=1.1E6,
                                 PENA_ADHERENCE=1.0E-3,
                                 ),
                   );
CHAMPMA=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA='SURF',MATER=MATE,),
                            ),
                      );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=(
                                  _F(JUSQU_A=0.5,
                                    NOMBRE=2,),
                                  _F(JUSQU_A=0.75,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=2,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=3.5,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=4.5,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=5.5,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=7,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=9.5,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=12,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=15,
                                     NOMBRE=10,),
                                    ),);
LINST=[0.5 , 0.75 , 2. , 3.5 , 4.5 , 5.5 , 7. , 9.5 , 12 , 15 ]

LCOEF=[1.83335E6 ,-1.83335E6 , 1.83335E6 , 1000.54574726 ,
-1000. , 1000. , 1000. , -1000., 1000. , 1000. ]   
nbc=len(LINST)                                 
#----------------MODE-I------------------------

DEPY1 = 1.0;

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIG1',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_MA='LIG3',
                                DX=0.0,
                                DY=DEPY1,),),);

#----------------MODE-II-----------------------
DEPX2 = 1.0;
DEPY2 = 1.0E-13;

CH21=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_NO='SURF_SUP',
                                DX=DEPX2,),),);

CH22=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_NO='SURF_INF',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='SURF_SUP',
                                DY=DEPY2,),
                             _F(NOEUD=('N6','N11',),
                                H1Y=DEPY2,),),
                      LIAISON_GROUP=(_F(GROUP_NO_1 = ('NINF'),
                                        GROUP_NO_2 = ('NINF'),
                                        DDL_1      = 'DX',
                                        DDL_2      = 'H1X',
                                        COEF_MULT_1=  1.,
                                        COEF_MULT_2=  -1.,
                                        COEF_IMPO= 0.,),
                                     _F(GROUP_NO_1 = ('NINF'),
                                        GROUP_NO_2 = ('NSUP'),
                                        DDL_1      = 'DX',
                                        DDL_2      = 'DX',
                                        COEF_MULT_1=  1.,
                                        COEF_MULT_2=  -0.5,
                                       COEF_IMPO= 0.,),
                                        ),);


#--------------------------------------------------------
#                       CALCUL - MODE I
#--------------------------------------------------------

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH1,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=0.5,),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_IMPO',
                                COEF_MULT=1.83335E6,
                                FISSURE=FISS,
                                DIRE_PILO='DY',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               NPREC=-1,
                               ),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

for i in range(1,nbc) :
  UTOT1=STAT_NON_LINE(reuse =UTOT1,
                    ETAT_INIT=_F(EVOL_NOLI=UTOT1,),
                    MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH1,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT = CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=LINST[i],),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_IMPO',
                                COEF_MULT=LCOEF[i],
                                FISSURE=FISS,
                                DIRE_PILO='DY',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               NPREC=-1,
                               ),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

#--------------------------------------------------------
#                      CALCUL - MODE II
#--------------------------------------------------------

UTOT2=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH22,),
                           _F(CHARGE=CH21,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=0.5,),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_IMPO',
                                COEF_MULT=1.83335E6,
                                FISSURE=FISS,
                                DIRE_PILO='DX',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=50,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,); 

for i in range(1,nbc) :
   UTOT2=STAT_NON_LINE(
                    reuse =UTOT2,
                    ETAT_INIT=_F(EVOL_NOLI=UTOT2,),
                    MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH22,),
                           _F(CHARGE=CH21,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=LINST[i],),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_IMPO',
                                COEF_MULT=LCOEF[i],
                                DIRE_PILO='DX',
                                FISSURE=FISS,
                               ),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=50,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);
# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------
MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK,);

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='C_PLAN',),);

RESXFEM1=POST_CHAM_XFEM(RESULTAT=UTOT1,
                        MODELE_VISU=MOD_VISU,);
                        
RESXFEM2=POST_CHAM_XFEM(RESULTAT=UTOT2,
                        MODELE_VISU=MOD_VISU,);                         
#--------------------------------------------------------
#                  VALIDATION DES RESULTATS
#--------------------------------------------------------
TOLE=1.0E-8 

LLAG=[3.66296853301E+05 ,
 1.8314842665E+05 ,
 1.098890559903E+06 ,
 1.75867720687844E+05 ,
 58622.573562549 ,
 1.75867720687844E+05 ,
 28117.686527187 ,
 4686.28108785798 ,
 28117.686527187 ,
 718.731177854856 ]
LDEP=[1.36362396705485E-07 ,
 6.818119835274E-08 ,
 4.0908719011645E-07 ,
 7.49999999999582E-04 ,
 2.49999999999582E-04 ,
 7.49999999999582E-04 ,
 1.49999999999958E-03 ,
 2.49999999999582E-04 ,
 1.49999999999958E-03 ,
 2.99999999999958E-03 ]

TAB1 = [None]*nbc
TAB2 = [None]*nbc
TAB3 = [None]*nbc
TAB4 = [None]*nbc
#--------------------------------------------------------
#                           MODE I    
#--------------------------------------------------------

for i in range(0,nbc) :

   TAB1[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NFISSU',),
                                   RESULTAT=RESXFEM1,
                                   NOM_CHAM='DEPL',
                                   INST=LINST[i],
                                   TOUT_CMP='OUI',
                                   OPERATION='EXTRACTION',),);
   TAB1[i]=CALC_TABLE(reuse = TAB1[i],
                      TABLE = TAB1[i],
                      ACTION = (_F(OPERATION = 'FILTRE',
                                   NOM_PARA  = 'NOEUD',
                                   CRIT_COMP = 'REGEXP',
                                   VALE_K    = 'N[^P]',),),);

   TAB2[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NINF',),
                                   RESULTAT=UTOT1,
                                   NOM_CHAM='DEPL',
                                   INST=LINST[i],
                                   TOUT_CMP='OUI',
                                   OPERATION='EXTRACTION',),);
   TEST_TABLE(TABLE=TAB1[i],
              NOM_PARA='LAGS_C',
              TYPE_TEST='MAX',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB1[i],
              NOM_PARA='LAGS_C',
              TYPE_TEST='MIN',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB2[i],
              NOM_PARA='H1Y',
              TYPE_TEST='MAX',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB2[i],
              NOM_PARA='H1Y',
              TYPE_TEST='MIN',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);

#--------------------------------------------------------
#                           MODE II   
#--------------------------------------------------------
              
for i in range(0,nbc) :

   TAB3[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NFISSU',),
                                   RESULTAT=RESXFEM2,
                                   NOM_CHAM='DEPL',
                                   INST=LINST[i],
                                   TOUT_CMP='OUI',
                                   OPERATION='EXTRACTION',),);
   TAB3[i]=CALC_TABLE(reuse = TAB3[i],
                      TABLE = TAB3[i],
                      ACTION = (_F(OPERATION = 'FILTRE',
                                   NOM_PARA  = 'NOEUD',
                                   CRIT_COMP = 'REGEXP',
                                   VALE_K    = 'N[^P]',),),);

   TAB4[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NINF',),
                                   RESULTAT=UTOT2,
                                   NOM_CHAM='DEPL',
                                   INST=LINST[i],
                                   TOUT_CMP='OUI',
                                   OPERATION='EXTRACTION',),);
   TEST_TABLE(TABLE=TAB3[i],
              NOM_PARA='LAGS_F1',
              TYPE_TEST='MAX',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              VALE_ABS='OUI',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB3[i],
              NOM_PARA='LAGS_F1',
              TYPE_TEST='MIN',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              VALE_ABS='OUI',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB4[i],
              NOM_PARA='H1X',
              TYPE_TEST='MAX',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB4[i],
              NOM_PARA='H1X',
              TYPE_TEST='MIN',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',); 
FIN();
