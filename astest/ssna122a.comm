DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),IGNORE_ALARM='CONTACT2_14',DEBUG=_F(SDVERI='OUI'))

MESH=LIRE_MAILLAGE(FORMAT='MED',);

MESH=MODI_MAILLAGE(reuse =MESH,
                   MAILLAGE=MESH,
                   ORIE_PEAU_2D=(_F(GROUP_MA='P_cont',),
                                 _F(GROUP_MA='M_cont',),),);

MAT_pu=DEFI_MATERIAU(ELAS=_F(E=210000.0,
                             NU=0.3,),);

MAT_fo=DEFI_MATERIAU(ELAS=_F(E=70000.0,
                             NU=0.3,),);

FONC=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                         1.0,1.0,
                         ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=2,),);

MO=AFFE_MODELE(MAILLAGE=MESH,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

MA=AFFE_MATERIAU(MAILLAGE=MESH,
                 AFFE=(_F(GROUP_MA='Massif',
                          MATER=MAT_fo,),
                       _F(GROUP_MA='Poincon',
                          MATER=MAT_pu,),),);

CL=AFFE_CHAR_CINE(MODELE=MO,
                  MECA_IMPO=(_F(GROUP_MA='Axe',
                                DX=0.0,),
                             _F(GROUP_MA='Encast',
                                DX=0.0,
                                DY=0.0,),),);

Force=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_CONTOUR=_F(GROUP_MA='Force',
                                      FY=-100.0,),);

Cont=DEFI_CONTACT(MODELE         = MO,
                  FORMULATION    = 'CONTINUE',
                  ALGO_RESO_CONT = 'NEWTON',
                  ALGO_RESO_GEOM = 'NEWTON',
                  ZONE=_F(GROUP_MA_MAIT='M_cont',
                          GROUP_MA_ESCL='P_cont',
                          CONTACT_INIT='INTERPENETRE',
                          ),);

ContFr=DEFI_CONTACT(MODELE       = MO,
                  FORMULATION    = 'CONTINUE',
                  FROTTEMENT     = 'COULOMB',
                    ALGO_RESO_CONT = 'NEWTON',
                    ALGO_RESO_GEOM = 'NEWTON',
                    ALGO_RESO_FROT = 'NEWTON',
                    ADAPT_COEF     = 'OUI',
                  ZONE=_F(GROUP_MA_MAIT='M_cont',
                          GROUP_MA_ESCL='P_cont',
                          CONTACT_INIT='INTERPENETRE',
                          COULOMB=0.1,
                          SANS_GROUP_NO_FR='P',
                          ),);

MESH=DEFI_GROUP(reuse =MESH,
                  MAILLAGE=MESH,
                  CREA_GROUP_NO=(_F(OPTION='NOEUD_ORDO',
                                   NOM='Bottom_n',
                                   GROUP_MA='M_cont',
                                   GROUP_NO_ORIG='M',),
                                 _F(OPTION='NOEUD_ORDO',
                                   NOM='Top_n',
                                   GROUP_MA='P_cont',
                                   GROUP_NO_ORIG='P',),),);
# Calcul sans frottement

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=MA,
                   EXCIT=(_F(CHARGE=Force,
                             FONC_MULT=FONC,),
                          _F(CHARGE=CL,),
                         ),
                   CONTACT=Cont,
                   COMPORTEMENT=_F(RELATION='ELAS',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=50,),
                   SOLVEUR=_F(),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=1,),);

RESU=CALC_CHAMP(reuse=RESU,RESULTAT=RESU,CONTRAINTE=('SIEF_ELNO'))


RESU=CALC_CHAMP(reuse =RESU,
               RESULTAT=RESU,
               CONTRAINTE='SIEF_NOEU',FORCE=('FORC_NODA','REAC_NODA'));

# Calcul avec frottement

RESUFr=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=MA,
                   EXCIT=(_F(CHARGE=Force,
                             FONC_MULT=FONC,),
                          _F(CHARGE=CL,),
                         ),
                   CONTACT=ContFr,
                   COMPORTEMENT=_F(RELATION='ELAS',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=50,),
                   SOLVEUR=_F(),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=1,),);

RESUFr=CALC_CHAMP(reuse=RESUFr,RESULTAT=RESUFr,CONTRAINTE=('SIEF_ELNO'))


RESUFr=CALC_CHAMP(reuse =RESUFr,
               RESULTAT=RESUFr,
               CONTRAINTE='SIEF_NOEU',FORCE=('FORC_NODA','REAC_NODA'));


IMPR_RESU(FORMAT='MED',
          UNITE=80,
          RESU=(_F(RESULTAT=RESU,
                  NOM_CHAM=('DEPL','FORC_NODA',),),
                _F(RESULTAT=RESUFr,
                  NOM_CHAM=('DEPL','FORC_NODA',),),),
                );

IMPR_RESU(FORMAT='MED',
          UNITE=81,
          RESU=(_F(RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP=('DX','DY',),),
                _F(RESULTAT=RESUFr,
                  NOM_CHAM='DEPL',
                  NOM_CMP=('DX','DY',),),),
                );

RELE=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                              INTITULE='Depl. axial',
                              RESULTAT=RESU,
                              NOM_CHAM='DEPL',
                              INST=1,
                              GROUP_NO='Bottom_n',
                              NOM_CMP='DY',),
                           _F(OPERATION='EXTRACTION',
                              INTITULE='Depl. radial',
                              RESULTAT=RESU,
                              NOM_CHAM='DEPL',
                              INST=1,
                              GROUP_NO='Bottom_n',
                              NOM_CMP='DX',),
                           _F(OPERATION='EXTRACTION',
                              INTITULE='Pression',
                              RESULTAT=RESU,
                              NOM_CHAM='DEPL',
                              INST=1,
                              GROUP_NO='Top_n',
                              NOM_CMP='LAGS_C',),
                           ),);

RELEFr=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                              INTITULE='Depl. axial avec frott.',
                              RESULTAT=RESUFr,
                              NOM_CHAM='DEPL',
                              INST=1,
                              GROUP_NO='Bottom_n',
                              NOM_CMP='DY',),
                           _F(OPERATION='EXTRACTION',
                              INTITULE='Depl. radial avec frott.',
                              RESULTAT=RESUFr,
                              NOM_CHAM='DEPL',
                              INST=1,
                              GROUP_NO='Bottom_n',
                              NOM_CMP='DX',),
                           _F(OPERATION='EXTRACTION',
                              INTITULE='Pression avec frott.',
                              RESULTAT=RESUFr,
                              NOM_CHAM='DEPL',
                              INST=1,
                              GROUP_NO='Top_n',
                              NOM_CMP=('LAGS_C','LAGS_F1',)),
                           ),);

MOINS_UN=FORMULE(NOM_PARA='LAGS_C',VALE='-1.0*LAGS_C',);
PRESTANG=FORMULE(NOM_PARA=('LAGS_C','LAGS_F1'),VALE='-0.1*LAGS_C*abs(LAGS_F1)',);

RELE=CALC_TABLE(reuse=RELE,
                TABLE=RELE,
                ACTION=(_F(OPERATION='OPER',
                           NOM_PARA='MLAGS_C',
                           FORMULE=MOINS_UN,),
                        ),);

RELEFr=CALC_TABLE(reuse=RELEFr,
                TABLE=RELEFr,
                ACTION=(_F(OPERATION='OPER',
                           NOM_PARA='MLAGS_C',
                           FORMULE=MOINS_UN,),
                        _F(OPERATION='OPER',
                           NOM_PARA='MLAGS_F1',
                           FORMULE=PRESTANG,),
                        ),);

DEPLax=RECU_FONCTION(TABLE=RELE,
                     PARA_X='ABSC_CURV',
                     PARA_Y='DY',);

DEPLra=RECU_FONCTION(TABLE=RELE,
                     PARA_X='ABSC_CURV',
                     PARA_Y='DX',);

PRES=RECU_FONCTION(TABLE=RELE,
                   PARA_X='ABSC_CURV',
                   PARA_Y='MLAGS_C',);

DEPLaxFr=RECU_FONCTION(TABLE=RELEFr,
                     PARA_X='ABSC_CURV',
                     PARA_Y='DY',);

DEPLraFr=RECU_FONCTION(TABLE=RELEFr,
                     PARA_X='ABSC_CURV',
                     PARA_Y='DX',);

PRESFr=RECU_FONCTION(TABLE=RELEFr,
                   PARA_X='ABSC_CURV',
                   PARA_Y='MLAGS_C',);

PRESTgFr=RECU_FONCTION(TABLE=RELEFr,
                   PARA_X='ABSC_CURV',
                   PARA_Y='MLAGS_F1',);

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=29,
              COURBE=(_F(FONCTION=DEPLax,LEGENDE='Sans frottement',MARQUEUR=0,),
                      _F(FONCTION=DEPLaxFr,LEGENDE='Frottement 0.1',MARQUEUR=0,),),
              TITRE='Deplacement axial en fonction du rayon',
              BORNE_X=(0.0,100.0,),
              BORNE_Y=(-0.14,0.0,),
              ECHELLE_X='LIN',
              ECHELLE_Y='LIN',
              GRILLE_X=10,
              GRILLE_Y=0.02,
              LEGENDE_X='Rayon (mm)',
              LEGENDE_Y='Deplacement axial (mm)',);

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=30,
              COURBE=(_F(FONCTION=DEPLra,LEGENDE='Sans frottement',MARQUEUR=0,),
                      _F(FONCTION=DEPLraFr,LEGENDE='Frottement 0.1',MARQUEUR=0,),),
              TITRE='Deplacement radial en fonction du rayon',
              BORNE_X=(0.0,100.0,),
              BORNE_Y=(-0.018,0.002,),
              ECHELLE_X='LIN',
              ECHELLE_Y='LIN',
              GRILLE_X=10,
              GRILLE_Y=0.002,
              LEGENDE_X='Rayon (mm)',
              LEGENDE_Y='Deplacement radial (mm)',);

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=31,
              COURBE=(_F(FONCTION=PRES,LEGENDE='Sans frottement',MARQUEUR=0,),
                      _F(FONCTION=PRESFr,LEGENDE='Frottement 0.1',MARQUEUR=0,),),
              TITRE='Pression',
              BORNE_X=(0.0,60.0,),
              BORNE_Y=(0.0,450.0,),
              ECHELLE_X='LIN',
              ECHELLE_Y='LIN',
              GRILLE_X=10,
              GRILLE_Y=50,
              LEGENDE_X='Rayon',
              LEGENDE_Y='Pression',);

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=32,
              COURBE=(_F(FONCTION=PRESTgFr,MARQUEUR=0,),),
              TITRE='Pression',
              BORNE_X=(0.0,60.0,),
              BORNE_Y=(-5.0,45.0,),
              ECHELLE_X='LIN',
              ECHELLE_Y='LIN',
              GRILLE_X=10,
              GRILLE_Y=5,
              LEGENDE_X='Rayon',
              LEGENDE_Y='Pression',);

# ===========================================
# Tests

TEST_RESU(RESU=(_F(GROUP_NO='M',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.130734196,
                   VALE_REFE=-0.13073410182805001,
                   ),
                _F(GROUP_NO='M',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIEF_NOEU',
                   NOM_CMP='SIYY',
                   VALE_CALC=-91.100611838,
                   VALE_REFE=-91.100555673515004,
                   ),
                ),
          )

TEST_RESU(RESU=(_F(GROUP_NO='M',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESUFr,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.128580159,
                   VALE_REFE=-0.12858017574262001,
                   ),
                _F(GROUP_NO='M',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESUFr,
                   NOM_CHAM='SIEF_NOEU',
                   NOM_CMP='SIYY',
                   VALE_CALC=-88.665558926,
                   VALE_REFE=-88.665240835356002,
                   ),
                ),
          )

FIN();
