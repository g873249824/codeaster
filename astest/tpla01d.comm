DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
# LECTURE DU MAILLAGE ----------------------------------------------
#

MAILL=LIRE_MAILLAGE(UNITE = 20,
                     FORMAT = 'MED')

IMPR_RESU(RESU=_F(MAILLAGE=MAILL))
#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ------------------------
#

MATE=DEFI_MATERIAU( THER=_F( RHO_CP = 2.0, LAMBDA = 1.))

#
# AFFECTATIONS: MATERIAU  MODELE  CHARGEMENT ----------------------
#

MODEL=AFFE_MODELE(
                  MAILLAGE=MAILL,
                     AFFE=_F( TOUT = 'OUI',  PHENOMENE = 'THERMIQUE',
                                       MODELISATION = 'COQUE_AXIS'))

CHMATE=AFFE_MATERIAU(   MAILLAGE=MAILL,
                    AFFE=_F( TOUT = 'OUI',  MATER = MATE))

CHARGE=AFFE_CHAR_THER(
                      MODELE=MODEL,
                  TEMP_IMPO=(
                     _F( GROUP_NO = 'A',
                         TEMP_INF = 100.0,
                         TEMP_MIL = 100.0,
                         TEMP_SUP = 100.0),
                     _F( GROUP_NO = 'B',
                         TEMP_INF = 20.,
                         TEMP_MIL = 20.,
                         TEMP_SUP = 20.),
                  ))

CARE=AFFE_CARA_ELEM(   MODELE=MODEL,
                       COQUE=_F( GROUP_MA = 'AB',
                                 EPAIS = 0.1,))

#
# RESOLUTION ------------------------------------------------------
#

TEMP=THER_LINEAIRE(MODELE=MODEL,
                   CHAM_MATER=CHMATE,
           CARA_ELEM=CARE,
                   EXCIT=_F(  CHARGE = CHARGE) )

#
# ANALYSE DES RESULTATS -------------------------------------------
#

