                       _F(  GROUP_MA = 'SURFESCL', ),
                       _F(  GROUP_MA = 'CORDINF', ),
                       _F(  GROUP_MA = 'CASTR',))
                    )

#---------------------------------------------------
MO=AFFE_MODELE(
