TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='P7',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU32,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=1.9332758863142E-06,
                   VALE_REFE=Uz,
