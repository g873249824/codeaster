                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=1.9107795124485E-06,
                   VALE_REFE=Uz,
                   PRECISION=0.050000000000000003,),
                ),
          )

# CAS 3 :

h=0.1

MAIL33=LIRE_MAILLAGE(FORMAT='MED',UNITE=23);

MAIL33=MODI_MAILLAGE(reuse=MAIL33,MAILLAGE=MAIL33,
ORIE_SHB=_F(GROUP_MA='GEOTOT'),)

