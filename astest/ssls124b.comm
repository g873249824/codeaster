                   RESULTAT=RESU31,
                   NOM_CHAM='SIEQ_ELNO',
                   VALE_CALC=490.1939943253,
                   ),
                ),
          )

Iy=b*h*h*h/12
Uz=F*L*L*L/3/YG/Iy


TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='P7',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU31,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=1.9443071181675E-09,
                   VALE_REFE=Uz,
                   PRECISION=0.040000000000000001,),
