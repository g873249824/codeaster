                _F(NUME_ORDRE=1,
                   TYPE_TEST='SOMM_ABS',
                   RESULTAT=RESU31,
                   NOM_CHAM='SIEF_ELGA',
                   VALE_CALC=12.303149306899,
                   ),
                _F(NUME_ORDRE=1,
                   TYPE_TEST='SOMM_ABS',
                   RESULTAT=RESU31,
                   NOM_CHAM='SIEF_ELNO',
                   VALE_CALC=30.168275718667,
                   ),
                _F(NUME_ORDRE=1,
                   TYPE_TEST='SOMM_ABS',
                   RESULTAT=RESU31,
                   NOM_CHAM='SIGM_ELNO',
