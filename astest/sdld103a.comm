F2 = FORMULE(NOM_PARA='INST',VALE='cos(OMEGA2*INST)-1. ')
F3 = FORMULE(NOM_PARA='INST',VALE='cos(OMEGA3*INST)-1. ')

# CALCUL DU DEPLACEMENT RELATIF AU NOEUD 2 : XRNO2
# ------------------------------------------------
