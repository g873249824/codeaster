XRNO2a = FORMULE(NOM_PARA='INST',VALE='''
-25.*(7*INST**2+((10.+7.*sqrt(2.))*F1(INST) +F2(INST)
                        +(10.-7.*sqrt(2.))*F3(INST))*0.001)
        ''')

