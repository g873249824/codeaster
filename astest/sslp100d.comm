               _F( NOEUD_FIN = 'N4',   SUPER_MAILLE = 'S_1', NOEUD_INIT = 'N4'),
               _F( NOEUD_FIN = 'N7',   SUPER_MAILLE = 'S_1', NOEUD_INIT = 'N7'),
               _F( NOEUD_FIN = 'N10',  SUPER_MAILLE = 'S_1', NOEUD_INIT = 'N10')),
