#  ---------------------
     DEFI_SUPER_MAILLE=_F( MACR_ELEM = S_123, SUPER_MAILLE = 'S_123'),
    DEFI_NOEUD=_F( TOUT = 'OUI', INDEX = (1,0,1,8,))
