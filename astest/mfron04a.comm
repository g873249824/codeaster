                             #F_MRZ_RZ=0.75,               C_MRZ_RZ=0.75,
                             #F_MTZ_TZ=0.75,               C_MTZ_TZ=0.75,),),


MATF=DEFI_MATERIAU( 
                  ELAS_META=_F(E = 80000.,
                               NU = 0.35,
                               F_ALPHA = 0.,
                               C_ALPHA = 0.,
                               PHASE_REFE = 'FROID',
                               EPSF_EPSC_TREF = 0.,),
                  UMAT=_F( NB_VALE=26,
                           C1  =80000.,    
                           C2  =0.35, 
                           C3  =2.39,       ## F1_A  
                           C4  =0.22,       ## F2_A   
                           C5  =9.36,       ## C_A
                           C6  =0.0,        ## F1_M
                           C7  =0.77E-04,   ## F2_M
                           C8  =0.99E-04,   ## C_M
                           C9  =4.39,       ## F1_N
                           C10 =2.96,       ## F2_N
                           C11 =6.11,       ## C_N
                           C12 =19922.8,    ## F1_Q
                           C13 =21023.7,    ## F2_Q,  
                           C14 =6219.,      ## C_Q
                           C15 =0.4414,     ## F_M11          
                           C16 =1.,         ## F_M22     
                           C17 =0.714,      ## F_M33  
                           C18 =0.75,       ## F_M12
                           C19 =0.75,       ## F_M13
                           C20 =0.75,       ## F_M23        
                           C21 =1.,         ## C_M11
                           C22 =1.,         ## C_M22
                           C23 =1.,         ## C_M33
                           C24 =0.75,       ## C_M12    
                           C25 =0.75,       ## C_M13  
                           C26 =0.75,       ## C_M23
                            ),)
                            
CMF=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F( TOUT = 'OUI', MATER = MATF ),
                  AFFE_VARC=(_F(TOUT='OUI',
                                EVOL=TEMPE,
                                NOM_VARC='M_ZIRC',
                                NOM_CHAM='META_ELNO'),
                             _F(TOUT='OUI',
                                EVOL=TEMPE,
                                VALE_REF=700.,
                                NOM_VARC='TEMP',
                                NOM_CHAM='TEMP'),))


MO=AFFE_MODELE(MAILLAGE=MAIL,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE ='MECANIQUE',
                       MODELISATION ='AXIS'))

CH_L=AFFE_CHAR_MECA(MODELE=MO,
                    FACE_IMPO=_F(GROUP_MA='FACE_INF',DNOR=0.,),
                    LIAISON_UNIF=_F(GROUP_MA='FACE_SUP',DDL='DY',),);

# ON MET UNE PRESSION INTERNE ET L EFFET DE FOND
RINT = 4.118;
REXT = 4.746;
COEF = ((RINT * RINT) / ((REXT * REXT) - (RINT * RINT)));

CHPC=AFFE_CHAR_MECA(MODELE=MO,
                    PRES_REP=(_F(GROUP_MA='FACE_INT',PRES=1.,),
                              _F(GROUP_MA='FACE_SUP',PRES=-COEF,),),);

FMULT=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,
                          1.1,7.5,
                          100.0,9.5,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

VF=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CMF,
                EXCIT=(_F(CHARGE=CHPC,FONC_MULT=FMULT,),
                       _F(CHARGE=CH_L,),),
               
                COMPORTEMENT=_F(RELATION='MFRONT',
                        LIBRAIRIE='meta_lema_ani.so',
                        NOM_ROUTINE='astermetalemani',
                        RESI_INTE_RELA=1e-10,
                        ),
               INCREMENT=_F(LIST_INST = L_INST),
               NEWTON=_F(MATRICE = 'TANGENTE',
                         REAC_ITER = 1),
               CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                              ITER_GLOB_MAXI = 30))
                              
#IMPR_RESU(RESU=_F(RESULTAT=VF)) # pour voir les variables internes

VFP=CALC_CHAMP( RESULTAT=VF,  DEFORMATION='EPSI_NOEU',CONTRAINTE='SIGM_NOEU');

