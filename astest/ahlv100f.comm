# E.F. CLASSIQUES PLANS
#  MODELISATION 'ACOUSTIQUE' PLANE AVEC ELEMENTS QUAD8 ET SEG3
# VALIDATION INDEPENDANTE PAR LMS_NT (J.P.ROSSION)
#
DEBUT(CODE=_F(NOM='AHLV100F', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

PRE_IDEAS()

#
MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(
         MAILLAGE=MA,
         AFFE=_F( TOUT = 'OUI',
              PHENOMENE = 'ACOUSTIQUE',
              MODELISATION = 'PLAN')
         )

#
# PROPRIETES MECANIQUES
#
AIR=DEFI_MATERIAU(
          FLUIDE=_F( RHO = 1.3,
                CELE_C = ('RI',343.0,0.,))
         )

CHM=AFFE_MATERIAU(
         MAILLAGE=MA,
         AFFE=_F( TOUT = 'OUI',
              MATER = AIR)
         )

#
# CONDITIONS AUX LIMITES
#
VELO=AFFE_CHAR_ACOU(
         MODELE=MO,
         VITE_FACE=_F( GROUP_MA = 'VELO',
                   VNOR = ('RI',0.014,0.0,))
         )

IMPE=AFFE_CHAR_ACOU(
         MODELE=MO,
         IMPE_FACE=_F( GROUP_MA = 'IMPE',
                   IMPE = ('RI',445.9,0.0,))
         )

#
# ASSEMBLAGE
#
MACRO_MATR_ASSE(
     MODELE=MO,
     CHARGE=IMPE,
     CHAM_MATER=CHM,
     NUME_DDL=CO("NDDL"),MATR_ASSE=(
     _F(  MATRICE = CO("MATK"), OPTION = 'RIGI_ACOU'),
               _F(  MATRICE = CO("MATM"), OPTION = 'MASS_ACOU'),
               _F(  MATRICE = CO("MATA"), OPTION = 'AMOR_ACOU'))
                )

VECEL=CALC_VECT_ELEM(
         OPTION='CHAR_ACOU',
         CHAM_MATER=CHM,
         CHARGE=VELO
        )

VECAS=ASSE_VECTEUR(
         VECT_ELEM=VECEL,
         NUME_DDL=NDDL
        )

#
#  REPONSE HARMONIQUE
#
HARMO=DYNA_VIBRA(TYPE_CALCUL='HARM',BASE_CALCUL='PHYS',
         MATR_MASS=MATM,
         MATR_RIGI=MATK,
         MATR_AMOR=MATA,
         FREQ=500.0,
          EXCIT=_F( VECT_ASSE = VECAS,
               COEF_MULT = -1.0,
               PHAS_DEG = 90.0,
               PUIS_PULS = 1)
         )

TEST_RESU(RESU=(
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'N1', NOM_CMP = 'PRES', VALE_C = ('RI',-6.2426,0.,)),
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'N33', NOM_CMP = 'PRES', VALE_C = ('RI',-6.2426,0.,)),
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'N2', NOM_CMP = 'PRES', VALE_C = ('RI',6.0237,1.6387,)),
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'N34', NOM_CMP = 'PRES', VALE_C = ('RI',6.0237,1.6387,)))
          )

FIN()
#
