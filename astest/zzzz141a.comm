                            TYPE_K='K8',
                            LISTE_K='VALEUR  ',),
                         _F(PARA='BOOLEEN',
                            LISTE_I=1,),),)

TEST_TABLE(TABLE=tab1,
           FILTRE=_F(NOM_PARA='TEST',
                     VALE_K='VALEUR  ',),
           NOM_PARA='BOOLEEN',
           VALE_I=1,
           PRECISION=1.e-3,
           CRITERE='ABSOLU',
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.10',)

