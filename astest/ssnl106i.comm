DEBUT(CODE=_F(NOM='SSNL106I', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(  )

MO=AFFE_MODELE(        MAILLAGE=MA,
     AFFE=_F( TOUT = 'OUI', PHENOMENE = 'MECANIQUE',MODELISATION = 'POU_D_TG')
                        )

CAREL=AFFE_CARA_ELEM(   MODELE=MO,POUTRE=(
          # SECTION CERCLE :
           _F(  GROUP_MA = 'GC1',   SECTION = 'CERCLE',
          CARA = (   'R',   ), VALE = (  0.1,)),
          # SECTION RECTANGLE  :
           _F(  GROUP_MA = 'GR1',   SECTION = 'RECTANGLE',
          CARA = (   'HY', 'HZ',  ), VALE = (  0.2,  0.1, )))
)

# ------ TRACTION SIMPLE AVEC ECROUISSAGE ------------------------------
# AFFECTE SUR LA SECTION CERCLE

MATC2=DEFI_MATERIAU(   ELAS=_F( E = 2.E11, NU = 0.3,
                              RHO=7850.0),
           ECRO_LINE=_F( D_SIGM_EPSI = 2.E9, SY = 150.E6),
       VMIS_POUTRE=_F( NP = 4.7124E6,
                MEY = 1.1781E5, MPY = 2.E5, CAY = 0.84, CBY = 0.002,
                MEZ = 1.1781E5, MPZ = 2.E5, CAZ = 0.84, CBZ = 0.0002,
                MPX = 1.E10)
                        )

# AFFECTE SUR LA SECTION RECTANGLE

MATR3=DEFI_MATERIAU(   ELAS=_F( E = 2.E11, NU = 0.3,
                              RHO=7850.0),
           ECRO_FLEJOU=_F( EP = 2.E10, SY = 150.E6, SU = 160.E6, PUISS = 1.0),
       VMIS_POUTRE=_F( NP = 3.0E6,
