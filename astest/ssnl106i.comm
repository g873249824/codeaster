                 NOEUD = 'C3',
                 MAILLE = 'SC2',
                 NOM_CMP = 'V6',
                 VALE = 3.00000E+06, REFERENCE = 'NON_REGRESSION',
                 PRECISION = 1.E-3),
           _F( RESULTAT = REST,
                 NUME_ORDRE = 21,
                 NOM_CHAM = 'VARI_ELNO',
                 NOEUD = 'C3',
                 MAILLE = 'SC2',
                 NOM_CMP = 'V7',
                 VALE = 1.02000E+00, REFERENCE = 'NON_REGRESSION',
                 PRECISION = 1.E-3))
          )


