DEBUT(CODE=_F(NOM='SSNP504G',NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'));

typma=1

if (typma==1):
  MAILLAG2=LIRE_MAILLAGE(INFO=1,FORMAT='MED');

if (typma==2):
  MAILLAG1=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
  MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='SURF'));

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,2.0625),
                                       VECT_NORMALE = (-0.25, 1.0),
                                       PRECISION = 0.0625)))

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('SURF'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN')));   

LN1=FORMULE(NOM_PARA=('X','Y'),VALE='Y-0.25*X-2.0625'); 
                    
LN2=FORMULE(NOM_PARA=('X','Y'),VALE='-Y+0.25*X+6.0625');

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1));

FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN2));
                    
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='P1P1',
                         INFO=1);
                         
CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      REAC_GEOM      = 'AUTOMATIQUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 10,
                      ITER_GEOM_MAXI = 10,
                      FROTTEMENT     = 'COULOMB',
                      ZONE = (_F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.0,   
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',
                                 ALGO_CONT     = 'PENALISATION',
                                 ALGO_FROT     = 'PENALISATION',
                                 COEF_PENA_CONT= 1.E14,
                                 COEF_PENA_FROT= 1.E9,
                                 FISS_MAIT     = FISS1,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                             _F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.0,   
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',   
                                 ALGO_CONT     = 'PENALISATION',
                                 ALGO_FROT     = 'PENALISATION',
                                 COEF_PENA_CONT= 1.E14,
                                 COEF_PENA_FROT= 1.E9,
                                 FISS_MAIT     = FISS2,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                            )
                    );  
                                        
E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                               MATER=ACIER));

#DEPX=2.016
DEPX=2.0
DEPY=1E-2
ZERO=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIGINF',
                                DY=DEPY),
                             _F(GROUP_MA='LIGSUP',
                                DY=-DEPY),
                             _F(GROUP_NO=('P1','P2','P3','P4'),
                                DX=0.0)));
                    
CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LICOTE',
                                DX=DEPX)),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                   LIAISON_DDL=(_F(NOEUD=('N131','N131'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,1),
                                   COEF_IMPO=DEPX),
                                _F(NOEUD=('N1364','N1364'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,1),
                                   COEF_IMPO=DEPX)),
                                 );  

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT');

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT');               

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      CONTACT_XFEM=CTXFEM,
                      LIAISON_XFEM='OUI',
                      INFO=1);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT, 
                   CONTACT = CTXFEM,
                   EXCIT=(
                          _F(CHARGE=CHXFEM),
                          _F(CHARGE=CH1,FONC_MULT=VAR1),
                          _F(CHARGE=CH2,FONC_MULT=VAR2)),  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='SURF'),
                   INCREMENT=_F(LIST_INST=L_INST),
                   CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                   RESI_GLOB_RELA=1E-06)),
                   NEWTON=_F(REAC_ITER=1),
                   SOLVEUR=_F(METHODE='MUMPS',
   
                              NPREC=-1),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                   INFO=1);

LAG = [None]*5
inst = [0.0,0.25,0.5,0.75,1.0]
vale = [[0.0,-6.0789654242659E+04,-5.9710310763853E+04,-5.1197045305081E+04,0.0],
        [0.0,-3.9911326085987E+05,-1.2010507924936E+05,-1.0176447185762E+05,0.0],
        [0.0,0.0,-1.9289293574495E+05,-1.5195304167722E+05,0.0],
        [0.0,0.0,-9.4470170833590E+05,-2.0680206864516E+05,0.0]]
if (typma==1):
  noeud = ['N65','N412','N710','N1008','N1306']

if (typma==2):
  noeud = ['NS412','NS469','NS530','NS591','NS652']

VALE_CALC=[
 [0.0,
 -63336.646860063,
 -60869.85554499,
 -51253.30937733,
 0.0,],
 [0.0,
 -4.2257813472247E+05,
 -1.2700635871608E+05,
 -1.0566405357467E+05,
 0.0,],
 [0.0,
 0.0,
 -2.0902755433001E+05,
 -1.6369904318189E+05,
 0.0,],
 [0.0,
 0.0,
 -9.2359944409479E+05,
 -2.3009800509598E+05,
 0.0,],
]

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO=('NFISSU'),
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1'),
                                         OPERATION='EXTRACTION'));
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              NOM_PARA=('NOEUD','INST','COOR_X','LAGS_C','LAGS_F1'),
              IMPR_FONCTION='NON',
              FILTRE=_F(NOM_PARA='LAGS_F1',
                        CRIT_COMP='NE',
                        VALE=0.0,
                        PRECISION=1.E-1,
                        CRITERE='ABSOLU'),
              TRI=_F(NOM_PARA=('COOR_X'),
                     ORDRE='CROISSANT'));
   
   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-2
        critere = 'ABSOLU'
      else:
        precision = 6.E-2 
        critere = 'RELATIF'
      if (i==3): precision = 10.E-2
      if ((i==4) and (j==3)): precision = 12.E-2
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE_CALC=VALE_CALC[i-1][j], VALE_REFE=vale[i-1][j], TOLE_MACHINE=5.E-06,
                 CRITERE=critere,
                 PRECISION=precision, 
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=(_F(NOM_PARA='NOEUD',
                           CRIT_COMP='EQ',
                           VALE_K=noeud[j])));


FIN();           
