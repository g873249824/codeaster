DEBUT(CODE=_F(NOM='SSLS120A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED',);

MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='AXIS',),);

MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_2D=_F(GROUP_MA='LIGNEGAU',),
                   );

E  = 210000000000.0
Nu = 0.3


ACIER1=DEFI_MATERIAU( ELAS=_F( E  = E,
                               NU = Nu, ),);

EL=2.1E11
ET=4.E10
EN=2.1E11
GLT=0.45E10
GTN=0.45E10
GLN=0.35E10
NULT=0.075
NULN=0.075
NUTN=0.0142857143
ACIER2=DEFI_MATERIAU(   ELAS_ORTH=_F(  E_L   = EL,
                                       E_T   = ET,
                                       E_N   = EN,
                                       G_LT  = GLT,
                                       G_TN  = GTN,
                                       G_LN  = GLN,
                                       NU_LT = NULT,
                                       NU_LN = NULN,
                                       NU_TN = NUTN,
                                       )
                      )

CHMAT1=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(GROUP_MA='VIROLE',
                            MATER=ACIER1,),);
CHMAT2=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(GROUP_MA='VIROLE',
                            MATER=ACIER2,),);

CARAELE=AFFE_CARA_ELEM(MODELE=MODMECA,
                       MASSIF=_F(GROUP_MA='VIROLE',
                                ANGL_REP=(0.0),),);

BLOCAGE=AFFE_CHAR_MECA(MODELE=MODMECA,
                       DDL_IMPO=(_F(GROUP_NO='PM',
                                    DY=0.0,)
                                  ),)

PHYDR=DEFI_FONCTION(NOM_PARA='Y',
                    NOM_RESU='TOUTRESU',
                    VALE=(0.0,15000.0,16.0,0.0),);

PH=AFFE_CHAR_MECA_F(MODELE=MODMECA,
                    FORCE_CONTOUR=_F(GROUP_MA='LIGNEGAU',
                                   FX=PHYDR,),);

# CAS ISOTROPE : SOLUTION ANALYTIQUE EPAISSEUR 0.04


ELAS1=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT1,
                    EXCIT=(_F(CHARGE=BLOCAGE,),
                           _F(CHARGE=PH,),),
                    );

CALC_CHAMP(reuse=ELAS1,RESULTAT=ELAS1,CONTRAINTE=('SIGM_ELNO'))


R = 5.7;

L = 16.0;

h = 0.04;

P = 15000.0;

DZMAX = (-Nu*P*R*L)/E/h/2.
DXMAX = P*R*R/E/h
SIG   = P*R/h


TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A3',
                  VALE=DZMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A4',
                  VALE=DZMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='PM',
                  VALE=DXMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIGM_ELNO',
                  NOM_CMP='SIZZ',
                  GROUP_NO='PM',MAILLE='M2',
                  VALE=SIG,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);


# CAS ORTHOTROPE : SOLUTION ANALYTIQUE

Nuthetaz=NUTN*EN/ET
Etheta=EN
Er=EL
DZMAX2 = (-(Nuthetaz)*P*R*L)/Etheta/h/2.
DXMAX2  = P*R**2/(h*Er)

ELAS2=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT2,
                    CARA_ELEM=CARAELE,
                    EXCIT=(_F(CHARGE=BLOCAGE,),
                           _F(CHARGE=PH,),),);

CALC_CHAMP(reuse=ELAS2,RESULTAT=ELAS2,CONTRAINTE=('SIGM_ELNO'))


TEST_RESU(RESU=_F(RESULTAT=ELAS2,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A3',
                  VALE=DZMAX2,PRECISION=0.02,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS2,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A4',
                  VALE=DZMAX2,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS2,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='PM',
                  VALE=DXMAX2,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS2,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIGM_ELNO',
                  NOM_CMP='SIZZ',
                  GROUP_NO='PM',MAILLE='M2',
                  VALE=SIG,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);


LIST=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=_F(JUSQU_A=1.0,
                                  NOMBRE=1,),);

ELAS3=STAT_NON_LINE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT2,
                    CARA_ELEM=CARAELE,
                    COMP_INCR=_F(RELATION='ELAS'),
                    INCREMENT=_F(LIST_INST=LIST,),
                    EXCIT=(_F(CHARGE=BLOCAGE,),
                           _F(CHARGE=PH,),),);

CALC_CHAMP(reuse=ELAS3,RESULTAT=ELAS3,CONTRAINTE=('SIGM_ELNO'))



TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A3',
                  VALE=DZMAX2,PRECISION=0.02,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A4',
                  VALE=DZMAX2,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='PM',
                  VALE=DXMAX2,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIGM_ELNO',
                  NOM_CMP='SIZZ',
                  GROUP_NO='PM',MAILLE='M2',
                  VALE=SIG,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

FIN();
