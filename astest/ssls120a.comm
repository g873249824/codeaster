DEBUT(CODE=_F(NOM='SSLS120A',NIV_PUB_WEB='INTERNET'),);

PRE_GIBI();

MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),);

MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='AXIS',),);

MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_2D=_F(GROUP_MA='LIGNEGAU',),
                   );

E  = 210000000000.0
Nu = 0.3


ACIER1=DEFI_MATERIAU( ELAS=_F( E  = E,
                               NU = Nu, ),);

NuLT = 0.0142857143
ET   = 2.1E11
EL   = 4.E10
NuTL = NuLT*ET/EL

ACIER2=DEFI_MATERIAU(   ELAS_ORTH=_F(  E_L   = EL,
                                       E_T   = ET,
                                       E_N   = 2.1E11,
                                       G_LT  = 0.45E10,
                                       G_TN  = 0.35E10,
                                       NU_LT = NuLT,
                                       NU_LN = 0.075,
                                       NU_TN = 0.075,
                                       )
                      )

CHMAT1=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(GROUP_MA='VIROLE',
                            MATER=ACIER1,),);
CHMAT2=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(GROUP_MA='VIROLE',
                            MATER=ACIER2,),);

CARAELE=AFFE_CARA_ELEM(MODELE=MODMECA,
                       MASSIF=_F(GROUP_MA='VIROLE',
                                ANGL_REP=(0.0,90.0),),);

BLOCAGE=AFFE_CHAR_MECA(MODELE=MODMECA,
                       DDL_IMPO=(_F(GROUP_NO='PM',
                                    DY=0.0,)
                                  ),)

PHYDR=DEFI_FONCTION(NOM_PARA='Y',
                    NOM_RESU='TOUTRESU',
                    VALE=(0.0,15000.0,16.0,0.0),);

PH=AFFE_CHAR_MECA_F(MODELE=MODMECA,
                    FORCE_CONTOUR=_F(GROUP_MA='LIGNEGAU',
                                   FX=PHYDR,),);

# CAS ISOTROPE : SOLUTION ANALYTIQUE EPAISSEUR 0.04


ELAS1=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT1,
                    EXCIT=(_F(CHARGE=BLOCAGE,),
                           _F(CHARGE=PH,),),
                    );

CALC_ELEM( reuse= ELAS1 ,RESULTAT= ELAS1 , 
                    
                    OPTION='SIGM_ELNO_DEPL',);

R = 5.7;

L = 16.0;

h = 0.04;

P = 15000.0;

DZMAX = (-Nu*P*R*L)/E/h/2.
DXMAX = P*R*R/E/h
SIG   = P*R/h


TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A3',
                  VALE=DZMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A4',
                  VALE=DZMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='PM',
                  VALE=DXMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIGM_ELNO_DEPL',
                  NOM_CMP='SIZZ',
                  GROUP_NO='PM',MAILLE='M2',
                  VALE=SIG,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);


# CAS ORTHOTROPE : SOLUTION ANALYTIQUE


DZMAX2 = (-(NuLT*ET/EL)*P*R*L)/ET/h/2.


ELAS2=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT2,
                    CARA_ELEM=CARAELE,
                    EXCIT=(_F(CHARGE=BLOCAGE,),
                           _F(CHARGE=PH,),),);

CALC_ELEM( reuse= ELAS2 ,RESULTAT= ELAS2 ,                     
                    NUME_COUCHE=1,OPTION='SIGM_ELNO_DEPL',);


TEST_RESU(RESU=_F(RESULTAT=ELAS2,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A3',
                  VALE=DZMAX2,PRECISION=0.02,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS2,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A4',
                  VALE=DZMAX2,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS2,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='PM',
                  VALE=DXMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS1,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIGM_ELNO_DEPL',
                  NOM_CMP='SIZZ',
                  GROUP_NO='PM',MAILLE='M2',
                  VALE=SIG,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);


LIST=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=_F(JUSQU_A=1.0,
                                  NOMBRE=1,),);

ELAS3=STAT_NON_LINE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT2,
                    CARA_ELEM=CARAELE,
                    COMP_INCR=_F(RELATION='ELAS'),
                    INCREMENT=_F(LIST_INST=LIST,),
                    EXCIT=(_F(CHARGE=BLOCAGE,),
                           _F(CHARGE=PH,),),);

CALC_ELEM( reuse= ELAS3 ,RESULTAT= ELAS3 ,                     
                    NUME_COUCHE=1,OPTION='SIEF_ELNO_ELGA',);


TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A3',
                  VALE=DZMAX2,PRECISION=0.02,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A4',
                  VALE=DZMAX2,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='PM',
                  VALE=DXMAX,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=ELAS3,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIEF_ELNO_ELGA',
                  NOM_CMP='SIZZ',
                  GROUP_NO='PM',MAILLE='M2',
                  VALE=SIG,PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

FIN();
