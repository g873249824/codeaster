DEBUT(  CODE=_F( NOM = 'TPLP302A',NIV_PUB_WEB='INTERNET'))

#
# LECTURE DU MAILLAGE ----------------------------------------------
#

MAILL=LIRE_MAILLAGE(  )

MAILL=DEFI_GROUP( reuse=MAILL,   MAILLAGE=MAILL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ------------------------
#

TIMP_100=DEFI_CONSTANTE( VALE=100.0)

TIMP_0=DEFI_CONSTANTE(   VALE=0.0)

MATE=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0, LAMBDA = 1.))

#
# AFFECTATIONS: MATERIAU  MODELE  CHARGEMENT ----------------------
#

MODEL=AFFE_MODELE( 
                  MAILLAGE=MAILL,
                     AFFE=_F( TOUT = 'OUI',  PHENOMENE = 'THERMIQUE',
                                       MODELISATION = 'COQUE'))

CHMATE=AFFE_MATERIAU(   MAILLAGE=MAILL,
                    AFFE=_F( TOUT = 'OUI',  MATER = MATE))

CHARGE=AFFE_CHAR_THER_F(
                      MODELE=MODEL,TEMP_IMPO=(
                  _F( GROUP_NO = 'NOE_IMP',  TEMP_SUP = TIMP_100,
                                 TEMP = TIMP_100, TEMP_INF = TIMP_100),
                  _F( GROUP_NO = 'NOE_GD',   TEMP_SUP = TIMP_0,
                                 TEMP = TIMP_0,   TEMP_INF = TIMP_0))

                  )

EPAIS=AFFE_CARA_ELEM( MODELE=MODEL,
                             COQUE=_F( GROUP_MA = 'TOUT',
                                       EPAIS = 0.005) )

#
# RESOLUTION ------------------------------------------------------
#


TEMP=THER_LINEAIRE( 
                      MODELE=MODEL,
                  CHAM_MATER=CHMATE,    CARA_ELEM=EPAIS,
                      EXCIT=_F(  CHARGE = CHARGE) )

#
# ANALYSE DES RESULTATS -------------------------------------------
#
TPR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMP, NUME_ORDRE=0)

#

TEST_RESU(CHAM_NO=(
      _F( CHAM_GD = TPR,  NOEUD = 'N21', NOM_CMP = 'TEMP_SUP',
