EPREV = EREV / 1.0E+3
EPMDB = EMDB / 1.0E+3

REXT = RINT + EPREV + EPMDB

COEFFOND = ( -1.0E+0*RINT*RINT ) / ( REXT*REXT-RINT*RINT )

