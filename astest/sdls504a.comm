#---------------------------------------------------------------------
#  - CALCUL DE STABILITE D'UNE POUTRE CONSOLE SOUMISE A UN EFFORT
#    DE FLEXION A UNE EXTREMITE. CALCUL DES CHARGES CRITIQUES
#    CONDUISANT AU FLAMBEMENT ELASTIQUE PAR DEVERSEMENT D'UNE POUTRE
#  - MODELISATION AVEC DES ELEMENTS 'COQUE_3D'(TRIA7)
#  + EN ANNEXE, TEST DU COUPLAGE INFO_MODE+CALC_MODES SUR UN CAS
#    DE FLAMBEMENT
#---------------------------------------------------------------------
#
DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET'))
MA=LIRE_MAILLAGE(FORMAT='MED', )

#
MAIL=CREA_MAILLAGE(     MAILLAGE=MA,
                       MODI_MAILLE=_F( TOUT = 'OUI',
                                    OPTION = 'TRIA6_7',
                                    )
                     )

#
MOEL=AFFE_MODELE(   MAILLAGE=MAIL,
                          AFFE=_F( TOUT = 'OUI',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'COQUE_3D',
                                )
                    )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAEL=AFFE_CARA_ELEM(   MODELE=MOEL,COQUE=(
                          _F( GROUP_MA = 'SEMELLES',
                                 EPAIS = 0.0113,
                                 A_CIS = 5650.,
                                 ),
                          _F( GROUP_MA = 'AME',
                                 EPAIS = 0.0075,
                                 ANGL_REP = (90., 0.,),
                                 A_CIS = 3750.,
                                 ))
                       )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MATERIAU=DEFI_MATERIAU(   ELAS=_F( E = 2.E+11,
                                  NU = 0.3,
                                  )
                          )

#
CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                                AFFE=_F( TOUT = 'OUI',   MATER = MATERIAU,  )
                          )

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------
#
CON_LI=AFFE_CHAR_MECA(        MODELE=MOEL,
                              DDL_IMPO=_F( GROUP_NO = 'ENCASTRE',
                                        DX = 0.,    DY = 0.,    DZ = 0.,
                                        DRX = 0.,   DRY = 0.,   DRZ = 0.,
                                        )
                         )

#
CHARGE=AFFE_CHAR_MECA(         MODELE=MOEL,
                           FORCE_NODALE=_F( GROUP_NO = 'P',
                                         FY = -1.,
                                         )
                         )

#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------
#
RES=MECA_STATIQUE(      MODELE=MOEL,
                      CHAM_MATER=CHAM_MAT,
                       CARA_ELEM=CAEL,EXCIT=(
                           _F( CHARGE = CON_LI,  ),
                           _F( CHARGE = CHARGE,  )),
                    )

#
SIGMA=CREA_CHAMP(   RESULTAT=RES,
                      NOM_CHAM='SIEF_ELGA',
                     OPERATION='EXTR',
                     TYPE_CHAM='ELGA_SIEF_R',
                     TYPE_MAXI='MINI',
                     TYPE_RESU='VALE'
                   )

#
MEL_RI_G=CALC_MATR_ELEM(     OPTION='RIGI_GEOM',
                               MODELE=MOEL,
                            CARA_ELEM=CAEL,
                            SIEF_ELGA=SIGMA
                          )

#
MEL_RI_M=CALC_MATR_ELEM(      OPTION='RIGI_MECA',
                                MODELE=MOEL,
                            CHAM_MATER=CHAM_MAT,
                             CARA_ELEM=CAEL,
                                CHARGE=(CHARGE, CON_LI,)
                          )

#
NUM=NUME_DDL(   MATR_RIGI=MEL_RI_M)

#
MAS_RI_M=ASSE_MATRICE(   MATR_ELEM=MEL_RI_M,   NUME_DDL=NUM)

#
MAS_RI_G=ASSE_MATRICE(   MATR_ELEM=MEL_RI_G,   NUME_DDL=NUM)

#
# PRE-ESTIMATIONS DU SPECTRE
# VALEURS ATTENDUES: 1, 3, 0 ET 5
NBMOD1 = INFO_MODE(MATR_RIGI=MAS_RI_M,MATR_RIGI_GEOM=MAS_RI_G,
             TYPE_MODE='MODE_FLAMB',COMPTAGE=_F(METHODE='STURM'),
             SOLVEUR=_F(METHODE='MUMPS'),
             CHAR_CRIT=(-1.E+6,-5.E+5,0.0,1.E+5,1.1E+6,),)

IMPR_TABLE(TABLE=NBMOD1,UNITE=6)
TEST_TABLE(TABLE=NBMOD1,REFERENCE='ANALYTIQUE',
           NOM_PARA='NB_MODE', TYPE_TEST='SOMM',
           VALE_CALC_I=9, VALE_REFE_I=9)
TEST_TABLE(TABLE=NBMOD1,REFERENCE='ANALYTIQUE',
           NOM_PARA='NB_MODE', TYPE_TEST='MIN',
           VALE_CALC_I=0, VALE_REFE_I=0)
TEST_TABLE(TABLE=NBMOD1,REFERENCE='ANALYTIQUE',
           NOM_PARA='NB_MODE', TYPE_TEST='MAX',
           VALE_CALC_I=5, VALE_REFE_I=5)

#
# REUTILISATION DU CALCUL DU NBRE DE CHARGES CRITIQUES D'INFO_MODE
# COMPARAISON AVEC LE CALCUL COMPLET STANDARD (OPTION='BANDE')
RESULT0=CALC_MODES(MATR_RIGI=MAS_RI_M,
                   OPTION='BANDE',
                   MATR_RIGI_GEOM=MAS_RI_G,
                   CALC_CHAR_CRIT=_F(CHAR_CRIT=(-1.E+06,1.E+05),
                                     TABLE_CHAR_CRIT=NBMOD1,
                                     ),
                   TYPE_RESU='MODE_FLAMB',
                   )

RESULT1=CALC_MODES(MATR_RIGI=MAS_RI_M,
                   OPTION='BANDE',
                   MATR_RIGI_GEOM=MAS_RI_G,
                   CALC_CHAR_CRIT=_F(CHAR_CRIT=(-1.E+06,1.+05),
                                     ),
                   VERI_MODE=_F(STOP_ERREUR='NON'), # Reponse provisoire suite Issue25191
                   AMELIORATION='OUI',
                   TYPE_RESU='MODE_FLAMB',
                   )
#
# CALCUL OPTION 'PLUS_PETITE'
RESULT=CALC_MODES(MATR_RIGI=MAS_RI_M,
                  OPTION='PLUS_PETITE',
                  MATR_RIGI_GEOM=MAS_RI_G,
                  TYPE_RESU='MODE_FLAMB',
                  )

RESULT_G=CALC_MODES(TYPE_RESU='GENERAL',
                    MATR_B=MAS_RI_G,
                    MATR_A=MAS_RI_M,
                    OPTION='PLUS_PETITE',
                    )

#
#---------------------------------------------------------------------
#                        VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(_F(PARA='CHAR_CRIT',
                   NUME_MODE=6,
                   RESULTAT=RESULT,
                   VALE_CALC= 1.07753483E+05,
                   VALE_REFE=1.0479782000000001E5,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                _F(PARA='CHAR_CRIT',
                   NUME_MODE=5,
                   RESULTAT=RESULT,
                   VALE_CALC=-1.07878765E+05,
                   VALE_REFE=-1.0479782000000001E5,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                ),
          )

TEST_RESU(RESU=(_F(RESULTAT = RESULT_G,
                   NUME_MODE = 6,
                   PARA = 'CHAR_CRIT',
                   VALE_CALC = 1.0775348330981E+05,
                   VALE_REFE = 104797.82,
                   REFERENCE='NON_DEFINI',
                   PRECISION = 0.03,
                   CRITERE = 'RELATIF',),
                _F(RESULTAT = RESULT_G,
                   NUME_MODE = 5,
                   PARA = 'CHAR_CRIT',
                   VALE_CALC = -1.0787876516812E+05,
                   VALE_REFE = -104797.82,
                   REFERENCE='NON_DEFINI',
                   PRECISION = 0.03,
                   CRITERE = 'RELATIF',),
                  ),
           )

TEST_RESU(RESU=(_F(RESULTAT = RESULT0,
                   NUME_MODE = 1,
                   PARA = 'CHAR_CRIT',
                   VALE_CALC = -7.8662373445668E+05,
                   VALE_REFE = -786624.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION = 0.03,
                   CRITERE = 'RELATIF',),
                _F(RESULTAT = RESULT0,
                   NUME_MODE = 4,
                   PARA = 'CHAR_CRIT',
                   VALE_CALC = -1.0787876516812E+05,
                   VALE_REFE = -107879.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION = 0.03,
                   CRITERE = 'RELATIF',),
                  ),
           )
TEST_RESU(RESU=(_F(RESULTAT = RESULT1,
                   NUME_MODE = 1,
                   PARA = 'CHAR_CRIT',
                   VALE_CALC = -7.8662373423126E+05,
                   VALE_REFE = -786624.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION = 0.03,
                   CRITERE = 'RELATIF',),
                _F(RESULTAT = RESULT1,
                   NUME_MODE = 4,
                   PARA = 'CHAR_CRIT',
                   VALE_CALC = -1.0787876516812E+05,
                   VALE_REFE = -107879.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION = 0.03,
                   CRITERE = 'RELATIF',),
                  ),
           )
#
FIN()
#
#
