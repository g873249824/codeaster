                     EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=1,
                                   NIVE_COUCHE='MOY',),);
R = 5.7;
L = 16.0;
h = 0.04;
P = 15000.0;
DZMAX = (-Nu*P*R*L)/E/h/2.
DXMAX = P*R*R/E/h
DXMIN = -P*R*R/E/h
SIG   = P*R/h
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='A3',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=ELAS1,
                  NOM_CHAM='DEPL',
