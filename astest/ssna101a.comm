DEBUT(   CODE=_F(  NOM = 'SSNA101A',NIV_PUB_WEB='INTERNET'))

MA=LIRE_MAILLAGE(  )

MA=DEFI_GROUP( reuse=MA,          MAILLAGE=MA,CREA_GROUP_NO=(
                 _F(  GROUP_MA = 'MAIL1'),
                 _F(  GROUP_MA = 'MAIL3'),
                 _F(  GROUP_MA = 'GF'),
                 _F(  GROUP_MA = 'CB'))
                   )

MA=DEFI_GROUP( reuse=MA,          MAILLAGE=MA,CREA_GROUP_NO=(
                 _F(  
                 NOM = 'GF_INT',
                 DIFFE = ('GF', 'MAIL1',)), _F( 
                 NOM = 'CB_INT',
                 DIFFE = ('CB', 'MAIL1',)))
                )

MO_MECA=AFFE_MODELE(    MAILLAGE=MA,
                               AFFE=_F(  TOUT = 'OUI',
                                      PHENOMENE = 'MECANIQUE',
                                      MODELISATION = 'AXIS_GRAD_VARI')
                        )

ACIER=DEFI_MATERIAU(         ELAS=_F(  E = 205800.,
                                        NU = 0.3,
                                        ALPHA = 0.0),
                            RUPT_FRAG=_F(  GC = 1.),
                            NON_LOCAL=_F(  LONG_CARA = 0.1,
                                        COEF_RIGI_MINI = 0.0001)
                        )

CM=AFFE_MATERIAU(     MAILLAGE=MA,
                             AFFE=_F(  TOUT = 'OUI',
                                    MATER = ACIER)
                     )

CH=AFFE_CHAR_MECA(      MODELE=MO_MECA,DDL_IMPO=(
                 _F(  GROUP_NO = 'MAIL1',  DX = 0.,  DY = 1.),
                 _F(  GROUP_NO = 'MAIL3',  DY = 0.),
                 _F(  GROUP_NO = 'GF_INT', DX = 0.),
                 _F(  GROUP_NO = 'CB_INT', DX = 0.))
                      )

RAMPE=DEFI_FONCTION(    NOM_PARA='INST',
                           VALE=(
                                  0., 0.,
                                  1., 0.002, )
                        )

INSTANTS=DEFI_LIST_REEL( 
                      DEBUT=0.,
                      INTERVALLE=_F(  JUSQU_A = 1.,  NOMBRE = 20)
               )

CALC=STAT_NON_LINE( 
                            MODELE=MO_MECA,
                        CHAM_MATER=CM,
                             EXCIT=_F( 
                CHARGE = CH,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F( 
                RELATION = 'RUPT_FRAG'),
                         INCREMENT=_F( 
                LIST_INST = INSTANTS,
                NUME_INST_FIN = 16),
                            NEWTON=_F( 
                PREDICTION = 'ELASTIQUE',
                MATRICE = 'ELASTIQUE'),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-4,
                ITER_GLOB_MAXI = 50),
                    LAGR_NON_LOCAL=_F( 
                RESI_DUAL_ABSO = 1.E-4,
                RESI_PRIM_ABSO = 1.E-7,
                ITER_PRIM_MAXI = 20,
                ITER_DUAL_MAXI = 50,
                R   = 10.)
              )

TEST_RESU(RESU=(  
          _F(  RESULTAT = CALC,
            NOM_CHAM = 'DEPL',
            NOM_CMP = 'DY',
            NUME_ORDRE = 16,
            NOEUD = 'N280',
            CRITERE = 'RELATIF',
            PRECISION = 1.E-2,
            VALE = 1.490E-03),

          _F(  RESULTAT = CALC,
            NOM_CHAM = 'VARI_NON_LOCAL',
            NOM_CMP = 'VANL',
            NUME_ORDRE = 16,
            NOEUD = 'N280',
            CRITERE = 'RELATIF',
            PRECISION = 5.E-2,
            VALE = 1.612E-02))
          )

FIN()
