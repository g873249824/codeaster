DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='OUI'))

tfin = 10
tfinc = 9
nbpas = tfin
nbarch = nbpas
cmultpilo = 10

PRE_GMSH()

MA_0  =  LIRE_MAILLAGE(FORMAT="ASTER",)
MA = CREA_MAILLAGE(MAILLAGE  = MA_0, LINE_QUAD = _F(TOUT='OUI'))

MA  =  DEFI_GROUP(
  reuse  = MA,
  MAILLAGE = MA,

  CREA_GROUP_MA = (
    _F(NOM = 'HAUT',      GROUP_MA = 'GM1'),
    _F(NOM = 'BAS',       GROUP_MA = 'GM3'),
    _F(NOM = 'DROITE',    GROUP_MA = 'GM4'),
    _F(NOM = 'J_G',       GROUP_MA = 'GM5'),
    _F(NOM = 'J_D',       GROUP_MA = 'GM2'),
    _F(NOM = 'J_H',       GROUP_MA = 'GM7'),
    _F(NOM = 'J_B',       GROUP_MA = 'GM6'),
    _F(NOM = 'MATRICE',   GROUP_MA = 'GM8'),
    _F(NOM = 'JOINT'   , GROUP_MA = 'GM9'),
    ),

   CREA_GROUP_NO = (
    _F(NOM = 'HAUT',      GROUP_MA = 'GM1'),
    _F(NOM = 'J_H'   ,   GROUP_MA = 'GM7'),
    _F(NOM = 'JOINT' ,   GROUP_MA = 'GM9'),
    _F(NOM = 'NO_JHD',   GROUP_MA = 'GM10'),
    _F(NOM = 'NO_JBD',   GROUP_MA = 'GM11'),
    _F(NOM = 'NO_JMD',   GROUP_MA = 'GM12'),
    _F(NOM = 'NO_JHG',   GROUP_MA = 'GM13'),
    _F(NOM = 'NO_JBG',   GROUP_MA = 'GM14'),
    _F(
       NOM = 'GN_J_D',
       GROUP_MA = 'GM2',
       OPTION = 'NOEUD_ORDO',
       GROUP_NO_ORIG = 'NO_JBD',
       GROUP_NO_EXTR = 'NO_JHD'),
    _F(
       NOM = 'GN_J_G',
       GROUP_MA = 'GM5',
       OPTION = 'NOEUD_ORDO',
       GROUP_NO_ORIG = 'NO_JBG',
       GROUP_NO_EXTR = 'NO_JHG'),
    )
  )

MA=MODI_MAILLAGE(reuse =MA,
                   MAILLAGE=MA,
                   ORIE_FISSURE=_F(GROUP_MA='JOINT'),
                   INFO=1
                   )


BETON = DEFI_MATERIAU(
          ELAS      = _F (
                          E  = 100 ,
                          NU = 0.,
                          ),
          RUPT_FRAG = _F(
                          GC = 0.9,
                          SIGM_C = 3,
                          PENA_LAGR=1000,
                         )
     )

CHMAT = AFFE_MATERIAU(
          MAILLAGE = MA,
          AFFE = (
       _F(
          GROUP_MA = 'MATRICE',
          MATER    =  BETON),
       _F(
          GROUP_MA = 'JOINT',
          MATER    = BETON),
        ))

MO = AFFE_MODELE(
  MAILLAGE = MA,
  AFFE = (
    _F(
      GROUP_MA     = 'MATRICE',
      PHENOMENE    = 'MECANIQUE',
      MODELISATION = 'AXIS'
      ),
    _F(
      GROUP_MA     = 'JOINT',
      PHENOMENE    = 'MECANIQUE',
      MODELISATION = 'AXIS_INTERFACE_S'
      ),
    )
  )

# AFFECTATION DES ANGLES NAUTIQUES : POSITION DE LA FISSURE DANS LE REPERE GLOBAL

ORIEN_FI=AFFE_CARA_ELEM(MODELE=MO,
                     MASSIF=(
                             _F(GROUP_MA='JOINT',ANGL_REP=(180, 0 ,0)),
                                 ));


FIXE=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(GROUP_MA='DROITE', DX=0., DY=0. ),
    _F(GROUP_MA='HAUT'  , DX=0.       ),
    _F(GROUP_MA='BAS'   , DX=0.       ),
    )
  )

TRACTION=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(GROUP_MA='J_G',  DX=0. ,DY=1. ),
    )
  )


INSTANTS = DEFI_LIST_REEL(
             DEBUT = -1.,
             INTERVALLE = (
                 _F(JUSQU_A =  0,       NOMBRE =  1),
                 _F(JUSQU_A =  tfin,    NOMBRE = nbpas),
             ))


INS_ARCH=DEFI_LIST_REEL(
             DEBUT = -1.,
             INTERVALLE = (
                 _F(JUSQU_A =  0,     NOMBRE =  1),
                 _F(JUSQU_A =  tfin,  NOMBRE = nbarch),
             ))



FCT = DEFI_FONCTION(
  NOM_PARA = 'INST',
  VALE     = (-1,0, 0,0.00001)
  )


EVOL = STAT_NON_LINE(
         MODELE = MO,
         CHAM_MATER = CHMAT,
         CARA_ELEM  = ORIEN_FI,
         EXCIT = (
      _F(CHARGE = FIXE),
      _F(CHARGE = TRACTION, FONC_MULT=FCT)),
         COMPORTEMENT = (
      _F(GROUP_MA = 'MATRICE', RELATION = 'ELAS'),
      _F(GROUP_MA = 'JOINT'  , RELATION = 'CZM_TAC_MIX')),
         INCREMENT = _F(
                         LIST_INST = INSTANTS,
                         INST_FIN  = 0.),
       RECH_LINEAIRE = _F (ITER_LINE_MAXI = 2),
       NEWTON  = _F(
                    MATRICE   = 'TANGENTE',
                    REAC_ITER = 1),
       CONVERGENCE = _F(ITER_GLOB_MAXI = 20),
       SOLVEUR     = _F(METHODE = 'MUMPS'),
       ARCHIVAGE   = _F(LIST_INST = INS_ARCH)
    )


EVOL = STAT_NON_LINE(reuse  = EVOL,
  ETAT_INIT = _F(EVOL_NOLI = EVOL),
  MODELE = MO,
  CHAM_MATER = CHMAT,
  CARA_ELEM  = ORIEN_FI,
  EXCIT = (
    _F(CHARGE = FIXE),
    _F(CHARGE = TRACTION, TYPE_CHARGE = 'FIXE_PILO')
    ),
  COMPORTEMENT = (
    _F(GROUP_MA = 'MATRICE', RELATION = 'ELAS'),
    _F(GROUP_MA = 'JOINT'  , RELATION = 'CZM_TAC_MIX'),
    ),
  INCREMENT = _F(
    LIST_INST = INSTANTS,
    INST_FIN  = tfinc,
    ),
  PILOTAGE  = _F (
    GROUP_MA       = 'JOINT',
    TYPE           = 'PRED_ELAS',
    COEF_MULT      = cmultpilo,
    ETA_PILO_R_MIN = 0,
    ETA_PILO_MAX   = 2,
    ETA_PILO_MIN   = 0.00001,
    PROJ_BORNES    = 'NON'
    ),
  RECH_LINEAIRE = _F (
    METHODE = 'PILOTAGE',
    ITER_LINE_MAXI = 0
    ),
  NEWTON  = _F(
    MATRICE   = 'TANGENTE',
    REAC_ITER = 1,
    PAS_MINI_ELAS = 1.E-3,
    ),
  CONVERGENCE = _F(
    RESI_GLOB_MAXI = 1.E-4,
    ITER_GLOB_MAXI = 10,
    ),
  SOLVEUR = _F(METHODE = 'MUMPS'),
  ARCHIVAGE =_F(LIST_INST = INS_ARCH)
  )


EVOL = CALC_CHAMP(reuse = EVOL,
                  RESULTAT = EVOL,
                  FORCE='FORC_NODA',
                  GROUP_MA='JOINT'
  )

# INTERPOLATION NODALE DES CHAMPS AUX PG
EVOL=CALC_CHAMP(reuse =EVOL,
                RESULTAT=EVOL,
                VARI_INTERNE='VARI_ELNO',
                CONTRAINTE='SIEF_ELNO',
                GROUP_MA='JOINT'
               );

EVOL=CALC_CHAMP(
                reuse =EVOL,
                RESULTAT=EVOL,
                VARI_INTERNE='VARI_NOEU',
                CONTRAINTE='SIEF_NOEU',
                GROUP_MA='JOINT'
               );


# DEPLACEMENT PILOTE
UPILO = POST_RELEVE_T(
 ACTION = _F(
   INTITULE     = 'U_PILOTE',
   OPERATION    = 'EXTRACTION',
   GROUP_NO     = 'NO_JHG',
   NOM_CHAM     = 'DEPL',
   NOM_CMP      = 'DY',
   RESULTAT     =  EVOL,
   TOUT_ORDRE   = 'OUI',
    ))

# FORCE RESULTANTE DANS LA DIRECTION Y, SUR UN RADIAN
# A MULITPLIER PAR 2PI

FRES = POST_RELEVE_T(
 ACTION = _F(
   INTITULE     = 'F_RESULTANTE',
   OPERATION    = 'EXTRACTION',
   GROUP_NO     = 'GN_J_G',
   NOM_CHAM     = 'FORC_NODA',
   RESULTANTE   = ('DX','DY'),
   RESULTAT     =  EVOL,
   TOUT_ORDRE   = 'OUI',
    ))



TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=0.157693079,
           VALE_REFE=0.1576931,
           NOM_PARA='DY',
           TABLE=UPILO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=12.549900398,
           VALE_REFE=12.549899999999999,
           NOM_PARA='DY',
           TABLE=FRES,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=0.265647395,
           VALE_REFE=0.26564739999999998,
           NOM_PARA='DY',
           TABLE=UPILO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=6.0,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=9.486832981,
           VALE_REFE=9.4868330000000007,
           NOM_PARA='DY',
           TABLE=FRES,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=6.0,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=0.363577006,
           VALE_REFE=0.36357699999999998,
           NOM_PARA='DY',
           TABLE=UPILO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=8.0,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=6.708203933,
           VALE_REFE=6.7081999999999997,
           NOM_PARA='DY',
           TABLE=FRES,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=8.0,),
           )

# TESTS DE QUANTITES LOCALES AUX PG
TEST_RESU(RESU=_F(INST=tfinc,
                  POINT=2,
                  RESULTAT=EVOL,
                  NOM_CHAM='VARI_ELGA',
                  NOM_CMP='V8',
                  VALE_CALC=0.41026334038946,
                  MAILLE='M2380',),
          )

TEST_RESU(RESU=_F(INST=tfinc,
                  POINT=2,
                  RESULTAT=EVOL,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SITX',
                  VALE_CALC=0.94868329805283,
                  MAILLE='M2380',),
          )

# TESTS DES EXTRAPOLATIONS NODALES
TEST_RESU(RESU=_F(INST=tfinc,
                  RESULTAT=EVOL,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V8',
                  VALE_CALC=0.41026334038946,
                  NOEUD='N2'
                  ),
          )

TEST_RESU(RESU=_F(INST=tfinc,
                  RESULTAT=EVOL,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='SITX',
                  VALE_CALC=0.94868329805283,
                  NOEUD='N2'
                  ),
          )


IMPR_TABLE(TABLE = UPILO)
IMPR_TABLE(TABLE = FRES)

FIN(FORMAT_HDF='OUI')
