                    CHAM_MATER=CHMAT,
                    CHAR_MECA_GLOBAL=CLIM,
                    CAS_CHARGE=(_F(NOM_CAS='CHARGE1',
                                   CHAR_MECA=CHAR_PR,),
                                _F(NOM_CAS='CHARGE2',
                                   CHAR_MECA=(CHAR_PR,CHAR_GR,),),),);
