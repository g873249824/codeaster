                      NOM_CMP    = 'V1',
                      INST       = 1.1600,
                      PRECISION  = 1.E-3,
                      VALE       = 1.98716935289E-03),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'VARI_NOEU_ELGA',
                      NOM_CMP    = 'V1',
                      INST       = 1.3400,
                      PRECISION  = 1.E-3,
                      VALE       = 6.35233522769E-03),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'VARI_NOEU_ELGA',
                      NOM_CMP    = 'V1',
                      INST       = 1.5300,
                      PRECISION  = 1.E-3,
                      VALE       = 1.0886266045E-02),

##########################
