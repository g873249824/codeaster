##########################
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'DEPL',
                      NOM_CMP    = 'DY',
                      INST       = 1.07000,
                      PRECISION  = 1.E-3,
                      VALE       = -1.050E-03),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
