                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'SIEF_NOEU_ELGA',
                      NOM_CMP    = 'SIXX',
                      INST       = 2.0,
                      PRECISION  = 1.E-3,
                      VALE       = -2.0E+6),
###########################
# --- CONTRAINTE SIYY --- #
###########################
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'SIEF_NOEU_ELGA',
                      NOM_CMP    = 'SIYY',
                      INST       = 1.07000,
                      PRECISION  = 1.E-3,
                      VALE       = -8.09000E+06),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'SIEF_NOEU_ELGA',
                      NOM_CMP    = 'SIYY',
                      INST       = 1.1600,
                      PRECISION  = 1.E-3,
                      VALE       = -8.19785989E+06),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'SIEF_NOEU_ELGA',
                      NOM_CMP    = 'SIYY',
                      INST       = 1.3400,
                      PRECISION  = 1.E-3,
                      VALE       = -6.894825305E+06),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'SIEF_NOEU_ELGA',
                      NOM_CMP    = 'SIYY',
                      INST       = 1.5300,
                      PRECISION  = 1.E-3,
                      VALE       = -5.805970149E+06),
###############################
# --- VARIABLE INTERNE V1 --- #
###############################
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'ANALYTIQUE',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'VARI_NOEU_ELGA',
                      NOM_CMP    = 'V1',
                      CRITERE    = 'ABSOLU',
                      INST       = 1.07000,
                      PRECISION  = 1.E-3,
                      VALE       = 0.000000E+0),
                 _F(  RESULTAT   = U2,
