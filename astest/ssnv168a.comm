DEBUT(CODE=_F(NOM='SSNV168A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE = LIRE_MAILLAGE( )

MODELE   = AFFE_MODELE( MAILLAGE =     MAILLAGE,
                        AFFE     = _F( TOUT         = 'OUI',
                                       PHENOMENE    = 'MECANIQUE',
                                       MODELISATION = 'AXIS')
                      )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS1   = DEFI_LIST_REEL( DEBUT      = 0.,
                           INTERVALLE = _F( JUSQU_A = 1.0, NOMBRE = 1 )
                          )

TEMPS2   = DEFI_LIST_REEL( DEBUT      = 1.,
                           INTERVALLE = _F( JUSQU_A = 2.0, NOMBRE = 100 )
                          )

#***********************************************************************
#    MATERIAU : SOL
#***********************************************************************
MATER  = DEFI_MATERIAU(  ELAS    = _F(  E  = 5800.0E6,
       NU = 0.3  ,),
      DRUCK_PRAGER = _F(  ECROUISSAGE = 'LINEAIRE',
       ALPHA       =    0.33   ,
       P_ULTM      =    0.01   ,
       SY       =    2.57E6 ,
       H       =  - 2.00E8 ,),
   );

CHMAT    = AFFE_MATERIAU(  MAILLAGE = MAILLAGE,
                           AFFE     = _F(  TOUT  = 'OUI',
                                           MATER = MATER )
                        );

#***********************************************************************
#    CHARGEMENT MECANIQUE : FORCE DE COMPRESSION + DDL IMPOSES
#***********************************************************************
CHAR_U1  = AFFE_CHAR_MECA( MODELE   = MODELE,
                           PRES_REP = _F( MAILLE = 'M4',
                                          PRES   = 1.    )
                         )

CHAR_U2  = AFFE_CHAR_MECA( MODELE   = MODELE,
                           PRES_REP = _F( MAILLE = 'M3',
                                          PRES   = 1.    )
                        )

DEPL_1   = AFFE_CHAR_MECA( MODELE    = MODELE,
                           FACE_IMPO = _F( MAILLE = 'M2',
                                           DY     = 0.   )
                         )

COEF1    = DEFI_FONCTION( NOM_PARA    = 'INST',
                          PROL_DROITE = 'CONSTANT',
                          VALE=( 0.0,  0.0,
                                 1.0,  2.0E6,)
                        )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************

U1=STAT_NON_LINE(
                  MODELE     = MODELE,
                  CHAM_MATER = CHMAT,
    EXCIT      = ( _F( CHARGE       = CHAR_U1,
                                     FONC_MULT    = COEF1),
     _F( CHARGE       = CHAR_U2,
                                     FONC_MULT    = COEF1),
     _F( CHARGE       = DEPL_1)),
                  COMP_INCR  = _F( RELATION       = 'DRUCKER_PRAGER',
                                   RESI_INTE_RELA = 1.E-6,
                                   ITER_INTE_MAXI = 100),
                  NEWTON     = _F( MATRICE        = 'TANGENTE',
                                   REAC_ITER      = 1,),
                  CONVERGENCE= _F( RESI_GLOB_RELA = 1.E-4,
                                   ITER_GLOB_MAXI = 50,),
                  INCREMENT  = _F( LIST_INST      = TEMPS1)
                )

U1=CALC_ELEM( reuse      =U1,
              OPTION     =( 'SIEF_ELNO_ELGA', 'VARI_ELNO_ELGA',),
              RESULTAT   = U1,
            )

#***********************************************************************
#    NOUVEAU CHARGEMENT MECANIQUE : DEPLACEMENT UNIAXIALE UNITAIRE
#***********************************************************************
SIG_INI = CREA_CHAMP( OPERATION = 'EXTR',
                      TYPE_CHAM = 'ELGA_SIEF_R',
                      NOM_CHAM  = 'SIEF_ELGA',
        RESULTAT  = U1,
                      INST      = 1.,
                    )

VAR_INI = CREA_CHAMP( OPERATION = 'EXTR',
                      TYPE_CHAM = 'ELGA_VARI_R',
                      NOM_CHAM  = 'VARI_ELGA',
        RESULTAT  = U1,
                      INST      = 1.
                    )

DEPL_2  = AFFE_CHAR_MECA( MODELE    = MODELE,
                          FACE_IMPO = _F( MAILLE = 'M4',
                                          DY     = 1.   )
                        )

COEF2   = DEFI_FONCTION( NOM_PARA    = 'INST',
                         PROL_DROITE = 'LINEAIRE',
                         PROL_GAUCHE = 'CONSTANT',
                         VALE=( 1.0,  0.0,
                                2.0, -0.015, )
                       )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************

U2 = STAT_NON_LINE(
                    MODELE     = MODELE,
                    CHAM_MATER = CHMAT,
      EXCIT      = ( _F( CHARGE       = CHAR_U2,
                                       FONC_MULT    = COEF1),
              _F( CHARGE       = DEPL_1),
              _F( CHARGE       = DEPL_2,
                                       FONC_MULT    = COEF2)),
                    COMP_INCR  = _F( RELATION       = 'DRUCKER_PRAGER',
                                     RESI_INTE_RELA = 1.E-6,
                                     ITER_INTE_MAXI = 100,
                                     ITER_INTE_PAS  = -10,),
                    ETAT_INIT  = _F( SIGM           = SIG_INI,
                                     VARI           = VAR_INI),
                    INCREMENT  = _F( LIST_INST      = TEMPS2),
                    NEWTON     = _F( MATRICE        = 'TANGENTE',
                                     REAC_ITER      = 1,),
                    CONVERGENCE= _F( RESI_GLOB_RELA = 1.E-6,
                                     ITER_GLOB_MAXI = 100)
                  )

U2       = CALC_ELEM( reuse      = U2,
                      OPTION     = ('SIEF_ELNO_ELGA','VARI_ELNO_ELGA'),
                      RESULTAT   = U2,
                    );

U2       = CALC_NO( reuse    = U2,
                    RESULTAT = U2,
                    OPTION   = ( 'SIEF_NOEU_ELGA', 'VARI_NOEU_ELGA')
                  )

TEST_RESU( RESU = (
