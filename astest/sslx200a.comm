            _F(  RESULTAT = RESU, NUME_ORDRE=1, NOM_CHAM='DEPL',
              CRITERE = 'RELATIF',   PRECISION = 1.E-7,
              NOEUD = 'N6',   NOM_CMP = 'DZ',    VALE = -6.0875000000000E-05),
   # CONTRAINTES

