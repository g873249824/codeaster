                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N6',
                   NOM_CMP='EPZZ',
                   VALE_CALC=-0.000413747627084,
                   VALE_REFE=-4.1300000000000001E-4,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=6.48000E5,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N6',
                   NOM_CMP='EPZZ',
                   VALE_CALC=-0.000572524821284,
                   VALE_REFE=-5.7300000000000005E-4,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=6.480000E6,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N6',
                   NOM_CMP='EPZZ',
                   VALE_CALC=-0.00126859189621,
                   VALE_REFE=-1.2700000000000001E-3,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=6.4800000E7,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N6',
                   NOM_CMP='EPZZ',
                   VALE_CALC=-0.00344086057586,
                   VALE_REFE=-3.4499999999999999E-3,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                ),
          )





