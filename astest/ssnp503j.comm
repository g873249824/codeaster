# CONTACT-FROTTANT EN GRANDS GLISSEMENTS
# AVEC XFEM POUR DES FISSURES HORIZONTALES
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

MAILLAG2=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
Y=3

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,Y,0.0),
                                       VECT_NORMALE = (0.0,1.0,0.0),
                                       PRECISION = 1E-1)))
MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU2',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,6,0.0),
                                       VECT_NORMALE = (0.0,1.0,0.0),
                                       PRECISION = 1E-1)))

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOLU'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D')));

LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y-2.5');

LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-Y+6.5');

FISS1=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1));

FISS2=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN2));

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='STANDARD',
                         INFO=1);


CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      REAC_GEOM      = 'AUTOMATIQUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 10,
                      ITER_GEOM_MAXI = 10,
                      FROTTEMENT     = 'COULOMB',ELIM_ARETE     = 'ELIM',
                      ZONE = (_F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.1,
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',
                                 FISS_MAIT     = FISS1,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                             _F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.1,
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',
                                 FISS_MAIT     = FISS2,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                            )
                    );

E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER));

DEPX=2.00
DEPY=1.E-3
DEPZ=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SINF',DY=DEPY,DZ=0.0),
                             _F(GROUP_MA='SSUP',DY=-DEPY,DZ=0.0),
                             _F(GROUP_MA=('L1','L2','L3','L4'),
                                DX=0.0),
                             ),
                   INFO=1);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SCOTE',
                               DX=DEPX,DZ=DEPZ)),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                   LIAISON_DDL=(_F(NOEUD=('N13','N13'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPX),
                                _F(NOEUD=('N43','N43'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPX),
                                _F(NOEUD=('N61','N61'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPX),
                                _F(NOEUD=('N76','N76'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPX),
                                _F(NOEUD=('N13','N13'),
                                   DDL=('DZ','H1Z'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPZ),
                                _F(NOEUD=('N43','N43'),
                                   DDL=('DZ','H1Z'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPZ),
                                _F(NOEUD=('N61','N61'),
                                   DDL=('DZ','H1Z'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPZ),
                                _F(NOEUD=('N76','N76'),
                                   DDL=('DZ','H1Z'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPZ)),
                    INFO=1);

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    CONTACT = CTXFEM,
                    EXCIT=(
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2)),
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 GROUP_MA='VOLU'),
                    INCREMENT=_F(LIST_INST=L_INST),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06
                                    )),
                    NEWTON=_F(REAC_ITER=1),
                    SOLVEUR=_F(METHODE='MUMPS',

                               NPREC=-1),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                    INFO=1);

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]

vale = [[0.0,-9.2184408151119E+03,-5.3136224576015E+03,-6.3318943452479E+03,-3.8408175690308E+03],
        [0.0,-1.5003217203069E+04,-8.0031950965622E+03,-1.4765678015293E+04,-6.7122964441696E+03],
        [0.0,0.0,-1.7575097916279E+04,-1.6339667015330E+04,-1.2605825792217E+04],
        [0.0,0.0,-1.7152486971972E+04,-2.1471848627878E+04,-1.7685793830428E+04]]

VALE_CALC=[
[ 0.0E+00,
 -9218.4244601579,
 -5313.624732926,
 -6331.907782329,
 -3840.8376202201,],
[ 0.0E+00,
 -15003.764062973,
 -8003.2551920813,
 -14766.305083706,
 -6712.5373518643,],
[ 0.0E+00,
 0.0E+00,
 -17576.202442892,
 -16341.034471799,
 -12606.803209622,],
[ 0.0E+00,
 0.0E+00,
 -17154.757020909,
 -21473.605336322,
 -17687.238798778,],
]

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO='NFISSU',
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2'),
                                         OPERATION='EXTRACTION'));
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              NOM_PARA=('NOEUD','INST','COOR_X','COOR_Z','LAGS_C','LAGS_F1','LAGS_F2'),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X'),
                     ORDRE='CROISSANT'));

   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-10
        critere = 'ABSOLU'
      else:
        precision = 1.0E-2
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE_CALC=VALE_CALC[i-1][j], VALE_REFE=vale[i-1][j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=(_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU'),
                        _F(NOM_PARA='COOR_Z',
                           CRIT_COMP='EQ',
                           VALE=0.0,
                           PRECISION=0.01,
                           CRITERE='ABSOLU')));

MA_XFEM=POST_MAIL_XFEM(
                       MODELE=MODELEK,
                       PREF_NOEUD_X = 'nx',
                       PREF_NOEUD_M = 'nm',
                       PREF_NOEUD_P = 'np',
                       PREF_MAILLE_X = 'mx');

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                   AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D'))

RES_XFEM=POST_CHAM_XFEM( MODELE_VISU= MOD_VISU,
                  RESULTAT=UTOT1,

                  INFO =1)

#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=RES_XFEM,NOM_CHAM='DEPL',TYPE_CHAM='VECT_3D',NOM_CMP=('DX','DY','DZ')))),

FIN();
