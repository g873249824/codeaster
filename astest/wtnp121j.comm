#    LINEAIRE SOUMIS A UN CHOC DE PRESSION EN 3D
#    MODELISATION VF DECENTRE ARETE (VFDA)


DEBUT(CODE=_F(NOM='WTNP121J', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#=============================================================
#=============================================================
METHODE='VFDA'
#=============================================================
#=============================================================

MODELISA = '3D_HH2SUDA'
NOEUD_G='NH3'
#=============================================================
#                     donnees
#=============================================================

PARM_ALPHA=1.
MOLVP = 1.E-10
UN_SUR_K= 0
EMMAG=1.E-10

#*********************************************************
# CI/CL
#*********************************************************
#
PGREF = 10.

PVP0 = 0.0001
P0=10000.
PG0 = 0.
PCINI = PG0+PGREF - P0
PRE1GAUCHE=0

#*********************************************************
# PAS DE TEMPS
#*********************************************************
#
TFINAL=100.
NBDECOUP=100
L_INST=DEFI_LIST_REEL(VALE=(0.,10.,100.,),);


#*********************************************************
# CRITERES DE CONVERGENCE
#*********************************************************
#

RESI_GLOB_MAXI = 1.E-16
ITER_GLOB_MAXI = 0
#*************************************************************
# Liste des instants de calculs
#*************************************************************
#

INST1=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=TFINAL,
                                    NOMBRE=NBDECOUP,),),
                     );

LI =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =INST1 ,),
                                ECHEC=_F(EVENEMENT     = 'ERREUR',
                                         ACTION        = 'DECOUPE',
                                         SUBD_METHODE  = 'MANUEL',
                                         SUBD_PAS      = 5,
                                         SUBD_PAS_MINI = 0.1,),)
#*********************************************************
# Definition des caracteristiques fluides
#**********************************************************

UN=DEFI_CONSTANTE(VALE=1.0,);
ZERO=DEFI_CONSTANTE(VALE=0.0,);
PERMINT=DEFI_CONSTANTE(VALE=1.E-13,);
HENRY=DEFI_CONSTANTE(VALE=1.0,);

#*********************************************************
# LECTURE MAILLAGE
#*********************************************************
#

MAILINI=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=CREA_MAILLAGE( MAILLAGE=MAILINI,
                      HEXA20_27=_F(TOUT='OUI',PREF_NOEUD='NH'),);
MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(
                _F(GROUP_MA='GAUCHE',),
                   ),);


#*********************************************************
# Modele de calcul
#*********************************************************
#
MODELT=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION=MODELISA,),
                  );

#*********************************************************
# DEFI_MATERIAU
#*********************************************************
#
MATERIAU=DEFI_MATERIAU(

   THM_LIQU=_F(
         RHO = 1.,
         UN_SUR_K = UN_SUR_K,
         ALPHA = 1.E-4,
         VISC = UN,
         D_VISC_TEMP = ZERO,),

   COMP_THM = 'LIQU_AD_GAZ_VAPE',

   THM_AIR_DISS=_F(
                   CP=0.,
                   COEF_HENRY=HENRY,
                   ),
     THM_VAPE_GAZ    =_F(
                   MASS_MOL   = MOLVP,
                   CP         = 1.,
                   VISC       = UN,
                   D_VISC_TEMP= ZERO,
                       ),
   THM_GAZ=_F(
          MASS_MOL    = 1.,
          VISC        = UN,
          D_VISC_TEMP = ZERO,
             ),

   THM_DIFFU=_F(
            R_GAZ      = 8.315,
            EMMAG      = EMMAG,
            RHO        = 1.,
            BIOT_COEF  = 1.,
            SATU_PRES  = UN,
            D_SATU_PRES= ZERO,
            PESA_X     = 0.0,
            PESA_Y     = 0.,
            PESA_Z     = 0.,
            PERMIN_X   = PERMINT,
            PERMIN_Y   = PERMINT,
            PERMIN_Z   = PERMINT,
            PERMINXY   = ZERO,
            PERMINYZ   = ZERO,
            PERMINZX   = ZERO,
            PERM_LIQU  = UN,
            D_PERM_LIQU_SATU=ZERO,
            PERM_GAZ   = UN,
            D_PERM_SATU_GAZ=ZERO,
            D_PERM_PRES_GAZ=ZERO,
            FICKV_T    = ZERO,
            FICKA_T    = ZERO,
            LAMB_T     = ZERO
              ),

   THM_INIT=_F(
         TEMP = 293.,
         PRE1 = 0.,
         PRE2 = PGREF,
         PORO = 0.5,
         PRES_VAPE=PVP0,
              ),
   )
#****************************************************************
# Affectation des materiaux
#****************************************************************
#

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(TOUT='OUI',
                             MATER=MATERIAU,),),);

#*************************************************************
# Affectation de l etat initial
#*************************************************************
#
PINIT=CREA_CHAMP(MAILLAGE=MAIL,
                    OPERATION='AFFE',
                    TYPE_CHAM='NOEU_DEPL_R',
                    AFFE=(
                    _F( TOUT='OUI'  ,
                    NOM_CMP  = ('PRE1','PRE2') ,
                    VALE = (PCINI,0.) , ),
                ),)
#*************************************************************
# Affectation des CL
#*************************************************************
#

CHIMP=AFFE_CHAR_CINE(MODELE=MODELT,
       MECA_IMPO=(_F(TOUT='OUI',PRE2=0.,),
                 _F(GROUP_MA = 'GAUCHE',PRE1=PRE1GAUCHE,),
                 ), );


#
#
#*************************************************************
#     STAT NON LINE
#*************************************************************
#
cal1=STAT_NON_LINE(MODELE=MODELT,
                 CHAM_MATER=CHMAT,

                 EXCIT=( _F(CHARGE=CHIMP,),),
                 COMP_INCR=_F(RELATION='KIT_HH',
                              PARM_THETA=0.111,
                              PARM_ALPHA=PARM_ALPHA,
                             RELATION_KIT=('LIQU_AD_GAZ_VAPE','HYDR_UTIL'),
                 ),

                INCREMENT=_F(
                   LIST_INST = LI,
                     INST_FIN=TFINAL,
                ),
                 ETAT_INIT  = _F ( DEPL = PINIT  ,
                                  ),

                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),

                 CONVERGENCE=_F( RESI_GLOB_MAXI = RESI_GLOB_MAXI,
                                 ITER_GLOB_MAXI = ITER_GLOB_MAXI,),

                 ARCHIVAGE=_F(LIST_INST     = INST1 ),
                 );


#
#
#*************************************************************
#     IMPRESSIONS
#*************************************************************


DDLSMIL= POST_RELEVE_T ( ACTION=_F( INTITULE   = 'DEPL'  ,
                       RESULTAT   =  cal1   ,
                                    LIST_INST  =  L_INST ,
                       GROUP_NO   = 'GAUCHE'  ,
                       NOM_CHAM   = 'DEPL'  ,
                       NOM_CMP    = ( 'PRE1',) ,
                       OPERATION  = 'EXTRACTION',),);

IMPR_TABLE ( TABLE = DDLSMIL,
             TRI   = (_F( NOM_PARA = 'COOR_X'    ,
            ORDRE    = 'CROISSANT' ,),),
             PAGINATION = 'INST' ,
             NOM_PARA=('INST','COOR_X', 'PRE1',),
           );




#============== t=100======================

TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='ANALYTIQUE',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='NH4',
                NOM_CMP='PRE1',
                VALE_CALC=-8.26326568E+03,
                VALE_REFE=-8263.2659999999996,
                CRITERE='RELATIF',
                PRECISION=1.E-2,
                ),
        )


TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='ANALYTIQUE',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='NH195',
                NOM_CMP='PRE1',
                VALE_CALC=-9.99000000E+03,
                VALE_REFE=-9990.0,
                CRITERE='RELATIF',
                PRECISION=1.E-2,
                ),
        )

FIN();
