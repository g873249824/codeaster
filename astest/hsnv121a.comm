DEBUT(CODE=_F(NOM='HSNV121A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(
               MAILLAGE=MA,
                  VERIF='MAILLE',
               AFFE=_F(
               TOUT = 'OUI',
               PHENOMENE = 'MECANIQUE',
               MODELISATION = '3D')
             )

COU_T20=DEFI_FONCTION(
               NOM_PARA='EPSI',
               PROL_DROITE='LINEAIRE',
               VALE=(
               0.010,    1000.,
               1.010,    3500.,
               )
             )

COU_T120=DEFI_FONCTION(
               NOM_PARA='EPSI',
               PROL_DROITE='LINEAIRE',
               VALE=(
               0.005,    1000.,
               1.005,    3000.,
               )
             )

COU_TRAC=DEFI_NAPPE(

              NOM_PARA='TEMP',
                  PARA=(20.,120.,),
              FONCTION=(COU_T20,COU_T120,),
              PROL_GAUCHE='CONSTANT',
               PROL_DROITE='CONSTANT'
             )

ACIER_T=DEFI_MATERIAU(
                   ELAS=_F(
               E = 200000.,
               NU = 0.3,
               ALPHA = 1.E-4),
              TRACTION=_F(  SIGM = COU_TRAC)
             )

L_INST=DEFI_LIST_REEL(
              DEBUT=0.0,INTERVALLE=(

               _F(  JUSQU_A = 1.00, NOMBRE = 1),
               _F(  JUSQU_A = 2.00, NOMBRE = 20))
             )

F_CHAR=DEFI_FONCTION(
                NOM_PARA='INST',
              PROL_DROITE='CONSTANT',
                    VALE=(
               0.,   0.,
               1.,   0.,
               2.,   293.3,
               )
             )

F_TEMP=DEFI_FONCTION(
                NOM_PARA='INST',
                NOM_RESU='TEMP',
              PROL_DROITE='CONSTANT',
                    VALE=(
               0.,    20.,
               1.,   120.,
               2.,   120.,
               )
             )

CHP_TEMP=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_F',
               MAILLAGE=MA,
                   AFFE=_F(
               TOUT = 'OUI',
               NOM_CMP = 'TEMP',
               VALE_F = F_TEMP)
             )

TEMP=CREA_RESU(
               OPERATION='AFFE', TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                 AFFE=_F(
               LIST_INST = L_INST,
               CHAM_GD = CHP_TEMP)
             )

CHP_MATT=AFFE_MATERIAU(
               MAILLAGE=MA,
                   AFFE=_F(
               TOUT = 'OUI',
               MATER = ACIER_T,),
                   AFFE_VARC=_F(
                TOUT='OUI',
                EVOL=TEMP,
                NOM_VARC='TEMP',
                NOM_CHAM='TEMP',
                VALE_REF=20.),)


CHR_LIAI=AFFE_CHAR_MECA(
              MODELE=MO,DDL_IMPO=(

               _F( NOEUD = 'NO2',  DX = 0.,  DY = 0.,  DZ = 0.),
               _F( NOEUD = 'NO6',  DX = 0.,  DY = 0.),
               _F( NOEUD = 'NO1',  DX = 0.,          DZ = 0.),
               _F( NOEUD = ('NO9', 'NO13', 'NO14', 'NO5', 'NO17',),  DX = 0.))
             )

CHR_TRAC=AFFE_CHAR_MECA(
                     MODELE=MO,
                  FACE_IMPO=_F( MAILLE = 'MA2',  DX = 1.)
             )

VT=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHP_MATT,EXCIT=(
                             _F(
                CHARGE = CHR_LIAI),),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_TRAC',
                DEFORMATION = 'SIMO_MIEHE'),
                         INCREMENT=_F(
                LIST_INST = L_INST,
                NUME_INST_FIN = 1),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_MAXI = 1.E-1,
                ITER_GLOB_MAXI = 50),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3)
              )

VT=STAT_NON_LINE( reuse=VT,
                         ETAT_INIT=_F(
                EVOL_NOLI = VT,
                NUME_ORDRE = 1),
                            MODELE=MO,
                        CHAM_MATER=CHP_MATT,EXCIT=(
                             _F(
                CHARGE = CHR_LIAI), _F(
                CHARGE = CHR_TRAC,
                FONC_MULT = F_CHAR,
                TYPE_CHARGE = 'DIDI')),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_TRAC',
                DEFORMATION = 'SIMO_MIEHE'),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                PREDICTION = 'EXTRAPOLE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3)
              )
ENERSM=POST_ELEM(ENER_ELAS=_F(TOUT='OUI'),
                 RESULTAT=VT,
                 INST=2.,
                 );
TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=VT,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='DX',
                   VALE_CALC=303.062840686,
                   VALE_REFE=303.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   RESULTAT=VT,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='DY',
                   VALE_CALC=-109.852131546,
                   VALE_REFE=-110.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   RESULTAT=VT,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='DZ',
                   VALE_CALC=-109.852131546,
                   VALE_REFE=-110.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=VT,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC= 1.45851118E+03,
                   VALE_REFE=1453.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=VT,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=0.250425385,
                   VALE_REFE=0.2475,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.014999999999999999,
                   MAILLE='MA1',),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=VT,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V2',
                   VALE_CALC=1.,
                   VALE_REFE=1.,
                   REFERENCE='NON_DEFINI',
                   MAILLE='MA1',),
                ),
          )
TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 5.6031658101886E+09,
           VALE_REFE= 5.6E9,
           NOM_PARA='TOTALE',
           TABLE=ENERSM,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,),
           )

VT = CALC_CHAMP(reuse=VT,RESULTAT=VT,ENERGIE=('ENEL_ELGA'),)


TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=VT,
                   NOM_CHAM='ENEL_ELGA',
                   MAILLE='MA1',POINT=1,
                   NOM_CMP='TOTALE',
                   VALE_CALC=5.6031658101886,
                   VALE_REFE=5.6,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=1.E-2,),))


FIN()
