                                NUME_COUCHE=1,
                                NIVE_COUCHE='SUP',
                                ANGLE=89,),);
# la maille la plus proche de L/SQRT(3)
TEST_RESU(   RESU=_F( RESULTAT = SITU89,
                    NOM_CHAM = 'SIGM_ELNO',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'SIXX',
                    MAILLE='M12',
                    GROUP_NO='P8',
                    REFERENCE='ANALYTIQUE',VALE=SIMAX,
                    PRECISION = 0.01,
                    CRITERE = 'RELATIF')
                    )

SITU91=POST_CHAMP(RESULTAT=MEST1B,
                  TOUT_ORDRE='OUI',
                  EXTR_TUYAU=_F(NOM_CHAM='SIGM_ELNO',
                                NUME_COUCHE=1,
                                NIVE_COUCHE='SUP',
                                ANGLE=91,),);
# la maille la plus proche de L/SQRT(3)
TEST_RESU(   RESU=_F( RESULTAT = SITU91,
                    NOM_CHAM = 'SIGM_ELNO',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'SIXX',
                    MAILLE='M12',
                    GROUP_NO='P8',
                    REFERENCE='ANALYTIQUE',VALE=SIMAX,
                    PRECISION = 0.01,
                    CRITERE = 'RELATIF')
                    )


SITU0=POST_CHAMP(RESULTAT=MEST1B,
                 TOUT_ORDRE='OUI',
                 EXTR_TUYAU=_F(NOM_CHAM='SIGM_ELNO',
                               NUME_COUCHE=1,
                               NIVE_COUCHE='SUP',
                               ANGLE=360,),);
# la maille la plus proche de L/SQRT(3)
TEST_RESU(   RESU=_F( RESULTAT = SITU0,
                    NOM_CHAM = 'SIGM_ELNO',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'SIXX',
                    MAILLE='M12',
                    GROUP_NO='P8',
                    REFERENCE='ANALYTIQUE',VALE=0.,
                    PRECISION = 0.01,
                    CRITERE = 'ABSOLU')
                    )

FIN( )
