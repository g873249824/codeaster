                    MAILLE='M11',
                    GROUP_NO='P8',
                    REFERENCE='ANALYTIQUE',VALE=SIMAX,
                    PRECISION = 0.01,
                    CRITERE = 'RELATIF')
                    )

SITU89=POST_CHAMP(RESULTAT=MEST1B,
                  TOUT_ORDRE='OUI',
