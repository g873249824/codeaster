DEBUT(CODE=_F(NOM='SSLL102I',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#

M1=LIRE_MAILLAGE(FORMAT='MED',  )


MA=CREA_MAILLAGE(     MAILLAGE=M1,
       MODI_MAILLE=_F(  OPTION = 'SEG3_4', TOUT = 'OUI')
)
MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(        MAILLAGE=MA,
  AFFE=_F( TOUT = 'OUI', PHENOMENE = 'MECANIQUE', MODELISATION = 'TUYAU_3M')
                        )
#
MATR=DEFI_MATERIAU(    ELAS=_F( E = 2.E11,
                               NU = 0.3)
                     )

#
CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,
                              AFFE=_F( TOUT = 'OUI',
                                    MATER = MATR)
                        )

#
CAREL=AFFE_CARA_ELEM(    MODELE=MO,INFO=2,
ORIENTATION=_F( GROUP_NO = 'P1', CARA = 'GENE_TUYAU', VALE = (0., 0., -1.,), ),
                           POUTRE=_F(
       GROUP_MA = 'TOUT',

                                    SECTION = 'CERCLE',
                                    CARA = ('R',  'EP',),
                                    VALE = ( 0.100,0.01,
                                              ))
                       )

#
F1=DEFI_FONCTION(    NOM_PARA='X',
                           VALE=(0.,  0.,
                                 6.,  6000.,
                                ),
                       INTERPOL='LIN'
                   )

#
CHARG1=AFFE_CHAR_MECA(      MODELE=MO,DDL_IMPO=(
                            _F( GROUP_NO = 'P1',
                                      DX = 0.,
                                      DY = 0.,
                                      DZ = 0.,
                                      DRX = 0.,
                                      DRY = 0.),
                            _F( GROUP_NO = 'P11',
                                      DY = 0.))
                        )

#
CHARG2=AFFE_CHAR_MECA_F(          MODELE=MO,
                              FORCE_POUTRE=_F(
       GROUP_MA = 'TOUT',

                                            FY = F1)
                          )

LINST=DEFI_LIST_REEL(DEBUT=0.0, INTERVALLE=_F(JUSQU_A=1.,  NOMBRE=1,));
MEST1=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CHMAT,
                 CARA_ELEM=CAREL,
                 EXCIT=(_F(CHARGE=CHARG1,),
                        _F(CHARGE=CHARG2,),),
                 COMP_INCR=_F(RELATION='ELAS',),
                 INCREMENT=_F(LIST_INST=LINST,),
                 NEWTON=_F(MATRICE='ELASTIQUE',),)

#
#
MEST1=CALC_ELEM( reuse=MEST1,
                         RESULTAT=MEST1,
                           OPTION=('EFGE_ELNO',
                                   )
                   )
MEST1=CALC_NO( reuse=MEST1,
                         RESULTAT=MEST1,
                           OPTION=('FORC_NODA','REAC_NODA',
                                   'EFGE_NOEU',
                                   )
                   )
#
TEST_RESU(   RESU=_F( RESULTAT = MEST1,
                    NOM_CHAM = 'DEPL',
                    NUME_ORDRE = 1,
                    GROUP_NO = 'P7',
                    NOM_CMP = 'DY',
                    REFERENCE='ANALYTIQUE',VALE=0.0093888,
                    PRECISION = 0.006,
                    CRITERE = 'RELATIF')
          )

TEST_RESU(   RESU=_F( RESULTAT = MEST1,
                    NOM_CHAM = 'EFGE_NOEU',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'VY',
                    GROUP_NO = 'P1',
                    REFERENCE='ANALYTIQUE',VALE=6000.,
                    PRECISION = 0.002,
                    CRITERE = 'RELATIF')
          )
TEST_RESU(   RESU=_F( RESULTAT = MEST1,
                    NOM_CHAM = 'REAC_NODA',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'DY',
                    GROUP_NO = 'P1',
                    REFERENCE='ANALYTIQUE',VALE=-6000.,
                    CRITERE = 'RELATIF')
          )

#
TEST_RESU(   RESU=_F( RESULTAT = MEST1,
                    NOM_CHAM = 'EFGE_NOEU',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'VY',
                    GROUP_NO='P11',
                    REFERENCE='ANALYTIQUE',VALE=-12000.,
                    CRITERE = 'RELATIF')
          )

TEST_RESU(   RESU=_F( RESULTAT = MEST1,
                    NOM_CHAM = 'REAC_NODA',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'DY',
                    GROUP_NO='P11',
                    REFERENCE='ANALYTIQUE',VALE=-12000.,
                    CRITERE = 'RELATIF')
          )

#
TEST_RESU(   RESU=_F( RESULTAT = MEST1,
                    NOM_CHAM = 'EFGE_NOEU',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'MFZ',
                    GROUP_NO='P8',
                    REFERENCE='ANALYTIQUE',VALE=-13856.41,
                    PRECISION = 0.002,
                    CRITERE = 'RELATIF')
          )

#
MEST1B=MECA_STATIQUE(        MODELE=MO,
                          CHAM_MATER=CHMAT,
                           CARA_ELEM=CAREL,EXCIT=(
                               _F( CHARGE = CHARG1),
                                     _F( CHARGE = CHARG2))
                      )

MEST1B=CALC_ELEM( reuse=MEST1B,
                         RESULTAT=MEST1B,
                           OPTION=('EFGE_ELNO', )
                   )
MEST1B=CALC_NO( reuse=MEST1B,
                         RESULTAT=MEST1B,
                           OPTION=('FORC_NODA','REAC_NODA',
                                   'EFGE_NOEU',
                                   )
                   )
TEST_RESU(   RESU=_F( RESULTAT = MEST1B,
                    NOM_CHAM = 'DEPL',
                    NUME_ORDRE = 1,
                    GROUP_NO = 'P7',
                    NOM_CMP = 'DY',
                    REFERENCE='ANALYTIQUE',VALE=0.0093888,
                    PRECISION = 0.006,
                    CRITERE = 'RELATIF')
          )

TEST_RESU(   RESU=_F( RESULTAT = MEST1B,
                    NOM_CHAM = 'EFGE_NOEU',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'VY',
                    GROUP_NO = 'P1',
                    REFERENCE='ANALYTIQUE',VALE=6000.,
                    PRECISION = 0.002,
                    CRITERE = 'RELATIF')
          )
TEST_RESU(   RESU=_F( RESULTAT = MEST1B,
                    NOM_CHAM = 'REAC_NODA',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'DY',
                    GROUP_NO = 'P1',
                    REFERENCE='ANALYTIQUE',VALE=-6000.,
                    CRITERE = 'RELATIF')
          )

#
TEST_RESU(   RESU=_F( RESULTAT = MEST1B,
                    NOM_CHAM = 'EFGE_NOEU',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'VY',
                    GROUP_NO='P11',
                    REFERENCE='ANALYTIQUE',VALE=-12000.,
                    CRITERE = 'RELATIF')
          )

TEST_RESU(   RESU=_F( RESULTAT = MEST1B,
                    NOM_CHAM = 'REAC_NODA',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'DY',
                    GROUP_NO='P11',
                    REFERENCE='ANALYTIQUE',VALE=-12000.,
                    CRITERE = 'RELATIF')
          )

#
TEST_RESU(   RESU=_F( RESULTAT = MEST1B,
                    NOM_CHAM = 'EFGE_NOEU',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'MFZ',
                    GROUP_NO='P8',
                    REFERENCE='ANALYTIQUE',VALE=-13856.41,
                    PRECISION = 0.002,
                    CRITERE = 'RELATIF')
          )
# test de ANGLE

IY=2.70098E-05

MZMAX=-13856.41

R=0.1

EP=0.01

SIMAX=MZMAX*(R-EP/2.)/IY

MEST1B=CALC_ELEM(reuse=MEST1B,
                 RESULTAT=MEST1B,
                 TOUT_ORDRE='OUI',
                 OPTION=('SIGM_ELNO',),);

SITU90=POST_CHAMP(RESULTAT=MEST1B,
                  TOUT_ORDRE='OUI',
                  EXTR_TUYAU=_F(NOM_CHAM='SIGM_ELNO',
                                NUME_COUCHE=1,
                                NIVE_COUCHE='SUP',
                                ANGLE=90,),);
# la maille la plus proche de L/SQRT(3)
TEST_RESU(   RESU=_F( RESULTAT = SITU90,
                    NOM_CHAM = 'SIGM_ELNO',
                    NUME_ORDRE = 1,
                    NOM_CMP = 'SIXX',
                    MAILLE='M12',
                    GROUP_NO='P8',
                    REFERENCE='ANALYTIQUE',VALE=SIMAX,
                    PRECISION = 0.01,
                    CRITERE = 'RELATIF')
                    )

TEST_RESU(   RESU=_F( RESULTAT = SITU90,
                    NOM_CHAM = 'SIGM_ELNO',
                    NUME_ORDRE = 1,
