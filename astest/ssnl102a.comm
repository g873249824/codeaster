DEBUT(   CODE=_F(  NOM = 'SSNL102A',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),  )

MO=AFFE_MODELE(   MAILLAGE=MA,
                     AFFE=_F(  MAILLE = 'D1',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'DIS_TR')  )

F1=DEFI_FONCTION(   NOM_PARA='INST',
         VALE=(0., 0., 1., 6., 3., 4., 6., 7., 7., 17., 8., 40., 10., 20., 12., 42., ),
                       PROL_DROITE='LINEAIRE',
                       PROL_GAUCHE='LINEAIRE'   )

LINST=DEFI_LIST_REEL(   DEBUT=0.,
                           INTERVALLE=_F( JUSQU_A = 12., PAS = 1.)   )

MAT02=DEFI_MATERIAU(   ASSE_CORN=_F( 
                         NU_1 = 10050.,   MU_1 = 150000.,
                         DXU_1 = 1.,   DRYU_1 = 67.E-3,  C_1 = 0.95,
                         NU_2 = 50000.,   MU_2 = 750000.,
                         DXU_2 = 10.,  DRYU_2 = 0.01,    C_2 = 0.95,
                         KY = 1.E6,  KZ = 1.E6,  KRX = 1.E9,  KRZ = 1.E9)  )

CHMAT=AFFE_MATERIAU(   MAILLAGE=MA,
                          AFFE=_F(  MAILLE = 'D1',
                                 MATER = MAT02)   )

CAREL=AFFE_CARA_ELEM(   MODELE=MO,
                    DISCRET=_F(  MAILLE = 'D1',
                              CARA = 'K_TR_D_L',
                              VALE = (1.E6, 1.E6, 1.E6, 1.E9, 1.E9, 1.E9,)),
                    ORIENTATION=_F(  MAILLE = 'D1',
                                  CARA = 'ANGL_NAUT',
                                  VALE = ( 0.,  0., 0., ))
                         )

CH1=AFFE_CHAR_MECA(   MODELE=MO,
                  DDL_IMPO=_F(  NOEUD = 'N2',
                             DX = 0.,  DY = 0.,  DZ = 0.,
                             DRX = 0.,  DRY = 0.,  DRZ = 0.),
                  FORCE_NODALE=_F(  NOEUD = 'N3',
                                 FX = 1000.,  FY = 0.,     FZ = 0.,
                                 MX = 0.,     MY = 3000.,  MZ = 0.)
                       )

STATNL=STAT_NON_LINE( 
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CAREL,
                             EXCIT=_F( 
                CHARGE = CH1,
                FONC_MULT = F1),
                         COMP_INCR=_F( 
                RELATION = 'ASSE_CORN',
                MAILLE = 'D1'),
                         INCREMENT=_F( 
                LIST_INST = LINST,
                NUME_INST_FIN = 12),
                       CONVERGENCE=_F( 
                ITER_GLOB_MAXI = 40),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1)
              )

STATNL=CALC_ELEM( reuse=STATNL,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=STATNL,
              )

#

TEST_RESU(RESU=( 
           _F(  RESULTAT = STATNL,         NUME_ORDRE = 12,
             PARA = 'INST',             VALE = 1.20000E+01),
           _F(  RESULTAT = STATNL,         NUME_ORDRE = 7,
             PARA = 'ITER_GLOB',        VALE_I = 0,
             CRITERE = 'ABSOLU',         PRECISION = 2.),
           _F(  RESULTAT = STATNL,         NUME_ORDRE = 12,
             PARA = 'ITER_GLOB',        VALE_I = 0,
             CRITERE = 'ABSOLU',         PRECISION = 6.),
           _F(  RESULTAT = STATNL,         NUME_ORDRE = 12,
             CRITERE = 'ABSOLU',        PRECISION = 1.E-05,
             PARA = 'RESI_GLOB_RELA',   VALE = 0.),
           _F(  RESULTAT = STATNL,         NUME_ORDRE = 12,
             CRITERE = 'ABSOLU',        PRECISION = 1.,
             PARA = 'RESI_GLOB',        VALE = 0.),
           _F(  RESULTAT = STATNL,         NUME_ORDRE = 12,
             CRITERE = 'ABSOLU',         PRECISION = 2.,
             PARA = 'ITER_LINE',        VALE_I = 0))
           )

TEST_RESU(RESU=(
              _F( 
                RESULTAT = STATNL, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
  NUME_ORDRE = 6, NOEUD = 'N3', VALE = 9.46805E-2),
              _F( 
                RESULTAT = STATNL, NOM_CHAM = 'DEPL', NOM_CMP = 'DRY',
  NUME_ORDRE = 6, NOEUD = 'N3', VALE = 1.27506E-3),

              _F( 
                RESULTAT = STATNL, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
  NUME_ORDRE = 12, NOEUD = 'N3', VALE = 3.7365),
              _F( 
                RESULTAT = STATNL, NOM_CHAM = 'DEPL', NOM_CMP = 'DRY',
  NUME_ORDRE = 12, NOEUD = 'N3', VALE = 4.9951E-2),

              _F( 
   RESULTAT = STATNL, NOM_CHAM = 'VARI_ELNO_ELGA', NOM_CMP = 'V1',
   NUME_ORDRE = 6, MAILLE = 'D1', NOEUD = 'N3', VALE = 9.65742E-2),

              _F( 
   RESULTAT = STATNL, NOM_CHAM = 'VARI_ELGA', NOM_CMP = 'V1',
   NUME_ORDRE = 7, MAILLE = 'D1', POINT = 2, VALE = 1.07699))

          )

FIN()
