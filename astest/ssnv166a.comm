           VALE_REFE=-1.1220000E7,
           NOM_PARA='K3',
           TABLE=K1_1,
           FILTRE=_F(NOM_PARA='NOEUD_FOND',
                     VALE_K='N5676',),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.23000000000000001,
           VALE_CALC= 9.8797654991678E+06,
           VALE_REFE=1.1710000E7,
