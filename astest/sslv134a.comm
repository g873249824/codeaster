                                MAILLE_ORIG = 'M3233',),
                   SYME='OUI',
                   LEVRE_SUP=_F(GROUP_MA = 'LEVINF',)
                  )

G01=CALC_G(        RESULTAT=RESU,
                   OPTION='CALC_G_GLOB',
                   THETA=_F(
                            R_INF=0.24,
                            FOND_FISS=FF1,
                            MODULE=1.0,
                            R_SUP=0.53),
           )

