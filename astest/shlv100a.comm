# SHLV100 A               COMMANDES                            31/05/91
# CYLINDRE SOUS PRESSION INTERNE HARMONIQUE AVEC EXTREMITES BLOQUEES
# SOLUTION ANALYTIQUE EN DEFORMATIONS PLANES
#
#         ..                      JWT                JWT
# K X + M X   = P    AVEC  P = P E        ET  X = X E
#       2
# (K - W M) X = P    AVEC  W = 0.2 RD/S   ET  P = 1 MPA
#
# MODELISATION : ELEMENTS MECANIQUE 3D (PENTA6+HEXA8+QUAD4)
# MAILLAGE     : ALIBABA > ASTER > SUPERTAB
#
# ----------------------------------------------------------------------
# GROUPES DE NOEUDS ET MAILLES DANS PLAN ABEF :
# A=GRNO1  B=GRNO2  C=GRNO3  D=GRNO4  E=GRNO5  F=GRNO6
# NOEUDS(AB)=GRNM11          SEG(EF)=GRMA12    SEG(AE)=GRMA13
# GROUPES DE NOEUDS ET MAILLES EN 3D :
# GRNO: A B C D E F    A2 B2 C2 D2 E2 F2   A3 B3 C3 D3 E3 F3  BORDAB
# GRMA: FACEEF         FACEAE
# ----------------------------------------------------------------------

DEBUT(CODE=_F(NOM='SHLV100A',
              NIV_PUB_WEB='INTERNET',),);

M=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

M=MODI_MAILLAGE(reuse =M,
                MAILLAGE=M,
                ORIE_PEAU_3D=_F(GROUP_MA=('FACEEF','FACEAE',),),
                MODELE=MO,);

MA=DEFI_MATERIAU(ELAS=_F(E=26.0,
                         NU=0.3,
                         RHO=35.0,
                         ALPHA=0.0,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=MA,),);

CH=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(TOUT='OUI',
                               DZ=0.0,),
                            _F(GROUP_NO='BORDAB',
                               DY=0.0,),),
                  FACE_IMPO=_F(GROUP_MA='FACEEF',
                               DNOR=0.0,),
                  PRES_REP=_F(GROUP_MA='FACEAE',
                              PRES=1.0,),);

KE=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                  MODELE=MO,
                  CHAM_MATER=CM,
                  CHARGE=CH,);

ME=CALC_MATR_ELEM(OPTION='MASS_MECA',
                  MODELE=MO,
                  CHAM_MATER=CM,
                  CHARGE=CH,);

VE=CALC_VECT_ELEM(OPTION='CHAR_MECA',
                  CHARGE=CH,
                  CHAM_MATER=CM,);

NU=NUME_DDL(MATR_RIGI=KE,);

KAS=ASSE_MATRICE(MATR_ELEM=KE,
                 NUME_DDL=NU,);

MAS=ASSE_MATRICE(MATR_ELEM=ME,
                 NUME_DDL=NU,);

VAS=ASSE_VECTEUR(VECT_ELEM=VE,
                 NUME_DDL=NU,);

FREQ=DEFI_LIST_REEL(VALE=0.03183,);

DYNAH=DYNA_LINE_HARM(MATR_MASS=MAS,
                     MATR_RIGI=KAS,
                     LIST_FREQ=FREQ,
                     EXCIT=_F(VECT_ASSE=VAS,
                              COEF_MULT=1.0,),);

DYNAH=CALC_ELEM(reuse =DYNAH,
                MODELE=MO,
                CHAM_MATER=CM,
                RESULTAT=DYNAH,
                OPTION='SIGM_ELNO_DEPL',);


