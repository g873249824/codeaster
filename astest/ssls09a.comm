# RESPONSABLE  DESROCHE X.DESROCHES

DEBUT( CODE=_F(  NOM = 'SSLS09A',NIV_PUB_WEB='INTERNET') )

MAIL=LIRE_MAILLAGE(FORMAT='MED',)

#
# DEFINITION DES GROUPES DE NOEUDS
MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'APPUI'),
                       _F(  GROUP_MA = 'LEF'),
                       _F(  GROUP_MA = 'LCD'))
                   )

#
# DEFINITION DU MODELE
MODELE=AFFE_MODELE(    MAILLAGE=MAIL,
                         AFFE=_F(  TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 2.1E11,  NU = 0.3,
                                 ALPHA = 1.E-5,  RHO = 8000.)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(    MAILLAGE=MAIL,
                          AFFE=_F(  TOUT = 'OUI',
                                 MATER = ACIER)  )

#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
CHARGT=AFFE_CHAR_MECA(    MODELE=MODELE,
                           DDL_IMPO=_F(  GROUP_NO = 'APPUI',  DY = 0.),
                           FORCE_INTERNE=_F(  TOUT = 'OUI',
                                           FY = -78500.)
                        )

#
# RESOLUTION
DEF=MECA_STATIQUE(    MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                        EXCIT=_F(  CHARGE = CHARGT)  )

#
# RECUPERATION DU CHAMP DE DEPLACEMENTS
U=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=DEF,
                 NUME_ORDRE=1
                  )

#
# CALCUL DES CONTRAINTES AUX NOEUDS

DEF=CALC_CHAMP(reuse=DEF,RESULTAT=DEF,CONTRAINTE=('SIGM_ELNO'))


#
# TEST_RESU DEPLACEMENTS
#
TEST_RESU(CHAM_NO=(   _F(  CHAM_GD = U,
                        NOEUD = 'N452',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001),
              _F(  CHAM_GD = U,
                        NOEUD = 'N504',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001),
              _F(  CHAM_GD = U,
                        NOEUD = 'N503',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001))
          )

#
TEST_RESU(   CHAM_NO=_F(  CHAM_GD = U,
                        NOEUD = 'N51',
                        NOM_CMP = 'DX',
                        REFERENCE='ANALYTIQUE',
                        VALE = 4.49E-7,
                        PRECISION = 0.015)
          )

#
# TEST_RESU CONTRAINTES
#
TEST_RESU(RESU=(   _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N51',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIYY',
                          REFERENCE='ANALYTIQUE',
                          VALE = -3.14E5,
                          PRECISION = 0.0001),
              _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N1',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIXX',
                          REFERENCE='ANALYTIQUE',
                          VALE = 0.,
                          PRECISION = 20.2,
                          CRITERE = 'ABSOLU'),
              _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N101',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIXX',
                          REFERENCE='ANALYTIQUE',
                          VALE = 0.,
                          PRECISION = 38.0,
                          CRITERE = 'ABSOLU'),
              _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N51',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIXX',
                          REFERENCE='ANALYTIQUE',
                          VALE = 0.,
                          PRECISION = 29.1,
                          CRITERE = 'ABSOLU'))
          )

FIN()
