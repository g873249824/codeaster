DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
K0=1.E+05

M0=450.0

X0=0.10

A0=X0 / 10.0

W0=pi/4.

V0=W0 * A0

#
PAS=0.02

DER=18.

METHOD1='DIFF_CENTRE'

METHOD2='DEVOGE'

#
# EMPLOI DE FORMULE POUR DEFINIR L ACCELERATION ET LA NON-LINEARITE----
ACCE_X = FORMULE(VALE="""A0*sin(W0*INST) *
                  (-W0**2 +(K0/M0)*(1. - abs(A0*sin(W0*INST))/X0))
                """,
                 A0=A0,
                 W0=W0,
                 K0=K0,
                 M0=M0,
                 X0=X0,
                 NOM_PARA='INST',)
f_NLIN = FORMULE(VALE='K0*DX*((abs(DX)-1.E-6)/X0) ',
                 K0=K0,
                 X0=X0,
                 NOM_PARA='DX',)

def NONLINEARITE_SOL(X) :
    X0 = 1.E-6
    if X == 0. : return 0
    sign = X/abs(X)
    X = abs(X)
    if X <= X0 : return 0
    else       : return sign* ( f_NLIN(X) )

FOR_EFDE = FORMULE(VALE='NONLINEARITE_SOL(DX)',
                   NONLINEARITE_SOL=NONLINEARITE_SOL,
                   NOM_PARA='DX',)


