DEBUT(   CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI', HIST_ETAPE='OUI'))

MAILLAGE=LIRE_MAILLAGE( FORMAT='MED', UNITE=20 )


MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                MAILLAGE=MAILLAGE,
                CREA_GROUP_MA=_F(
                  OPTION='SPHERE',
                  GROUP_NO_CENTRE = 'NY10',
                  RAYON = 1.E-3,
                  NOM='GRMAY10'
                  )
                  );
MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                MAILLAGE=MAILLAGE,
                CREA_GROUP_MA=_F(
                  INTERSEC=('GRMAY10', 'DISFROT'),
                  NOM='RNFRY10'
                  )
                  );


MODELE=AFFE_MODELE(
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = ('COQUE',),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'Q4GG'),
                     _F(  GROUP_MA = 'CABLE2',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE'),
                     _F(  GROUP_MA = 'DISFROT',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'DIS_T')
                            ) )


CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       COQUE=(_F(  GROUP_MA = 'COQUE',
                               EPAIS = 1.,
                               VECTEUR = (0.,0.,1.),
                               ),
                               ),
                       BARRE=_F(  GROUP_MA = 'CABLE2',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 1.5E-2, )),
                       DISCRET = _F(GROUP_MA = 'DISFROT',
                                    CARA = 'K_T_D_L',
                                    VALE = (1.E10,1.E10,1.E10),
                                    ),
                               )


BETON = DEFI_MATERIAU(ELAS=_F(
                    E = 3E10,
                    NU = 0.,
                    RHO = 2500,
                   ),
                    BPEL_BETON=_F()
                  );


ACIER = DEFI_MATERIAU(ELAS=_F(
                    E = 2E11,
                    NU = 0.,
                    RHO = 7800,
                   ),
                   BPEL_ACIER=_F(
                              FROT_COURB = 0.2,
                              FROT_LINE = 3.0E-3)
                  )


CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAILLAGE,AFFE=(
                       _F(  GROUP_MA = ('COQUE',),
                              MATER = BETON),
                       _F(  GROUP_MA = 'CABLE2',
                              MATER = ACIER),
                       _F(  GROUP_MA = 'DISFROT',
                              MATER = ACIER),      
                              )
                         )


CL=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                      DDL_IMPO=(
                           _F(  GROUP_MA=('COTE1','COTE2'),
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,
                               DRX = 0.,
                               DRY = 0.,
                               DRZ = 0.),
                               ),
                        INFO=1 )

CABLEPR=DEFI_CABLE_BP(MODELE=MODELE,
                      CHAM_MATER=CHMATER,
                      CARA_ELEM=CARAELEM,
                      GROUP_MA_BETON='COQUE',
                      DEFI_CABLE=_F(
                            GROUP_MA = 'CABLE2',
                            GROUP_NO_ANCRAGE = ( 'ANCR1',  'ANCR2', ),),
                            TYPE_ANCRAGE = ( 'ACTIF',   'ACTIF',  ),
                            TENSION_INIT=2.0E+7,
                            RECUL_ANCRAGE=0.01 )

#
TABINI = RECU_TABLE(CO=CABLEPR, NOM_TABLE='CABLE_BP')
#IMPR_TABLE(TABLE= TABINI, NOM_PARA=('NOEUD_CABLE','ABSC_CURV','TENSION'),SEPARATEUR=';')

temps=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=(_F(JUSQU_A=1.0,NOMBRE=5,),
                                    ),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = temps,),)

U_PREC=CALC_PRECONT(MODELE=MODELE,
                    CHAM_MATER=CHMATER,
                    CARA_ELEM=CARAELEM,
                    EXCIT=(_F(CHARGE=CL,),),
                    CABLE_BP=CABLEPR,
                    COMPORTEMENT=(_F( RELATION = 'ELAS',DEFORMATION = 'GROT_GDEP',
                                    GROUP_MA='COQUE',),
                                  _F( RELATION = 'ELAS',DEFORMATION = 'PETIT_REAC',
                                    GROUP_MA ='CABLE2',),
                                  _F( RELATION = 'ELAS',DEFORMATION = 'PETIT',
                                    GROUP_MA ='DISFROT',),
                                 ),
                    INCREMENT=_F(LIST_INST=temps,),
                    )


FROT=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                      RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'NON',
                               RELA_CINE = 'OUI',
                               TYPE_EPX = 'FROT'),
                      INFO=1 )



CARAEPX=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       COQUE=(_F(  GROUP_MA = 'COQUE',
                               EPAIS = 1.,
                               VECTEUR = (0.,0.,1.),
                               ),
                               ),
                       BARRE=_F(  GROUP_MA = 'CABLE2',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 1.5E-2, )),
                               )



# équilibre activé

U_EPX1 = CALC_EUROPLEXUS(
   ETAT_INIT=_F(RESULTAT=U_PREC,CONTRAINTE='OUI',EQUILIBRE='OUI'),
   CARA_ELEM = CARAEPX,
   COMPORTEMENT =(
                 _F(
                    RELATION = 'ELAS',
                    GROUP_MA = ('CABLE2','COQUE'),
                   ),
                 _F(
                    RELATION = 'BPEL_FROT',
                    GROUP_MA = ('DISFROT'),
                   ),
              ),
   EXCIT=(
          _F(CHARGE=CL,),
          _F(CHARGE=FROT,),
          ),
   LANCEMENT ='OUI',
   CALCUL = _F(TYPE_DISCRETISATION  ='AUTO',
               INST_INIT = 0,
               INST_FIN  = 0.01,
               NMAX      = 2000000,
               CSTAB = 0.8,
               ),
   ARCHIVAGE   = _F(PAS_NBRE=159,),
   );




# équilibre désactivé

U_EPX2 = CALC_EUROPLEXUS(
   ETAT_INIT=_F(RESULTAT=U_PREC,CONTRAINTE='OUI',EQUILIBRE='NON'),
   CARA_ELEM = CARAEPX,
   COMPORTEMENT =(
                 _F(
                    RELATION = 'ELAS',
                    GROUP_MA = ('CABLE2','COQUE'),
                   ),
                 _F(
                    RELATION = 'BPEL_FROT',
                    GROUP_MA = ('DISFROT'),
                   ),
              ),
   EXCIT=(
          _F(CHARGE=CL,),
          _F(CHARGE=FROT,),
          ),
   LANCEMENT ='OUI',
   CALCUL = _F(TYPE_DISCRETISATION  ='AUTO',
               INST_INIT = 0,
               INST_FIN  = 0.2,
               NMAX      = 2000000,
               CSTAB = 0.8,
               ),
   AMORTISSEMENT= _F(TYPE_AMOR='QUASI_STATIQUE',
                     FREQUENCE = 50.,
                     COEF_AMOR = 0.1,
                    ),
   ARCHIVAGE   = _F(PAS_NBRE=3166,),
   );


CALC_CHAMP(reuse = U_EPX2, RESULTAT = U_EPX2, CONTRAINTE = 'SIEF_NOEU', 
           GROUP_MA = 'CABLE2')

ERR_RELA = FORMULE(NOM_PARA=('TENSION', 'N'), VALE='abs(TENSION-N)/TENSION')
nume_ordre1 = 3166
TABEPX1 = CREA_TABLE(RESU = (_F(RESULTAT = U_EPX2, NOM_CHAM ='SIEF_NOEU', 
                                GROUP_MA='CABLE2', NUME_ORDRE = nume_ordre1, 
                                NOM_CMP ='N'),
                        ))

TABINI = CALC_TABLE(reuse = TABINI, TABLE=TABINI, ACTION=_F(OPERATION='RENOMME',
                                NOM_PARA=('NOEUD_CABLE','NOEUD'), ))

TABINI = CALC_TABLE(reuse=TABINI ,TABLE=TABINI, ACTION=_F(OPERATION='COMB',
                                TABLE=TABEPX1,
                                NOM_PARA=('NOEUD'),
                                RESTREINT='NON' ) )

TABINI = CALC_TABLE(reuse=TABINI ,TABLE=TABINI, ACTION=_F(OPERATION='OPER',
                                FORMULE = ERR_RELA,
                                NOM_PARA=('ERR_RELA'),
                                ))
                                
IMPR_TABLE(TABLE = TABINI, NOM_PARA=('NOEUD', 'ABSC_CURV', 'TENSION', 'N','ERR_RELA'), 
                                     SEPARATEUR=';')




TEST_RESU(RESU=(_F(INST = 1.0,
                  RESULTAT=U_PREC,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DX',
                  VALE_CALC=-0.000146058993962,
                  ),
                _F(INST = 1.0,
                  RESULTAT=U_PREC,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DY',
                  VALE_CALC=-0.00361862774358,
                  ),
                _F(INST = 1.0,
                  RESULTAT=U_PREC,
                  NOM_CHAM='SIEF_ELGA',
                  MAILLE='M150',
                  POINT = 1,
                  NOM_CMP='N',
                  VALE_CALC=14051238.3598,
                  ),
                )
          )



TEST_RESU(RESU=(_F(NUME_ORDRE = 159,
                  RESULTAT=U_EPX1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DX',
                  VALE_CALC=-0.000146058993962,
                  VALE_REFE=-0.000146058993962,
                  REFERENCE = 'ANALYTIQUE',
                  ),
                _F(NUME_ORDRE = 159,
                  RESULTAT=U_EPX1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DY',
                  VALE_CALC=-0.00361862774358,
                  VALE_REFE=-0.00361862774358,
                  REFERENCE = 'ANALYTIQUE',
                  ),
                _F(NUME_ORDRE = 159,
                  RESULTAT=U_EPX1,
                  NOM_CHAM='SIEF_ELGA',
                  MAILLE='M150',
                  POINT = 1,
                  NOM_CMP='N',
                  VALE_CALC=14051238.3598,
                  VALE_REFE=14051238.3598,
                  REFERENCE = 'ANALYTIQUE',
                  ),
                )
          )

TEST_RESU(RESU=(_F(NUME_ORDRE = 3166,
                  RESULTAT=U_EPX2,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DX',
                  VALE_CALC=-0.000136725487245,
                  VALE_REFE=-1.36726E-04,
                  REFERENCE = 'SOURCE_EXTERNE',
                  ),
                _F(NUME_ORDRE = 3166,
                  RESULTAT=U_EPX2,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DY',
                  VALE_CALC=-0.00369190584599,
                  VALE_REFE=-3.69190E-03,
                  REFERENCE = 'SOURCE_EXTERNE',
                  ),
                _F(NUME_ORDRE = 3166,
                  RESULTAT=U_EPX2,
                  NOM_CHAM='SIEF_ELGA',
                  MAILLE='M150',
                  POINT = 1,
                  NOM_CMP='N',
                  VALE_CALC=14278815.9432,
                  VALE_REFE=14278815.0,
                  REFERENCE = 'SOURCE_EXTERNE',
                  ),
                )
          )

FIN()
