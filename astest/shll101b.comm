                   VALE_REFE_C=(-0.2099+0j),
                   REFERENCE = 'ANALYTIQUE',
                   PRECISION=5.0000000000000001E-4,
                   RESULTAT=DYNA_F1,
                   NOM_CHAM='ACCE',
                   NOEUD='N2',
                   NOM_CMP='DX',
                   ),
                ),
          )

