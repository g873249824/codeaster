                              R_SUP = 47.,
                              FOND_FISS=FOND),
                     EXCIT=_F(CHARGE=CH,),
                     OPTION='CALC_K_G')

G4=CALC_G(        RESULTAT=RESU,
                  NUME_ORDRE=1,
                  THETA=_F(
                           R_INF=3.0,
                           R_SUP=48.,
                           FOND_FISS=FOND,
                           MODULE=1.0,
                           DIRECTION=(1.,0.,0.,)),
                  EXCIT=_F(CHARGE=CH,),)

GK4=CALC_G(         RESULTAT=RESU,
                    NUME_ORDRE=1,
                    THETA=_F(R_INF = 3.0,
                              R_SUP = 48.,
                              FOND_FISS=FOND),
                    EXCIT=_F(CHARGE=CH,),
                    OPTION='CALC_K_G')


GK5=CALC_G(         RESULTAT=RESU,
                    NUME_ORDRE=1,
                    THETA=_F( FOND_FISS=FOND,
                             ),
                    EXCIT=_F(CHARGE=CH,),
                    OPTION='CALC_K_G')


