                            R_INF=5.0,
                            R_SUP=47.,
                            FOND_FISS=FOND,
                            MODULE=1.0,
                            DIRECTION=(1.,0.,0.,)),
                   EXCIT=_F(CHARGE=CH,),)

GK3=CALC_G(          RESULTAT=RESU,
