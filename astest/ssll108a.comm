DEBUT(CODE=_F(NOM='SSLL108A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))


MA=LIRE_MAILLAGE( )

MATER1=DEFI_MATERIAU(  ELAS=_F(  E = 200000.,   NU = 0.3,
                                  RHO = 10000.) )


CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
             AFFE=_F(  GROUP_MA = ('GRMA',),    MATER = MATER1)  )

MOD=AFFE_MODELE(  MAILLAGE=MA,AFFE=(
             _F(  GROUP_MA = ('GRMA',),
                   MODELISATION = '2D_DIS_T',
                   PHENOMENE = 'MECANIQUE'))
                   )

CELEM=AFFE_CARA_ELEM(   MODELE=MOD,
               VERIF=( 'MAILLE',  'NOEUD',  ),
                DISCRET_2D= _F( GROUP_MA = 'GRMA',
                                CARA = 'K_T_D_L',
                                VALE = (1000., 1000.,),)
                     )

CH=AFFE_CHAR_MECA(
            MODELE=MOD,
#
     DDL_IMPO=_F( NOEUD = 'P1',  DX = 0., DY = 0., ),
#
     FORCE_NODALE=_F( NOEUD = 'P11',  FX = 10.,)
                                       )



DEP=MECA_STATIQUE(MODELE=MOD, CARA_ELEM=CELEM,
                               CHAM_MATER=CHMAT,
                   EXCIT=_F( CHARGE = CH), )

CALC_ELEM( reuse= DEP ,RESULTAT= DEP ,
                     OPTION=(
                                                'EFGE_ELNO_DEPL',))

L_INST=DEFI_LIST_REEL(
              DEBUT=0.0,INTERVALLE=(

               _F(  JUSQU_A = 1.00, NOMBRE = 1))
             )

F_CHAR=DEFI_FONCTION(
                NOM_PARA='INST',
              PROL_DROITE='CONSTANT',
                    VALE=(
               0.,   0.,
               1.,   1.,
                         )
                    )
DEP2=STAT_NON_LINE(
                            MODELE=MOD,
                         CHAM_MATER=CHMAT,
                         CARA_ELEM=CELEM,
                         EXCIT=(
                             _F(
                CHARGE = CH),),
                         COMP_INCR=_F(
                RELATION = 'ELAS',
                                     ),
                         INCREMENT=_F(
                LIST_INST = L_INST,
                NUME_INST_FIN = 1),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_MAXI = 1.E-1,
                ITER_GLOB_MAXI = 50),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3)
              )


TEST_RESU(
  RESU = (
# DEPLACEMENTS
       _F(  RESULTAT = DEP2,  NOM_CHAM = 'DEPL', NUME_ORDRE = 1,
        CRITERE = 'RELATIF',   PRECISION = 1.E-7,
         NOEUD = 'P6',       NOM_CMP = 'DX',    VALE = 0.05),
       _F(  RESULTAT = DEP2,  NOM_CHAM = 'DEPL', NUME_ORDRE = 1,
        CRITERE = 'RELATIF',   PRECISION = 1.E-7,
         NOEUD = 'P11',       NOM_CMP = 'DX',    VALE = 0.1),
# EFFORT NORMAL
       _F( RESULTAT = DEP2, NOM_CHAM = 'SIEF_ELGA',
         MAILLE = 'S10', POINT = 1, NUME_ORDRE =1,
        NOM_CMP = 'N', VALE = 10., ),
        _F( RESULTAT = DEP2, NOM_CHAM = 'SIEF_ELGA',
         MAILLE = 'S5', POINT = 1, NUME_ORDRE =1,
         NOM_CMP = 'N', VALE = 10., ),
# DEPLACEMENTS
       _F(  RESULTAT = DEP,  NOM_CHAM = 'DEPL', NUME_ORDRE = 1,
        CRITERE = 'RELATIF',   PRECISION = 1.E-7,
         NOEUD = 'P6',       NOM_CMP = 'DX',    VALE = 0.05),
       _F(  RESULTAT = DEP,  NOM_CHAM = 'DEPL', NUME_ORDRE = 1,
        CRITERE = 'RELATIF',   PRECISION = 1.E-7,
         NOEUD = 'P11',       NOM_CMP = 'DX',    VALE = 0.1),
# EFFORT NORMAL
       _F( RESULTAT = DEP, NOM_CHAM = 'SIEF_ELGA',
         MAILLE = 'S10', POINT = 1, NUME_ORDRE =1,
        NOM_CMP = 'N', VALE = 10., ),
        _F( RESULTAT = DEP, NOM_CHAM = 'SIEF_ELGA',
         MAILLE = 'S5', POINT = 1, NUME_ORDRE =1,
         NOM_CMP = 'N', VALE = 10., ),
                ),
           )
