                            MODELE=MO,
                        CHAM_MATER=CHP_MATE,EXCIT=(
                             _F( 
                CHARGE = CHR_PLAN), _F( 
                CHARGE = CHR_ROTA,
                FONC_MULT = F_ROTA), _F( 
