                             CHARGE=CHR_FORC )

F1=ASSE_VECTEUR(       VECT_ELEM=VE1,
                          NUME_DDL=NUM1 )

CHA1=AFFE_CHAR_MECA(
              
                      MODELE=MO,
                   VECT_ASSE=F1
              )

