# MAILLAGE : ELEMENT 3D HEXAEDRE A 8 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
#

DEBUT( CODE=_F(  NOM = 'SSNV128N',NIV_PUB_WEB='INTERNET') )

#......................................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 3D HEXA8
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#......................................................................

PRE_GIBI()
#
MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'), )
#
MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOM='NCONTAR',
                                GROUP_MA='SCONTAR',),
                             _F(NOM='NPLAQ',
                                GROUP_MA='VPLAQ',),
                             _F(NOM='NPLAQZ',
                                DIFFE=('NPLAQ','NCONTAR',),))
              )
#
MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = '3D',)
              )
#
MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=(_F(GROUP_MA = 'SPRESV',),
                               _F(GROUP_MA = 'SPRESH',))
                )
#
MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11,
                             NU = 0.2,)
                    )
#
MATBAT=DEFI_MATERIAU(ELAS=_F(E = 1.0E16,
                             NU = 0.0,)
                    )
#
CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA = 'VPLAQ',
                             MATER = MATPLA,),
                          _F(GROUP_MA = 'VBATI',
                             MATER = MATBAT,))
                    )
#
CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA = 'SENCA',  DX = 0.0, DY = 0.0, DZ=0.0,),
                              _F(GROUP_MA = 'SBLBAX', DX = 0.0, DY = 0.0, DZ=0.0,),
                              _F(GROUP_MA = 'SBLOCX', DX = 0.0,),
                              _F(GROUP_MA = 'LBLOCY', DY = 0.0,),
                              _F(GROUP_NO = 'NPLAQZ', DZ = 0.0,)),
                    PRES_REP=(_F(GROUP_MA = 'SPRESV', PRES = 5.E07,),
                              _F(GROUP_MA = 'SPRESH', PRES = 15.E07,))
                   )
#
CHA4=AFFE_CHAR_MECA(MODELE=MO,
                    CONTACT=_F(GROUP_MA_ESCL  = 'SBATIR',
                               GROUP_MA_MAIT  = 'SCONTAR',
                               APPARIEMENT    = 'MAIT_ESCL',
                               RECHERCHE      = 'NOEUD_VOISIN',
                               NORMALE        = 'MAIT',
                               METHODE        = 'CONTINUE',
                               INTEGRATION    = 'NOEUD',
                               MODL_AXIS      = 'NON',
                               SEUIL_INIT     = 0,
                               COEF_REGU_FROT = 100.0,
                               COEF_REGU_CONT = 100.0,
                               ITER_GEOM_MAXI = 1,
                               ITER_FROT_MAXI = 8,
                               ITER_CONT_MAXI = 4,
                               FROTTEMENT     = 'COULOMB',
                               COULOMB        = 1.0,
                               PROJECTION     = 'LINEAIRE',)
                    )
#
#
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0,)
                    )
#
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 1,)
                      )
#
#-----------------------------------------------------------
#
U4M=STAT_NON_LINE(MODELE      = MO,
                 CHAM_MATER  = CHMAT,
                 EXCIT       =(_F(CHARGE = CHA1,
                                  FONC_MULT = RAMPE,),
                               _F(CHARGE = CHA4,)),
                 COMP_ELAS   = _F(RELATION = 'ELAS',),
                 NEWTON      = _F(REAC_ITER = 1,),
                INCREMENT  = _F(LIST_INST = L_INST,
                                SUBD_PAS = 1,
                                COEF_SUBD_PAS_1 = 1.,
                                SUBD_PAS_MINI = 0.000000001,),
                 CONVERGENCE = _F(ARRET = 'OUI',
                                  ITER_GLOB_MAXI = 20,
                                  RESI_GLOB_RELA = 1.0E-5,),
#                 INFO=2,
                )
#
TEST_RESU(RESU=(_F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPA',
                   NOM_CMP    = 'DX',
                   VALE       = 2.86E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                  ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPB',
                   NOM_CMP    = 'DX',
                   VALE       = 2.72E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPC',
                   NOM_CMP    = 'DX',
                   VALE       = 2.28E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPD',
                   NOM_CMP    = 'DX',
                   VALE       = 1.98E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPE',
                   NOM_CMP    = 'DX',
                   VALE       = 1.50E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                ))
       )
#
#-----------------------------------------------------------
#
U4L=STAT_NON_LINE(MODELE      = MO,
                 CHAM_MATER  = CHMAT,
                 EXCIT       =(_F(CHARGE = CHA1,
                                  FONC_MULT = RAMPE,),
                               _F(CHARGE = CHA4,)),
                 COMP_ELAS   = _F(RELATION = 'ELAS',),
                 SOLVEUR     = _F(METHODE = 'LDLT',
                                  RENUM = 'SANS',),
                 NEWTON      = _F(REAC_ITER = 1,),
                INCREMENT  = _F(LIST_INST = L_INST,
                                SUBD_PAS = 1,
                                COEF_SUBD_PAS_1 = 1.,
                                SUBD_PAS_MINI = 0.000000001,),
                 CONVERGENCE = _F(ARRET = 'OUI',
                                  ITER_GLOB_MAXI = 20,
                                  RESI_GLOB_RELA = 1.0E-5,),
#                 INFO=2,
                )
#
TEST_RESU(RESU=(_F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPA',
                   NOM_CMP    = 'DX',
                   VALE       = 2.86E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPB',
                   NOM_CMP    = 'DX',
                   VALE       = 2.72E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPC',
                   NOM_CMP    = 'DX',
                   VALE       = 2.28E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPD',
                   NOM_CMP    = 'DX',
                   VALE       = 1.98E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPE',
                   NOM_CMP    = 'DX',
                   VALE       = 1.50E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                ))
       )
#
#
FIN()
#
