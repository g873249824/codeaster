EPSMIN = -0.005;

NBCYCLE = 10;

NPAS=250

TFIN = ((TINI + (DT / 2.0)) + ((2.0 * DT) * NBCYCLE));

epsmoy = 0.0;

def epsimp(INST):
    vit=(EPSMAX-EPSMIN)/DT
    t1=INST-TINI
    if t1 <= DT/2. :
        epsi=vit*t1+epsmoy
    else :
        t2=t1-DT/2.
        t3=t2 - 2*DT*int(t2/2/DT)
        if t3 <= DT :
            epsi = EPSMAX - vit*t3
        else :
            epsi = EPSMIN + vit*(t3-DT)
    return epsi

