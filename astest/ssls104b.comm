DEBUT(CODE=_F( NOM = 'SSLS104B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# COQUE CYLINDRIQUE PINCEE AVEC DIAPHRAGMES
# MAILLAGE 1/8 DE CYLINDRE
#
# MODELISATION MEC3QU9H: 121 NOEUDS --  32 MAILLES QUAD9
#


MATERI_1=DEFI_MATERIAU( ELAS=_F(  E = 3.00E+6,      NU = 0.3000) )

MAILLA_1=LIRE_MAILLAGE( FORMAT='MED',UNITE = 19  )

MA1=CREA_MAILLAGE(MAILLAGE=MAILLA_1,
                  MODI_MAILLE=_F(TOUT='OUI',
                                 OPTION='QUAD8_9'));

MA1=DEFI_GROUP( reuse=MA1,   MAILLAGE=MA1, INFO=2,
  CREA_GROUP_NO=(_F(  NOM = 'AB' , GROUP_MA='AB'),
                 _F(  NOM = 'BF' , GROUP_MA='BF'),
                 _F(  NOM = 'EA' , GROUP_MA='EA'),
                 _F(  NOM = 'FE', GROUP_MA='FE'),
                 _F(  NOM = 'BFSANSB' , DIFFE=('BF','AB')),
                 _F(  NOM = 'EASANSA' , DIFFE=('EA','AB'))));

MAILLA_2=LIRE_MAILLAGE( FORMAT='MED',UNITE = 23  )

MA2=CREA_MAILLAGE(MAILLAGE=MAILLA_2,
                  MODI_MAILLE=_F(TOUT='OUI',
                                 OPTION='QUAD8_9'));

MA2=DEFI_GROUP( reuse=MA2,   MAILLAGE=MA2, INFO=2,
  CREA_GROUP_NO=(_F(  NOM = 'EF' , GROUP_MA='EF'),
                 _F(  NOM = 'CD' , GROUP_MA='CD'),
                 _F(  NOM = 'FC' , GROUP_MA='FC'),
                 _F(  NOM = 'DE' , GROUP_MA='DE'),
                 _F(  NOM = 'FCSANSC' , DIFFE=('FC','CD')),
                 _F(  NOM = 'DESANSD' , DIFFE=('DE','CD'))));

MA3=ASSE_MAILLAGE(MAILLAGE_1=MA1,
                  MAILLAGE_2=MA2,
                  OPERATION='COLLAGE',
                  COLLAGE=_F(GROUP_MA_1='FE',
                             GROUP_MA_2='EF'));

MA3=DEFI_GROUP( reuse=MA3,   MAILLAGE=MA3,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MA3=DEFI_GROUP( reuse=MA3,   MAILLAGE=MA3, INFO=2,
  CREA_GROUP_NO=(_F(  NOM = 'BCSANSBC', UNION=('BFSANSB','FCSANSC')),
                 _F(  NOM = 'ADSANSAD', UNION=('EASANSA','DESANSD'))));

CH_MAT_1=AFFE_MATERIAU(  MAILLAGE=MA3,
                            AFFE=_F(  TOUT = 'OUI',   MATER = MATERI_1) )

MODELE_1=AFFE_MODELE(  MAILLAGE=MA3, VERIF='MAILLE',
                     AFFE=_F( TOUT = 'OUI', MODELISATION = 'COQUE_3D',
                                PHENOMENE = 'MECANIQUE') )

CHARGE_1=AFFE_CHAR_MECA(MODELE=MODELE_1,DDL_IMPO=(
                  _F( GROUP_NO = 'AB',       DX = 0.,  DY = 0.,
                                                     DRZ = 0.),
                           _F( GROUP_NO = 'CD',       DZ = 0.,
                                              DRX = 0., DRY = 0.),
                           _F( GROUP_NO = 'BCSANSBC', DX = 0.,
                                              DRY = 0., DRZ = 0.),
                           _F( GROUP_NO = 'ADSANSAD', DY = 0.,
                                              DRX = 0., DRZ = 0.),
                           _F( GROUP_NO = 'C',        DX = 0., DRZ = 0.),
                           _F( GROUP_NO = 'D',        DY = 0., DRZ = 0.)),
                  FORCE_NODALE=_F(  GROUP_NO = 'C',  FY = -0.25) )

CARAC__1=AFFE_CARA_ELEM(   MODELE=MODELE_1,
                               COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 3.000,
                                  A_CIS = 0.83333333333333) )


MATELE_1=CALC_MATR_ELEM(   MODELE=MODELE_1,     CHARGE=CHARGE_1,
                               CARA_ELEM=CARAC__1,
                               CHAM_MATER=CH_MAT_1,
                               OPTION='RIGI_MECA' )

VEL=CALC_VECT_ELEM(   CHARGE=CHARGE_1,  OPTION='CHAR_MECA' )

NU=NUME_DDL(   MATR_RIGI=MATELE_1 )

MATASS=ASSE_MATRICE(   MATR_ELEM=MATELE_1,   NUME_DDL=NU )

VECASS=ASSE_VECTEUR(   VECT_ELEM=VEL,        NUME_DDL=NU )

MATASS=FACTORISER( reuse=MATASS,  MATR_ASSE=MATASS )

VECASS=RESOUDRE(reuse=VECASS, MATR=MATASS,     CHAM_NO=VECASS)

TEST_RESU(CHAM_NO=_F(CRITERE='RELATIF',
                     NOM_CMP='DY',
                     GROUP_NO='C',
                     PRECISION=0.029999999999999999,
                     CHAM_GD=VECASS,
                     VALE_CALC=-1.78055605E-05,
                     VALE_REFE=-1.8247999999999999E-05,
                     REFERENCE='NON_DEFINI',),
          )

#---------------------------------------------------------------------
FIN()
#
