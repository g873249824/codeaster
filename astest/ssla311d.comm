# ET DONC LA VALEUR A TESTER VAUT 7.37409E-3*0.1=7.37409E-4
#

TEST_TABLE(PRECISION=0.02,
           VALE_CALC=0.000730834528052,
           VALE_REFE=7.3740899999999996E-4,
           REFERENCE='NON_DEFINI',
           NOM_PARA='G',
           TABLE=G0,)

