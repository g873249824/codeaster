DEBUT(CODE=_F(NOM='SSNV104B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',)

MO=AFFE_MODELE(
                           MAILLAGE=MA,
                          AFFE=_F(
                                TOUT = 'OUI',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'AXIS')
