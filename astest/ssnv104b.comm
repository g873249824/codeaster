                     MODELE=MO,DDL_IMPO=(
                     _F(  GROUP_NO = 'SPHSUP',  DY = -2.0),
                              _F(  GROUP_NO = 'SPHINF',  DY = 2.0),
                              _F(  GROUP_NO = 'AXE',     DX = 0.0)),

                   )

CHA2= DEFI_CONTACT(MODELE      = MO,
                   FORMULATION = 'DISCRETE',
                   ZONE =_F(  APPARIEMENT = 'NODAL',
                              NORMALE    = 'ESCL',
                               ALGO_CONT = 'LAGRANGIEN',
                               GROUP_MA_MAIT = 'CON1',
                               GROUP_MA_ESCL = 'CON2'),
                  );


