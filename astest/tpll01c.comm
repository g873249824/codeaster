# CAS TEST TPLL01C

DEBUT(CODE=_F( NOM = 'TPLL01C',NIV_PUB_WEB='INTERNET'))

PHI0=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=0.0         )

PHI1=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=-1200.0     )

COEH=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=30.0        )

T_EX=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=140.0       )

T_IM=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=100.0       )

ACIER=DEFI_MATERIAU( THER=_F(  LAMBDA = 0.75, RHO_CP = 2.0))

MAIL=LIRE_MAILLAGE( )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = 'PLAN',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(  MODELE=MOTH,FLUX_REP=(
          _F( GROUP_MA = 'GRMA13', FLUN = PHI0),
          _F( GROUP_MA = 'GRMA12', FLUN = PHI1)),
           ECHANGE=_F( GROUP_MA = 'GRMA14', COEF_H = COEH, TEMP_EXT = T_EX),
          TEMP_IMPO=_F( GROUP_NO = 'GRNM15', TEMP = T_IM)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,
       INTERVALLE=_F(    JUSQU_A = 10.0,   NOMBRE = 2)
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           TEMP_INIT=_F( STATIONNAIRE = 'OUI'),
          INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH)
             )

T2=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
                   NUME_ORDRE=2)

FLUXN=CALC_CHAM_ELEM(    MODELE=MOTH,  TEMP=T2,
            CHAM_MATER=CHMAT,   OPTION='FLUX_ELNO_TEMP')

FLUXG=CALC_CHAM_ELEM(    MODELE=MOTH,  TEMP=T2,
            CHAM_MATER=CHMAT,   OPTION='FLUX_ELGA_TEMP')

TEST_RESU(CHAM_NO=(
   _F( CHAM_GD = T2, NOEUD = 'N24',  NOM_CMP = 'TEMP', VALE = 100.0),
   _F( CHAM_GD = T2, NOEUD = 'N6',   NOM_CMP = 'TEMP', VALE = 20.0),
   _F( CHAM_GD = T2, NOEUD = 'N14',  NOM_CMP = 'TEMP', VALE = 60.0)),CHAM_ELEM=(
   
   _F( CHAM_GD = FLUXN, MAILLE = 'M1', POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( CHAM_GD = FLUXN, MAILLE = 'M2', POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0),
   _F( CHAM_GD = FLUXG, MAILLE = 'M1', POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( CHAM_GD = FLUXG, MAILLE = 'M2', POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0))
          )

CO=INTE_MAIL_2D(   MAILLAGE=MAIL,
                      DEFI_SEGMENT=_F( ORIGINE = ( 0.04,  0.07, ),
                                    EXTREMITE = ( 0.07,  0.03, ))
                   )

TR1_C=POST_RELEVE_T(    ACTION=_F(  INTITULE = 'FLU_NORM',
                           CHEMIN = CO,
                           CHAM_GD = FLUXN,
                           TRAC_NOR = 'OUI',
                           NOM_CMP = ( 'FLUX', 'FLUY', ),
                           OPERATION = 'EXTRACTION')  )

TEST_TABLE(    TABLE=TR1_C,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 0.00000E+00),
                NOM_PARA='TRAC_NOR',
                    VALE=1200.0
            )

TEST_TABLE(    TABLE=TR1_C,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 2.50000E-02),
                NOM_PARA='TRAC_NOR',
                    VALE=1200.0
            )

TEST_TABLE(    TABLE=TR1_C,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 5.00000E-02),
                NOM_PARA='TRAC_NOR',
                    VALE=1200.0
            )

FIN()
