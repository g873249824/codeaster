# CAS TEST TPLL01C

DEBUT(CODE=_F(NOM='TPLL01C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PHI0=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=0.0         )

PHI1=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=-1200.0     )

COEH=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=30.0        )

T_EX=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=140.0       )

T_IM=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=100.0       )

ACIER=DEFI_MATERIAU( THER=_F(  LAMBDA = 0.75, RHO_CP = 2.0))

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = 'PLAN',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(  MODELE=MOTH,FLUX_REP=(
          _F( GROUP_MA = 'GRMA13', FLUN = PHI0),
          _F( GROUP_MA = 'GRMA12', FLUN = PHI1)),
           ECHANGE=_F( GROUP_MA = 'GRMA14', COEF_H = COEH, TEMP_EXT = T_EX),
          TEMP_IMPO=_F( GROUP_NO = 'GRNM15', TEMP = T_IM)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,
       INTERVALLE=_F(    JUSQU_A = 10.0,   NOMBRE = 2)
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
          INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH)
             )

TEMPE=CALC_CHAMP(reuse=TEMPE,RESULTAT=TEMPE,THERMIQUE=('FLUX_ELNO','FLUX_ELGA'))



TEST_RESU(RESU=(_F(NUME_ORDRE=2,
                   RESULTAT=TEMPE,
                   NOM_CHAM='TEMP',
                   NOEUD='N24',
                   NOM_CMP='TEMP',
                   VALE_CALC=100.0,
                   TOLE_MACHINE=1.E-3,),
                _F(NUME_ORDRE=2,
                   RESULTAT=TEMPE,
                   NOM_CHAM='TEMP',
                   NOEUD='N6',
                   NOM_CMP='TEMP',
                   VALE_CALC=20.0,
                   TOLE_MACHINE=1.E-3,),
                _F(NUME_ORDRE=2,
                   RESULTAT=TEMPE,
                   NOM_CHAM='TEMP',
                   NOEUD='N14',
                   NOM_CMP='TEMP',
                   VALE_CALC=60.0,
                   TOLE_MACHINE=1.E-3,),
                _F(NUME_ORDRE=2,
                   POINT=3,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELNO',
                   NOM_CMP='FLUX',
                   VALE_CALC=960.0,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='M1',),
                _F(NUME_ORDRE=2,
                   POINT=2,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELNO',
                   NOM_CMP='FLUY',
                   VALE_CALC=720.0,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='M2',),
                _F(NUME_ORDRE=2,
                   POINT=3,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELGA',
                   NOM_CMP='FLUX',
                   VALE_CALC=960.0,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='M1',),
                _F(NUME_ORDRE=2,
                   POINT=2,
                   RESULTAT=TEMPE,
                   NOM_CHAM='FLUX_ELGA',
                   NOM_CMP='FLUY',
                   VALE_CALC=720.0,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='M2',),
                ),
          )

CHEM=INTE_MAIL_2D(   MAILLAGE=MAIL,
                      DEFI_SEGMENT=_F( ORIGINE = ( 0.04,  0.07, ),
                                    EXTREMITE = ( 0.07,  0.03, ))
                   )

TR1_C=POST_RELEVE_T(    ACTION=_F(  INTITULE = 'FLU_NORM',
                           CHEMIN = CHEM,
                           RESULTAT = TEMPE,
                           TRAC_NOR = 'OUI',
                           NUME_ORDRE=2,
                           NOM_CHAM='FLUX_ELNO',
                           NOM_CMP = ( 'FLUX', 'FLUY', ),
                           OPERATION = 'EXTRACTION')  )

TEST_TABLE(VALE_CALC=1200.0,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='TRAC_NOR',
           TABLE=TR1_C,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=0.0,),
           )

TEST_TABLE(VALE_CALC=1200.0,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='TRAC_NOR',
           TABLE=TR1_C,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=0.025000000000000001,),
           )

TEST_TABLE(VALE_CALC=1200.0,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='TRAC_NOR',
           TABLE=TR1_C,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=0.050000000000000003,),
           )

FIN()
