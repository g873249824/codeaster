F_NU=DEFI_CONSTANTE(   VALE=0.3)

F_ALPHA=DEFI_CONSTANTE(   VALE=0.)

ACIER=DEFI_MATERIAU(      ELAS_FO=_F(    E = F_E,
                                         NU = F_NU,
                                         ALPHA = F_ALPHA, TEMP_DEF_ALPHA=100.0,),

