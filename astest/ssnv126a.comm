                               PARA=(900., 1000., 1025., 1050.,),
                           NOM_PARA_FONC='X',DEFI_FONCTION=(
                        _F(  PROL_DROITE = 'LINEAIRE',
                                        PROL_GAUCHE = 'LINEAIRE',
                                        VALE = ( .01,    14.355,
                                               100.,   14.855,
                                               200.,   15.355,  )),
                                      _F(  PROL_DROITE = 'LINEAIRE',
                                        PROL_GAUCHE = 'LINEAIRE',
