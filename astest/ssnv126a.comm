                                           M_VP = M_T,
                                           K_VP = K_T,
                                           A_D = A_T,
                                           R_D = R_T,
                                           K_D = KD_T))

MAT=AFFE_MATERIAU(            MAILLAGE=EPROU,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER,
                                           TEMP_REF = 1000.))

