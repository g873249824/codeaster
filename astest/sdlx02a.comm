RIGIDITE=ASSE_MATRICE(  MATR_ELEM=RIGI_ELE,   NUME_DDL=NUME_DDL )

MASSE=ASSE_MATRICE(  MATR_ELEM=MASS_ELE,   NUME_DDL=NUME_DDL )

MASSE_DI=ASSE_MATRICE(  MATR_ELEM=MASS_ELD,   NUME_DDL=NUME_DDL )

