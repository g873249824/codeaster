                    METHODE='TRI_DIAG',
                  CALC_FREQ=_F(  NMAX_FREQ = 9) )

MODE_DIA=NORM_MODE(reuse=MODE_DIA,   MODE=MODE_DIA,   NORME='TRAN_ROTA',)

