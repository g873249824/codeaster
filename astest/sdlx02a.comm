                         OPTION=( 'SIEF_ELGA_DEPL', 'EFGE_ELNO_DEPL', ),
                         CHAM_MATER=CHAMPMAT,   CARA_ELEM=CARA_ELE  )

MODE_DIA=CALC_NO(reuse=MODE_DIA,  RESULTAT=MODE_DIA,
                   MODELE=MODELE,   OPTION='REAC_NODA',
                       CHAM_MATER=CHAMPMAT,   CARA_ELEM=CARA_ELE    )

