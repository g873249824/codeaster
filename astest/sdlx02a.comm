                         CHAM_MATER=CHAMPMAT,   CARA_ELEM=CARA_ELE,
                        MASS_INER=_F(  TOUT = 'OUI')                   )

MODE=MODE_ITER_SIMULT(  MATR_A=RIGIDITE,   MATR_B=MASSE,
                    METHODE='TRI_DIAG',
                  CALC_FREQ=_F(  NMAX_FREQ = 9) )

