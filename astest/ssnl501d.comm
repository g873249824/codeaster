#---------------------------------------------------------------------
#  - CALCUL QUASI-STATIQUE D'UNE POUTRE CONSTITUEE D'UN MATERIAU
#    ELASTIQUE PARFAITEMENT PLASTIQUE
#  - MODELISATION AVEC DES ELEMENTS 'COQUE_3D'(QUAD9)
#---------------------------------------------------------------------
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),);
MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED',);
MAIL_E=CREA_MAILLAGE(MAILLAGE=MAIL,
                     MODI_MAILLE=_F(OPTION='QUAD8_9',
                     GROUP_MA='TOUT_ELT',),);
MAIL_E=DEFI_GROUP(reuse =MAIL_E,
              MAILLAGE=MAIL_E,
              CREA_GROUP_NO=(_F(GROUP_MA='LFE',),),);
MOD_E=AFFE_MODELE(MAILLAGE=MAIL_E,
                  AFFE=_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='COQUE_3D',),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAR_EL_E=AFFE_CARA_ELEM(MODELE=MOD_E,
                        COQUE=_F(EPAIS=0.025,COQUE_NCOU=3,
                                 GROUP_MA='TOUT_ELT',),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT_E=DEFI_MATERIAU(ECRO_LINE=_F(D_SIGM_EPSI=0.0,
                                 SY=2.35000000E8,),
                    ELAS=_F(NU=0.3,
                            E=2.E11,),);
CH_MAT_E=AFFE_MATERIAU(AFFE=_F(MATER=MAT_E,
                               TOUT='OUI',),
                       MAILLAGE=MAIL_E,);
#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------
#
CHAR0=AFFE_CHAR_MECA(MODELE=MOD_E,
                     DDL_IMPO=(_F(GROUP_MA='LAB',
                                  DZ=0.0,
                                  DX=0.0,
                                  DY=0.0,
                                  DRZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,),
                               _F(GROUP_MA='LFE',
                                  DRZ=0.0,),),
                     LIAISON_OBLIQUE=_F(GROUP_NO = 'LFE',
                                        DX = 0.,
                                        ANGL_NAUT = (135., 0., 0.,),),);
PRES_LIM=AFFE_CHAR_MECA(MODELE=MOD_E,
                        FORCE_COQUE=_F(PRES=-2.61111111E5,
                                       TOUT='OUI',),);
#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------
#
LIS_INST=DEFI_LIST_REEL(INTERVALLE=(_F(JUSQU_A=5.0,
                                       NOMBRE=10,),
                                    _F(JUSQU_A=8.5,
                                       NOMBRE=20,),
                                    _F(JUSQU_A=25.0,
                                       NOMBRE=40,),),
                        DEBUT=0.0,);
RESULT_E=STAT_NON_LINE(CHAM_MATER=CH_MAT_E,
                       MODELE=MOD_E,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=40,
                                      RESI_GLOB_RELA=1.E-06,),
               COMPORTEMENT=_F(
                                    ITER_INTE_MAXI=40,
                                    RELATION='VMIS_ISOT_LINE',),
                       INCREMENT=_F(LIST_INST=LIS_INST,),
                       PILOTAGE=_F(TYPE='DDL_IMPO',
                                   GROUP_NO='E',
                                   NOM_CMP='DX',
                                   COEF_MULT=-213.9708),
                       CARA_ELEM=CAR_EL_E,
                       NEWTON=_F(REAC_ITER=1,
                                 MATRICE='TANGENTE',),
                       EXCIT=(_F(CHARGE=CHAR0,),
                              _F(CHARGE=PRES_LIM,
                                 TYPE_CHARGE='FIXE_PILO',),),);
TABLE_DX=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='TAB_DX',
                                 GROUP_NO='E',
                                 NOM_CMP='DX',
                                 RESULTAT=RESULT_E,
                                 NOM_CHAM='DEPL',
                                 TOUT_ORDRE='OUI',),);
IMPR_TABLE(TABLE=TABLE_DX,);

TABLE_DY=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='TAB_DY',
                                 GROUP_NO='E',
                                 NOM_CMP='DY',
                                 RESULTAT=RESULT_E,
                                 NOM_CHAM='DEPL',
                                 TOUT_ORDRE='OUI',),);
IMPR_TABLE(TABLE=TABLE_DY,);

DEPX=RECU_FONCTION(RESULTAT=RESULT_E,
                   GROUP_NO='E',
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   TOUT_ORDRE='OUI',);
DEPY=RECU_FONCTION(RESULTAT=RESULT_E,
                   GROUP_NO='E',
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   TOUT_ORDRE='OUI',);
ETA=RECU_FONCTION(RESULTAT=RESULT_E,
                  NOM_PARA_RESU='ETA_PILOTAGE',
                  TOUT_ORDRE='OUI',);
IMPR_FONCTION(COURBE=_F(FONCTION=DEPX,),
            UNITE=25, UNITE_DIGR=26,
            FORMAT='AGRAF',
            TITRE='Depx en fonction du temps',);
IMPR_FONCTION(COURBE=_F(FONCTION=ETA,),
            UNITE=27, UNITE_DIGR=28,
            FORMAT='AGRAF',
            TITRE='Eta en fonction du temps',);
IMPR_FONCTION(COURBE=_F(FONCTION=DEPY,),
            UNITE=29, UNITE_DIGR=30,
            FORMAT='AGRAF',
            TITRE='Depy en fonction du temps',);
DEPL=CREA_CHAMP(NOM_CHAM='DEPL',
                TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                NUME_ORDRE=1,
                RESULTAT=RESULT_E,);
#
#---------------------------------------------------------------------
#                        VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(_F(NUME_ORDRE=53,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESULT_E,
                   VALE_CALC=1.108221921,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.16,),
                _F(NUME_ORDRE=63,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESULT_E,
                   VALE_CALC=1.108446309,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.16,),
                ),
          )

FIN();
