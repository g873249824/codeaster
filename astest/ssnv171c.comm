# TEST 1-B (A PARTIR DE LA SD RESULTAT ISSUE DE LIRE_RESU)

DEFI_FICHIER(UNITE=81,);

IMPR_RESU(FORMAT='MED',
          UNITE=81,
          RESU=_F(RESULTAT=MONO1IMP,),);

MONO2IMP=LIRE_RESU(TYPE_RESU='EVOL_NOLI',
                   FORMAT='MED',
                   MODELE=TROISD,
                   COMP_INCR=_F(RELATION='MONOCRISTAL',
                                COMPOR=COMPORT,),
                   CHAM_MATER=MAT,
                   CARA_ELEM=ORIEN1,
                   UNITE=81,
                   FORMAT_MED=(
                   _F(NOM_CHAM='DEPL',
                      NOM_CHAM_MED='MONO1IMPDEPL',),
                   _F(NOM_CHAM='SIEF_ELGA',
                      NOM_CHAM_MED='MONO1IMPSIEF_ELGA',),
                   _F(NOM_CHAM='EPSG_ELGA',
                      NOM_CHAM_MED='MONO1IMPEPSG_ELGA',),
                   _F(NOM_CHAM='VARI_ELGA',
                      NOM_CHAM_MED='MONO1IMPVARI_ELGA',
                     ),
                  ),
                   TOUT_ORDRE='OUI',);

AE3=POST_ELEM(WEIBULL=_F(TOUT='OUI',
                         OPTION='SIGM_ELGA',
                         CORR_PLAST='OUI',),
              RESULTAT=MONO2IMP,
              TOUT_ORDRE='OUI',);

AE3_2=POST_ELEM(WEIBULL=_F(TOUT='OUI',
                         OPTION='SIGM_ELMOY',
                         CORR_PLAST='OUI',),
              RESULTAT=MONO2IMP,
              TOUT_ORDRE='OUI',);

AE3_3=POST_ELEM(WEIBULL=_F(TOUT='OUI',
                         OPTION='SIGM_ELMOY',
                         CORR_PLAST='NON',),
              RESULTAT=MONO2IMP,
              TOUT_ORDRE='OUI',);


TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=278.610,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='SIGMA_WEIBULL',
           TABLE=AE3,
           FILTRE=(_F(NOM_PARA='NUME_ORDRE',
                      VALE_I=100,),
                   _F(NOM_PARA='LIEU',
                      VALE_K='CUBE',),
                   ),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=278.610,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='SIGMA_WEIBULL',
           TABLE=AE3_2,
           FILTRE=(_F(NOM_PARA='NUME_ORDRE',
                      VALE_I=100,),
                   _F(NOM_PARA='LIEU',
                      VALE_K='CUBE',),
                   ),
           )

TEST_TABLE(CRITERE='ABSOLU',
