                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V43',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=4.89624E-04,
                   PRECISION=(0.01,1e-06,),
                   REFERENCE='NON_REGRESSION',),
                   ),);


# TEST DE WEIBULL

AE2=POST_ELEM(RESULTAT=SOLNL,
