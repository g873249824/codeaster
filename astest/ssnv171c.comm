DEBUT(CODE=_F(NOM='SSNV171C',NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'));

CUBE=LIRE_MAILLAGE();

TROISD=AFFE_MODELE(MAILLAGE=CUBE,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);
TFIN = 1.5;


LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.87,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=TFIN,
                                    NOMBRE=100,),),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=145200.0,
                            NU=0.3,),
                    WEIBULL=_F(M=24.,
                               VOLU_REFE=1.E-3,
                               SIGM_REFE=2630.0E0,),
                    MONO_VISC1=_F(N=10.0,
                                  K=40.0,
                                  C=10.0,),
                    MONO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H=0.0,),
                    MONO_CINE1=_F(D=36.68,),);

COEF=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                         1.0,1.0,
                         ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',);

MAT=AFFE_MATERIAU(MAILLAGE=CUBE,
                  AFFE=_F(TOUT='OUI',
                          MATER=ACIER,),);

# ATTENTION PAR RAPPORT AUX MODELISATION A ET B, ON EST EN TRACTION
# AU LIEU DE COMPRESSION, POUR POUVOIR TESTER WEIBULL

TRAC=AFFE_CHAR_MECA(MODELE=TROISD,
                    DDL_IMPO=(_F(NOEUD='NO4',
                                 DX=0.0,
                                 DY=0.0,),
                              _F(NOEUD='NO8',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,),
                              _F(NOEUD='NO2',
                                 DX=0.0,),
                              _F(NOEUD='NO6',
                                 DX=0.0,),),
                    FORCE_NODALE=(_F(NOEUD=('NO3','NO7',),
                                     FX=-35.0,),
                                  _F(NOEUD=('NO1','NO5',),
                                     FX=-35.0,),),);

COMPORT=DEFI_COMPOR(MONOCRISTAL=_F(MATER=ACIER,
                                   ECOULEMENT='MONO_VISC1',
                                   ECRO_ISOT='MONO_ISOT1',
                                   ECRO_CINE='MONO_CINE1',
                                   ELAS='ELAS',
                                   FAMI_SYST_GLIS='OCTAEDRIQUE',),);

ORIEN1=AFFE_CARA_ELEM(MODELE=TROISD,
                      MASSIF=_F(GROUP_MA='TOUT',
                                ANGL_REP=(30.0,0.0,0.0,),),);

MONOEXPL=STAT_NON_LINE(MODELE=TROISD,
                       CHAM_MATER=MAT,
                       CARA_ELEM=ORIEN1,
                       EXCIT=_F(CHARGE=TRAC,
                                FONC_MULT=COEF,
                                TYPE_CHARGE='FIXE_CSTE',),
                       COMP_INCR=_F(RELATION='MONOCRISTAL',
                                    COMPOR=COMPORT,
                                    ITER_INTE_MAXI=100,
                                    ALGO_INTE='RUNGE_KUTTA',),
                       INCREMENT=_F(LIST_INST=LINST,),
                       CONVERGENCE=_F(ITER_GLOB_MAXI=100,),
                       NEWTON=_F(PREDICTION='EXTRAPOLE',
                                 MATRICE='ELASTIQUE',
                                 REAC_ITER=0,),);

MONOEXPL=CALC_CHAMP(reuse =MONOEXPL,
                    RESULTAT=MONOEXPL,
                    DEFORMATION=('EPSI_ELGA','EPSP_ELGA',),);

TEST_RESU(RESU=(_F(INST=1.5,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   RESULTAT=MONOEXPL,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC=209.999826985,
                   VALE_REFE=210.0,
                   MAILLE='MA1',),
                _F(INST=1.5,
                   POINT=1,
                   RESULTAT=MONOEXPL,
                   NOM_CHAM='EPSI_ELGA',
                   NOM_CMP='EPXX',
                   VALE_CALC=1.88744E-3,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='MA1',),
                _F(INST=1.5,
                   POINT=1,
                   RESULTAT=MONOEXPL,
                   NOM_CHAM='EPSI_ELGA',
                   NOM_CMP='EPYY',
                   VALE_CALC=-5.021232E-4,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='MA1',),
                _F(INST=1.5,
                   POINT=1,
                   RESULTAT=MONOEXPL,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPYY',
                   VALE_CALC=-6.8238999999999995E-05,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='MA1',),
                _F(INST=1.5,
                   POINT=1,
                   RESULTAT=MONOEXPL,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V56',
                   VALE_CALC=4.8136700000000001E-4,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='MA1',),
                ),
          )

MONO1IMP=STAT_NON_LINE(MODELE=TROISD,
                       CHAM_MATER=MAT,
                       CARA_ELEM=ORIEN1,
                       EXCIT=_F(CHARGE=TRAC,
                                FONC_MULT=COEF,
                                TYPE_CHARGE='FIXE_CSTE',),
                       COMP_INCR=_F(RELATION='MONOCRISTAL',
                                    COMPOR=COMPORT,
                                    ITER_INTE_MAXI=100,
                                    ITER_INTE_PAS=4,
                                    ALGO_INTE='NEWTON',),
                       INCREMENT=_F(LIST_INST=LINST,),
                       CONVERGENCE=_F(ITER_GLOB_MAXI=100,),
                       NEWTON=_F(REAC_ITER=1,),);

MONO1IMP=CALC_CHAMP(reuse =MONO1IMP,
                    RESULTAT=MONO1IMP,
                    DEFORMATION=('EPSI_ELGA','EPSP_ELGA','EPSG_ELGA',),);

TEST_RESU(RESU=(_F(INST=1.5,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   RESULTAT=MONO1IMP,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC=209.999870818,
                   VALE_REFE=210.0,
                   MAILLE='MA1',),
                _F(INST=1.5,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=MONO1IMP,
                   NOM_CHAM='EPSI_ELGA',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.89495026E-03,
                   VALE_REFE=1.8873842500000001E-3,
                   PRECISION=5.0000000000000001E-3, TOLE_MACHINE=(5.0000000000000001E-3, 9.9999999999999995E-07),
                   MAILLE='MA1',),
                _F(INST=1.5,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=MONO1IMP,
                   NOM_CHAM='EPSI_ELGA',
                   NOM_CMP='EPYY',
                   VALE_CALC=-5.03293675E-04,
                   VALE_REFE=-5.021232E-4,
                   PRECISION=3.0000000000000001E-3, TOLE_MACHINE=(3.0000000000000001E-3, 9.9999999999999995E-07),
                   MAILLE='MA1',),
                _F(INST=1.5,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=MONO1IMP,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPYY',
                   VALE_CALC=-6.94095567E-05,
                   VALE_REFE=-6.8239150000000005E-05,
                   PRECISION=0.02, TOLE_MACHINE=(0.02, 9.9999999999999995E-07),
                   MAILLE='MA1',),
                _F(INST=1.5,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=MONO1IMP,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V56',
                   VALE_CALC= 4.89623679E-04,
                   VALE_REFE=4.8136750000000002E-4,
                   PRECISION=0.02, TOLE_MACHINE=(0.02, 9.9999999999999995E-07),
                   MAILLE='MA1',),
                ),
          )

# TESTS DE WEIBULL
# ---------------
# TEST 1-A (A PARTIR DE LA SD RESULTAT ISSUE DE STAT_NON_LINE)

AE2=POST_ELEM(WEIBULL=_F(TOUT='OUI',
                         OPTION='SIGM_ELGA',
                         CORR_PLAST='OUI',),
              RESULTAT=MONO1IMP,
              TOUT_ORDRE='OUI',);

