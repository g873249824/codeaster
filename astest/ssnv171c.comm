FIN()

IMPR_RESU(MODELE=TROISD,FORMAT='RESULTAT',
          RESU=_F(
                  MAILLAGE=CUBE,
                  RESULTAT=SOLNL,
                  NOM_CHAM='VARI_ELGA',
                  INST = TFIN, 
   ),);

