DEBUT(CODE=_F(NOM='SSNV171C',
              NIV_PUB_WEB='INTERNET',),);

CUBE=LIRE_MAILLAGE();

TROISD=AFFE_MODELE(MAILLAGE=CUBE,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);
TFIN = 1.5;


LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.87,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=TFIN,
                                    NOMBRE=100,),),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=145200.0,
                            NU=0.3,),
                    ECOU_VISC1=_F(N=10.0,
                                  K=40.0,
                                  C=10.0,),
                    ECRO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H=0.0,),
                    ECRO_CINE1=_F(D=36.68,),
                    WEIBULL=_F( M = 24., 
                                VOLU_REFE = 1.E-3, 
                                SIGM_REFE = 2630.0E0)
                    );

COEF=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.0,),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);

MAT=AFFE_MATERIAU(MAILLAGE=CUBE,
                  AFFE=_F(TOUT='OUI',
                          MATER=ACIER,),);

# ATTENTION PAR RAPPORT AUX MODELISATION A ET B, ON EST EN TRACTION
# AU LIEU DE COMPRESSION, POUR POUVOIR TESTER WEIBULL

TRAC=AFFE_CHAR_MECA(MODELE=TROISD,
                    DDL_IMPO=(_F(NOEUD='NO4',
                                 DX=0.0,
                                 DY=0.0,),
                              _F(NOEUD='NO8',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,),
                              _F(NOEUD='NO2',
                                 DX=0.0,),
                              _F(NOEUD='NO6',
                                 DX=0.0,),),
                    FORCE_NODALE=(_F(NOEUD=('NO3','NO7',),
                                     FX=-35.0,),
                                  _F(NOEUD=('NO1','NO5',),
                                     FX=-35.0,),),);

COMPORT=DEFI_COMPOR(MONOCRISTAL=(_F(MATER=ACIER,
                                    ECOULEMENT='ECOU_VISC1',
                                    ECRO_ISOT='ECRO_ISOT1',
                                    ECRO_CINE='ECRO_CINE1',
                                    ELAS='ELAS',
                                    FAMI_SYST_GLIS='OCTAEDRIQUE',),),);


#  TST POLY1
ORIEN1=AFFE_CARA_ELEM(MODELE=TROISD,
                     MASSIF=_F(GROUP_MA='TOUT',
                               ANGL_REP=(30.0,0.0,0.0,),),);

COMPORP1=DEFI_COMPOR(POLYCRISTAL=(
                               _F(MONOCRISTAL=COMPORT,
                                  FRAC_VOL=1.0,
                                  ANGL_REP=(0.,0.,0.)),),
                    LOCALISATION='BETA',DA=0.,DL=0.,
                    );

#  TST POLY2
ORIEN2=AFFE_CARA_ELEM(MODELE=TROISD,
                     MASSIF=_F(GROUP_MA='TOUT',
                               ANGL_REP=(15.,0.,0.,),),);

COMPORP2=DEFI_COMPOR(POLYCRISTAL=(
                               _F(MONOCRISTAL=COMPORT,
                                  FRAC_VOL=1.0,
                                  ANGL_REP=(15.,0.,0.)),),
                    LOCALISATION='BETA',DA=0.,DL=0.,
                    );

SOLNL=STAT_NON_LINE(MODELE=TROISD,
                    CHAM_MATER=MAT,
                    CARA_ELEM=ORIEN1,
                    EXCIT=(_F(CHARGE=TRAC,
                              FONC_MULT=COEF,
                              TYPE_CHARGE='FIXE_CSTE',),),
                    COMP_INCR=(_F(RELATION='MONOCRISTAL',
                                  COMPOR=COMPORT,
                                  ITER_INTE_MAXI=100,
                                  ITER_INTE_PAS=4,
                                  RESO_INTE='IMPLICITE',),),
                    INCREMENT=(_F(LIST_INST=LINST,
                                  SUBD_PAS=4,
                                  SUBD_PAS_MINI=1e-06,),),
                    NEWTON=(_F(REAC_ITER=1,),),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=100),),);

SOLNL=CALC_ELEM(reuse =SOLNL,
                RESULTAT=SOLNL,
                OPTION=('EPSI_ELGA_DEPL','EPSP_ELGA','EPSG_ELGA_DEPL'),);

TEST_RESU(RESU=(_F(RESULTAT=SOLNL,
