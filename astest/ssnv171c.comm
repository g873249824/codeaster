DEBUT(CODE=_F(NOM='SSNV171C',NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'));



ACIER=DEFI_MATERIAU(ELAS=_F(E=145200.0,
                            NU=0.3,),
                    WEIBULL=_F(M=24.,
                               VOLU_REFE=1.E-3,
                               SIGM_REFE=2630.0E0,),
                    MONO_VISC1=_F(N=10.0,
                                  K=40.0,
                                  C=1.0,),
                    MONO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H=0.0,),
                    MONO_CINE1=_F(D=36.68,),);


COMPORT=DEFI_COMPOR(MONOCRISTAL=_F(MATER=ACIER,
                                   ECOULEMENT='MONO_VISC1',
                                   ECRO_ISOT='MONO_ISOT1',
                                   ECRO_CINE='MONO_CINE1',
                                   ELAS='ELAS',
                                   FAMI_SYST_GLIS='OCTAEDRIQUE',),);

COMPORP=DEFI_COMPOR(POLYCRISTAL=_F(MONOCRISTAL=COMPORT,
                                   FRAC_VOL=1.0,
                                   ANGL_REP=(15.,0.,0.,),),
                    LOCALISATION='BETA',
                    DL=0.,
                    DA=0.,);
                    
CHARGE=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,140.0,),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);

TFIN = 1.5;


LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.87,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=TFIN,
                                    NOMBRE=300,),),);
                                    
DEFLIST1 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = LINST),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS=10,
                                 SUBD_NIVEAU=10),)

MONOEXPL=SIMU_POINT_MAT(INFO=1,
                   COMP_INCR=_F(RELATION='MONOCRISTAL',
                                COMPOR=COMPORT,
                                ALGO_INTE='RUNGE_KUTTA',),
                   MASSIF=_F(ANGL_REP=30.0),
                   ANGLE=0.,
                   NEWTON=_F(PREDICTION='EXTRAPOLE',MATRICE='ELASTIQUE'),
                   MATER = ACIER,  
                   INCREMENT=_F(LIST_INST=DEFLIST1),
                   SIGM_IMPOSE=_F(SIXX=CHARGE),
                        );  

# reference : ssnv171a

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=210.0,
           VALE_REFE=210.0,
           NOM_PARA='SIXX',
           TABLE=MONOEXPL,
           FILTRE=_F(PRECISION=1.E-06,
                     NOM_PARA='INST',
                     VALE=1.5,),
           )

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_REFE=1.8913169223994E-03,
           VALE_CALC=1.8794771952458E-03,
           PRECISION=7.E-3,
           NOM_PARA='EPXX',
           TABLE=MONOEXPL,
           FILTRE=_F(PRECISION=1.E-06,
                     NOM_PARA='INST',
                     VALE=1.5,),
           )

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_REFE=-5.0273159559248E-04,
           VALE_CALC=-5.0089998342754E-04 ,
           PRECISION=4.E-3,
           NOM_PARA='EPYY',
           TABLE=MONOEXPL,
           FILTRE=_F(PRECISION=1.E-06,
                     NOM_PARA='INST',
                     VALE=1.5,),
           )


POLYEXPL=SIMU_POINT_MAT(INFO=1,
                   COMP_INCR=_F(RELATION='POLYCRISTAL',
                                COMPOR=COMPORP,
                                ALGO_INTE='RUNGE_KUTTA',),
                   MASSIF=_F(ANGL_REP=15.0),
                   ANGLE=0.,
                   NEWTON=_F(PREDICTION='EXTRAPOLE',MATRICE='ELASTIQUE'),
                   MATER = ACIER,  
                   INCREMENT=_F(LIST_INST=DEFLIST1),
                   SIGM_IMPOSE=_F(SIXX=CHARGE),
                        );  


TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=210.0,
           VALE_REFE=210.0,
           NOM_PARA='SIXX',
           TABLE=POLYEXPL,
           FILTRE=_F(PRECISION=1.E-06,
                     NOM_PARA='INST',
                     VALE=1.5,),
           )

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_REFE=1.8913169223994E-03,
           VALE_CALC=1.8794771952458E-03,
           PRECISION=7.E-3,
           NOM_PARA='EPXX',
           TABLE=POLYEXPL,
           FILTRE=_F(PRECISION=1.E-06,
                     NOM_PARA='INST',
                     VALE=1.5,),
           )

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_REFE=-5.0273159559248E-04,
           VALE_CALC=-5.0089998342754E-04 ,
           PRECISION=4.E-3,
           NOM_PARA='EPYY',
           TABLE=POLYEXPL,
           FILTRE=_F(PRECISION=1.E-06,
                     NOM_PARA='INST',
                     VALE=1.5,),
           )


FIN();
