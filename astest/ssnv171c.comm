                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=4.48670E-04,
                   PRECISION=(0.01,1e-06,),REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=SOLNL,
                   INST=TFIN,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPYY',
