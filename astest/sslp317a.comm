DEBUT(CODE=_F(NOM='SSLP317A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'));

# nbre de raffinement
nb_raff = 3          

# initialisation
MA    = [None]*(nb_raff+1)
MO    = [None]*(nb_raff+1)
CHERR = [None]*(nb_raff+1)
FISS1 = [None]*(nb_raff+1)
FISS2 = [None]*(nb_raff+1)
FISS3 = [None]*(nb_raff+1)
FISS4 = [None]*(nb_raff+1)

PRE_GIBI();

r2 = 0.05

num_calc=0   

MA[num_calc]=LIRE_MAILLAGE();

for num_calc in range(nb_raff+1) :

   MO[num_calc]=AFFE_MODELE(MAILLAGE=MA[num_calc],
                            AFFE=(_F(GROUP_MA=('SURF','LIG2','LIG3','LIG4'),
                                     PHENOMENE='MECANIQUE',
                                     MODELISATION='D_PLAN',),),);

   # entaille
   FISS1[num_calc]=DEFI_FISS_XFEM(MODELE=MO[num_calc],  
                                  TYPE_DISCONTINUITE='INTERFACE', 
                                  DEFI_FISS=_F(FORM_FISS     = 'ENTAILLE',
                                               DEMI_LONGUEUR = 0.3,
                                               RAYON_CONGE   = 0.05,
                                               CENTRE        = (0., -0.3, 0.),
                                               VECT_X        = (1., 0., 0.),
                                               VECT_Y        = (0., 1., 0.),
                                               ))

   # inclusion circulaire
   FISS2[num_calc]=DEFI_FISS_XFEM(MODELE=MO[num_calc],  
                                  TYPE_DISCONTINUITE='INTERFACE',
                                  DEFI_FISS=_F(FORM_FISS      = 'ELLIPSE',
                                               DEMI_GRAND_AXE = r2,
                                               DEMI_PETIT_AXE = r2,
                                               CENTRE         = (0., 0., 0.),
                                               VECT_X         = (1., 0., 0. ),
                                               VECT_Y         = (0., 1., 0. ),
                                               ),)

   FISS3[num_calc]=DEFI_FISS_XFEM(MODELE=MO[num_calc],  
                                  DEFI_FISS=_F(FORM_FISS  = 'SEGMENT',
                                               PFON_ORIG  = (0.2, 0.25, 0.),
                                               PFON_EXTR  = (0.4, 0.25, 0.),))


   FISS4[num_calc]=DEFI_FISS_XFEM(MODELE=MO[num_calc],  
                                  DEFI_FISS=_F(FORM_FISS  = 'SEGMENT',
                                               PFON_ORIG  = (-0.3, 0.25, 0.),
                                               PFON_EXTR  = (-0.1, 0.25, 0.),))


   # on ne raffine pas la derniere iteration
   if num_calc < nb_raff :

      CHERR[num_calc] = RAFF_XFEM(FISSURE=(FISS1[num_calc],FISS2[num_calc],FISS3[num_calc],FISS4[num_calc]),)

      # SUBTILITE MACRO_COMMANDE VIS A VIS DES ENTREES  
      MA[num_calc+1]=CO('MA_%d' % (num_calc+1))

      MACR_ADAP_MAIL(ADAPTATION='RAFFINEMENT',
                  CHAM_GD = CHERR[num_calc],
                  CRIT_RAFF_PE = 0.2,
                  USAGE_CMP = 'RELATIF',
                  MAILLAGE_N = MA[num_calc],
                  MAILLAGE_NP1 = MA[num_calc+1],
                  ELEMENTS_NON_HOMARD='IGNORER')


# definition group de noeuds pour bloquer l'inclusion centrale
MA[nb_raff]=DEFI_GROUP( reuse =MA[nb_raff],
                        MAILLAGE=MA[nb_raff],
                        CREA_GROUP_NO=_F(NOM='NC',OPTION='ENV_SPHERE',POINT=(0.,0.),PRECISION=1/50.,RAYON=1/50.),
                        INFO=2 )
       
# definition group de noeuds pour bloquer l'entaille
MA[nb_raff]=DEFI_GROUP( reuse =MA[nb_raff],
                        MAILLAGE=MA[nb_raff],
                        CREA_GROUP_NO=_F(NOM='NE1',OPTION='ENV_SPHERE',POINT=(0.,-0.3),PRECISION=1/200.,RAYON=1/200.),
                        INFO=2 )

MA[nb_raff]=DEFI_GROUP( reuse =MA[nb_raff],
                        MAILLAGE=MA[nb_raff],
                        CREA_GROUP_NO=_F(NOM='NE2',OPTION='ENV_SPHERE',POINT=(0.1,-0.3),PRECISION=1/200.,RAYON=1/200.),
                        INFO=2 )

                              
# impression du dernier maillage
IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(MAILLAGE=MA[nb_raff]));

#*********************************************************************
#                          CALCUL                                    *
#*********************************************************************

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO[nb_raff],
                         FISSURE=(FISS1[nb_raff],FISS2[nb_raff],FISS3[nb_raff],FISS4[nb_raff]),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=205000000000.0,
                            NU=0.3),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MA[nb_raff],
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA='SURF',MATER=ACIER,),
                       );

CHARBLO=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=_F(GROUP_MA='LIG1',
                                   DX=0.0,
                                   DY=0.0,),
                       );

# pour bloquer les modes rigides de l'inclusion centrale et de l'entaille
CHARINC=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=_F(GROUP_NO=('NC','NE1','NE2'),
                                   DX=0.0,DY=10.0,),
                       );

CHPRES=AFFE_CHAR_MECA(MODELE=MODELEK,
                      PRES_REP=_F(GROUP_MA=('LIG2','LIG3','LIG4'),PRES=-1.e6,),);

RESU=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CHARBLO),
                          _F(CHARGE=CHARINC),
                          _F(CHARGE=CHPRES))
                   )

#---------------POST--------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU = MOD_VISU,
                        RESULTAT    = RESU)


IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(RESULTAT=RES_XFEM,NOM_CHAM='DEPL',TYPE_CHAM='VECT_2D',NOM_CMP=('DX','DY')),);

#---------------TEST--------------------------------------------

MA[nb_raff]=DEFI_GROUP(reuse=MA[nb_raff],
                       MAILLAGE=MA[nb_raff],
                       CREA_GROUP_NO=_F(GROUP_MA='SURF'))

UN=FORMULE(NOM_PARA=('X','Y'),VALE='1.');         

GEOM=CREA_CHAMP(TYPE_CHAM='NOEU_GEOM_R',
                OPERATION='EXTR',
                MAILLAGE=MA[nb_raff],
                NOM_CHAM='GEOMETRIE',);  

CHF=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_F',
               OPERATION='AFFE',
               MAILLAGE=MA[nb_raff],
               AFFE=_F(TOUT='OUI',
                       NOM_CMP='X1',
                       VALE_F=UN));

CHUN=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_R',
                OPERATION='EVAL',
                CHAM_F=CHF,
                CHAM_PARA=GEOM);
                    
TAB=POST_RELEVE_T(TITRE='TAB',
                  ACTION=_F(INTITULE='TAB',
                            NOM_CMP='X1',
                            GROUP_NO='SURF',
                            CHAM_GD=CHUN,
                            OPERATION='EXTRACTION'),);

# NOMBRE DE NOEUDS DU MAILLAGE FINAL
nbno = 3128

TEST_TABLE(TABLE=TAB,
           TYPE_TEST='SOMM',
           NOM_PARA='X1',
           VALE=nbno,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='NON_REGRESSION',
           VERSION='11.00.24')

FIN();
