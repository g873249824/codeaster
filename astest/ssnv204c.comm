DEBUT(CODE=_F(NOM='SSNV204C',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

# modules mecaniques [kPa]
K=516.2E6
G=238.2E6
# =>
YOUNG = 9.*K*G /(3.*K+G)
POISSON = (3.*K-2.*G) /(6.*K+2.*G)

print 'young=',YOUNG,' /// nu=',POISSON

# pression de preconsolidation [en kPa]
P0=100000.


#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************
MAILLAGE=LIRE_MAILLAGE(FORMAT='MED', );

MODELE=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=_F(TOUT         = 'OUI',
                           PHENOMENE    = 'MECANIQUE',
                           MODELISATION = '3D'));

MAILLAGE=MODI_MAILLAGE(reuse =MAILLAGE,
                       MAILLAGE=MAILLAGE,
                       ORIE_PEAU_3D=_F(GROUP_MA=('DEVANT','DROIT','ARRIERE','GAUCHE','BAS','HAUT','APPUI','COTE')),
                       INFO=1);


#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************
TEMPS0=DEFI_LIST_REEL(DEBUT=-10.,
                      INTERVALLE=(_F(JUSQU_A = 0., NOMBRE = 1),
                                  _F(JUSQU_A =  10., NOMBRE = 200)));


#***********************************************************************
#    MATERIAU : SOL
#***********************************************************************
SOL0=DEFI_MATERIAU(ELAS=_F(E = YOUNG, NU = POISSON),
                  HUJEUX=_F(N        = 0.4,
                            BETA     = 24.,
                            D        = 2.5,
                            B        = 0.2,
                            PHI      = 33.,
                            ANGDIL   = 33.,
                            PCO      = -1.E6,
                            PREF     = -1.E6,
                            AMON     = 0.008,
                            ACYC     = 0.0001,
                            CMON     = 0.2,
                            CCYC     = 0.1,
                            RD_ELA   = 0.005,
                            RI_ELA   = 0.001,
                            RHYS     = 0.05,
                            RMOB     = 0.9,
                            XM       = 1.,
                            RD_CYC   = 0.005,
                            RI_CYC   = 0.001,
                            DILA     = 1.0,));

SOL=DEFI_MATERIAU(ELAS=_F(E = YOUNG, NU = POISSON),
                  HUJEUX=_F(N        = 0.4,
                            BETA     = 24.,
                            D        = 2.5,
                            B        = 0.2,
                            PHI      = 33.,
                            ANGDIL   = 33.,
                            PCO      = -1.E6,
                            PREF     = -1.E6,
                            AMON     = 0.008,
                            ACYC     = 0.0001,
                            CMON     = 0.2,
                            CCYC     = 0.1,
                            RD_ELA   = 0.005,
                            RI_ELA   = 0.001,
                            RHYS     = 0.05,
                            RMOB     = 0.9,
                            XM       = 1.,
                            RD_CYC   = 0.005,
                            RI_CYC   = 0.001,
                            DILA     = 1.0,));

CHMAT0=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                     AFFE=_F(TOUT='OUI', MATER=SOL0));

CHMAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                    AFFE=_F(TOUT='OUI', MATER=SOL));


#***********************************************************************
#    CHARGEMENT MECANIQUE : FORCE DE COMPRESSION + DDL IMPOSES
#***********************************************************************
CHAR_U1=AFFE_CHAR_MECA(MODELE=MODELE,
                       PRES_REP=_F(GROUP_MA='COTE',
                                   PRES = 1.));

CHAR_U2=AFFE_CHAR_MECA(MODELE=MODELE,
                       PRES_REP=_F(GROUP_MA='APPUI',
                                   PRES = 1.));

CHAR_U3=AFFE_CHAR_MECA(MODELE=MODELE,
                       FACE_IMPO=(_F(GROUP_MA='HAUT',DZ = 1.),
                                  _F(GROUP_MA='DROIT',DX = 1.),
                                  _F(GROUP_MA='ARRIERE',DY = 1.),),);


DEPL_1=AFFE_CHAR_MECA(MODELE=MODELE,
                FACE_IMPO=(_F(GROUP_MA='BAS',DZ = 0.),
                           _F(GROUP_MA='GAUCHE',DX = 0.),
                           _F(GROUP_MA='DEVANT',DY = 0.)));

COEF0=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_DROITE='CONSTANT',
                    VALE=(-10., 0.0,
                           0., P0 ));

#***********************************************************************
#    PRECONSOLIDATION ELASTIQUE JUSQU'A P0 (100 kPa)
#***********************************************************************
U0M=STAT_NON_LINE(MODELE=MODELE,
                 CHAM_MATER=CHMAT0,
                 EXCIT=(_F(CHARGE    = CHAR_U1,
                           FONC_MULT = COEF0),
                        _F(CHARGE    = CHAR_U2,
                           FONC_MULT = COEF0),
                        _F(CHARGE    = DEPL_1)),
                 COMPORTEMENT=_F(RELATION = 'ELAS'),
                 NEWTON=_F(MATRICE = 'ELASTIQUE'),
                 CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                ITER_GLOB_MAXI = 20),
                 INCREMENT=_F(LIST_INST = TEMPS0, INST_FIN = 0.));
# ----------- FIN

SIG0=CREA_CHAMP(INFO=2,
                TYPE_CHAM='ELGA_SIEF_R',
                OPERATION='EXTR',
                RESULTAT=U0M,
                NOM_CHAM='SIEF_ELGA',
                INST=0.0,);

COEF1=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_DROITE='CONSTANT',
                    VALE=( 0.0, 0.0,
                           5.0, 0.005,
                           10.0, -0.005,));

DEFLIST0 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = TEMPS0),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS=10,
                                 SUBD_PAS_MINI =0.00001 ),)


#***********************************************************************
#    CONSOLIDATION PLASTIQUE DRAINE JUSQU'A 300KPA
#***********************************************************************
U0=STAT_NON_LINE(MODELE=MODELE,
                 CHAM_MATER=CHMAT,
                 EXCIT=(_F(CHARGE    = CHAR_U3,
                           TYPE_CHARGE ='DIDI',
                           FONC_MULT = COEF1),
                        _F(CHARGE    = DEPL_1)),
                 ETAT_INIT=_F(SIGM=SIG0),
                 COMPORTEMENT=_F(RELATION = 'HUJEUX',
                              ALGO_INTE='NEWTON_RELI',
                              RESI_INTE_RELA = 1.E-8,
                              ITER_INTE_PAS = 10),
                 NEWTON=_F(MATRICE = 'TANGENTE',
                           REAC_ITER=1),
                 CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                ITER_GLOB_MAXI = 20),

                 ARCHIVAGE=_F(LIST_INST = TEMPS0),
                 INCREMENT=_F(LIST_INST = DEFLIST0,
                              INST_INIT = 0.0,
                              INST_FIN =  10));

#*******************************************************************************
#                     POST-TRAITEMENT des RESULTATS
#*******************************************************************************

#*******************************************************************************
#                                     U0
#*******************************************************************************

U0=CALC_CHAMP(reuse=U0,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO'),RESULTAT=U0)


U0=CALC_CHAMP(reuse=U0,

           CONTRAINTE='SIGM_NOEU',VARI_INTERNE='VARI_NOEU',DEFORMATION='EPSI_NOEU',
           RESULTAT=U0);

TAB_SIG=POST_RELEVE_T(ACTION=_F(INTITULE='SIGM_NOEU',
                                GROUP_NO='C',
                                RESULTAT=U0,
                                NOM_CHAM='SIGM_NOEU',
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));

TAB_VAR=POST_RELEVE_T(ACTION=_F(INTITULE='VARI_NOEU',
                                GROUP_NO='C',
                                RESULTAT=U0,
                                NOM_CHAM='VARI_NOEU',
                                NOM_CMP=('V4','V8','V23','V32'),
                                OPERATION='EXTRACTION'));

TAB_EPSZ=POST_RELEVE_T(ACTION=_F(INTITULE='EPSI_NOEU',
                                GROUP_NO='C',
                                RESULTAT=U0,
                                NOM_CHAM='EPSI_NOEU',
                                NOM_CMP='EPZZ',
                                OPERATION='EXTRACTION'));

TAB_EPSV=POST_RELEVE_T(ACTION=_F(INTITULE='EPSI_VOLU',
                                GROUP_NO='C',
                                RESULTAT=U0,
                                NOM_CHAM='EPSI_NOEU',
                                INVARIANT='OUI',
                                OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=TAB_SIG);
IMPR_TABLE(TABLE=TAB_EPSV);
IMPR_TABLE(TABLE=TAB_VAR);

####################################################
# CONTROLE DE NON-REGRESSION DU CODE
####################################################

###################################################
#
# PENDANT L'ETAT DE TRACTION DU MATERIAU
#
###################################################

# VALEUR DU RAYON ISOTROPE CYCLIQUE

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=1.E-2,
           VALE_CALC=0.039999043861827,
           VALE_REFE=0.039998499999999999,
           NOM_PARA='V8',
           TABLE=TAB_VAR,
           FILTRE=_F(CRITERE='ABSOLU',
                     PRECISION=9.9999999999999995E-07,
                     NOM_PARA='INST',
                     VALE=3.0,),
           )

# VALEUR DE LA DEFORMATION VOLUMIQUE PLASTIQUE

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=1.E-2,
           VALE_CALC= 7.4287794545865E-03,
           VALE_REFE=7.4295300000000002E-3,
           NOM_PARA='V23',
           TABLE=TAB_VAR,
           FILTRE=_F(CRITERE='ABSOLU',
                     PRECISION=9.9999999999999995E-07,
                     NOM_PARA='INST',
                     VALE=3.0,),
           )

# VALEUR DES CONTRAINTES PRINCIPALES

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=1.E-2,
           VALE_CALC=-2.000000000,
           VALE_REFE=-2.0,
           NOM_PARA='SIXX',
           TABLE=TAB_SIG,
           FILTRE=_F(CRITERE='ABSOLU',
                     PRECISION=9.9999999999999995E-07,
                     NOM_PARA='INST',
                     VALE=3.0,),
           )

###################################################
#
# APRES RECHARGEMENT ISOTROPE DU MATERIAU
#
###################################################

# VALEUR DU RAYON ISOTROPE MONOTONE

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=1.E-2,
           VALE_CALC=0.10945942352451,
           VALE_REFE=0.10943,
           NOM_PARA='V4',
           TABLE=TAB_VAR,
           FILTRE=_F(CRITERE='ABSOLU',
                     PRECISION=9.9999999999999995E-07,
                     NOM_PARA='INST',
                     VALE=10.0,),
           )

# VALEUR DE LA DEFORMATION VOLUMIQUE PLASTIQUE

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.029999999999999999,
           VALE_CALC=-0.020576043409169,
           VALE_REFE=-0.020572,
           NOM_PARA='V23',
           TABLE=TAB_VAR,
           FILTRE=_F(CRITERE='ABSOLU',
                     PRECISION=9.9999999999999995E-07,
                     NOM_PARA='INST',
                     VALE=10.0,),
           )

# VALEUR DES CONTRAINTES PRINCIPALES

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.02,
           VALE_CALC=-4.4839282637925E+05,
           VALE_REFE=-4.48200E5,
           NOM_PARA='SIXX',
           TABLE=TAB_SIG,
           FILTRE=_F(CRITERE='ABSOLU',
                     PRECISION=9.9999999999999995E-07,
                     NOM_PARA='INST',
                     VALE=10.0,),
           )

FIN();
