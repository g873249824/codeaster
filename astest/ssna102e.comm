# REFERENCE : CALCULS AVEC ABAQUS, SYSTUS, SAMCEF

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
# --------------------------------------------------------------------
#       LECTURE DU MAILLAGE ET CREATION DE GROUPES DE NOEUDS
# --------------------------------------------------------------------

MAIL=LIRE_MAILLAGE(FORMAT='MED',);
#

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='LCD1',),
                               _F(GROUP_MA='LBC4',),
                               _F(GROUP_MA='LAB1',),
                               _F(GROUP_MA='CEP2',),
                               _F(GROUP_MA='CME2',),
                               _F(GROUP_MA='CEP3',),
                               _F(GROUP_MA='CME3',),
                               _F(GROUP_MA='LCD4',),
                               _F(GROUP_MA='LCD4I',),
                               _F(DIFFE=('LCD4I','PC4',),
                                  NOM='LCD4P',),),);
#
# --------------------------------------------------------------------
#                   MODELISATION 2D AXISYMETRIQUE
# --------------------------------------------------------------------

MOD=AFFE_MODELE(MAILLAGE=MAIL,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='AXIS',),);
#
# --------------------------------------------------------------------
#         DEFINITION ET AFFECTATION D'UN MATERIAU ELASTIQUE
# --------------------------------------------------------------------

ELAS=DEFI_MATERIAU(ELAS=_F(E=200000.,
                           NU=0.3,),);
#

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ELAS,),);
#
# --------------------------------------------------------------------
#                   ENCASTREMENT DE LA LIGNE LBC4
# --------------------------------------------------------------------

BLOQ=AFFE_CHAR_MECA(MODELE=MOD,
                    DDL_IMPO=_F(GROUP_NO='LBC4',
                                DX=0.0,
                                DY=0.0,),);
#
# --------------------------------------------------------------------
#               DEPLACEMENT IMPOSE SUR LA LIGNE LCD1
# --------------------------------------------------------------------

DEPL=AFFE_CHAR_MECA(MODELE=MOD,
                    DDL_IMPO=_F(GROUP_NO='LCD1',
                                DY=-2.0,),);
#
# --------------------------------------------------------------------
#     SUPPRESSION DES MOUVEMENTS DE CORPS RIGIDE DES DEUX SPHERES
# --------------------------------------------------------------------

LIAI=AFFE_CHAR_MECA(MODELE=MOD,
                    LIAISON_DDL=(_F(GROUP_NO=('PA1','PP2',),
                                    DDL=('DY','DY',),
                                    COEF_MULT=(1.,-1.,),
                                    COEF_IMPO=0.,),
                                 _F(GROUP_NO=('PM3','PD4',),
                                    DDL=('DY','DY',),
                                    COEF_MULT=(1.,-1.,),
                                    COEF_IMPO=0.,),),);
CONT = DEFI_CONTACT(MODELE        = MOD,
                    FORMULATION   = 'DISCRETE',
                    ITER_GCP_MAXI = 500,
                    PRE_COND      = 'DIRICHLET',
                    RESI_ABSO     = 1.E-9,
                    # NE PAS MODIFIER PARAMETRES REAC_GEOM: TEST BUG FICHE 19580
                    REAC_GEOM     = 'CONTROLE',
                    NB_ITER_GEOM  = 10,
                    # Le preconditionneur ne sera active que lorsque
                    # le jeu initial aura ete divise par 1000
                    COEF_RESI     = 1.E-3,
                    ZONE=(_F(APPARIEMENT='MAIT_ESCL',
                             ALGO_CONT='GCP',
                             GROUP_MA_MAIT='LAB1',
                             GROUP_MA_ESCL='CEP2',
                             SANS_GROUP_NO=('PP2',),),
                          _F(APPARIEMENT='MAIT_ESCL',
                             ALGO_CONT='GCP',
                             GROUP_MA_MAIT='CME2',
                             GROUP_MA_ESCL='CEP3',),
                          _F(APPARIEMENT='MAIT_ESCL',
                             ALGO_CONT='GCP',
                             GROUP_MA_MAIT='CME3',
                             GROUP_MA_ESCL='LCD4',
                             SANS_GROUP_NO=('PD4',),),
                          ),
                     );

#
# --------------------------------------------------------------------
#         DECOUPAGE EN INCREMENTS DE CHARGE (1 EN ELASTICITE)
#         ET RAMPE MULTIPLICATRICE POUR LE DEPLACEMENT IMPOSE
# --------------------------------------------------------------------

FONC=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.,0.,1.,1.,),);
#

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.,
                                   NOMBRE=1,),);
#
# --------------------------------------------------------------------
#                            CALCUL NON LINEAIRE
# --------------------------------------------------------------------

RESU=STAT_NON_LINE(SOLVEUR=_F(),
                   MODELE=MOD,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=BLOQ,),
                          _F(CHARGE=DEPL,
                             FONC_MULT=FONC,),
                          _F(CHARGE=LIAI,),),
                   CONTACT  = CONT,
                   COMPORTEMENT=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=LINST,),
                   NEWTON=_F(MATRICE='ELASTIQUE',),
                   CONVERGENCE=_F(RESI_GLOB_MAXI=1.E-6,
# NE PAS MODIFIER: TEST BUG FICHE 19580
                                  ITER_GLOB_MAXI=3,
                                  ),
                   INFO=1,);
#
# --------------------------------------------------------------------
#                    CALCUL DES CONTRAINTES AUX NOEUDS
# --------------------------------------------------------------------
#

RESU=CALC_CHAMP(reuse =RESU,
               RESULTAT=RESU,
                CONTRAINTE=('SIGM_ELNO'),);

RESU=CALC_CHAMP(reuse =RESU,
             RESULTAT=RESU,
             CONTRAINTE='SIGM_NOEU',);
#
# --------------------------------------------------------------------
#          CALCUL DES FORCES NODALES ET DES REACTIONS NODALES
# --------------------------------------------------------------------

RESU=CALC_CHAMP(reuse =RESU,
             RESULTAT=RESU,
             FORCE='REAC_NODA',);
#
# --------------------------------------------------------------------
#                COMPARAISON SOURCE EXTERNE
# --------------------------------------------------------------------

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='PA4',
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.84140095839,
                  VALE_REFE=-0.81000000000000005,
                  CRITERE='RELATIF',
                  PRECISION=0.040000000000000001,),
          )

#
# --------------------------------------------------------------------
#                CALCUL DE LA REACTION A L'ENCASTREMENT
# --------------------------------------------------------------------
#                COMPARAISON SOURCE EXTERNE
# --------------------------------------------------------------------

REACENC=POST_RELEVE_T(ACTION=_F(INTITULE='ENCASTREMENT',
                                GROUP_NO='LBC4',
                                RESULTAT=RESU,
                                NOM_CHAM='REAC_NODA',
                                INST=1.,
                                RESULTANTE=('DX','DY',),
                                REPERE='GLOBAL',
                                OPERATION='EXTRACTION',),);
#

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.016,
           VALE_CALC=17501.9327491,
           VALE_REFE=1.7229580000000002E4,
           NOM_PARA='DY',
           TABLE=REACENC,)


FIN();
#
