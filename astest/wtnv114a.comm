   D_PERM_LIQU_SATU = ZERO,
   PERM_GAZ = PERMGAZ,
   D_PERM_SATU_GAZ = DPERGSA,
   D_PERM_PRES_GAZ = DPERGPG,
   FICK = FICK,
   D_FICK_TEMP = DFICKTE,
   D_FICK_GAZ_PRES = DFICKPG,
   LAMBDA = UN,
   D_LAMBDA_TEMP = ZERO),
                              THM_INIT=_F( 
   TEMP = 273.,
   PRE1 = 0.,
   PRE2 = 1.E5,
   PORO = 0.4,
   PRES_VAPE = 1.),
                                     ELAS=_F( 
   E = 225.E6,
   NU = 0.,
   RHO = 2000.,
   ALPHA = 8.E-6)
   )

CHMAT0=AFFE_MATERIAU(   MAILLAGE=MAIL,
                          AFFE=_F( TOUT = 'OUI',
                        MATER = MATERIAU))

CH_FLU1=AFFE_CHAR_MECA(   MODELE=MODELE,FLUX_THM_REP=(
                          _F( 
                                  GROUP_MA = 'HAUT',
                                  FLUN_HYDR1 = 0.005),
                          _F( 
                                  GROUP_MA = 'BAS',
                                  FLUN_HYDR1 = 0.))
                                   )

CHARHO=AFFE_CHAR_MECA(   MODELE=MODELE,
                          DDL_IMPO=_F(  TOUT = 'OUI',
                                     DX = 0.,
                                     DY = 0.)
                                   )

RESU0=STAT_NON_LINE( 
                            MODELE=MODELE,
                           SOLVEUR=_F( 
                METHODE = 'LDLT'),
                        CHAM_MATER=CHMAT0,EXCIT=(
                             _F( 
                CHARGE = CHARHO),
                             _F( 
                CHARGE = CH_FLU1)),
                         COMP_INCR=_F( 
                RELATION = 'KIT_HM',
                RELATION_KIT = (
                  'ELAS',       'LIQU_SATU',
                  'HYDR_UTIL',       )),
                         INCREMENT=_F( 
                LIST_INST = LI),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 121),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-8,
                ITER_GLOB_MAXI = 120,
                ITER_INTE_MAXI = 5),
                        PARM_THETA=1.,
                         ARCHIVAGE=_F( 
                LIST_INST = ARCHI)
              )

IMPR_RESU(  MODELE=MODELE,
