   RHO = 1000.,
   UN_SUR_K = 3.77E-9,
   ALPHA = 1.E-4,
   CP = 2.85E6,
   VISC = VISCOLIQ, 
   D_VISC_TEMP = DVISCOL,
   LAMBDA = UN,
   D_LAMBDA_TEMP = ZERO),
                              THM_GAZ=_F( 
   MASS_MOL = 28.96E-3,
   CP = 1000.,
   VISC = UN,
   D_VISC_TEMP = ZERO,
   LAMBDA = UN,
   D_LAMBDA_TEMP = ZERO),                     
                              THM_DIFFU=_F( 
   R_GAZ = 8.315,
