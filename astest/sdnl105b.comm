                                        MODE_MECA = MODES,
                                        NMAX_MODE = 10)
                             )

#
MACRO2=MACR_ELEM_DYNA(  BASE_MODALE=BAMO2, )

#
###
###### MODELE GENERALISE :
###
#
MODEGE=DEFI_MODELE_GENE( SOUS_STRUC=(_F( NOM = 'BARRE1',
                                         MACR_ELEM_DYNA = MACRO1,
                                         ANGL_NAUT = (0., 0., 0.,),
                                         TRANS=(0.,0.,0.),),
                             _F( NOM = 'BARRE2',
                                         MACR_ELEM_DYNA = MACRO2,
                                         ANGL_NAUT = (0., 0., 0.,),
                                         TRANS=(0.,0.2,0.),),
                             _F( NOM = 'BARRE3',
                                         MACR_ELEM_DYNA = MACRO1,
                                         TRANS=(0.,0.4,0.),
