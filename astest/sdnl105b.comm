                  MATR_MASS=M_GENE,
                  CALC_FREQ=_F(NMAX_FREQ=15,
                               ),
                  )


#
SQUEL=DEFI_SQUELETTE(  MODELE_GENE=MODEGE,SOUS_STRUC=(
                            _F( NOM = 'BARRE1',  TOUT = 'OUI'),
                            _F( NOM = 'BARRE2',  TOUT = 'OUI'),
                            _F( NOM = 'BARRE3',  TOUT = 'OUI')))

