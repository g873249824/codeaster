# LA METHODE SOUS_POINT DE PROJ_CHAMP EST TESTEE POUR LES 3 CAS SUIVANTS :
# 1/ PROJECTION D'UN CHAMP AUX NOEUD (NOEU) ISOLE
# 2/ PROJECTION D'UN CHAMP AUX NOEUD PAR ELEMENT (ELNO) ISOLE
# 3/ PROJECTION D'UN RESULTAT DE TYPE EVOL_THER COMPOSE
#    DES CHAMPS TEMP ET HYDR_ELNO

# ELEMENT DKT TRIANGULAIRE


# MAILLAGE ET MODELE THERMIQUE 3D
# -------------------------------
MAIL=LIRE_MAILLAGE()

MOTHER=AFFE_MODELE(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI', PHENOMENE='THERMIQUE', MODELISATION='3D',),
                  );

# DEFINITION DES FONCTIONS DE TEMPERATURE ET D'HYDRATATION
# --------------------------------------------------------

FTEMP = FORMULE(NOM_PARA=('X','Y','Z','INST'),VALE='2.*X +3.*Y +4.*Z +5.*INST')
FHYDR = FORMULE(NOM_PARA=('X','Y','Z','INST'),VALE='-2.*X -3.*Y -4.*Z -5.*INST')


# CONSTRUCTION DES CHAMPS DE GEOMETRIE
# ------------------------------------

CHXN=CREA_CHAMP(OPERATION='EXTR',
                TYPE_CHAM='NOEU_GEOM_R',
                NOM_CHAM='GEOMETRIE',
                MAILLAGE=MAIL,
                )

CHXL=CREA_CHAMP(OPERATION='DISC',
                TYPE_CHAM='ELNO_GEOM_R',
                MODELE=MOTHER,
                CHAM_GD=CHXN,)


# AFFECTATION DES FONCTIONS
# -------------------------


TEMP_F=CREA_CHAMP(OPERATION='AFFE',
                 TYPE_CHAM='NOEU_NEUT_F',
                 MAILLAGE=MAIL,
                 AFFE=_F( TOUT = 'OUI',
                          NOM_CMP = ('X1',),
                          VALE_F = (FTEMP,)
                         )
                )

HYDR_F=CREA_CHAMP(OPERATION='AFFE',
                 TYPE_CHAM='ELNO_NEUT_F',
                 MODELE=MOTHER,
                 PROL_ZERO='OUI',
                 AFFE=_F( TOUT = 'OUI',
                          NOM_CMP = ('X1',),
                          VALE_F = (FHYDR,)
                         )
                )

# CONSTRUCTION DU MODELE MECANIQUE A SOUS-POINTS
# ----------------------------------------------


MACOQ=LIRE_MAILLAGE(UNITE=19)

MOCOQ=AFFE_MODELE(MAILLAGE=MACOQ,
                  AFFE=_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='DKT',),
                  );


COQCA=AFFE_CARA_ELEM( MODELE=MOCOQ,
                        INFO=1,
                        COQUE=_F(  GROUP_MA=('COQUE'),
                                   COQUE_NCOU = 2,
                                   EPAIS = 1.0,
                                    ),
                    );



# PROJECTION
# ----------

# 1/ PROJECTION D'UN CHAMP AUX NOEUD (NOEU) ISOLE

INSTN0=CREA_CHAMP(OPERATION='AFFE',
                TYPE_CHAM='NOEU_INST_R',
                MAILLAGE=MAIL,
                AFFE=_F( TOUT = 'OUI',
                            NOM_CMP = ('INST',),
                            VALE =0.0,
                        )
                )

TEMP0= CREA_CHAMP(OPERATION='EVAL',
                 TYPE_CHAM='NOEU_NEUT_R',
                 CHAM_F=TEMP_F,
                 CHAM_PARA=( CHXN,INSTN0)
                 )

TEMP1=CREA_CHAMP(OPERATION='ASSE',
                TYPE_CHAM='NOEU_TEMP_R',
                MAILLAGE=MAIL,
                ASSE=_F( TOUT = 'OUI',
                        CHAM_GD = TEMP0,
                        NOM_CMP = ('X1',),
                        NOM_CMP_RESU = ('TEMP',),
                    )
                )

# FONCTIONS POUR TESTER TRANSF_GEOM_1/2
FX = FORMULE(NOM_PARA=('X','Y'),VALE=' 2.*X + 3.*Y + 4.')
FY = FORMULE(NOM_PARA=('X','Y'),VALE='-3.*X + 2.*Y + 7.')
FZ = FORMULE(NOM_PARA=('Z'),VALE='Z+1')


TEMP_SP=PROJ_CHAMP(PROJECTION='OUI',METHODE='SOUS_POINT',
                   TRANSF_GEOM_1=(FX,FY,FZ),
                   TRANSF_GEOM_2=(FX,FY,FZ),
                   MAILLAGE_1 = MAIL,
                   MODELE_2   = MOCOQ,
                   CHAM_GD    = TEMP1,
                   CARA_ELEM  = COQCA,
                   )

# 2/ PROJECTION D'UN CHAMP AUX NOEUD PAR ELEMENT (ELNO) ISOLE


INSTL0=CREA_CHAMP(OPERATION='AFFE',
                TYPE_CHAM='ELNO_INST_R',
                MODELE=MOTHER,
                AFFE=_F( TOUT = 'OUI',
                            NOM_CMP = ('INST',),
                            VALE =0.0,
                        )
                )

HYDR0= CREA_CHAMP(OPERATION='EVAL',
                  TYPE_CHAM='ELNO_NEUT_R',
                  CHAM_F=HYDR_F,
                  CHAM_PARA=( CHXL,INSTL0)
                 )

HYDR1=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='ELNO_HYDR_R',
                    MODELE=MOTHER,
                    ASSE=_F( TOUT = 'OUI',
                            CHAM_GD = HYDR0,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('HYDR',),
                        )
                )


HYDR_SP=PROJ_CHAMP(PROJECTION='OUI',METHODE='SOUS_POINT',
                   MAILLAGE_1 = MAIL,
                   MODELE_2   = MOCOQ,
                   CHAM_GD    = HYDR1,
                   CARA_ELEM  = COQCA,
                   )

# 3/ PROJECTION D'UN RESULTAT DE TYPE EVOL_THER COMPOSE
#    DES CHAMPS TEMP ET HYDR_ELNO

l_inst=[0.0,1.0,2.0,]


TEMP={}
HYDR={}
l_affe_temp=[]
l_affe_hydr=[]

for i,inst in enumerate(l_inst):


    INSTN=CREA_CHAMP(OPERATION='AFFE',
                TYPE_CHAM='NOEU_INST_R',
                MAILLAGE=MAIL,
                AFFE=_F( TOUT = 'OUI',
                            NOM_CMP = ('INST',),
                            VALE =inst,
                        )
                )

    INSTL=CREA_CHAMP(OPERATION='AFFE',
                TYPE_CHAM='ELNO_INST_R',
                MODELE=MOTHER,
                AFFE=_F( TOUT = 'OUI',
                            NOM_CMP = ('INST',),
                            VALE = inst,
                        )
                )


    TEMP2=CREA_CHAMP(OPERATION='EVAL',
                    TYPE_CHAM='NOEU_NEUT_R',
                    CHAM_F=TEMP_F,
                    CHAM_PARA=( CHXN,INSTN,)   )

    HYDR2=CREA_CHAMP(OPERATION='EVAL',
                    TYPE_CHAM='ELNO_NEUT_R',
                    CHAM_F=HYDR_F,
                    CHAM_PARA=( CHXL,INSTL,)   )

    TEMP[i]=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='NOEU_TEMP_R',
                    MAILLAGE=MAIL,
                    ASSE=_F( TOUT = 'OUI',
                            CHAM_GD = TEMP2,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('TEMP',),
                        )
                )
    HYDR[i]=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='ELNO_HYDR_R',
                    MODELE=MOTHER,
                    ASSE=_F( TOUT = 'OUI',
                            CHAM_GD = HYDR2,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('HYDR',),
                        )
                )
    DETRUIRE(CONCEPT=_F(NOM=(TEMP2,HYDR2,INSTN,INSTL)))

    l_affe_temp.append(_F( CHAM_GD = TEMP[i],  MODELE = MOTHER, INST = inst,))
    l_affe_hydr.append(_F( CHAM_GD = HYDR[i],  MODELE = MOTHER, INST = inst,))



RESUTHER = CREA_RESU( OPERATION = 'AFFE',
                      TYPE_RESU = 'EVOL_THER',
                      NOM_CHAM  = 'TEMP',
                      AFFE = l_affe_temp )

RESUTHER = CREA_RESU(reuse=RESUTHER,
                     OPERATION = 'AFFE',
                     TYPE_RESU = 'EVOL_THER',
                     NOM_CHAM  = 'HYDR_ELNO',
                     AFFE = l_affe_hydr )


RESU_SP=PROJ_CHAMP(PROJECTION='OUI',METHODE='SOUS_POINT',
                   MODELE_1 = MOTHER,
                   MODELE_2   = MOCOQ,
                   RESULTAT=RESUTHER,
                   CARA_ELEM=COQCA,
                   )

#IMPR_RESU(RESU=_F(RESULTAT=RESU_SP,))


#TEST_RESU

# 1/

TEST_RESU(CHAM_ELEM=(_F(REFERENCE='ANALYTIQUE',
                        POINT=1,
                        NOM_CMP='TEMP',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=4,
                        MAILLE='TRIA1',
                        CHAM_GD=TEMP_SP,
                        VALE_CALC=4.833333333,
                        VALE_REFE=4.8333333333333304,),
                     _F(REFERENCE='ANALYTIQUE',
                        POINT=3,
                        NOM_CMP='TEMP',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=1,
                        MAILLE='TRIA4',
                        CHAM_GD=TEMP_SP,
                        VALE_CALC=3.166666667,
                        VALE_REFE=3.1666666666666701,),
                     _F(REFERENCE='ANALYTIQUE',
                        POINT=5,
                        NOM_CMP='TEMP',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=2,
                        MAILLE='TRIA6',
                        CHAM_GD=TEMP_SP,
                        VALE_CALC=10.000000000,
                        VALE_REFE=10.0,),
                     _F(REFERENCE='ANALYTIQUE',
                        POINT=7,
                        NOM_CMP='TEMP',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=5,
                        MAILLE='TRIA7',
                        CHAM_GD=TEMP_SP,
                        VALE_CALC=11.666666667,
                        VALE_REFE=11.6666666666667,),
                     ),
          )

# 2/


TEST_RESU(CHAM_ELEM=(_F(REFERENCE='ANALYTIQUE',
                        POINT=1,
                        NOM_CMP='HYDR',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=4,
                        MAILLE='TRIA1',
                        CHAM_GD=HYDR_SP,
                        VALE_CALC=-4.833333333,
                        VALE_REFE=-4.8333333333333304,),
                     _F(REFERENCE='ANALYTIQUE',
                        POINT=3,
                        NOM_CMP='HYDR',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=1,
                        MAILLE='TRIA4',
                        CHAM_GD=HYDR_SP,
                        VALE_CALC=-3.166666667,
                        VALE_REFE=-3.1666666666666701,),
                     _F(REFERENCE='ANALYTIQUE',
                        POINT=5,
                        NOM_CMP='HYDR',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=2,
                        MAILLE='TRIA6',
                        CHAM_GD=HYDR_SP,
                        VALE_CALC=-10.000000000,
                        VALE_REFE=-10.0,),
                     _F(REFERENCE='ANALYTIQUE',
                        POINT=7,
                        NOM_CMP='HYDR',
                        PRECISION=9.9999999999999995E-07,
                        SOUS_POINT=5,
                        MAILLE='TRIA7',
                        CHAM_GD=HYDR_SP,
                        VALE_CALC=-11.666666667,
                        VALE_REFE=-11.6666666666667,),
                     ),
          )

# 3/

TEST_RESU(RESU=(_F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   SOUS_POINT=4,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=4.833333333,
                   VALE_REFE=4.8333333333333304,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA1',),
                _F(INST=1.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=3,
                   SOUS_POINT=1,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=8.166666667,
                   VALE_REFE=8.1666666666666696,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA4',),
                _F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=5,
                   SOUS_POINT=2,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=20.000000000,
                   VALE_REFE=20.0,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA6',),
                _F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=7,
                   SOUS_POINT=5,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   VALE_CALC=21.666666667,
                   VALE_REFE=21.6666666666667,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA7',),
                ),
          )

TEST_RESU(RESU=(_F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   SOUS_POINT=4,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='HYDR_ELNO',
                   NOM_CMP='HYDR',
                   VALE_CALC=-4.833333333,
                   VALE_REFE=-4.8333333333333304,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA1',),
                _F(INST=1.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=3,
                   SOUS_POINT=1,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='HYDR_ELNO',
                   NOM_CMP='HYDR',
                   VALE_CALC=-8.166666667,
                   VALE_REFE=-8.1666666666666696,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA4',),
                _F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=5,
                   SOUS_POINT=2,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='HYDR_ELNO',
                   NOM_CMP='HYDR',
                   VALE_CALC=-20.000000000,
                   VALE_REFE=-20.0,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA6',),
                _F(INST=2.0,
                   REFERENCE='ANALYTIQUE',
                   POINT=7,
                   SOUS_POINT=5,
                   RESULTAT=RESU_SP,
                   NOM_CHAM='HYDR_ELNO',
                   NOM_CMP='HYDR',
                   VALE_CALC=-21.666666667,
                   VALE_REFE=-21.6666666666667,
                   PRECISION=9.9999999999999995E-07,
                   MAILLE='TRIA7',),
                ),
          )

FIN()
