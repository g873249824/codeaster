DEBUT( CODE=_F(  NOM = 'SSNV189C',NIV_PUB_WEB='INTERNET') );


maillage=LIRE_MAILLAGE(VERI_MAIL=_F(APLAT=1.E-3,),);

maillage=DEFI_GROUP(reuse =maillage,
                    MAILLAGE=maillage,
                    CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

modmeca=AFFE_MODELE(MAILLAGE=maillage,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='C_PLAN',),);

mat=DEFI_MATERIAU(ELAS_HYPER=_F(C10=2.668,
                                C01=0.271,
                                C20=0.466,
                                K=2939,
                                RHO=1000.0),);

chmat=AFFE_MATERIAU(MAILLAGE=maillage,
                    AFFE=_F(TOUT='OUI',
                            MATER=mat,),);

encastr=AFFE_CHAR_MECA(MODELE=modmeca,
                       DDL_IMPO=(_F(GROUP_MA='L4',
                                    DX=0,
                                    DY=0,),
                                 _F(GROUP_MA='L2',
                                    DX=0,
                                    DY=50,),),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=10,),);

l=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,0.0,1.0,1.0,),);

RESU=STAT_NON_LINE(MODELE=modmeca,
                   CHAM_MATER=chmat,
                   EXCIT=_F(CHARGE=encastr,
                            FONC_MULT=l,),
                   COMP_INCR=_F(RELATION='ELAS_HYPER',
                                DEFORMATION='GROT_GDEP',
                                ITER_INTE_MAXI=100,
                                TOUT='OUI',),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=1.0,),
                   ARCHIVAGE=_F(LIST_INST=L_INST,
                                ARCH_ETAT_INIT='OUI',),);

RESU=CALC_ELEM(reuse =RESU,


               RESULTAT=RESU,
               OPTION='SIEF_ELNO_ELGA');

IMPR_RESU(FORMAT='GMSH',
          UNITE=37,
          RESU=_F(MAILLAGE=maillage,
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',),);

IMPR_RESU(MODELE=modmeca,
          FORMAT='GMSH',
          UNITE=37,
          RESU=_F(RESULTAT=RESU,
                  NOM_CHAM='SIEF_ELNO_ELGA',
                  NOM_CMP=('SIXX','SIYY',),),);


RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION='FORC_NODA',

             );

LH=INTE_MAIL_2D(MAILLAGE=maillage,
                DEFI_CHEMIN=_F(GROUP_MA='L2',),);

TAB_FORC=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                                 CHEMIN=LH,
                                 RESULTAT=RESU,
                                 NOM_CHAM='FORC_NODA',
                                 LIST_INST=L_INST,
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TAB_FORC)

FYA=RECU_FONCTION(TABLE=TAB_FORC,
                  PARA_X='INST',
                  PARA_Y='DY',);

DYA=RECU_FONCTION(RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  NOEUD='N1',);

IMPR_FONCTION(FORMAT='XMGRACE',
              COURBE=_F(FONC_X=DYA,
                        FONC_Y=FYA,),);

TEST_FONCTION(VALEUR=(
_F(FONCTION=FYA,
   REFERENCE='SOURCE_EXTERNE',
   VALE_PARA = 1.0,
   VALE_REFE=181.327,
   PRECISION=0.0003,
   CRITERE='RELATIF',),
            ),)

TEST_FONCTION(VALEUR=(
_F(FONCTION  = FYA,
   REFERENCE = 'NON_REGRESSION',
   VERSION   = '8.2.9',
   VALE_PARA = 1.0,
   VALE_REFE = 181.36489292729,
   PRECISION = 1E-10,
   CRITERE   = 'RELATIF',),
            ),)

FIN();
