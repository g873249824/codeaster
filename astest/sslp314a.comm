#
# MAILLAGE G1, MATERIAU M1
#

DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED', )

MA=DEFI_GROUP( reuse=MA,        MAILLAGE=MA,
                     CREA_GROUP_NO=_F(  GROUP_MA = ( 'LPB1PA1',  'LPA2PB2',
                                                  'LPA1PA',   'LPBPB1',
                                                  'LPAPA2',   'LPB2PB', ))
                 )

MO=AFFE_MODELE(     MAILLAGE=MA,
                          AFFE=_F(  TOUT = 'OUI',
                                 PHENOMENE = 'MECANIQUE',
                                 MODELISATION = 'C_PLAN')
                  )

MAT1=DEFI_MATERIAU(    ELAS=_F(  E = 2.E12,
                                NU = 0.3,
                                ALPHA = 0.)
                          )

MAT2=DEFI_MATERIAU(    ELAS=_F(  E = 8.E12,
                                NU = 0.3,
                                ALPHA = 0.)
                          )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,AFFE=(
                              _F(  GROUP_MA = 'VTOT1',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'LPB1PA1',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'LPA1PA',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'LPBPB1',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'LEVR_SUP',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'VTOT2',
                                     MATER = MAT2),
                              _F(  GROUP_MA = 'LPA2PB2',
                                     MATER = MAT2),
                              _F(  GROUP_MA = 'LPAPA2',
                                     MATER = MAT2),
                              _F(  GROUP_MA = 'LPB2PB',
                                     MATER = MAT2),
                              _F(  GROUP_MA = 'LEVR_INF',
                                     MATER = MAT2),
                                     ),
                         )

CH=AFFE_CHAR_MECA(          MODELE=MO,DDL_IMPO=(
                            _F(  GROUP_NO = 'LPA1PA',
                                       DX = 0.),
                                     _F(  GROUP_NO = 'LPAPA2',
                                       DX = 0.),
                                     _F(  GROUP_NO = 'PA',
                                       DY = 0.)),LIAISON_UNIF=(
                        _F(  GROUP_NO = 'LPBPB1',
                                       DDL = 'DX'),
                                     _F(  GROUP_NO = 'LPB2PB',
                                       DDL = 'DX')),FORCE_CONTOUR=(
                        _F(  GROUP_MA = 'LPB1PA1',
                                        FY = 75.E6),
                                      _F(  GROUP_MA = 'LPA2PB2',
                                        FY = -75.E6))
                    )

CHAMDEPL=MECA_STATIQUE(        MODELE=MO,
                              CHAM_MATER=CHMAT,
                                   EXCIT=_F( CHARGE = CH)
                          )

FOND1=DEFI_FOND_FISS(    MAILLAGE=MA, SYME='NON',
                            FOND_FISS=_F( GROUP_NO = ('P0',)),
                            LEVRE_SUP=_F(GROUP_MA = 'LEVR_SUP',),
                            LEVRE_INF=_F(GROUP_MA = 'LEVR_INF',),                           
                      )


G01=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(
                            R_INF=3.57125E-4,
                            FOND_FISS=FOND1,
                            MODULE=1.,
                            R_SUP=7.14250E-4,
                            ),
                     )

GK01=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F(
                                     R_INF = 3.57125E-4,
                                     R_SUP = 7.14250E-4,
                                     FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )


G02=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(
                            R_INF=7.1425E-4,
                            FOND_FISS=FOND1,
                            MODULE=1.,
                            R_SUP=1.071375E-3,
                            ),
                     )

GK02=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F(   R_INF = 7.1425E-4,
                               R_SUP = 1.071375E-3,
                               FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )


G03=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(
                            R_INF=1.071375E-3,
                            FOND_FISS=FOND1,
                            MODULE=1.,
                            R_SUP=7.1425E-3,
                            ),
                     )

GK03=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F(  R_INF = 1.071375E-3,
                              R_SUP = 7.1425E-3,
                              FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )


G04=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(
                            R_INF=3.57125E-4,
                            FOND_FISS=FOND1,
                            MODULE=1.,
                            R_SUP=7.1425E-3,
                            ),
                     )

GK04=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F( R_INF = 3.57125E-4,
                             R_SUP = 7.1425E-3,
                             FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )

#
# COMPARAISON AVEC LES VALEURS ISSUES DE CASTEM 2000
#
# Une solution analytique existe ( cf MURAKAMI case 8.20)

TEST_TABLE(PRECISION=1.E-2,
           VALE_CALC=86.0390553292,
           VALE_REFE=85.459999999999994,
           REFERENCE='SOURCE_EXTERNE',
           NOM_PARA='G',
           TABLE=G01,)

