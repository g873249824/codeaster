           TABLE=GK02,)

TEST_TABLE(PRECISION=5.0000000000000001E-3,
           VALE_CALC=85.1591436395,
           VALE_REFE=85.459999999999994,
           REFERENCE='SOURCE_EXTERNE',
           NOM_PARA='G_IRWIN',
           TABLE=GK03,)

TEST_TABLE(PRECISION=5.0000000000000001E-3,
           VALE_CALC=85.2906283192,
           VALE_REFE=85.459999999999994,
           REFERENCE='SOURCE_EXTERNE',
           NOM_PARA='G_IRWIN',
           TABLE=GK04,)

