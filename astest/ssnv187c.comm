DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET') );

PRE_GMSH();

MAIL=LIRE_MAILLAGE(INFO=2,VERI_MAIL=_F(APLAT=1.E-3,));

MAIL=DEFI_GROUP(reuse =MAIL,
                    MAILLAGE=MAIL,
                    CREA_GROUP_MA=_F(GROUP_MA='GM20',
                                     NOM='SURF_GAU'),
                    );

MAIL=DEFI_GROUP(reuse =MAIL,
                    MAILLAGE=MAIL,
                    CREA_GROUP_MA=_F(GROUP_MA='GM21',
                                     NOM='SURF_BAS'),
                    );

MAIL=DEFI_GROUP(reuse =MAIL,
                    MAILLAGE=MAIL,
                    CREA_GROUP_MA=_F(GROUP_MA='GM22',
                                     NOM='SURF_HAU'),
                    );

MAIL=DEFI_GROUP(reuse =MAIL,
                    MAILLAGE=MAIL,
                    CREA_GROUP_MA=_F(GROUP_MA='GM23',
                                     NOM='SURF_DEV'),
                    );

MAIL=DEFI_GROUP(reuse =MAIL,
                    MAILLAGE=MAIL,
                    CREA_GROUP_MA=_F(GROUP_MA='GM24',
                                     NOM='SURF_DER'),
                    );

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);



MAIL=MODI_MAILLAGE(reuse=MAIL,MAILLAGE=MAIL,
                   ORIE_PEAU_3D=_F(GROUP_MA='SURF_HAU',),)

FORCE=AFFE_CHAR_MECA(MODELE=MODELE,
                     PRES_REP=_F(GROUP_MA='SURF_HAU',
                                 PRES=0.876,),);

DEPL=AFFE_CHAR_MECA(MODELE=MODELE,
                     DDL_IMPO=(_F(GROUP_MA='SURF_BAS',
                                  DY=0.0,),
                               _F(GROUP_MA='SURF_GAU',
                                  DX=0.0,),
                               _F(GROUP_MA='SURF_DEV',
                                  DZ=0.0,),
                               _F(GROUP_MA='SURF_DER',
                                  DZ=0.0,),
                               ),);

LISTE=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=20,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,0.0,1.0,1.0,),);



# MATERIAU HYPERELASTIQUE: MOONEY-RIVLIN
C01 = 2.3456;
C10 = 0.709;
C20 = 0.0;
NU  = 0.499
K   = (6*(C10+C01))/(3*(1-2*NU))

RUBBER1=DEFI_MATERIAU(ELAS_HYPER=_F(C10=C10,
                                   C01=C01,
                                   C20=C20,
                                   K=K,
                                   RHO=1000.0),);

CHMAT1=AFFE_MATERIAU(MAILLAGE=MAIL,
                    MODELE=MODELE,
                    AFFE=_F(TOUT='OUI',
                            MATER=RUBBER1,),);

RESU1=STAT_NON_LINE(MODELE=MODELE,
                   CHAM_MATER=CHMAT1,
                   EXCIT=(
                          _F(CHARGE=FORCE,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=DEPL,),),
                   NEWTON=(_F(REAC_INCR=1,
                              MATRICE='TANGENTE',
                              REAC_ITER=1,)),
                   COMPORTEMENT=_F(RELATION='ELAS_HYPER',
                                DEFORMATION = 'GROT_GDEP',
                                TOUT='OUI',),
                   CONVERGENCE=(_F(ARRET='OUI',
                                   ITER_GLOB_MAXI=20)),
                   INCREMENT=_F(LIST_INST=LISTE,),);

TEST_RESU(RESU=_F(NUME_ORDRE=20,
                  GROUP_NO='GM100',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.034072487756090002,
                  VALE_REFE=-0.0340091,
                  CRITERE='RELATIF',
                  PRECISION=2.E-3,),
          )

# MATERIAU HYPERELASTIQUE: NEO-HOOKEEN
C01 = 0.0;
C10 = 1.2345;
C20 = 0.0;
NU  = 0.499
K   = (6*(C10+C01))/(3*(1-2*NU))


RUBBER2=DEFI_MATERIAU(ELAS_HYPER=_F(C10=C10,
                                   C01=C01,
                                   C20=C20,
                                   K=K,
                                   RHO=1000.0),);


CHMAT2=AFFE_MATERIAU(MAILLAGE=MAIL,
                    MODELE=MODELE,
                    AFFE=_F(TOUT='OUI',
                            MATER=RUBBER2,),);

RESU2=STAT_NON_LINE(MODELE=MODELE,
                   CHAM_MATER=CHMAT2,
                   EXCIT=(
                          _F(CHARGE=FORCE,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=DEPL,),),
                   NEWTON=(_F(REAC_INCR=1,
                              MATRICE='TANGENTE',
                              PREDICTION='ELASTIQUE',
                              REAC_ITER=1,)),
                   COMPORTEMENT=_F(RELATION='ELAS_HYPER',
                                DEFORMATION = 'GROT_GDEP',
                                TOUT='OUI',),
                   CONVERGENCE=(_F(ARRET='OUI',
                                   ITER_GLOB_MAXI=20)),
                   INCREMENT=_F(LIST_INST=LISTE,),);



TEST_RESU(RESU=_F(NUME_ORDRE=20,
                  GROUP_NO='GM100',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.078330830948144997,
                  VALE_REFE=-0.078174999999999994,
                  CRITERE='RELATIF',
                  PRECISION=2.E-3,),
          )

# MATERIAU HYPERELASTIQUE: SIGNORINI
C01 = 1.2345;
C10 = 0.1234;
C20 = 0.456;
NU  = 0.499
K   = (6*(C10+C01))/(3*(1-2*NU))


RUBBER3=DEFI_MATERIAU(ELAS_HYPER=_F(C10=C10,
                                   C01=C01,
                                   C20=C20,
                                   K=K,
                                   RHO=1000.0),);


CHMAT3=AFFE_MATERIAU(MAILLAGE=MAIL,
                    MODELE=MODELE,
                    AFFE=_F(TOUT='OUI',
                            MATER=RUBBER3,),);

RESU3=STAT_NON_LINE(MODELE=MODELE,
                   CHAM_MATER=CHMAT3,
                   EXCIT=(
                          _F(CHARGE=FORCE,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=DEPL,),),
                   NEWTON=(_F(REAC_INCR=1,
                              MATRICE='TANGENTE',
                              REAC_ITER=1,)),
                   COMPORTEMENT=_F(RELATION='ELAS_HYPER',
                                DEFORMATION = 'GROT_GDEP',
                                TOUT='OUI',),
                   CONVERGENCE=(_F(ARRET='OUI',
                                   ITER_GLOB_MAXI=20)),
                   INCREMENT=_F(LIST_INST=LISTE,),);



TEST_RESU(RESU=_F(NUME_ORDRE=20,
                  GROUP_NO='GM100',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU3,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.071058425921551996,
                  VALE_REFE=-0.066199999999999995,
                  CRITERE='RELATIF',
                  PRECISION=0.075,),
          )

FIN();
