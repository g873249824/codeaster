MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=_F(GROUP_MA=('VOL'),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D'),
                     );        


LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-12.5');
                     
LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-37.5');
     

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN1));

                    
FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN2));
                    

                                                                
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         INFO=2,
                        );


E=205000.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL'),
                                MATER=ACIER,
                                ),
                             ));
ZERO=0.0
DEPX=1.E-3
DEPY=2.E-3
DEPZ=3.E-3

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(GROUP_NO='face_mil',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(GROUP_NO='face_hau',
                                    DX=ZERO,
                                    DY=ZERO,   
                                    DZ=DEPZ),
                                 _F(GROUP_NO='face_bas',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=-DEPZ,
                                    ),
                                 ),
                       INFO=1);

        

UTOT1=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CH1), 
                          ),
                   SOLVEUR=_F(METHODE='MUMPS',
   
                             ),
                   INFO=1);


# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D')) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)
 
