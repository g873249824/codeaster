                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,25.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),   
                            )    
           );


MAILLAGE=DEFI_GROUP(reuse = MAILLAGE,
                  MAILLAGE = MAILLAGE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'face_hau',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'H',
