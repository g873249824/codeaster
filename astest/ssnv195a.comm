MAILLAGE=LIRE_MAILLAGE(FORMAT='MED',INFO=1);

MAILLAGE=DEFI_GROUP(reuse = MAILLAGE,
                  MAILLAGE = MAILLAGE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'H',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,50.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'B',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,0.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
