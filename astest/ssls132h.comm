DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#-----------------------------------------------------------------------
# VALIDATION DES MEMBRANES SUR POUTRE CONSOLE (QUADRANGLE)
# VERIFICATION DE LA PREDEFORMATION
#-----------------------------------------------------------------------

MAILL1=LIRE_MAILLAGE(FORMAT='MED'   )

MAILL2=CREA_MAILLAGE(
               MAILLAGE=MAILL1,
               CREA_MAILLE=_F(
                   NOM = 'ACPLUS',
                   GROUP_MA = 'CONSOLE',
                   PREF_MAILLE = 'B',
                              )
                           )


MAILTOT=CREA_MAILLAGE(
               MAILLAGE=MAILL2,
               CREA_MAILLE=_F(
                   NOM = 'ACMOINS',
                   GROUP_MA = 'CONSOLE',
                   PREF_MAILLE = 'C',
                              )
                           )
MAILTOT = DEFI_GROUP(
             reuse    = MAILTOT,
             MAILLAGE = MAILTOT,
             CREA_GROUP_NO = _F(
                   TOUT_GROUP_MA = 'OUI'))

# -----------------------------------------------------
# DEFINITION DES MATERIAUX
# -----------------------------------------------------

E_A = 2.E11
section_l = 0.2

ACIER=DEFI_MATERIAU(ELAS_MEMBRANE =_F(M_LLLL = E_A*section_l,
                                      M_LLTT = 0.,
                                      M_LLLT = 0.,
                                      M_TTTT = 0.,
                                      M_TTLT = 0.,
                                      M_LTLT = 0.,),)

BETON=DEFI_MATERIAU(
            ELAS=_F(  E = 1.E4,   NU = 0.0,),
                          )

# -------------------------------------------------
# AFFECTATION DES MATERIAUX
# -------------------------------------------------

MATAF=AFFE_MATERIAU(    MAILLAGE=MAILTOT,
                          AFFE=(
                          _F(  GROUP_MA = ('ACPLUS','ACMOINS'),
                                 MATER = ACIER,
                               ),
                          _F(  GROUP_MA = ('CONSOLE'),
                                 MATER = BETON,
                               )
                               )
                      )

# -------------------------------------------------
# AFFECTATION DES MODELES
# -------------------------------------------------

LEMOD=AFFE_MODELE(    MAILLAGE=MAILTOT,     
                        AFFE=(
                              _F(  GROUP_MA = ('ACPLUS','ACMOINS'),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = 'MEMBRANE',
                             ),
                              _F(  GROUP_MA = ('CONSOLE',),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = 'DKT',
                             )
                          )
                      )

# -------------------------------------------------
# AFFECTATION DES CARACTERISTIQUES
# -------------------------------------------------

LACAR=AFFE_CARA_ELEM(    MODELE=LEMOD,
                      COQUE=(
                          _F( GROUP_MA = ('CONSOLE', 'B0X','B1X','B0Y','B1Y',),
                                EPAIS = .2,
                                ANGL_REP = (0.0, 0.0,),
                             ),
                             ),
                       MEMBRANE=(
                                 _F( GROUP_MA = 'ACPLUS',
                                     ANGL_REP=(0,0),),
                                 _F( GROUP_MA = 'ACMOINS',
                                     ANGL_REP=(0,0),),
                                )
                        )

# ----------------------------------------------
# CHARGEMENT
# ----------------------------------------------

ZCHM=AFFE_CHAR_MECA(
             MODELE=LEMOD,
            DDL_IMPO=(
                   _F( GROUP_MA  = 'B0X',
                       DX = 0., DY=0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0. ),
                  ),
            PRE_EPSI = _F(
                   GROUP_MA = ('ACPLUS','ACMOINS'),
                   EXX     = 1.E-3,)

            )

# ----------------------------------------------
# CALCUL
# ----------------------------------------------

ZLIST=DEFI_LIST_REEL(   DEBUT=0.,
             INTERVALLE=_F( JUSQU_A = 1.0,  NOMBRE = 1, )
           )

LEDEP=STAT_NON_LINE(
                            MODELE=LEMOD,
                        CHAM_MATER=MATAF,
                         CARA_ELEM=LACAR,
                             EXCIT=_F(
                CHARGE = ZCHM,
                ),
                         COMPORTEMENT=_F(
                RELATION = 'ELAS',
                ),
                         INCREMENT=_F(
                LIST_INST = ZLIST,
                NUME_INST_FIN = 1,
                ),
              )

LEDEP=CALC_CHAMP(reuse =LEDEP,
                 RESULTAT=LEDEP,
                 CONTRAINTE=('EFGE_ELNO',),
                 DEFORMATION=('EPSI_ELGA','EPSI_ELNO',),
                 VARI_INTERNE=('VARI_ELNO',),
                 FORCE=('FORC_NODA','REAC_NODA'),);

#
# -------------------------------------------------
# POST TRAITEMENT DES RESULTATS
# -------------------------------------------------

TAB = POST_RELEVE_T(
        ACTION = _F(
               OPERATION = 'MOYENNE',
               INTITULE = 'RESU',
               GROUP_NO = 'B1X',
               RESULTAT = LEDEP,
               NOM_CHAM = 'DEPL',
               TOUT_ORDRE = 'OUI',
               NOM_CMP = 'DX'))

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=0.099999997,
           VALE_REFE=0.10000000000000001,
           NOM_PARA='DX',
           TABLE=TAB,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                      VALE_K='MOMENT_0',),
                   _F(CRITERE='ABSOLU',
                      CRIT_COMP='GT',
                      PRECISION=0.0,
                      NOM_PARA='INST',
                      VALE=0.0,),
                   ),
           )

FIN()
