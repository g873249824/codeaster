# ECRASEMENT DE DEUX BLOCS
# CONTACT METHODE CONTINUE,
# CONTACT_INIT='OUI'
# FORCE IMPOSEE

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM='CONTACT3_16',)
MA=LIRE_MAILLAGE(FORMAT='MED',);
MA=DEFI_GROUP( reuse=MA,MAILLAGE=MA,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'AB',),
                       _F(  GROUP_MA = 'CD',),
                       _F(  GROUP_MA = 'EF',),
                       _F(  GROUP_MA = 'FG',),
                       _F(  GROUP_MA = 'GH',),
                       _F(  GROUP_MA = 'HE',),),
                    );

#---------------------------------------------------
MO=AFFE_MODELE(
                MAILLAGE=MA,AFFE=(
                 _F( TOUT='OUI',
                     PHENOMENE='MECANIQUE',
                     MODELISATION='D_PLAN',),),
                                );

MA=MODI_MAILLAGE(reuse =MA,
                   MAILLAGE=MA,
                   ORIE_PEAU_2D=_F(GROUP_MA='CD',),
                   );
#--------------------------------------------------
MAT2=DEFI_MATERIAU(ELAS=_F(E = 20.0E+05,
                           NU = 0.,)
                          );
#
MAT1=DEFI_MATERIAU(ELAS=_F( E = 20.0E+05,
                            NU = 0.,),
                          );

CHMAT=AFFE_MATERIAU(
                            MAILLAGE=MA,
                            AFFE=(
                           _F(
                                 GROUP_MA = ('PLAQUE1'),
                                 MATER = MAT2,
                                ),
                           _F(
                                 GROUP_MA = ('PLAQUE2'),
                                 MATER = MAT1,
                                ),),
                           )
#--------------------------------------------------------
CHA1=AFFE_CHAR_MECA(
        MODELE=MO,
        DDL_IMPO=(_F(GROUP_NO = 'GH',  DX = 0.0, DY =  0.0,),
                  _F(GROUP_NO = 'C' ,   DX = 0.0,),),
                   );

FLAT=AFFE_CHAR_MECA(MODELE=MO,
                    PRES_REP=_F(GROUP_MA='CD',
                                PRES=100000.0,),);

CHA2 = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    ALGO_RESO_CONT = 'POINT_FIXE',
                    ALGO_RESO_GEOM = 'POINT_FIXE',
                    REAC_GEOM      = 'CONTROLE',
                    NB_ITER_GEOM   = 2,
                    ZONE =_F(
                             GROUP_MA_ESCL = 'AB',
                             GROUP_MA_MAIT = 'EF',
                             CONTACT_INIT  = 'OUI',
                             INTEGRATION   = 'SIMPSON',
                             ORDRE_INT     = 4,
                             ALGO_CONT     = 'STANDARD',
                             COEF_CONT     = 1000.,
                          ),
                   );

#-----------------------------------------------------------
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1., NOMBRE=1,)
                      );
FONC=DEFI_FONCTION(
                   NOM_PARA='INST',
                   VALE=(0., 0.0,
                         1., 1.0,)
                   );
#-----------------------------------------------
RESU=STAT_NON_LINE (
                   MODELE     = MO,
                   CHAM_MATER = CHMAT,
                   INCREMENT  =_F(
                               LIST_INST=L_INST,
                               NUME_INST_FIN=1,),
                   EXCIT      =(
                               _F(CHARGE=CHA1,
                                FONC_MULT=FONC,),
                               _F(CHARGE=FLAT,),),
                   CONTACT    = CHA2,
                   COMPORTEMENT  =_F(
                                RELATION='ELAS'),
                   NEWTON     =_F(MATRICE='TANGENTE',
                                  REAC_ITER=1,
                                  REAC_INCR=1,),
                   CONVERGENCE=_F(
                                ARRET='OUI',
                                ITER_GLOB_MAXI=30,
                                RESI_GLOB_MAXI=2.E-8,
                                 ),
                   SOLVEUR     =_F(
                               METHODE='LDLT',
                               RENUM='SANS',),
                   );


RESU=CALC_CHAMP(reuse=RESU,RESULTAT=RESU,CRITERES=('SIEQ_ELNO'),CONTRAINTE=('SIGM_ELNO'))

RESU=CALC_CHAMP( reuse=RESU,
                RESULTAT=RESU,
                CONTRAINTE='SIGM_NOEU',CRITERES='SIEQ_NOEU'
              );

SIELNO=CREA_CHAMP(  OPERATION='EXTR',
                    TYPE_CHAM='ELNO_SIEF_R',
                    NOM_CHAM='SIGM_ELNO',
                    RESULTAT=RESU,
                    NUME_ORDRE=1
                       );

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOEUD='N14',
                  NOM_CMP='LAGS_C',
                  VALE_CALC=-1.00350805E+05,
                  VALE_REFE=-1.E5,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='A',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='LAGS_C',
                  VALE_CALC=-1.00080194E+05,
                  VALE_REFE=-1.E5,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='B',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='LAGS_C',
                  VALE_CALC=-1.00080148E+05,
                  VALE_REFE=-1.E5,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOEUD='N14',
                  NOM_CMP='DY',
                  VALE_CALC=-0.049978394,
                  VALE_REFE=-0.050000000000000003,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='A',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.049996688,
                  VALE_REFE=-0.050000000000000003,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='B',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.049996703,
                  VALE_REFE=-0.050000000000000003,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

#
FIN( );
