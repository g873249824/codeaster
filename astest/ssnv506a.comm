#  PAR UN INDENTEUR SPHERIQUE ELASTIQUE
#  CAS TEST SSNV506A
#  CONTACT UNILATERAL
#  MODELISATION 2D AXISYMETRIQUE
#  UNITE DE LONGUEUR : LE MM

DEBUT(CODE=_F(NOM='SSNV506A',NIV_PUB_WEB='INTERNET'),);

PRE_GIBI();

SSNV506A=LIRE_MAILLAGE();

SSNV506A=DEFI_GROUP(reuse =SSNV506A,
                    MAILLAGE=SSNV506A,
                    CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

#-------------------------------------------------------------
#                 GRANDEURS PHYSIQUES
#-------------------------------------------------------------

ACIER_B=DEFI_MATERIAU(ELAS=_F(E=210000.,
                              NU=0.3,),
                      ECRO_LINE=_F(D_SIGM_EPSI=0.0,
                                   SY=50,),);

AC_SPH=DEFI_MATERIAU(ELAS=_F(E=21000000.,
                                NU=0.3,),);

DPLY=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,
                         1.0,1.0,),
                   INTERPOL='LIN',);

#----------------------------------------------------------------
#                       MODELE
#----------------------------------------------------------------

MO=AFFE_MODELE(MAILLAGE=SSNV506A,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

#REORIENTATION DES NORMALES
SSNV506A=MODI_MAILLAGE(reuse =SSNV506A,
                       MAILLAGE=SSNV506A,
                       ORIE_PEAU_2D=_F(GROUP_MA=('SMAI','SESC'),),
                       );

#AFFECTATION DU MATERIAU
CHMAT=AFFE_MATERIAU(MAILLAGE=SSNV506A,
                    AFFE=(_F(GROUP_MA='MAT1',
                             MATER=AC_SPH,),
                          _F(GROUP_MA='MAT2',
                             MATER=ACIER_B,),),);

#CONDITIONS AUX LIMITES
                               
                               
CHA1 = DEFI_CONTACT(MODELE       = MO,
                   FORMULATION  = 'DISCRETE',
                   
                   ZONE=(
                        _F(
                            GROUP_MA_MAIT='SMAI',
                               GROUP_MA_ESCL='SESC',
## UNIQUEMENT SI ON MET LE LIAISON_DDL PLUS BAS
#                               SANS_GROUP_NO=('PLANX','LS','LB'),
#                               SANS_GROUP_NO=('E','S',),
                         ),                           
                                                                           
                       ),  
                  )                               

CHA2=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='NDPL',
                                 DY=-100.0,),
## INUTILE EN AXI (SI L'AXE D'AXISYMETRIE COINCIDE "PARFAITEMENT"
##                 AVEC X=0)
                              _F(GROUP_NO=('LS','LB'),
                                 DX=0.0,),
                              _F(GROUP_NO='PLANX',
                                 DX=0.0,DY=0.0,),),
## CETTE LIAISON ENTRE LES NOEUDS E ET S NE SERT A RIEN
## IL N'Y A PAS DE MOUVEMENT DE CORPS RIGIDES CAR
## ON EST EN AXI ET LA SPHERE EST COMPLETEMENT BLOQUEE
#                     LIAISON_DDL=_F(GROUP_NO=('E','S'),
#                                    DDL=('DY','DY'),
#                                    COEF_MULT=(1.0,-1.0),
#                                    COEF_IMPO=0.0,),
                                   );

# CHA2=AFFE_CHAR_CINE(MODELE=MO,
#                     MECA_IMPO=(_F(GROUP_NO='NDPL',
#                                  DY=-100.0,),
# #                               _F(GROUP_NO=('LS','LB'),
# #                                  DX=0.0,),
#                               _F(GROUP_NO='PLANX',
#                                  DX=0.0,DY=0.0,),),
#                                    );

#------------------------------------------------------------------ 
#                         RESOLUTION
#------------------------------------------------------------------

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.01,
                                    NOMBRE=5,),
                                 _F(JUSQU_A=0.15,
                                    NOMBRE=24,),
                                 _F(JUSQU_A=1.0,
                                    NOMBRE=17,),),);

U=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',#METHODE='MUMPS',
                           ),
                MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(
                       _F(CHARGE=CHA2,
                          FONC_MULT=DPLY,),),
                CONTACT  = CHA1,          
                COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                             GROUP_MA='MAT2',),
                COMP_ELAS=_F(RELATION='ELAS',
                             GROUP_MA='MAT1',),
                INCREMENT=_F(LIST_INST=LINST,
                             INST_FIN=1.,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=15,),
                ARCHIVAGE=_F(ARCH_ETAT_INIT='OUI',),
                INFO=1,
                );

#------------------------------------------------------------------ 
#                         POST TRAITEMENT
#------------------------------------------------------------------

U=CALC_NO(reuse =U,
          RESULTAT=U,
          OPTION='REAC_NODA',
          MODELE=MO,
          CHAM_MATER=CHMAT,
          );

LINST2=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.,
                                   NOMBRE=10,),);

TAB_REAC=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO='SESC',
                                 RESULTAT=U,
                                 NOM_CHAM='REAC_NODA',
                                 LIST_INST=LINST2,
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TAB_REAC,
           FORMAT='TABLEAU',);

#------------------------------------------------------------------ 
#                         TESTS DES RESULTATS
#------------------------------------------------------------------
# TESTS SUR DES VALEURS ANALYTIQUES
