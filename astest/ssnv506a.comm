                     VALE=1.0),
           VALE=-9.1189947458763E+06,
           VERSION='9.2.18',
           REFERENCE='NON_REGRESSION',);

# TESTS DE NON-REGRESSION (AUTRES)
TEST_RESU(RESU=_F(RESULTAT=U,
                  NOEUD='N295',
                  INST=1.,
                  NOM_CHAM='VALE_CONT',
                  NOM_CMP='RN',
                  VALE=1.1058333934147E+06,
                  #PRECISION=1.5E-4,
                  CRITERE='RELATIF',
                  REFERENCE='NON_REGRESSION',
                  VERSION='7.1.6',),);

TEST_RESU(RESU=_F(RESULTAT=U,
                  NOEUD='N294',
                  INST=1.,
                  NOM_CHAM='VALE_CONT',
                  NOM_CMP='RN',
