NU_FO=DEFI_CONSTANTE(VALE=0.3)

ALPHA_FO=DEFI_CONSTANTE(VALE=1.E-5)

MAT=DEFI_MATERIAU(ELAS=_F( E = 2.0E+5, NU = 0.3, RHO = 8.E-6, ALPHA = 1.E-5),
