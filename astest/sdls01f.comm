# PLAQUE CARREE MINCE  LIBRE OU ENCASTREE SUR UN BORD
# DIMENSION: 1M*1M*0.01M
#
# CAS 1: ENCASTREE    CAS 2: LIBRE                 REF: SFM.VPCS SDLS01
#
# MODELISATION MEC3TR7H: 441 NOEUDS -- 200 MAILLES TRIA6

MA=LIRE_MAILLAGE(FORMAT='MED', )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(  MAILLAGE=MA,
                  AFFE=_F(  TOUT = 'OUI',       PHENOMENE = 'MECANIQUE',
                         MODELISATION = 'COQUE_3D') )

CARELEM=AFFE_CARA_ELEM(   MODELE=MO,
                            COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.01,
                                  A_CIS = 0.83333333333333) )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.1E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

#--------------------------------------------------------------------

# ---- CAS1: PLAQUE ENCASTREE SUR UN COTE

CHCAS1=AFFE_CHAR_MECA(  MODELE=MO,
           DDL_IMPO=_F(  GROUP_NO = 'AB',       DX = 0.,  DY = 0., DZ = 0.,
                                        DRX = 0., DRY = 0., DRZ = 0.) )

# ---- CAS2: PLAQUE LIBRE-LIBRE   ==>   PAS DE CHCAS2


# ------ CAS1


ASSEMBLAGE(   MODELE=MO,           CARA_ELEM=CARELEM,
                       CHARGE=CHCAS1,       CHAM_MATER=CHMAT,
                      NUME_DDL=CO("NUM"),MATR_ASSE=(
                _F(  MATRICE = CO("MATASSR1"),  OPTION = 'RIGI_MECA'),
                          _F(  MATRICE = CO("MATASSM1"),  OPTION = 'MASS_MECA'))
                   )

# ------ CAS2


ASSEMBLAGE(   MODELE=MO,           CARA_ELEM=CARELEM,
                                           CHAM_MATER=CHMAT,
                      NUME_DDL=CO("NUM2"),MATR_ASSE=(
                _F(  MATRICE = CO("MATASSR2"),  OPTION = 'RIGI_MECA'),
                          _F(  MATRICE = CO("MATASSM2"),  OPTION = 'MASS_MECA'))
                   )

