DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(FORMAT='MED')

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(
                      _F( GROUP_MA = 'HAUT'),
                      _F( GROUP_MA = 'BAS'))
                   )

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL,
             CREA_GROUP_MA=_F(NOM='TOUT',TOUT='OUI',)
                   )

MODELE=AFFE_MODELE(   MAILLAGE=MAIL,
                        AFFE=_F(  TOUT = 'OUI',
                      PHENOMENE = 'MECANIQUE',
                      MODELISATION = '3D_THMS')
                     )

#
#
####################################
#  LISTE DES INSTANTS DE CALCUL
####################################
LI=DEFI_LIST_REEL(   DEBUT=0.,
                       INTERVALLE=_F(  JUSQU_A = 1.E13, NOMBRE = 1)
                     )

############################################
#
# DEFINITION DES FONCTIONS DE COMPORTEMENT :
# VISCOSITE LIQUIDE ET GAZ : UNITE : PA.S
#
UN=DEFI_CONSTANTE(    VALE=1.)

ZERO=DEFI_CONSTANTE(    VALE=0.)

#
# ON FAIT TOURNER LE REPERE POUR TESTER L'ANISOTROPIE DANS LE CAS THERMIQUE : LE MILIEU RESTE CEPENDANT ICI ISOTROPE
#
#=> ANGLE NAUTIQUE UTILISES PAR ASTER (A. Nautique classique)
#
BETA = -60.
ALPHA = 30.

ORIREP=AFFE_CARA_ELEM(MODELE=MODELE,
       MASSIF=( _F(GROUP_MA=('TOUT'),
              ANGL_REP=(ALPHA,BETA),
                        ),),);
#
#
# PERMEABILITE INTRINSEQUE, RELATIVE DU LIQUIDE, RELATIVE DU GAZ
# UNITE INTRINSEQUE : METRES CARRES , UNITE RELATIVE : SANS
#
KINT=DEFI_CONSTANTE(    VALE=3.E-16)

PERMLIQ=DEFI_CONSTANTE(    VALE=1.)

DPERMLI=DEFI_CONSTANTE(    VALE=0.)

PERMGAZ=DEFI_CONSTANTE(    VALE=1.)

DPERGSA=DEFI_CONSTANTE(    VALE=0.)

DPERGPG=DEFI_CONSTANTE(    VALE=0.)


DCONDLI = DEFI_CONSTANTE ( VALE = 0.   )
CONDGAZ = DEFI_CONSTANTE ( VALE = 0.02 )
DCONDGA = DEFI_CONSTANTE ( VALE = 0.   )

#
# CONDUCTIVITES DU SOLIDE, DU LIQUIDE, DU GAZ
# UNITES : WATTS PAR METRE CARRE
#
CONDHOMO=DEFI_CONSTANTE(    VALE=1.7)

DCONDHO=DEFI_CONSTANTE(    VALE=0.)

CONDLIQ=DEFI_CONSTANTE(    VALE=3.E-16)

ARGILE0=DEFI_MATERIAU(     THM_LIQU=_F(
   RHO = 1000.,
   UN_SUR_K = 3.77E-9,
   ALPHA = 1.E-4,
   CP = 2.85E6,
   VISC = UN,
   D_VISC_TEMP = ZERO),
#
                              THM_GAZ=_F(
   MASS_MOL = 28.96E-3,
   CP = 1000.,
   VISC = UN,
   D_VISC_TEMP = ZERO),
#
                              THM_DIFFU=_F(
   R_GAZ = 8.315,
   RHO = 1.6E3,
   CP = 2.85E6,
   BIOT_L = 1.E-12,
   BIOT_N = 1.E-12,
   SATU_PRES = UN,
   D_SATU_PRES = ZERO,
   PESA_X = 0.,
   PESA_Y = 0.,
   PESA_Z = 0.,
   PERMIN_L = KINT,
   PERMIN_N = KINT,
   PERM_LIQU = UN,
   D_PERM_LIQU_SATU = ZERO,
   PERM_GAZ = PERMGAZ,
   D_PERM_SATU_GAZ = DPERGSA,
   D_PERM_PRES_GAZ = DPERGPG,
   LAMB_TL = CONDHOMO,
   LAMB_TN = CONDHOMO,
  ),
                              THM_INIT=_F(
   TEMP = 273.,
   PRE1 = 0.,
   PRE2 = 0.,
   PORO = 0.4,
   PRES_VAPE = 1.),
                               COMP_THM = 'LIQU_SATU',

#                               ELAS=_F(
#   E = 225.E6,
#   NU = 0.,
#   RHO = 2.E3,
#   ALPHA = 8.E-6)
#   )
# CE CAS EST ISOTROPE MAIS ON VEUT TESTER ELAS_ISTR

                               ELAS_ISTR=_F(
   E_L  = 225.E6,
   E_N  = 225.E6,
   NU_LT = 0.,
   NU_LN = 0.,
   G_LN = 112.25E6,
   RHO = 2.E3,
   ALPHA_L = 8.E-6,
   ALPHA_N = 8.E-6,
     )
   )

CHMAT0=AFFE_MATERIAU(   MAILLAGE=MAIL,
                          AFFE=_F( GROUP_MA = 'VOLUME',
                        MATER = ARGILE0)
                 )

CHAR0=AFFE_CHAR_MECA(   MODELE=MODELE,
                        DDL_IMPO=(
                                  _F(  TOUT = 'OUI',
                                     DX = 0.,
                                     DY = 0.,
                                     DZ = 0.),
                                  _F(NOEUD=('NO1','NO3','NO8','NO6','NO13','NO15','NO20','NO18'),
                                     PRE1 = 0.),
                                  _F(NOEUD=('NO13','NO18','NO20','NO15'),
                                     TEMP = 0.)),

                        FLUX_THM_REP=_F(  GROUP_MA = 'HAUT',
                                   FLUN = 0.5,
                                   FLUN_HYDR1 = 0.)
                  );

MESTAT=STAT_NON_LINE( MODELE=MODELE,
                CARA_ELEM=ORIREP,
                SOLVEUR=_F( METHODE = 'MUMPS',PCENT_PIVOT=120),
                        CHAM_MATER=CHMAT0,
                             EXCIT=_F(
                CHARGE = CHAR0),
                         COMPORTEMENT=_F( PARM_THETA=1.,
                RELATION = 'KIT_THM',
                RELATION_KIT = (
                  'ELAS',       'LIQU_SATU',  'HYDR_UTIL'),
                ITER_INTE_MAXI = 5),
                         INCREMENT=_F(
                LIST_INST = LI),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 20),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 20),
              )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=MESTAT,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.2940782,
                  VALE_REFE=0.294118,
                  REFERENCE='NON_DEFINI',
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=MESTAT,
                  NOM_CHAM='DEPL',
                  NOEUD='NO20',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.0,

                  CRITERE='ABSOLU',
                  ),
          )

FIN()
