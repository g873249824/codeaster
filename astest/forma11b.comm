#                          VERI_MODE=_F(STOP_ERREUR='NON',),
#                          TITRE='2/ METHODE DE LANCZOS AVEC OPTION MODE RIGIDE',);
#IMPR_RESU(RESU=_F(RESULTAT=MODE_LAS,
#                  TOUT_CHAM='NON',
#                  TOUT_PARA='OUI',),);
MODE_LAA=MODE_ITER_SIMULT(MATR_A=K_ASSE,
                          MATR_B=M_ASSE,
                          METHODE='TRI_DIAG',
                          OPTION='MODE_RIGIDE',
                          CALC_FREQ=_F(OPTION='BANDE',
                                       FREQ=(0.0,2800.0,),),
                          TITRE='2/ METHODE DE LANCZOS AVEC OPTION',);
IMPR_RESU(RESU=_F(RESULTAT=MODE_LAA,
                  TOUT_CHAM='NON',
                  TOUT_PARA='OUI',),);

TEST_RESU(RESU=(_F(RESULTAT=MODE_LAA,
                   NUME_ORDRE=11,
                   PARA='FREQ',
                   VALE=2470.841,
                   PRECISION=1e-05,
                   REFERENCE='NON_REGRESSION',
                   VERSION='7.3.11',)))

# FIN DE LA QUESTION 2.
#FIN();

