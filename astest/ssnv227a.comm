#       D4UN MATERIAU DE TYPE MOONEY-RIVLIN

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='OUI',),DEBUG=_F(SDVERI='OUI'))

Mesh=LIRE_MAILLAGE(FORMAT='MED',NOM_MED='Mblock');


Mod3d=AFFE_MODELE(MAILLAGE=Mesh,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);


Mesh=MODI_MAILLAGE(reuse=Mesh,MAILLAGE=Mesh,
                   ORIE_PEAU_3D=_F(GROUP_MA='Atop',),)


Pression = -6.000
FORCE=AFFE_CHAR_MECA(MODELE=Mod3d,
                     PRES_REP=_F(GROUP_MA='Atop',
                                 PRES=Pression),);

DEPL=AFFE_CHAR_MECA(MODELE=Mod3d,
                     DDL_IMPO=(_F(GROUP_MA='Abot',DZ=0.0),
                               _F(GROUP_NO='Nfixx',DY=0.0),
                               _F(GROUP_NO='Nfixy',DX=0.0),),);


LISTE=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,NOMBRE=20,),
                     INFO=2);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,0.0,
                      1.0,1.0,),
                INFO=2);



# MATERIAU HYPER-ELASTIQUE: Mooney-Rivlin
C01 = 2.3456;
C10 = 0.709;
C20 = 0.0;
NU  = 0.499
K   = (6*(C10+C01))/(3*(1-2*NU))

rubber=DEFI_MATERIAU(ELAS_HYPER=_F(C10=C10,
                                   C01=C01,
                                   C20=C20,
                                   K=K,),);


MatRub=AFFE_MATERIAU(MAILLAGE=Mesh,
                    MODELE=Mod3d,
                    AFFE=_F(TOUT='OUI',
                            MATER=rubber,),);

# CALCUL EN PETITES PERTURBATIONS (HPP)
# PRESSION APPLIQUEE = 6Pa

LIST_HPP=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.e-6,NOMBRE=1,),
                     INFO=2);

res_HPP=STAT_NON_LINE(MODELE=Mod3d,
                   CHAM_MATER=MatRub,
                   EXCIT=(_F(CHARGE=FORCE,FONC_MULT=RAMPE),
                          _F(CHARGE=DEPL,),),
                   NEWTON=(_F(REAC_INCR=1,
                              MATRICE='TANGENTE',
                              REAC_ITER=1,)),
                   COMPORTEMENT=_F(RELATION='ELAS_HYPER',
                                DEFORMATION = 'GROT_GDEP',
                                TOUT='OUI',),
                   CONVERGENCE=(_F(ARRET='OUI',)),
                   INCREMENT=_F(LIST_INST=LIST_HPP,),);

# POST-TRAITEMENT

CALC_CHAMP( reuse=res_HPP,  RESULTAT=res_HPP,
         CONTRAINTE='SIEF_NOEU'
       )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=res_HPP,
                  NOM_CHAM='DEPL',
                  NOEUD='N1535',
                  NOM_CMP='DZ',
                  VALE_CALC=1.40865225E-05,
                  VALE_REFE=1.40865E-05,
                  CRITERE='RELATIF',
                  PRECISION=1.E-3,),
          )

# CALCUL EN GRANDS DEPLACEMENTS AVEC PRESSION SUIVEUSE
# PRESSION APPLIQUEE = 6MPa

res=STAT_NON_LINE(MODELE=Mod3d,
                   CHAM_MATER=MatRub,
                   EXCIT=(_F(CHARGE=FORCE,FONC_MULT=RAMPE,TYPE_CHARGE='SUIV'),
                          _F(CHARGE=DEPL,),),
                   NEWTON=(_F(REAC_INCR=1,
                              MATRICE='TANGENTE',
                              REAC_ITER=1,)),
                   COMPORTEMENT=_F(RELATION='ELAS_HYPER',
                                DEFORMATION = 'GROT_GDEP',
                                TOUT='OUI',),
                   CONVERGENCE=(_F(ARRET='OUI',
                                   ITER_GLOB_MAXI=50)),
                   INCREMENT=_F(LIST_INST=LISTE,),);

# POST-TRAITEMENT

CALC_CHAMP( reuse=res,  RESULTAT=res,
         CONTRAINTE='SIEF_NOEU',DEFORMATION='EPSI_NOEU',
       )

TEST_RESU(RESU=(_F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='DX',
                   VALE_CALC=-0.39037848251481,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='DEPL',
                   NOEUD='N4',
                   NOM_CMP='DX',
                   VALE_CALC=0.39037835975348,
                   CRITERE='RELATIF',
                   ),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='DY',
                   VALE_CALC=0.93690821071492,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='DEPL',
                   NOEUD='N8',
                   NOM_CMP='DY',
                   VALE_CALC=-0.93690821072894,
                   CRITERE='RELATIF',
                   ),
                ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=20,
                  RESULTAT=res,
                  NOM_CHAM='DEPL',
                  NOEUD='N1535',
                  NOM_CMP='DZ',
                  VALE_CALC=17.425967947298,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='SIXX',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='SIYY',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='SIZZ',
                   VALE_REFE=6.0,
                   VALE_CALC=6.0,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=1.E-3,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='SIXY',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='SIXZ',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='SIYZ',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='EPXX',
                   VALE_CALC=-0.15615136845366,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='EPYY',
                   VALE_CALC=-0.15615136845366,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='EPZZ',
                   VALE_CALC=0.40525506854181,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='EPXY',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='EPXZ',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                _F(NUME_ORDRE=20,
                   RESULTAT=res,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N1535',
                   NOM_CMP='EPYZ',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                ),
          )

FIN();
