DEBUT(CODE=_F(NOM='SSLS105C',NIV_PUB_WEB='INTERNET'),);

MAT=DEFI_MATERIAU(ELAS=_F(E=6.825E7,
                          NU=0.3,),);

#  300 SHB8


PRE_GIBI(UNITE_GIBI=21,UNITE_MAILLAGE=31);

MAIL31=LIRE_MAILLAGE(UNITE=31);

MAIL31=MODI_MAILLAGE(reuse=MAIL31,MAILLAGE=MAIL31,
ORIE_SHB8=_F(GROUP_MA='VOLTOT'),)

MAIL31=DEFI_GROUP(reuse =MAIL31,
                MAILLAGE=MAIL31,
                CREA_GROUP_NO=(_F(GROUP_MA='SUD1',),
                               _F(GROUP_MA='SUG1',),),);
                               

MODMEC31=AFFE_MODELE(MAILLAGE=MAIL31,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB8',),);
                            

CHM31=AFFE_MATERIAU(MAILLAGE=MAIL31,
                  AFFE=_F(TOUT='OUI',
                          MATER=MAT,),);


CHARG31=AFFE_CHAR_MECA(MODELE=MODMEC31,
                      DDL_IMPO=(_F(GROUP_MA=('SUD1'),
                                   DY=0.0,),
                                _F(GROUP_MA='SUG1',
                                   DX=0.0,),
                                _F(GROUP_NO='PCE',
                                   DZ=0.0,),),
                      FORCE_NODALE=(_F(GROUP_NO='PAE',
                                      FX=-2.,),
                                    _F(GROUP_NO='PBE',
                                      FY=2.,),),);

RESU31=MECA_STATIQUE(MODELE=MODMEC31,
                    CHAM_MATER=CHM31,
                    EXCIT=_F(CHARGE=CHARG31,),);

RESU31=CALC_ELEM(reuse =RESU31,
                OPTION='SIEF_ELNO_ELGA',
                RESULTAT=RESU31,);

TEST_RESU(RESU=(
            _F(  RESULTAT = RESU31, NOM_CHAM='DEPL', PRECISION = 20.E-2, REFERENCE='SOURCE_EXTERNE',NUME_ORDRE=1,
              GROUP_NO = 'PAE',      NOM_CMP = 'DX',           VALE = -0.185),

            _F(   RESULTAT = RESU31, NOM_CHAM='DEPL',  PRECISION = 20.E-2, REFERENCE='SOURCE_EXTERNE',NUME_ORDRE=1,
              GROUP_NO = 'PBE',      NOM_CMP = 'DY',           VALE = 0.185),
              ),)


