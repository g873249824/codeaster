DEBUT(CODE=_F(NOM='SSNL134A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='OUI'),
      IGNORE_ALARM='FONCT0_62',)

#Acquisition du maillage du  portique bi-encastre

MAILLAGE=LIRE_MAILLAGE(UNITE=17,FORMAT='MED',);

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);
#Acquisition du maillage de la section en fibres

M_SECT=LIRE_MAILLAGE(UNITE=18,FORMAT='MED',);
#Corps du fichier de commandes : modele, materiau, caracteristiques de la poutre.

MOD_TGM=AFFE_MODELE(MAILLAGE=MAILLAGE,
                    AFFE=_F(GROUP_MA='OMEGA',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='POU_D_TGM',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=720.0,
                            NU=0.3,),
                    ECRO_LINE=_F(SY=10.44,
                      D_SIGM_EPSI=72.0,),);

GF=DEFI_GEOM_FIBRE(
   SECTION =_F(GROUP_FIBRE='SECF',
             GROUP_MA_SECT='SECT',
             MAILLAGE_SECT=M_SECT,
            COOR_AXE_POUTRE=(0.,0.,),),)

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,MATER_SECT=ACIER,
                  MULTIFIBRE=_F(GROUP_FIBRE='SECF',MATER=ACIER,
                                 RELATION='VMIS_ISOT_LINE',),
                     )

MATER=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                    AFFE=_F(GROUP_MA='OMEGA',MATER=ACIER,),
                  AFFE_COMPOR=_F(GROUP_MA='OMEGA',COMPOR=COMPF),
                    );

CARACT=AFFE_CARA_ELEM(MODELE=MOD_TGM,
                      POUTRE=_F(GROUP_MA='OMEGA',
                                SECTION='GENERALE',
                                CARA=('A','IY','IZ','AY','AZ','JX','JG','IYR2','IZR2',),
                                VALE=(6.0,4.455,1.98,1.2,1.2,8.0,0.0,0.0,0.0,),),
                      GEOM_FIBRE=GF,
                      MULTIFIBRE=_F(GROUP_MA='OMEGA',
                                   GROUP_FIBRE='SECF'),);

#Conditions aux limites (bi-encastrement) et chargement

LIAISON=AFFE_CHAR_MECA(MODELE=MOD_TGM,
                       DDL_IMPO=(_F(GROUP_NO=('PA','PC',),
                                    DX=0.0,
                                    DY=0.0,
                                    DRX=0.0,
                                    DRY=0.0,),
                                 _F(GROUP_NO='OMEGA',
                                    DZ=0.0,
                                    GRX=0.0,),),);

FORCE=AFFE_CHAR_MECA(MODELE=MOD_TGM,
                     FORCE_NODALE=_F(GROUP_NO='PX',
                                     FY=-1.0,),);

FO_MULT=DEFI_FONCTION(NOM_PARA='INST',
                      VALE=(0.0,0.0,10.0,10.0,),
                      PROL_DROITE='EXCLU',
                      PROL_GAUCHE='EXCLU',);

tforc=0.5
tfin=10.0;

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=tforc,
                                     NOMBRE=5,),
                                  _F(JUSQU_A=tfin,
                                     PAS=0.01,),),);

RESO0=STAT_NON_LINE(MODELE=MOD_TGM,
                    CHAM_MATER=MATER,
                    CARA_ELEM=CARACT,
                    EXCIT=(_F(CHARGE=LIAISON,),
                           _F(CHARGE=FORCE,
                              FONC_MULT=FO_MULT,
                              ),),
                    COMP_INCR=_F(RELATION='MULTIFIBRE',DEFORMATION='GROT_GDEP',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=tforc,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1e-6,
                                   ITER_GLOB_MAXI=50,),
                    );

RESO=STAT_NON_LINE(MODELE=MOD_TGM,
                    CHAM_MATER=MATER,
                    CARA_ELEM=CARACT,
                    ETAT_INIT=_F(EVOL_NOLI=RESO0),
                    EXCIT=(_F(CHARGE=LIAISON,),
                           _F(CHARGE=FORCE,
                              TYPE_CHARGE='FIXE_PILO',
                              ),),
                    COMP_INCR=_F(RELATION='MULTIFIBRE',DEFORMATION='GROT_GDEP',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=tfin,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='LONG_ARC',
                                SELECTION='ANGL_INCR_DEPL',
                                COEF_MULT=0.01,
                                GROUP_NO='OMEGA',
                                NOM_CMP=('DX','DY',),
                                ETA_PILO_MAX=2.0,),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1e-5,
                                   ITER_GLOB_MAXI=10,),
                    );

