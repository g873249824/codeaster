# Preparation des fonctions pour les tests

ETA_PILO=RECU_FONCTION(RESULTAT=RESO,
                       NOM_PARA_RESU='ETA_PILOTAGE',);

DEPX=RECU_FONCTION(RESULTAT=RESO,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   GROUP_NO='PX',);

DEPY=RECU_FONCTION(RESULTAT=RESO,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='PX',);

MDEPY=CALC_FONCTION(COMB=_F(FONCTION=DEPY,
                            COEF=-1.0,),);

T_ETA=CREA_TABLE(FONCTION=_F(FONCTION=ETA_PILO,),);

T_DEPX=CREA_TABLE(FONCTION=_F(FONCTION=DEPX,),);

T_DEPY=CREA_TABLE(FONCTION=_F(FONCTION=MDEPY,),);

TAB_COMB=CALC_TABLE(TABLE=T_ETA,
                    ACTION=(_F(OPERATION='COMB', TABLE=T_DEPX, NOM_PARA='INST'),
                            _F(OPERATION='COMB', TABLE=T_DEPY, NOM_PARA='INST'),),);
IMPR_TABLE(TABLE=TAB_COMB,);

DEPX_ETA=RECU_FONCTION(TABLE=TAB_COMB,
                     PARA_X='DX',
                     PARA_Y='ETA_PILO',
                     INTERPOL='LIN',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',);

DEPY_ETA=RECU_FONCTION(TABLE=TAB_COMB,
                     PARA_X='DY',
                     PARA_Y='ETA_PILO',
                     INTERPOL='LIN',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',);

# Impression des courbes

IMPR_FONCTION(FORMAT='XMGRACE',
              COURBE=(_F(FONC_X=DEPX,
                         FONC_Y=ETA_PILO,
                         LEGENDE='DX',MARQUEUR=0,),
                      _F(FONC_X=MDEPY,
                         FONC_Y=ETA_PILO,
                         LEGENDE='-DY',MARQUEUR=0,),
                      _F(LIST_PARA=DEPX_REF,
                         LIST_RESU=ETA_REF,
                         LEGENDE='DX (ref.)',),
                      _F(LIST_PARA=DEPY_REF,
                         LIST_RESU=ETA_REF,
                         LEGENDE='-DY (ref.)',),),
              TITRE='Reponse force-deplacement',
              BORNE_X=(0.0,105.0,),
              BORNE_Y=(-1.0,2.0,),
              ECHELLE_X='LIN',
              ECHELLE_Y='LIN',
              GRILLE_X=10,
              GRILLE_Y=0.5,
              LEGENDE_X='Deplacement (cm)',
              LEGENDE_Y='Force (N)',);

