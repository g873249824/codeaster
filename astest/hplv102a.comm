# MODELISATION AXISYMETRIQUE
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',);
#--------------------------------------------------------------------
#               DEBUT DU THERMIQUE                                  #
#--------------------------------------------------------------------

MOTH=AFFE_MODELE(MAILLAGE=M,
                 
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='THERMIQUE',
                         MODELISATION='AXIS',),);

MATH=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,
                           RHO_CP=0.0,),);

CMTH=AFFE_MATERIAU(MAILLAGE=M,
                   AFFE=_F(TOUT='OUI',
                           MATER=MATH,),);

CHTH=AFFE_CHAR_THER(MODELE=MOTH,
                    TEMP_IMPO=(_F(GROUP_NO='GRNO3',
                                  TEMP=0.0,),
                               _F(NOEUD='N3',
                                  TEMP=-1.0,),
                               _F(GROUP_NO='GRNO5',
                                  TEMP=-1.0,),),);

METH=CALC_MATR_ELEM(OPTION='RIGI_THER',
                    MODELE=MOTH,
                    CHAM_MATER=CMTH,
                    CHARGE=CHTH,);

VETH=CALC_VECT_ELEM(OPTION='CHAR_THER',
                    CHARGE=CHTH,);

NUTH=NUME_DDL(MATR_RIGI=METH,);

KTH=ASSE_MATRICE(MATR_ELEM=METH,
                 NUME_DDL=NUTH,);

FTH=ASSE_VECTEUR(VECT_ELEM=VETH,
                 NUME_DDL=NUTH,);

KTH=FACTORISER(reuse =KTH,
              MATR_ASSE=KTH,);

TEMP=RESOUDRE(MATR=KTH,
               CHAM_NO=FTH,);
#--------------------------------------------------------------------
#               FIN DU THERMIQUE                                    #
#--------------------------------------------------------------------

MO=AFFE_MODELE(MAILLAGE=M,
               
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

MA=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                         NU=0.3,
                         ALPHA=1e-06,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',  MATER=MA,),
                 AFFE_VARC =_F(TOUT='OUI', VALE_REF=0.,CHAM_GD=TEMP, NOM_VARC='TEMP',),
                 );
CMST=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',  MATER=MA,),
                 );

CH=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_NO='GRNM1',
                               DX=0.0,),
                            _F(GROUP_NO='GRNM2',
                               DY=0.0,),),);

ME=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                  MODELE=MO,
                  CHAM_MATER=CM,
                  CHARGE=CH,);

VE=CALC_VECT_ELEM(OPTION='CHAR_MECA',
                  CHARGE=CH,
                  CHAM_MATER=CM,);

NU=NUME_DDL(MATR_RIGI=ME,);

K=ASSE_MATRICE(MATR_ELEM=ME,
               NUME_DDL=NU,);

F=ASSE_VECTEUR(VECT_ELEM=VE,
               NUME_DDL=NU,);

K=FACTORISER(reuse =K,
            MATR_ASSE=K,);

DEPLA=RESOUDRE(MATR=K,
                CHAM_NO=F,);

RESU=CREA_RESU(  OPERATION='AFFE',
                 TYPE_RESU='EVOL_ELAS',
                 NOM_CHAM='DEPL',
                      AFFE=_F(  CHAM_GD = DEPLA,
                                INST=0.,
                                MODELE=MO,
                                CHAM_MATER = CM)     )

#--------------------------------------------------------------------
#               CALCUL DE G SUR 4 COURONNES                         #
#--------------------------------------------------------------------


FOND=DEFI_FOND_FISS(MAILLAGE=M,
                    FOND_FISS=(_F( NOEUD='N3'),
                              ),
                    SYME='OUI',
                    LEVRE_SUP=_F(GROUP_MA='GRMA5',),
                    
                    )


G1=CALC_G(        THETA=_F(R_INF=1.0,
                           FOND_FISS=FOND,
                           MODULE=1.0,
                           DIRECTION=(1.0,0.0,0.0,),
                           R_SUP=4.0),
                  RESULTAT=RESU,
                  EXCIT=_F(CHARGE=CH,),);


G2=CALC_G(        THETA=_F(
                           R_INF=0.5,
                           FOND_FISS=FOND,
                           MODULE=1.0,
                           DIRECTION=(1.0,0.0,0.0,),
                           R_SUP=4.5),
                  RESULTAT=RESU,
                  EXCIT=_F(CHARGE=CH,),);


G3=CALC_G(        THETA=_F(
                           R_INF=1.5,
                           FOND_FISS=FOND,
                           MODULE=1.0,
                           DIRECTION=(1.0,0.0,0.0,),
                           R_SUP=3.5),
                  RESULTAT=RESU,
                  EXCIT=_F(CHARGE=CH,),);


G4=CALC_G(        THETA=_F(
                           R_INF=1.0,
                           FOND_FISS=FOND,
                           MODULE=1.0,
                           DIRECTION=(1.0,0.0,0.0,),
                           R_SUP=4.5),
                  RESULTAT=RESU,
                  EXCIT=_F(CHARGE=CH,),);
#--------------------------------------------------------------------
#            TEST DES RESULTATS DE G                                #
#--------------------------------------------------------------------

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.02,
           VALE_CALC=2.91879655878e-06,
           VALE_REFE=2.9556e-06,
           NOM_PARA='G',
           TABLE=G1,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.02,
           VALE_CALC=2.91869623326e-06,
           VALE_REFE=2.9556e-06,
           NOM_PARA='G',
           TABLE=G2,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.02,
           VALE_CALC=2.9185635229e-06,
           VALE_REFE=2.9556e-06,
           NOM_PARA='G',
           TABLE=G3,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.02,
           VALE_CALC=2.91872329286e-06,
           VALE_REFE=2.9556e-06,
           NOM_PARA='G',
           TABLE=G4,)

FIN();
#
