DEBUT(CODE=_F(NOM='HSNV100Q',
              NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'));

#......................................................................
# CAS TEST THERMOPLASTICITE ELEMENTAIRE
#......................................................................
# DEFINITION DES CARACTERISTIQUES DU MATERIAU

MAIL=LIRE_MAILLAGE(FORMAT='MED');

#
# DONNEES DE MODELISATION

FCT1=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(0.200E-2,400.,
                         0.400E-2,500.,
                         ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE');

#

FCT2=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(0.100E-2,200.,
                         0.300E-2,300.,
                         ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE');

#

CTRACB=DEFI_NAPPE(NOM_PARA='TEMP',
                  PARA=(0.,50.),
                  FONCTION=(FCT1,FCT2),
                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE');

#
# MATERIAU ISOTROPE

MAT=DEFI_MATERIAU(ELAS=_F(E=200.E+3,
                          NU=.3,
                          ALPHA=10.0E-6),
                  TRACTION=_F(SIGM=CTRACB),
                  THER=_F(LAMBDA=1.0E-3,
                          RHO_CP=0.0E-3));

#

CM=AFFE_MATERIAU(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         MATER=MAT));

#

T0=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
              OPERATION='AFFE',
              MAILLAGE=MAIL,
              AFFE=_F(TOUT='OUI',
                      NOM_CMP='TEMP',
                      VALE=0.0));

L_REEL=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=(_F(JUSQU_A=66.666E0,
                                     NOMBRE=1),
                                  _F(JUSQU_A=80.000E0,
                                     NOMBRE=2),
                                  _F(JUSQU_A=90.000E0,
                                     NOMBRE=2)));

L_INST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                   LIST_INST=L_REEL),
                      ECHEC=_F(SUBD_METHODE='MANUEL'));

#

TIMPVAR=DEFI_FONCTION(NOM_PARA='INST',NOM_RESU='TEMP',VALE=(0.E0,0.E0,
                            100.E0,100.E0,
                            ));

#

MOTHER=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='THERMIQUE',
                           MODELISATION='AXIS'));

#

MOMECA=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='AXIS_INCO_GD'));

#

CHTHER=AFFE_CHAR_THER_F(MODELE=MOTHER,
                        TEMP_IMPO=(_F(GROUP_NO='GRNO1',
                                      TEMP=TIMPVAR),
                                   _F(GROUP_NO='GRNO2',
                                      TEMP=TIMPVAR),
                                   _F(GROUP_NO='GRNO3',
                                      TEMP=TIMPVAR),
                                   _F(GROUP_NO='GRNO4',
                                      TEMP=TIMPVAR)));

#

TEMPE=THER_LINEAIRE(MODELE=MOTHER,
                    CHAM_MATER=CM,
                    EXCIT=_F(CHARGE=CHTHER),
                    INCREMENT=_F(LIST_INST=L_REEL),
                    ETAT_INIT=_F(CHAM_NO=T0));

CHMECA=AFFE_CHAR_MECA(MODELE=MOMECA,
                      DDL_IMPO=(_F(GROUP_NO='GRNO1',
                                   DY=0.),
                                _F(GROUP_NO='GRNO3',
                                   DY=0.)));

CTM=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          MATER=MAT),
                  AFFE_VARC=_F(TOUT='OUI',
                               NOM_VARC='TEMP',
                               EVOL=TEMPE,
                               NOM_CHAM='TEMP',
                               VALE_REF=0.));

U=STAT_NON_LINE(MODELE=MOMECA,
                CHAM_MATER=CTM,
                EXCIT=_F(CHARGE=CHMECA),
                COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',
                             DEFORMATION='SIMO_MIEHE',
                             ),
                INCREMENT=_F(LIST_INST=L_INST),
                CONVERGENCE=_F(ITER_GLOB_MAXI=10),
                SOLVEUR=_F(METHODE='MUMPS',
   NPREC=-1));

U=CALC_CHAMP(reuse=U,RESULTAT=U,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO'))


#

TEST_RESU(RESU=(_F(RESULTAT=TEMPE,
                   NUME_ORDRE=5,
                   NOM_CHAM='TEMP',
                   NOM_CMP='TEMP',
                   GROUP_NO='A',
                   VALE=90.0),
                _F(RESULTAT=U,
                   NUME_ORDRE=1,
                   NOM_CHAM='EPSI_ELNO',
                   NOM_CMP='EPXX',
                   MAILLE='M5',
                   NOEUD='N3',
                   VALE=0.00086666,
                   PRECISION=0.0015),
                _F(RESULTAT=U,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIYY',
                   MAILLE='M5',
                   NOEUD='N3',
                   VALE=-133.333,
                   PRECISION=0.004),
                _F(RESULTAT=U,
                   NUME_ORDRE=3,
                   NOM_CHAM='EPSI_ELNO',
                   NOM_CMP='EPZZ',
                   MAILLE='M5',
                   NOEUD='N4',
                   VALE=0.0011,
                   PRECISION=0.002),
                _F(RESULTAT=U,
                   NUME_ORDRE=3,
                   NOM_CHAM='VARI_ELNO',
                   NOM_CMP='V1',
                   MAILLE='M5',
                   NOEUD='N4',
                   VALE=0.0003,
                   PRECISION=0.005),
                _F(RESULTAT=U,
                   NUME_ORDRE=3,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIYY',
                   MAILLE='M5',
                   NOEUD='N4',
                   VALE=-100.0,
                   PRECISION=0.004),
                _F(RESULTAT=U,
                   NUME_ORDRE=5,
                   NOM_CHAM='EPSI_ELNO',
                   NOM_CMP='EPZZ',
                   MAILLE='M5',
                   NOEUD='N1',
                   VALE=0.001275,
                   PRECISION=0.002),
                _F(RESULTAT=U,
                   NUME_ORDRE=5,
                   NOM_CHAM='VARI_ELNO',
                   NOM_CMP='V1',
                   MAILLE='M5',
                   NOEUD='N1',
                   VALE=0.000525,
                   PRECISION=0.004),
                _F(RESULTAT=U,
                   NUME_ORDRE=5,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIYY',
                   MAILLE='M5',
                   NOEUD='N1',
                   VALE=-75.0,
                   PRECISION=5.E-3)));

#

FIN();

