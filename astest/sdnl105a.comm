#

CHARTRAN=AFFE_CHAR_MECA(MODELE=BARRE,
                        FORCE_NODALE=_F(NOEUD='N8',
                                        FY=1000.,),);

#

K_ELEM=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                      MODELE=BARRE,
                      CHAM_MATER=CHMAT,
                      CARA_ELEM=CARA,
                      CHARGE=GUIDAGE,);

