                             MATER=MATERIO1,),
                          _F(GROUP_MA='GROUP_MA_RIGIDE',
                             MATER=MATERIO2,),),);

#

BARRE=AFFE_MODELE(MAILLAGE=MAYA,
                  AFFE=_F(GROUP_MA='GROUP_MA_TOUT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='POU_D_E',),);

#

CARA=AFFE_CARA_ELEM(MODELE=BARRE,
                    POUTRE=_F(GROUP_MA='GROUP_MA_TOUT',
                              SECTION='CERCLE',
                              CARA=('R','EP',),
                              VALE=(0.1,0.01,),),);

#

