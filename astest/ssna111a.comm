DEBUT(CODE=_F(NOM='SSNA111A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'))



MA=LIRE_MAILLAGE(FORMAT='MED',);


MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOM='GM12',
                                GROUP_MA='GM12',),
                             ),);


MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

# On reoriente les group_ma GM11 et GM12 qui
# interviennent dans le contact car leurs normales
# doivent absolument etre sortantes
MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=(_F(GROUP_MA=('GM11')),
                               _F(GROUP_MA=('GM12'))),
                 );


MAT1=DEFI_MATERIAU(ELAS=_F(E=2.E5,
                           NU=0.3,),
                   ECRO_LINE=_F(D_SIGM_EPSI=5000.0,
                                SY=300.0,),
                                );

MAT2=DEFI_MATERIAU(ELAS=_F(E=2.E9,
                           NU=0.3,),);


CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA=('GM15'),
                             MATER=MAT1,),
                          _F(GROUP_MA=('GM17',),
                             MATER=MAT2,),
                             ),);

CHA=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_MA='GM14',
                                DX= 0.0,
                                DY= 0.0,),
                             _F(GROUP_MA=('GM9','GM11',),
                                DX=0.0,
                                DY=-0.4,),
                             _F(GROUP_MA=('GM10',),
                                DY=-0.4,),
                                ),);

CONT = DEFI_CONTACT   (MODELE      = MO,
                       FORMULATION = 'DISCRETE',
                       ZONE =_F(
                                 APPARIEMENT='MAIT_ESCL',
                                 GROUP_MA_MAIT='GM11',
                                 GROUP_MA_ESCL='GM12',
                                 NORMALE='MAIT_ESCL',
                               ),
                      );

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=50,),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST,),
                        ECHEC=_F(ACTION='DECOUPE',
                                 SUBD_METHODE='MANUEL',
                                 SUBD_PAS=4,
                                 SUBD_PAS_MINI=1.E-4,),)

F=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,  0.0,
                      0.8,  1.0,
                      1.0,  0.98),
                      );

RESU=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CHA,
                            FONC_MULT=F,),
                   CONTACT = CONT,
                   COMP_INCR=(
                              _F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='SIMO_MIEHE',
                                GROUP_MA='GM15',
                                ),
                              _F(RELATION='ELAS',
                                GROUP_MA='GM17',),
                                  ),
                   CONVERGENCE=(_F(ITER_GLOB_MAXI=20,)),
                   INCREMENT=_F(LIST_INST=DEFLIST,
                                INST_FIN=1.,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             PREDICTION='ELASTIQUE',
                             REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=L_INST,),);


# On calcule la force de reaction du massif
# attention : la force est en n/radian
RESU=CALC_CHAMP(reuse =RESU,


             FORCE='REAC_NODA',
             RESULTAT=RESU,);

TAB_FORC=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                            GROUP_NO='GM12',
                            RESULTAT=RESU,
                            NOM_CHAM='REAC_NODA',
                            TOUT_ORDRE='OUI',
                            RESULTANTE='DY',
                            OPERATION='EXTRACTION',),);

TEST_RESU(RESU=(_F(GROUP_NO='GM16',
                   INST=0.5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.247980070,
                   VALE_REFE=-0.25,
                   PRECISION=8.9999999999999993E-3,),
                _F(GROUP_NO='GM16',
                   INST=0.5,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.247980070,
                   VALE_REFE=-0.24987860862094999,
                   PRECISION=1.E-2, TOLE_MACHINE=(1.E-2, 1.E-3),
                   ),
                _F(GROUP_NO='GM16',
                   INST=0.5,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=RESU,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   VALE_CALC=3.248040141,
                   VALE_REFE=3.2482000000000002,
                   PRECISION=1.E-2, TOLE_MACHINE=(1.E-2, 1.E-3),
                   ),
                _F(GROUP_NO='GM16',
                   INST=0.5,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=RESU,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=-8.769704784,
                   VALE_REFE=-8.7703000000000007,
                   PRECISION=1.E-2, TOLE_MACHINE=(1.E-2, 1.E-3),
                   ),
                ),
          )

TEST_RESU(RESU=(_F(GROUP_NO='GM16',
                   INST=1.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.389440536,
                   VALE_REFE=-0.39200000000000002,
                   PRECISION=7.0000000000000001E-3,),
                _F(GROUP_NO='GM16',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.389440536,
                   VALE_REFE=-0.38944000000000001,
                   PRECISION=1.E-2, TOLE_MACHINE=(1.E-2, 1.E-3),
                   ),
                _F(GROUP_NO='GM16',
                   INST=1.0,
                   RESULTAT=RESU,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   VALE_CALC=1.8135741352096,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(GROUP_NO='GM16',
                   INST=1.0,
                   RESULTAT=RESU,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=-4.8966501652687002,
                   TOLE_MACHINE=1.E-3,
                   ),
                ),
          )

FY=RECU_FONCTION(TABLE=TAB_FORC,
                 PARA_X='INST',
                 PARA_Y='DY')

DY=RECU_FONCTION(RESULTAT=RESU,
                 NOM_CHAM='DEPL',
                 NOM_CMP='DY',
                 GROUP_NO='GM16')

IMPR_FONCTION(FORMAT='AGRAF',
            LEGENDE_X='Deplacement (mm)',
            LEGENDE_Y='Force (N/rd)',
            COURBE=(_F(FONC_X = DY,
                       FONC_Y = FY,)));

RNY=RECU_FONCTION(RESULTAT=RESU,
                 NOM_CHAM='REAC_NODA',
                 NOM_CMP='DY',
                 GROUP_NO='GM16')

FIN();
