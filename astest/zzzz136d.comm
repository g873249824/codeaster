DEBUT(CODE=_F(NOM='ZZZZ136D',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED')

MA=DEFI_GROUP(reuse=MA,MAILLAGE=MA,
              CREA_GROUP_MA=(_F(UNION=('GAE','GEB'),NOM='GAB'),),
              CREA_GROUP_NO=(_F(GROUP_MA='GEF'),
                             _F(GROUP_MA='GAB'),),)

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       MODELISATION='D_PLAN',
                       PHENOMENE='MECANIQUE'),)

MAT=DEFI_MATERIAU(ELAS=_F(NU=0.3,E=2.1e+11),)

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',MATER=MAT),)

CH=AFFE_CHAR_MECA(MODELE=MO,
                  LIAISON_GROUP=(_F(GROUP_MA_1='GEF',GROUP_MA_2='GFE',DDL_1='DNOR',COEF_MULT_1=1.0,DDL_2='DNOR',COEF_MULT_2=-1.0,COEF_IMPO=.0,
                  ),),
                  DDL_IMPO=(_F(GROUP_MA='GDA',DX=0,DY=0),
                            _F(GROUP_MA='GBC',DY=0),
                            _F(GROUP_MA='GBC',DX=.1),
                            )),


RESU=MECA_STATIQUE(EXCIT=_F(CHARGE=CH),
                   MODELE=MO,
                   CHAM_MATER=CHMAT)

CALC_ELEM(reuse=RESU,RESULTAT=RESU,OPTION=('SIGM_ELNO','SIEQ_ELNO'),)


TB1=POST_RELEVE_T(ACTION=_F(INTITULE='CONTRAINTES AB',
                            OPERATION='EXTRACTION',
                            RESULTAT=RESU,
                            NOM_CHAM='SIGM_ELNO',
                            NOM_CMP='SIXX',
                            GROUP_NO='GAB'),)

TB2=POST_RELEVE_T(ACTION=_F(INTITULE='TRESCA MOYEN EF',
                            OPERATION='MOYENNE',
                            RESULTAT=RESU,
                            NOM_CHAM='SIEQ_ELNO',
                            NOM_CMP='TRESCA',
                            GROUP_NO='GEF'),)

TB3=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EF REPERE UTILISATEUR',
                            REPERE='UTILISATEUR',
                            ANGL_NAUT=(60.0,0,0),
                            OPERATION='EXTRACTION',
                            RESULTAT=RESU,
                            NOM_CHAM='DEPL',
                            NOM_CMP='DX',
                            GROUP_NO='GEF'),)

IMPR_TABLE(TABLE=TB1)

TEST_TABLE( TABLE=TB1,
           FILTRE=_F( NOM_PARA = 'ABSC_CURV', VALE = 47.2783),
           NOM_PARA='SIXX',
           PRECISION=0.001,
           VALE=6.8481239601528E+08,
           REFERENCE='NON_REGRESSION')

IMPR_TABLE(TABLE=TB2)

TEST_TABLE( TABLE=TB2,
            FILTRE=_F( NOM_PARA = 'QUANTITE', VALE_K = 'MINIMUM'),
            NOM_PARA='TRESCA',
            PRECISION=0.001,
            VALE=2.1823374017079E+08,
            REFERENCE='NON_REGRESSION')

IMPR_TABLE(TABLE=TB3)      

TEST_TABLE( TABLE=TB3,
            FILTRE=_F( NOM_PARA = 'ABSC_CURV', VALE = 4.91197),
            NOM_PARA='DX',
            PRECISION=0.001,
            VALE=0.06461675,
            REFERENCE='NON_REGRESSION')

FIN()
