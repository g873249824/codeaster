                              SUBD_COEF_PAS_1=1.0,),
                 NEWTON=_F(REAC_INCR=1,
                           MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                ITER_GLOB_MAXI=10,
                                ARRET='OUI',),
                 ARCHIVAGE=_F(LIST_INST=L_ARCH_R,),);

UR=CALC_ELEM(reuse =UR,
             RESULTAT=UR,
             OPTION='SIEF_ELNO_ELGA',);

UR=CALC_NO(reuse =UR,
           RESULTAT=UR,
           OPTION='REAC_NODA');

