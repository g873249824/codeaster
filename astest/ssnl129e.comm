             OPTION='SIEF_ELNO_ELGA',);

UM=CALC_NO(reuse =UM,
           RESULTAT=UM,
           OPTION='REAC_NODA');
         
# ------------------------------------------------------------

#  TROISIEME CALCUL : VITESSE RAPIDE  10E3 s-1
