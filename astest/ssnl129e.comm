#     MODELISATION D_PLAN
DEBUT(CODE=_F( NOM = 'SSNL129E',NIV_PUB_WEB='INTERNET'));

PRE_GIBI();

MAIL=LIRE_MAILLAGE();

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='L12',),
                               _F(GROUP_MA='L23',),
                               _F(GROUP_MA='L41',) ,),);

MO=AFFE_MODELE(MAILLAGE=MAIL,
               INFO=2,
               VERIF='MAILLE',
               AFFE=_F(GROUP_MA='MAILLAGE',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN',),);
                       
# DEFINITION & AFFECTATION DES PROPRIETES MECANIQUES
# ----------------------------------------------------------------
#xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx#
# Courbe Sigma=f(epsilon_totale) du CEA
#xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx#

SIGM_F=DEFI_FONCTION(NOM_PARA='EPSI',
                     VALE=
                    (0.002409731,477.1267177,
                     1.002586229,1007.073281,),
                     PROL_DROITE='LINEAIRE',);

MA_EPR=DEFI_MATERIAU(ELAS=_F(E=215000.0,
                             NU=0.3,
                             ALPHA=0.0,),
                     ECRO_LINE=_F(  D_SIGM_EPSI = 529.853045,
                                    SY = 477.1267117),
                     VISC_SINH=_F(
                                   SIGM_0=6176.,
                                   EPSI_0=3.31131121483e13,
                                   M=6.76,),
                     TRACTION=_F(SIGM=SIGM_F,)  );

CM=AFFE_MATERIAU(MAILLAGE=MAIL,
                 AFFE=(_F(TOUT='OUI',
                          MATER=MA_EPR,),),);
CH=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_NO='L12',
                               DX=0.0,),
                            _F(GROUP_NO='L41',
                               DY=0.0,),),);
CHT=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=_F(GROUP_NO='L23',
                                DY=1.),);
# ------------------------------------------------------------

#  PREMIER CALCUL : VITESSE LENTE  10-3 s-1
# ------------------------------------------------------------

IFIN = 2000.0;

FCT_L=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,IFIN,2.0,),
                    PROL_DROITE='EXCLU',
                    PROL_GAUCHE='EXCLU',);

L_INSTL=DEFI_LIST_REEL(DEBUT=0.0,
                       INTERVALLE=(_F(JUSQU_A=IFIN,
                                      NOMBRE=100,),),);

L_ARCH_L=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=IFIN,
                                      NOMBRE=20,),);

UL=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CM,
                 EXCIT=(_F(CHARGE=CH,),
                        _F(CHARGE=CHT,
                           FONC_MULT = FCT_L),),
                 COMP_INCR=_F(RELATION='VISC_ISOT_LINE',
                              DEFORMATION='SIMO_MIEHE',
                              RESI_INTE_RELA=1e-09,
                              ITER_INTE_MAXI=10,),
                 INCREMENT=_F(LIST_INST=L_INSTL,
                              SUBD_METHODE='UNIFORME', SUBD_PAS=3,
                              SUBD_PAS_MINI=1e-10,
                              SUBD_COEF_PAS_1=1.0,),
                 NEWTON=_F(REAC_INCR=1,
                           MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                ITER_GLOB_MAXI=10,
                                ARRET='OUI',),
                 ARCHIVAGE=_F(LIST_INST=L_ARCH_L,),);

UL=CALC_ELEM(reuse =UL,
             RESULTAT=UL,
             OPTION='SIEF_ELNO_ELGA',);

UL=CALC_NO(reuse =UL,
           RESULTAT=UL,
           OPTION='REAC_NODA',);
