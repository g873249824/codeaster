#Déformation totale
EPS13 = CREA_CHAMP(TYPE_CHAM='NOEU_EPSI_R', OPERATION='EXTR', RESULTAT=VL, NOM_CHAM='EPSI_NOEU',INST=1.3);

EPSY13=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='NOEU_NEUT_R',
                    MODELE=MO,
                    ASSE=_F(CHAM_GD=EPS13 ,
                            TOUT='OUI',
                            NOM_CMP  =('EPYY',),
                            NOM_CMP_RESU=('X1'),   ),)

#Déformation thermique
VAR13 = CREA_CHAMP(TYPE_CHAM='NOEU_VAR2_R', OPERATION='EXTR', RESULTAT=VL, NOM_CHAM='VARI_NOEU',INST=1.3);


VAR713=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='NOEU_NEUT_R',
                    MODELE=MO,
                    ASSE=_F(CHAM_GD=VAR13 ,
                            TOUT='OUI',
                            NOM_CMP  =('V7',),
                            NOM_CMP_RESU=('X1'),   ),)
#Déformation mécanique = Déformation totale - Déformation thermique
EPM13 = CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='NOEU_NEUT_R',
                    MODELE=MO,
                    ASSE=(_F(CHAM_GD=EPSY13 ,
                            TOUT='OUI',  
                            CUMUL='OUI',COEF_R=1
                                ),
                            _F(CHAM_GD=VAR713 ,
                            TOUT='OUI',
                            CUMUL='OUI',COEF_R=-1
                                ),
                            )
                    )
