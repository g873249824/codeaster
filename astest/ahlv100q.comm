         MODELE=MO,
         VITE_FACE=_F( GROUP_MA = 'velo',
                   VNOR = ('RI',0.014,0.0,))
         )

IMPE=AFFE_CHAR_ACOU(
         MODELE=MO,
         IMPE_FACE=_F( GROUP_MA = 'impe',
                   IMPE = ('RI',445.9,0.0,))
         )

