                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                     TABLE = DEP_NFI,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^P]')));

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                     TABLE = DEP_NFI,
                     ACTION=_F(OPERATION='TRI',
                               NOM_PARA='COOR_X',
                               ORDRE='CROISSANT'));

IMPR_TABLE(TABLE=DEP_NFI);

VC_PPR=POST_RELEVE_T(ACTION=_F(INTITULE='CONT_NOEU',
                               GROUP_NO=('PPR'),
                               RESULTAT=UTOT1,
                               NOM_CHAM='CONT_NOEU',
                               INST=1.0,
                               TOUT_CMP='OUI',
                               OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=VC_PPR);

