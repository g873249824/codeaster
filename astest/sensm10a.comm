DEBUT(CODE=_F( NOM = 'SENSM10A',NIV_PUB_WEB='INTERNET'),
          BASE=_F( FICHIER = 'GLOBALE',LONG_REPE = 15000)  )
#
#......................................................................
# COUDE SOUS PRESSION EN 3D
# (CHARGE - DECHARGE PARTIELLE - CHARGE)
# PLASTICITE VON-MISES A ECROUISSAGE ISOTROPE LINEAIRE
# SENSIBILITE AUX PARAMETRES MATERIAU ET CHARGEMENT (PRESSION)
#......................................................................
#

MAIL=LIRE_MAILLAGE();

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='ENCAST',),
                               _F(GROUP_MA='SYMETRIE',),
                               _F(GROUP_MA='SURFINT',),
                               _F(GROUP_MA='SURFEXT',),),);

# DEFINITION DES ELEMENTS FINIS UTILISES POUR LE CALCUL MECANIQUE
#


MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_3D=_F(GROUP_MA='SURFINT',),
                  );

# DEFINITION DU MATERIAU UTILISE
#

PS_DX =DEFI_PARA_SENSI(VALE=0.00000,);
PS_DY =DEFI_PARA_SENSI(VALE=0.00000,);
DY_FOND =DEFI_PARA_SENSI(VALE=0.10000,);

PS_E1 = DEFI_PARA_SENSI(VALE=210000.0,);
PS_E2 = DEFI_PARA_SENSI(VALE=210000.0,);

PS_SIGY = DEFI_PARA_SENSI(VALE=200.0,);

PS_DSDE = DEFI_PARA_SENSI(VALE=2000.0,);

NU = DEFI_CONSTANTE(VALE=0.3,);

ACIER1=DEFI_MATERIAU(ELAS_FO=_F(E=PS_E1,
                            NU=NU,),
                    ECRO_LINE_FO=_F(D_SIGM_EPSI=PS_DSDE,
                                 SY=PS_SIGY,),);

ACIER2=DEFI_MATERIAU(ELAS_FO=_F(E=PS_E2,
                            NU=NU,),
                    ECRO_LINE_FO=_F(D_SIGM_EPSI=PS_DSDE,
                                 SY=PS_SIGY,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=(_F(GROUP_MA='TUY1',MATER=ACIER1,),
                        _F(GROUP_MA='TUY2',MATER=ACIER2,))
                       );

# DEFINITION DES CONDITIONS AUX LIMITES
#    ENCASTREMENT SUR LA SECTION ENCAST
#    SYMETRIE PAR RAPPORT AU PLAN XOY
#    CHARGEMENT THERMIQUE
#
#
PSP = DEFI_PARA_SENSI(VALE=2.5,);



CHARGT=AFFE_CHAR_MECA(MODELE=MODMECA,
                      DDL_IMPO=(_F(GROUP_NO='ENCAST',
                                   DX=0.0,
#                                   DY=0.0,
                                   DZ=0.0,),
                                _F(GROUP_NO='SYMETRIE',
                                   DZ=0.0,),),
                                   );
                                  
                                  
CH_DIR1=AFFE_CHAR_MECA_F(MODELE=MODMECA,
                   DDL_IMPO=(_F(GROUP_NO='ENCAST',
                                DY=PS_DY,),
                            ),
                       );
CH_DIR2=AFFE_CHAR_MECA_F(MODELE=MODMECA,
                      FACE_IMPO=_F(GROUP_MA='EFOND',
                                   DY=DY_FOND,),
                       );


CHARGD=AFFE_CHAR_MECA_F(MODELE=MODMECA,
                      PRES_REP=_F(GROUP_MA='SURFINT',
                                  PRES=PSP,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
  VALE=(0.0,0.0,0.4,1.0,0.6,0.0,1.0,1.0),);


toto = 1.

RAMPED=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,0.0,1.0,toto),);

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=
                            (
                      _F(JUSQU_A=0.2, NOMBRE=1,),
                      _F(JUSQU_A=0.4, NOMBRE=2,),
                      _F(JUSQU_A=0.42, NOMBRE=1,),
                      _F(JUSQU_A=0.6, NOMBRE=2,),
                      _F(JUSQU_A=1.0, NOMBRE=4,),
                             ),
                         );

EVOL=STAT_NON_LINE(MODELE=MODMECA,
            CHAM_MATER=CHMAT,
            EXCIT=(_F(CHARGE=CHARGT,
                            FONC_MULT=RAMPE,),
                          _F(CHARGE=CH_DIR1,
                            FONC_MULT=RAMPE,),
                          _F(CHARGE=CH_DIR2,
                            FONC_MULT=RAMPE,),
                          _F(CHARGE=CHARGD,
                            FONC_MULT=RAMPED,),
                         ),
            COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
            INCREMENT=_F(LIST_INST=INSTANTS,
                                NUME_INST_FIN=10,),
            NEWTON=_F(REAC_INCR=1,
                             REAC_ITER=1,),
            CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
                                  ITER_GLOB_MAXI=20,),
            SENSIBILITE=(PSP,PS_E1,PS_E2,PS_SIGY,PS_DSDE,PS_DY,DY_FOND),);

EVOL = CALC_NO( reuse      =  EVOL         ,
              RESULTAT   =  EVOL         ,
              MODELE     =  MODMECA     ,
              CHAM_MATER =  CHMAT      ,
              OPTION     = ('REAC_NODA',),);

EVOL = CALC_NO( reuse      =  EVOL         ,
              RESULTAT   =  EVOL         ,
              MODELE     =  MODMECA     ,
              CHAM_MATER =  CHMAT      ,
              OPTION     = ('REAC_NODA',),
              SENSIBILITE = (PSP,PS_SIGY,PS_DSDE)
              );



