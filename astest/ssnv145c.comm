DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
#
# MODELISATION : COQUE_3D
#
# MAILLAGE     : MEC3TR7H
# PRESSION SUIVEUSE COQUE 3D SOUR FORME DE FONCTION
#

MA1=LIRE_MAILLAGE(FORMAT='MED',  )

MA1=DEFI_GROUP( reuse=MA1,   MAILLAGE=MA1,
                     CREA_GROUP_MA=_F(  NOM = 'TOUT',  TOUT = 'OUI'))

#
#

MO1=AFFE_MODELE(  MAILLAGE=MA1,
          AFFE=_F( TOUT = 'OUI', MODELISATION = 'COQUE_3D',
          PHENOMENE = 'MECANIQUE') )

#

MAT=DEFI_MATERIAU(   ELAS=_F(  E = 12.E6,  NU = 0.3,  ALPHA = 0.)
                        )

#

CHMAT1=AFFE_MATERIAU(   MAILLAGE=MA1,
                          AFFE=_F(  TOUT = 'OUI',  MATER = MAT)
                        )

#
#

CHA1=AFFE_CHAR_MECA(   MODELE=MO1,

                          DDL_IMPO=_F( GROUP_NO = 'G_41',  DX = 0., DY = 0., DZ = 0.,
                                            DRX = 0., DRY = 0., DRZ = 0.)
                        )

#

PRE1=DEFI_FONCTION(  NOM_PARA='X',  VALE=(-0.1, 1.,  10.1, 1.,),
                    PROL_GAUCHE='CONSTANT',PROL_DROITE='CONSTANT', )

CHA2=AFFE_CHAR_MECA_F(         MODELE=MO1,
                FORCE_COQUE=_F(  GROUP_MA='G_1',   PRES = PRE1)
                        )

CHA3=AFFE_CHAR_MECA(         MODELE=MO1,
                FORCE_COQUE=_F(  GROUP_MA='G_2',   PRES = 1.)
                        )


CARA1=AFFE_CARA_ELEM(
                               MODELE=MO1,

                                     COQUE=_F(
                                       GROUP_MA = 'TOUT',
                                       EPAIS = 0.1,
                                       COEF_RIGI_DRZ = 1.E-4)
                          )

#
#
########################################################################
#
#

L_INST=DEFI_LIST_REEL(   DEBUT=0.0,
                             INTERVALLE=_F(  JUSQU_A = 22.,  NOMBRE = 10)
                         )

#

D_FONC=DEFI_FONCTION(
                            NOM_PARA='INST',
                                VALE=(
                                        0.,    0.,
                                       1000., 1000.,
                                     )
                         )

#
#

NONLI=STAT_NON_LINE(
                            MODELE=MO1,
                        CHAM_MATER=CHMAT1,
                         CARA_ELEM=CARA1,EXCIT=(
                             _F(
                CHARGE = CHA1),
                _F(
                CHARGE = CHA2,
                FONC_MULT = D_FONC,
                TYPE_CHARGE = 'SUIV'),
                _F(
                CHARGE = CHA3,
                FONC_MULT = D_FONC,
                TYPE_CHARGE = 'SUIV'),
                ),
                         COMPORTEMENT=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'GROT_GDEP'),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 50),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                REAC_ITER = 1,
                MATRICE = 'TANGENTE')
              )

