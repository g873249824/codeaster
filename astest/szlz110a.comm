PRE_IDEAS();

MAILLAGE=LIRE_MAILLAGE();

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=(_F(GROUP_MA='BLOC_X',
                                      NOM='NBLOC_X',
                                      CRIT_NOEUD='TOUS',),
                                   _F(GROUP_MA='BLOC_Y',
                                      NOM='NBLOC_Y',
                                      CRIT_NOEUD='TOUS',),
                                   _F(GROUP_MA='BLOC_Z',
                                      NOM='NBLOC_Z',
                                      CRIT_NOEUD='TOUS',),),);

#////////////////////////////////////////////////////////////////////////////////////
#                      C A L C U L   T H E R M I Q U E
#////////////////////////////////////////////////////////////////////////////////////
# ==================================================
# CARACTERISTIQUES THERMOELASTIQUES DU MATERIAU
#  <RHO>, <RHO_CP>, <LAMBDA>, <YOUNG>, <NU>, <ALPHA>
# ==================================================
# RHO = CONSTANTE
# DENSITE VOLUMIQUE [KG/M3]
# ------------------------------------
RHO=7695.0;
RHO_CSTE=DEFI_CONSTANTE(NOM_RESU='TOUTRESU',
                        VALE=RHO,);

# RHO_CP EN FONCTION DU TEMPS
# CAPACITE CALORIFIQUE VOLUMIQUE [J/M3.K]
# ----------------------------------------
ROCP=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,5000000.0,240.0,5000000.0,510.0,4800000.0,615.0,4300000.0,1050.0,4800000.0,1560.0,5100000.0),
                   INTERPOL='LIN',
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='CONSTANT',);

# LAMBDA EN FONCTION DU TEMPS
# CONDUCTIVITE THERMIQUE [W/M.K]
# ------------------------------------
LAMBDA=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=
                     (0.0,31.0,240.0,31.0,510.0,31.5,615.0,35.799999999999997,1050.0,31.800000000000001,
                            1560.0,30.699999999999999),
                     INTERPOL='LIN',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',);

# YOUNG(E) 545C
# MODULE D'YOUNG [PA]
# ------------------------------------
YOUNG=DEFI_FONCTION(NOM_PARA='TEMP',
                    VALE=(545.0,143006.0E+6,),
                    INTERPOL='LIN',
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

# NU 545C
# COEFFICIENT DE POISSON [DIMENSIONLESS]
# ------------------------------------
NU=DEFI_FONCTION(NOM_PARA='TEMP',
                 PROL_DROITE='CONSTANT',
                 PROL_GAUCHE='CONSTANT',
                 VALE=(545.0,0.33,),);

# ALPHA EN FONCTION DE LA TEMPERATURE
# COEFFICIENT DE DILATATION MOYEN [1/K]
# ------------------------------------
ALPHA1=DEFI_FONCTION(NOM_PARA='TEMP',
                    VALE=
                    (100.0,9.80E-06,
                     200.0,1.12E-05,
                     300.0,1.20E-05,
                     400.0,1.24E-05,
                     500.0,1.27E-05,
                     600.0,1.31E-05),
                    INTERPOL='LIN',
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

ALPHA=DEFI_CONSTANTE(VALE=0.0)

# =====================================================
# CARACTERISTIQUES PLASTIQUES DU MATERIAU CIN2_CHAB_FO
#  <R0>, <BE>, <RINF>, <KETW>, <GAMA>, <XINF1>, <XINF2>
# =====================================================
# R0 545C
# LIMITE ELASTIQUE (DONNEE LMT CACHAN) [PA]
# -----------------------------------------
R0=DEFI_FONCTION(NOM_PARA='TEMP',
                 VALE=(545.0, 96.97062389E+6,),
                 PROL_DROITE='CONSTANT',
                 PROL_GAUCHE='CONSTANT',);

# PETITB 545C
# COEFFICIENT B [DIMENSIONLESS]
# --------------------------------
PETITB=DEFI_FONCTION(NOM_PARA='TEMP',
                 VALE=(545.0,0.5057370491E-1,),
                 PROL_DROITE='CONSTANT',
                 PROL_GAUCHE='CONSTANT',);

# RINF 545C
# LIMITE D'ECROUISSAGE ISOTROPE [PA]
# ----------------------------------
RINF=DEFI_FONCTION(NOM_PARA='TEMP',
                   VALE=(545.0,0.1990869913,),
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='CONSTANT',);

# PETITK 545C
# COEFFICIENTS DE L'ECROUISSAGE CINEMATIQUE NON-LINEAIRE, K ET W [DIMENSIONLESS]
# ------------------------------------------------------------------------------
PETITK=DEFI_FONCTION(NOM_PARA='TEMP',
                     VALE=(545.0,16.76114200,),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',);

# PETITW 545C
# COEFFICIENTS DE L'ECROUISSAGE CINEMATIQUE NON-LINEAIRE, K ET W [DIMENSIONLESS]
# ------------------------------------------------------------------------------
PETITW=DEFI_FONCTION(NOM_PARA='TEMP',
                     VALE=(545.0,-0.1200711719,),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',);

# GAMA1 545C
# COEFFICIENT DE NON-LINEARITE, A1 ET A2 [DIMENSIONLESS]
# ------------------------------------------------------
GAMA1=DEFI_FONCTION(NOM_PARA='TEMP',
                    VALE=(545.0,223.3767118,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

# GAMA2 545C
# COEFFICIENT DE NON-LINEARITE, A1 ET A2 [DIMENSIONLESS]
# ------------------------------------------------------
GAMA2=DEFI_FONCTION(NOM_PARA='TEMP',
                    VALE=(545.0,1.736095686,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

# C1INF 545C
# LIMITE D'ECROUISSAGE CINEMATIQUE C1 [PA]
# ----------------------------------------
C1INF=DEFI_FONCTION(NOM_PARA='TEMP',
                    VALE=(545.0,627.1347233E+6,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

# C2INF 545C
# LIMITE D'ECROUISSAGE CINEMATIQUE C2 [PA]
# ----------------------------------------
C2INF=DEFI_FONCTION(NOM_PARA='TEMP',
                    VALE=(545.0,3.355557795E+6,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

# AINF 545C
# COEFFICIENT A_INF [DIMENSIONLESS]
# ---------------------------------
AINF=DEFI_FONCTION(NOM_PARA='TEMP',
                  VALE=(545.0,4.932634278,),
                  PROL_DROITE='CONSTANT',
                  PROL_GAUCHE='CONSTANT',);

# =====================================================
# CARACTERISTIQUES VISQUEUSES DU MATERIAU LEMAITRE
#  <EXP_N>, <UNSURK>, <UNSURM>
# =====================================================
# EXP_N 545C
# EXPOSANT N DANS LA LOI DE NORTON [DIMENSIONLESS] - IDENTIFICATION S. LECLERCQ (EDF R&D - MMC)
# ---------------------------------------------------------------------------------------------
EXP_N=DEFI_FONCTION(NOM_PARA='TEMP',
                    VALE=(545.0,3.698687712,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

# UNSURETA 545C
# FACTEUR 1/ETA DANS LA LOI DE NORTON [MPA.S-1] - IDENTIFICATION S. LECLERCQ (EDF R&D - MMC)
# ETA = 278.5754646 MPA.S-1
# ---------------------------------------------------------------------------------------------
ETA=710.1811270E+6;
INVETA=1./ETA;
UNSURETA=DEFI_FONCTION(NOM_PARA='TEMP',
                       VALE=(545.0,INVETA,),
                       PROL_DROITE='CONSTANT',
                       PROL_GAUCHE='CONSTANT',);

# UNSURM 545C
# EXPOSANT 1/M DANS LA LOI DE NORTON [DIMENSIONLESS] - IDENTIFICATION S. LECLERCQ (EDF R&D - MMC)
# ATTN CETTE VALEUR DOIT ETRE ASSIGNEE A 0 (D'APRES NOTES DE PH. DE BONNIERES)
# ---------------------------------------------------------------------------------------------
UNSURM=DEFI_FONCTION(NOM_PARA='TEMP',
                     VALE=(0.0,0.0,),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',);

# T_S
# COEFFICIENT S PRESENT AU NUMERATEUR DE LA LOI DE LEMAITRE_SERMAGE
# -----------------------------------------------------------------
T_S=DEFI_FONCTION(NOM_PARA='TEMP',
                  PROL_DROITE='CONSTANT',
                  PROL_GAUCHE='CONSTANT',
                  VALE=(20., 7.0,),);

# EPSP_S
# VALEUR DU SEUIL DE PLASTICITE
# -----------------------------
EPSP_S=DEFI_FONCTION(NOM_PARA='TEMP',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     VALE=(20.,1.005E-6,),);

# =======================================================================
# DEFINITION DU MATERIAU THERMO-ELASTO-VISCO-PLASTIQUE
# <P10CD910> ACIER DESIGNATION 10 CD9-10 "CROMESCO 3"
#     (D'APRES LA NORME NFA 36 206)
#     REF. NOTE HP-44/97-048, J.-M. VIALETTES,
#          "CALCULS D'ENDOMMAGEMENT DU COLLECTEUR SHT DE CHAMPAGNE/OISE 2
#          DANS LE CAS D'UN DEMARRAGE, ILOTAGE ET DES PERTURBATIONS"
# =======================================================================
RHOF=DEFI_CONSTANTE(VALE = RHO,)
P10CD910=DEFI_MATERIAU(ELAS_FO=_F(E=YOUNG,
                                  NU=NU,
                                  RHO=RHOF,
                                  TEMP_DEF_ALPHA=25.0,
                                  ALPHA=ALPHA,),
                       CIN2_CHAB_FO=_F(R_0=R0,
                                       R_I=RINF,
                                       B=PETITB,
                                       C1_I=C1INF,
                                       C2_I=C2INF,
                                       K=PETITK,
                                       W=PETITW,
                                       G1_0=GAMA1,
                                       G2_0=GAMA2,
                                       A_I=AINF,),
                       LEMAITRE_FO=_F(N=EXP_N,
                                      UN_SUR_K=UNSURETA,
                                      UN_SUR_M=UNSURM,),
                       DOMMA_LEMAITRE=_F(S = T_S,
                                         EPSP_SEUIL = EPSP_S,
                                         EXP_S = 1.003,),
                       THER_FO=_F(LAMBDA=LAMBDA,
                                  RHO_CP=ROCP,),);

P10CD911=DEFI_MATERIAU(ELAS_FO=_F(E=YOUNG,
                                  NU=NU,
                                  RHO=RHOF,
                                  TEMP_DEF_ALPHA=25.0,
                                  ALPHA=ALPHA,),
                       CIN2_CHAB_FO=_F(R_0=R0,
                                       R_I=RINF,
                                       B=PETITB,
                                       C1_I=C1INF,
                                       C2_I=C2INF,
                                       K=PETITK,
                                       W=PETITW,
                                       G1_0=GAMA1,
                                       G2_0=GAMA2,
                                       A_I=AINF,),
                       LEMAITRE_FO=_F(N=EXP_N,
                                      UN_SUR_K=UNSURETA,
                                      UN_SUR_M=UNSURM,),
                       DOMMA_LEMAITRE=_F(S = T_S,
                                         EPSP_SEUIL = EPSP_S,
                                         EXP_S = 0.8,),
                       THER_FO=_F(LAMBDA=LAMBDA,
                                  RHO_CP=ROCP,),);

# =============================================================
# AFFECTATION DU MATERIAU THERMO-ELASTO-PLASTIQUE AU MODELE FEM
# <MAT_PLAS>
# =============================================================
MAT_PLAS=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       AFFE=_F(TOUT='OUI',
                               MATER=P10CD910,
                               ),);
MAT_PLA1=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       AFFE=_F(TOUT='OUI',
                               MATER=P10CD911,
                               ),);

# ====================================================================
# DEFINITION DES PAS DE TEMPS POUR LA RESOLUTION DU PROBLEME THERMIQUE
# <L_INST> / NOMBRE = 21
# ====================================================================
TPS_FIN=1000.0;
NBRE_INC=20;
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=TPS_FIN,
                                     NOMBRE=NBRE_INC,),),);

# ===============================================
# AFFECTATION DU PHENOMENE PHYSIQUE AU MODELE FEM
# <MOTHER> = THERMIQUE 3D
# ===============================================
MOTHER=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=(_F(TOUT='OUI',
                            PHENOMENE='THERMIQUE',
                            MODELISATION='3D',),),);


# ============================================
# RESOLUTION DU PROBLEME EN THERMIQUE LINEAIRE
# RHO_CP ET LAMBDA DEPENDENT DU TEMPS
# <RESUTHER>
# ============================================
T_CONST = FORMULE(NOM_PARA='INST',VALE='20.0 ');
TEMPCSTE=AFFE_CHAR_THER_F(MODELE=MOTHER,
                          TEMP_IMPO=_F(TOUT='OUI',
                                       TEMP=T_CONST,),);
RESUTHER=THER_LINEAIRE(MODELE=MOTHER,
                       CHAM_MATER=MAT_PLAS,
                       EXCIT=(_F(CHARGE=TEMPCSTE,),),
                       INCREMENT=_F(
                                    LIST_INST=L_INST,
                                   ),
                       ETAT_INIT=_F(STATIONNAIRE='OUI',),
                       ARCHIVAGE=_F(LIST_INST=L_INST,),);

MAT_TH=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       AFFE=_F(TOUT='OUI',
                               MATER=P10CD910),
                       AFFE_VARC=_F( TOUT='OUI',
                                     EVOL=RESUTHER,
                                     NOM_VARC='TEMP',
                                     NOM_CHAM='TEMP',VALE_REF=20.),
                     );

MAT_TH1=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       AFFE=_F(TOUT='OUI',
                               MATER=P10CD911,),
                       AFFE_VARC=_F( TOUT='OUI',
                                     EVOL=RESUTHER,
                                     NOM_VARC='TEMP',
                                     NOM_CHAM='TEMP',VALE_REF=20.),
                     );

#////////////////////////////////////////////////////////////////////////////////////
#            C A L C U L   M E C A N I Q U E (T H E R M O M E C A N I Q U E)
#////////////////////////////////////////////////////////////////////////////////////
# ===================================
# POURSUITE DU CALCUL THERMIQUE
# ===================================
# ===============================================
# AFFECTATION DU PHENOMENE PHYSIQUE AU MODELE FEM
# <MODMECA> = ELASTICITE LINEAIRE 3D
# ===============================================
MODMECA=AFFE_MODELE(MAILLAGE=MAILLAGE,
                    AFFE=(_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='3D',),),);

# ===============================================
# ORIENTATION DES PEAUX DANS LE MODELE MECANIQUE
# ===============================================
MAILLAGE=MODI_MAILLAGE(reuse =MAILLAGE,
                       MAILLAGE=MAILLAGE,
                       ORIE_PEAU_3D=_F(GROUP_MA=('PEAUCUBE',),),
                       );

# ===========================================================
# DEFINITION DES CHARGEMENTS DE TYPE MECANIQUE ET CL
# ===========================================================
# BLOCAGE DU COLLECTEUR SUIVANT X,Y ET Z <BLOC_COL>
# -------------------------------------------------
BLOCAGES=AFFE_CHAR_MECA(MODELE=MODMECA,
                        DDL_IMPO=(_F(GROUP_NO='NBLOC_X',
                                      DX=0.0,),
                                   _F(GROUP_NO='NBLOC_Y',
                                      DY=0.0,),
                                   _F(GROUP_NO='NBLOC_Z',
                                      DZ=0.0,),),);

STRESS=100.E+6;
FCREEP_Z = FORMULE(NOM_PARA='INST',VALE='STRESS/1.E-4 ');

# VITESSE DE DEPLACEMENT IMPOSE
# ------------------------------------
HAUTEUR=0.01;
EPS_FIN=0.01;
EPS_IMPO=HAUTEUR*EPS_FIN;
DZ_IMPO=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=
                    (0.0,0.0,
                     10000.0,EPS_IMPO),
                    INTERPOL='LIN',
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

PRESSION=AFFE_CHAR_MECA_F(MODELE=MODMECA,
                          FACE_IMPO=(_F(GROUP_MA=('LOAD'),
                                        DZ=DZ_IMPO,),),);

# ====================================================
# RESOLUTION DU PROBLEME EN ELASTO-PLASTICITE CHABOCHE
# <RESUPLAS> = VMIS_CIN2_CHAB
# ====================================================
RESUPLAS=STAT_NON_LINE(MODELE=MODMECA,
                       CHAM_MATER=MAT_TH,
                       EXCIT=(_F(CHARGE=BLOCAGES,),
                              _F(CHARGE=PRESSION,),),
                       COMPORTEMENT=_F(RELATION='VISC_CIN2_CHAB',
                                    DEFORMATION='PETIT',
                                    ITER_INTE_MAXI=50,
                                    TOUT='OUI',),
                       INCREMENT=_F(LIST_INST=L_INST,),
                       NEWTON=_F(REAC_INCR=1,
                                 REAC_ITER=1,
                                 PREDICTION='TANGENTE',
                                 MATRICE='TANGENTE',),
                       CONVERGENCE=_F(RESI_GLOB_MAXI=1.E-4,
                                      ITER_GLOB_MAXI=50),
                       SOLVEUR=_F(METHODE='LDLT',
                                 ),
                       INFO=1,);

RESUPLA1=STAT_NON_LINE(MODELE=MODMECA,
                       CHAM_MATER=MAT_TH1,
                       EXCIT=(_F(CHARGE=BLOCAGES,),
                              _F(CHARGE=PRESSION,),),
                       COMPORTEMENT=_F(RELATION='VISC_CIN2_CHAB',
                                    DEFORMATION='PETIT',
                                    ITER_INTE_MAXI=50,
                                    TOUT='OUI',),
                       INCREMENT=_F(LIST_INST=L_INST,),
                       NEWTON=_F(REAC_INCR=1,
                                 REAC_ITER=1,
                                 PREDICTION='TANGENTE',
                                 MATRICE='TANGENTE',),
                       CONVERGENCE=_F(RESI_GLOB_MAXI=1.E-4,
                                      ITER_GLOB_MAXI=50),
                       SOLVEUR=_F(METHODE='LDLT',
                                 ),
                       INFO=1,);

# ====================================================================
# DEFINITION DES PAS D'ORDRE POUR LE POST-TRAITEMENT
# <L_ORDRE>
# ====================================================================
L_ORDRE=DEFI_LIST_ENTI(DEBUT=1,
                       INTERVALLE=_F(JUSQU_A=NBRE_INC,
                                     PAS=1,),);

# ==============================================================================
# CALCUL DES CHAMPS ELEMTAIRES POUR CAS ELASTIQUE <MECA_STATIQUE>
# ==============================================================================

RESUPLAS=CALC_CHAMP(reuse=RESUPLAS,LIST_ORDRE=L_ORDRE,CRITERES=('ENDO_ELGA','ENDO_ELNO'),RESULTAT=RESUPLAS)


RESUPLA1=CALC_CHAMP(reuse=RESUPLA1,LIST_ORDRE=L_ORDRE,CRITERES=('ENDO_ELGA','ENDO_ELNO'),RESULTAT=RESUPLA1)


TEST_RESU(RESU=(_F(NUME_ORDRE=15,
                   POINT=1,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELGA',
                   NOM_CMP='TRIAX',
                   VALE_CALC=0.33333333332826,
                   MAILLE='M1',),
                _F(NUME_ORDRE=15,
                   POINT=1,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELGA',
                   NOM_CMP='SI_ENDO',
                   VALE_CALC=1.0643261926851E+8,
                   MAILLE='M1',),
                _F(NUME_ORDRE=15,
                   POINT=1,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELGA',
                   NOM_CMP='COENDO',
                   VALE_CALC=5658.0555283177,
                   MAILLE='M1',),
                _F(NUME_ORDRE=15,
                   POINT=1,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELGA',
                   NOM_CMP='DOM_LEM',
                   VALE_CALC=5.4373164013782E-3,
                   MAILLE='M1',),
                _F(NUME_ORDRE=15,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELNO',
                   NOEUD='N1',
                   NOM_CMP='TRIAX',
                   VALE_CALC=0.33333333332826,
                   MAILLE='M1',),
                _F(NUME_ORDRE=15,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELNO',
                   NOEUD='N1',
                   NOM_CMP='SI_ENDO',
                   VALE_CALC=1.0643261926851E8,
                   MAILLE='M1',),
                _F(NUME_ORDRE=15,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELNO',
                   NOEUD='N1',
                   NOM_CMP='COENDO',
                   VALE_CALC=5658.0555283177,
                   MAILLE='M1',),
                _F(NUME_ORDRE=15,
                   RESULTAT=RESUPLA1,
                   NOM_CHAM='ENDO_ELNO',
                   NOEUD='N1',
                   NOM_CMP='DOM_LEM',
                   VALE_CALC=5.437316401378E-3,
                   MAILLE='M1',),
                ),
          )

# ====================================================================
# RELEVE DE L'ENDOMMAGEMENT DE LEMAITRE-SERMAGE
# ====================================================================
DOMLE=POST_RELEVE_T(ACTION=(_F(INTITULE='DOMMAGE S=1.003',
                               GROUP_NO='POST_NO',
                               RESULTAT=RESUPLAS,
                               NOM_CHAM='ENDO_ELNO',
                               NOM_CMP=('DOM_LEM'),
                               OPERATION='EXTRACTION',),),
                    INFO=1,);

DOMLE1=POST_RELEVE_T(ACTION=(_F(INTITULE='DOMMAGE S=0.8',
                               GROUP_NO='POST_NO',
                               RESULTAT=RESUPLA1,
                               NOM_CHAM='ENDO_ELNO',
                               NOM_CMP=('DOM_LEM'),
                               OPERATION='EXTRACTION',),),
                    INFO=1,);

IMPR_TABLE(TABLE=DOMLE,
           UNITE=38, FORMAT='AGRAF',);
IMPR_TABLE(TABLE=DOMLE1,
           UNITE=38, FORMAT='AGRAF',);

# --------------------------------------------------------------------
# VERIFICATION DES SOLUTIONS OBTENUES ...
# 1. AVEC S=1.003
# --------------------------------------------------------------------
TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.0,
           VALE_REFE=0.0,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DOM_LEM',
           TABLE=DOMLE,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=100.0,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.0,
           VALE_REFE=0.0,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DOM_LEM',
           TABLE=DOMLE,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=150.0,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.0,
           VALE_REFE=0.0,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DOM_LEM',
           TABLE=DOMLE,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=200.0,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.0,
           VALE_REFE=0.0,
           REFERENCE='AUTRE_ASTER',
           NOM_PARA='DOM_LEM',
           TABLE=DOMLE,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=250.0,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.0,
           VALE_REFE=0.0,
           REFERENCE='AUTRE_ASTER',
