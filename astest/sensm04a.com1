                                   TABLE=TB_EPS[i],
                                   NOM_PARA=('NUME_ORDRE', 'NOEUD'),),
				_F(OPERATION='RENOMME',
				   NOM_PARA=('EPXY', 'EPXY_EPS'),),
				_F(OPERATION='OPER',
				   NOM_PARA='DIFF',
				   FORMULE=V_EPXY[i]),
				_F(OPERATION='EXTR',
				   NOM_PARA=('NOEUD', 'NUME_ORDRE', 'INST', 'EPXYDPS', 'DIFF', 'EPXY_REF', 'EPXY_EPS'),),),
			TITRE='Comparaison entre derivee / differences finies'
			)

