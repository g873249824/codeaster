#---------------------------------------------------------------------
#  - CALCUL QUASI-STATIQUE D'UN CYLINDRE SOUMIS A DEUX CHARGES
#    DIAMETRALEMENT OPPOSEES
#  - MODELISATION AVEC DES ELEMENTS 'COQUE_D_PLAN'
#---------------------------------------------------------------------
#
DEBUT( CODE=_F( NOM = 'SSLS501A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
PRE_IDEAS()

#
MA=LIRE_MAILLAGE( )

MO=AFFE_MODELE(   MAILLAGE=MA,
                        AFFE=_F( TOUT = 'OUI',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'COQUE_D_PLAN')
                  )

#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAR_ELE=AFFE_CARA_ELEM(   MODELE=MO,
                             COQUE=_F( GROUP_MA = 'TOUT_ELT',
                                    EPAIS = 0.1,
                                    MODI_METRIQUE = 'OUI')
                          )

#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT=DEFI_MATERIAU(   ELAS=_F( E = 1.E+05,
                             NU = 0.)
                     )

#
CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MA,
                                AFFE=_F( TOUT = 'OUI',
                                      MATER = MAT)
                          )

#
#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------
#
CHAR=AFFE_CHAR_MECA(         MODELE=MO,DDL_IMPO=(
                             _F( GROUP_NO = 'A',
                                       DX = 0., DRZ = 0.),
                             _F( GROUP_NO = 'B',
                                       DY = 0., DRZ = 0.)),
                         FORCE_NODALE=_F( GROUP_NO = 'A',
                                       FY = -0.75)
                       )

#
#---------------------------------------------------------------------
#                         RESOLUTION
#---------------------------------------------------------------------
#
RESUL=MECA_STATIQUE(       MODELE=MO,
                         CHAM_MATER=CHAM_MAT,
                          CARA_ELEM=CAR_ELE,
                              EXCIT=_F( CHARGE = CHAR)
                       )

#
RESUL=CALC_CHAMP(reuse=RESUL,RESULTAT=RESUL,CONTRAINTE=('EFGE_ELNO'))

#
#---------------------------------------------------------------------
#                         VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   RESULTAT=RESUL,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.013558268,
                   VALE_REFE=-0.013455,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.02,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   RESULTAT=RESUL,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=0.012318995,
                   VALE_REFE=0.012255,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.02,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   RESULTAT=RESUL,
                   NOM_CHAM='EFGE_ELNO',
                   NOM_CMP='NXX',
                   VALE_CALC=-0.026211866,
                   VALE_REFE=0.0,
                   REFERENCE='NON_DEFINI',
                   CRITERE='ABSOLU',
                   PRECISION=0.20000000000000001,
                   MAILLE='M32',),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   RESULTAT=RESUL,
                   NOM_CHAM='EFGE_ELNO',
                   NOM_CMP='MXX',
                   VALE_CALC=-0.477010465,
                   VALE_REFE=-0.47699999999999998,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.02,
                   MAILLE='M32',),
                ),
          )

#
FIN()
#
#
