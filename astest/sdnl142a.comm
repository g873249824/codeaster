# Test de non-régression
TEST_RESU(RESU=_F(RESULTAT=dyna,
                   INST= 1.,
                   NOM_CHAM='DEPL',
                   NOEUD='N0002',
                   NOM_CMP='DX',
                   VALE_CALC=0.319065493681,
                   ),)
TEST_RESU(RESU=_F(RESULTAT=dyna,
                   INST= 1.,
                   NOM_CHAM='DEPL',
                   NOEUD='N0002',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.159532746841,
                   ),)

# Test de non-régression
TEST_RESU(RESU=_F(RESULTAT=dyna,
                   INST= 2.,
                   NOM_CHAM='DEPL',
                   NOEUD='N0002',
                   NOM_CMP='DX',
                   VALE_CALC=0.836123404232,
                   ),)
TEST_RESU(RESU=_F(RESULTAT=dyna,
                   INST= 2.,
                   NOM_CHAM='DEPL',
                   NOEUD='N0002',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.418061702116,
                   ),)

FIN();
