           PRECISION=0.006,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=S_T,
           FILTRE=_F(NOM_PARA='COOR_X',
                     VALE=4.37942,),
           NOM_PARA='SIYY',
           VALE=175.829,
           PRECISION=0.015,
           REFERENCE='AUTRE_ASTER',);

#--------------------------------------------------------------------------------
