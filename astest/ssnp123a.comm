DEBUT(CODE=_F(NOM='SSNP123A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

# DEFINITION DU PROBLEME
#=======================

PRE_GIBI();

MA=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_SI',),);

MATERIAU=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                               NU=0.3,),
                       ECRO_LINE=_F(D_SIGM_EPSI=1000.0,
                                    SY=200.0,),);

CH_MATER=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATERIAU,),);

SYMETRIE=AFFE_CHAR_MECA(MODELE=MO,
                        FACE_IMPO=(_F(GROUP_MA=('LCD','LBC',),
                                      DY=0.0,),
                                   _F(GROUP_MA=('LDE','LEF',),
                                      DX=0.0,),),);

TRACTION=AFFE_CHAR_MECA(MODELE=MO,
                        FACE_IMPO=_F(GROUP_MA='LFG',
                                     DY=0.1,),);

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=5,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0, 0.0,
                          1.0, 1.0,),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =INSTANTS ),
                        ECHEC=_F(SUBD_METHODE='UNIFORME',
                                 SUBD_PAS=4,
                                 SUBD_PAS_MINI = 0.001),)

# CALCUL
#========

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CH_MATER,
                   EXCIT=(_F(CHARGE=TRACTION,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=SYMETRIE,),),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
                   INCREMENT=_F(LIST_INST=DEFLIST),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=INSTANTS,),);

# POST TRAITEMENT
#=================

RESU=CALC_ELEM(reuse =RESU,

               RESULTAT=RESU,
               OPTION=('SIEF_ELNO',
                       'ERME_ELEM',
                       'ERME_ELNO',
                      ),
               );

RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION='SIEF_NOEU',);

ECHAN=INTE_MAIL_2D(MAILLAGE=MA,
                   DEFI_SEGMENT=_F(ORIGINE=(0.0,1.0,),
                                   EXTREMITE=(5.0,1.0,),),
                   PRECISION=0.001,
                   INFO=1,);

S_T=POST_RELEVE_T(ACTION=_F(INTITULE='S_EXT',
                            CHEMIN=ECHAN,
                            RESULTAT=RESU,
                            NOM_CHAM='SIEF_NOEU',
                            INST=1.0,
                            CRITERE='RELATIF',
                            NOM_CMP='SIYY',
                            REPERE='GLOBAL',
                            MOYE_NOEUD='OUI',
                            OPERATION='EXTRACTION',),
                  INFO=1,);

IMPR_TABLE(TABLE=S_T,);


# TESTS
#=======

# ON TESTE PAR RAPPORT A LA MODELISATION B : ELEMENTS QUASI-INCOMPRESSIBLES

TEST_TABLE(TABLE=S_T,
           FILTRE=_F(NOM_PARA='COOR_X',
                     VALE=0.0,),
           NOM_PARA='SIYY',
           VALE=234.174,
           PRECISION=0.004,
           REFERENCE='AUTRE_ASTER',);

