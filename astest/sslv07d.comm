DEBUT(CODE=_F(NOM='SSLV07D', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA1=LIRE_MAILLAGE( UNITE = 19 )

DEFI_GROUP( reuse=MA1, MAILLAGE=MA1,
            CREA_GROUP_NO=(_F( NOM = 'B', NOEUD='N18'),
                           _F( NOM = 'C', NOEUD='N10'),
                           _F( NOM = 'BX' ,
                               NOEUD=('N27','N46','N18'))))

