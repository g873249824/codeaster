                   VALE_CALC= 1.09828363E-03,
                   VALE_REFE=1.1000000000000001E-3,
                   PRECISION=2.E-3,
                   MAILLE='M30',),
                _F(NUME_ORDRE=3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='V1',
                   VALE_CALC= 2.98601754E-04,
                   VALE_REFE=2.9999999999999997E-4,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M30',),
                _F(NUME_ORDRE=3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-99.688228708716,
                   VALE_REFE=-100.0,
                   PRECISION=3.5000000000000001E-3,
                   MAILLE='M30',),
                _F(NUME_ORDRE=5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.27269491E-03,
                   VALE_REFE=1.2750000000000001E-3,
                   PRECISION=2.E-3,
                   MAILLE='M30',),
                _F(NUME_ORDRE=5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='V1',
                   VALE_CALC= 5.23151222E-04,
                   VALE_REFE=5.2499999999999997E-4,
                   PRECISION=4.0000000000000001E-3,
                   MAILLE='M30',),
