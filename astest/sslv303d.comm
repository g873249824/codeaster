#       (3D ET FORCE_INTERNE_F)
DEBUT(CODE=_F(NOM='SSLV303D', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(FORMAT='MED',)

#
# DEFINITION DES GROUPES DE NOEUDS
MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'ENCAST'),
                       _F(  GROUP_MA = 'CONT_PR'),
                       _F(  GROUP_MA = ( 'B',  'SECT_MED', )))
                   )

#
# DEFINITION DU MODELE
MODELE=AFFE_MODELE(    MAILLAGE=MAIL,
                         AFFE=_F(  TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS_FOURIER')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 2.1E11,  NU = 0.3,
                                 ALPHA = 1.E-5,  RHO = 7800.)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(    MAILLAGE=MAIL,
                          AFFE=_F(  TOUT = 'OUI',
                                 MATER = ACIER)  )

F_Y=DEFI_FONCTION(    NOM_PARA='Y',
                        VALE=( 0.,     10000.,
                               0.24,   10000., )
                    )

# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
CLIM=AFFE_CHAR_MECA(    MODELE=MODELE,
                            DDL_IMPO=_F(  GROUP_NO = 'ENCAST',
                                       DX = 0.,  DY = 0.,  DZ = 0.)  )

#   GRAVITE
GRAD_R=DEFI_FONCTION(    NOM_PARA='X',
                           VALE=( 0.,        76518.,
                                  0.006,     76518., )
                        )

GCIR_Y=DEFI_FONCTION(    NOM_PARA='Y',
                           VALE=( 0.,       76518.,
                                  0.24,     76518., )
                       )

CHAR_GR=AFFE_CHAR_MECA_F(    MODELE=MODELE,
                               FORCE_INTERNE=_F(  TOUT = 'OUI',
                                               FX = GRAD_R, FZ = GCIR_Y)
                          )

#   PRESSION
CHAR_PR=AFFE_CHAR_MECA_F(   MODELE=MODELE,
                              PRES_REP=_F(  GROUP_MA = 'CONT_PR',
                                         PRES = F_Y)
                          )

DEF0=MACRO_ELAS_MULT(        MODELE=MODELE,
                             CHAM_MATER=CHMAT,
                             CHAR_MECA_GLOBAL=CLIM,CAS_CHARGE=(
                             _F(MODE_FOURIER = 1,
                                TYPE_MODE = 'SYME',
                                CHAR_MECA = CHAR_GR,
                                OPTION = 'SANS' ),
                             _F(MODE_FOURIER = 1,
                                TYPE_MODE = 'SYME',
                                CHAR_MECA = CHAR_PR,
                                OPTION = 'SANS' ))
                     )

U0_GR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=DEF0,
                    NUME_ORDRE=1
                  )

U0_PR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=DEF0,
                    NUME_ORDRE=2
                  )

#
# CALCUL DES CONTRAINTES

DEF0=CALC_ELEM( reuse= DEF0, RESULTAT= DEF0,
                OPTION=('SIGM_ELNO',),
                )

SIG0_GR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELNO_SIEF_R',
                      RESULTAT=DEF0,
                      NUME_ORDRE=1,
                      NOM_CHAM='SIGM_ELNO'
                  )

SIG0_PR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELNO_SIEF_R',
                      RESULTAT=DEF0,
                      NUME_ORDRE=2,
                      NOM_CHAM='SIGM_ELNO'
                  )
# RECOMBINAISONS DE FOURIER EN DEPLACEMENTS ET CONTRAINTES
# POUR TETA = 0. (GRAVITE) ET TETA = 0. (PRESSION)

RU0_GR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='DEPL',
           AFFE=_F( CHAM_GD=U0_GR,NUME_MODE=1,TYPE_MODE='SYME'),)

RU0_PR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='DEPL',
           AFFE=_F( CHAM_GD=U0_PR,NUME_MODE=1,TYPE_MODE='SYME'),)

RSIG0_GR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='SIGM_ELNO',
           AFFE=_F( CHAM_GD=SIG0_GR,NUME_MODE=1,TYPE_MODE='SYME'),)

RSIG0_PR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='SIGM_ELNO',
           AFFE=_F( CHAM_GD=SIG0_PR,NUME_MODE=1,TYPE_MODE='SYME'),)

CO_FOUR1=COMB_FOURIER(   RESULTAT=RU0_GR,
                         NOM_CHAM=( 'DEPL',),
                         ANGL=( 0., ),
                        )

CO_FOUR2=COMB_FOURIER(   RESULTAT=RU0_PR,
                         NOM_CHAM=( 'DEPL',),
                         ANGL=( 0., ),
                        )

CO_FOUR3=COMB_FOURIER(   RESULTAT=RSIG0_GR,
                         NOM_CHAM=( 'SIGM_ELNO',),
                         ANGL=( 0., ),
                        )

CO_FOUR4=COMB_FOURIER(   RESULTAT=RSIG0_PR,
                         NOM_CHAM=( 'SIGM_ELNO',),
                         ANGL=( 0., ),
                        )

U_GR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='NOEU_DEPL_R',
             RESULTAT=CO_FOUR1,ANGL=0.,NOM_CHAM='DEPL')

U_PR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='NOEU_DEPL_R',
             RESULTAT=CO_FOUR2,ANGL=0.,NOM_CHAM='DEPL')

SIG_GR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='ELNO_SIEF_R',
             RESULTAT=CO_FOUR3,ANGL=0.,NOM_CHAM='SIGM_ELNO')

SIG_PR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='ELNO_SIEF_R',
             RESULTAT=CO_FOUR4,ANGL=0.,NOM_CHAM='SIGM_ELNO')
#
TEST_RESU(CHAM_NO=(
#                 UR
            _F(  CHAM_GD = U_GR,
                      NOEUD = 'N249',
                      NOM_CMP = 'DX',
                      VALE = 3.566E-7,
                      PRECISION = 0.007),
              _F(  CHAM_GD = U_GR,
                    NOEUD = 'N87',
                    NOM_CMP = 'DX',
                    VALE = 3.566E-7,
                    PRECISION = 0.009),
#                  UT
             _F(  CHAM_GD = U_GR,
                    NOEUD = 'N249',
                    NOM_CMP = 'DY',
                    VALE = 0.,
                    CRITERE = 'ABSOLU',
                    PRECISION = 0.001),
             _F(  CHAM_GD = U_GR,
                    NOEUD = 'N87',
                    NOM_CMP = 'DY',
                    VALE = 0.,
                    CRITERE = 'ABSOLU',
                    PRECISION = 0.001))
          )

#
T1=POST_RELEVE_T(    ACTION=_F(  GROUP_NO = ( 'B', ),
                                INTITULE = 'SIG',
                                CHAM_GD = SIG_GR,
                                NOM_CMP = ('SIXX', 'SIYY', 'SIZZ',),
                                OPERATION = 'EXTRACTION')
                   )

#
TEST_TABLE(       TABLE=T1,
                  FILTRE=_F(  NOM_PARA = 'NOEUD',
                           VALE_K = 'N2'),
                NOM_PARA='SIYY',
               PRECISION=0.042,
                    VALE=-2.496E5            )

#
TEST_RESU(CHAM_NO=(
#                 UR
            _F(  CHAM_GD = U_PR,
                      NOEUD = 'N249',
                      NOM_CMP = 'DX',
                      VALE = -7.82E-6,
                      PRECISION = 0.014),
              _F(  CHAM_GD = U_PR,
                    NOEUD = 'N87',
                    NOM_CMP = 'DX',
                    VALE = -7.82E-6,
                    PRECISION = 0.016))
          )

#
T2=POST_RELEVE_T(    ACTION=_F(  GROUP_NO = ( 'B', ),
                                INTITULE = 'SIG',
                                CHAM_GD = SIG_PR,
                                NOM_CMP = ('SIXX', 'SIYY', 'SIZZ',),
                                OPERATION = 'EXTRACTION')
                   )

#
TEST_TABLE(       TABLE=T2,
                  FILTRE=_F(  NOM_PARA = 'NOEUD',
                           VALE_K = 'N2'),
                NOM_PARA='SIXX',
               PRECISION=0.185,
                    VALE=1.73E6            )

TEST_TABLE(       TABLE=T2,
                  FILTRE=_F(  NOM_PARA = 'NOEUD',
                           VALE_K = 'N2'),
                NOM_PARA='SIYY',
               PRECISION=0.018,
                    VALE=5.56E6            )

TEST_TABLE(       TABLE=T2,
                  FILTRE=_F(  NOM_PARA = 'NOEUD',
                           VALE_K = 'N2'),
                NOM_PARA='SIZZ',
               PRECISION=0.088,
                    VALE=1.74E6            )

FIN()
#
