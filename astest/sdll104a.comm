INTERRE1=REST_SPEC_PHYS(         MODE_MECA=FREQ1,
                                         BANDE=( 0., 35., ),
                              INTE_SPEC_GENE=DYNALEA1,
                                   MODE_STAT=MODESTA1,
                               EXCIT=_F(   NOEUD = 'PA1',
                                        NOM_CMP = 'DY'),
                                   MOUVEMENT='ABSOLU',
