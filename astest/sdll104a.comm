INTKTJ1=DEFI_INTE_SPEC(    DIMENSION=1,
                         KANAI_TAJIMI=_F(  NUME_ORDRE_I = 1,
                                        NUME_ORDRE_J = 1,
                                        FREQ_MIN = 0.,
                                        FREQ_MAX = 50.,
                                        PAS = 1.,
                                        AMOR_REDUIT = 0.6,
                                        FREQ_MOY = 5.,
                                        VALE_R = So,
                                        INTERPOL = 'LIN',
                                        PROL_GAUCHE = 'CONSTANT',
                                        PROL_DROITE = 'CONSTANT')
                       )

INTKTJ2=DEFI_INTE_SPEC(    DIMENSION=1,
                         KANAI_TAJIMI=_F(  NOEUD_I = 'PA11',
                                        NOM_CMP_I = 'DY',
                                        NOEUD_J = 'PA11',
                                        NOM_CMP_J = 'DY',
                                        FREQ_MIN = 0.,
                                        FREQ_MAX = 50.,
                                        PAS = 1.,
                                        AMOR_REDUIT = 0.6,
                                        FREQ_MOY = 5.,
                                        VALE_R = So,
                                        INTERPOL = 'LIN',
                                        PROL_GAUCHE = 'CONSTANT',
                                        PROL_DROITE = 'CONSTANT')
                       )

INTCS=DEFI_INTE_SPEC(  
