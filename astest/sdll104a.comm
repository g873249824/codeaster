                                        NOM_CMP_I = 'DY',
                                        NOEUD_J = 'PB11',
                                        NOM_CMP_J = 'DY',
                                       VALE_C = 1.+1.j,
                                       FREQ_MIN = 0.,
                                       FREQ_MAX = 25.)
                          )



