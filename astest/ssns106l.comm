# COMPARAISON DES MODELE GLRC_DM ET DHRC
#
# DETAILS : MODELISATION : DKTG (GLRC_DM OU DHRC)
#            MAILLAGE     : TRIA3
#            CHARGEMENT   : CYCLIQUE DISTORSION ET CISAILLEMENT

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'),)
# SDVERI='NON' car la verification est trop couteuse en CPU

# PARAMETRES
EP = 0.1;

D0 = 0.0033;
fx = 5000000.0;

t1 = 1.0;
t2 = (3.0 * t1);
t3 = (4.0 * t1);

dt = 0.05;

EGM = 35625.E6;
EGF = 38660.E6;
EB  = 32308.E6;

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='A1A2',),
                               _F(GROUP_MA='A4A3',),
                               _F(GROUP_MA='A2A4',),
                               _F(GROUP_MA='A3A1',),
                               _F(DIFFE=('BORDS','A1',),
                                  NOM='CL_IMPOS',),),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='DALLE',
                                      VECT_NORM=(0.,0.,1.,),
                                      GROUP_NO='A1',),);

MA=CREA_MAILLAGE(MAILLAGE=MAIL,
                 CREA_MAILLE=(_F(NOM='DALLE_G',
                                 GROUP_MA='DALLE',
                                 PREF_MAILLE='G',),),);

MO_G=AFFE_MODELE(MAILLAGE=MA,
                 INFO=1,
                 AFFE=_F(GROUP_MA=('DALLE_G','A1A2','A4A3','A2A4','A3A1',),
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKTG',),);

MAT_G=DEFI_MATERIAU(ELAS_GLRC=_F(E_M=EGM,
                                 NU_M=0.18,
                                 E_F=EGF,
                                 NU_F=0.17,),
                    GLRC_DM=_F(GAMMA_T=0.06,
                               GAMMA_C=0.8,
                               GAMMA_F=0.45,
                               NYT=3.7E5,
                               MYF=9.E3,),);

#BETON DHRC

INCLUDE(UNITE=12, INFO=1,);

# -- AFFECTATION DU MATERIAU POUR GLRC_DM --

MATER_G=AFFE_MATERIAU(MAILLAGE=MA,
                      AFFE=(_F(GROUP_MA='DALLE_G',
                               MATER=MAT_G,),),);

# -- AFFECTATION DU MATERIAU POUR DHRC --

MATER_D=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='DALLE_G',
                             MATER=MAT_D,),),);

# -- AFFECTATION DES CARACTERISTIQUES POUR GLRC_DM ET DHRC --

CAEL_G=AFFE_CARA_ELEM(MODELE=MO_G,
                      COQUE=_F(GROUP_MA='DALLE_G',
                               EPAIS=EP,
                               ANGL_REP=(0.0,0.0,),
                               INER_ROTA='OUI',),);

UX_IMPOS=FORMULE(VALE='D0*Y',
                 NOM_PARA='Y',);

UY_IMPOS=FORMULE(VALE='D0*X',
                 NOM_PARA='X',);

COND_0=AFFE_CHAR_CINE(MODELE=MO_G,
                      MECA_IMPO=_F(GROUP_NO='A1',
                                   DX=0.0,
                                   DY=0.0,
                                   DZ=0.0,
                                   DRX=0.0,
                                   DRY=0.0,
                                   DRZ=0.0,),);

COND_1=AFFE_CHAR_CINE_F(MODELE=MO_G,
                        MECA_IMPO=_F(GROUP_NO='CL_IMPOS',
                                     DX=UX_IMPOS,
                                     DY=UY_IMPOS,),);

chf1=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.,0.,
                         t1,1.,
                         t2,-1.,
                         t3,0.,),
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='CONSTANT',);

inst1=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=t3,
                                   PAS=dt,),);

DEFLIST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                    LIST_INST=inst1,),
                       ECHEC=_F(ACTION='DECOUPE',
                                SUBD_METHODE='MANUEL',
                                SUBD_PAS=4,
                                SUBD_PAS_MINI=0.0001,),);

inst1R=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=3.95,
                                   PAS=dt,),);

DEFLISTR=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                    LIST_INST=inst1R,),
                       ECHEC=_F(ACTION='DECOUPE',
                                SUBD_METHODE='MANUEL',
                                SUBD_PAS=4,
                                SUBD_PAS_MINI=0.0001,),);

RESU_G=STAT_NON_LINE(MODELE=MO_G,
                     CHAM_MATER=MATER_G,
                     CARA_ELEM=CAEL_G,
                     EXCIT=(_F(CHARGE=COND_0,
                               FONC_MULT=chf1,),
                            _F(CHARGE=COND_1,
                               FONC_MULT=chf1,),),
                     COMPORTEMENT=_F(RELATION='GLRC_DM',),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  PRECISION=0.1,),
                     NEWTON=_F(REAC_ITER=1,),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=25,),);

RESU_G=CALC_CHAMP(reuse=RESU_G,
                  RESULTAT=RESU_G,
                  FORCE='FORC_NODA',
                  VARI_INTERNE=('VARI_ELNO','VARI_NOEU',),
                  PRECISION=1.E-3,)

RESU_D=STAT_NON_LINE(MODELE=MO_G,
                     CHAM_MATER=MATER_D,
                     CARA_ELEM=CAEL_G,
                     EXCIT=(_F(CHARGE=COND_0,
                               FONC_MULT=chf1,),
                            _F(CHARGE=COND_1,
                               FONC_MULT=chf1,),),
                     COMPORTEMENT=_F(RELATION='DHRC',),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  PRECISION=0.1,),
                     NEWTON=_F(REAC_ITER=1,),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=25,),);

RESU_D=CALC_CHAMP(reuse=RESU_D,
                  RESULTAT=RESU_D,
                  FORCE='FORC_NODA',
                  VARI_INTERNE=('VARI_ELNO','VARI_NOEU',),
                  PRECISION=1.E-3,)

############### POST TRAITEMENT #################################

FORM_A=FORMULE(VALE='abs(DX+DY)',
               NOM_PARA=('DX','DY',),);

FORM_V=FORMULE(VALE='(DX*DX+DY*DY)**0.5*(DX+DY)/abs(DX+DY)',
               NOM_PARA=('DX','DY',),);

DIF_REL = FORMULE(VALE='(VG-V)/V',
                  NOM_PARA=('V','VG',),);

DIF_RELD = FORMULE(VALE='(VG-VD)/VD',
                  NOM_PARA=('VD','VG',),);

######################################################################

TFN_B_G=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='B_FNG',
                                RESULTAT=RESU_G,
                                NOM_CHAM='FORC_NODA',
                                GROUP_NO='B',
                                NOM_CMP=('DX','DY',),),);

TVAR_G=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='D',
                               RESULTAT=RESU_G,
                               NOM_CHAM='VARI_NOEU',
                               GROUP_NO='A4',
                               TOUT_CMP='OUI',),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='EXTR',
                             NOM_PARA=('INST','DX','DY',),),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='OPER',
                             FORMULE=FORM_A,
                             NOM_PARA='DI',),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='FILTRE',
                             NOM_PARA='DI',
                             CRIT_COMP='NE',
                             VALE=0.0,),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='SUPPRIME',
                             NOM_PARA='DI',),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=(_F(OPERATION='OPER',
                              FORMULE=FORM_V,
                              NOM_PARA='VG',),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DX','FXG',),),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DY','FYG',),),),);

TVAR_G=CALC_TABLE(reuse =TVAR_G,
                  TABLE=TVAR_G,
                  ACTION=(_F(OPERATION='EXTR',
                             NOM_PARA=('INST','V1','V2',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V1','D1',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V2','D2',),),),);

#####################################################################

TFN_B_D=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='B_FND',
                                RESULTAT=RESU_D,
                                NOM_CHAM='FORC_NODA',
                                GROUP_NO='B',
                                NOM_CMP=('DX','DY',),),);

TVAR_D=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='VARI',
                               RESULTAT=RESU_D,
                               NOM_CHAM='VARI_NOEU',
                               GROUP_NO='A4',
                               TOUT_CMP='OUI',),);

TFN_B_D=CALC_TABLE(reuse =TFN_B_D,
                   TABLE=TFN_B_D,
                   ACTION=_F(OPERATION='EXTR',
                             NOM_PARA=('INST','DX','DY',),),);

TFN_B_D=CALC_TABLE(reuse =TFN_B_D,
                   TABLE=TFN_B_D,
                   ACTION=_F(OPERATION='OPER',
                             FORMULE=FORM_A,
                             NOM_PARA='DI',),);

TFN_B_D=CALC_TABLE(reuse =TFN_B_D,
                   TABLE=TFN_B_D,
                   ACTION=_F(OPERATION='FILTRE',
                             NOM_PARA='DI',
                             CRIT_COMP='NE',
                             VALE=0.0,),);

TFN_B_D=CALC_TABLE(reuse =TFN_B_D,
                   TABLE=TFN_B_D,
                   ACTION=_F(OPERATION='SUPPRIME',
                             NOM_PARA='DI',),);

TFN_B_D=CALC_TABLE(reuse =TFN_B_D,
                   TABLE=TFN_B_D,
                   ACTION=(_F(OPERATION='OPER',
                              FORMULE=FORM_V,
                              NOM_PARA='VD',),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DX','FXD',),),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DY','FYD',),),),);

TVAR_D=CALC_TABLE(reuse =TVAR_D,
                  TABLE=TVAR_D,
                  ACTION=(_F(OPERATION='EXTR',
                             NOM_PARA=('INST','V1','V2','V3','V4','V5','V6',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V1','D1',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V2','D2',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V3','EX1',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V4','EY1',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V5','EX2',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('V6','EY2',),),),);

####################################################################

TFN_B=CALC_TABLE(TABLE=TFN_B_G,
                 ACTION=(
                         _F(OPERATION='COMB',
                           TABLE=TFN_B_D,
                           NOM_PARA='INST',),),);

TFN_B=CALC_TABLE(reuse =TFN_B,
                 TABLE=TFN_B,
                 ACTION=(_F(OPERATION='OPER',
                            FORMULE=DIF_RELD,
                            NOM_PARA='VG_VD',),
                         _F(OPERATION='COMB',
                            TABLE=TVAR_G,
                            NOM_PARA='INST',),
                         _F(OPERATION='COMB',
                            TABLE=TVAR_D,
                            NOM_PARA='INST',),),);

IMPR_TABLE(TABLE=TFN_B,);
IMPR_TABLE(TABLE=TVAR_G,);

#----- DISTORSION POSITIF - PHASE ELASTIQUE -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.160150512563,
           NOM_PARA='VG_VD',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.25,),
           )

#----- DISTORSION POSITIF - PHASE ENDOMMAGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.193327158041,
           NOM_PARA='VG_VD',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

#----- DISTORSION POSITIF - PHASE DECHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.193327158041,
           NOM_PARA='VG_VD',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.5,),
           )

#----- DISTORSION NEGATIF - PHASE CHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.193327158041,
           NOM_PARA='VG_VD',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,),
           )

#----- DISTORSION NEGATIF - PHASE DECHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.193327158041,
           NOM_PARA='VG_VD',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.5,),
           )
#####################################################################
#----- CISAILLEMENT

COND_1G=AFFE_CHAR_MECA(MODELE=MO_G,
                       FORCE_ARETE=(_F(GROUP_MA='A2A4',
                                       FY=fx,),
                                    _F(GROUP_MA='A4A3',
                                       FX=fx,),
                                    _F(GROUP_MA='A3A1',
                                       FY=-(fx),),
                                    _F(GROUP_MA='A1A2',
                                       FX=-(fx),),),);

COND_0G=AFFE_CHAR_MECA(MODELE=MO_G,
                       DDL_IMPO=_F(GROUP_NO='A1',
                                   DX=0.0,
                                   DY=0.0,
                                   DZ=0.0,
                                   DRX=0.0,
                                   DRY=0.0,),
                       LIAISON_DDL=_F(GROUP_NO=('A4','A4',),
                                      DDL=('DX','DY',),
                                      COEF_MULT=(1.0,-1.0,),
                                      COEF_IMPO=0.0,),);

chf1c=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                         t3,1.,
                         ),PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',);

inst1c=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=t3,
                                   PAS=(dt * 2),),);

DEFLISTC=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                     LIST_INST=inst1c,),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS=4,
                                 SUBD_PAS_MINI=0.0001,),);

RESU_GC=STAT_NON_LINE(MODELE=MO_G,
                     CHAM_MATER=MATER_G,
                     CARA_ELEM=CAEL_G,
                     EXCIT=(_F(CHARGE=COND_0G,),
                            _F(CHARGE=COND_1G,
                               FONC_MULT=chf1,),),
                     COMPORTEMENT=_F(RELATION='GLRC_DM',),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  PRECISION=0.1,),
                     NEWTON=_F(REAC_INCR=1,
                               MATRICE='TANGENTE',
                               PAS_MINI_ELAS=0.0,
                               REAC_ITER=1,),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=25,),);


RESU_GC=CALC_CHAMP(reuse =RESU_GC,RESULTAT=RESU_GC,PRECISION=1.E-3,
             FORCE=('FORC_NODA',),
             DEFORMATION=('DEGE_ELNO',),
             VARI_INTERNE=('VARI_ELNO','VARI_NOEU',),
             )

RESU_DC=STAT_NON_LINE(MODELE=MO_G,
                     CHAM_MATER=MATER_D,
                     CARA_ELEM=CAEL_G,
                     EXCIT=(_F(CHARGE=COND_0G,),
                            _F(CHARGE=COND_1G,
                               FONC_MULT=chf1,),),
                     COMPORTEMENT=_F(RELATION='DHRC',),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  PRECISION=0.1,),
                     NEWTON=_F(REAC_INCR=1,
                               MATRICE='TANGENTE',
                               PAS_MINI_ELAS=0.0,
                               REAC_ITER=1,),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=25,),);


RESU_DC=CALC_CHAMP(reuse =RESU_DC,RESULTAT=RESU_DC,PRECISION=1.E-3,
             FORCE=('FORC_NODA',),
             DEFORMATION=('DEGE_ELNO',),
             VARI_INTERNE=('VARI_ELNO','VARI_NOEU',),
             )

EPS_GC=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='EPSG',
                             RESULTAT=RESU_GC,
                             NOM_CHAM='DEGE_ELNO',
                             GROUP_NO='A4',
                             TOUT_CMP='OUI',),);

TFN_B_GC=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='B_FNG',
                               RESULTAT=RESU_GC,
                               NOM_CHAM='FORC_NODA',
                               GROUP_NO='B',
                               NOM_CMP=('DX','DY',),),);

TVAR_GC=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                              INTITULE='D',
                              RESULTAT=RESU_GC,
                              NOM_CHAM='VARI_NOEU',
                              GROUP_NO='B',
                              TOUT_CMP='OUI',),);

EPS_GC=CALC_TABLE(reuse=EPS_GC,
                TABLE=EPS_GC,
                ACTION=(_F(OPERATION='EXTR',
                           NOM_PARA=('INST','EXY',),),
                        _F(OPERATION='RENOMME',
                           NOM_PARA=('EXY','EXY_G',),),),);

#IMPR_TABLE(TABLE=EPS_GC,);

TFN_B_GC=CALC_TABLE(reuse =TFN_B_GC,
                  TABLE=TFN_B_GC,
                  ACTION=_F(OPERATION='EXTR',
                            NOM_PARA=('INST','DX','DY',),),);

TFN_B_GC=CALC_TABLE(reuse =TFN_B_GC,
                  TABLE=TFN_B_GC,
                  ACTION=_F(OPERATION='OPER',
                            FORMULE=FORM_A,
                            NOM_PARA='DI',),);

TFN_B_GC=CALC_TABLE(reuse =TFN_B_GC,
                  TABLE=TFN_B_GC,
                  ACTION=_F(OPERATION='FILTRE',
                            NOM_PARA='DI',
                            CRIT_COMP='NE',
                            VALE=0.0,),);

TFN_B_GC=CALC_TABLE(reuse =TFN_B_GC,
                  TABLE=TFN_B_GC,
                  ACTION=_F(OPERATION='SUPPRIME',
                            NOM_PARA='DI',),);

TFN_B_GC=CALC_TABLE(reuse =TFN_B_GC,
                  TABLE=TFN_B_GC,
                  ACTION=(_F(OPERATION='OPER',
                             FORMULE=FORM_V,
                             NOM_PARA='VG',),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('DX','FXG',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('DY','FYG',),),),);

TVAR_GC=CALC_TABLE(reuse =TVAR_GC,
                 TABLE=TVAR_GC,
                 ACTION=(_F(OPERATION='EXTR',
                            NOM_PARA=('INST','V1',),),
                         _F(OPERATION='RENOMME',
                            NOM_PARA=('V1','D1',),),),);

#IMPR_TABLE(TABLE=TFN_B_GC,);

EPS_DC=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='EPSD',
                             RESULTAT=RESU_DC,
                             NOM_CHAM='DEGE_ELNO',
                             GROUP_NO='A4',
                             TOUT_CMP='OUI',),);

TFN_B_DC=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='B_FND',
                               RESULTAT=RESU_DC,
                               NOM_CHAM='FORC_NODA',
                               GROUP_NO='B',
                               NOM_CMP=('DX','DY',),),);

TVAR_DC=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                              INTITULE='D',
                              RESULTAT=RESU_DC,
                              NOM_CHAM='VARI_NOEU',
                              GROUP_NO='B',
                              TOUT_CMP='OUI',),);

EPS_DC=CALC_TABLE(reuse=EPS_DC,
                TABLE=EPS_DC,
                ACTION=(_F(OPERATION='EXTR',
                           NOM_PARA=('INST','EXY',),),
                        _F(OPERATION='RENOMME',
                           NOM_PARA=('EXY','EXY_D',),),),);

#IMPR_TABLE(TABLE=EPS_DC,);

TFN_B_DC=CALC_TABLE(reuse =TFN_B_DC,
                  TABLE=TFN_B_DC,
                  ACTION=_F(OPERATION='EXTR',
                            NOM_PARA=('INST','DX','DY',),),);

TFN_B_DC=CALC_TABLE(reuse =TFN_B_DC,
                  TABLE=TFN_B_DC,
                  ACTION=_F(OPERATION='OPER',
                            FORMULE=FORM_A,
                            NOM_PARA='DI',),);

TFN_B_DC=CALC_TABLE(reuse =TFN_B_DC,
                  TABLE=TFN_B_DC,
                  ACTION=_F(OPERATION='FILTRE',
                            NOM_PARA='DI',
                            CRIT_COMP='NE',
                            VALE=0.0,),);

TFN_B_DC=CALC_TABLE(reuse =TFN_B_DC,
                  TABLE=TFN_B_DC,
                  ACTION=_F(OPERATION='SUPPRIME',
                            NOM_PARA='DI',),);

TFN_B_DC=CALC_TABLE(reuse =TFN_B_DC,
                  TABLE=TFN_B_DC,
                  ACTION=(_F(OPERATION='OPER',
                             FORMULE=FORM_V,
                             NOM_PARA='VD',),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('DX','FXD',),),
                          _F(OPERATION='RENOMME',
