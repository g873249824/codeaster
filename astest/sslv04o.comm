                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MA))

DEFI_GROUP(reuse=M,       MAILLAGE=M,
                  CREA_GROUP_NO=_F( NOM = 'AB', GROUP_MA = 'AB')
                )

TA=POST_ELEM(    MODELE=MO,
                     AIRE_INTERNE=_F(  GROUP_MA_BORD = 'AE7')
                 )

MODI_MAILLAGE(  reuse=M,
        INFO=2,
               MAILLAGE=M,
           ORIE_PEAU_2D=_F(  GROUP_MA = ( 'AE7', ))
                  )

CH=AFFE_CHAR_MECA(         MODELE=MO,DDL_IMPO=(
                              _F(  GROUP_NO = 'AB',  DY = 0.),
                              _F(  GROUP_NO = ('PY1', 'PY2',),  DX = 0.)),
                              PRES_REP=_F(  GROUP_MA = 'AE7',  PRES = 60.))


RESU=MECA_STATIQUE(         MODELE=MO,    CHAM_MATER=CM,
                          EXCIT=_F(  CHARGE = CH),

                    )

CALC_ELEM( reuse= RESU ,RESULTAT= RESU ,

                      OPTION=( 'EPSI_ELNO_DEPL', 'SIGM_ELNO_DEPL',)
