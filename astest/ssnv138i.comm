DZ_P2=RECU_FONCTION(  RESULTAT=NONLI,    NOM_CHAM='DEPL',
                   GROUP_NO='P2',    NOM_CMP='DZ'  )

IMPR_FONCTION(COURBE=(_F(FONCTION=DRY_P2),
                      _F(FONCTION=DX_P2),
                      _F(FONCTION=DZ_P2),))


