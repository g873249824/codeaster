                                  SUBD_PAS_MINI=1e-06,),),
                    NEWTON=(_F(REAC_ITER=1,),),
                    CONVERGENCE=(_F( ITER_INTE_MAXI=100,),),
                      ARCHIVAGE=_F(LIST_INST=LINST,),
                                    );

SOLCHAB=CALC_ELEM(reuse =SOLCHAB,
                RESULTAT=SOLCHAB,
                OPTION=('EPSI_ELGA_DEPL','EPSP_ELGA',),);

TEST_RESU(RESU=(_F(RESULTAT=SOLCHAB,
