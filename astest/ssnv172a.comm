                   MAILLE='MA1',
                   POINT=1,
                   VALE=-6.7577049928861E-04,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=SOLNL,
                   INST=TFIN,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=-1.8057172603154E-05,
                   REFERENCE='NON_REGRESSION',),),);

# COMPARASION A VISC_CIN1_CHAB, MAIS PROBLEME DE SENSIBILITE AU PAS DE 
