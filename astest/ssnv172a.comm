                                  ),
                                  
                    ECOU_VISC2=_F( N=10.0,
                                   K=40.0,
                                   C=10.0,
                                   D=1.E10,
                                   A=0.000000000,),
                                   
                    ECOU_VISC3=_F( K=40.0,
                                   TAUMU=75.5,
                                   GAMMA0=10.E-6,
                                   V=1.E-6,),
                                   
                    ECOU_PLAS1=_F(C=40000.0,),
                                   
                    ECRO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H=0.0,),
                                  
                    ECRO_ISOT2=_F(R_0      =75.5,
                                  B1       =19.34,
                                  B2       =0.00000,
                                  Q1       =9.77,  
                                  Q2       =0.00000,  
                                  H        =0.),
                                  
