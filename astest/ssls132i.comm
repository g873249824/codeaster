# MODELISATION A : 3D + GRILLE_MEMBRANE + MEMBRANE

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

#-----------------------------------------------------------------------
# LECTURE ET ENRICHISSEMENT DU MAILLAGE
#-----------------------------------------------------------------------

MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL2=CREA_MAILLAGE(MAILLAGE=MAILL1,
                     CREA_MAILLE=_F(NOM='ACPLUS',
                                      GROUP_MA='ACIERSUP',
                                      PREF_MAILLE='B',),);

MAILTOT=CREA_MAILLAGE(MAILLAGE=MAILL2,
                      CREA_MAILLE=_F(NOM='ACMOINS',
                                       GROUP_MA='ACIERINF',
                                       PREF_MAILLE='C',),);

MAILTOT=DEFI_GROUP(reuse =MAILTOT,
                   MAILLAGE=MAILTOT,
                   CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

#
# -----------------------------------------------------
# DEFINITION DES MATERIAUX
# -----------------------------------------------------
E_A = 2.E11
RHO_A = 7800.
section_l = 0.2


ACIER=DEFI_MATERIAU(ELAS=_F(E=E_A,
                            NU=0.0,
                            RHO=RHO_A,),);
                            

ACIER_D=DEFI_MATERIAU(ELAS_MEMBRANE =_F(M_LLLL = E_A*section_l,
                                      M_LLTT = 0.,
                                      M_LLLT = 0.,
                                      M_TTTT = 0.,
                                      M_TTLT = 0.,
                                      M_LTLT = 0.,
                                      RHO    = RHO_A*section_l,),)
                                      
BETON=DEFI_MATERIAU(ELAS=_F(E=3.E10,
                            NU=0.0,
                            RHO=2500.,),);

#
# -------------------------------------------------
# AFFECTATION DES MATERIAUX
# -------------------------------------------------

MATAF=AFFE_MATERIAU(MAILLAGE=MAILTOT,
                    AFFE=(_F(GROUP_MA='ACPLUS',
                             MATER=ACIER,),
                          _F(GROUP_MA='ACMOINS',
                             MATER=ACIER_D,),                            
                          _F(GROUP_MA='console',
                             MATER=BETON,),),);

#
# -------------------------------------------------
# AFFECTATION DES MODELES
# -------------------------------------------------

LEMOD=AFFE_MODELE(MAILLAGE=MAILTOT,
                  AFFE=(
                  _F(GROUP_MA='ACPLUS',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='GRILLE_MEMBRANE',),
                  _F(GROUP_MA='ACMOINS',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='MEMBRANE',),                          
                  _F(GROUP_MA='console',
                     PHENOMENE='MECANIQUE',
                     MODELISATION='3D'),),);

# -------------------------------------------------
# AFFECTATION DES CARACTERISTIQUES
# -------------------------------------------------

LACAR=AFFE_CARA_ELEM(MODELE=LEMOD,
                     MEMBRANE=(
                                _F( GROUP_MA = 'ACMOINS',
                                 ANGL_REP=(0.,0.),),
                               ),
                     GRILLE=(
                                _F(GROUP_MA='ACPLUS',
                                SECTION=section_l,
                                ANGL_REP=(0,0,),
                                ),),);

# ----------------------------------------------
# CHARGEMENT
# ----------------------------------------------

CLIM=AFFE_CHAR_MECA(MODELE=LEMOD,
                    PESANTEUR=_F(GRAVITE=9.81,
                                 DIRECTION=(0.,0.,-1.,),),
                    DDL_IMPO=_F(GROUP_MA='B0X',
                                DX=0.,
                                DY=0.,
                                DZ=0.,),);

CHAR=AFFE_CHAR_MECA(MODELE=LEMOD,
                    DDL_IMPO=_F(GROUP_MA='B1X',
                                DZ=-0.1,),);

FMULT=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,
                          1.,0.,
                          2.,1.,),);

ZLIST=DEFI_LIST_REEL(DEBUT=1.,
                     INTERVALLE=_F(JUSQU_A=2.0,
                                   NOMBRE=2,),);

# ----------------------------------------------
# CALCUL IMPLEX
# ----------------------------------------------
                                   

LIMPLEX=STAT_NON_LINE(MODELE=LEMOD,
                    CHAM_MATER=MATAF,
                    CARA_ELEM=LACAR,
                    EXCIT=(_F(CHARGE=CLIM,),
                           _F(CHARGE=CHAR,
                              FONC_MULT=FMULT,),),
                    COMPORTEMENT=_F(RELATION='ELAS',),
                    METHODE='IMPLEX',
                    INCREMENT=_F(LIST_INST=ZLIST,),);


# -------------------------------------------------
# POST TRAITEMENT DES RESULTATS
# -------------------------------------------------
# CALCUL DES GRANDEURS INTERESSANTES

LIMPLEX=CALC_CHAMP(reuse = LIMPLEX,
                RESULTAT=LIMPLEX,
                DEFORMATION ='EPSI_ELNO',
                CONTRAINTE = 'SIEF_ELNO',
                FORCE = 'FORC_NODA');


# OBTENTION DE LA COURBE FORCE-DEPLACEMENT

TAB_UZ_I=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='DEPLACEMENT',
                               RESULTAT=LIMPLEX,
                               NOM_CHAM='DEPL',
                               TOUT_ORDRE='OUI',
                               PRECISION=1.E-06,
                               GROUP_NO='B1X',
                               NOM_CMP='DZ',),);

TAB_FZ_I=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='RESU',
                               RESULTAT=LIMPLEX,
                               NOM_CHAM='FORC_NODA',
                               TOUT_ORDRE='OUI',
                               GROUP_NO='B1X',
                               RESULTANTE='DZ',),);


IMPR_TABLE(TABLE=TAB_FZ_I,);

# ----------------------------------------------
# CALCUL NEWTON
# ----------------------------------------------
 

LEDEP=STAT_NON_LINE(MODELE=LEMOD,
                    CHAM_MATER=MATAF,
                    CARA_ELEM=LACAR,
                    EXCIT=(_F(CHARGE=CLIM,),
                           _F(CHARGE=CHAR,
                              FONC_MULT=FMULT,),),
                    COMPORTEMENT=_F(RELATION='ELAS',),
                    #METHODE='IMPLEX',
                    INCREMENT=_F(LIST_INST=ZLIST,),);


# -------------------------------------------------
# POST TRAITEMENT DES RESULTATS
# -------------------------------------------------
# CALCUL DES GRANDEURS INTERESSANTES

LEDEP=CALC_CHAMP(reuse =LEDEP,
                RESULTAT=LEDEP,
                DEFORMATION ='EPSI_ELNO',
                CONTRAINTE = 'SIEF_ELNO',
                FORCE = 'FORC_NODA');


# OBTENTION DE LA COURBE FORCE-DEPLACEMENT

TAB_UZ=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='DEPLACEMENT',
                               RESULTAT=LEDEP,
                               NOM_CHAM='DEPL',
                               TOUT_ORDRE='OUI',
                               PRECISION=1.E-06,
                               GROUP_NO='B1X',
                               NOM_CMP='DZ',),);

TAB_FZ=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='RESU',
                               RESULTAT=LEDEP,
                               NOM_CHAM='FORC_NODA',
                               TOUT_ORDRE='OUI',
                               GROUP_NO='B1X',
                               RESULTANTE='DZ',),);


IMPR_TABLE(TABLE=TAB_FZ,);

###################################################################################
# insertion de test de non-regression en comparant ELAS avec IMPLEX
DIFF = FORMULE(NOM_PARA=('A','B',), VALE="abs(A-B)" )

TAB_FZ_I = CALC_TABLE(reuse=TAB_FZ_I,
    TABLE=TAB_FZ_I,
    ACTION=(
        _F(OPERATION='RENOMME',NOM_PARA=('DZ','DZ_IMP',),),
    ),
)

TAB_DIFF = CALC_TABLE(
    TABLE=TAB_FZ,
    ACTION=(
        _F(OPERATION='COMB',TABLE=TAB_FZ_I,NOM_PARA=('NUME_ORDRE',),),
    ),
)


TAB_DIFF = CALC_TABLE(reuse=TAB_DIFF,
    TABLE=TAB_DIFF,
    ACTION=(
        _F(OPERATION='OPER',NOM_PARA='DIFF',FORMULE=DIFF,NOM_COLONNE=('DZ','DZ_IMP'),),
        _F(OPERATION='SUPPRIME',NOM_PARA=('DZ','DZ_IMP'),),
    ),
)

IMPR_TABLE(TABLE=TAB_DIFF,);

TEST_TABLE(
        TABLE= TAB_DIFF,
        FILTRE=(_F(NOM_PARA='DIFF',CRIT_COMP='MAXI'), ),
        NOM_PARA='DIFF',CRITERE='ABSOLU',
        VALE_REFE=0.0, PRECISION=1.0E-06, REFERENCE='ANALYTIQUE',
        VALE_CALC=0.0, ORDRE_GRANDEUR= 1.0,
)
    
TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=316569.326963,
           NOM_PARA='DZ',
           TABLE=TAB_FZ,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,),
           )



FIN();
