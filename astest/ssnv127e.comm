# CALCUL 2D
# MODELE
# MAILLAGE : ELEMENT 2D TRIANGLES A 6 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
#
# TEST ISSU DE LA VALIDATION INDEPENDANTE CONTACT FROTTEMENT
# LES ELEMENTS DE BORD SONT DES SEG3
#

DEBUT(CODE=_F(NOM='SSNV127E',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='OUI'))


#_____________________________________________________________________
#
#                    LECTURE DE SSNV127E.MGIB
#_____________________________________________________________________
#



SSNV127E=LIRE_MAILLAGE(FORMAT='MED',);
#
#


SSNV127E=DEFI_GROUP(reuse =SSNV127E,
                    MAILLAGE=SSNV127E,
                    CREA_GROUP_NO=(_F(GROUP_MA='CC4',),
                                   _F(GROUP_MA='AD1',),
                                   _F(GROUP_MA='AD2',),
                                   _F(GROUP_MA='AD3',),
                                   _F(GROUP_MA='BD3',),
                                   _F(GROUP_MA='AD4',),
                                   _F(GROUP_MA='BD4',),
                                   _F(GROUP_MA='BCC2',
                                      OPTION='NOEUD_ORDO',
                                      NOM='RELEVE',
                                      GROUP_NO_ORIG='A1',
                                      GROUP_NO_EXTR='B2',),),);
# POUR ELIMINER LES NOEUDS COMMUNS AUX SURFACES DE CONTACT
SSNV127E=DEFI_GROUP(reuse=SSNV127E,
                    MAILLAGE=SSNV127E,
                    CREA_GROUP_NO=(_F(GROUP_MA='BCC1',),
                                   _F(GROUP_MA='BCC2',),
                                   _F(INTERSEC=('BCC1','BCC2',),
                                      NOM='BCC_SGN'),),)

#
#_____________________________________________________________________
#
#                    DEFINITION DU MODELE
#_____________________________________________________________________
#



MO=AFFE_MODELE(MAILLAGE=SSNV127E,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

SSNV127E=MODI_MAILLAGE(reuse =SSNV127E,
                       MAILLAGE=SSNV127E,
                       ORIE_PEAU_2D=_F(GROUP_MA='BCC1',),
                      );

SSNV127E=MODI_MAILLAGE(reuse =SSNV127E,
                       MAILLAGE=SSNV127E,
                       ORIE_PEAU_2D=_F(GROUP_MA='BCC2',),
                      );
#_____________________________________________________________________
#
#                    DEFINITION DES MATERIAUX
#_____________________________________________________________________
#



MAT1=DEFI_MATERIAU(ELAS=_F(E=2.10000000000E11,
                           NU=0.29999999999999999,),);
#
#


CHMAT=AFFE_MATERIAU(MAILLAGE=SSNV127E,
                    AFFE=_F(GROUP_MA='SURFF',
                            MATER=MAT1,),);
#_____________________________________________________________________
#
#                    AFFECTATION DES CONDITIONS AUX LIMITES
#                       ET DU CHARGEMENT
#_____________________________________________________________________
#



CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='CC4',
                                 DX=0.0,
                                 DY=0.0,),
                              _F(GROUP_NO='AD1',
                                 DX=0.0,),
                              _F(GROUP_NO='AD2',
                                 DX=0.0,),
                              _F(GROUP_NO='AD3',
                                 DX=0.0,),
                              _F(GROUP_NO='AD4',
                                 DX=0.0,),
                              _F(GROUP_NO='BD3',
                                 DX=0.0,),
                              _F(GROUP_NO='BD4',
                                 DX=0.0,),),
                    FORCE_NODALE=_F(GROUP_NO='O2',
                                    FY=-9.37500E5,),);
#
#

CHA2=DEFI_CONTACT(MODELE      = MO,
                  FORMULATION = 'DISCRETE',
                  FROTTEMENT  = 'COULOMB',
                  ZONE=_F(
                           ALGO_CONT     = 'LAGRANGIEN',
                           ALGO_FROT     = 'LAGRANGIEN',
                           COULOMB       = 0.4,
                           GROUP_MA_MAIT = 'BCC1',
                           GROUP_MA_ESCL = 'BCC2',
                           SANS_GROUP_NO = 'BCC_SGN',
                         ),
                  );


#_____________________________________________________________________
#
#               DEFINITION DES FONCTIONS TEMPORELLES
#_____________________________________________________________________
#



RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0,),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);
#_____________________________________________________________________
#
#                            RESOLUTION
#_____________________________________________________________________
#



U=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE=CHA1,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  =CHA2,
                COMP_ELAS=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='ELASTIQUE',
                          REAC_INCR=0),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20),
                );

U=CALC_CHAMP(reuse=U,CONTRAINTE=('SIGM_ELNO'),RESULTAT=U)

#
#

TPRESS=POST_RELEVE_T(ACTION=_F(INTITULE='PRESS',
                              GROUP_NO='RELEVE',
                              RESULTAT=U,
                              INST=1.,
                              NOM_CHAM='SIGM_ELNO',
                              NOM_CMP='SIXX',
                              REPERE='POLAIRE',
                              OPERATION='EXTRACTION',),);


TCONT=POST_RELEVE_T(ACTION=_F(INTITULE='CONT',
                              GROUP_NO='RELEVE',
                              RESULTAT=U,
                              INST=1.,
                              NOM_CHAM='VALE_CONT',
                              TOUT_CMP='OUI',
                              OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TPRESS)
IMPR_TABLE(TABLE=TCONT)


