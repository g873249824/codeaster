DEBUT(CODE=_F( NOM = 'SSNP151B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),PAR_LOT='OUI')

#-----------------------------------------------------------
# PARAMETRES DE LA ZONE COHESIVE
#-------------------------------------------------------------
tenac = 150.0
sigc  = 1800.0

# VALEURS DES COEFFICIENTS DE FORME DE LA LOI CZM
# CHOISIES DE TELLE SORTE QUE DELE = 0.01 ET DELP = 0.06
coee=0.08571428571429
coep=0.51428571428571

delc = 2*tenac/((sigc)*(1.-coee+coep))
dele=coee*delc
delp=coep*delc

#-----------------------------------------------------------
# LECTURE DU MAILLAGE
#-------------------------------------------------------------
MAIL0=LIRE_MAILLAGE(FORMAT='MED');

#-----------------------------------------------------------
# TRANSFORME EN MAILLAGE QUADRATIQUE
#-------------------------------------------------------------
MAIL=CREA_MAILLAGE(MAILLAGE  = MAIL0, LINE_QUAD = _F(TOUT='OUI'));

MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_FISSURE=_F(GROUP_MA='joint'));

#-----------------------------------------------------------
# MODELISATION DU MAILLAGE
#------------------------------------------------------------------
MO=AFFE_MODELE(MAILLAGE=MAIL,
               AFFE=(_F(GROUP_MA=('CT', 'goupi'),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D'),
            _F(GROUP_MA='joint',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D_INTERFACE'),
             ));

# DEFINITION DU REPERE LOCAL A LA FISSURE
ORIEN_FI=AFFE_CARA_ELEM(MODELE=MO,
                     MASSIF=( _F(GROUP_MA='joint',ANGL_REP=(-90, 0 ,-90))));

#-----------------------------------------------------------
# RELATION CONTRAINTE-DEFORMATION DE L'EPROUVETTE CT
#-------------------------------------------------------------
SIGM_F=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(0.0025,495.,
                           0.01,530.,
                           0.02,570.,
                           0.03,604.,
                           0.04,634.,
                           0.05,660.,
                           0.06,681.,
                           0.07,700.,
                           0.08,716.,
                           0.09,730.,
                           0.1,740.,
                           0.11,749.,
                           0.12,755.,
                           0.13,765.,
                           0.137,770.,
                           0.14,771.,
                           0.15,778.,
                           0.2,809.,
                           0.3,855.,
                           0.4,889.,
                           0.5,917.,
                           0.6,940.,
                           0.7,960.,
                           0.8,977.,
                           0.9,993.,
                           1.0,1008.,
                           ),PROL_DROITE='LINEAIRE');

#-----------------------------------------------------------
# MODELISATION DU MATERIAU DE L'EPROUVETTE CT, ZONE COHESIVE Y COMPRISE
#-------------------------------------------------------------
MA_EPR=DEFI_MATERIAU(ELAS=_F(E=2.07E5,
                             NU=0.3,
                             ALPHA=0.0),
                     TRACTION=_F(SIGM=SIGM_F),
                     RUPT_DUCT = _F(
                                    GC = tenac,
                                    SIGM_C = sigc,
                                    COEF_EXTR = coee,
                                    COEF_PLAS = coep,
                            ));

#-----------------------------------------------------------
# MODELISATION DU MATERIAU DE LA GOUPILLE
#-------------------------------------------------------------
MA_GOUPI=DEFI_MATERIAU(ELAS=_F(E=1E9,
                     NU=0.3));

CM=AFFE_MATERIAU(MAILLAGE=MAIL,
                 AFFE=(_F(GROUP_MA=('CT','joint'),
                         MATER=MA_EPR),
                       _F(GROUP_MA='goupi',
                         MATER=MA_GOUPI)));

#-----------------------------------------------------------
# CONDITIONS AUX LIMITES DE CHARGEMENT ET DE SYMETRIE
#-------------------------------------------------------------
CH=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_MA='goupi',
                               DY=4.0,
                               DX=0.0,
                               DZ=0.0),
                            _F(GROUP_MA='dz0',
                               DZ=0.0)));

#-----------------------------------------------------------
# CONDITION LIMITE DE SYMETRIE SUR LES ELEMENTS DE JOINT
#-------------------------------------------------------------
SYM=AFFE_CHAR_CINE(MODELE=MO,MECA_IMPO=(_F(GROUP_MA='joint_b',DY=0.0)));

#-----------------------------------------------------------
# LISTE DES INSTANTS DE CALCUL
#-------------------------------------------------------------
L_INST1=DEFI_LIST_REEL(DEBUT=0.0,
                       INTERVALLE=(_F(JUSQU_A=0.15,NOMBRE=3),
                                   _F(JUSQU_A=0.4,NOMBRE=10)),
                    );

LINST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                  LIST_INST=L_INST1),
                     ECHEC=_F(ACTION        = 'DECOUPE',
                              SUBD_METHODE  = 'MANUEL',
                              SUBD_NIVEAU   = 10));

#-----------------------------------------------------------
# LISTE DES INSTANTS D'ARCHIVAGE
#-------------------------------------------------------------
LIS_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=0.4,NOMBRE=8));

FCT = DEFI_FONCTION(
      NOM_PARA = 'INST',
          VALE     = (0.0, 0.0 ,1.0, 1.0),
                    )

#-----------------------------------------------------------
# CALCUL NON LINEAIRE
#-------------------------------------------------------------

UP=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CM,
                 CARA_ELEM  = ORIEN_FI,
                 EXCIT=(_F(CHARGE=CH,FONC_MULT = FCT),
                        _F(CHARGE=SYM),
                        ),
                 COMP_ELAS=_F(RELATION='ELAS',
                              GROUP_MA='goupi',
                              ),
                 COMP_INCR=(_F(RELATION='VMIS_ISOT_TRAC',
                              DEFORMATION='PETIT_REAC',
                              GROUP_MA='CT'),
                            _F(RELATION = 'CZM_TRA_MIX',
                               GROUP_MA = 'joint')),
                 INCREMENT=_F(LIST_INST=LINST,INST_FIN=0.3),
                 CONVERGENCE = _F(
                        RESI_REFE_RELA = 1.E-6,
                        SIGM_REFE      = sigc,
                        DEPL_REFE      = delc,
                        ITER_GLOB_MAXI = 12,
                  ),
                 SOLVEUR    = _F(METHODE = 'MUMPS'),
                 NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
                 ARCHIVAGE=_F(LIST_INST=LIS_ARCH),
               );

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(GROUP_MA='goupi'));

#-----------------------------------------------------------
# CALCUL DE CHAMPS COMPLEMENTAIRES
#-------------------------------------------------------------
UP=CALC_CHAMP(reuse =UP,
             RESULTAT=UP,
             FORCE='REAC_NODA');

#-----------------------------------------------------------
# FORCE APPLIQUEE SUR LA GOUPILLE
#-------------------------------------------------------------
TFORCE=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='force',
                               RESULTAT=UP,
                               NOM_CHAM='REAC_NODA',
                               GROUP_NO='goupi',
                               RESULTANTE='DY'));

COEF = FORMULE(VALE='4*INST',
                NOM_PARA='INST');

#-----------------------------------------------------------
# LA REPONSE GLOBALE (FORCE EN FONCTION DU DEPLACEMENT)
# ----------------------------------------------------------
FORCE=CALC_TABLE(TABLE=TFORCE,
                ACTION=_F(OPERATION='OPER',
                           FORMULE=COEF,
                           NOM_PARA='DEPLACE'));

#-----------------------------------------------------------
# TEST SUR LA REPONSE GLOBALE
# ----------------------------------------------------------
TEST_TABLE(
           VALE_CALC=4.4713199999999997E4,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=FORCE,
           FILTRE=_F(NOM_PARA='DEPLACE',
                     VALE=0.40000000000000002,),
           )

TEST_TABLE(
           VALE_CALC=5.0959099999999999E4,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=FORCE,
           FILTRE=_F(NOM_PARA='DEPLACE',
                     VALE=0.80000000000000004,),
           )

TEST_TABLE(
           VALE_CALC=5.22155E4,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=FORCE,
           FILTRE=_F(NOM_PARA='DEPLACE',
                     VALE=1.0,),
           )

TEST_TABLE(
           VALE_CALC=5.3212599999999999E4,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=FORCE,
           FILTRE=_F(NOM_PARA='DEPLACE',
                     VALE=1.2,),
           )

#-----------------------------------------------------------
# IMPRESSION DE LA FORCE EN FONCTION DU DEPLACEMENT
# ----------------------------------------------------------
IMPR_TABLE(TABLE=FORCE,
           FORMAT='TABLEAU',
           NOM_PARA=('DEPLACE','DY'));


FIN();
