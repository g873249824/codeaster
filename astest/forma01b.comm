                   VALE_CALC=299.23578099999997,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=-97.428384909,
                   VALE_REFE=-100.0,
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   RESULTAT=RESU2,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=-97.428384899999998,
