PRE_GIBI();

MAIL=LIRE_MAILLAGE();
#
# DEFINITION DES ELEMENTS FINIS UTILISES
#
#


MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(GROUP_MA=('TUYAU','EFOND'),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='DKT',),);
# ORIENTATION DES NORMALES AUX COQUES RENTRANTES DANS LE TUYAU
#


MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='TUYAU',
                                      VECT_NORM=(1.0,0.0,0.0),
                                      GROUP_NO='A1',),
                   MODELE=MODMECA,);
# DEFINITION DU MATERIAU UTILISE
#
#


ACIER=DEFI_MATERIAU(ELAS=_F(E=2.04000000000E11,
                            NU=0.3,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);
#
# CARACTERISTIQUES DES COQUES
#
#


CARA_COQ=AFFE_CARA_ELEM(MODELE=MODMECA,
                        COQUE=_F(GROUP_MA=('TUYAU','EFOND'),
                                 EPAIS=0.02,
                                 ANGL_REP=(0.0,-90.0),),);
#
# DEFINITION DES CONDITIONS AUX LIMITES
#    ENCASTREMENT SUR LA SECTION ENCAST
#
#


BLOQUAGE=AFFE_CHAR_MECA(MODELE=MODMECA,
                        DDL_IMPO=(_F(GROUP_MA='ENCAST',
                                     DX=0.0,
                                     DY=0.0,
                                     DZ=0.0,
                                     DRX=0.0,
                                     DRY=0.0,
                                     DRZ=0.0,),
                                  _F(GROUP_MA='SYMETRIE',
                                     DZ=0.0,
                                     DRX=0.0,
                                     DRY=0.0,),),);
#
# DEFINITION DES CHARGEMENTS
#    CHARGEMENT 1 : FY=100KN (FX,FZ,MX,MY,MZ NULS)
#    CHARGEMENT 2 : PRESSION INTERNE DE 15 MPA 
#
#

FYTOT = 100000.0;

EPTUB = 0.02;

REXT = 0.2;


RMOY=DEFI_VALEUR(R8=EVAL("""REXT-(EPTUB/2)"""),);

FYREP=DEFI_VALEUR(R8=EVAL("""FYTOT/2./PI/RMOY"""),);

RINT=DEFI_VALEUR(R8=EVAL("""RMOY-(EPTUB/2)"""),);

SINT=DEFI_VALEUR(R8=EVAL("""PI*(RINT*RINT)"""),);
#
# 2*PI*R*FY = 100 KN
#
#


CHARG1=AFFE_CHAR_MECA(MODELE=MODMECA,
                      FORCE_ARETE=_F(GROUP_MA='EFOND',
                                     FY=FYREP,),);
#
# 2*PI*R*FX = PINT * SINT ;
#

PINT = 15000000.0;


PRES=DEFI_VALEUR(R8=EVAL("""PINT*RINT/RMOY"""),);


CHARG2=AFFE_CHAR_MECA(MODELE=MODMECA,
                      FORCE_COQUE=_F(GROUP_MA='TUYAU',
                                     PRES=PRES,),);

RESU1=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARA_COQ,
                    EXCIT=(_F(CHARGE=BLOQUAGE,),
                           _F(CHARGE=CHARG1,),),);

RESU1=CALC_ELEM(reuse =RESU1,
                MODELE=MODMECA,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARA_COQ,
                OPTION=('SIGM_ELNO_DEPL','EQUI_ELNO_SIGM'),
                RESULTAT=RESU1,
                NIVE_COUCHE='SUP',);


RESU2=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARA_COQ,
                    EXCIT=(_F(CHARGE=BLOQUAGE,),
                           _F(CHARGE=CHARG2,),),);

RESU2=CALC_ELEM(reuse =RESU2,
                MODELE=MODMECA,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARA_COQ,
                OPTION='SIGM_ELNO_DEPL',
                RESULTAT=RESU2,
                NIVE_COUCHE='SUP',);

RESU2=CALC_ELEM(reuse =RESU2,
                MODELE=MODMECA,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARA_COQ,
                OPTION='EQUI_ELNO_SIGM',
                RESULTAT=RESU2,);

IMPR_RESU(MODELE=MODMECA,
          RESU=(_F(RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   GROUP_MA='EFOND',),
                _F(RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   GROUP_MA='EFOND',),),);

IMPR_RESU(MODELE=MODMECA,
          RESU=(_F(FORMAT='CASTEM',
                   MAILLAGE=MAIL,),
                _F(FORMAT='CASTEM',
                   RESULTAT=RESU1,
                   NOM_CHAM=('DEPL','SIGM_ELNO_DEPL','EQUI_ELNO_SIGM'),),
                _F(FORMAT='CASTEM',
                   NOM_CHAM=('DEPL','SIGM_ELNO_DEPL','EQUI_ELNO_SIGM'),
                   RESULTAT=RESU2,),),)

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(GROUP_MA='EFOND',),);

TABDEP1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPB1',
                                GROUP_NO='EFOND',
                                RESULTAT=RESU1,
                                NOM_CHAM='DEPL',
                                TOUT_CMP='OUI',
                                OPERATION='MOYENNE',),);

TABDEP2=POST_RELEVE_T(ACTION=_F(INTITULE='DEPB2',
                                GROUP_NO='EFOND',
                                RESULTAT=RESU2,
                                NOM_CHAM='DEPL',
                                TOUT_CMP='OUI',
                                OPERATION='MOYENNE',),);

IMPR_TABLE(TABLE=TABDEP1,);

IMPR_TABLE(TABLE=TABDEP2,);

TEST_TABLE(TABLE=TABDEP1,
           FILTRE=_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
           NOM_PARA='DX',
           VALE=-0.027601,
           PRECISION=1.E-4,
           REFERENCE='NON_REGRESSION',VERSION='6.3.20',);

TEST_TABLE(TABLE=TABDEP1,
           FILTRE=_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
           NOM_PARA='DY',
           VALE=0.1060267,
           PRECISION=1.E-4,
           REFERENCE='NON_REGRESSION',VERSION='6.3.20',);

TEST_TABLE(TABLE=TABDEP1,
           FILTRE=_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
           NOM_PARA='DRZ',
           VALE=0.03274179,
           PRECISION=1.E-4,
           REFERENCE='NON_REGRESSION',VERSION='6.3.20',);

TEST_TABLE(TABLE=TABDEP2,
           FILTRE=_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
           NOM_PARA='DX',
           VALE=-0.2846271,
           PRECISION=1.E-4,
           REFERENCE='NON_REGRESSION',VERSION='6.3.20',);

TEST_TABLE(TABLE=TABDEP2,
           FILTRE=_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
           NOM_PARA='DY',
           VALE=0.468679,
           PRECISION=1.E-4,
           REFERENCE='NON_REGRESSION',VERSION='6.3.20',);

TEST_TABLE(TABLE=TABDEP2,
           FILTRE=_F(NOM_PARA='QUANTITE',
                     VALE_K='MOMENT_0',),
           NOM_PARA='DRZ',
           VALE=0.1305473,
           PRECISION=1.E-4,
           REFERENCE='NON_REGRESSION',VERSION='6.3.20',);

FIN();
