                   CRITERE='RELATIF',
                   PRECISION=0.14999999999999999,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   RESULTAT=RESU,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=-85.881414500000005,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
