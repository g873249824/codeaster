                  NOEUD='N2',
                  NOM_CMP='PRE1',
                  VALE_CALC= 13442.59996853507,
                  VALE_REFE = 1.34554E+04,
                  CRITERE='RELATIF',
                  PRECISION=0.05,
                  ),
          )
TEST_RESU(RESU=_F(INST=1800.,
