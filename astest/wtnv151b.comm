    TYPE_CHAM='ELGA_SIEF_R'
)
R_HH2M = STAT_NON_LINE(
    CHAM_MATER=fieldmat,
    COMPORTEMENT=_F(
        RELATION='KIT_HH2M',
        RELATION_KIT=('HYDR_UTIL', 'LIQU_AD_GAZ_VAPE', 'ELAS'),
        TOUT='OUI'
    ),
    CONVERGENCE=_F(ITER_GLOB_MAXI=10,RESI_GLOB_RELA=1.E-6),
