                    _F(GROUP_MA=('DROITE', ),COEF_11=EC11,COEF_12=0.,PRE1_EXT=1.5E8,
                                             COEF_21=0.,COEF_22=0.,PRE2_EXT=1.e5,),
                  ), );


DEPLHH2M = CREA_CHAMP(
    AFFE=(
        _F(NOM_CMP=('PRE1', ), TOUT='OUI', VALE=(10000.0,)), 
