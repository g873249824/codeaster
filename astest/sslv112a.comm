               TYPE_RESU='EVOL_ELAS',
               NOM_CHAM='DEPL',
               AFFE=_F(CHAM_GD=MONDEPL,
                       MODELE=MO,
                       CHAM_MATER=CM,
                       INST=0.0,),);

