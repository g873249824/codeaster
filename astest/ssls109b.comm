                         CHAM_MATER=MATAF,    CARA_ELEM=LACAR,
                             OPTION='SIEF_ELGA_DEPL' )

RESU0=CALC_NO(  reuse=RESU0,   RESULTAT=RESU0,  MODELE=LEMOD,
