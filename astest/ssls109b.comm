                               CARA_ELEM=LACAR,
                               EXCIT=_F(  CHARGE = LECHAR,    )       )

RESU0=CALC_ELEM(reuse=RESU0,     RESULTAT=RESU0,        MODELE=LEMOD,
                         CHAM_MATER=MATAF,    CARA_ELEM=LACAR,
                             OPTION='EFGE_ELNO_DEPL' )

