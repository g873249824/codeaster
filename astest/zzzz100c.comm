L02=DEFI_LIST_REEL(  DEBUT=0.,
                         INTERVALLE=_F(  JUSQU_A = 4.,  NOMBRE = DEUX) )

DFC1=DEFI_FONCTION(     NOM_PARA='INST',    NOM_RESU='FREQ',
                          VALE=(0., 1., 2., 2.,  4., 4., ),
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='LINEAIRE'
                     )

