                                   _F(CHARGE = CHC,),
                                   _F(CHARGE = HYDR,),
                                  ),
                    CONTACT =  CTXFEM,
                    COMPORTEMENT   = _F(RELATION       = 'KIT_HM', PARM_THETA=0.56999999999999995,
                                        RELATION_KIT   = ('ELAS', 
                                                          'LIQU_SATU',
                                                          'HYDR_UTIL',),
                                        RESI_INTE_RELA = 1.E-6,
                                        ITER_INTE_PAS  = -10,
                                        ITER_INTE_MAXI = 100,
                                       ),
                    NEWTON      = _F(MATRICE   = 'TANGENTE',
                                     REAC_ITER = 1,
                                    ),
                    CONVERGENCE = _F(RESI_GLOB_RELA = 1.E-6,
                                     ITER_GLOB_MAXI = 50,
                                    ),
                    SOLVEUR     = _F(METHODE='MUMPS',
                                     NPREC=-1),
                    INCREMENT   = _F(LIST_INST = L_BINS,),
                    INFO=1,
