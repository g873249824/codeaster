                             _F(GROUP_MA=('HAUT'),DY=-0.0001),
                            ),
                   INFO=1,);

def flux(x,y) :
   if x> 4.9:    return 0.04
   if x< 4.9:    return 0

FLUX = FORMULE(VALE='flux(X,Y)',
                  NOM_PARA=('X','Y'),);

