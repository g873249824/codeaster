                  NUME_ORDRE=4,
                  PARA='FREQ',
                  VALE=5506.33,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',),);

# CHARGEMENT DE TYPE TORSION : LA LISTE D INSTANT EST DIFFERENTE CAR
# LA VITESSE DE L ONDE DE TORSION EST INFERIEURE A CELLE DE TRACTION
# D OU UN TEMPS SUPERIEUR POUR ATTEINDRE LE BOUT DE LA POUTRE

DYNATRAM=DYNA_LINE_TRAN(MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                        CARA_ELEM=CARAC,
                        MATR_MASS=MASSE,
                        MATR_RIGI=RIGID,
                        NEWMARK=_F(),
                        EXCIT=(_F(CHARGE=CHARMX,),
                               _F(CHARGE=CL,),),
                        INCREMENT=_F(LIST_INST=LISTE2,),
                        ARCHIVAGE=_F(PAS_ARCH=100,),);

DYNATRAM=CALC_ELEM(reuse =DYNATRAM,
                   MODELE=MODELE,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CARAC,
                   RESULTAT=DYNATRAM,
                   OPTION=('SIEF_ELGA_DEPL','SIEF_ELNO_ELGA',),);

DYNATRAM=CALC_NO(reuse =DYNATRAM,
                 RESULTAT=DYNATRAM,
                 OPTION='REAC_NODA',
                 MODELE=MODELE,
                 CHAM_MATER=CHMAT,
                 CARA_ELEM=CARAC,
                 EXCIT=(_F(CHARGE=CHARMX,),
                        _F(CHARGE=CL,),),);

# TEST SUR LES DEPLACEMENTS

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=5,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DRX',
                  GROUP_NO='B',
                  VALE=8.66477582461e-09,
                  PRECISION=0.0015,
                  REFERENCE='ANALYTIQUE',),);

