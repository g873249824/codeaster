                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DX',
                  GROUP_NO='A',
                  VALE=-1.99999819062,
                  PRECISION=0.05,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                  NUME_ORDRE=20,
                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DX',
                  GROUP_NO='B',
                  VALE=0.0,
                  CRITERE='ABSOLU',
                  REFERENCE='ANALYTIQUE',),);

# RECHERCHE DES MODES PROPRES DE TRACTIONS/COMPRESSIONS

MODE_DX=MODE_ITER_INV(MATR_A=RIGID,
                      MATR_B=MASSE,
                      CALC_FREQ=_F(
                      OPTION='PROCHE',
                      FREQ=(1263.497,3790.49,6317.484,8844.4477,11371.473,),),);

TEST_RESU(RESU=_F(RESULTAT=MODE_DX,
                  NUME_ORDRE=1,
                  PARA='FREQ',
                  VALE=1263.497,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=MODE_DX,
                  NUME_ORDRE=2,
                  PARA='FREQ',
