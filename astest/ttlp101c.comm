DEBUT(CODE=_F(NOM='TTLP101C', NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'), IGNORE_ALARM=('CALCULEL2_63'))

#----------------------------------------------
#            MAILLAGE
#----------------------------------------------

MA=LIRE_MAILLAGE(INFO=1,FORMAT='MED',);

# longueur du cote de la plaque
L = 1.

# GROUPES SUR LESQUELS ON VA IMPOSER LES DIRICHLETS
MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(GROUP_MA='SURFSUP',),
                             _F(GROUP_MA='SURFINF',),),)

#----------------------------------------------
#            MODELE ET FISSURE
#----------------------------------------------

MODTHIN=AFFE_MODELE(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='THERMIQUE',
                            MODELISATION='3D',),);

FISSTH=DEFI_FISS_XFEM(MODELE=MODTHIN,
                      DEFI_FISS=_F(FORM_FISS='DEMI_PLAN',
                                   PFON   =( 0.5*L, 0.5*L, 0.,),
                                   NORMALE=(    0.,    1., 0.,),
                                   DTAN   =(   -1.,    0., 0.,),),
                      INFO=1,);

MODTHX=MODI_MODELE_XFEM(MODELE_IN=MODTHIN,
                        FISSURE=FISSTH,
                        INFO=2,);

#----------------------------------------------
#            MATERIAU
#----------------------------------------------

MATE=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,RHO_CP = 2.0))

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    MODELE=MODTHX,
                    AFFE=_F(TOUT='OUI',MATER=MATE,),)


#----------------------------------------------
#            CHARGEMENT THERMIQUE
#----------------------------------------------

TIMPSUP=DEFI_FONCTION(NOM_PARA='INST',
                      NOM_RESU='TEMP',  
        VALE=(0.,    20. ,
              1.,    40.,))

TIMPINF=DEFI_FONCTION(NOM_PARA='INST',
                      NOM_RESU='TEMP',  
        VALE=(0.,    10. ,
              1.,    20.,))

CHTH=AFFE_CHAR_THER_F(MODELE=MODTHX,
                      TEMP_IMPO=(
                               _F(GROUP_NO = 'SURFSUP',TEMP = TIMPSUP),
                               _F(GROUP_NO = 'SURFINF',TEMP = TIMPINF),
                               ))

COECHP=DEFI_CONSTANTE(VALE=2.)

CHTHEPF=AFFE_CHAR_THER_F(MODELE=MODTHX,
                         ECHANGE_PAROI=_F(FISSURE = FISSTH,
                                          COEF_H  = COECHP,),)

#----------------------------------------------
#            CALCUL THERMIQUE LINEAIRE TRANSITOIRE
#----------------------------------------------

LR8=DEFI_LIST_REEL(DEBUT=0.,
                   INTERVALLE=(_F(JUSQU_A = 1., NOMBRE = 5)))

TEMPEX=THER_LINEAIRE(MODELE=MODTHX,
                     CHAM_MATER=CHMAT,
                    EXCIT=(
                           _F(CHARGE=CHTH,),
                           _F(CHARGE=CHTHEPF,),
                           ),
                     ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
                     INCREMENT=_F( LIST_INST = LR8),                    
                     )

#----------------------------------------------
#            POST-TRAITEMENTS
#----------------------------------------------

MA_VISU=POST_MAIL_XFEM(MODELE=MODTHX)

# GROUPES SUR LESQUELS ON VA TESTER LES VALEURS DE T
LZ = 0.1
hsain = L/11.
hfiss = 0.5*hsain
MA_VISU=DEFI_GROUP(reuse =MA_VISU,
                   MAILLAGE=MA_VISU,
                   CREA_GROUP_MA=(
                                  _F(NOM='MSUPTMP1',OPTION='SPHERE',
                                     POINT=(L,0.5*L+hfiss,0.),RAYON=1.e-6*hfiss),
                                  _F(NOM='MSUPTMP2',OPTION='SPHERE',
                                     POINT=(L,0.5*L+hfiss,LZ),RAYON=1.e-6*hfiss),
                                  _F(NOM='MINFTMP1',OPTION='SPHERE',
                                     POINT=(L,0.5*L-hfiss,0.),RAYON=1.e-6*hfiss),
                                  _F(NOM='MINFTMP2',OPTION='SPHERE',
                                     POINT=(L,0.5*L-hfiss,LZ),RAYON=1.e-6*hfiss),
                                  _F(NOM='MASUPTMP',UNION=('MSUPTMP1','MSUPTMP2'),),
                                  _F(NOM='MAINFTMP',UNION=('MINFTMP1','MINFTMP2'),),
                                  ),
                   CREA_GROUP_NO=(
                                  _F(GROUP_MA='MASUPTMP',),
                                  _F(GROUP_MA='MAINFTMP',),
                                  _F(NOM='GNFON1',OPTION='ENV_SPHERE',
                                     POINT=(0.5*L,0.5*L,0.),RAYON=1.e-6*hfiss,
                                     PRECISION=1.e-6*hfiss),
                                  _F(NOM='GNFON2',OPTION='ENV_SPHERE',
                                     POINT=(0.5*L,0.5*L,LZ),RAYON=1.e-6*hfiss,
                                     PRECISION=1.e-6*hfiss),
                                  _F(NOM='GNOTMP1',OPTION='ENV_SPHERE',
                                     POINT=(L,0.5*L,0.    ),RAYON=1.e-6*hfiss,PRECISION=1.e-6*hfiss),
                                  _F(NOM='GNOTMP2',OPTION='ENV_SPHERE',
                                     POINT=(L,0.5*L,LZ    ),RAYON=1.e-6*hfiss,PRECISION=1.e-6*hfiss),
#                                   _F(NOM='GNOTMP',UNION=('GNOTMP1','GNOTMP2','GNOTMP3'),),
                                  _F(NOM='TESTSUP1',INTERSEC=('GNOTMP1','MASUPTMP')),
                                  _F(NOM='TESTSUP2',INTERSEC=('GNOTMP2','MASUPTMP')),
                                  _F(NOM='TESTINF1',INTERSEC=('GNOTMP1','MAINFTMP')),
                                  _F(NOM='TESTINF2',INTERSEC=('GNOTMP2','MAINFTMP')),
                                  ),)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_VISU,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='THERMIQUE',
                             MODELISATION='3D',),)

RES_VISU=POST_CHAM_XFEM(MODELE_VISU = MOD_VISU,
                        RESULTAT    = TEMPEX)

#----------------------------------------------
#            RELEVES
#----------------------------------------------

TEMPSUP1=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='TESTSUP1',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

TEMPSUP2=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='TESTSUP2',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

TEMPINF1=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='TESTINF1',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

TEMPINF2=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='TESTINF2',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

TEMPFON1=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='GNFON1',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

TEMPFON2=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='GNFON2',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

#----------------------------------------------
#            TESTS
#----------------------------------------------

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=29.147198346097,
           VALE_REFE=29.156091860463,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPSUP1,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=29.147198346097,
           VALE_REFE=29.156091860463,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPSUP1,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=29.147196329322,
           VALE_REFE=29.156091860463,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPSUP2,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=29.147196329322,
           VALE_REFE=29.156091860463,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPSUP2,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=23.407327209161,
           VALE_REFE=23.393394671258,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPINF1,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=23.407327209161,
           VALE_REFE=23.393394671258,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPINF1,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=23.407330322254,
           VALE_REFE=23.393394671258,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPINF2,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=23.407330322254,
           VALE_REFE=23.393394671258,
           PRECISION=1.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPINF2,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=26.292041634221,
           VALE_REFE=26.25259365185,
           PRECISION=5.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPFON1,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=26.292041634221,
           VALE_REFE=26.25259365185,
           PRECISION=5.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPFON1,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=26.291992880553,
           VALE_REFE=26.25259365185,
           PRECISION=5.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPFON2,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=26.291992880553,
           VALE_REFE=26.25259365185,
           PRECISION=5.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPFON2,)

#----------------------------------------------
#            IMPRESSIONS
#----------------------------------------------

IMPR_TABLE(TABLE=TEMPSUP1)
IMPR_TABLE(TABLE=TEMPSUP2)
IMPR_TABLE(TABLE=TEMPINF1)
IMPR_TABLE(TABLE=TEMPINF2)
IMPR_TABLE(TABLE=TEMPFON1)
IMPR_TABLE(TABLE=TEMPFON2)

IMPR_RESU(FORMAT='MED',UNITE=83,RESU=_F(MAILLAGE=MA))
IMPR_RESU(FORMAT='MED',UNITE=81,RESU=_F(MAILLAGE=MA_VISU))
IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_VISU))

FIN();
