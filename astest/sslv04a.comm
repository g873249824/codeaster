DEBUT(CODE=_F(NOM='SSLV04A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',);

DEFI_GROUP(reuse=M, MAILLAGE=M,
           CREA_GROUP_NO=_F(NOM = 'DIRI', GROUP_MA = 'DIRI'));

MO=AFFE_MODELE(MAILLAGE=M,
               VERIF='MAILLE',
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = '3D'));

MA=DEFI_MATERIAU(ELAS=_F(E = 200000.,
                         NU = 0.3,
                         ALPHA = 0.));

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT = 'OUI',
                         MATER = MA))

CH=AFFE_CHAR_MECA(MODELE=MO,DDL_IMPO=(_F(GROUP_NO = 'DIRI', DY = 0.),
                                      _F(GROUP_NO = 'F',    DZ = 0.)),
                            FACE_IMPO=_F(GROUP_MA = 'DNOR', DNOR = 0.),
                            PRES_REP =_F(GROUP_MA = 'PRES', PRES = 60.));

nor_DNOR = CREA_CHAMP(TYPE_CHAM = 'NOEU_GEOM_R',
                      OPERATION = 'NORMALE',
                      MODELE    = MO,
                      GROUP_MA  = 'DNOR');

IMPR_RESU(RESU=_F(CHAM_GD= nor_DNOR));

TEST_RESU(CHAM_NO=(
           _F(CHAM_GD = nor_DNOR, REFERENCE = 'ANALYTIQUE',
              GROUP_NO = 'E', VALE = -0.707, NOM_CMP = 'X'),
           _F(CHAM_GD = nor_DNOR, REFERENCE = 'ANALYTIQUE',
              GROUP_NO = 'E', VALE =  0.707, NOM_CMP = 'Y'),
           _F(CHAM_GD = nor_DNOR, REFERENCE = 'ANALYTIQUE',
              GROUP_NO = 'F', VALE = -0.707, NOM_CMP = 'X'),
           _F(CHAM_GD = nor_DNOR, REFERENCE = 'ANALYTIQUE',
              GROUP_NO = 'F', VALE =  0.707, NOM_CMP = 'Y')));

nor_PRES = CREA_CHAMP(TYPE_CHAM = 'NOEU_GEOM_R',
                      OPERATION = 'NORMALE',
                      MODELE    = MO,
                      GROUP_MA  = 'PRES'  );

IMPR_RESU(RESU=_F(CHAM_GD= nor_PRES ));

TEST_RESU(CHAM_NO=(
           _F(CHAM_GD = nor_PRES, REFERENCE = 'ANALYTIQUE',
              GROUP_NO = 'A', VALE = -1. , NOM_CMP = 'X'),
           _F(CHAM_GD = nor_PRES, CRITERE = 'ABSOLU', PRECISION = 1.5E-2,
              GROUP_NO = 'A', VALE =  0. , NOM_CMP = 'Y'),
           _F(CHAM_GD = nor_PRES, REFERENCE = 'ANALYTIQUE', PRECISION = 1.5E-2,
              GROUP_NO = 'E', VALE = -0.707, NOM_CMP = 'X'),
           _F(CHAM_GD = nor_PRES, REFERENCE = 'ANALYTIQUE', PRECISION = 1.5E-2,
              GROUP_NO = 'E', VALE = -0.707, NOM_CMP = 'Y')));

RESU=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CM,
                   EXCIT=_F(CHARGE = CH));


