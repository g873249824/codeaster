                   NOM_CMP='DX',
                   VALE_CALC= 1.49999997E-03,
                   VALE_REFE=150./k_n,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='ANALYTIQUE',
