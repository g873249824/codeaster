MA_0=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED');

MA_0=DEFI_GROUP(reuse =MA_0,
                 MAILLAGE=MA_0,
                 CREA_GROUP_NO = (
                        _F(
                           NOM = 'BJOI',
                           GROUP_MA = 'LJOI',
                           OPTION = 'NOEUD_ORDO',
                           GROUP_NO_ORIG = 'H2',
                           GROUP_NO_EXTR = 'H3'),
                        _F(
                           NOM = 'HJOI',
                           GROUP_MA = 'LESC',
                           OPTION = 'NOEUD_ORDO',
                           GROUP_NO_ORIG = 'A',
                           GROUP_NO_EXTR = 'B'),
                       # _F(TOUT_GROUP_MA='OUI'),
                       )
                 );


MAILL = CREA_MAILLAGE(
  MAILLAGE = MA_0,
  CREA_FISS = _F(
    NOM = 'FISS',
    GROUP_NO_2 = 'HJOI',
    GROUP_NO_1 = 'BJOI',
    PREF_MAILLE = 'MJ',
    )
  )



MAILL=DEFI_GROUP(reuse =MAILL,
                 MAILLAGE=MAILL,
                 CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'));

MODE=AFFE_MODELE(MAILLAGE=MAILL,
                 VERIF='MAILLE',
                 AFFE=(_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='D_PLAN'),
                       _F(GROUP_MA='RES_LAT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T'),
                       _F(GROUP_MA='RES_HAUT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T'),
                       _F(
                         GROUP_MA     = 'FISS',
                         PHENOMENE    = 'MECANIQUE',
                         MODELISATION = 'PLAN_JOINT'
                         ),
                      ),

 );

#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------

CARA=AFFE_CARA_ELEM(MODELE=MODE,
                    DISCRET_2D=(_F(GROUP_MA='RES_LAT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(2.0,0.0)),
                                _F(GROUP_MA='RES_HAUT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(0.005,0.0)),
                              ));

#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------

MPATIN=DEFI_MATERIAU(ELAS=_F(E=2.10000E5,
                             NU=0.0));

MBATI=DEFI_MATERIAU(ELAS=_F(E=1.E5,
                            NU=0.0));

# MATERIAU JOINT MECANIQUE AVEC FROTTEMENT
k_n=1.E5
MAT_JF=DEFI_MATERIAU(
                  JOINT_MECA_FROT=_F(K_N=k_n,
                                     MU=0.3,
                                ));

CM=AFFE_MATERIAU(MAILLAGE=MAILL,
                 MODELE=MODE,
                 AFFE=(_F(GROUP_MA='PATIN',
                          MATER=MPATIN),
                       _F(GROUP_MA='BATI',
                          MATER=MBATI),
                        _F(GROUP_MA='FISS',
                          MATER=MAT_JF),
                       ));
#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------

# CONDITIONS AUX LIMITES
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='BATI',
                                     DX=0.0,DY=0.0),
                                  _F(GROUP_NO=('R1','R2','X','Y'),
                                     DX=0.0,DY=0.0)));

# CHARGEMENT 1 : APPLICATION DE LA PRESSION NORMALE
PNOR=AFFE_CHAR_MECA(MODELE=MODE,
                    PRES_REP=_F(GROUP_MA='FACESUP',
                                PRES=300.0));

# CHARGEMENT 2 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*0.99)
PTAN_99=AFFE_CHAR_MECA(MODELE=MODE,
                       PRES_REP=_F(GROUP_MA='FACELAT',
                                   PRES=178.19999999999999));

# CHARGEMENT 3 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*1.01)
PTAN_101=AFFE_CHAR_MECA(MODELE=MODE,
                        PRES_REP=_F(GROUP_MA='FACELAT',
                                    PRES=181.80000000000001));


#---------------------------------------------------------------------
#                         RESOLUTION
#---------------------------------------------------------------------


RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,
                          1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE');

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=2));


# CHARGEMENT 1
U_NOR=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE),
                           _F(CHARGE=COND_LIM)),
                    COMP_INCR=(_F(RELATION='ELAS',
                                 GROUP_MA=('BATI','PATIN')),
                               _F(RELATION = 'JOINT_MECA_FROT',
                                   GROUP_MA = 'FISS'),
                               ),
                    INCREMENT=_F(LIST_INST=L_INST),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1),
                    SOLVEUR=_F(METHODE='MUMPS',
   
                              ),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=30),
                    );

U_NOR=CALC_CHAMP(reuse =U_NOR,
              RESULTAT=U_NOR,
              FORCE='REAC_NODA',

              );

# CHARGEMENT 2
U_99=STAT_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CM,
                   CARA_ELEM=CARA,
                   EXCIT=(_F(CHARGE=PNOR,
                             FONC_MULT=RAMPE),
                          _F(CHARGE=PTAN_99,
                             FONC_MULT=RAMPE),
                          _F(CHARGE=COND_LIM)),
                    COMP_INCR=(_F(RELATION='ELAS',
                                 GROUP_MA=('BATI','PATIN')),
                               _F(RELATION = 'JOINT_MECA_FROT',
                                   GROUP_MA = 'FISS'),
                               ),
                    INCREMENT=_F(LIST_INST=L_INST),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1),
                    SOLVEUR=_F(METHODE='MUMPS',
   
                              ),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=30),
                    );

U_99=CALC_CHAMP(reuse =U_99,
             RESULTAT=U_99,
             FORCE='REAC_NODA',

              );

# CHARGEMENT 3
U_101=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE),
                           _F(CHARGE=PTAN_101,
                              FONC_MULT=RAMPE),
                           _F(CHARGE=COND_LIM)),
                    COMP_INCR=(_F(RELATION='ELAS',
                                 GROUP_MA=('BATI','PATIN')),
                               _F(RELATION = 'JOINT_MECA_FROT',
                                   GROUP_MA = 'FISS'),
                               ),
                    INCREMENT=_F(LIST_INST=L_INST),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1),
                    SOLVEUR=_F(METHODE='MUMPS',
   
                              ),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=30),
                    );

U_101=CALC_CHAMP(reuse =U_101,
              RESULTAT=U_101,
              FORCE='REAC_NODA',

              );


