                   NOM_CMP='DY',
                   VALE_CALC=-2.59807609E-03,
                   VALE_REFE=-sqrt(3)*150./k_n,
                   PRECISION=1.E-2,
                   ),
                ),
          )

TU_NOR=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
