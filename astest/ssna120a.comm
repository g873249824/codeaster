DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),PAR_LOT='OUI',DEBUG=_F(SDVERI='OUI'))

#-----------------------------------------------------------
# PARAMETRES DE LA ZONE COHESIVE
#-------------------------------------------------------------
tenac = 130.0
sigc  = 1200.0

# VALEURS DES COEFFICIENTS DE FORME DE LA LOI CZM
# CHOISIES DE TELLE SORTE QUE DELE = 0.01 ET DELP = 0.07
coee=0.06382978723404
coep=0.44680851063830

delc = 2*tenac/((sigc)*(1.-coee+coep))
dele=coee*delc
delp=coep*delc

#-----------------------------------------------------------
# LECTURE DU MAILLAGE
#-------------------------------------------------------------
MAIL0=LIRE_MAILLAGE(FORMAT='MED');

#-----------------------------------------------------------
# TRANSFORME EN MAILLAGE QUADRATIQUE
#-------------------------------------------------------------
MAIL=CREA_MAILLAGE(MAILLAGE  = MAIL0, LINE_QUAD = _F(TOUT='OUI'));

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(GROUP_MA='top'));

MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_FISSURE=_F(GROUP_MA='joint'),
                   INFO=1,
                   );

#-----------------------------------------------------------
# MODELISATION DU MAILLAGE
#------------------------------------------------------------------
MO=AFFE_MODELE(MAILLAGE=MAIL,
               AFFE=(_F(GROUP_MA=('cylind', 'sym_b', 'top', 'axe'),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS'),
                    _F(GROUP_MA='joint',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS_INTERFACE'),
                     ));

ORIEN_FI=AFFE_CARA_ELEM(MODELE=MO,
                     MASSIF=( _F(GROUP_MA='joint',ANGL_REP=(90, 0 ,0))));

#-----------------------------------------------------------
# RELATION CONTRAINTE-DEFORMATION DE L'EPROUVETTE CT
#-------------------------------------------------------------
SIGM_F=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(0.0025,495.,
                           0.01,530.,
                           0.02,570.,
                           0.03,604.,
                           0.04,634.,
                           0.05,660.,
                           0.06,681.,
                           0.07,700.,
                           0.08,716.,
                           0.09,730.,
                           0.1,740.,
                           0.11,749.,
                           0.12,755.,
                           0.13,765.,
                           0.137,770.,
                           0.14,771.,
                           0.15,778.,
                           0.2,809.,
                           0.3,855.,
                           0.4,889.,
                           0.5,917.,
                           0.6,940.,
                           0.7,960.,
                           0.8,977.,
                           0.9,993.,
                           1.0,1008.,
                           ),PROL_DROITE='LINEAIRE');

#-----------------------------------------------------------
# MODELISATION DU MATERIAU DE L'EPROUVETTE AE, ZONE COHESIVE Y COMPRISE
#-------------------------------------------------------------
MA_EPR=DEFI_MATERIAU(ELAS=_F(E=2.07E5,
                             NU=0.3,
                             ALPHA=0.0),
                             TRACTION=_F(SIGM=SIGM_F),
                             RUPT_DUCT = _F(
                                     GC = tenac,
                                     SIGM_C = sigc,
                                     COEF_EXTR = coee,
                                     COEF_PLAS = coep,
                            ));

CM=AFFE_MATERIAU(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         MATER=MA_EPR));

#-----------------------------------------------------------
# CONDITIONS AUX LIMITES DE CHARGEMENT ET DE SYMETRIE
#-------------------------------------------------------------
CH=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_MA='axe',
                               DX=0.0),
                            _F(GROUP_MA='top',
                               DY=0.5)));

#-----------------------------------------------------------
# CONDITION LIMITE DE SYMETRIE SUR LES ELEMENTS DE JOINT
#-------------------------------------------------------------
SYM=AFFE_CHAR_MECA(MODELE=MO,DDL_IMPO=(_F(GROUP_MA='joint_b',DY=0.0)));

#-----------------------------------------------------------
# LISTE DES INSTANTS DE CALCUL
#-------------------------------------------------------------
L_INST1=DEFI_LIST_REEL(DEBUT=0.0,
                       INTERVALLE=_F(JUSQU_A=1.0,
                                     NOMBRE=20));

LINST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                  LIST_INST=L_INST1),
                     ECHEC=_F(ACTION='DECOUPE',
                                 SUBD_METHODE='MANUEL',
                              SUBD_NIVEAU=10));

#-----------------------------------------------------------
# LISTE DES INSTANTS D'ARCHIVAGE
#-------------------------------------------------------------
LIS_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=20));

FCT = DEFI_FONCTION(
          NOM_PARA = 'INST',
              VALE     = (0.0, 0.0 ,1.0, 1.0),
                    )

#-----------------------------------------------------------
# CALCUL NON LINEAIRE
#-------------------------------------------------------------
UP=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CM,
                         CARA_ELEM  = ORIEN_FI,
                 EXCIT=(_F(CHARGE=CH,FONC_MULT = FCT),
                        _F(CHARGE=SYM),
                        ),
                 COMPORTEMENT=(_F(RELATION='VMIS_ISOT_TRAC',
                               DEFORMATION='PETIT_REAC',
                               GROUP_MA='cylind'),
                            _F(RELATION = 'CZM_TRA_MIX',
                               GROUP_MA = 'joint')),
                 INCREMENT=_F(LIST_INST=LINST),
                 CONVERGENCE = _F(
                                  RESI_REFE_RELA = 1.E-6,
                                  SIGM_REFE      = sigc,
                                  DEPL_REFE      = delc,
                                  ITER_GLOB_MAXI = 12,
                  ),
                 SOLVEUR    = _F(METHODE = 'MUMPS'),
                 NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
                 ARCHIVAGE=_F(LIST_INST=LIS_ARCH),
                           );

#-----------------------------------------------------------
# CALCUL DE CHAMPS COMPLEMENTAIRES
#-------------------------------------------------------------
UP=CALC_CHAMP(reuse =UP,
             RESULTAT=UP,
             FORCE='REAC_NODA');


# INTERPOLATION NODALE DES CHAMPS AUX PG
UP=CALC_CHAMP(reuse =UP,
              RESULTAT=UP,
              VARI_INTERNE='VARI_ELNO',
              CONTRAINTE='SIEF_ELNO',
              GROUP_MA='joint'
              );

UP=CALC_CHAMP(
              reuse =UP,
              RESULTAT=UP,
              VARI_INTERNE='VARI_NOEU',
              CONTRAINTE='SIEF_NOEU',
              GROUP_MA='joint'
              );


contra=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='Contraction diametrale',
                                RESULTAT=UP,
                                NOM_CHAM='DEPL',
                                GROUP_NO='fond_ent',
                                NOM_CMP='DX'));

force=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='force',
                               RESULTAT=UP,
                               NOM_CHAM='REAC_NODA',
                               GROUP_NO='top',
                               RESULTANTE='DY'));

coefDX = FORMULE(VALE='-2*DX',
                NOM_PARA='DX');

coefDY = FORMULE(VALE='2*DY*3.1415926/1000',
                NOM_PARA='DY');

#-----------------------------------------------------------
# CONTRACTION DU DIAMETRE
#-------------------------------------------------------------
contra1=CALC_TABLE(TABLE=contra,
                ACTION=_F(OPERATION='OPER',
                          FORMULE=coefDX,
                          NOM_PARA='NDX'));

#-----------------------------------------------------------
# FORCE APPLIQUEE SUR L'EPROUVETTE
#-------------------------------------------------------------
force1=CALC_TABLE(TABLE=force,
                ACTION=_F(OPERATION='OPER',
                          FORMULE=coefDY,
                          NOM_PARA='FR'));

#-----------------------------------------------------------
# TEST SUR LE CONTRACTION DU DIAMETRE
# ----------------------------------------------------------
TEST_TABLE(
           VALE_CALC=0.60577645811890,
           NOM_PARA='NDX',
           TABLE=contra1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6,),
           )

#-----------------------------------------------------------
# TEST SUR LA FORCE
# ----------------------------------------------------------
TEST_TABLE(
           VALE_CALC=28.869329959747,
           NOM_PARA='FR',
           TABLE=force1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6,),
           )

#-----------------------------------------------------------
# TEST SUR LE CONTRACTION DU DIAMETRE
# ----------------------------------------------------------
TEST_TABLE(
           VALE_CALC=0.93163276664640,
           NOM_PARA='NDX',
           TABLE=contra1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8,),
           )

#-----------------------------------------------------------
# TEST SUR LA FORCE
# ----------------------------------------------------------
TEST_TABLE(
           VALE_CALC=23.602269800408,
           NOM_PARA='FR',
           TABLE=force1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8,),
           )

#-----------------------------------------------------------
# TEST SUR LE CONTRACTION DU DIAMETRE
# EN COMPARANT AVEC LES RESULTATS ISSUS
# D'UN CALCUL DE COMPORTEMENT ROUSSELIER
# ----------------------------------------------------------
TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.05,
           VALE_CALC=0.33326151133450,
           VALE_REFE=0.349885,
           NOM_PARA='NDX',
           TABLE=contra1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4,),
           )

#-----------------------------------------------------------
# TEST SUR LA FORCE
# EN COMPARANT AVEC LES RESULTATS ISSUS
# D'UN CALCUL DE COMPORTEMENT ROUSSELIER
# ----------------------------------------------------------
TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.05,
           VALE_CALC=29.385097860382,
           VALE_REFE=29.3597,
           NOM_PARA='FR',
           TABLE=force1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4,),
           )

#-----------------------------------------------------------
# TEST SUR LE CONTRACTION DU DIAMETRE
# EN COMPARANT AVEC LES RESULTATS ISSUS
# D'UN CALCUL DE COMPORTEMENT ROUSSELIER
# ----------------------------------------------------------
TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.05,
           VALE_CALC=0.60577645811890,
           VALE_REFE=0.620591,
           NOM_PARA='NDX',
           TABLE=contra1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6,),
           )

#-----------------------------------------------------------
# TEST SUR LA FORCE
# EN COMPARANT AVEC LES RESULTATS ISSUS
# D'UN CALCUL DE COMPORTEMENT ROUSSELIER
# ----------------------------------------------------------
TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.05,
           VALE_CALC=28.869329959747,
           VALE_REFE=28.6069,
           NOM_PARA='FR',
           TABLE=force1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6,),
           )

#-----------------------------------------------------------
# TEST SUR LE CONTRACTION DU DIAMETRE
# EN COMPARANT AVEC LES RESULTATS ISSUS
# D'UN CALCUL DE COMPORTEMENT ROUSSELIER
# ----------------------------------------------------------
TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.05,
           VALE_CALC=0.93163276664640,
           VALE_REFE=0.95468299999999995,
           NOM_PARA='NDX',
           TABLE=contra1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8,),
           )

#-----------------------------------------------------------
# TEST SUR LA FORCE
# EN COMPARANT AVEC LES RESULTATS ISSUS
# D'UN CALCUL DE COMPORTEMENT ROUSSELIER
# ----------------------------------------------------------
TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.05,
           VALE_CALC=23.602269800408,
           VALE_REFE=22.673100000000002,
           NOM_PARA='FR',
           TABLE=force1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8,),
           )

#-----------------------------------------------------------
# RELATION ENTRE LE CONTRACTION DU DIAMETRE
# ET LA FORCE
# ----------------------------------------------------------
frcontra=CALC_TABLE(TABLE=contra1,
                ACTION=(_F(OPERATION='COMB',
                           TABLE=force1,
                           NOM_PARA='INST'),
                        _F(OPERATION='EXTR',
                           NOM_PARA=('NDX','FR')),
                        ));



# TESTS DE QUANTITES LOCALES AUX PG
TEST_RESU(RESU=_F(INST=1.0,
                  POINT=2,
                  RESULTAT=UP,
                  NOM_CHAM='VARI_ELGA',
                  NOM_CMP='V7',
                  VALE_CALC=9.0998171083362E-03,
                  MAILLE='M114',),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  POINT=2,
                  RESULTAT=UP,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SIGN',
                  VALE_CALC=940.24205167886,
                  MAILLE='M114',),
          )

# TESTS DES EXTRAPOLATIONS NODALES
TEST_RESU(RESU=_F(INST=1.0,
                  RESULTAT=UP,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V7',
                  VALE_CALC=0.010345531595503,
                  NOEUD='NS76'
                  ),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  RESULTAT=UP,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='SIGN',
                  VALE_CALC=840.30059940748,
                  NOEUD='NS76'
                  ),
          )


IMPR_TABLE(TABLE=frcontra,
           FORMAT='TABLEAU');


FIN();
