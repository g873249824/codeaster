                              CHARGE=CH,
                          CHAM_MATER=CM,
                              OPTION='RIGI_MECA'                      )

ME=CALC_MATR_ELEM(       MODELE=MO,
                              CHARGE=CH,
                          CHAM_MATER=CM,
                              OPTION='MASS_MECA'                      )

VE=CALC_VECT_ELEM(       CHARGE=CH,
                          CHAM_MATER=CM,
                              OPTION='CHAR_MECA'                      )

NU=NUME_DDL(          MATR_RIGI=KE                               )

KAS=ASSE_MATRICE(      MATR_ELEM=KE,
                            NUME_DDL=NU                               )

MAS=ASSE_MATRICE(      MATR_ELEM=ME,
                             NUME_DDL=NU                  )

VAS=ASSE_VECTEUR(      VECT_ELEM=VE,
                            NUME_DDL=NU                               )

DYNA=DYNA_LINE_HARM(MODELE=MO,
                    CHAM_MATER=CM,
                    MATR_MASS=MAS,
                    MATR_RIGI=KAS,
                    FREQ=0.0318309886184,
                    EXCIT=_F(VECT_ASSE=VAS,
                             COEF_MULT=1,),);

DYNA=CALC_ELEM(reuse =DYNA,
               MODELE=MO,
               CHAM_MATER=CM,
               RESULTAT=DYNA,
               NUME_ORDRE=1,
               OPTION='SIGM_ELNO_DEPL',);

