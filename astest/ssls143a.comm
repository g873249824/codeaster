MA=LIRE_MAILLAGE(FORMAT="ASTER", )


BETON=DEFI_MATERIAU(ELAS=_F( E = 3.0E+10,NU = 0.2,RHO = 2500.E0,) )

GF=DEFI_GEOM_FIBRE(
           FIBRE = (_F( GROUP_FIBRE='SBET',
                       CARA = 'SURFACE',
                            #COOR_AXE_POUTRE = (0.0,0.0,),
                           COOR_AXE_POUTRE = (0.0,-0.5,),
                            VALE =( -0.1, 0.375,0.05,
                                    -0.1, 0.125,0.05,
                                    -0.1, -0.125,0.05,
                                    -0.1, -0.375,0.05,
                                    0.1, 0.375,0.05,
                                    0.1, 0.125,0.05,
                                    0.1, -0.125,0.05,
                                    0.1, -0.375,0.05,
                                    ),),
                    ),

            )

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,MATER_SECT=BETON,
                  MULTIFIBRE=(
                              _F(GROUP_FIBRE='SBET',
                                 MATER=BETON, RELATION='ELAS'),
                                ),
                                )

