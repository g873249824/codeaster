                     MULTIFIBRE=_F(GROUP_MA = 'POUTRE',PREC_AIRE=1.E-4, PREC_INERTIE=1.E-4,GROUP_FIBRE=('SBET')),
                     )
CHMAT=AFFE_MATERIAU(MAILLAGE=MA, AFFE=_F( GROUP_MA = 'POUTRE',MATER = (BETON)),
                        AFFE_COMPOR=_F(GROUP_MA = 'POUTRE',COMPOR=COMPF))



# CONDITIONS AUX LIMITES

ENCAST=AFFE_CHAR_MECA( MODELE=MODELE,
