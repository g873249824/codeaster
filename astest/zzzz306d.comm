DEBUT(CODE=_F(NOM='ZZZZ306D',NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM='MODELISA4_1',)

#----------------------------------------------------------------------------------
# L'OBJECTIF DE CE TEST EST DE VALIDER LA PROGRAMMATION DES ROUTINES
# ECLA2D.F ET ECLA3D.F POUR TOUS LES SCHEMAS DE POINTS DE GAUSS DISPONIBLES.
#
# POUR CELA, ON CREE UN CHAMP 'ELGA' ANALYTIQUE ET ON VERIFIE QU'APRES PROJECTION
# DE CE CHAMP DU MAILLAGE SUR LUI MEME, LES VALEURS RESTENT IDENTIQUES.
#
# MODELISATION D :
#   TRIA / FPG3
#   TRIA / FPG6
#   QUAD / FPG4
#   QUAD / FPG9
#
#----------------------------------------------------------------------------------

# Alarmes ignorees :
# MODELISA4_1 : on utilise partiellement LINE_QUAD car la conformite du maillage n'a pas d'importance.

MA1 = LIRE_MAILLAGE( )
MAILLAGE=CREA_MAILLAGE(MAILLAGE=MA1,  LINE_QUAD=_F(MAILLE=('T1','T2','Q2'),),);

MODELE   = AFFE_MODELE(  MAILLAGE =    MAILLAGE, INFO=2,
           AFFE=(
              _F( MAILLE=('T1','T2','Q1','Q2'), PHENOMENE = 'MECANIQUE', MODELISATION = 'D_PLAN', ),
              _F( MAILLE=('T2',), PHENOMENE = 'MECANIQUE', MODELISATION = 'AXIS_HHM', ),
          ))


SIGXX = FORMULE(VALE='2.*(X+0.2) +5.*(Y+1.2)*(Y+1.2)', NOM_PARA=('X','Y'),);
CHXN=CREA_CHAMP(TYPE_CHAM='NOEU_GEOM_R', OPERATION='EXTR',
                MAILLAGE=MAILLAGE, NOM_CHAM='GEOMETRIE',);
CHXG=CREA_CHAMP(TYPE_CHAM='ELGA_GEOM_R', OPERATION='DISC',
                MODELE=MODELE, CHAM_GD=CHXN,);

SIEFINI1=CREA_CHAMP(TYPE_CHAM='ELGA_NEUT_F', OPERATION='AFFE', MODELE=MODELE, PROL_ZERO='OUI',
                    AFFE=_F(TOUT='OUI', NOM_CMP=('X1',), VALE_F=(SIGXX,),),);

SIEFINI2=CREA_CHAMP(TYPE_CHAM='ELGA_NEUT_R', OPERATION='EVAL', CHAM_F=SIEFINI1, CHAM_PARA=CHXG,);

CH1=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R', OPERATION='ASSE', MODELE=MODELE, PROL_ZERO='OUI',
                    ASSE=_F(TOUT='OUI', CHAM_GD=SIEFINI2, NOM_CMP=('X1',), NOM_CMP_RESU=('SIXX',),),);
IMPR_RESU(RESU=_F(CHAM_GD=CH1,NOM_CMP='SIXX',),)

CH2=PROJ_CHAMP(CHAM_GD=CH1,MODELE_1=MODELE, MODELE_2=MODELE,METHODE='ECLA_PG')

IMPR_RESU(RESU=_F(CHAM_GD=CH2,NOM_CMP='SIXX',),)


# 1.1 TEST_RESU POUR TRIANGLE / FPG3 :
#---------------------------------------
TEST_RESU(CHAM_ELEM=(
       _F(  CHAM_GD = CH1, MAILLE ='T1', POINT=1,  NOM_CMP='SIXX', VALE= 1.84055555555556E+01 , REFERENCE='NON_REGRESSION', PRECISION = 1.E-9),
       _F(  CHAM_GD = CH1, MAILLE ='T1', POINT=2,  NOM_CMP='SIXX', VALE= 1.94055555555556E+01 , REFERENCE='NON_REGRESSION', PRECISION = 1.E-9),
       _F(  CHAM_GD = CH1, MAILLE ='T1', POINT=3,  NOM_CMP='SIXX', VALE= 2.74888888888889E+01 , REFERENCE='NON_REGRESSION', PRECISION = 1.E-9),
 ))
TEST_RESU(CHAM_ELEM=(
       _F(  CHAM_GD = CH2, MAILLE ='T1', POINT=1,  NOM_CMP='SIXX', VALE= 1.84055555555556E+01 , REFERENCE='AUTRE_ASTER', PRECISION = 1.E-9),
       _F(  CHAM_GD = CH2, MAILLE ='T1', POINT=2,  NOM_CMP='SIXX', VALE= 1.94055555555556E+01 , REFERENCE='AUTRE_ASTER', PRECISION = 1.E-9),
       _F(  CHAM_GD = CH2, MAILLE ='T1', POINT=3,  NOM_CMP='SIXX', VALE= 2.74888888888889E+01 , REFERENCE='AUTRE_ASTER', PRECISION = 1.E-9),
 ))

