#
# AFFECTATIONS: MATERIAU  MODELE  CHARGEMENT ----------------------
#


ACIER=DEFI_MATERIAU( THER_ORTH=_F(  LAMBDA_L = 1.E0,
                                       LAMBDA_T = 0.75E0,
                                       LAMBDA_N = 1.E0,
                                             RHO_CP = 1.0))

