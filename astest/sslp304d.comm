# MODELISATION COQUE_3D GROT_GDEP
#


DEBUT(CODE=_F(NOM='SSLP304D',NIV_PUB_WEB='INTERNET'),);

MAINI=LIRE_MAILLAGE(FORMAT='MED',);

MAINI=DEFI_GROUP(reuse =MAINI,
                 MAILLAGE=MAINI,
                 CREA_GROUP_MA=_F(NOM='TOUT',
                                  TOUT='OUI',),
                 CREA_GROUP_NO=_F(GROUP_MA='TOUT',),);

MAIL=CREA_MAILLAGE(MAILLAGE=MAINI,
                   MODI_MAILLE=(_F(TOUT='OUI',
                                   OPTION='QUAD8_9',),
                                _F(TOUT='OUI',
                                   OPTION='TRIA6_7',),),);
#
# DEFINITION DES GROUPES DE NOEUDS
#


MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='BORD_CH1',),
                               _F(GROUP_MA='BORD_CH2',),
                               _F(NOEUD=('N171','N336','N345'),
                                  NOM='RESULT',),),);
#
# DEFINITION DU MODELE
#


MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='COQUE_3D',),);
#
# DEFINITION DU MATERIAU
#


ACIER=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=4.E10,
                                 E_T=1.E10,
                                 NU_LT=0.3,
                                 G_LT=4.500000000E9,
                                 G_TN=3.500000000E9,),);
#
#


CARA=AFFE_CARA_ELEM(MODELE=MODELE,
                    COQUE=_F(GROUP_MA='TOUT',
                             EPAIS=1.0,COQUE_NCOU=1,
                             ANGL_REP=(30.0,0.0),),);
#
# ATTRIBUTION DU MATERIAU
#


CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);
#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
#


CHARGT=AFFE_CHAR_MECA(MODELE=MODELE,
                      DDL_IMPO=(_F(GROUP_NO='A',
                                   DX=0.0,
                                   DY=0.0,),
                                _F(GROUP_NO='TOUT',
                                   DZ=0.0,
                                   DRX=0.0,
                                   DRY=0.0,
                                   DRZ=0.0,),
                                _F(GROUP_NO='B',
                                   DX=0.0,),),
                      FORCE_ARETE=(_F(GROUP_MA='BORD_CH2',
                                      FX=1.E4,),
                                   _F(GROUP_MA='BORD_CH1',
                                      FX=-1.E4,),),);

linst=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);

DEF=STAT_NON_LINE(MODELE=MODELE,
                  CHAM_MATER=CHMAT,
                  CARA_ELEM=CARA,
                  EXCIT=_F(CHARGE=CHARGT,),
                  COMP_ELAS=_F(RELATION='ELAS',
                               DEFORMATION='GROT_GDEP',),
                  INCREMENT=_F(LIST_INST=linst,),);
#
# RESOLUTION
#


U=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
             OPERATION='EXTR',
             RESULTAT=DEF,
             NOM_CHAM='DEPL',
             NUME_ORDRE=1,);
#
#
# TEST_RESU DU DEPLACEMENT
