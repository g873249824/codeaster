  TEST_TABLE(TABLE=DEPZON[i],
             NOM_PARA='DZ',
             TYPE_TEST='MIN',
             VALE_CALC=VALE_CALC[(i-1)*6+4], VALE_REFE=0,
             CRITERE='ABSOLU',
             PRECISION=preci, 
