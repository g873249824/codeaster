CH1=AFFE_CHAR_MECA(MODELE=MODELEK,PRES_REP=_F(GROUP_MA=('SURFGAU','SURFDRO'),PRES=10000.));


def pr(z) :
   if z<1.5  :  return -10000.
   if z>1.5  :  return  10000.
   if z==1.5  :  return  0.
FCTPRES=FORMULE(NOM_PARA=('X','Y','Z'),VALE='pr(Z)');
# pour tester l'option CHAR_MECA_PRES_F
CH2=AFFE_CHAR_MECA_F(MODELE=MODELEK,PRES_REP=_F(GROUP_MA=('SURFGAU','SURFDRO'),PRES=FCTPRES));

# pour tester l'option CHAR_MECA_FR2D3D
CH3=AFFE_CHAR_MECA(MODELE=MODELEK,
                   FORCE_FACE=(_F(GROUP_MA='SURFGAU',FY=10000.),
                               _F(GROUP_MA='SURFDRO',FY=-10000.),
                             ));

# pour tester l'option CHAR_MECA_FF2D3D
def prinv(z) :
   if z<1.5  :  return  10000.
   if z>1.5  :  return -10000.
   if z==1.5  :  return  0.
