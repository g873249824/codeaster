                                    _F(GROUP_NO='C',DX=0.0,DY=0.0),
                                    _F(GROUP_NO='D',DX=0.0,DY=0.0,DZ=0.0),
                                    _F(GROUP_NO='E',       DY=0.0),
                                    _F(GROUP_NO='F',DX=0.0,DY=0.0),
                                    ),
                          INFO=1);

