# CETTE MODELISATION TESTE L'APPLICATION D'UNE PRESSION (CONSTANTE ET FONCTION)

#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------

# precision
n=1000.

# dimensions de la structure
lx=1.
ly=2.
lz=3.

MA_INIT=LIRE_MAILLAGE(FORMAT='MED',INFO=1);

MAILLAG1=CREA_MAILLAGE(MAILLAGE=MA_INIT,LINE_QUAD=_F(TOUT='OUI'))

MAILLAG1=MODI_MAILLAGE(reuse =MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFGAU','SURFDRO')),
                       );

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(_F(GROUP_MA=('VOL','SURFSUP','SURFINF')),
                                   _F(NOM='A',OPTION='ENV_SPHERE',POINT=(lx  ,ly/2. ,0. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='B',OPTION='ENV_SPHERE',POINT=(0.  ,ly/2. ,0. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='C',OPTION='ENV_SPHERE',POINT=(lx  ,ly/2. ,3./5. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='D',OPTION='ENV_SPHERE',POINT=(lx  ,ly/2. ,lz ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='E',OPTION='ENV_SPHERE',POINT=(0.  ,ly/2. ,lz ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='F',OPTION='ENV_SPHERE',POINT=(lx  ,ly/2. ,lz-3./5. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='SURFGAU',OPTION='PLAN',POINT=(0.,0.,0.),VECT_NORMALE=(0.,1.,0.),PRECISION=1/(4.*n)),
                                   _F(NOM='SURFDRO',OPTION='PLAN',POINT=(0.,ly,0.),VECT_NORMALE=(0.,1.,0.),PRECISION=1/(4.*n)),
                                  ),
                    INFO=2);

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=_F(GROUP_MA=('VOL','SURFGAU','SURFDRO'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D'),
                     );

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-1.5');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN),
                    GROUP_MA_ENRI='VOL');



MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,FISSURE=FISS,INFO=1);


#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=1.E10
nu=0.
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('VOL'),
                                MATER=ACIER,
                                ),
                             );

