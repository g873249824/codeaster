DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

MA=LIRE_MAILLAGE(FORMAT='MED');

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(GROUP_MA=('CAB5',
                                          'CAB1',
                                          'CAB4',
                                          'CAB3',
                                          'CAB2',
                                          'PP',
                                          'SU3',
                                          ),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(GROUP_MA='VOLTOT',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='3D',),
                     _F(GROUP_MA=('CAB1',
                                   'CAB2',
                                   'CAB3',
                                   'CAB4',
                                   'CAB5',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='BARRE',),),);

CE=AFFE_CARA_ELEM(MODELE=MO,
                  BARRE=_F(GROUP_MA=('CAB5',
                                      'CAB4',
                                      'CAB3',
                                      'CAB2',
                                      'CAB1',),
                           SECTION='CERCLE',
                           CARA='R',
                           VALE=2.8209E-2,),);

MBETON=DEFI_MATERIAU(ELAS=_F(E=4.E10,
                             NU=0.2,
                             RHO=2500.,),
                     BPEL_BETON=_F(PERT_FLUA=0,),);

MCABLE=DEFI_MATERIAU(ELAS=_F(E=1.93E11,
                             NU=0.3,
                             RHO=7850.,),
                     BPEL_ACIER=_F(F_PRG=1.94E11,
                                   FROT_COURB=0.0,
                                   FROT_LINE=1.5E-3,),);

CMAT=AFFE_MATERIAU(MAILLAGE=MA,
                   AFFE=(_F(GROUP_MA='VOLTOT',
                            MATER=MBETON,),
                         _F(GROUP_MA=('CAB1',
                                       'CAB2',
                                       'CAB3',
                                       'CAB4',
                                       'CAB5',
                                       ),
                            MATER=MCABLE,),),);

CLIM=AFFE_CHAR_MECA(MODELE=MO,
                    PESANTEUR=_F(GRAVITE=9.81,
                                 DIRECTION=(0.,0.,-1.,),),
                    DDL_IMPO=(_F(GROUP_NO='PX',
                                 DY=0.,),
                              _F(GROUP_NO='PY',
                                 DX=0.,),
                              _F(GROUP_NO='PP',
                                 DX=0.,
                                 DY=0.,),
                              _F(GROUP_NO='SU3',
                                 DZ=0.,),),);

CAB_BP12=DEFI_CABLE_BP(MODELE=MO,
                      CHAM_MATER=CMAT,
                      CARA_ELEM=CE,
                      GROUP_MA_BETON='VOLTOT',
                      DEFI_CABLE=(_F(GROUP_MA='CAB1',
                                     GROUP_NO_ANCRAGE= ('PC1D','PC1F'),),
                                  _F(GROUP_MA='CAB2',
                                     GROUP_NO_ANCRAGE=('PC2D','PC2F'),),),
                      TYPE_ANCRAGE=('ACTIF','PASSIF',),
                      TENSION_INIT=3.75E6,
                      RECUL_ANCRAGE=0.001,);
CAB_BP34=DEFI_CABLE_BP(MODELE=MO,
                      CHAM_MATER=CMAT,
                      CARA_ELEM=CE,
                      GROUP_MA_BETON='VOLTOT',
                      DEFI_CABLE=(_F(GROUP_MA='CAB3',
                                     GROUP_NO_ANCRAGE=('PC3D','PC3F'),),
                                  _F(GROUP_MA='CAB4',
                                     GROUP_NO_ANCRAGE=('PC4D','PC4F'),),),
                      TYPE_ANCRAGE=('ACTIF','PASSIF',),
                      TENSION_INIT=3.75E6,
                      RECUL_ANCRAGE=0.001,);
CAB_BP5=DEFI_CABLE_BP(MODELE=MO,
                      CHAM_MATER=CMAT,
                      CARA_ELEM=CE,
                      GROUP_MA_BETON='VOLTOT',
                      DEFI_CABLE=(_F(GROUP_MA='CAB5',
                                     GROUP_NO_ANCRAGE=('PC5D','PC5F'),),),
                      TYPE_ANCRAGE=('ACTIF','PASSIF',),
                      TENSION_INIT=3.75E6,
                      RECUL_ANCRAGE=0.001,);

CMCAB12=AFFE_CHAR_MECA(MODELE=MO,
                      RELA_CINE_BP=_F(CABLE_BP=CAB_BP12,
                                      SIGM_BPEL='NON',
                                      RELA_CINE='OUI',),);

CMCAB34=AFFE_CHAR_MECA(MODELE=MO,
                      RELA_CINE_BP=_F(CABLE_BP=CAB_BP34,
                                      SIGM_BPEL='NON',
                                      RELA_CINE='OUI',),);

CMCAB5=AFFE_CHAR_MECA(MODELE=MO,
                      RELA_CINE_BP=_F(CABLE_BP=CAB_BP5,
                                      SIGM_BPEL='NON',
                                      RELA_CINE='OUI',),);

LIST=DEFI_LIST_REEL(DEBUT=0.,
                    INTERVALLE=(_F(JUSQU_A=150.,
                                   NOMBRE=1,),
                                _F(JUSQU_A=300.,
                                   NOMBRE=1,),
                                _F(JUSQU_A=450.,
                                   NOMBRE=1,),
                                _F(JUSQU_A=600.,
                                   NOMBRE=1,),),
                    INFO=1,);

# APPLICATION DE LA PESANTEUR
#--------------------------------
RES0=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CMAT,
                   CARA_ELEM=CE,
                   EXCIT=(_F(CHARGE=CLIM,),
                          _F(CHARGE=CMCAB12,),
                          _F(CHARGE=CMCAB34,),
                          _F(CHARGE=CMCAB5,),),
                   COMPORTEMENT=(_F(RELATION='ELAS',
                                    GROUP_MA='VOLTOT'),
                              _F(RELATION='SANS',
                                 GROUP_MA='CABLE',),),
                   INCREMENT=_F(LIST_INST=LIST,
                                INST_FIN=150.0,),
                   NEWTON=_F(),);
# MISE EN TENSION DES CABLES 1 ET 2
#----------------------------------
RES0=CALC_PRECONT(reuse = RES0,
                  MODELE=MO,
                  CHAM_MATER=CMAT,
                  CARA_ELEM=CE,
                  CABLE_BP=(CAB_BP12),
                  CABLE_BP_INACTIF=(CAB_BP5,CAB_BP34),
                  INCREMENT=_F(LIST_INST=LIST,
                               INST_FIN=300.0,),
                  ETAT_INIT=_F(EVOL_NOLI=RES0,),
                  EXCIT=_F(CHARGE=CLIM,),
                  COMPORTEMENT=(_F(RELATION='ELAS',
                                GROUP_MA='VOLTOT',),
                             _F(RELATION='ELAS',
                                GROUP_MA='CABLE',),),);
# MISE EN TENSION DES CABLES 3 ET 4
#----------------------------------
RES0=CALC_PRECONT(reuse =RES0,
                  MODELE=MO,
                  CHAM_MATER=CMAT,
                  CARA_ELEM=CE,
                  CABLE_BP=(CAB_BP34,),
                  CABLE_BP_INACTIF=CAB_BP5,
                  INCREMENT=_F(LIST_INST=LIST,
                               INST_FIN=450.0,),
                  ETAT_INIT=_F(EVOL_NOLI=RES0,),
                  EXCIT=(_F(CHARGE=CMCAB12,),
                         _F(CHARGE=CLIM,),),
                  COMPORTEMENT=(_F(RELATION='ELAS',
                                GROUP_MA='VOLTOT',),
                             _F(RELATION='ELAS',
                                GROUP_MA='CABLE',),),);
# MISE EN TENSION DU CABLES 5
#----------------------------------
RES0=CALC_PRECONT(reuse =RES0,
                  MODELE=MO,
                  CHAM_MATER=CMAT,
                  CARA_ELEM=CE,
                  CABLE_BP=CAB_BP5,
                  INCREMENT=_F(LIST_INST=LIST,
                               INST_FIN=600.,),
                  ETAT_INIT=_F(EVOL_NOLI=RES0,),
                  EXCIT=(_F(CHARGE=CLIM,),
                         _F(CHARGE=CMCAB12,),
                         _F(CHARGE=CMCAB34,),),
                  COMPORTEMENT=(_F(RELATION='ELAS',
                                GROUP_MA='VOLTOT',),
                             _F(RELATION='ELAS',
                                GROUP_MA='CABLE ',),),);

# ---------------------
# POST-TRAITEMENT
# ---------------------
RES0=CALC_CHAMP(reuse =RES0,
                RESULTAT=RES0,
                CONTRAINTE= 'SIEF_ELNO',);
#
#IMPR_RESU(FORMAT='MED',
#          RESU=_F(RESULTAT=RES0,),);
#
# RECUPERATION DE LA TENSION DANS LES CABLES SELON BPEL
# ----------------------------------------------------
TAB_1_I=RECU_TABLE(CO=CAB_BP12,
                 NOM_TABLE='CABLE_BP',);
IMPR_TABLE(TABLE=TAB_1_I,);
#
# RECUPERATION DE LA TENSION APRES MISE EN CHARGE
#------------------------------------------------
TAB_1_F=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                            INTITULE='cable1',
                            RESULTAT=RES0,
                            NOM_CHAM='SIEF_ELNO',
                            GROUP_NO='CAB1',
                            NOM_CMP='N',),);
IMPR_TABLE(TABLE=TAB_1_F,);
#
#TRACE DES DIFFERENTS PROFILS DE CONTRAINTE
#------------------------------------------------
N1_I=RECU_FONCTION(TABLE=TAB_1_I,
                   PARA_X='ABSC_CURV',
                   PARA_Y='TENSION',
                   FILTRE=_F(NOM_PARA='NOM_CABLE',
                             VALE_K='CAB1',),);
N1_0=RECU_FONCTION(TABLE=TAB_1_F,
                   PARA_X='ABSC_CURV',
                   PARA_Y='N',
                   FILTRE=_F(NOM_PARA='INST',
                             VALE=0,),);
N1_150=RECU_FONCTION(TABLE=TAB_1_F,
                   PARA_X='ABSC_CURV',
                   PARA_Y='N',
                   FILTRE=_F(NOM_PARA='INST',
                             VALE=150,),);
N1_300=RECU_FONCTION(TABLE=TAB_1_F,
                   PARA_X='ABSC_CURV',
                   PARA_Y='N',
                   FILTRE=_F(NOM_PARA='INST',
                             VALE=300,),);
N1_450=RECU_FONCTION(TABLE=TAB_1_F,
                   PARA_X='ABSC_CURV',
                   PARA_Y='N',
                   FILTRE=_F(NOM_PARA='INST',
                             VALE=450,),);
N1_600=RECU_FONCTION(TABLE=TAB_1_F,
                     PARA_X='ABSC_CURV',
                     PARA_Y='N',
                     FILTRE=_F(NOM_PARA='INST',
                               VALE=600,),);
IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=38,
              COURBE=(_F(FONCTION=N1_I),
                      _F(FONCTION=N1_0,),
                      _F(FONCTION=N1_150,),
                      _F(FONCTION=N1_300,),
                      _F(FONCTION=N1_450,),
                      _F(FONCTION=N1_600,),),);

