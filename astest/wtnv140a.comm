import numpy as np
import numpy.linalg as ln

DEBUT(CODE=_F(NOM='WTNV140A',NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI',),);


## ANGLE NAUTIQUE UTILISES PAR ASTER (A. Nautique classique)
# 
BETA = -60.
ALPHA = 30.


UN=DEFI_CONSTANTE(VALE=1.)
ZERO=DEFI_CONSTANTE(VALE=0.)
#
PLIMP = 0.E6

#CONTRAINTES TOTALES ASSOCIÃ‰ES AU REPERE GLOBAL DES CONTRAINTES IN SITU

SIGXX=-25.E+6
SIGYY=-22.E+6
SIGZZ=-29.E+6
SIGXY=0.
SIGXZ=0.
SIGYZ=0.



# COEFFICIENTS DE BIOT REPERE LN
CBIOTL = 0.3
CBIOTN = 0.6





#CONTRAINTE DE PRESSION DE FLUIDE DANS LE REPERE GLOBAL (ICI NULLE CAR LA PRESSION EST NULLE PARTOUT
SIPXX0=0.
SIPYY0=0.
SIPZZ0=0.
SIPXY0=0.
SIPXZ0=0.
SIPYZ0=0.




#CONTRAINTES  EFFECTIVES
SIEFX=SIGXX-SIPXX0
SIEFY=SIGYY-SIPYY0
SIEFZ=SIGZZ-SIPZZ0
SIEFXY=SIGXY-SIPXY0
SIEFXZ=SIGXZ-SIPXZ0
SIEFYZ=SIGYZ-SIPYZ0


# MODULE DE YOUNG
YOUNG=9.E9

#COEFFICIENT DE POISSON
POISSON=0.24

## PRESREP CONFINEMENT INITIAL

PRESREPY = -(SIGYY)
PRESREPX = -(SIGXX)
PRESREPZ = -(SIGZZ)
#***********************************************************************
#    MAILLAGE 
#***********************************************************************
MAILIN=LIRE_MAILLAGE(FORMAT='MED',)

MAIL = CREA_MAILLAGE(MAILLAGE  = MAILIN,
                     LINE_QUAD = _F(TOUT = 'OUI'),
                     )
MAIL=DEFI_GROUP(reuse=MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_MA=(_F(NOM='ROCHE',GROUP_MA='GM33'),
                               _F(NOM='HAUT',GROUP_MA='GM27'),
                               _F(NOM='BAS',GROUP_MA='GM29'),
                               _F(NOM='GAUCHE',GROUP_MA='GM28'),
                               _F(NOM='DERECHA',GROUP_MA='GM30'),
                               _F(NOM='DERRIERE',GROUP_MA='GM31'),
                               _F(NOM='DEVANT',GROUP_MA='GM32'),
                                                            ),);
MAIL=DEFI_GROUP(reuse=MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='ROCHE',),
                               _F(GROUP_MA='DERECHA',),
                               _F(GROUP_MA='DEVANT',),
                               _F(GROUP_MA='HAUT',),
                               _F(GROUP_MA='GAUCHE',),
                               _F(GROUP_MA='BAS'),
                               _F(GROUP_MA='DERRIERE'),
                               _F(NOM='BIDON1',
                                  UNION=('DERECHA','HAUT',),),
                               _F(NOM='DROITE',
                                  DIFFE=('BIDON1','HAUT',),),
                               ),
                 );
MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_3D=_F(GROUP_MA=('HAUT',
                                             'BAS',
                                             'GAUCHE',
                                             'DERECHA',
                                             'DERRIERE',
                                             'DEVANT',),),
                  );
MAIL=DEFI_GROUP( reuse=MAIL,
                 MAILLAGE=MAIL,
                 CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))


#***********************************************************************
#    CARACTERISTIQUES DU MATERIAU
#***********************************************************************

########
VISCOLIQ=DEFI_CONSTANTE(VALE=1.E-3)

DVISCOL=DEFI_CONSTANTE(VALE=0.)

# PERMEABILITE INTRINSEQUE 
KINT=DEFI_CONSTANTE(VALE=5.E-20)


# PRESSION IMPOSEE INITIALEMENT ET IMPOSEE A L INI
PLIMP = 0.E6


# COEFFICIENTS DE BIOT REPERE LN
CBIOTL = 0.6
CBIOTN = 0.3 

MAT=DEFI_MATERIAU(
       ELAS_ISTR=_F(
           E_L  = YOUNG,
           E_N  = 2*YOUNG,
           RHO = 2700,
           NU_LT = POISSON,
           NU_LN = 2*POISSON,
           G_LN = 8.88E+9,
         ),
       COMP_THM = 'LIQU_SATU',
                              
 THM_LIQU=_F(  
     RHO = 1000.,  
     UN_SUR_K = 0.,  
     VISC = VISCOLIQ,   
     D_VISC_TEMP = DVISCOL,),
     
 THM_DIFFU=_F(
     RHO=2410.,
     BIOT_L=CBIOTL,
     BIOT_N=CBIOTN,
     PESA_X=0.0,
     PESA_Y=0.0,
     PESA_Z=0.0,
     PERMIN_L=KINT,
     PERMIN_N=KINT,
        ),

 THM_INIT=_F(PRE1 = 0.,  
             PORO = 0.14,),
     ); 
#***********************************************************************
#    LISTE DES INSTANTS DE CALCUL
#***********************************************************************

TEMPS1=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=(_F(JUSQU_A = 1.,NOMBRE = 10,),),
                     );                      

##################################################################################################
#     
#                         MODELE MECA
#
##################################################################################################

MODELEM=AFFE_MODELE(MAILLAGE=MAIL,
            AFFE=_F(TOUT='OUI',
            PHENOMENE = 'MECANIQUE',
            MODELISATION ='3D_SI',),
        );


ORIREP=AFFE_CARA_ELEM(MODELE=MODELEM,
       MASSIF=( _F(GROUP_MA=('TOUT',),
                   ANGL_REP=(ALPHA,BETA),
                        ),),
    INFO=2,);

CHMAT1=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=_F(TOUT='OUI',  
                     MATER = MAT,),
                    );

#***********************************************************************
#        CONDITIONS INITIALES POUR LES CHAMPS DE CONTRAINTES ET DE 
#                  DEPLACEMENTS POUR LA PHASE 1 
#***********************************************************************

SIGIM=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                  OPERATION='AFFE',
                  MODELE=MODELEM,
    AFFE=(_F(TOUT='OUI',
      NOM_CMP=('SIXX','SIYY','SIZZ',
            'SIXY','SIYZ','SIXZ',
            ),
      VALE=(SIEFX,SIEFY,SIEFZ, 
         SIEFXY,SIEFXZ,SIEFYZ, 
         ),),
                          ), 
                   );

DEPIM=CREA_CHAMP(MAILLAGE=MAIL, 
     OPERATION='AFFE', 
     TYPE_CHAM='NOEU_DEPL_R',
     AFFE=(_F(TOUT='OUI',
       NOM_CMP = 'DX',
       VALE=0.,),
             _F(TOUT='OUI',
       NOM_CMP = 'DY',
       VALE=0.,),
             _F(TOUT='OUI',
       NOM_CMP = 'DZ',
       VALE=0.,),
                ),
                  );

#***********************************************************************
#             CHARGEMENT MECANIQUE (CONDITIONS AUX LIMITES)
#***********************************************************************



CH_PREY=AFFE_CHAR_CINE(MODELE=MODELEM,
                      MECA_IMPO=(_F(GROUP_MA='HAUT', DY=1.,),),
                      );
COEF=DEFI_FONCTION(      NOM_PARA='INST',
                          PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                         VALE=( 0.,  0.0,
                                1.0,  -0.01, )
                       )
CH_PREX=AFFE_CHAR_MECA(MODELE=MODELEM,
                      PRES_REP=(_F(GROUP_MA='DERECHA', PRES=PRESREPX,),),
                      );

CH_PREZ=AFFE_CHAR_MECA(MODELE=MODELEM,
                      PRES_REP=(_F(GROUP_MA='DERRIERE', PRES=PRESREPZ,),),
                      );

CH_SYM2=AFFE_CHAR_CINE(MODELE=MODELEM,
                      MECA_IMPO=(
                                 _F(GROUP_MA='DEVANT', DZ=0.0,   ),
                                 _F(GROUP_MA='GAUCHE', DX=0.0,   ),
                                 _F(GROUP_MA='BAS',    DY=0.0,   ),
                                ),
                      );

            
#*****************************************************************
#              CALCUL MECANIQUE
#*****************************************************************

UM=STAT_NON_LINE(MODELE=MODELEM,
   CHAM_MATER=CHMAT1,
         CARA_ELEM=ORIREP,
   EXCIT=(
                        _F(CHARGE = CH_SYM2,),
                        _F(CHARGE = CH_PREY,FONC_MULT =COEF ),
                        _F(CHARGE = CH_PREX,),
                        _F(CHARGE = CH_PREZ,),
                       ),
   ETAT_INIT=_F(SIGM=SIGIM,
                   DEPL=DEPIM,),
   COMP_INCR=_F(RELATION = 'ELAS',          
         ),
   NEWTON=_F(MATRICE ='TANGENTE',
                           REAC_ITER=1,),
   CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-12,
           ITER_GLOB_MAXI = 50,),
   INCREMENT=_F(LIST_INST = TEMPS1,
               INST_FIN=1.,
                 ),
                );

TEST_RESU(RESU=(_F(NUME_ORDRE=10,
                   RESULTAT=UM,
                   NOM_CHAM='DEPL',
                   NOEUD='N7',
                   NOM_CMP='DX',
                   VALE_CALC=5.98402E-03,
                   ),
                _F(NUME_ORDRE=10,
                   RESULTAT=UM,
                   NOM_CHAM='DEPL',
                   NOEUD='N7',
                   NOM_CMP='DZ',
                   VALE_CALC=3.5698E-03,
                   ),
                _F(NUME_ORDRE=10,
                   RESULTAT=UM,
                   NOM_CHAM='DEPL',
                   NOEUD='N216',
                   NOM_CMP='DY',
                   VALE_CALC=-1.96585E-03,
                   ),
                ),
          )

##################################################################################################
#     
#                         MEME CALCUL EN THM
#
##################################################################################################

PHASE1=AFFE_MODELE(MAILLAGE=MAIL,
            AFFE=_F(TOUT='OUI',
      PHENOMENE = 'MECANIQUE',
             MODELISATION ='3D_HMS',),
        );
#
# CI
#
SIGINI=CREA_CHAMP(TYPE_CHAM='CART_SIEF_R',
                  OPERATION='AFFE',
                  MODELE=PHASE1,
    AFFE=(_F(TOUT='OUI',
      NOM_CMP=('SIXX','SIYY','SIZZ',
         'SIXY','SIYZ','SIXZ',
         'SIPXX','SIPYY','SIPZZ',
         'SIPXY','SIPXZ','SIPYZ',
         'M11','FH11X','FH11Y','FH11Z',),
      VALE=(SIEFX,SIEFY,SIEFZ, 
            SIEFXY,SIEFXZ,SIEFYZ, 
            SIPXX0,SIPYY0,SIPZZ0,
            SIPXY0,SIPXZ0,SIPYZ0,
            0.,0.,0.,0.,),),
                          ), 
                   );

DEPINIT=CREA_CHAMP(MAILLAGE=MAIL, 
     OPERATION='AFFE', 
     TYPE_CHAM='NOEU_DEPL_R',
     AFFE=(_F(TOUT='OUI',
       NOM_CMP = 'DX',
       VALE=0.,),
                         _F(TOUT='OUI',
       NOM_CMP = 'DY',
       VALE=0.,),
                         _F(TOUT='OUI',
       NOM_CMP = 'DZ',
       VALE=0.,),
                         _F(TOUT='OUI',
       NOM_CMP = 'PRE1',
       VALE= PLIMP,),),
                  );

# BLOCAGE DES DEPLACEMENT AUX LIMITES DU MODELE
CH_INF1=AFFE_CHAR_CINE(MODELE=PHASE1,
                      MECA_IMPO=(_F(GROUP_NO='DROITE', PRE1=PLIMP,),
                                 _F(GROUP_NO='DERRIERE', PRE1=PLIMP,),
                                 _F(GROUP_NO='HAUT', PRE1=PLIMP,),),);

CH_P=AFFE_CHAR_CINE(MODELE=PHASE1,
                      MECA_IMPO=(_F(TOUT='OUI', PRE1=PLIMP,),
                                 ),);
# CALCUL HM

U0=STAT_NON_LINE(MODELE=PHASE1,
   CHAM_MATER=CHMAT1,
                 CARA_ELEM=ORIREP,
   EXCIT=(
                        _F(CHARGE = CH_SYM2,),
                        _F(CHARGE = CH_PREY,FONC_MULT =COEF ),
                        _F(CHARGE = CH_PREX,),
                        _F(CHARGE = CH_PREZ,),
                        _F(CHARGE = CH_P,),
                        ),
   ETAT_INIT=_F(
                              SIGM=SIGINI,
         DEPL=DEPINIT,),
   COMP_INCR=_F(RELATION = 'KIT_HM',          
         RELATION_KIT = ('ELAS',
                  'LIQU_SATU',
                  'HYDR_UTIL',),),
   NEWTON=_F(
   MATRICE ='TANGENTE',
                           REAC_ITER=1,),
   CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-9,
           ITER_GLOB_MAXI = 50,),
                 SOLVEUR=_F(
                             METHODE='MUMPS',
                             ),
   INCREMENT=_F(LIST_INST = TEMPS1,
         INST_FIN=1.,
           ),
                );

##

TEST_RESU(RESU=(_F(NUME_ORDRE=10,
                   RESULTAT=U0,
                   NOM_CHAM='DEPL',
                   NOEUD='N7',
                   NOM_CMP='DX',
                   VALE_CALC=5.98402E-03,
                   ),
                _F(NUME_ORDRE=10,
                   RESULTAT=U0,
                   NOM_CHAM='DEPL',
                   NOEUD='N7',
                   NOM_CMP='DZ',
                   VALE_CALC=3.5698E-03,
                   ),
                _F(NUME_ORDRE=10,
                   RESULTAT=U0,
                   NOM_CHAM='DEPL',
                   NOEUD='N216',
                   NOM_CMP='DY',
                   VALE_CALC=-1.96585E-03,
                   ),
                ),
          )

FIN();
