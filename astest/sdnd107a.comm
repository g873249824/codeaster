# Solution Théorique
def Deplacement(t):
    wt  = 14593.0/4792.0
    As  = 5516.0/214807.0;  Lsc = 1573.0/2072.0
    Ac  = 3137.0/29305.0
    Ae  = 413.0/58610.0;    Le  = 38132.0/1685.0
    tmp = As*exp(-Lsc*t)*sin(wt*t)-Ac*exp(-Lsc*t)*cos(wt*t)+Ae*exp(-Le*t)+0.10
    return tmp

def Force(t):
    wt  = 14593.0/4792.0
    Bs  = 5625.0/7831.0;    Lsc = 1573.0/2072.0
    Bc  = 9170.0/11289.0
    Be  = 12692.0/3517.0;   Le  = 38132.0/1685.0
    tmp = -Bs*exp(-Lsc*t)*sin(wt*t)+Bc*exp(-Lsc*t)*cos(wt*t)+Be*exp(-Le*t)
    return tmp

# Différences entre les valeurs théorique et calculées
