# TEST V6.03.130
# CHARGEMENT EN PRESSION
# MAILLAGE : TETRA4
# COMPORTEMENT ELASTIQUE

DEBUT(CODE=_F(
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',);

# DEFINITION DU MATERIAU

ACIER=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                            NU=0.3,),);

# DEFINITION DES INSTANTS POUR STAT_NON_LINE

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

F_INST=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                           1.0,1.0,
                           ),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=(_F(GROUP_MA='HAUT',),
                               _F(GROUP_MA='ARRIERE',),
                               _F(GROUP_MA='AVANT',),
                               _F(GROUP_MA='GAUCHE',),
                               _F(GROUP_MA='DROITE',),
                               _F(GROUP_MA='S1',),
                               _F(GROUP_MA='S3',),
                               _F(GROUP_MA='S4',),),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

# REMARQUE : ON IMPOSE EXPLICITEMENT LA PRESSION NULLE SUR LES BORDS LIBRES
#            POUR QUE LE CALCUL DE L'INDICATEUR D'ERREUR SOIT CORRECT
PRES_0 = 0.0;


CHP=AFFE_CHAR_MECA(MODELE=MO,
                   PRES_REP=(_F(GROUP_MA='HAUT',
                                PRES=1.0,),
                             _F(GROUP_MA='ARRIERE',
                                PRES=PRES_0,),
                             _F(GROUP_MA='AVANT',
                                PRES=PRES_0,),
                             _F(GROUP_MA='GAUCHE',
                                PRES=PRES_0,),
                             _F(GROUP_MA='DROITE',
                                PRES=PRES_0,),
                             _F(GROUP_MA='S1',
                                PRES=PRES_0,),
                             _F(GROUP_MA='S3',
                                PRES=PRES_0,),
                             _F(GROUP_MA='S4',
                                PRES=PRES_0,),),);

CHCL=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='PT2',
                                 DX=0.0,),
                              _F(GROUP_NO='PT3',
                                 DX=0.0,),
                              _F(GROUP_MA='L1210',
                                 DY=0.0,),
                              _F(GROUP_MA='S2',
                                 DZ=0.0,),),);

DEPLA=STAT_NON_LINE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    EXCIT=(_F(CHARGE=CHP,
                              FONC_MULT=F_INST,),
                           _F(CHARGE=CHCL,),),
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),);

DEPLA=CALC_CHAMP(reuse=DEPLA,RESULTAT=DEPLA,INST=1.,ENERGIE=('ETOT_ELEM'))


DEPLA=CALC_ERREUR(reuse =DEPLA,
                  RESULTAT=DEPLA,
                  INST=1.,
                  OPTION=('ERME_ELEM','ERME_ELNO','SING_ELEM','SING_ELNO',),
                  PREC_ERR=0.8,);

# TEST DE SING_ELEM VALEUR MAX ET MIN DES 2 COMPOSANTES

TEST_RESU(RESU=(_F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELEM',
                   NOM_CMP='DEGRE',
                   VALE_CALC=1.0,
                   VALE_REFE=1.0,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELEM',
                   NOM_CMP='DEGRE',
                   VALE_CALC=0.56143710907161,
                   VALE_REFE=0.5,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   PRECISION=0.13,
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELEM',
                   NOM_CMP='RAPPORT',
                   VALE_CALC=2.771692437,
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELEM',
                   NOM_CMP='RAPPORT',
                   VALE_CALC=0.155414815,
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELEM',
                   NOM_CMP='TAILLE',
                   VALE_CALC=9.099605692,
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELEM',
                   NOM_CMP='TAILLE',
                   VALE_CALC=0.438363812,
                   CRITERE='RELATIF',
                   ),
                ),
          )

# TEST DE SING_ELNO

TEST_RESU(RESU=(_F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELNO',
                   NOM_CMP='DEGRE',
                   VALE_CALC=1.0,
                   VALE_REFE=1.0,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELNO',
                   NOM_CMP='DEGRE',
                   VALE_CALC=0.56143710907161,
                   VALE_REFE=0.5,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   PRECISION=0.13,
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELNO',
                   NOM_CMP='RAPPORT',
                   VALE_CALC=2.771692437,
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELNO',
                   NOM_CMP='RAPPORT',
                   VALE_CALC=0.155414815,
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELNO',
                   NOM_CMP='RAPPORT',
                   VALE_CALC=2.771692437,
                   CRITERE='RELATIF',
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=DEPLA,
                   NOM_CHAM='SING_ELNO',
                   NOM_CMP='RAPPORT',
                   VALE_CALC=0.155414815,
                   CRITERE='RELATIF',
                   ),
                ),
          )

FIN();
