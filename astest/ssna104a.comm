DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA = LIRE_MAILLAGE(FORMAT='MED',);

MA = DEFI_GROUP(reuse =MA,
                MAILLAGE=MA,
                CREA_GROUP_NO =(
                       _F(GROUP_MA = 'DAB',),
                       _F(GROUP_MA = 'DCD',),
                        )
                )

MO = AFFE_MODELE( MAILLAGE =MA,
                  AFFE=_F(
                       TOUT ='OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS',)
                 )
UN=DEFI_CONSTANTE( VALE=1. )
ZERO=DEFI_CONSTANTE( VALE=0. )

MAT = DEFI_MATERIAU(
            ELAS=_F( E = 1., NU = 0.3, ALPHA = 0.),
            LEMAITRE_FO=_F( N = UN,UN_SUR_K = UN,UN_SUR_M = ZERO)
           )
CHMAT = AFFE_MATERIAU (MAILLAGE = MA,
                       AFFE =_F(TOUT='OUI',
                                MATER = MAT,
                                )
                       )

# FONCTION DE HEAVISIDE*P0
# L INSTANT DE REFERENCE EST T=0

F_PRES = DEFI_FONCTION (NOM_PARA = 'INST',
                        PROL_DROITE = 'CONSTANT',
                        PROL_GAUCHE = 'CONSTANT',
                        VALE =( 0.,  1.E-3,
                                0.9, 1.E-3,)
                        )

BLOC = AFFE_CHAR_MECA (MODELE = MO,
                       DDL_IMPO=_F(GROUP_NO =('DAB','DCD'),
                                   DY=0.)
                      )
PRES = AFFE_CHAR_MECA (MODELE = MO,
                          PRES_REP=_F (GROUP_MA = 'DDA',
                                    PRES = 1.E-3)
                          )

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
                        INTERVALLE=(
                        _F( PAS = 1.E-6,       JUSQU_A = 1.E-6),
                        _F( PAS = 0.999E-3,    JUSQU_A = 0.001),
                        _F( PAS = 1.E-3,       JUSQU_A = 0.01),
                        _F( PAS = 2.E-3,       JUSQU_A = 0.03),
                        _F( PAS = 5.E-3,       JUSQU_A = 0.1),
                        _F( PAS = 1.E-2,       JUSQU_A = 0.9),
)
                      )

RESU = STAT_NON_LINE (MODELE = MO,
                      CHAM_MATER = CHMAT,
                      EXCIT =(
                         _F( CHARGE = BLOC),
                         _F( CHARGE = PRES),),
                      COMPORTEMENT =_F(RELATION ='LEMAITRE',PARM_THETA = 0.5,
                                    ITER_INTE_MAXI = 1000),
                      INCREMENT =_F(LIST_INST =L_INST),
                      NEWTON    =_F(
                                MATRICE ='TANGENTE',
                                REAC_ITER= 1),
                      CONVERGENCE=_F(
                                RESI_GLOB_RELA = 1.E-6,
                                ITER_GLOB_MAXI = 25)
                        )

RESU = CALC_CHAMP( reuse= RESU,
                  RESULTAT = RESU,
                   CONTRAINTE=('SIGM_ELNO'),)
RESU = CALC_CHAMP ( reuse= RESU,
                  RESULTAT = RESU,
                  CONTRAINTE='SIGM_NOEU',)


TEST_RESU(RESU=(_F(NUME_ORDRE=115,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.14493496E-03,
                   VALE_REFE=2.1449799999999999E-3,),
                _F(NUME_ORDRE=115,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=-4.81687140E-06,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',),
                _F(NUME_ORDRE=115,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   VALE_CALC= 2.75974611E-04,
                   VALE_REFE=2.7912E-4,
                   PRECISION=0.014999999999999999,),
                _F(NUME_ORDRE=115,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIZZ',
                   VALE_CALC= 6.63504109E-04,
                   VALE_REFE=6.6660000000000005E-4,
                   PRECISION=5.0000000000000001E-3,),
                ),
          )

FIN()
