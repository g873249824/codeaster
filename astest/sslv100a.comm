# CYLINDRE CREUX EN DEFORMATIONS PLANES
#
# MODELISATION : ELEMENTS MECANIQUE D_PLAN (TRIA3 QUAD4)
# MAILLAGE     : ASTER (ISSU DE GIBI)
# ----------------------------------------------------------------------
# GROUPES DE NOEUDS: A , B , C , D , E , F , BORDAB
# GROUPES DE MAILLES:  AE , FE
# ----------------------------------------------------------------------
#

DEBUT(CODE=_F(NOM='SSLV100A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE()

DEFI_GROUP(reuse=M,       MAILLAGE=M,
                  CREA_GROUP_NO=_F( NOM = 'DIRI', GROUP_MA = 'AB'))

MO=AFFE_MODELE(            MAILLAGE=M,
                                   VERIF='MAILLE',
                                  AFFE=_F(  TOUT = 'OUI',
                                         PHENOMENE = 'MECANIQUE',
                                         MODELISATION = 'D_PLAN'))

MA=DEFI_MATERIAU(            ELAS=_F(  E = 200000.,
                                         NU = 0.3,
                                         ALPHA = 0.) )

CM=AFFE_MATERIAU(        MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MA))

CH=AFFE_CHAR_MECA(         MODELE=MO,
                              DDL_IMPO=_F(  GROUP_NO = 'DIRI',  DY = 0.),
                             FACE_IMPO=_F(  GROUP_MA = 'FE',  DNOR = 0.),
                              PRES_REP=_F(  GROUP_MA = 'AE',  PRES = 60.))

RESU=MECA_STATIQUE(         MODELE=MO,    CHAM_MATER=CM,
                          EXCIT=_F(  CHARGE = CH),
                      )


CALC_ELEM( reuse= RESU ,RESULTAT= RESU ,

                      OPTION=('EPSI_ELNO_DEPL', 'SIGM_ELNO_DEPL',))

CALC_NO( reuse=RESU,                      RESULTAT=RESU,
                     OPTION=('EPSI_NOEU_DEPL', 'SIGM_NOEU_DEPL',))

