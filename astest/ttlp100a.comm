DEBUT(  CODE=_F(  NOM = 'TTLP100A',NIV_PUB_WEB='INTERNET') )

#

MAIL=LIRE_MAILLAGE( )

MAIL=DEFI_GROUP( reuse=MAIL,
                       MAILLAGE=MAIL,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'BORDG'),
                       _F(  GROUP_MA = 'BORDD'))
                    )

#

LONTOT=0.990

# SOMME DES LONG. DES 2 PLAQUES

LONMAX=1.000

# ABSCISSE MAXIMALE
#
# =====================================================================
#      DEFINITION DU MODELE, DU CHAMP DE MATERIAU ET DU CHARGEMENT
# =====================================================================
#
# MODELE
# ------
#

MODTHER=AFFE_MODELE( 
                           MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',
                                  PHENOMENE = 'THERMIQUE',
                                  MODELISATION = 'PLAN')
                        )

#
# CHAMP DE MATERIAU
# -----------------
#

CONDUC=40.

CHAMAS=7.3E-4

#

MAT=DEFI_MATERIAU( 
                              THER=_F(  LAMBDA = CONDUC,
                                     RHO_CP = CHAMAS)
                        )

CHMAT=AFFE_MATERIAU( 
                             MAILLAGE=MAIL,
                             AFFE=_F(  TOUT = 'OUI',
                                    MATER = MAT,
                                    TEMP_REF = 20.)
                          )

#
# CHARGEMENT
# ----------
#

TEMPG=100.

TEMPD=300.

COEFH=80.

#

CHATHER=AFFE_CHAR_THER( 
                              MODELE=MODTHER,TEMP_IMPO=(
                              _F(  GROUP_NO = 'BORDG',
                                          TEMP = TEMPG),
                              _F(  GROUP_NO = 'BORDD',
                                          TEMP = TEMPD)),
                                ECHANGE_PAROI=_F(  GROUP_MA_1 = 'INTERG',
                                                GROUP_MA_2 = 'INTERD',
                                                COEF_H = COEFH)
                           )

#
# =====================================================================
#                            LISTE D'INSTANTS
# =====================================================================
#

LINST=DEFI_LIST_REEL( 
                            DEBUT=0.0,
                            INTERVALLE=_F(  JUSQU_A = 5.E-2,  NOMBRE = 50)
                          )

#
# =====================================================================
#    CALCUL TRANSITOIRE LINEAIRE TENDANT VERS UN ETAT STATIONNAIRE
# =====================================================================
#

TEMPINIT=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R',
MAILLAGE=MAIL,AFFE=(
                             _F(  GROUP_NO = 'RECTG',
                                    NOM_CMP = 'TEMP',
                                    VALE = TEMPG),
                             _F(  GROUP_NO = 'RECTD',
                                    NOM_CMP = 'TEMP',
                                    VALE = TEMPD))
                          )

#

RESUTHLI=THER_LINEAIRE( 
                              MODELE=MODTHER,
                              CHAM_MATER=CHMAT,
                              EXCIT=_F(  CHARGE = CHATHER),
                              TEMP_INIT=_F(  CHAM_NO = TEMPINIT),
                              PARM_THETA=0.57,
                              INCREMENT=_F(  LIST_INST = LINST)
                           )

#
# =====================================================================
#                VERIFICATION QUE L'ON A BIEN ATTEINT
#                LA SOLUTION ANALYTIQUE STATIONNAIRE
# =====================================================================
#

TSTAT=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
                        RESULTAT=RESUTHLI,
                        NOM_CHAM='TEMP',
                        NUME_ORDRE=50
                     )

#

GRAD = FORMULE(REEL="""(REEL:BIDON) =
COEFH*(TEMPD-TEMPG)/(CONDUC+COEFH*LONTOT)""")

TINTG = FORMULE(REEL="""(REEL:X) =
TEMPG + COEFH*(TEMPD-TEMPG)/(CONDUC+COEFH*LONTOT)*X """)

TINTD = FORMULE(REEL="""(REEL:X) =
TEMPD - COEFH*(TEMPD-TEMPG)/(CONDUC+COEFH*LONTOT)*(LONMAX-X)""")
#

TEST_RESU(CHAM_NO=(  _F(  CHAM_GD = TSTAT,
                         NOEUD = 'N3',
                         NOM_CMP = 'TEMP',
                         VALE = EVAL("""TINTG(0.25)"""),
                         CRITERE = 'RELATIF',
                         PRECISION = 1.E-6),
                       _F(  CHAM_GD = TSTAT,
                         NOEUD = 'N5',
                         NOM_CMP = 'TEMP',
                         VALE = EVAL("""TINTG(0.495)"""),
                         CRITERE = 'RELATIF',
                         PRECISION = 1.E-6),
                       _F(  CHAM_GD = TSTAT,
                         NOEUD = 'N101',
                         NOM_CMP = 'TEMP',
                         VALE = EVAL("""TINTD(0.505)"""),
                         CRITERE = 'RELATIF',
                         PRECISION = 1.E-6),
                       _F(  CHAM_GD = TSTAT,
                         NOEUD = 'N103',
                         NOM_CMP = 'TEMP',
                         VALE = EVAL("""TINTD(0.75)"""),
                         CRITERE = 'RELATIF',
                         PRECISION = 1.E-6))
            )

#

FIN()
#
