                   REFERENCE='AUTRE_ASTER',
                   PRECISION=1.E-2,
                   VALE_REFE=230.773539034,
                   VALE_CALC=232.990802961),
                _F(NUME_ORDRE=7,
                   PARA='FREQ',
                   RESULTAT=MODES,
                   REFERENCE='AUTRE_ASTER',
