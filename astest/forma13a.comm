                   PRECISION=3.E-3,
                   VALE_REFE=259.379355318,
                   VALE_CALC=259.995449785),
                _F(NUME_ORDRE=9,
                   PARA='FREQ',
                   RESULTAT=MODES,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=6.E-3,
                   VALE_REFE=261.071699943,
                   VALE_CALC=262.404357484),
                _F(NUME_ORDRE=10,
                   PARA='FREQ',
                   RESULTAT=MODES,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=2.5E-2,
                   VALE_REFE=278.070045602,
                   VALE_CALC=284.769516921),),);


FIN();
