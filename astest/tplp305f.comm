DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#----------------------------------------------
#            MAILLAGE
#----------------------------------------------

MAILLAG1=LIRE_MAILLAGE(INFO=1,FORMAT='MED',);

# longueur du barreau
LY = 5.

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_MA=_F(NOM='SURF',GROUP_MA='G_2D_7',),
                    CREA_GROUP_NO=(
#                                  GROUPES SUR LESQUELS ON VA IMPOSER LES DIRICHLETS
                                   _F(NOM = 'LIGNSUP',
                                      OPTION = 'PLAN',
                                      POINT = (0.,0.5*LY,),
                                      VECT_NORMALE = (0.,1.,),
                                      PRECISION = 0.1*LY,),
                                   _F(NOM = 'LIGNINF',
                                      OPTION = 'PLAN',
                                      POINT = (0.,-0.5*LY,),
                                      VECT_NORMALE = (0.,1.,),
                                      PRECISION = 0.1*LY,),
#                                 GROUPES SUR LESQUELS ON VA TESTER LES VALEURS DE T
                                   _F(NOM = 'NOXHSUP',
                                      OPTION = 'PLAN',
                                      POINT = (0.,0.5*LY/5.,),
                                      VECT_NORMALE = (0.,1.,),
                                      PRECISION = 0.1*LY,),
                                   _F(NOM = 'NOXHINF',
                                      OPTION = 'PLAN',
                                      POINT = (0.,-0.5*LY/5.,),
                                      VECT_NORMALE = (0.,1.,),
                                      PRECISION = 0.1*LY,),
                                   ),
                    );

IMPR_RESU(FORMAT='MED',UNITE=83,RESU=_F(MAILLAGE=MAILLAG1))

#----------------------------------------------
#            MODELE ET FISSURE (INTERFACE)
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='THERMIQUE',
                             MODELISATION='PLAN',),);

LN1=FORMULE(NOM_PARA=('X','Y'),VALE='Y');

FISS1=DEFI_FISS_XFEM(MAILLAGE=MAILLAG1,
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1),
                    INFO=1,);

MODELEX=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS1,
                         INFO=1,);
 
#----------------------------------------------
#            MATERIAU
#----------------------------------------------

MATE=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,RHO_CP = 2.0))

CHMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                    MODELE=MODELEX,
                    AFFE=_F(TOUT='OUI',MATER=MATE,),)


#----------------------------------------------
#            CHARGEMENT THERMIQUE
#----------------------------------------------

CHTH=AFFE_CHAR_THER(MODELE=MODELEX,
                    TEMP_IMPO=(
                             _F(GROUP_NO = 'LIGNSUP',TEMP = 20.),
                             _F(GROUP_NO = 'LIGNINF',TEMP = 10.),
                             ))

#----------------------------------------------
#            CALCUL THERMIQUE LINEAIRE STATIONNAIRE
#----------------------------------------------

TEMPE=THER_LINEAIRE(MODELE=MODELEX,
                    CHAM_MATER=CHMAT,
                    EXCIT=_F(CHARGE=CHTH),)

#----------------------------------------------
#            POST-TRAITEMENTS
#----------------------------------------------


MA_VISU=POST_MAIL_XFEM(MODELE=MODELEX)

MA_VISU=DEFI_GROUP(reuse =MA_VISU,
                   MAILLAGE=MA_VISU,
                   CREA_GROUP_NO=(
#                                 GROUPES SUR LESQUELS ON VA TESTER LES VALEURS DE T
                                  _F(NOM = 'NOCSUP',
                                     OPTION = 'PLAN',
                                     POINT = (0.,0.5*LY/5.,),
                                     VECT_NORMALE = (0.,1.,),
                                     PRECISION = 1.e-2*LY,),
                                  _F(NOM = 'NOCINF',
                                     OPTION = 'PLAN',
                                     POINT = (0.,-0.5*LY/5.,),
                                     VECT_NORMALE = (0.,1.,),
                                     PRECISION = 1.e-2*LY,),
                                  ),
                   )

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_VISU,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='THERMIQUE',
                             MODELISATION='PLAN',),)

RES_VISU=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = TEMPE)

#----------------------------------------------
#            RELEVES
#----------------------------------------------

# RELEVES SUR LE RESULTAT XFEM

TEMXSUP=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='NOXHSUP',
                                RESULTAT=TEMPE,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),)

TEMXINF=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='NOXHINF',
                                RESULTAT=TEMPE,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),)

# RELEVES SUR LE RESULTAT VISU

TEMCSUP=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='NOCSUP',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),)

TEMCINF=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='NOCINF',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),)

#----------------------------------------------
#            TESTS
#----------------------------------------------

# TESTS SUR LE RESULTAT XFEM

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=20.0,
           VALE_REFE=20.,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMXSUP,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=5.0,
           VALE_REFE=5.,
           NOM_PARA='H1',
           TYPE_TEST='MAX',
           TABLE=TEMXSUP,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=10.0,
           VALE_REFE=10.,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMXINF,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=5.0,
           VALE_REFE=5.,
           NOM_PARA='H1',
           TYPE_TEST='MAX',
           TABLE=TEMXINF,)

# -- 

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=20.0,
           VALE_REFE=20.,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMXSUP,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=5.0,
           VALE_REFE=5.,
           NOM_PARA='H1',
           TYPE_TEST='MIN',
           TABLE=TEMXSUP,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=10.0,
           VALE_REFE=10.,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMXINF,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=5.0,
           VALE_REFE=5.,
           NOM_PARA='H1',
           TYPE_TEST='MIN',
           TABLE=TEMXINF,)

# TESTS SUR LE RESULTAT VISU

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=20.0,
           VALE_REFE=20.,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMCSUP,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=10.0,
           VALE_REFE=10.,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMCINF,)

# -- 

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=20.0,
           VALE_REFE=20.,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMCSUP,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=10.0,
           VALE_REFE=10.,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMCINF,)

# TESTS SUR LE CHAMP 'TEMP_ELGA' DANS LE RESULTAT XFEM

# # POUR AFFICHER AU FORMAT TEXTE LE CHAMP TEMP_ELGA
# CHTELGA=CREA_CHAMP(TYPE_CHAM = 'ELGA_TEMP_R',
#                    OPERATION = 'EXTR',
#                    RESULTAT  =  TEMPE,
#                    NOM_CHAM  = 'TEMP_ELGA',
#                    NUME_ORDRE = 0,
#                    INFO = 2)

TEST_RESU(RESU=(
                _F(RESULTAT=TEMPE,
                   NOM_CHAM='TEMP_ELGA',
                   NOM_CMP='TEMP',
                   NUME_ORDRE = 0,
                   MAILLE='M2',
                   POINT=1,
                   REFERENCE = 'ANALYTIQUE' ,
                   VALE_REFE=10.,
                   VALE_CALC=10.0,),
                _F(RESULTAT=TEMPE,
                   NOM_CHAM='TEMP_ELGA',
                   NOM_CMP='TEMP',
                   NUME_ORDRE = 0,
                   MAILLE='M3',
                   POINT=1,
                   REFERENCE = 'ANALYTIQUE' ,
                   VALE_REFE=20.,
                   VALE_CALC=20.0,),
                _F(RESULTAT=TEMPE,
                   NOM_CHAM='TEMP_ELGA',
                   NOM_CMP='TEMP',
                   NUME_ORDRE = 0,
                   MAILLE='M3',
                   POINT=13,
                   REFERENCE = 'ANALYTIQUE' ,
                   VALE_REFE=10.,
                   VALE_CALC=10.0,),
                _F(RESULTAT=TEMPE,
                   NOM_CHAM='TEMP_ELGA',
                   NOM_CMP='TEMP',
                   NUME_ORDRE = 0,
                   MAILLE='M4',
                   POINT=1,
                   REFERENCE = 'ANALYTIQUE' ,
                   VALE_REFE=20.,
                   VALE_CALC=20.0,),
                ),
          )

IMPR_TABLE(TABLE=TEMXSUP)
IMPR_TABLE(TABLE=TEMXINF)

IMPR_TABLE(TABLE=TEMCSUP)
IMPR_TABLE(TABLE=TEMCINF)

IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_VISU))
IMPR_RESU(FORMAT='MED',UNITE=81,RESU=_F(MAILLAGE=MA_VISU))

FIN();
