MA=LIRE_MAILLAGE(FORMAT='MED',)


MA=DEFI_GROUP( reuse=MA,
               MAILLAGE=MA,
               CREA_GROUP_NO=_F(
                TOUT_GROUP_MA = 'OUI')
              )


#IMPR_RESU(RESU=_F(MAILLAGE=MA));

