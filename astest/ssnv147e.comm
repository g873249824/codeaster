                ETA_PILO_R_MAX = 10.,
                SELECTION='RESIDU',
                ),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1,
                REAC_ITER_ELAS=1,
                ),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 10,
                ),

             )


TABLE = RECU_TABLE(CO=REPONSE, NOM_PARA='ETA_PILOTAGE')

TEST_RESU(RESU=(_F(NUME_ORDRE=19,
                   GROUP_NO='E',
