                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIZZ',
                   VALE_CALC=0.5,
                   MAILLE='M1',),
                _F(NUME_ORDRE=19,
                   POINT=1,
                   RESULTAT=REPONSE,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=0.90000000000000002,
                   MAILLE='M1',),
                ),
          )

