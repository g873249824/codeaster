MO=AFFE_MODELE(
                MAILLAGE=MA,
                AFFE=_F(
               GROUP_MA = ('BLOC', 'FIXE', 'TRAC',),
               PHENOMENE = 'MECANIQUE',
               MODELISATION = '3D')
             )

CALCAIRE=DEFI_MATERIAU(
                    ELAS=_F(
                E = 20000.,
                NU = 0.),
               BETON_ECRO_LINE=_F(
                SYT = 6.,
                D_SIGM_EPSI = -10000.),
               )

CAL_FAIB=DEFI_MATERIAU(
                    ELAS=_F(
                E = 20000.,
                NU = 0.),
               BETON_ECRO_LINE=_F(
                SYT = 5.,
                D_SIGM_EPSI = -10000.),
               )

CHMAT=AFFE_MATERIAU(
                MAILLAGE=MA,AFFE=(
                    _F(
                GROUP_MA = 'BLOC',
                MATER = CALCAIRE), _F(
                MAILLE = 'M1',
                MATER = CAL_FAIB))
              )

L_INST=DEFI_LIST_REEL(
               DEBUT=0.0,INTERVALLE=(

                _F(  JUSQU_A = 1.,       NOMBRE = 1),
                _F(  JUSQU_A = 100.,     NOMBRE = 990))
              )

LIAISON=AFFE_CHAR_MECA(
               MODELE=MO,
                FACE_IMPO=_F( GROUP_MA = 'FIXE',  DX = 0., DY = 0., DZ = 0.)
              )

TRACTION=AFFE_CHAR_MECA(
                      MODELE=MO,
               FORCE_FACE=_F( GROUP_MA = 'TRAC', FZ = 1.)
              )

RAMPE=DEFI_FONCTION(
              NOM_PARA='INST',
                  VALE=(0., 0.,
                        1., 1.,)
             )

REPONSE=STAT_NON_LINE(
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, FONC_MULT = RAMPE)),
                  COMP_INCR=_F(
                RELATION = 'ENDO_ISOT_BETON'),

                INCREMENT=_F(
                LIST_INST = L_INST,
                INST_FIN = 1.),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50)
             )


REPONSE=STAT_NON_LINE( reuse=REPONSE,
                  ETAT_INIT=_F(
                EVOL_NOLI = REPONSE),
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, TYPE_CHARGE = 'FIXE_PILO')),
                  COMP_INCR=_F(
                RELATION = 'ENDO_ISOT_BETON'),

                INCREMENT=_F(
                LIST_INST = L_INST,),
               PILOTAGE=_F(
                TYPE = 'PRED_ELAS',
