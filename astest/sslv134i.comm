DEBUT(CODE=_F(NOM='SSLV134I', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'),
      IGNORE_ALARM=('CALCULEL2_63')
      )

# <CALCULEL2_63> : dans un modele, il existe des elements de type "bord" qui n'ont pas de voisin avec rigidite
#  le modele provoquant ces alarmes ne sert qu'a la visualisation

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MA=DEFI_MATERIAU(ELAS=_F(E=2e+11,
                         NU=0.3,
                         RHO = 7850,),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_2D=_F(GROUP_MA=('haut','bas',),),);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(GROUP_MA=('FACE','haut','bas', 'BD',),),);

MODE=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F(GROUP_MA=('FACE','haut','bas', 'BD',),
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'AXIS',), );
LT = FORMULE(VALE='X-2',
             NOM_PARA='X',);

LN = FORMULE(VALE='Y',
             NOM_PARA='Y',);

FISS=DEFI_FISS_XFEM(MODELE=MODE,
                    TYPE_DISCONTINUITE='FISSURE',
                    DEFI_FISS=_F(FONC_LT=LT,
                                 FONC_LN=LN,),
                    );

MODEX=MODI_MODELE_XFEM(MODELE_IN=MODE,
                       FISSURE=FISS,);

CHAR=AFFE_CHAR_MECA(MODELE=MODEX,
                    DDL_IMPO=(  _F(GROUP_NO='C',
                                 DY=0.0,),
                                 ),
                    FORCE_CONTOUR=(
                                _F(  GROUP_MA = 'haut',  FY = 1.E6,),
                                _F(  GROUP_MA = 'bas', FY = -1.E6,),),
                    );

MATE=AFFE_MATERIAU(MAILLAGE=MAIL,
                   MODELE=MODEX,
                   AFFE=_F(TOUT='OUI',
                           MATER=MA,),);

RESU=MECA_STATIQUE(MODELE=MODEX,
                   CHAM_MATER=MATE,
                   EXCIT=(_F(CHARGE=CHAR),
                            ),
                   );
# IMPR_RESU(RESU=_F(RESULTAT=RESU,),);


# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------
#
MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODEX)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F( GROUP_MA=('FACE','haut','bas', 'BD',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='AXIS',),)

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = RESU,
                        )

#IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_XFEM));

#-------------------------------------------------------
# Taille d'element h = 0.1
TAB=CALC_G(THETA=_F(FISSURE=FISS,
                    R_INF=0.1,
                    R_SUP=0.3,),
           RESULTAT=RESU,
           COMP_ELAS=_F(RELATION='ELAS',),
           OPTION='CALC_K_G',);

IMPR_TABLE(TABLE=TAB,);

SIF=POST_K1_K2_K3(MODELISATION='AXIS',
                    FISSURE=FISS,
                    MATER=MA,
                    RESULTAT=RESU,
                    ABSC_CURV_MAXI=0.3,
                    );

IMPR_TABLE(TABLE=SIF,);
# ---------------
# VALEUR DE REFERENCE MILIEU INFINI :
G_REF=11.58648
K1_REF=1.595769E6


TEST_TABLE(       TABLE=TAB,
                  NOM_PARA='G',
                   REFERENCE='ANALYTIQUE',
                   VALE=G_REF,
                 PRECISION=0.021        )

TEST_TABLE(       TABLE=TAB,
                  NOM_PARA='K1',
                   REFERENCE='ANALYTIQUE',
                   VALE=K1_REF,
                 PRECISION=0.04        )


# VALEUR DE REFERENCE OBETNUS DE FISSURE MAILLE DANS SSLV134C (DE CALC_G)
G_REF=11.7803
K1_REF=1.6431E6


TEST_TABLE(       TABLE=TAB,
                  NOM_PARA='G',
                   REFERENCE='AUTRE_ASTER',
                   VALE=G_REF,
                 PRECISION=0.004        )

TEST_TABLE(       TABLE=TAB,
                  NOM_PARA='K1',
                   REFERENCE='AUTRE_ASTER',
                   VALE=K1_REF,
                 PRECISION=0.008        )

# -------------------------------------------------------------
#            POUR VALIDER LA FORCE CONTRIGUGE
# -------------------------------------------------------------
# LA VITESSE DE TOTATIO EST 150 TRS/MINUTE
VAL_ROT = 15.708

CHAR_ROT= AFFE_CHAR_MECA(MODELE=MODEX,
                         DDL_IMPO=_F(GROUP_NO='C',DY=0.0,),
                         FORCE_CONTOUR=(_F(GROUP_MA = 'haut',  FY =  1.E6,),
                                        _F(GROUP_MA = 'bas',   FY = -1.E6,),),
                         ROTATION=_F(VITESSE = VAL_ROT,
                                     AXE = (0.,1.,0.,),
                                     CENTRE =(0.,0.,0.,),
                                     ),
                         )

RESU_ROT=MECA_STATIQUE(MODELE=MODEX,
                       CHAM_MATER=MATE,
                       EXCIT=_F(CHARGE=CHAR_ROT))

MA_XROT=POST_MAIL_XFEM(MODELE=MODEX)

MOD_XROT=AFFE_MODELE(MAILLAGE=MA_XROT,
                     AFFE=_F( GROUP_MA=('FACE'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='AXIS',),);

RES_XROT=POST_CHAM_XFEM(MODELE_VISU = MOD_XROT,
                        RESULTAT    = RESU_ROT)

#IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_XROT));

#-------------------------------------------------------
# Taille d'element h = 0.1
TAB_ROT=CALC_G(THETA=_F(FISSURE=FISS,
                    R_INF=0.1,
                    R_SUP=0.3,),
           RESULTAT=RESU_ROT,
           COMP_ELAS=_F(RELATION='ELAS',),
           OPTION='CALC_K_G',);

IMPR_TABLE(TABLE=TAB_ROT,);

SIF_ROT=POST_K1_K2_K3(MODELISATION='AXIS',
                    FISSURE=FISS,
                    MATER=MA,
                    RESULTAT=RESU_ROT,
                    ABSC_CURV_MAXI=0.3,
                    );

IMPR_TABLE(TABLE=SIF_ROT,);

# VALEUR DE REFERENCE OBETNUS DE FISSURE MAILLE DANS SSLV134C (DE CALC_G)
G_REF=2136.52
K1_REF=21.911E6


TEST_TABLE(       TABLE=TAB_ROT,
                  NOM_PARA='G',
                   REFERENCE='AUTRE_ASTER',
                   VALE=G_REF,
                 PRECISION=0.003        )

TEST_TABLE(       TABLE=TAB_ROT,
                  NOM_PARA='K1',
                   REFERENCE='AUTRE_ASTER',
                   VALE=K1_REF,
                 PRECISION=0.01        )

FIN();
