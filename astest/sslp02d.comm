DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))


ma=LIRE_MAILLAGE(FORMAT='MED',);

ma=MODI_MAILLAGE(reuse =ma,
                 MAILLAGE=ma,
                 ORIE_PEAU_2D=_F(GROUP_MA='CD',),);

mod=AFFE_MODELE(MAILLAGE=ma,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='C_PLAN',),);

mater=DEFI_MATERIAU(ELAS=_F(E=3.e4,
                            NU=.25,),);

chmat=AFFE_MATERIAU(MODELE=mod,
                    AFFE=_F(TOUT='OUI',
                            MATER=mater,),);

Charge=AFFE_CHAR_MECA(MODELE=mod,
                      DDL_IMPO=(_F(GROUP_MA='AB',
                                   DY=0.,),
                                _F(GROUP_MA='DE',
                                   DX=0.,),),
                      FORCE_CONTOUR=_F(GROUP_MA='CD',
                                       FY=2.5,),);

RESU=MECA_STATIQUE(MODELE=mod,
                   CHAM_MATER=chmat,
                   EXCIT=_F(CHARGE=Charge,),
                   OPTION='SANS',);

RESU=CALC_CHAMP(reuse =RESU,
                RESULTAT=RESU,
                CONTRAINTE='SIGM_NOEU',);

IMPR_RESU(RESU=_F(RESULTAT=RESU,IMPR_COOR='OUI',
                  NOM_CHAM='SIGM_NOEU',NOM_CMP=('SIXX','SIYY','SIXY'),
                  GROUP_NO=('A','E','F'),),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST=0.,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIYY',
                  GROUP_NO='A',
                  VALE_REFE=7.5,
                  REFERENCE='ANALYTIQUE',
                  PRECISION= 0.015,
                  VALE_CALC=7.61078430503,),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST=0.,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIXX',
                  GROUP_NO='E',
                  VALE_REFE=-2.5,
                  PRECISION= 0.045,
                  REFERENCE='ANALYTIQUE',
                  VALE_CALC=-2.60014083795,),);

#
# --- On passe dans le repere cylindrique
#
RESU1=MODI_REPERE(
                  RESULTAT    = RESU,
                  NUME_ORDRE  = 1,
                  MODI_CHAM  =  _F(NOM_CHAM   = 'SIGM_NOEU',
                                   NOM_CMP    = ('SIXX','SIYY','SIZZ','SIXY',),
                                   TYPE_CHAM  = 'TENS_2D',),
                   REPERE     = 'CYLINDRIQUE',
                   AFFE = _F( ORIGINE    = (0.0, 0.0)))

TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST=0.,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIZZ',
                  GROUP_NO='A',
                  VALE_REFE=7.5,
                  PRECISION= 0.015,
                  REFERENCE='ANALYTIQUE',
                  VALE_CALC=7.61078430503,),);

TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST=0.,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIZZ',
                  GROUP_NO='E',
                  VALE_REFE=-2.5,
                  PRECISION= 0.045,
                  REFERENCE='ANALYTIQUE',
                  VALE_CALC=-2.60014083795,),);

TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST=0.,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIZZ',
                  GROUP_NO='F',
                  VALE_REFE=2.5,
                  PRECISION= 0.0025,
                  REFERENCE='ANALYTIQUE',
                  VALE_CALC=2.5048049475 ,),);

FIN();
