                       NOM_CHAM=( 'DEPL',
                                  'SIGM_ELNO_DEPL', ),
                       ANGL=45. )

U_R=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=U,
                  NUME_ORDRE=1
