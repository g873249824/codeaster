DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(FORMAT='MED',)

#
# DEFINITION DES GROUPES DE NOEUDS
MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'ENCAST'),
                       _F(  GROUP_MA = ('LAB', 'LBH', 'LHE', 'LEC', 'LCD',)))
                   )

#
# DEFINITION DU MODELE
MODELE=AFFE_MODELE(    MAILLAGE=MAIL,
                         AFFE=_F(  TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS_FOURIER')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 2.1E11,  NU = 0.3,
                                 ALPHA = 1.E-5,  RHO = 7800.)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(    MAILLAGE=MAIL,
                          AFFE=_F(  TOUT = 'OUI',
                                 MATER = ACIER)  )

# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
CLIM=AFFE_CHAR_MECA(    MODELE=MODELE,
                            DDL_IMPO=_F(  GROUP_NO = 'ENCAST',
                                       DX = 0.,  DY = 0.,  DZ = 0.)  )

#   PRESSION RADIALE SUR BC
PR1_RAD=AFFE_CHAR_MECA(     MODELE=MODELE,
                              PRES_REP=_F(  GROUP_MA = ('LBH', 'LHE', 'LEC',),
                                         PRES = 250.)
                          )

#   PRESSION LONGIT. SUR BC
F_Y=DEFI_FONCTION(    NOM_PARA='Y',
                        VALE=( 0.,         0.,
                               0.24,   10000., )
                    )

PR1_LON=AFFE_CHAR_MECA_F(   MODELE=MODELE,
                              FORCE_CONTOUR=_F(  GROUP_MA = ('LBH', 'LHE', 'LEC',),
                                              FY = F_Y)
                          )

#   PRESSION NORMALE SUR CD
F_R=DEFI_FONCTION(    NOM_PARA='X',
                        VALE=( 0.,        0.,
                               0.006,   583.333, )
                    )

PR2_NOR=AFFE_CHAR_MECA_F(   MODELE=MODELE,
                              PRES_REP=_F(  GROUP_MA = 'LCD',
                                         PRES = F_R)
                          )

#   PRESSION TANGENTIELLE SUR  CD
PR2_TAN=AFFE_CHAR_MECA(     MODELE=MODELE,
                              FORCE_CONTOUR=_F(  GROUP_MA = 'LCD',
                                              FX = 10000.,  FZ = 10000.)
                          )

#  RESOLUTION
DEF=MACRO_ELAS_MULT(MODELE=MODELE,
                    CHAM_MATER=CHMAT,
                    CHAR_MECA_GLOBAL=CLIM,
                    CAS_CHARGE=_F(MODE_FOURIER = 1,
                                  TYPE_MODE = 'ANTI',
                                  CHAR_MECA = (PR1_RAD, PR1_LON,
                                               PR2_NOR, PR2_TAN,),
                                  OPTION = 'SANS',),)
DEF=CALC_CHAMP(reuse= DEF,
               RESULTAT = DEF,
               CONTRAINTE='SIGM_ELNO',);

# RECUPERATION DES CHAMPS ELEMENTAIRES
U0=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=DEF,
                 NUME_ORDRE=1
                   )

#
# RECOMBINAISONS DE FOURIER EN DEPLACEMENTS ET CONTRAINTES
# POUR TETA = 45. DEGRES
U=COMB_FOURIER(    RESULTAT=DEF,
                       NOM_CHAM=( 'DEPL',
                                  'SIGM_ELNO', ),
                       ANGLE=45. )

U_R=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=U,
                  NUME_ORDRE=1
                   )

