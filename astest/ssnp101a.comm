DEBUT(CODE=_F( NOM = 'SSNP101A',NIV_PUB_WEB='INTERNET'))

#......................................................................
# PLAQUE CARREE EN TRACTION CISAILLEMENT CALCUL 2D DEF. PLANES
# LOI DE LEMAITRE
#......................................................................


M=LIRE_MAILLAGE( )

MO=AFFE_MODELE( 
            MAILLAGE=M,    VERIF='MAILLE',
           AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = 'D_PLAN')
           )

ACIER=DEFI_MATERIAU( 
                ELAS=_F( E = 178600., NU = 0.3, ALPHA = 0.),
            LEMAITRE=_F( N = 11.,UN_SUR_K = 0.00032840723,UN_SUR_M = 0.178571429)
           )

CM=AFFE_MATERIAU( 
            MAILLAGE=M,   
           AFFE=_F( TOUT = 'OUI',MATER = ACIER, TEMP_REF = 20.) )

# LES FORCES SONT MULTIPLIEES PAR 2 PAR RAPPORT AU TEST SSNP05A

P30=DEFI_FONCTION( 
           NOM_PARA='INST',PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',
           VALE=(0.0, 0.0, 30.0, 60.62177826,3630.0, 60.62177826,
           3660.0,121.2435565,7260.0,121.2435565,)
           )

M30=DEFI_FONCTION( 
           NOM_PARA='INST',PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',
           VALE=(0.0, 0.0, 30.0,-60.62177826,3630.0,-60.62177826,
           3660.0,-121.2435565,7260.0,-121.2435565,)
           )

M52M30=DEFI_FONCTION( 
           NOM_PARA='INST',PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',
           VALE=(0.0, 0.0, 30.0,-165.6217783,3630.0,-165.6217783,
           3660.0,-121.2435565,7260.0,-121.2435565,)
           )

M52P30=DEFI_FONCTION( 
           NOM_PARA='INST',PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',
           VALE=(0.0, 0.0, 30.0,-44.37822174,3630.0,-44.37822174,
           3660.0,121.2435565,7260.0,121.2435565,)
           )

CH1=AFFE_CHAR_MECA( 
            MODELE=MO,DDL_IMPO=(
           _F( NOEUD = 'N4', DX = 0.,DY = 0.),
                    _F( NOEUD = 'N3', DX = 0.))
           )

CH2=AFFE_CHAR_MECA_F( 
            MODELE=MO,FORCE_NODALE=(
           _F( NOEUD = ('N2',), FX = M52P30,  FY = M30),
                        _F( NOEUD = ('N3',), FX = P30,     FY = P30),
                        _F( NOEUD = ('N1',), FX = M52M30,  FY = M30),
                        _F( NOEUD = ('N4',), FX = M30,     FY = P30))
                          )

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,INTERVALLE=(
             _F( JUSQU_A = 30.0,       NOMBRE = 20),
             _F( JUSQU_A = 3630.0,     NOMBRE = 15),
             _F( JUSQU_A = 3660.0,     NOMBRE = 3),
             _F( JUSQU_A = 3720.0,     NOMBRE = 1))
           )

U=STAT_NON_LINE( 
                            MODELE=MO,
                        CHAM_MATER=CM,EXCIT=(
                             _F( 
                CHARGE = CH1), _F( 
                CHARGE = CH2)),
                         COMP_INCR=_F( 
                RELATION = 'LEMAITRE'),
                         INCREMENT=_F( 
                LIST_INST = L_INST),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F( 
                RESI_GLOB_MAXI = 1.E-6,
                ITER_GLOB_MAXI = 25,
                ITER_INTE_MAXI = 1000)
              )

U=CALC_ELEM( reuse=U,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=U,
                            MODELE=MO,
                        CHAM_MATER=CM
              )

U=CALC_ELEM(reuse=U,   MODELE=MO,      RESULTAT=U,
             CHAM_MATER=CM,   OPTION='EPSI_ELNO_DEPL')

U=CALC_ELEM(reuse=U,   MODELE=MO,      RESULTAT=U,
             CHAM_MATER=CM,   OPTION='EPSP_ELNO')

TEST_RESU(RESU=(

  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 20,NOM_CMP = 'EPXX',VALE = 1.762E-04,PRECISION = 0.005),
  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 20,NOM_CMP = 'EPXY',VALE = 1.816E-04,PRECISION = 0.005),
  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 35,NOM_CMP = 'EPXX',VALE = 1.903E-03,PRECISION = 0.005),
  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 35,NOM_CMP = 'EPXY',VALE = 2.079E-03,PRECISION = 0.005),
  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 38,NOM_CMP = 'EPXX',VALE = 1.913E-03,PRECISION = 0.005),
  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 38,NOM_CMP = 'EPXY',VALE = 2.190E-03,PRECISION = 0.005),
  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 39,NOM_CMP = 'EPXX',VALE = 1.874E-03,PRECISION = 0.005),
  _F( RESULTAT = U,NOM_CHAM = 'EPSP_ELNO',MAILLE = 'M1',NOEUD = 'N2',
NUME_ORDRE = 39,NOM_CMP = 'EPXY',VALE = 3.181E-03,PRECISION = 0.005))

           )

FIN()
