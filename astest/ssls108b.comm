DEFI_FICHIER( UNITE=37,
              FICHIER='./REPE_OUT/MA12.pos')

IMPR_RESU ( MODELE=MODELE_1,
            FORMAT='GMSH', VERSION=1.2, UNITE=37,
            RESU=_F( MAILLAGE=MAILLA_1 )   ) ;

DEFI_FICHIER(ACTION='LIBERER',UNITE=37)

TEST_FICHIER(FICHIER='./REPE_OUT/MA12.pos',
             NB_CHIFFRE=6,
             EPSILON=1.E-14,
             VALE_K='f0be8f5636a96d0981010f1989cd40ea',
             REFERENCE='NON_REGRESSION',
             VERSION='7.2.23',
             INFO=1)

