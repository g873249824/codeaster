DEFI_FICHIER( UNITE=38,
              FICHIER='./REPE_OUT/DEP12.pos')

IMPR_RESU ( MODELE=MODELE_1,
            FORMAT='GMSH', VERSION=1.2, UNITE=38,
            RESU=(_F( CHAM_GD=DEFZ, TYPE_CHAM='VECT_3D',NOM_CMP=('DX','DY','DZ' )   ),
                  _F( CHAM_GD=DEFZ, NOM_CMP=('DRX','DRY','DRZ' ) ),),)

