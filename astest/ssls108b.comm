DEFI_FICHIER( UNITE=35,
              FICHIER='./REPE_OUT/MA10.pos')

IMPR_RESU ( MODELE=MODELE_1,
            FORMAT='GMSH', VERSION=1.0, UNITE=35,
            RESU=_F( MAILLAGE=MAILLA_1 )   ) ;

DEFI_FICHIER(ACTION='LIBERER',UNITE=35)

TEST_FICHIER(FICHIER='./REPE_OUT/MA10.pos',
             NB_CHIFFRE=6,
             EPSILON=1.E-14,
