T = 10.

import numpy as NP
from math import pi

sro=DEFI_FONCTION(NOM_PARA='FREQ',
                  VALE=SRO_7,
                  #INTERPOL='LOG',
                  PROL_DROITE='CONSTANT',
                  PROL_GAUCHE='CONSTANT',);


