          FREQ=NP.arange(0., FCOUP, 1./4./pi),
          FREQ_COUP=FCOUP, AMOR_REDUIT=amort,
          DUREE=T, NORME=10.,
          FRACT=0.5,),
          INTERPOL='LIN',)


F_DSP = CALC_FONCTION(COMB_C=(_F(FONCTION=FR_DSP,
         COEF_C = 1.+0.j), ),)

