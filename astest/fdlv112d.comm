                      AFFE=( _F(GROUP_MA=( 'STRVOU2' ),
                                PHENOMENE= 'MECANIQUE',
                                MODELISATION= '3D'),
                             _F(GROUP_MA = 'PBASE',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'DIS_TR'),
