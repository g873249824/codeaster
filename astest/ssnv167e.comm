                   GROUP_NO='NH3',
                   NOM_CMP='DZ',
                   VALE_CALC= 1.00000000E+04,
                   VALE_REFE= 1.E4,
                   PRECISION=1.E-08,),
                _F(INST=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SOL,
                   NOM_CHAM='REAC_NODA',
                   GROUP_NO='NH4',
                   NOM_CMP='DZ',
                   VALE_CALC= 1.00000000E+04,
                   VALE_REFE=1.E4,
                   PRECISION=1.E-08,),
                ),
          )

TEST_RESU(RESU=(_F(INST=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SOL,
                   NOM_CHAM='REAC_NODA',
                   GROUP_NO='NB5',
                   NOM_CMP='DZ',
                   VALE_CALC= -1.00000000E+04,
                   VALE_REFE=-1.E4,
                   PRECISION=1.E-08,),
                _F(INST=1,
