                   GROUP_NO='NB8',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.1,
                   VALE_REFE=-0.1,
                   PRECISION=1.E-08,),
                ),
          )

TEST_RESU(RESU=(_F(INST=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SOL,
                   NOM_CHAM='REAC_NODA',
                   GROUP_NO='NH1',
                   NOM_CMP='DZ',
                   VALE_CALC= 1.00000000E+04,
                   VALE_REFE=1.E4,
                   PRECISION=1.E-08,),
                _F(INST=1,
