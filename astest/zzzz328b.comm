# zzzz328b.para = tps_job 60 mem_job 256Mo ncpus 1 liste_test S

DEBUT(CODE=_F(NOM='ZZZZ328B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI',),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',);

MAT=DEFI_MATERIAU(ELAS=_F(E=1e9,
                            NU=0.2,),);

L_TPS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);

# MODELISATION D_PLAN

MO0=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN',),);

MOMAT0=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

AX=0.0
AY=0.0
BX=1.E-3
BY=0.0
CX=2.E-3
CY=-2.E-3
DX=1.E-3
DY=-2.E-3

BLOC0=AFFE_CHAR_MECA(MODELE=MO0,
                       DDL_IMPO=(_F(GROUP_NO='A',
                                    DX=AX,
                                    DY=AY,),
                                 _F(GROUP_NO='B',
                                    DX=BX,
                                    DY=BY,),
                                 _F(GROUP_NO='C',
                                    DX=CX,
                                    DY=CY,),
                                 _F(GROUP_NO='D',
                                    DX=DX,
                                    DY=DY,),),);

U0=STAT_NON_LINE(MODELE=MO0,
                CHAM_MATER=MOMAT0,
                EXCIT=_F(CHARGE=BLOC0,),
                COMP_INCR=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_TPS,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH,),);

U0=CALC_CHAMP(reuse = U0, ENERGIE='ENEL_ELEM', RESULTAT=U0, );

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U0,
                  NOM_CHAM='ENEL_ELEM',
                  NOM_CMP='TOTALE',
                  VALE_REFE=2430.555556,
                  VALE_CALC=2430.555556,
                  MAILLE='M5',),);

# MODELISATION D_PLAN_SI

MO1=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_SI',),);

MOMAT1=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

BLOC1=AFFE_CHAR_MECA(MODELE=MO1,
                       DDL_IMPO=(_F(GROUP_NO='A',
                                    DX=AX,
                                    DY=AY,),
                                 _F(GROUP_NO='B',
                                    DX=BX,
                                    DY=BY,),
                                 _F(GROUP_NO='C',
                                    DX=CX,
                                    DY=CY,),
                                 _F(GROUP_NO='D',
                                    DX=DX,
                                    DY=DY,),),);

U1=STAT_NON_LINE(MODELE=MO1,
                CHAM_MATER=MOMAT1,
                EXCIT=_F(CHARGE=BLOC1,),
                COMP_INCR=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_TPS,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH,),);

U1=CALC_CHAMP(reuse = U1, ENERGIE='ENEL_ELEM', RESULTAT=U1, );

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U1,
                  NOM_CHAM='ENEL_ELEM',
                  NOM_CMP='TOTALE',
                  VALE_REFE=2430.555556,
                  VALE_CALC=2430.555556,
                  MAILLE='M5',),);

# MODELISATION D_PLAN_GRAD_EPSI

MA2 = CREA_MAILLAGE(MAILLAGE = MA,
                    LINE_QUAD = _F(TOUT = 'OUI'),)

MO2=AFFE_MODELE(MAILLAGE=MA2,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_GRAD_EPSI',),);

MAT2=DEFI_MATERIAU(ELAS=_F(E=1e9,
                           NU=0.2,),
                   ENDO_ORTH_BETON=_F(K0=3000e6,
                                      K1=10500000.0,
                                      ECROB=1000.0,
                                      ECROD=60000.0,),
                   NON_LOCAL = _F(LONG_CARA=0.0,),);

MOMAT2=AFFE_MATERIAU(MAILLAGE=MA2,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT2,),);

NS1X=(AX+DX)*0.5
NS1Y=(AY+DY)*0.5
NS2X=(AX+BX)*0.5
NS2Y=(AY+BY)*0.5
NS3X=(BX+CX)*0.5
NS3Y=(BY+CY)*0.5
NS4X=(CX+DX)*0.5
NS4Y=(CY+DY)*0.5

BLOC2=AFFE_CHAR_MECA(MODELE=MO2,
                       DDL_IMPO=(_F(GROUP_NO='A',
                                    DX=AX,
                                    DY=AY,),
                                 _F(GROUP_NO='B',
                                    DX=BX,
                                    DY=BY,),
                                 _F(GROUP_NO='C',
                                    DX=CX,
                                    DY=CY,),
                                 _F(GROUP_NO='D',
                                    DX=DX,
                                    DY=DY,),
                                 _F(NOEUD='NS1',
                                    DX=NS1X,
                                    DY=NS1Y,),
                                 _F(NOEUD='NS2',
                                    DX=NS2X,
                                    DY=NS2Y,),
                                 _F(NOEUD='NS3',
                                    DX=NS3X,
                                    DY=NS3Y,),
                                 _F(NOEUD='NS4',
                                    DX=NS4X,
                                    DY=NS4Y,),),);

U2=STAT_NON_LINE(MODELE=MO2,
                CHAM_MATER=MOMAT2,
                EXCIT=_F(CHARGE=BLOC2,),
                COMP_INCR=_F(RELATION='ENDO_ORTH_BETON',),
                INCREMENT=_F(LIST_INST=L_TPS,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH,),);

U2=CALC_CHAMP(reuse = U2, ENERGIE='ENEL_ELEM', RESULTAT=U2, );

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U2,
                  NOM_CHAM='ENEL_ELEM',
                  NOM_CMP='TOTALE',
                  VALE_REFE=2430.555556,
                  VALE_CALC=2430.555556,
                  MAILLE='M5',),);

# MODELISATION D_PLAN_GRAD_SIGM

MO3=AFFE_MODELE(MAILLAGE=MA2,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_GRAD_SIGM',),);

MAT3=DEFI_MATERIAU(
                ELAS=_F(E = 1.E9,
                        NU = 0.2),
                ENDO_HETEROGENE=_F(
                SY = 5.0E20,
                WEIBULL = 6.0,
                KI = 1.0E6,
                EPAI = 1.0,
                GR = 121.0,
                ),
               NON_LOCAL=_F(
               LONG_CARA = 0.1)
              )

MOMAT3=AFFE_MATERIAU(MAILLAGE=MA2,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT3,),);

BLOC3=AFFE_CHAR_MECA(MODELE=MO3,
                       DDL_IMPO=(_F(GROUP_NO='A',
                                    DX=AX,
                                    DY=AY,),
                                 _F(GROUP_NO='B',
                                    DX=BX,
                                    DY=BY,),
                                 _F(GROUP_NO='C',
                                    DX=CX,
                                    DY=CY,),
                                 _F(GROUP_NO='D',
                                    DX=DX,
                                    DY=DY,),
                                 _F(NOEUD='NS1',
                                    DX=NS1X,
                                    DY=NS1Y,),
                                 _F(NOEUD='NS2',
                                    DX=NS2X,
                                    DY=NS2Y,),
                                 _F(NOEUD='NS3',
                                    DX=NS3X,
                                    DY=NS3Y,),
                                 _F(NOEUD='NS4',
                                    DX=NS4X,
                                    DY=NS4Y,),),);

U3=STAT_NON_LINE(MODELE=MO3,
                CHAM_MATER=MOMAT3,
                EXCIT=_F(CHARGE=BLOC3,),
                COMP_INCR=_F(RELATION='ENDO_HETEROGENE',),
                INCREMENT=_F(LIST_INST=L_TPS,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH,),);

U3=CALC_CHAMP(reuse = U3, ENERGIE='ENEL_ELEM', RESULTAT=U3, );

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U3,
                  NOM_CHAM='ENEL_ELEM',
                  NOM_CMP='TOTALE',
                  VALE_REFE=2430.555556,
                  VALE_CALC=2430.555556,
                  MAILLE='M5',),);

# MODELISATION PLAN_ELDI

MO4=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='PLAN_ELDI',),);

MAT4=DEFI_MATERIAU(
                ELAS=_F(E = 1.E9,
                        NU = 0.2),
                RUPT_FRAG = _F(SIGM_C = 1.E20,
                               GC     = 100,),);

MOMAT4=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT4,),);

BLOC4=AFFE_CHAR_MECA(MODELE=MO4,
                       DDL_IMPO=(_F(GROUP_NO='A',
                                    DX=AX,
                                    DY=AY,),
                                 _F(GROUP_NO='B',
                                    DX=BX,
                                    DY=BY,),
                                 _F(GROUP_NO='C',
                                    DX=CX,
                                    DY=CY,),
                                 _F(GROUP_NO='D',
                                    DX=DX,
                                    DY=DY,),),);

U4=STAT_NON_LINE(MODELE=MO4,
                CHAM_MATER=MOMAT4,
                EXCIT=_F(CHARGE=BLOC4,),
                COMP_INCR=_F(RELATION='CZM_EXP',),
                INCREMENT=_F(LIST_INST=L_TPS,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH,),);

U4=CALC_CHAMP(reuse = U4, ENERGIE='ENEL_ELEM', RESULTAT=U4, );

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U4,
                  NOM_CHAM='ENEL_ELEM',
                  NOM_CMP='TOTALE',
                  VALE_REFE=2430.555556,
                  VALE_CALC=2430.555556,
                  MAILLE='M5',),);

FIN();
