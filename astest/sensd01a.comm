      _F( RESULTAT = DYHA,    NUME_ORDRE = 2, PARA = 'FREQ',
           CRITERE = 'ABSOLU', VALE = 40., PRECISION = 1.E-8, REFERENCE = 'ANALYTIQUE',),
     _F(  RESULTAT = DYHA,   NUME_ORDRE = 2,
          NOM_CHAM = 'DEPL',   NOEUD = 'NO3',
          CRITERE = 'RELATIF', PRECISION = 1.E-5,
