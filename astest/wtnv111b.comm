                  RESULTAT=MESTAT,
                  NOM_CHAM='DEPL',
                  NOEUD='N1',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.0,

                  CRITERE='ABSOLU',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=MESTAT,
                  NOM_CHAM='DEPL',
                  NOEUD='N3',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.294091352,
                  VALE_REFE=0.29411799999999999,
                  REFERENCE='NON_DEFINI',
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

FIN()
