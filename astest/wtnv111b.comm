                                     ELAS=_F(
   E = 225.E6,
   NU = 0.,
   RHO = 2.E3,
   ALPHA = 8.E-6)
   )

CHMAT0=AFFE_MATERIAU(   MAILLAGE=MAIL,
                          AFFE=_F( TOUT = 'OUI',
                        MATER = MATERIAU))

CHAR0=AFFE_CHAR_MECA(   MODELE=MODELE,DDL_IMPO=(
                          _F(  TOUT = 'OUI',
                                     DX = 0.,
                                     DY = 0.),
                          _F(NOEUD=('N1','N2','N3','N4'),
                                    PRE1=0.),
                          _F(NOEUD=('N1','N2'),
                                     TEMP = 0.)),
                        FLUX_THM_REP=_F(  GROUP_MA = 'HAUT',
                                   FLUN = 0.5,
                                   FLUN_HYDR1 = 0.)
                  )

MESTAT=STAT_NON_LINE(   MODELE=MODELE,
                SOLVEUR=_F( METHODE = 'MUMPS'),
                          CHAM_MATER=CHMAT0,
                          EXCIT=_F( CHARGE = CHAR0),
                          COMPORTEMENT=_F( RELATION = 'KIT_THM',PARM_THETA=1.,
                          RELATION_KIT = ('ELAS', 'LIQU_SATU',  'HYDR_UTIL'),
