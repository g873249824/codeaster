#
#       CAS_TEST SSNV209C: 2D, ELEMENTS Q4, METHODE XFEM

# SURFACE RIGIDE, MODELE1_63 PEUT ETRE IGNOREE
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),IGNORE_ALARM='MODELE1_63',DEBUG=_F(SDVERI='OUI'))

#......................................................
#  CALCUL TEST ELEMENTS FINIS XFEM ET CONTACT 2D QUA4
#  PLAQUE EN CONTACT FROTTANT SUR BATI RIGIDE
#  AVEC XFEM
#......................................................
#
# La fissure passe au milieu des mailles, le decoupage
# est active.
#
#----------------------------------------------
#                MAILLAGE
#----------------------------------------------

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'));

y0=0.00102564

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO= (_F(GROUP_MA='SURF'),
                                   # Set des neouds du haut 
                                    _F(OPTION='PLAN',
                                       NOM='BATIH',
                                       POINT=(0.04,y0),
                                       VECT_NORMALE=(0.,1.),
                                       PRECISION=1.E-4),                                       
                                   ),  
                    CREA_GROUP_MA= (
                                   # Définition du bâti
                                    _F(OPTION='BANDE',
                                       NOM='BATI',
                                       GROUP_NO_CENTRE='P1',
                                       VECT_NORMALE= (0.,1.),
                                       DIST=0.0073)),
                    INFO=1);

MAILLAG2= COPIER(CONCEPT= MAILLAG1)

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(NOM='NFISSU',
                                      OPTION='PLAN',
                                      POINT=(0.,0.),
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=5E-4)),
                    INFO=1);

MAILLAG2=MODI_MAILLAGE(reuse=MAILLAG2,
                       MAILLAGE=MAILLAG2,
                       ORIE_PEAU_2D=(_F(GROUP_MA = 'LIG2'),
                                     _F(GROUP_MA = 'LIG3'),
                                     _F(GROUP_MA = 'LIG4')));

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('LIG3'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('LIG1'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('SURF'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('LIG2'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('LIG4'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN')),
                     INFO=1);

# FORME ANALYTIQUE POUR LA FISSURE

LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y');

FISS=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN),
                    INFO=1);

# MODELE XFEM

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='STANDARD',
                         INFO=1);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=1.3E11
nu=0.2
rho=7800.

#
# CE BLOC SERT A VALIDER LA VARIABLE DE COMMANDE ESPACE
# ON DEFINIT E COMME FONCTION DE L'ESPACE
#
CHGEOM=CREA_CHAMP(TYPE_CHAM='NOEU_GEOM_R',
                  OPERATION='EXTR',
                  MAILLAGE=MAILLAG2,
                  NOM_CHAM='GEOMETRIE');

MATVAR=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho));

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('SURF'),
                                MATER=MATVAR)),
                       AFFE_VARC=_F(NOM_VARC='GEOM',
                                    CHAM_GD=CHGEOM));

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------


CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      FROTTEMENT     = 'COULOMB',
                      ITER_CONT_MAXI = 4,
                      ITER_FROT_MAXI = 20,
                      REAC_GEOM      = 'SANS',
                      ZONE=(
                            _F(
                               FISS_MAIT      = FISS,
                               INTEGRATION    = 'SIMPSON',
                               CONTACT_INIT   = 'OUI',
                               COULOMB        = 1.0,
                               ALGO_CONT      = 'STANDARD',
                               COEF_CONT      = 100.,
                               ALGO_FROT      = 'STANDARD',
                               COEF_FROT      = 100.,
                             ),
                         ),
                   );

H_plus = -2.

CHCINE=AFFE_CHAR_MECA(MODELE=MODELEK,
#                 Blockage du bâti
                  DDL_IMPO=(
                            _F(GROUP_MA=('BATI'),
                                DX=0.,
                                DY=0.),),
#                 Condition de part et d'autre de la lèvre
                   LIAISON_GROUP=(
                                 # --- DX ---
                                 _F(GROUP_NO_1 = ('BATIH'),
                                    GROUP_NO_2 = ('BATIH'),
                                    DDL_1      = ('DX', 'H1X'),
                                    DDL_2      = ('DX', 'H1X'),
                                    COEF_MULT_1= (0., 0.),
                                    COEF_MULT_2= (1., H_plus),
                                    COEF_IMPO= 0.),
                                 # --- DY ---
                                 _F(GROUP_NO_1 = ('BATIH'),
                                    GROUP_NO_2 = ('BATIH'),
                                    DDL_1      = ('DY', 'H1Y'),
                                    DDL_2      = ('DY', 'H1Y'),
                                    COEF_MULT_1= (0., 0.),
                                    COEF_MULT_2= (1., H_plus),
                                    COEF_IMPO= 0.),
                                ),
                  );

CHMECA=AFFE_CHAR_MECA(MODELE=MODELEK,
#                     FORCE IMPOSEE EN A
                      PRES_REP=(_F(GROUP_MA = 'LIG3',
                                   PRES = 5.E7)));

def pr(y) :
   if y  < 1e-15:  return 0.E07
   if y  > 1e-15:  return 15.E7
   if y == 1e-15:  return 0.

FCTPRES = FORMULE(VALE='pr(Y)',
                  NOM_PARA=('X','Y'));

CHAF=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                      PRES_REP=_F(GROUP_MA = ('LIG2','LIG4'),
                                  PRES=FCTPRES));

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0));

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=1.0,NOMBRE=1));


# LES SYSTEMES LINEAIRES SONT DIFFICILES A RESOUDRE.
# ON DEVERROUILLE LA DETECTION DE SINGULARITE. LE PROCESSUS DE NEWTON
# CORRIGE LES EVENTUELLES DERIVES.
UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA1,
                    EXCIT=(
                           _F(CHARGE=CHCINE),
                           _F(CHARGE=CHMECA,
                              FONC_MULT=RAMPE),
                           _F(CHARGE=CHAF,
                              FONC_MULT=RAMPE),
                           ),
                    CONTACT  = CTXFEM,
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',
                                 ),
                    INCREMENT=_F(LIST_INST=L_INS1,
                                 INST_FIN=1.0,
                                 ),
                    CONVERGENCE=_F(ARRET='OUI',
                                   ITER_GLOB_MAXI=40,
                                   RESI_GLOB_RELA=1.E-6,
                                   ),
                    SOLVEUR=_F(METHODE='MUMPS',

                               NPREC=-1.,
                               ),
                    NEWTON=_F(REAC_ITER=1),INFO=2,
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                    );

#-----------------------------------------------------------
#             POST-TRAITEMENT POUR LA VISUALISATION
#-----------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK)

MA_XFEM=DEFI_GROUP(reuse=MA_XFEM,
                   MAILLAGE=MA_XFEM,
                   DETR_GROUP_NO=_F(NOM='NFISSU'),
                   CREA_GROUP_NO=(_F(OPTION='ENV_SPHERE',
                                     NOM='PPA',
                                     POINT=(0.,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPB',
                                     POINT=(0.00125,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPC',
                                     POINT=(0.005,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPD',
                                     POINT=(0.0075,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPE',
                                     POINT=(0.01125,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(NOM='NFISSU',
                                     OPTION='PLAN',
                                     POINT=(0.,0.),
                                     VECT_NORMALE=(0.,1.),
                                     PRECISION=1.E-6)),
                   INFO=1);

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='D_PLAN'))

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        )

#----------------------------------------------
#                   POST RELEVE
#----------------------------------------------

DEP_NFI=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                     TABLE = DEP_NFI,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]')));

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                     TABLE = DEP_NFI,
                     ACTION=_F(OPERATION='TRI',
                               NOM_PARA='COOR_X',
                               ORDRE='CROISSANT'));

IMPR_TABLE(TABLE=DEP_NFI);

DEP_PPA=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPA'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));
DEP_PPA = CALC_TABLE(reuse = DEP_PPA,
                     TABLE = DEP_PPA,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]')));

DEP_PPB=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPB'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));
DEP_PPB = CALC_TABLE(reuse = DEP_PPB,
                     TABLE = DEP_PPB,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]')));

DEP_PPC=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPC'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));
DEP_PPC = CALC_TABLE(reuse = DEP_PPC,
                     TABLE = DEP_PPC,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]')));

DEP_PPD=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPD'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));
DEP_PPD = CALC_TABLE(reuse = DEP_PPD,
                     TABLE = DEP_PPD,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]')));

DEP_PPE=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPE'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));
DEP_PPE = CALC_TABLE(reuse = DEP_PPE,
                     TABLE = DEP_PPE,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]')));

#----------------------------------------------
#                   TEST TABLE
#----------------------------------------------

preci=1.e-2;


TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=1.E-2,
           VALE_CALC=2.85699280545E-05,
           VALE_REFE=2.84594384304E-05,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           TABLE=DEP_PPA,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=1.E-2,
           VALE_CALC=2.7194625078E-05,
           VALE_REFE=2.70792364103E-05,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           TABLE=DEP_PPB,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=1.E-2,
           VALE_CALC=2.28391436994E-05,
           VALE_REFE=2.27402555462E-05,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           TABLE=DEP_PPC,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=1.E-2,
           VALE_CALC=1.97868622915E-05,
           VALE_REFE=1.97270669318E-05,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           TABLE=DEP_PPD,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=1.E-2,
           VALE_CALC=1.53993958873E-05,
           VALE_REFE=1.53641594362E-05,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           TABLE=DEP_PPE,)

FIN();
