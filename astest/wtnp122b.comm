#    LINEAIRE SOUMIS A UN CHOC DE PRESSION EN 1D


DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))


#=============================================================
#=============================================================
METHODE='EF'
#=============================================================
#=============================================================

MODELISA = 'D_PLAN_HH2S'


#=============================================================
#                     donnees
#=============================================================

PARM_ALPHA=1.
MOLVP =0.0001

#*********************************************************
# CI/CL
#*********************************************************

PCREF=0.
PGREF=1.E4
PVPREF=0.001
PG0=10000.
PC0=0.

PRE2G0=0.

#*********************************************************
# PAS DE TEMPS
#*********************************************************
#
TFINAL=100.
NBDECOUP=100

#*********************************************************
# CRITERES DE CONVERGENCE
#*********************************************************
#

RESI_GLOB_MAXI = 1.E-12

ITER_GLOB_MAXI = 5




UN=DEFI_CONSTANTE(VALE=1.0,);
ZERO=DEFI_CONSTANTE(VALE=0.0,);

HENRY=DEFI_CONSTANTE(VALE=10000000000.0,);


VISCOLIQ=DEFI_CONSTANTE(VALE=1.,);
VISCOGAZ=DEFI_CONSTANTE(VALE=1.,);

DVISCOL=DEFI_CONSTANTE(VALE=0.0,);
DVISCOG=DEFI_CONSTANTE(VALE=0.0,);


VALKINT=1.E-3 /PGREF
KINT=DEFI_CONSTANTE(VALE=VALKINT,);
#
#
#*************************************************************
# Liste des instants de calculs
#*************************************************************
#

INST1=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=TFINAL,
                                    NOMBRE=NBDECOUP,),),
                     );

LI =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =INST1 ,),
                                ECHEC=_F(EVENEMENT = 'ERREUR',
                                         ACTION    = 'ARRET',),)



#*********************
# Lecture du maillage
#*********************

MAIL=LIRE_MAILLAGE(FORMAT='MED',);
MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(
                _F(GROUP_MA='MHAUT',),
                _F(GROUP_MA='MBAS',),
                _F(GROUP_MA='MDROIT',),
                _F(GROUP_MA='MGAUCHE',),
                   ),);


#*********************************************************
# Modele de calcul
#*********************************************************
#
MODELT=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION=MODELISA,),
                  );



#*********************************************************
# Definition des caracteristiques fluides
#**********************************************************


MATERIAU=DEFI_MATERIAU(

         THM_LIQU=_F(
               RHO = 1000.0,
                VISC = VISCOLIQ,
       UN_SUR_K=0.,
          D_VISC_TEMP = DVISCOL,),

         COMP_THM = 'LIQU_AD_GAZ_VAPE',

         THM_AIR_DISS=_F(
                   CP=0.,
                   COEF_HENRY=HENRY,
                   ),

         THM_VAPE_GAZ    =_F(
                   MASS_MOL   = MOLVP,
                   CP         = 1870.,
                   VISC       = VISCOGAZ,
                   D_VISC_TEMP= DVISCOG,
                       ),

         THM_GAZ=_F(
                  MASS_MOL    = 0.002,
       CP=1000.0,
                  VISC        = VISCOGAZ,
                  D_VISC_TEMP = DVISCOG,
             ),

         THM_DIFFU=_F(
            R_GAZ      = 8.315,
            RHO=2200.0,
            BIOT_L  = 1.,
            BIOT_T  = 1.,
            BIOT_N  = 1.,
#*****SATURATION******
            SATU_PRES  = ZERO,
            D_SATU_PRES= ZERO,
#*****GRAVITE******
            PESA_X     = 0.0,
            PESA_Y     = 0.,
            PESA_Z     = 0.,
#*****TENSEUR******
            PERMIN_L   = KINT,
            PERMIN_T   = ZERO,
            PERM_LIQU  = UN,
            D_PERM_LIQU_SATU=ZERO,
            PERM_GAZ   = UN,
            D_PERM_SATU_GAZ=ZERO,
            D_PERM_PRES_GAZ=ZERO,
#*****FICK******
            FICKV_T    = ZERO,
            FICKA_T    = ZERO,
            LAMB_TL    = ZERO,
            LAMB_TT    = ZERO,
              ),

        THM_INIT=_F(
         TEMP = 303.,
         PRE1 = PCREF,
         PRE2 = PGREF,
         PORO = 1.,
         PRES_VAPE=PVPREF,
                 ),
   );
#****************************************************************
# Affectation des materiaux
#****************************************************************
#

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='TOUT',
                             MATER=MATERIAU,),),);

#*************************************************************
# Affectation de l etat initial
#*************************************************************
#

#***********************
#ESSAI COMPARAISON HDI
#***********************
PINIT=CREA_CHAMP(MAILLAGE=MAIL,
                    OPERATION='AFFE',
                    TYPE_CHAM='NOEU_DEPL_R',
                    AFFE=(
                          _F( GROUP_MA = 'TOUT'  ,
                                     NOM_CMP  = 'PRE1' ,
                              VALE = PC0 , ),
                         _F( GROUP_MA = 'TOUT'  ,
                                 NOM_CMP  = 'PRE2' ,
                               VALE = PG0 , ),
                         ),
                );

#*************************************************************
# Affectation des CL
#*************************************************************
#


CHIMP=AFFE_CHAR_CINE(MODELE=MODELT,
       MECA_IMPO=(
                 _F(TOUT='OUI',PRE1=0.,),
                 _F(GROUP_MA = 'MGAUCHE',PRE2=PRE2G0,),
                 ),

       );



#
#
#*************************************************************
#     STAT NON LINE
#*************************************************************
#
cal1=STAT_NON_LINE(MODELE=MODELT,
                 CHAM_MATER=CHMAT,

                 EXCIT=( _F(CHARGE=CHIMP,),),

                 COMPORTEMENT=_F(RELATION='KIT_HH',
                              RELATION_KIT=('LIQU_AD_GAZ_VAPE','HYDR_UTIL'),
                 ),

                 ETAT_INIT  = _F ( DEPL = PINIT  ,
                                  ),
                INCREMENT=_F(
                   LIST_INST = LI,
                     INST_FIN=TFINAL,
                ),
               SOLVEUR=_F(METHODE='MULT_FRONT',
                            STOP_SINGULIER='NON',),

                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),

                 CONVERGENCE=_F(
                                 RESI_GLOB_MAXI = RESI_GLOB_MAXI,
                                 ITER_GLOB_MAXI = ITER_GLOB_MAXI,),

                 );


#
#*************************************************************
# Impressions
#*************************************************************

L_INST=DEFI_LIST_REEL(VALE=(1.,10.,100.,),);


PR1_BAS=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
        GROUP_NO=('MBAS'),
        RESULTAT=cal1,
        NOM_CHAM='DEPL',
        LIST_INST=L_INST,
        NOM_CMP=('PRE1'),
        OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=PR1_BAS,
      PAGINATION = 'INST' ,
      NOM_PARA=('INST','COOR_X','PRE1'),
      TRI   = (_F( NOM_PARA = ('COOR_X' ,),
      ORDRE    = 'CROISSANT',),),);

PR2_BAS=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
        GROUP_NO=('MBAS'),
        RESULTAT=cal1,
        NOM_CHAM='DEPL',
        LIST_INST=L_INST,
        NOM_CMP=('PRE2'),
        OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=PR2_BAS,
      PAGINATION = 'INST' ,
      NOM_PARA=('INST','COOR_X','PRE2'),
      TRI   = (_F( NOM_PARA = ('COOR_X' ,),
      ORDRE    = 'CROISSANT',),),);

# NOEUD DU BAS
TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='ANALYTIQUE',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='N104',
                NOM_CMP='PRE2',
                VALE_CALC=992.859156677,
                VALE_REFE=889.31760999999995,
                CRITERE='RELATIF',
                PRECISION=0.12,),
        )

# NOEUD DU HAUT
TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='ANALYTIQUE',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='N103',
                NOM_CMP='PRE2',
                VALE_CALC=992.859156678,
                VALE_REFE=889.31760999999995,
                CRITERE='RELATIF',
                PRECISION=0.12,),
        )

# NON REGRESSION
# NOEUD DU BAS
TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='NON_DEFINI',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='N104',
                NOM_CMP='PRE2',
                VALE_CALC=992.859156677,
                VALE_REFE=992.85915667740005,
                CRITERE='RELATIF',
                PRECISION=1.E-2,),
        )

# NOEUD DU HAUT
TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='NON_DEFINI',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='N103',
                NOM_CMP='PRE2',
                VALE_CALC=992.859156678,
                VALE_REFE=992.85915667759002,
                CRITERE='RELATIF',
                PRECISION=1.E-2,),
        )


FIN();
