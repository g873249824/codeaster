                      CHAM_GD=FORCE,
                      VALE_CALC=-5.60034579E-03,
                      VALE_REFE=-5.6008400000000002E-3,),
                   ),
          )

# =====================================================================
#                   C A L C U L    H A R M O N I Q U E
