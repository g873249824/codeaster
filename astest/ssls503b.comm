#---------------------------------------------------------------------
#  - CALCUL QUASI-STATIQUE D'UNE PLAQUE SOUMISE A UNE PRESSION
#    UNIFORMEMENT REPARTIE
#  - MODELISATION AVEC DES ELEMENTS 'DKT'(QUAD4)
#---------------------------------------------------------------------
#
DEBUT( CODE=_F( NOM = 'SSLS503B',NIV_PUB_WEB='INTERNET'))

#
#PRE_IDEAS ();
#
MA=LIRE_MAILLAGE(FORMAT='MED', )

#
MO=AFFE_MODELE(   MAILLAGE=MA,
                        AFFE=_F( TOUT = 'OUI',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'DKT')
                  )

#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAR_ELE=AFFE_CARA_ELEM(   MODELE=MO,
                             COQUE=_F( GROUP_MA = 'TOUT_ELT',
                                    EPAIS = 0.012,
                                    COQUE_NCOU=3,
                                    ANGL_REP = (0.,-48.5,))
                          )

#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT=DEFI_MATERIAU(   ELAS_ORTH=_F( E_L = 4.E+10,
                                  E_T = 0.16E+10,
                                  G_LT = 8.E+08,
                                  G_TN = 3.2E+08,
                                  NU_LT = 0.25)
                     )

#
MULTI=DEFI_COMPOSITE( COUCHE=(  _F( EPAIS = 0.004,
                                  MATER = MAT,
                                  ORIENTATION = 0.),
                          _F( EPAIS = 0.004,
                                  MATER = MAT,
                                  ORIENTATION = 90.),
                          _F( EPAIS = 0.004,
                                  MATER = MAT,
                                  ORIENTATION = 0.))
                        )

#
CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MA,
                                AFFE=_F( TOUT = 'OUI',
                                      MATER = MULTI)
                          )

#
#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------
#
CHA=AFFE_CHAR_MECA(        MODELE=MO,DDL_IMPO=(
                           _F( GROUP_NO = 'COTEBC',
                                          DY = 0.),
                           _F( GROUP_NO = 'COTECD',
                                          DY = 0.)),LIAISON_OBLIQUE=(
                    _F( GROUP_NO = 'COTEAD',
                                          DY = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,)),
                    _F( GROUP_NO = 'COTEAD',
                                          DRX = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,)),
                    _F( GROUP_NO = 'COTEAB',
                                          DX = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,)),
                    _F( GROUP_NO = 'COTEAB',
                                          DRY = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,))),
                        FORCE_COQUE=_F( TOUT = 'OUI',
                                     PRES = -3000.)
                      )

#
#---------------------------------------------------------------------
#                         RESOLUTION
#---------------------------------------------------------------------
#
RESU=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHAM_MAT,
                   CARA_ELEM=CAR_ELE,
                   EXCIT=_F( CHARGE = CHA));

RESU=CALC_CHAMP(reuse =RESU,
               RESULTAT=RESU,
               NUME_ORDRE=1,
               CONTRAINTE=('SIGM_ELNO',),
               DEFORMATION=('EPSI_ELNO',),);

#
#COUCHE 2 : CALCUL DES CONTRAINTES
##################################
SIG_C2_S=POST_CHAMP(RESULTAT=RESU,
                    EXTR_COQUE=_F(NOM_CHAM=('SIGM_ELNO','EPSI_ELNO'),
                                  NUME_COUCHE=2,
                                  NIVE_COUCHE='SUP',),);
#
SIG_C2_M=POST_CHAMP(RESULTAT=RESU,
                    EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                  NUME_COUCHE=2,
                                  NIVE_COUCHE='MOY',),);
#
#COUCHE 3 : CALCUL DES CONTRAINTES
##################################
SIG_C3_S=POST_CHAMP(RESULTAT=RESU,
                    EXTR_COQUE=_F(NOM_CHAM=('SIGM_ELNO','EPSI_ELNO',),
                                  NUME_COUCHE=3,
                                  NIVE_COUCHE='SUP',),);
#
#
#---------------------------------------------------------------------
#                         VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='A',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=0.015004998,
                  VALE_REFE=0.01507,
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=SIG_C3_S,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXX',
                   VALE_CALC= 2.43965302E+07,
                   VALE_REFE=2.4216000E7,
                   PRECISION=1.E-2,
                   MAILLE='MA6',),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A',
