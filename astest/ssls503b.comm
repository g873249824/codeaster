                   NOM_CMP='SIXY',
                   VALE_CALC= 1.23356761E+06,
                   VALE_REFE=1.282500E6,
                   PRECISION=0.050000000000000003,
                   MAILLE='MA31',),
                _F(NUME_ORDRE=1,
                   GROUP_NO='D',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=SIG_C2_M,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXZ',
                   VALE_CALC=-2.01128585E+05,
                   VALE_REFE=-2.35250E5,
                   PRECISION=0.14999999999999999,
                   MAILLE='MA36',),
                _F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=SIG_C2_M,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIYZ',
                   VALE_CALC= 8.60603313E+04,
                   VALE_REFE=8.8950E4,
                   PRECISION=0.040000000000000001,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   REFERENCE='NON_DEFINI',
                   RESULTAT=SIG_C2_S,
                   NOM_CHAM='EPSI_ELNO',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.56930640E-08,
                   VALE_REFE=1.5718400000000001E-08,
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   REFERENCE='NON_DEFINI',
                   RESULTAT=SIG_C2_S,
                   NOM_CHAM='EPSI_ELNO',
                   NOM_CMP='EPYY',
                   VALE_CALC=-1.05857673E-07,
                   VALE_REFE=-1.0586999999999999E-07,
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   REFERENCE='NON_DEFINI',
                   RESULTAT=SIG_C3_S,
                   NOM_CHAM='EPSI_ELNO',
