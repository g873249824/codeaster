#


SIELNO=CREA_CHAMP(TYPE_CHAM='ELNO_SIEF_R',
                  OPERATION='EXTR',
                  RESULTAT=RESU,
                  NOM_CHAM='SIEF_ELNO_ELGA',
                  NUME_ORDRE=10,);

TEST_RESU(CHAM_ELEM=_F(CHAM_GD=SIELNO,
                       MAILLE='M31',
                       NOEUD='N291',
                       NOM_CMP='SIYY',
