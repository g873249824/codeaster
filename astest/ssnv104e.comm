                _F(RESULTAT=RESU,
                   NUME_ORDRE=10,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   NOEUD='N287',
                   VALE=-0.162911,
                   CRITERE='RELATIF',
                   PRECISION=0.02,),
                _F(RESULTAT=RESU,
                   NUME_ORDRE=10,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='N285',
                   VALE=-0.16594600000000001,
                   CRITERE='RELATIF',
                   PRECISION=0.059999999999999998,),
                _F(RESULTAT=RESU,
                   NUME_ORDRE=10,
                   NOM_CHAM='DEPL',
