DEBUT(CODE=_F(NOM='SDLX02C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAILLAGE=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED', )

MODELE=AFFE_MODELE(  MAILLAGE=MAILLAGE,AFFE=(
                  _F(  GROUP_MA = 'POU_D_T',  PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'POU_D_E'),
                       _F(  GROUP_MA = 'POU_C_T',  PHENOMENE = 'MECANIQUE',
                         MODELISATION = 'POU_C_T'))                  )

#

MAT=DEFI_MATERIAU( ELAS=_F(  E = 1.658E+11,  RHO = 1.3404106E+04,
                                   NU = 0.3,  ALPHA = 1.288E-05)    )

CHAMPMAT=AFFE_MATERIAU(  MAILLAGE=MAILLAGE,
                            AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

COND_LIM=AFFE_CHAR_MECA(  MODELE=MODELE,
                         DDL_IMPO=_F(  GROUP_NO = ('P100', 'P1500',),
                                    DX = 0.,  DY = 0.,  DZ = 0.,
                                    DRX = 0.,  DRY = 0.,  DRZ = 0.)   )

#

CARA_ELE=AFFE_CARA_ELEM(  MODELE=MODELE,POUTRE=(
                  _F(  GROUP_MA = 'POU_D_T',   SECTION = 'GENERALE',
                           CARA = ( 'A',  'IZ', 'IY', 'AY', 'AZ', 'JX',
                                  'EZ', 'EY', 'RY', 'RZ', 'RT',      ),
                           VALE = ( 3.4390E-03,  1.3770E-05,  1.3770E-05,
                                  2.0,  2.0,  2.7540E-05,  0.0,  0.0,
                                  1.0,  1.0,  1.0,        )),
                         _F(  GROUP_MA = 'POU_C_T',   SECTION = 'GENERALE',
                           CARA = ( 'A',  'IZ', 'IY', 'AY', 'AZ', 'JX',
                                  'EZ', 'EY', 'RY', 'RZ', 'RT',      ),
                           VALE = ( 3.4390E-03, 5.8870E-06, 5.8870E-06,
                                  2.0,  2.0,  2.7540E-05,  0.0,  0.0,
                                  1.0,  1.0,  1.0,        ))),DEFI_ARC=(
             _F(  GROUP_MA = 'COUDE1', CENTRE = (0., -1.828, -0.922,)),
             _F(  GROUP_MA = 'COUDE2', CENTRE = (0.922, -0.922, 0., )))
                                        )

MACRO_MATR_ASSE(MODELE=MODELE,  CHARGE=COND_LIM,
              CARA_ELEM=CARA_ELE, CHAM_MATER=CHAMPMAT, NUME_DDL=CO("NUMDDL"),MATR_ASSE=(
                _F( MATRICE = CO("RIGIDITE"), OPTION = 'RIGI_MECA'),
                          _F( MATRICE = CO("MASSE"),    OPTION = 'MASS_MECA')))

MODE=MODE_ITER_SIMULT(  MATR_RIGI=RIGIDITE,   MATR_MASS=MASSE,
                    METHODE='TRI_DIAG',
                  CALC_FREQ=_F(  NMAX_FREQ = 8) )


TEST_RESU(RESU=(
       _F( RESULTAT = MODE,      NUME_MODE = 1,  PRECISION = 0.001,
                             PARA = 'FREQ',  VALE = 10.39),
       _F( RESULTAT = MODE,      NUME_MODE = 2,  PRECISION = 0.004,
                             PARA = 'FREQ',  VALE = 20.02),
       _F( RESULTAT = MODE,      NUME_MODE = 3,  PRECISION = 0.002,
                             PARA = 'FREQ',  VALE = 25.45),
       _F( RESULTAT = MODE,      NUME_MODE = 4,  PRECISION = 0.004,
                             PARA = 'FREQ',  VALE = 48.32),
       _F( RESULTAT = MODE,      NUME_MODE = 5,  PRECISION = 0.004,
                             PARA = 'FREQ',  VALE = 52.60),
       _F( RESULTAT = MODE,      NUME_MODE = 6,  PRECISION = 0.009,
                             PARA = 'FREQ',  VALE = 84.81),
       _F( RESULTAT = MODE,      NUME_MODE = 7,  PRECISION = 0.009,
                             PARA = 'FREQ',  VALE = 87.16),
       _F( RESULTAT = MODE,      NUME_MODE = 8,  PRECISION = 0.003,
                             PARA = 'FREQ',  VALE = 129.31)) )

MACRO_MATR_ASSE(MODELE=MODELE,  CHARGE=COND_LIM,
              CARA_ELEM=CARA_ELE, CHAM_MATER=CHAMPMAT, NUME_DDL=NUMDDL,MATR_ASSE=(
                _F( MATRICE = CO("RIGIDITD"), OPTION = 'RIGI_MECA'),
                         _F( MATRICE = CO("MASSED"),    OPTION = 'MASS_MECA_DIAG')))

MODE_DIA=MODE_ITER_SIMULT(  MATR_RIGI=RIGIDITD,   MATR_MASS=MASSED,
                    METHODE='TRI_DIAG',
                  CALC_FREQ=_F(  NMAX_FREQ = 8) )


TEST_RESU(RESU=(
       _F( RESULTAT = MODE_DIA, NUME_MODE = 1,  PRECISION = 0.001,
                            PARA = 'FREQ',  VALE = 10.39),
       _F( RESULTAT = MODE_DIA, NUME_MODE = 2,  PRECISION = 0.005,
                            PARA = 'FREQ',  VALE = 20.02),
       _F( RESULTAT = MODE_DIA, NUME_MODE = 3,  PRECISION = 0.002,
                            PARA = 'FREQ',  VALE = 25.45),
       _F( RESULTAT = MODE_DIA, NUME_MODE = 4,  PRECISION = 0.002,
                            PARA = 'FREQ',  VALE = 48.32),
       _F( RESULTAT = MODE_DIA, NUME_MODE = 5,  PRECISION = 0.004,
                            PARA = 'FREQ',  VALE = 52.60),
       _F( RESULTAT = MODE_DIA, NUME_MODE = 6,  PRECISION = 0.012,
                            PARA = 'FREQ',  VALE = 84.81),
       _F( RESULTAT = MODE_DIA, NUME_MODE = 7,  PRECISION = 0.01,
                            PARA = 'FREQ',  VALE = 87.16),
       _F( RESULTAT = MODE_DIA, NUME_MODE = 8,  PRECISION = 0.007,
                             PARA = 'FREQ',  VALE = 129.31)) )

FIN( )
