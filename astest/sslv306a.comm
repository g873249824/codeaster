DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET') )

MAIL=LIRE_MAILLAGE(FORMAT='MED',)

#
# DEFINITION DES GROUPES DE NOEUDS
MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'ENCAST'),
                       _F(  GROUP_MA = 'FACE_TR'),
                       _F(  GROUP_MA = 'RESU_DEP'),
                       _F(  GROUP_MA = 'RESU_CON'))
                   )

#
# DEFINITION DU MODELE MECANIQUE
MODELE=AFFE_MODELE(    MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',
                         PHENOMENE = 'MECANIQUE',
                         MODELISATION = '3D')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 2.1E11,  NU = 0.3,
                                 ALPHA = 1.E-5,  RHO = 7800.)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(    MAILLAGE=MAIL,
                             AFFE=_F(  TOUT = 'OUI',
                                    MATER = ACIER)  )

#
F_Y=DEFI_FONCTION(    NOM_PARA='Y',
                        VALE=( 0.,           0.,
                               0.0999999,   -0.0714E-5,),
                        PROL_DROITE='LINEAIRE',
                       PROL_GAUCHE='LINEAIRE'
                    )

# DEFINITION DES DEPLACEMENTS IMPOSES
CHARGT1=AFFE_CHAR_MECA(    MODELE=MODELE,DDL_IMPO=(
                            _F(  GROUP_NO = 'ENCAST',
                                       DX = 0.,  DY = 0.,  DZ = 0.),
                            _F(  GROUP_NO = 'FACE_TR',
                                       DY = 0.952E-5))
                        )

CHARGT2=AFFE_CHAR_MECA_F(    MODELE=MODELE,
                              DDL_IMPO=_F(  GROUP_NO = 'FACE_TR',
                                         DX = F_Y)
                        )

# RESOLUTION
DEF=MECA_STATIQUE(    MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                        _F(  CHARGE = CHARGT1),
                        _F(  CHARGE = CHARGT2))  )


# CALCUL DES CONTRAINTES
DEF=CALC_CHAMP(reuse=DEF,RESULTAT=DEF,CONTRAINTE=('SIGM_ELNO'))


T1=POST_RELEVE_T(    ACTION=_F(  GROUP_NO = 'RESU_DEP',
                                INTITULE = 'DEP',
                                RESULTAT=DEF,
                                NUME_ORDRE=1,
                                NOM_CHAM='DEPL',
                                NOM_CMP = ('DX',  'DY',  'DZ',),
                                OPERATION = 'EXTRACTION')
                   )

#
T2=POST_RELEVE_T(    ACTION=_F(  GROUP_NO = 'RESU_CON',
                                INTITULE = 'SIG',
                                RESULTAT = DEF,
                                NUME_ORDRE=1,
                                NOM_CHAM='SIGM_ELNO',
                                NOM_CMP = ('SIXX',  'SIYY', 'SIZZ',),
                                OPERATION = 'EXTRACTION')
                   )

