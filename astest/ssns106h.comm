# COMPARAISON DU MODELE GLRC_DM AVEC ENDO_ISOT ET LABORD_1D
#
# DETAILS : MODELISATION : MULTI-COUCHES
#                          (DKT- ENDO_ISOT_BETON+2*GRILLE ACIER)
#                           .VS.
#                           DKTG (GLRC_DM)
#                           .VS.
#                           MULTI-FIBRES
#                           (LABORD_1D + 4 BARRES D'ACIER)
#            MAILLAGE     : TRIA3 ET POUT
#            CHARGEMENT   : CYCLIQUE TRACTION - COMPRESSION
#            REMARQUE 1   : LES PARAMETRES GLRC_DM ONT ETE IDENTIFIES
#                           PAR LE TEST SSNS106A

DEBUT(CODE=_F(NOM='SSNS106H',
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

# PARAMETRES
EP = 0.1;

DEP0 = 0.001;

t1 = 1.0;

t2 = (3 * t1);

t3 = (4 * t1);

dt = 0.05;

fx = 500000.0;

EGM = 35625000000.0;

EGF = 38660000000.0;

EB = 32308000000.0;


# -- MAILLAGE MULTI-COUCHES --

MAIL=LIRE_MAILLAGE();

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='A1A2',),
                               _F(GROUP_MA='A4A3',),
                               _F(GROUP_MA='A2A4',),
                               _F(GROUP_MA='A3A1',),),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='DALLE',
                                      VECT_NORM=(0.,0.,1.,),
                                      GROUP_NO='A1',),);

# MAILLAGE ENDO_ISOT

MA=CREA_MAILLAGE(MAILLAGE=MAIL,
                 CREA_GROUP_MA=(_F(NOM='ACIER2L',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A2L',),
                                _F(NOM='ACIER1T',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A1T',),
                                _F(NOM='ACIER2T',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A2T',),
                                _F(NOM='ACIER1L',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A1L',),
                                _F(NOM='DALLE_G',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='G',),),);

# -- MAILLAGE MULTI-FIBRES (LABORD_1D) --

POUT=LIRE_MAILLAGE(UNITE=17,FORMAT='MED',);

MASEC=LIRE_MAILLAGE(UNITE=21,
                    FORMAT='MED',);

GF=DEFI_GEOM_FIBRE(INFO=2,
                   SECTION=_F(GROUP_FIBRE='SECF',
                              TOUT_SECT='OUI',
                              MAILLAGE_SECT=MASEC,
                              COOR_AXE_POUTRE=(0.,0.,),),
                   FIBRE=_F(GROUP_FIBRE='SACI',
                            VALE=(0.,0.04,0.0008,0.,-0.04,0.0008,),
                            COOR_AXE_POUTRE=(0.,0.,),),);

# -- MODELE ENDO_ISOT --

MO=AFFE_MODELE(MAILLAGE=MA,
               INFO=1,
               AFFE=(_F(GROUP_MA='DALLE',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='DKT',),
                     _F(GROUP_MA='ACIER1L',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),
                     _F(GROUP_MA='ACIER1T',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),
                     _F(GROUP_MA='ACIER2L',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),
                     _F(GROUP_MA='ACIER2T',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),),);

# -- MODELE GLRC_DM --

MO_G=AFFE_MODELE(MAILLAGE=MA,
                 INFO=1,
                 AFFE=_F(GROUP_MA='DALLE_G',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKTG',),);

# -- MODELE LABORD_1D --

MO_L=AFFE_MODELE(MAILLAGE=POUT,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_EM',),);

# -- MATERIAUX --
# BETON ENDO_ISOT

BETON_E=DEFI_MATERIAU(ELAS=_F(E=EB,
                              NU=0.2,
                              RHO=2500.0,),
                      BETON_ECRO_LINE=_F(D_SIGM_EPSI=(-0.2 * EB),
                                         SYT=3.4E6,),);

# BETON LABORD_1D

BETON_L=DEFI_MATERIAU(ELAS=_F(E=3.2308000000E10,
                              NU=0.0,
                              RHO=2400.0,),
                      LABORD_1D=_F(Y01=341.,
                                   Y02=7075.,
                                   A1=0.01038,
                                   A2=.5999E-5,
                                   B1=1.2,
                                   B2=2.,
                                   BETA1=.0867E+7,
                                   BETA2=-.35E+8,
                                   SIGF=3.5E+6,),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.E11,
                            NU=0.2,
                            RHO=7800.0,
                            AMOR_ALPHA=3.7771128018011707E-05,
                            AMOR_BETA=2.6476993459625175,),
                    ECRO_LINE=_F(D_SIGM_EPSI=3.E8,
                                 SY=5.70000000E8,),
                    INFO=1,);

MAT_G=DEFI_MATERIAU(ELAS=_F(E=EGM,
                            NU=0.18,),
                    GLRC_DM=_F(GAMMA_T=0.02,
                               GAMMA_F=0.32,
                               NYT=3.7E5,
                               MYF=9.0E3,
                               EF=EGF,
                               NUF=0.17,),);

MATOR=DEFI_MATERIAU(ELAS=_F(E=2.e11,
                            NU=0.0,
                            RHO=7800.0,),);

# -- AFFECTATION DU MATERIAU POUR ENDO_ISOT --

MATER=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='DALLE_G',
                             MATER=MAT_G,),
                          _F(GROUP_MA='DALLE',
                             MATER=BETON_E,),
                          _F(GROUP_MA='ACIER1L',
                             MATER=ACIER,),
                          _F(GROUP_MA='ACIER1T',
                             MATER=ACIER,),
                          _F(GROUP_MA='ACIER2L',
                             MATER=ACIER,),
                          _F(GROUP_MA='ACIER2T',
                             MATER=ACIER,),),);
COEF_DRZ = 1e-10;


# -- AFFECTATION DU COMPORTEMENT ET DU MATERIAU POUR LABORD_1D --

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,
                  MATER_SECT=MATOR,
                  MULTIFIBRE=(_F(GROUP_FIBRE='SECF',
                                 MATER=BETON_L,
                                 RELATION='LABORD_1D',),
                              _F(GROUP_FIBRE='SACI',
                                 MATER=ACIER,
                                 RELATION='VMIS_CINE_LINE',),),);

CHMAT=AFFE_MATERIAU(MAILLAGE=POUT,
                    AFFE=_F(GROUP_MA='POUTRE',
                            MATER=(BETON_L,ACIER,MATOR,),),
                    AFFE_COMPOR=_F(GROUP_MA='POUTRE',
                                   COMPOR=COMPF,),);

# -- AFFECTATION DES CARACTERISTIQUES POUR ENDO_ISOT --

CAEL=AFFE_CARA_ELEM(MODELE=MO,
                    INFO=1,
                    COQUE=_F(GROUP_MA='DALLE',
                             EPAIS=EP,
                             ANGL_REP=(0.0,0.0,),
                             COQUE_NCOU=11,
                             INER_ROTA='OUI',
                             MODI_METRIQUE='NON',),
                    GRILLE=(_F(GROUP_MA='ACIER1L',
                               SECTION=8.0E-4,
                               ANGL_REP=(0.0,0.0,),
                               EXCENTREMENT=(-(EP) * 0.4),
                               COEF_RIGI_DRZ=COEF_DRZ,),
                            _F(GROUP_MA='ACIER1T',
                               SECTION=8.0E-4,
                               ANGL_REP=(90.0,0.0,),
                               EXCENTREMENT=(-(EP) * 0.4),
                               COEF_RIGI_DRZ=COEF_DRZ,),
                            _F(GROUP_MA='ACIER2L',
                               SECTION=8.0E-4,
                               ANGL_REP=(0.0,0.0,),
                               EXCENTREMENT=(EP * 0.4),
                               COEF_RIGI_DRZ=COEF_DRZ,),
                            _F(GROUP_MA='ACIER2T',
                               SECTION=8.0E-4,
                               ANGL_REP=(90.0,0.0,),
                               EXCENTREMENT=(EP * 0.4),
                               COEF_RIGI_DRZ=COEF_DRZ,),),);

# -- AFFECTATION DES CARACTERISTIQUES POUR GLRC_DM --

CAEL_G=AFFE_CARA_ELEM(MODELE=MO_G,
                      COQUE=_F(GROUP_MA='DALLE_G',
                               EPAIS=EP,
                               ANGL_REP=(0.0,0.0,),
                               INER_ROTA='OUI',),);

# -- AFFECTATION DES CARACTERISTIQUES POUR LABORD_1D --

CAEL_L=AFFE_CARA_ELEM(MODELE=MO_L,
                      INFO=1,
                      POUTRE=_F(GROUP_MA='POUTRE',
                                SECTION='RECTANGLE',
                                CARA=('HY','HZ',),
                                VALE=(1.0,0.1,),),
                      ORIENTATION=_F(GROUP_MA='POUTRE',
                                     CARA='ANGL_VRIL',
                                     VALE=0.0,),
                      GEOM_FIBRE=GF,
                      MULTIFIBRE=_F(GROUP_MA='POUTRE',
                                    GROUP_FIBRE=('SECF','SACI',),
                                PREC_AIRE=1.61E-02,
                                PREC_INERTIE=6.3E-02,),);

# -- CONDITIONS LIMITES POUR ENDO_ISOT --

COND_0=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=(_F(GROUP_NO='A1',
                                   DX=0.0,
                                   DY=0.0,
                                   DZ=0.0,
                                   DRX=0.0,
                                   DRY=0.0,
                                   DRZ=0.0,),
                                _F(GROUP_MA='A3A1',
                                   DX=0.0,
                                   DRY=0.0,),),);

# -- CONDITIONS LIMITES POUR GLRC_DM --

COND_0G=AFFE_CHAR_MECA(MODELE=MO_G,
                       DDL_IMPO=(_F(GROUP_NO='A1',
                                    DX=0.0,
                                    DY=0.0,
                                    DZ=0.0,
                                    DRX=0.0,
                                    DRY=0.0,
                                    DRZ=0.0,),
                                 _F(GROUP_MA='A3A1',
                                    DX=0.0,
                                    DRY=0.0,),),);

# -- CONDITIONS LIMITES POUR LABORD_1D --

COND_0L=AFFE_CHAR_MECA(MODELE=MO_L,
                       DDL_IMPO=_F(GROUP_NO='A',
                                   DX=0.,
                                   DY=0.,
                                   DZ=0.,
                                   DRX=0.,
                                   DRY=0.,
                                   DRZ=0.,),);

# -- CHARGEMENT IMPOSE POUR ENDO_ISOT --

COND_1=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=_F(GROUP_MA='A2A4',
                                  DX=DEP0,),);

# -- CHARGEMENT IMPOSE POUR GLRC_DM --

COND_1G=AFFE_CHAR_MECA(MODELE=MO_G,
                       DDL_IMPO=_F(GROUP_MA='A2A4',
                                   DX=DEP0,),);

# -- CHARGEMENT IMPOSE POUR LABORD_1D --

COND_1L=AFFE_CHAR_MECA(MODELE=MO_L,
                       DDL_IMPO=_F(GROUP_NO='B',
                                   DX=DEP0,),);

# -- FONCTION MULTIPLICATRICE POUR LE CHARGEMENT --

chf1=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                         t1,1.,
                         t2,-1.,
                         t3,0.0,
                         ),PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',);

inst1=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=t3,
                                   PAS=dt,),);

# ON DEFINIT UNE LISTE D INSTANTS AVEC UN PAS PLUS PETIT POUR PERMETTRE LE CALCUL AVEC ENDO_ISOT

inst2=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=t3,
                                   PAS=(dt / 2),),);

linst1=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                   LIST_INST=inst1,),
                      ECHEC=_F(ACTION        = 'DECOUPE',
                               SUBD_METHODE  = 'MANUEL',
                               SUBD_PAS=4,
                               SUBD_PAS_MINI=0.0001,),);

linst2=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                   LIST_INST=inst2,),
                      ECHEC=_F(ACTION        = 'DECOUPE',
                               SUBD_METHODE  = 'MANUEL',
                               SUBD_PAS=4,
                               SUBD_PAS_MINI=0.0001,),);

#-----------------------------------------------
# CALCUL DES RESULTATS POUR LE CHARGEMENT chf1
#-----------------------------------------------

RESU_R1=STAT_NON_LINE(MODELE=MO,
                      CHAM_MATER=MATER,
                      CARA_ELEM=CAEL,
                      EXCIT=(_F(CHARGE=COND_0,
                                FONC_MULT=chf1,),
                             _F(CHARGE=COND_1,
                                FONC_MULT=chf1,),),
                      COMP_INCR=(_F(RELATION='ENDO_ISOT_BETON',
                                    ITER_CPLAN_MAXI=5,
                                    GROUP_MA='DALLE',),
                                 _F(RELATION='GRILLE_ISOT_LINE',
                                    GROUP_MA=('ACIER1L','ACIER2L','ACIER1T','ACIER2T',),),),
                      INCREMENT=_F(LIST_INST=linst2,
                                   PRECISION=0.1,),
                      NEWTON=_F(REAC_ITER=1,),
                      CONVERGENCE=_F(ITER_GLOB_MAXI=26,),
                      ARCHIVAGE=_F(LIST_INST=inst2,),);


RESU_R1=CALC_CHAMP(reuse =RESU_R1,RESULTAT=RESU_R1,PRECISION=1.E-3,
             FORCE=('FORC_NODA',),)

RESU_G1=STAT_NON_LINE(MODELE=MO_G,
                      CHAM_MATER=MATER,
                      CARA_ELEM=CAEL_G,
                      EXCIT=(_F(CHARGE=COND_0G,
                                FONC_MULT=chf1,),
                             _F(CHARGE=COND_1G,
                                FONC_MULT=chf1,),),
                      COMP_INCR=_F(RELATION='GLRC_DM',
                                   ITER_INTE_MAXI=100,),
                      INCREMENT=_F(LIST_INST=linst2,
                                   PRECISION=0.1,),
                      NEWTON=_F(REAC_ITER=1,),
                      CONVERGENCE=_F(ITER_GLOB_MAXI=26,),
                      INFO=1,);

RESU_G1=CALC_CHAMP(reuse =RESU_G1,
                   RESULTAT=RESU_G1,PRECISION=1.E-3,
                   FORCE=('FORC_NODA',),
                   VARI_INTERNE=('VARI_NOEU'),);


RESU_L1=STAT_NON_LINE(MODELE=MO_L,
                      CHAM_MATER=CHMAT,
                      CARA_ELEM=CAEL_L,
                      EXCIT=(_F(CHARGE=COND_0L,
                                FONC_MULT=chf1,),
                             _F(CHARGE=COND_1L,
                                FONC_MULT=chf1,),),
                      COMP_INCR=_F(RELATION='MULTIFIBRE',),
                      INCREMENT=_F(LIST_INST=linst2,),
                      NEWTON=_F(MATRICE='TANGENTE',
                                REAC_ITER=1,),
                      CONVERGENCE=_F(RESI_GLOB_RELA=1.E-5,),);

RESU_L1=CALC_CHAMP(reuse =RESU_L1,
                  RESULTAT=RESU_L1,
                   FORCE=('REAC_NODA',),
                   DEFORMATION=('DEGE_ELNO'),);

# -- POST TRAITEMENT --

EPS=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                            INTITULE='TABEPS',
                            RESULTAT=RESU_L1,
                            NOM_CHAM='DEGE_ELNO',
                            NOEUD='N2',
                            TOUT_CMP='OUI',),),);


EPS=CALC_TABLE(reuse =EPS,
               TABLE=EPS,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TFXA4_R1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                 INTITULE='A4FX',
                                 RESULTAT=RESU_R1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A4',
                                 NOM_CMP='DX',),
                                ),

                                 );

TFXA4_R1=CALC_TABLE(reuse =TFXA4_R1,
               TABLE=TFXA4_R1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);


TFXA4_G1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                 INTITULE='A4FX',
                                 RESULTAT=RESU_G1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A4',
                                 NOM_CMP='DX',),),);

TFXA4_G1=CALC_TABLE(reuse =TFXA4_G1,
               TABLE=TFXA4_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TFXA2_R1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                 INTITULE='A2FX',
                                 RESULTAT=RESU_R1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A2',
                                 NOM_CMP='DX',),),);

TFXA2_R1=CALC_TABLE(reuse =TFXA2_R1,
               TABLE=TFXA2_R1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TFXA2_G1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                 INTITULE='A2FX',
                                 RESULTAT=RESU_G1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A2',
                                 NOM_CMP='DX',),),);


TFXA2_G1=CALC_TABLE(reuse =TFXA2_G1,
               TABLE=TFXA2_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);
TFXA6_R1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                 INTITULE='A6FX',
                                 RESULTAT=RESU_R1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A6',
                                 NOM_CMP='DX',),),);

TFXA6_R1=CALC_TABLE(reuse =TFXA6_R1,
               TABLE=TFXA6_R1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TFXA6_G1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                 INTITULE='A6FX',
                                 RESULTAT=RESU_G1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A6',
                                 NOM_CMP='DX',),),);


TFXA6_G1=CALC_TABLE(reuse =TFXA6_G1,
               TABLE=TFXA6_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TD_G1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                              INTITULE='A4FX',
                              RESULTAT=RESU_G1,
                              NOM_CHAM='VARI_NOEU',
                              GROUP_NO='A4',
                              NOM_CMP=('V1','V2',),),),);


TD_G1=CALC_TABLE(reuse =TD_G1,
               TABLE=TD_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TFX_L1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                               INTITULE='FXL',
                               RESULTAT=RESU_L1,
                               NOM_CHAM='REAC_NODA',
                               NOEUD='N2',
                               NOM_CMP='DX',),),);


TFX_L1=CALC_TABLE(reuse =TFX_L1,
               TABLE=TFX_L1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

EPS=CALC_TABLE(reuse =EPS,
               TABLE=EPS,
               ACTION=_F(OPERATION='EXTR',
                         NOM_PARA=('INST','EPXX',),),);

TFX_L1=CALC_TABLE(reuse =TFX_L1,
                  TABLE=TFX_L1,
                  ACTION=(_F(OPERATION='EXTR',
                             NOM_PARA=('INST','DX',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('DX','FXL',),),),);

TD_G1=CALC_TABLE(reuse =TD_G1,
                 TABLE=TD_G1,
                 ACTION=(_F(OPERATION='EXTR',
                            NOM_PARA=('INST','V1','V2',),),
                         _F(OPERATION='RENOMME',
                            NOM_PARA=('V1','D1',),),
                         _F(OPERATION='RENOMME',
                            NOM_PARA=('V2','D2',),),),);

TFXA2_R1=CALC_TABLE(reuse =TFXA2_R1,
                    TABLE=TFXA2_R1,
                    ACTION=(_F(OPERATION='EXTR',
                               NOM_PARA=('INST','DX',),),
                            _F(OPERATION='RENOMME',
                               NOM_PARA=('DX','FXA2',),),),);

TFXA2_G1=CALC_TABLE(reuse =TFXA2_G1,
                    TABLE=TFXA2_G1,
                    ACTION=(_F(OPERATION='EXTR',
                               NOM_PARA=('INST','DX',),),
                            _F(OPERATION='RENOMME',
                               NOM_PARA=('DX','FXA2',),),),);

TFXA4_R1=CALC_TABLE(reuse =TFXA4_R1,
                    TABLE=TFXA4_R1,
                    ACTION=(_F(OPERATION='EXTR',
                               NOM_PARA=('INST','DX',),),
                            _F(OPERATION='RENOMME',
