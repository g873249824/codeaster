DEBUT(CODE=_F(NOM='SSNV128S',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#......................................................................
#  CALCUL TEST USURE DE TYPE ARCHARD METHODE CONTINUE
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#......................................................................

MA=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'));

MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(GROUP_MA = 'LCONTA'));

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'D_PLAN'));

MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=(_F(GROUP_MA = 'LPRESV'),
                               _F(GROUP_MA = 'LBATI'),
                               _F(GROUP_MA = 'LCONTA'),
                               _F(GROUP_MA = 'LPRESH')));

MATPLA=DEFI_MATERIAU(ELAS=_F(E = 2.1E11, NU = 0.3));

MATBAT=DEFI_MATERIAU(ELAS=_F(E = 1.E16, NU = 0.0));

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA = 'SPLAQ',
                             MATER = MATPLA),
                          _F(GROUP_MA = 'SBATI',
                             MATER = MATBAT)));

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA = 'LBATIX', DX = 0.0),
                              _F(GROUP_MA = 'LENCAV', DX = 0.0, DY = 0.0),
                              _F(GROUP_NO = 'PPS',    DY = 0.0),
                              _F(GROUP_MA = 'LBLOCX', DX = 0.0)),
                    PRES_REP=(_F(GROUP_MA = 'LPRESV', PRES = 5.E07)));

CHA2=AFFE_CHAR_MECA(MODELE=MO,
                    PRES_REP=(_F(GROUP_MA = 'LPRESH', PRES = 5.E07)));
#
CHA4 = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    FROTTEMENT     = 'COULOMB',
                    ALGO_RESO_CONT = 'POINT_FIXE',
                    ALGO_RESO_GEOM = 'POINT_FIXE',
                    ALGO_RESO_FROT = 'NEWTON',
                    RESI_FROT      = 1.E-2,
                    ZONE =(
                           _F(
                             GROUP_MA_MAIT = 'LBATI',
                             GROUP_MA_ESCL = 'LCONTA',
                             SANS_GROUP_NO = ('PPS',),
                             COULOMB       = 1.0,
                             CONTACT_INIT  = 'INTERPENETRE',
                             SEUIL_INIT    = 0.0,
                             ALGO_CONT     = 'STANDARD',
                             COEF_CONT     = 1.0,
                             ALGO_FROT     = 'STANDARD',
                             COEF_FROT     = 1.E5,
                             USURE         = 'ARCHARD',
                             K             = 1.0E-12,
                             H             = 1.0,
                             ),
                          ),
                   );

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0));

LINST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 100));

L_INST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                  LIST_INST=LINST,),
                     ECHEC=_F(EVENEMENT='ERREUR',
                              ACTION='DECOUPE',
                              SUBD_METHODE = 'MANUEL',
                              SUBD_PAS=5,
                              SUBD_NIVEAU=5,),);

F2=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,  0.0,
                      0.01, 1  ,
                      0.02, 0.0,
                      0.03, -1 ,
                      0.04, 0.0));

#-----------------------------------------------------------

U4M=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE = CHA1, FONC_MULT = RAMPE),
                       _F(CHARGE = CHA2, FONC_MULT = F2),
                       ),
                CONTACT  = CHA4,
                COMP_ELAS=_F(RELATION = 'ELAS'),
                NEWTON=_F(REAC_ITER=1),
                INCREMENT=_F(LIST_INST = L_INST,
                             INST_FIN = 0.04),
                CONVERGENCE=_F(ITER_GLOB_MAXI = 20,
                               RESI_GLOB_RELA = 1.0E-5),
                INFO=1);

#
# TEST DE NON REGRESSION
# ======================

TEST_RESU(RESU=(_F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-9.3096502691552998E5,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N4',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-1.7470670526767999E6,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N10',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-1.981000E6,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N30',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-2.3055127156365002E6,
                   TOLE_MACHINE=1.E-3,
                   ),
                ),
          )

PRFUS4=POST_RELEVE_T(ACTION=_F(INTITULE='USURE',
                             GROUP_NO='LCONTA',
                             RESULTAT=U4M,
                             NOM_CHAM='VALE_CONT',
                             #NUME_ORDRE=4,
                             #NOM_CMP='HN',
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION'));

LAGSC4=POST_RELEVE_T(ACTION=_F(INTITULE='LAMBDA',
                             GROUP_NO='LCONTA',
                             RESULTAT=U4M,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=4,
                             NOM_CMP='LAGS_C',
                             OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=PRFUS4);
IMPR_TABLE(TABLE=LAGSC4);

FIN();
