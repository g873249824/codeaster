#
# 5 modes propres (m=1, n=2,3,4,5,6) d'un tuyau élastique mince 
# sous 5 différentes conditions aux limites : 
# FF, SF, CS, CC, SS
# ( S, C, F représentent simply-supported, clamped et free )
# 
# Référence : Free vibration analysis of cylindrical shells using 
#             the Haar wavelet method, 2013, Xiang Xie, Guoyong Jin, Zhigang Liu
#-------------------------------------------------------------------
# 1) type d'éléments finis : DKT avec QUAD4
#-------------------------------------------------------------------

# définition des constants
epai = 2.5e-3

nu = 0.3

module = 2.0e11

rho = 7800

########################################################################
DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

mesh = LIRE_MAILLAGE(UNITE=20)

mesh = MODI_MAILLAGE(reuse=mesh,
                     MAILLAGE=mesh,
                     ORIE_PEAU_3D=_F(GROUP_MA=('surf', )))

model = AFFE_MODELE(AFFE=_F(MODELISATION=('DKT', ),
                            PHENOMENE='MECANIQUE',
                            TOUT='OUI'),
                    MAILLAGE=mesh)

carac = AFFE_CARA_ELEM(COQUE=_F(EPAIS=epai,
                                GROUP_MA=('surf', )),
                       MODELE=model)

acier = DEFI_MATERIAU(ELAS=_F(E=module,
                              NU=nu,
                              RHO=rho))

fieldmat = AFFE_MATERIAU(AFFE=_F(MATER=(acier, ),
                                 TOUT='OUI'),
                         MAILLAGE=mesh)

# définitions de 5 conditions aux limites
CC = AFFE_CHAR_MECA(DDL_IMPO=_F(GROUP_MA=('circle_1', 'circle_2'),
                                LIAISON='ENCASTRE'),
                    MODELE=model)

SS = AFFE_CHAR_MECA(DDL_IMPO=(_F(DX=0.0,
                                 DY=0.0,
                                 GROUP_MA=('circle_1', )),
                              _F(DX=0.0,
                                 DY=0.0,
                                 GROUP_MA=('circle_2', ))),
                    MODELE=model)

SF = AFFE_CHAR_MECA(DDL_IMPO=_F(DX=0.0,
                                DY=0.0,
                                GROUP_MA=('circle_1', )),
                    MODELE=model)

CS = AFFE_CHAR_MECA(DDL_IMPO=(_F(GROUP_MA=('circle_1', ),
                                 LIAISON='ENCASTRE'),
                              _F(DX=0.0,
                                 DY=0.0,
                                 GROUP_MA=('circle_2', ))),
                    MODELE=model)

########################################################################
# cas FF

ASSEMBLAGE(CARA_ELEM=carac,
           CHAM_MATER=fieldmat,
           MATR_ASSE=(_F(MATRICE=CO('MASSE'),
                         OPTION='MASS_MECA'),
                      _F(MATRICE=CO('RIGID'),
                         OPTION='RIGI_MECA')),
           MODELE=model,
           NUME_DDL=CO('NUMEDDL'))

# chercher des modes sous une bande de fréquence
#modes = CALC_MODES(CALC_FREQ=_F(FREQ=(600.0, 8500.0)),
                   #MATR_MASS=MASSE,
                   #MATR_RIGI=RIGID,
                   #OPTION='BANDE',
                   #VERI_MODE=_F(STOP_ERREUR='NON'))

modesFF = CALC_MODES(CALC_FREQ=_F(FREQ=(598.52, 657.335, 1858.85, 3564.7, 5767.42, 8466.42)),
                     MATR_MASS=MASSE,
                     MATR_RIGI=RIGID,
                     OPTION='PROCHE',
                     SOLVEUR=_F(METHODE='MULT_FRONT',),),

########################################################################
# cas SF

ASSEMBLAGE(CARA_ELEM=carac,
           CHAM_MATER=fieldmat,
           CHARGE=SF,
           MATR_ASSE=(_F(MATRICE=CO('MASSESF'),
                         OPTION='MASS_MECA'),
                      _F(MATRICE=CO('RIGIDSF'),
                         OPTION='RIGI_MECA')),
           MODELE=model,
           NUME_DDL=CO('NUMESF'))
           
modesSF = CALC_MODES(CALC_FREQ=_F(FREQ=(657.94, 1859.73, 3565.61, 5768.3, 8467.3)),
                     MATR_MASS=MASSESF,
                     MATR_RIGI=RIGIDSF,
                     OPTION='PROCHE',
                     SOLVEUR=_F(METHODE='MULT_FRONT',),),

########################################################################
# cas CS

ASSEMBLAGE(CARA_ELEM=carac,
           CHAM_MATER=fieldmat,
           CHARGE=CS,
           MATR_ASSE=(_F(MATRICE=CO('MASSECS'),
                         OPTION='MASS_MECA'),
                      _F(MATRICE=CO('RIGIDCS'),
                         OPTION='RIGI_MECA')),
           MODELE=model,
           NUME_DDL=CO('NUMECS'))
           
modesCS = CALC_MODES(CALC_FREQ=_F(FREQ=(675.59, 1866.57, 3572.85, 5775.87, 8474.16)),
                     MATR_MASS=MASSECS,
                     MATR_RIGI=RIGIDCS,
                     OPTION='PROCHE',
                     SOLVEUR=_F(METHODE='MULT_FRONT',),),

########################################################################
# cas CC

ASSEMBLAGE(CARA_ELEM=carac,
           CHAM_MATER=fieldmat,
           CHARGE=CC,
           MATR_ASSE=(_F(MATRICE=CO('MASSECC'),
                         OPTION='MASS_MECA'),
                      _F(MATRICE=CO('RIGIDCC'),
                         OPTION='RIGI_MECA')),
           MODELE=model,
           NUME_DDL=CO('NUMECC'))

modesCC = CALC_MODES(CALC_FREQ=_F(FREQ=(690.073, 1868.14, 3573.32, 5776.09, 8474.27,)),
                     MATR_MASS=MASSECC,
                     MATR_RIGI=RIGIDCC,
                     OPTION='PROCHE',
                     SOLVEUR=_F(METHODE='MULT_FRONT',),),                   

########################################################################
# cas SS

ASSEMBLAGE(CARA_ELEM=carac,
           CHAM_MATER=fieldmat,
           CHARGE=SS,
           MATR_ASSE=(_F(MATRICE=CO('MASSESS'),
                         OPTION='MASS_MECA'),
                      _F(MATRICE=CO('RIGIDSS'),
                         OPTION='RIGI_MECA')),
           MODELE=model,
           NUME_DDL=CO('NUMESS'))

modesSS = CALC_MODES(CALC_FREQ=_F(FREQ=(667.023, 1865.4, 3572.44, 5775.67, 8474.04,)),
                     MATR_MASS=MASSESS,
                     MATR_RIGI=RIGIDSS,
                     OPTION='PROCHE',
                     SOLVEUR=_F(METHODE='MULT_FRONT',),),                            

#IMPR_RESU(FORMAT='MED',
          #RESU=_F(RESULTAT=modesFF),
          #UNITE=80)

########################################################################
#                             VALIDATIONS
          
## MODES POUR FF
TEST_RESU(RESU=(_F(NUME_ORDRE=2,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesFF,
                   VALE_CALC=(657.334857437, ),
                   VALE_REFE=(654.216, )),
                _F(NUME_ORDRE=3,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesFF,
                   VALE_CALC=(1858.84847736, ),
                   VALE_REFE=(1850.287, )),
                _F(NUME_ORDRE=4,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesFF,
                   VALE_CALC=(3564.69880787, ),
                   VALE_REFE=(3547.66, )),
                _F(NUME_ORDRE=5,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesFF,
                   VALE_CALC=(5767.41993879, ),
                   VALE_REFE=(5737.228, )),
                _F(NUME_ORDRE=6,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesFF,
                   VALE_CALC=(8466.4215778, ),
                   VALE_REFE=(8416.323, ))))
                   
## MODES POUR SF
