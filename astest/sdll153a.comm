### MODES POUR CC
TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesCC,
                   VALE_CALC=(690.072895497, ),
                   VALE_REFE=(687.249, )),
                _F(NUME_ORDRE=2,
                   PARA='FREQ',
