                   RESULTAT=modesSF,
                   VALE_CALC=(5768.29630728, ),
                   VALE_REFE=(5738.698, )),
                _F(NUME_ORDRE=5,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesSF,
                   VALE_CALC=(8467.30045253, ),
                   VALE_REFE=(8417.793, ))))                   

### MODES POUR CS
TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesCS,
                   VALE_CALC=(675.589572725, ),
                   VALE_REFE=(671.113, )),
                _F(NUME_ORDRE=2,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesCS,
                   VALE_CALC=(1866.57023643, ),
                   VALE_REFE=(1856.454, )),
                _F(NUME_ORDRE=3,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesCS,
                   VALE_CALC=(3572.8549462, ),
                   VALE_REFE=(3553.286, )),
                _F(NUME_ORDRE=4,
                   PARA='FREQ',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesCS,
