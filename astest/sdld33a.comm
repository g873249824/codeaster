                                  AXE=(1.,0.,0.,),
                                  SPEC_OSCI=SPEC2,),       ),
                        DEPL_MULT_APPUI=(_F(  NUME_CAS =1,
                                          NOM_CAS ='DEPL EN X NUL EN P1 ET P4',
                                          MODE_STAT = MODE_STA,
                                          GROUP_NO = ( 'GROUP_NO_PO1', ),
