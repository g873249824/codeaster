                                          NOM_CAS ='DEPL EN X NUL EN P1 ET P4',
                                          MODE_STAT = MODE_STA,
                                          GROUP_NO = ( 'GROUP_NO_PO5', ),
                                          DX = 0.06),),
                        COMB_MODE=_F(TYPE='SRSS',),
                        GROUP_APPUI=(_F(NOEUD =('P1','P3',),),
                                     _F(GROUP_NO =('GROUP_NO_PO5',),),
                                     ),
                        OPTION=('DEPL','REAC_NODA'),);

