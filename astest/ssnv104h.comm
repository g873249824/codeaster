#

DEBUT(CODE=_F(NOM='SSNV104H',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#

MA=LIRE_MAILLAGE(FORMAT='MED',)

#

MO=AFFE_MODELE(
                           MAILLAGE=MA,
                          AFFE=_F(
                                TOUT = 'OUI',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'AXIS')
                        )

#

MAT=DEFI_MATERIAU(
                            ELAS=_F(
                                  E = 20000.0,
                                  NU = 0.3,
                                  ALPHA = 0.01)
                          )

#

CHMAT=AFFE_MATERIAU(
                            MAILLAGE=MA,
                           AFFE=_F(
                                 TOUT = 'OUI',
                                 MATER = MAT)
                           )

#

CHA1=AFFE_CHAR_MECA(
                             MODELE=MO,DDL_IMPO=(
                             _F(  GROUP_NO = 'SPHSUP',  DY = -5.0),
                                      _F(  GROUP_NO = 'SPHINF',  DY = 5.0),
                                      _F(  GROUP_NO = 'AXE',     DX = 0.0),
                                      _F(  NOEUD = ('N55', 'N291',), DX = 0.0,
                                                          DY = 0.0),
                                                          ),
                           )

CHA2=DEFI_CONTACT(MODELE       = MO,
                  FORMULATION  = 'DISCRETE',
                  LISSAGE='OUI',
                  ZONE=_F(
                            ALGO_CONT     = 'PENALISATION',
                            E_N           = 1.E+10,
                            GROUP_MA_MAIT = 'CONTBAS',
                            GROUP_MA_ESCL = 'CONTHAUT'),
                         )

#

L_INST=DEFI_LIST_REEL(
                             DEBUT=0.0,
                              INTERVALLE=_F( JUSQU_A = 1.,  NOMBRE = 5)
                           )

#

FONC=DEFI_FONCTION(
                           NOM_PARA='INST',
                           VALE=( 0.,   0.0,
                                  1.,   1.0, )
                        )

#

RESU=STAT_NON_LINE(
                   MODELE     = MO,
                   CHAM_MATER = CHMAT,
                   EXCIT      = (_F(
                                 CHARGE = CHA1,
                                 FONC_MULT = FONC
                                 ),
                                ),
                  CONTACT  = CHA2,
                  COMPORTEMENT=_F( RELATION = 'ELAS'),
                  NEWTON=_F(   MATRICE = 'TANGENTE',
                               REAC_ITER = 1,  ),
                  INCREMENT=_F( LIST_INST = L_INST,
                                NUME_INST_FIN = 5),
                  CONVERGENCE=_F( ARRET = 'OUI',
                                  ITER_GLOB_MAXI = 30,
                                  RESI_GLOB_RELA = 1.E-5,
                                  RESI_GLOB_MAXI = 1.E-2),
                  SOLVEUR=_F(SYME='OUI',
                             METHODE = 'MULT_FRONT'),
              )

RESU=CALC_CHAMP(reuse=RESU,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=RESU)


#
SIELNO1=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELNO_SIEF_R',
NOM_CHAM='SIGM_ELNO',
                        RESULTAT=RESU,
                        NUME_ORDRE=1
