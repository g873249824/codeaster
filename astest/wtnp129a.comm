                               TOUT_CMP='OUI',
                               NUME_ORDRE=74,
                               GROUP_NO='DROITE',),);

IMPR_TABLE(TABLE=EXDEP1,
           FORMAT='TABLEAU',
           UNITE=8,
           NOM_PARA=('INST','NOEUD','COOR_X','COOR_Y','PRE1','DY',),);

TEST_TABLE(
