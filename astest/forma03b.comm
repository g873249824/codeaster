DEBUT(CODE=_F(
              NIV_PUB_WEB='INTERNET',
VISU_EFICAS='OUI',),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

# LECTURE DU MAILLAGE
MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_2D=_F(GROUP_MA='haut',),);

# AFFECTATION DU MODELE (CONTRAINTES PLANES)
MODE=AFFE_MODELE(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='C_PLAN',),);

# COURBE DE TRACTION
CTRAC = LIRE_FONCTION(UNITE=21,
                      NOM_PARA='EPSI',
                      PROL_DROITE='CONSTANT',)

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=29,
              COURBE=_F(FONCTION=CTRAC,),
              TITRE='Courbe de traction',
              LEGENDE_X='Deformation',
              LEGENDE_Y='Contrainte (MPa)',);


# DEFINITION DU MATERIAU
ACIER =DEFI_MATERIAU(ELAS=_F(E=200000.0,
                         NU=0.3,),
                 TRACTION=_F(SIGM=CTRAC,),);

CHMATE=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=_F(TOUT='OUI',
                             MATER=ACIER,),);

# CONDITIONS DE SYMETRIE (QUART DE PLAQUE)
CHSYME=AFFE_CHAR_CINE(MODELE=MODE,
                      MECA_IMPO=(_F(GROUP_MA='bas',
                                 DY=0.0,),
                                 _F(GROUP_MA='gauche',
                                 DX=0.0,),),
                                );

# CHARGEMENT EN FORCE
CHFORC=AFFE_CHAR_MECA(MODELE=MODE,
                      FORCE_CONTOUR=_F(GROUP_MA='haut',
                                       FY=1.,),);

# RAMPE APPLICATION CHARGEMENT
RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                           1000.0,1000.0,
                           ),);

# DISCRETISATION EN TEMPS
TFIN  = 230.0;
TELAS = 10.0;

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=TELAS,
                                     NOMBRE=1,),
                                  _F(JUSQU_A=TFIN,
                                     NOMBRE=50,),
                                  ),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST,),
                        ECHEC=_F(EVENEMENT='ERREUR',
                                 SUBD_NIVEAU=5,
                                 ACTION='DECOUPE',),)

########################################################################
#
# PREMIER CALCUL : ON RESTE ELASTIQUE
#
########################################################################

RESUNL=STAT_NON_LINE(MODELE=MODE,
                     CHAM_MATER=CHMATE,
                     EXCIT=(_F(CHARGE=CHSYME,),
                            _F(CHARGE=CHFORC,
                               FONC_MULT=RAMPE,),),
                     COMPORTEMENT=_F(RELATION='VMIS_ISOT_TRAC',),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  INST_FIN=TELAS,),);

########################################################################
#
# PREMIER CALCUL : PLASTIFICATION
#
########################################################################

RESUNL=STAT_NON_LINE(reuse =RESUNL,
                     MODELE=MODE,
                     CHAM_MATER=CHMATE,
                     EXCIT=(_F(CHARGE=CHSYME,),
                            _F(CHARGE=CHFORC,
                               FONC_MULT=RAMPE,),),
                     COMPORTEMENT=_F(RELATION='VMIS_ISOT_TRAC',),
                     ETAT_INIT=_F(EVOL_NOLI=RESUNL,),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  INST_FIN=TFIN,
                                  ),
                     NEWTON=_F(REAC_ITER=1,),
                     );

########################################################################
#
# POST-TRAITEMENTS STANDARDS
#
########################################################################

# INTERPOLATION DES CONTRAINTES AUX NOEUDS
RESUNL=CALC_CHAMP(reuse=RESUNL,RESULTAT=RESUNL,CONTRAINTE=('SIGM_ELNO'))


RESUNL=CALC_CHAMP(reuse =RESUNL,
               RESULTAT=RESUNL,
               CONTRAINTE='SIGM_NOEU');

# INTERPOLATION DES VARIABLES INTERNES AUX NOEUDS
RESUNL=CALC_CHAMP(reuse=RESUNL,RESULTAT=RESUNL,VARI_INTERNE=('VARI_ELNO'))


RESUNL=CALC_CHAMP(reuse =RESUNL,
               RESULTAT=RESUNL,
               VARI_INTERNE='VARI_NOEU',);

########################################################################
#
# IMPRESSION DES RESULTATS
#
########################################################################

IMPR_RESU(FORMAT='MED',
          RESU=_F(RESULTAT=RESUNL,),);

#########################################################################
#
# TROISIEME CALCUL : CHARGEMENT PLASTIQUE PUIS DECHARGE ELASTIQUE
#
########################################################################


# DISCRETISATION EN TEMPS
TCHAR   = 230.0;
TDECHAR = 300.0;

# RAMPE APPLICATION CHARGE PUIS DECHARGE
RAMPE2=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                            TCHAR,TCHAR,
                            TDECHAR,0.,
                            ),);

# DISCRETISATION EN TEMPS
L_INST2=DEFI_LIST_REEL(DEBUT=0.0,
                       INTERVALLE=(_F(JUSQU_A=TELAS,
                                      NOMBRE=1,),
                                   _F(JUSQU_A=TCHAR,
                                      NOMBRE=30,),
                                   _F(JUSQU_A=TDECHAR,
                                      NOMBRE=10,),),);

DEFLIST2 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST2,),
                        ECHEC=_F(EVENEMENT    = 'ERREUR',
                                 ACTION       = 'DECOUPE',),)

########################################################################
#
# TROISIEME CALCUL : CHARGEMENT PLASTIQUE PUIS DECHARGE
#
########################################################################

RESUNL2=STAT_NON_LINE(MODELE=MODE,
                      CHAM_MATER=CHMATE,
                      EXCIT=(_F(CHARGE=CHSYME,),
                          _F(CHARGE=CHFORC,
                             FONC_MULT=RAMPE2,),),
                      COMPORTEMENT=_F(RELATION='VMIS_ISOT_TRAC',),
                      INCREMENT=_F(LIST_INST=DEFLIST2),
                      NEWTON=_F(PREDICTION='ELASTIQUE',
                                REAC_ITER=1,),
                      ARCHIVAGE=_F(LIST_INST=L_INST2,),);

########################################################################
#
# POST-TRAITEMENTS STANDARDS
#
########################################################################
# INTERPOLATION DES CONTRAINTES AUX NOEUDS
RESUNL2=CALC_CHAMP(reuse=RESUNL2,RESULTAT=RESUNL2,CONTRAINTE=('SIGM_ELNO'))


RESUNL2=CALC_CHAMP(reuse =RESUNL2,
               RESULTAT=RESUNL2,
               CONTRAINTE='SIGM_NOEU');

# INTERPOLATION DES VARIABLES INTERNES AUX NOEUDS
RESUNL2=CALC_CHAMP(reuse=RESUNL2,RESULTAT=RESUNL2,VARI_INTERNE=('VARI_ELNO'))


RESUNL2=CALC_CHAMP(reuse =RESUNL2,
               RESULTAT=RESUNL2,
               VARI_INTERNE='VARI_NOEU',);

                ########################################################################
#
# IMPRESSION DES RESULTATS
#
########################################################################

IMPR_RESU(FORMAT='MED',
          RESU=_F(RESULTAT=RESUNL2,),);

########################################################################
#
# TESTS - CALCUL CHARGEMENT PLASTIQUE
# PARTIE ELASTIQUE (IDENTIQUE MODELISATION A)
#
########################################################################


TEST_RESU(RESU=_F(INST=10.,
                  GROUP_NO='B',
                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=RESUNL,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIYY',
                  VALE_CALC=29.809234113,
                  VALE_REFE=29.809234113416,
                  CRITERE='RELATIF',
                  PRECISION=1.E-08,),
          )

TEST_RESU(RESU=_F(INST=10.,
                  GROUP_NO='A',
                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=RESUNL,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIXX',
                  VALE_CALC=-9.821390921,
                  VALE_REFE=-9.8213909205095007,
                  CRITERE='RELATIF',
                  PRECISION=1.E-08,),
          )

                      ########################################################################
#
# TESTS - CALCUL CHARGEMENT PLASTIQUE
# PARTIE PLASTIQUE
#
########################################################################

TEST_RESU(RESU=_F(INST=230.,
                  GROUP_NO='B',
                  RESULTAT=RESUNL,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIYY',
                  VALE_CALC=272.38393470555,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=230.,
                  GROUP_NO='A',
                  RESULTAT=RESUNL,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIXX',
                  VALE_CALC=-213.68185288997,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=230.,
                  GROUP_NO='B',
                  RESULTAT=RESUNL,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V1',
                  VALE_CALC=0.35979129617121,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=230.,
                  GROUP_NO='B',
                  RESULTAT=RESUNL,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V2',
                  VALE_CALC=1.0,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=230.,
                  GROUP_NO='A',
                  RESULTAT=RESUNL,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V1',
                  VALE_CALC=3.5713883748082E-03,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=230.,
                  GROUP_NO='A',
                  RESULTAT=RESUNL,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V2',
                  VALE_CALC=1.0,
                  CRITERE='RELATIF',
                  ),
          )

 ########################################################################
#
# TESTS - CALCUL CHARGEMENT PLASTIQUE PUIS DECHARGE
# DECHARGE
#
########################################################################

TEST_RESU(RESU=_F(INST=300.,
                  GROUP_NO='B',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESUNL2,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIYY',
                  VALE_CALC=-298.101100502,
                  VALE_REFE=-298.10110050242002,
                  CRITERE='RELATIF',
                  PRECISION=1.E-08,),
          )

TEST_RESU(RESU=_F(INST=300.,
                  GROUP_NO='A',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESUNL2,
                  NOM_CHAM='SIGM_NOEU',
                  NOM_CMP='SIXX',
                  VALE_CALC=16.841677074,
                  VALE_REFE=16.841677073602,
                  CRITERE='RELATIF',
                  PRECISION=1.E-08,),
          )

TEST_RESU(RESU=_F(INST=300.,
                  GROUP_NO='B',
                  RESULTAT=RESUNL2,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V1',
                  VALE_CALC=0.36437022323172003,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=300.,
                  GROUP_NO='B',
                  RESULTAT=RESUNL2,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V2',
                  VALE_CALC=1.5,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=300.,
                  GROUP_NO='A',
                  RESULTAT=RESUNL2,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V1',
                  VALE_CALC=3.5534604597225001E-3,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(INST=300.,
                  GROUP_NO='A',
                  RESULTAT=RESUNL2,
                  NOM_CHAM='VARI_NOEU',
                  NOM_CMP='V2',
                  VALE_CALC=0.0,
                  CRITERE='ABSOLU',
                  ),
          )

FIN();
