DEBUT(CODE=_F(NOM='SENSM11A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'),
          BASE=_F( FICHIER = 'GLOBALE',LONG_REPE = 15000)  )
#
#......................................................................
# PLAQUE CIRCULAIRE EPAISSE EN FLEXION EN 2D AXI
# PLASTICITE VON-MISES A ECROUISSAGE ISOTROPE LINEAIRE
# SENSIBILITE AUX PARAMETRES MATERIAU ET CHARGEMENT (FORCE NODALE)
#......................................................................
#

M=LIRE_MAILLAGE( )

MO=AFFE_MODELE(
            MAILLAGE=M,    VERIF='MAILLE',
           AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = 'AXIS')
           )


E=DEFI_CONSTANTE(VALE=210000.,);
PS_E1=DEFI_PARA_SENSI(VALE=220000.,);
PS_E2=DEFI_PARA_SENSI(VALE=230000.,);
PS_DSDE=DEFI_PARA_SENSI(VALE=2000.,);
PS_SIGY=DEFI_PARA_SENSI(VALE=200.,);
NU=DEFI_CONSTANTE(VALE=0.3,);


ACIER=DEFI_MATERIAU(ELAS_FO=_F(E= E,
                             NU= NU,),
                    ECRO_LINE_FO=_F(D_SIGM_EPSI=PS_DSDE,
                                 SY=PS_SIGY,),
                                );

ACIER1=DEFI_MATERIAU(ELAS_FO=_F(E= PS_E1,
                             NU= NU,),
                    ECRO_LINE_FO=_F(D_SIGM_EPSI=PS_DSDE,
                                 SY=PS_SIGY,),
                                );

ACIER2=DEFI_MATERIAU(ELAS_FO=_F(E= PS_E2,
                             NU= NU,),
                    ECRO_LINE_FO=_F(D_SIGM_EPSI=PS_DSDE,
                                 SY=PS_SIGY,),
                                );

CM=AFFE_MATERIAU(
            MAILLAGE=M,
           AFFE=(_F( MAILLE = 'M1' , MATER = ACIER1),
                _F( MAILLE = 'M2' , MATER = ACIER),
                _F( MAILLE = 'M3' , MATER = ACIER2))
                )

PS_FOR=DEFI_PARA_SENSI(VALE=100.,);

RAMPE=DEFI_FONCTION(
           NOM_PARA='INST',PROL_DROITE='CONSTANT',PROL_GAUCHE='LINEAIRE',
           VALE=(0.0, 0.0, 60.0, -1.0,)
           )

CH1=AFFE_CHAR_MECA(
            MODELE=MO,
           DDL_IMPO=(_F( NOEUD = 'N1', DX = 0., DY = 0.),
                     _F( NOEUD = 'N4', DX = 0.),),
           )

CH2=AFFE_CHAR_MECA_F(
            MODELE=MO,
            FORCE_NODALE=_F(NOEUD = 'N3', FY = PS_FOR)
                          )

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
             INTERVALLE=_F( JUSQU_A = 60.0,       NOMBRE = 15)
           )


EVOL= STAT_NON_LINE ( MODELE=MO,
                   CHAM_MATER=CM,
                   EXCIT=(_F(CHARGE=CH1),
                          _F(CHARGE=CH2,
                             FONC_MULT=RAMPE)),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                                RESI_INTE_RELA = 1.E-8),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=60.,
                                SUBD_METHODE='UNIFORME', SUBD_PAS=4,
                                SUBD_PAS_MINI=1.E-4,
                                SUBD_COEF_PAS_1=1.0,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             PREDICTION='ELASTIQUE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(
                            RESI_GLOB_RELA = 1.E-8),
                   ARCHIVAGE=_F(LIST_INST=L_INST,
                                ARCH_ETAT_INIT='OUI',),
                   SENSIBILITE=(PS_FOR,PS_E1,PS_E2,PS_DSDE,PS_SIGY),
                   INFO=1);



# SENSIBILITE PAR RAPPORT A LA FORCE NODALE IMPOSEE

TEST_RESU(RESU=(
   _F(RESULTAT = EVOL,
      INST = 60.,
      NOEUD = 'N8',MAILLE = 'M1',
      NOM_CHAM = 'DEPL',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'DX',   SENSIBILITE = PS_FOR,
      VALE = 4.00907E-5, REFERENCE = 'AUTRE_ASTER' ),
   _F(RESULTAT = EVOL,
      INST = 60.,
      MAILLE = 'M1',POINT = 4,
      NOM_CHAM = 'SIEF_ELGA',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'SIXX',   SENSIBILITE = PS_FOR,
      VALE = -1.26602E+0, REFERENCE = 'AUTRE_ASTER' ),
   _F(RESULTAT = EVOL,
      INST = 60.,
      MAILLE = 'M1',POINT = 3,
      NOM_CHAM = 'VARI_ELGA',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'V1',   SENSIBILITE = PS_FOR,
      VALE = 1.6996E-3, REFERENCE = 'AUTRE_ASTER' ),
      ));


# SENSIBILITE PAR RAPPORT AU PARAMETRE PS_E1

TEST_RESU(RESU=(
   _F(RESULTAT = EVOL,
      INST = 60.,
      NOEUD = 'N4',MAILLE = 'M1',
      NOM_CHAM = 'DEPL',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'DY',   SENSIBILITE = PS_E1,
      VALE = 2.5687E-8, REFERENCE = 'AUTRE_ASTER' ),
      ));


# SENSIBILITE PAR RAPPORT AU PARAMETRE PS_E2

TEST_RESU(RESU=(
   _F(RESULTAT = EVOL,
      INST = 60.,
      MAILLE = 'M3',POINT = 3,
      NOM_CHAM = 'SIEF_ELGA',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'SIYY',   SENSIBILITE = PS_E2,
      VALE = 9.46376E-5, REFERENCE = 'AUTRE_ASTER' ),
      ));


# SENSIBILITE PAR RAPPORT AU PARAMETRE PS_DSDE

TEST_RESU(RESU=(
   _F(RESULTAT = EVOL,
      INST = 60.,
      MAILLE = 'M1',POINT = 3,
      NOM_CHAM = 'SIEF_ELGA',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'SIXY',   SENSIBILITE = PS_DSDE,
      VALE = 7.7095E-4, REFERENCE = 'AUTRE_ASTER' ),
      ));


# SENSIBILITE PAR RAPPORT AU PARAMETRE PS_SIGY

TEST_RESU(RESU=(
   _F(RESULTAT = EVOL,
      INST = 60.,
      MAILLE = 'M1',POINT = 1,
      NOM_CHAM = 'VARI_ELGA',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'V1',   SENSIBILITE = PS_SIGY,
      VALE = -4.9779E-4, REFERENCE = 'AUTRE_ASTER' ),
      ));


FIN( )
