# ON S'ARRANGE QUE LA SOLUTION SOIT SINUSOIDALLE DE FORME U = Q0*SIN(K*X)*SIN(FREQ*T)
# F0 - EFFORT APPLIQUE, VIT0, VIT1, VIT2, VITF - VITESSE INITIALES 

DEBUT(     CODE=_F(  NOM = 'SDLS115A',NIV_PUB_WEB='INTERNET'))
RHO  = 2500.E0  
EP   = 0.1

EY  = 4.3880E10 
NU  = 0.

LONG = 1.0
LARG = 1.0
C0   = sqrt(EY/RHO)
K    = 3.1416/8.0 * LONG

FREQ = K*C0
Q0   = 1.0E-4

F0   = EY*Q0*K * EP * cos(K*LONG) 

XN = LONG/3.0
VIT0 = FREQ*Q0* sin(K* 0.0)
VIT1 = FREQ*Q0* sin(K* XN)
VIT2 = FREQ*Q0* sin(K* 2*XN)
VITF = FREQ*Q0* sin(K* LONG)


TMAX = 1.2E-3
DT = 3.0E-5

PRE_GIBI();

MAIL=LIRE_MAILLAGE();

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='A1A2',),
                               _F(GROUP_MA='A4A3' ,),
                               _F(GROUP_MA='A2A4' ,),
                               _F(GROUP_MA='A3A1' ,),
                               _F(GROUP_MA='M1' ,),
                               _F(GROUP_MA='M2' ,),
                             ),);

MOD1=AFFE_MODELE(MAILLAGE=MAIL,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='DKT',),);



MAT=DEFI_MATERIAU(                           
                  ELAS = _F(
                             E    =  EY,   
                             NU   =  NU,              
                             RHO  = RHO,
                           ),
                 );

                             
CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(TOUT='OUI',MATER=MAT,),),
                    );
                             
CELE1=AFFE_CARA_ELEM(MODELE=MOD1,
                        COQUE=_F(GROUP_MA='MESH',
                                 EPAIS=EP,
                                 INER_ROTA = 'OUI',
                                 ANGL_REP=(0.0,0.0),
                                ),
                       );


COND_L1=AFFE_CHAR_MECA(MODELE=MOD1,
                        DDL_IMPO=(
                                  _F(GROUP_NO='A1',
                                     DX=0.0,
                                     DY=0.0,
                                     DZ=0.0,
                                     DRX=0.0,
                                     DRY=0.0,
                                     DRZ=0.0,
                                     ),
                                  _F(GROUP_NO='A3A1',
                                     DX=0.0,
                                     DZ=0.0,
                                     DRX=0.0,
                                     DRY=0.0,
                                     DRZ=0.0,
                                      ),
                                  _F(GROUP_NO='A1A2',
                                     DZ=0.0,
                                     DRX=0.0,
                                     DRY=0.0,
                                     DRZ=0.0,
                                      ),
                                  ),
                     FORCE_ARETE = _F(
                                      GROUP_MA = 'A2A4',
                                      FX       = 1.0
                                      )         
                       );


ffonc = FORMULE (
                  NOM_PARA = 'INST',
                  VALE = ''' F0*sin(FREQ*INST) '''
                  )

INST1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=(
                          _F(JUSQU_A = TMAX,PAS = DT),
                      ),);              

chf1  = CALC_FONC_INTERP (
                          FONCTION   = ffonc,
                          LIST_PARA  = INST1,
                          PROL_DROITE = 'CONSTANT',
                          PROL_GAUCHE = 'CONSTANT',
                          )
IMPR_CO(
         CONCEPT = _F(
                     NOM = 'chf1'
                    )
        )
        
NN1  = int(TMAX/DT)

VIT_INI = CREA_CHAMP(MAILLAGE=MAIL,
                    OPERATION='AFFE', 
                    TYPE_CHAM='NOEU_DEPL_R',
                    AFFE=(
                          _F(GROUP_NO = 'M1',
                             NOM_CMP  = 'DX',
                             VALE     = VIT1,),
                          _F(GROUP_NO = 'M2',
                             NOM_CMP  = 'DX',
                             VALE     = VIT2,),
                          _F(GROUP_NO = 'A2A4',
                             NOM_CMP  = 'DX',
                             VALE     = VITF,),
                          )
                     )        
                             
                 
RESU1 = DYNA_TRAN_EXPLI( 
                       MODELE=MOD1,
                       CHAM_MATER= CHMAT,
                       CARA_ELEM = CELE1,
                       MASS_DIAG='NON',
                       COMP_INCR=_F(
                                    RELATION ='ELAS',                                   ),
                       INCREMENT=_F( LIST_INST = INST1,
                                     PRECISION =1.E-1),
                       EXCIT=(   _F( CHARGE = COND_L1, 
                                     FONC_MULT = chf1,),
                             ),
                       DIFF_CENT = _F(), 
                       ETAT_INIT = _F(
                                       VITE = VIT_INI,
                                      ),              
                     );
                    
                 
                 

IMPR_RESU(MODELE = MOD1,
          FORMAT = 'RESULTAT',
          RESU = (_F(
                     MAILLAGE = MAIL,
                     RESULTAT = RESU1,
                     NOEUD    = 'N2', 
                     NOM_CHAM = (
                                 'DEPL',
                                 'VITE',
                                 'ACCE',
                                ) ,
           ),),);  
            
           
UXRES = Q0*sin(K*LONG)*sin(FREQ*TMAX)
TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  NOEUD='N2',
                  INST=1.2E-3,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE = UXRES,
                  PRECISION = 1.E-4,
                  REFERENCE='ANALYTIQUE',
                  ),);

FIN();
