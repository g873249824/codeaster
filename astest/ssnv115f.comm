DEBUT( CODE=_F( NOM = 'SSNV115F',NIV_PUB_WEB='INTERNET') )

#

MATER=DEFI_MATERIAU( ELAS=_F(  E = 2000.,   NU = 0.3),
                     ECRO_LINE=_F(  D_SIGM_EPSI = 200.,   SY = 1.E+2) )

#

MAILL=LIRE_MAILLAGE(   )

MAILL=DEFI_GROUP( reuse=MAILL,   MAILLAGE=MAILL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

#

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAILL,
                          AFFE=_F(  TOUT = 'OUI',   MATER = MATER) )

#

MODEL=AFFE_MODELE(  MAILLAGE=MAILL,
                        AFFE=_F( TOUT = 'OUI',   MODELISATION = 'COQUE_D_PLAN',
                              PHENOMENE = 'MECANIQUE') )

#

ZERO=DEFI_CONSTANTE(    VALE=0.0 )

VAL_FX=DEFI_CONSTANTE(    VALE=5.0 )

#

CHARGE=AFFE_CHAR_MECA_F(  MODELE=MODEL,
            DDL_IMPO=_F( GROUP_NO = 'O',      DX = ZERO, DY = ZERO, DRZ = ZERO),
           FORCE_NODALE=_F( GROUP_NO = 'A',   FX = VAL_FX)   )

#

CARELEM=AFFE_CARA_ELEM(  MODELE=MODEL,
                 COQUE=_F(COQUE_NCOU=7,
       GROUP_MA = 'TOUT',
 EPAIS = 0.05,
                       A_CIS = 0.833333333,     MODI_METRIQUE = 'NON') )

#

RAMPE=DEFI_FONCTION(  NOM_PARA='INST',
            PROL_GAUCHE='LINEAIRE', PROL_DROITE='LINEAIRE',
            VALE=(0.0,0.0,1.0,1.0,) )

#

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
                INTERVALLE=_F( JUSQU_A = 1.,  NOMBRE = 10) )

#
#
# INTEGRATION PAR COUCHES DANS L'EPAISSEUR DE LA COQUE
# COQUE_NCOU : 7
# MATRICE TANGENTE
#

U=STAT_NON_LINE(
                            MODELE=MODEL,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CARELEM,
                             EXCIT=_F(
                CHARGE = CHARGE,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_LINE',
               ),
                         INCREMENT=_F(
                LIST_INST = L_INST,
                NUME_INST_FIN = 1),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                ITER_INTE_MAXI = 30,
                ITER_GLOB_MAXI = 10,
                RESI_GLOB_RELA = 1.E-6),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3,
                RESI_LINE_RELA = 1.E-5)
              )

#
U=STAT_NON_LINE( reuse=U,
                            MODELE=MODEL,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CARELEM,
                             EXCIT=_F(
                CHARGE = CHARGE,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_LINE',
               ),
                         ETAT_INIT=_F(
                EVOL_NOLI = U,
                NUME_ORDRE = 1),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                ITER_INTE_MAXI = 30,
                ITER_GLOB_MAXI = 10,
                RESI_INTE_RELA = 1.E-8,
                RESI_GLOB_RELA = 1.E-6),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3,
                RESI_LINE_RELA = 1.E-5)
              )

#

TEST_RESU(RESU=(
     _F( RESULTAT = U, NUME_ORDRE = 10,  NOM_CHAM = 'DEPL',
      PRECISION = 1.5E-2, NOEUD = 'NO000002', NOM_CMP = 'DX', VALE = 0.02743),
     _F(  RESULTAT = U, NUME_ORDRE = 10,  NOM_CHAM = 'DEPL',
      PRECISION = 1.8E-2, NOEUD = 'NO000002', NOM_CMP = 'DY', VALE = -0.2804))
           )

#
# INTEGRATION PAR COUCHES DANS L'EPAISSEUR DE LA COQUE
# COQUE_NCOU : 7
# MATRICE ELASTIQUE
#

V=STAT_NON_LINE(
                            MODELE=MODEL,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CARELEM,
                             EXCIT=_F(
                CHARGE = CHARGE,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_LINE',
               ),
                         INCREMENT=_F(
                LIST_INST = L_INST,
                NUME_INST_FIN = 1),
                            NEWTON=_F(
                MATRICE = 'ELASTIQUE'),
                       CONVERGENCE=_F(
                ITER_INTE_MAXI = 30,
                ITER_GLOB_MAXI = 300,
                RESI_INTE_RELA = 1.E-8,
                RESI_GLOB_RELA = 1.E-6)
              )

#
V=STAT_NON_LINE( reuse=V,
                            MODELE=MODEL,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CARELEM,
                             EXCIT=_F(
                CHARGE = CHARGE,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_LINE',
               ),
                         ETAT_INIT=_F(
                EVOL_NOLI = V,
                NUME_ORDRE = 1),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'ELASTIQUE'),
                       CONVERGENCE=_F(
                ITER_INTE_MAXI = 30,
                ITER_GLOB_MAXI = 300,
                RESI_INTE_RELA = 1.E-8,
                RESI_GLOB_RELA = 1.E-6)
              )

#

TEST_RESU(RESU=(
     _F( RESULTAT = V, NUME_ORDRE = 10,  NOM_CHAM = 'DEPL',
      PRECISION = 1.5E-2, NOEUD = 'NO000002', NOM_CMP = 'DX', VALE = 0.02743),
     _F(  RESULTAT = V, NUME_ORDRE = 10,  NOM_CHAM = 'DEPL',
      PRECISION = 1.8E-2, NOEUD = 'NO000002', NOM_CMP = 'DY', VALE = -0.2804))
           )

#

#---------------------------------------------------------------------
FIN()
#
