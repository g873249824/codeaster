MATER=DEFI_MATERIAU( ELAS=_F(  E = 1., NU = 0.3,  RHO = 1.) )

MAILL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),   )

MAILL=DEFI_GROUP( reuse=MAILL,   MAILLAGE=MAILL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAILL,
                          AFFE=_F(  TOUT = 'OUI',   MATER = MATER) )

MODEL=AFFE_MODELE(  MAILLAGE=MAILL,    VERIF='MAILLE',
                        AFFE=_F( TOUT = 'OUI',   MODELISATION = 'DST',
                              PHENOMENE = 'MECANIQUE') )

CINE=AFFE_CHAR_MECA(   MODELE=MODEL,DDL_IMPO=(
              _F( GROUP_NO = 'GRNO_ABC', DX = 0.,  DY = 0.,  DZ = 0.,
                                          DRX = 0.,  DRY = 0.,  DRZ = 0.),
                       _F( GROUP_NO = 'GRNO_OA',  DY = 0.,  DRX = 0.,  DRZ = 0.),
                       _F( GROUP_NO = 'GRNO_OC',  DX = 0.,  DRY = 0.,  DRZ = 0.),
                       _F( NOEUD = 'O',        DX = 0.,  DY = 0.,
                                          DRX = 0.,  DRY = 0.,  DRZ = 0.))
                           )

PRES=AFFE_CHAR_MECA(   MODELE=MODEL,
                    FORCE_COQUE=_F(  TOUT = 'OUI',   PRES = 1.)   )

CHF3=AFFE_CHAR_MECA(   MODELE=MODEL,
                    FORCE_COQUE=_F(  TOUT = 'OUI',   F3 = -1.)   )

CHPE=AFFE_CHAR_MECA(   MODELE=MODEL,
                      PESANTEUR=( 10., 0., 0., -1.,                 )   )

CARELEM=AFFE_CARA_ELEM(   MODELE=MODEL,
                               COQUE=_F(  
       GROUP_MA = 'TOUT',
  EPAIS = 0.1) )

MATELEM=CALC_MATR_ELEM(   MODELE=MODEL,        CHARGE=CINE,
                               CARA_ELEM=CARELEM,   CHAM_MATER=CHMAT,
                               OPTION='RIGI_MECA' )

VEPR=CALC_VECT_ELEM(   CHARGE=(CINE, PRES, ),  CARA_ELEM=CARELEM,
                               CHAM_MATER=CHMAT, OPTION='CHAR_MECA' )

VEF3=CALC_VECT_ELEM(   CHARGE=(CINE, CHF3, ),  CARA_ELEM=CARELEM,
                               CHAM_MATER=CHMAT, OPTION='CHAR_MECA' )

VEPE=CALC_VECT_ELEM(   CHARGE=(CINE, CHPE, ),  CARA_ELEM=CARELEM,
                               CHAM_MATER=CHMAT, OPTION='CHAR_MECA' )

NU=NUME_DDL(         MATR_RIGI=MATELEM )

MATASS=ASSE_MATRICE(     MATR_ELEM=MATELEM,   NUME_DDL=NU )

VEPRAS=ASSE_VECTEUR(     VECT_ELEM=VEPR,      NUME_DDL=NU )

VEF3AS=ASSE_VECTEUR(     VECT_ELEM=VEF3,      NUME_DDL=NU )

VEPEAS=ASSE_VECTEUR(     VECT_ELEM=VEPE,      NUME_DDL=NU )

MATASS=FACT_LDLT(reuse=MATASS,MATR_ASSE=MATASS )

DEPR=RESO_LDLT(        MATR_FACT=MATASS,   CHAM_NO=VEPRAS)

DEF3=RESO_LDLT(        MATR_FACT=MATASS,   CHAM_NO=VEF3AS)

DEPE=RESO_LDLT(        MATR_FACT=MATASS,   CHAM_NO=VEPEAS)

