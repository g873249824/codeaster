                   NOEUD='N2',
                   NOM_CMP='V1',
                   VALE_CALC= 2.99999780E-04,
                   VALE_REFE=2.9999999999999997E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N2',
                   NOM_CMP='SIYY',
                   VALE_CALC=-100.000024408,
                   VALE_REFE=-100.0,
                   MAILLE='M1',),
                _F(NUME_ORDRE=5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.27499998E-03,
                   VALE_REFE=1.2750000000000001E-3,
                   MAILLE='M1',),
                _F(NUME_ORDRE=5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='V1',
                   VALE_CALC= 5.24999760E-04,
                   VALE_REFE=5.2499999999999997E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=5,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N3',
                   NOM_CMP='SIYY',
                   VALE_CALC=-75.000013326,
                   VALE_REFE=-75.0,
                   MAILLE='M1',),
                ),
          )

#  ATESTS DES ENERGIES
U=CALC_CHAMP(reuse=U,ENERGIE=('ETOT_ELGA','ENEL_ELGA'),RESULTAT=U)



TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=U,
                  NOM_CHAM='ENEL_ELGA',
                  NOM_CMP='TOTALE',
                  VALE_CALC=0.044443556,
                  VALE_REFE=0.044443999999999997,
                  MAILLE='M1',),
          )

