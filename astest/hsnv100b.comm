T_POT=POST_ELEM(
                    RESULTAT=U,
                    ENER_POT=_F( TOUT = 'OUI',)  )

T_TOT=POST_ELEM(
                    RESULTAT=U,
                    ENER_TOTALE=_F( TOUT = 'OUI',)  )

IMPR_TABLE(TABLE=T_POT);

