TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=0.177774222,
           VALE_REFE=0.17777000000000001,
           NOM_PARA='TOTALE',
           TABLE=T_POT,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
