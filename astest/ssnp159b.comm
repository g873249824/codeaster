DEBUT(CODE=_F(NOM='SSNP159B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(    MAILLAGE=MA,
                   VERIF='MAILLE',
                   AFFE=_F(  TOUT = 'OUI',
                             PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'AXIS')
                       )


ACIER=DEFI_MATERIAU(
                   ELAS=_F(
                   E = 200000.,
                   NU = 0.3,),
                   ECRO_LINE=_F(D_SIGM_EPSI=2000,
                                SY=1000,)
             )

L_INST=DEFI_LIST_REEL(
              DEBUT=0.0,INTERVALLE=(
                                   _F(  JUSQU_A = 2.00, NOMBRE = 20),)
             )


F_CHAR=DEFI_FONCTION(
                NOM_PARA='INST',
                PROL_DROITE='CONSTANT',
                    VALE=(
                          0.,   0.,
                          2.,  290,
                          )
                  )

CHR_LIAI=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
                                  _F(  NOEUD = 'NO1',  DY = 0.),
                                  _F(  NOEUD = 'NO2',  DY = 0.))
                          )

CHR_TRAC=AFFE_CHAR_MECA(  MODELE=MO,
                          FACE_IMPO=_F(  MAILLE = 'MA2',  DY = 1.)
                          )

CHP_MATL=AFFE_MATERIAU(   MAILLAGE=MA,
                          AFFE=_F(  TOUT = 'OUI',
                                    MATER = ACIER,),
                       )
                       
 ###################################################           
#CALCUL ET TEST EN PETITES DEFORMATIONS 
PT=STAT_NON_LINE(
                  MODELE=MO,
                  CHAM_MATER=CHP_MATL,
                  EXCIT=(_F(CHARGE = CHR_LIAI),
                        _F(CHARGE = CHR_TRAC,
                           FONC_MULT = F_CHAR,
                           TYPE_CHARGE = 'DIDI')),
                  COMP_INCR=_F(RELATION = 'VMIS_ISOT_LINE',
                               DEFORMATION = 'PETIT'),
                  INCREMENT=_F(LIST_INST = L_INST),
                  NEWTON=_F(MATRICE = 'TANGENTE',
                            REAC_ITER = 1),
                  CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                 ITER_GLOB_MAXI = 50),
                  RECH_LINEAIRE=_F(ITER_LINE_MAXI = 0)
              )

ENERPT=POST_ELEM(ENER_ELAS=_F(TOUT='OUI'),
                 RESULTAT=PT,
                 );

TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=PT,
                   NOM_CHAM='DEPL',
                   NOEUD='NO3',
                   NOM_CMP='DY',
                   VALE_CALC=290.000000000,
                   VALE_REFE=290.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=PT,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIYY',
                   VALE_CALC= 1.57000000E+03,
                   VALE_REFE=1570.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=PT,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=0.282150000,
                   VALE_REFE=0.28199999999999997,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.014999999999999999,
                   MAILLE='MA1',),
                ),
          )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 3.08112500E+09,
           VALE_REFE=3.080000000E9,
           NOM_PARA='TOTALE',
           TABLE=ENERPT,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,),
           )
           
PT = CALC_CHAMP(reuse=PT,RESULTAT=PT,ENERGIE=('ENEL_ELGA'),)


TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=PT,
                   NOM_CHAM='ENEL_ELGA',
                   MAILLE='MA1',POINT=1,
                   NOM_CMP='TOTALE',
                   VALE_CALC=3.081125*2,
                   VALE_REFE=3.08*2,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=1.E-2,),))

