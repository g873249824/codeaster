DEBUT(CODE=_F(NOM='SSLL102F', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#
MA=LIRE_MAILLAGE(  )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'GROUP_MA_TOUT', TOUT = 'OUI'))

#
MO=AFFE_MODELE(    MAILLAGE=MA,
                        AFFE=_F( TOUT = 'OUI',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'POU_D_E')
                 )

#
MATR=DEFI_MATERIAU(    ELAS=_F( E = 2.E11,
                               NU = 0.3)
                     )

#
CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,
                              AFFE=_F( TOUT = 'OUI',
                                    MATER = MATR)
                        )

#
CAREL=AFFE_CARA_ELEM(    MODELE=MO,
                           POUTRE=_F(
       GROUP_MA = 'GROUP_MA_TOUT',

                                    SECTION = 'GENERALE',
                                    CARA = ('A',  'IY',
                                               'IZ', 'JX',
                                               'RY', 'RZ',  'RT',
                                              ),
                                    VALE = ( 3.14159E-2, 7.85398E-5,
                                                7.85398E-5, 1.57080E-4,
                                                0.100,   0.100,   0.100,
                                              ))
                       )

#
F1=DEFI_FONCTION(    NOM_PARA='X',
                           VALE=(0.,  0.,
                                 6.,  6000.,
                                ),
                       INTERPOL='LIN'
                   )

#
CHARG1=AFFE_CHAR_MECA(      MODELE=MO,DDL_IMPO=(
                            _F( NOEUD = 'N1',
                                      DX = 0.,
                                      DY = 0.,
                                      DZ = 0.,
                                      DRX = 0.,
                                      DRY = 0.),
                            _F( NOEUD = 'N11',
                                      DY = 0.))
                        )

#
CHARG2=AFFE_CHAR_MECA_F(          MODELE=MO,
                              FORCE_POUTRE=_F(
       GROUP_MA = 'GROUP_MA_TOUT',

                                            FY = F1)
                          )

#
MEST1=MECA_STATIQUE(        MODELE=MO,
                         SOLVEUR=_F( STOP_SINGULIER = 'OUI'),
                          CHAM_MATER=CHMAT,
                           CARA_ELEM=CAREL,EXCIT=(
                               _F( CHARGE = CHARG1),
                                     _F( CHARGE = CHARG2))
                      )

#
MEST1=CALC_CHAMP(reuse=MEST1,RESULTAT=MEST1,CONTRAINTE=('EFGE_ELNO',))


#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MEST1,
                  NOM_CHAM='DEPL',
                  NOEUD='N7',
                  NOM_CMP='DY',
                  VALE_CALC= 3.22871363E-03,
                  VALE_REFE=3.2287129999999998E-3,
                  CRITERE='RELATIF',
                  PRECISION=9.9999999999999995E-07,),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MEST1,
                  NOM_CHAM='EFGE_ELNO',
                  NOEUD='N1',
                  NOM_CMP='VY',
                  VALE_CALC= 6.00000000E+03,
                  VALE_REFE=6000.0,
                  CRITERE='RELATIF',
                  PRECISION=9.9999999999999995E-07,
                  MAILLE='S11',),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MEST1,
                  NOM_CHAM='EFGE_ELNO',
                  NOEUD='N11',
                  NOM_CMP='VY',
                  VALE_CALC=-1.20000000E+04,
                  VALE_REFE=-1.2000E4,
                  CRITERE='RELATIF',
                  PRECISION=9.9999999999999995E-07,
                  MAILLE='S110',),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MEST1,
                  NOM_CHAM='EFGE_ELNO',
                  NOEUD='N8',
                  NOM_CMP='MFZ',
                  VALE_CALC=-1.38564065E+04,
                  VALE_REFE=-1.385641E4,
                  CRITERE='RELATIF',
                  PRECISION=9.9999999999999995E-07,
                  MAILLE='S17',),
          )

#
FIN( )
