DEBUT(CODE=_F( NOM = 'WTNP125A',NIV_PUB_WEB='INTERNET'))

tint=236520000.
tfin=630720000.

nbpas1=25
nbpas2=25

pini=48700000
pint=33700000
pfin=33700000

sigxx=62000000.0
sigyy=sigxx-pini

kni=1.E9
ouvm=2.17E-3
ouvini = 3.04E-4


VISCOL=DEFI_CONSTANTE(VALE=1.E-3,);

VISCOG=DEFI_CONSTANTE(VALE=10.2E-6,);

RHOL=DEFI_CONSTANTE(VALE=1.E03,);

DVISCOL=DEFI_CONSTANTE(VALE=0.0,);

DVISCOG=DEFI_CONSTANTE(VALE=0.0,);

PERML=DEFI_CONSTANTE(VALE=1.688E-17,);

UN=DEFI_CONSTANTE(    VALE=1.,);

####################################
#  LISTE DES INSTANTS DE CALCUL
####################################

LISTINST=DEFI_LIST_REEL(DEBUT = 0.,
                        INTERVALLE=(_F(JUSQU_A = tint,
                                      NOMBRE = nbpas1,),
                                   _F(JUSQU_A = tfin,
                                      NOMBRE = nbpas2,),),);


####################################
#   OPERATIONS SUR LE MAILLAGE
#
####################################

MA0=LIRE_MAILLAGE(FORMAT='MED',
                   VERI_MAIL=_F(APLAT=1.E-9,
                                 VERIF='OUI'),);

MA=CREA_MAILLAGE(MAILLAGE = MA0, LINE_QUAD = _F(TOUT='OUI'));



MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=_F(GROUP_MA=('HAUT','BAS','DROITE','GAUCHE'),),
                 );

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_FISSURE=_F(GROUP_MA='FISSURE'),
                 );

MA=DEFI_GROUP(reuse = MA, MAILLAGE = MA, CREA_GROUP_NO=(_F(NOM='GAUCHE_2',GROUP_MA='GAUCHE_2',),),);
MA=DEFI_GROUP(reuse = MA, MAILLAGE = MA, CREA_GROUP_NO=(_F(NOM='GAUCHE_F',DIFFE=('GAUCHE_2','BORD_FIS'),),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               GRANDEUR_CARA=_F(LONGUEUR=2500.,
                                PRESSION=50.E6,),
                    AFFE=(_F(GROUP_MA=('MASSIF','BAS','HAUT','DROITE_1',
                            'DROITE_3','GAUCHE_1','GAUCHE_3',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='D_PLAN_HMS',),
                         _F(GROUP_MA=('FISSURE','GAUCHE_2','DROITE_2'),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='PLAN_JHMS',),));

####################################
#   ORIENTATION FISSURE
#
####################################

ORIEN_FI=AFFE_CARA_ELEM(MODELE=MO,
                        MASSIF=_F(GROUP_MA='FISSURE',
                                  ANGL_REP=(90,0,0,),),);

####################################
#   DEFINITION DU MATERIAU
#
####################################

MAT=DEFI_MATERIAU(ELAS=_F(E=200000000.0,
                          NU=0.25,
                          ALPHA=0.0,),
                       COMP_THM='LIQU_SATU',
                       THM_INIT=_F(PRE1=0.0,
                                   PORO=0.4055,),
                       THM_DIFFU=_F(RHO=1600.0,
                                    BIOT_COEF=0.9,
                                    PESA_X=0.0,
                                    PESA_Y=00.0,
                                    PESA_Z=00.0,
                                    PERM_IN=PERML,),
                       THM_LIQU=_F(RHO=1000.0,
                                   UN_SUR_K=1/3.E9,
                                   VISC=VISCOL,
                                   D_VISC_TEMP=DVISCOL,),
                       JOINT_BANDIS=_F(K=kni,
                                     DMAX=ouvm,
                                     GAMMA=2.,),
                         );

CM=AFFE_MATERIAU(MAILLAGE=MA,
                 AFFE=_F(TOUT='OUI',
                         MATER=MAT,),);

####################################
#   DEFINITION DES CHARGEMENTS
#
####################################

LIAISON=AFFE_CHAR_MECA(
  MODELE=MO,
  DDL_IMPO= (
    _F(GROUP_MA='BAS',DY=0),
    _F(GROUP_MA=('DROITE'),DX=0),
    ),
  );

FLUH=AFFE_CHAR_MECA(MODELE=MO,
                      FLUX_THM_REP=_F(GROUP_MA=('HAUT','BAS','DROITE_1','DROITE_3','DROITE_2'),
                                         FLUN_HYDR1=0.,),);

PWELL=AFFE_CHAR_MECA_F(MODELE=MO,
                          DDL_IMPO=(_F(GROUP_MA=('GAUCHE',),
                                      PRE1=UN,),
                                   ),);

COMP=AFFE_CHAR_MECA(
   MODELE=MO,
   PRES_REP=(
      _F(GROUP_MA=('HAUT','GAUCHE'),PRES=62000000.0),
     ),
   );

# DEFINITION DE L'EVOLUTION DU CHARGEMENT

FCT_MON = DEFI_FONCTION(
  NOM_PARA = 'INST',
  VALE     = (0,pini , tint ,pint, tfin, pfin ),
  INTERPOL   ='LIN'
                );

####################################
#   INITIALISATION DES CHAMPS
#
####################################

CHAMNO=CREA_CHAMP(MAILLAGE=MA,
                    OPERATION='AFFE',
                    TYPE_CHAM='NOEU_DEPL_R',
                    AFFE=(
                           _F(TOUT='OUI',
                             NOM_CMP=('DX','DY','PRE1','LH1',),
                             VALE=(0.,0.,pini,0.),),
                             ),);

SIGINIT=CREA_CHAMP(TYPE_CHAM='CART_SIEF_R',
                   OPERATION='AFFE',
                   MAILLAGE=MA,
                   AFFE=(_F(GROUP_MA='MASSIF',
                           NOM_CMP=('SIXX','SIYY','SIZZ','SIXY','SIXZ','SIYZ','SIP','M11','FH11X','FH11Y',),
                           VALE=(-sigyy,-sigyy,0.0,0.0,0.0,0.0,-pini,0.,0.,0.,),),
                         _F(GROUP_MA='FISSURE',
                           NOM_CMP=('SIGN','SITX','SIP','M11','FH11X','LH1P','LH1M','DPRE1P','DPRE1M'),
                           VALE=(-sigyy,0.0,-pini,ouvini*1000.,0.,0.,0.,0.,0.),),),);

CH_VARI=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='ELGA_VARI_R', MODELE=MO, INFO=2,
       AFFE=(
           _F( GROUP_MA = 'MASSIF',   NOM_CMP = ('V1','V2',  'V3'),
                                     VALE = ( 0 , 0, 0)),
           _F( GROUP_MA = 'FISSURE',   NOM_CMP = ('V1',     'V2',  'V3',),
                                     VALE = ( ouvini**2/12, 0., ouvini)),
                    ),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =LISTINST ),
                        ECHEC=_F(
                  EVENEMENT     = 'ERREUR',
                                         ACTION        = 'DECOUPE',
                                         SUBD_METHODE  = 'MANUEL',
                  SUBD_PAS  = 5,
                  SUBD_PAS_MINI = 1.E-10,
                  SUBD_NIVEAU=12,
                                 ),);

####################################
#   STAT_NON_LINE
#
####################################

U_1=STAT_NON_LINE(
  MODELE     = MO,
  CHAM_MATER = CM,
  CARA_ELEM  = ORIEN_FI,
  EXCIT      = (
                 _F(CHARGE = LIAISON),
                 _F(CHARGE = COMP),
                 _F(CHARGE = FLUH),
                 _F(CHARGE = PWELL,
                    FONC_MULT = FCT_MON),
               ),
  ETAT_INIT=_F(DEPL=CHAMNO,SIGM=SIGINIT,VARI=CH_VARI),
  COMP_INCR=(_F(RELATION='KIT_HM',
               RELATION_KIT=('ELAS','LIQU_SATU','HYDR_UTIL',),
               GROUP_MA=('MASSIF',),),
            _F(RELATION='KIT_HM',
               RELATION_KIT=('JOINT_BANDIS','LIQU_SATU','HYDR_UTIL',),
               GROUP_MA='FISSURE',),
                ),
  INCREMENT=_F(LIST_INST=DEFLIST,
               INST_FIN=tfin,),
  NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
  CONVERGENCE=_F(
                 ITER_GLOB_MAXI=10,),
  SOLVEUR = _F(METHODE = 'MUMPS', NPREC=-1),
  );

####################################
#   TESTS DE VALIDATION
#
####################################

# TEST PRE1 NS735 ET NS2273 A 7,5 ANS ET 20 ANS

TEST_RESU(RESU=_F(NUME_ORDRE=25,
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=U_1,
                  NOM_CHAM='DEPL',
                  NOEUD='NS735',
                  NOM_CMP='PRE1',
                  VALE_CALC= 4.01668209E+07,
                  VALE_REFE=3.9950000E7,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

