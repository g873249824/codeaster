                  NOM_CHAM='DEPL',
                  NOEUD='NS735',
                  NOM_CMP='PRE1',
                  VALE_CALC= 3.85578704E+07,
                  VALE_REFE=3.8450000E7,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=25,
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=U_1,
                  NOM_CHAM='DEPL',
                  NOEUD='NS2273',
                  NOM_CMP='PRE1',
                  VALE_CALC= 4.26712868E+07,
                  VALE_REFE=4.2680000E7,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=50,
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=U_1,
                  NOM_CHAM='DEPL',
                  NOEUD='NS2273',
                  NOM_CMP='PRE1',
                  VALE_CALC= 4.08799964E+07,
                  VALE_REFE=4.0980000E7,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

# TEST V1 NS2117 A 7,5 ANS ET 20 ANS

U_1=CALC_CHAMP(reuse=U_1,GROUP_MA='FISSURE',RESULTAT=U_1,CONTRAINTE=('SIEF_ELNO'),VARI_INTERNE=('VARI_ELNO'))


TEST_RESU(RESU=_F(NUME_ORDRE=25,
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=U_1,
                  NOM_CHAM='VARI_ELNO',
                  NOEUD='NS2273',
                  NOM_CMP='V3',
                  VALE_CALC= 2.20052210E-04,
                  VALE_REFE=2.2000000000000001E-4,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,
                  MAILLE='M1211',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=50,
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=U_1,
                  NOM_CHAM='VARI_ELNO',
                  NOEUD='NS2273',
                  NOM_CMP='V3',
                  VALE_CALC= 2.03354023E-04,
                  VALE_REFE=2.04E-4,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,
                  MAILLE='M1211',),
          )

# TEST FH11X NS2117 A 7,5 ANS ET 20 ANS

TEST_RESU(RESU=_F(NUME_ORDRE=25,
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=U_1,
                  NOM_CHAM='SIEF_ELNO',
                  NOEUD='NS2273',
                  NOM_CMP='FH11X',
                  VALE_REFE=-7.0189E-3,
                  VALE_CALC=-7.0189349999999996E-3,
                  PRECISION=1.E-2,
                  MAILLE='M1211',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=50,
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=U_1,
                  NOM_CHAM='SIEF_ELNO',
                  NOEUD='NS2273',
                  NOM_CMP='FH11X',
                  VALE_REFE=-5.3847E-3,
                  VALE_CALC=-5.3847130000000002E-3,
                  PRECISION=1.E-2,
                  MAILLE='M1211',),
          )

#-----------------------------------------------------------------------------------
# DONNEES POUR COURBES DOC V
#-----------------------------------------------------------------------------------

# PROFILS DE PRESSION

AXE_FISS=INTE_MAIL_2D(MAILLAGE=MA,
                    DEFI_SEGMENT=_F(  ORIGINE = ( 0.,  0., ),
                                       EXTREMITE = ( 2500.,  0., ),
                                       ),
                    PRECISION=0.0000000000001,
                                       );

V_X_01=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='PRE1 selon x',
                               RESULTAT=U_1,
                               NOM_CHAM='DEPL',
                               INST=(tint,tfin),
                               CHEMIN=AXE_FISS,
                               NOM_CMP=('PRE1',),),)

IMPR_TABLE(TABLE=V_X_01,
           FORMAT='TABLEAU_CROISE',
           NOM_PARA=('INST','COOR_X','PRE1'),
          );

# PROFILS D'OUVERTURE DE FISSURE

V_X_02=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='V1 selon x',
                               RESULTAT=U_1,
                               NOM_CHAM='VARI_ELNO',
                               INST=(tint,tfin),
                               CHEMIN=AXE_FISS,
                               NOM_CMP=('V3',),),);

IMPR_TABLE(TABLE=V_X_02,
           FORMAT='TABLEAU_CROISE',
           NOM_PARA=('INST','COOR_X','V3'),
          );

# DEBIT EN SORTIE DE FISSURE EN FONCTION DU TEMPS

V_X_03=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='FH11X1 selon t',
                               RESULTAT=U_1,
                               NOM_CHAM=('SIEF_ELNO',),

                               GROUP_NO='GAUCHE_F',
                               NOM_CMP=('FH11X',),),);

DEBJOUR=FORMULE(NOM_PARA=('FH11X',),
                 VALE='-FH11X*86400',);

V_X_03=CALC_TABLE(TABLE=V_X_03,
                  reuse=V_X_03,
                  ACTION=_F(OPERATION='OPER',
                            FORMULE=DEBJOUR,
                            NOM_PARA='DEBIT_JOUR',),
                 );

IMPR_TABLE(TABLE=V_X_03,
           FORMAT='TABLEAU',
           NOM_PARA=('INST','DEBIT_JOUR'),
          );

FIN();
