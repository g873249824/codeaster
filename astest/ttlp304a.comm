  _F( CHAM_GD = TEMP_12, NOEUD = 'N6',  NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
                            VALE = -16.1025, PRECISION = 5.E-2),
  _F( CHAM_GD = TEMP_12, NOEUD = 'N10', NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
