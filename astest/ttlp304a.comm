                   _F(    JUSQU_A = 4.30E3,   NOMBRE = 38),
                   _F(    JUSQU_A = 4.32E3,   NOMBRE = 1))
                  )

#
# RESOLUTION ------------------------------------------------------
#

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
                      ETAT_INIT=_F( VALE = -1.111E0),
                      INCREMENT=_F( LIST_INST = LR8),
                       CHAM_MATER=CHMAT,
                        CARA_ELEM=ESSAI,
                          EXCIT=_F( CHARGE = CHTH)
               )

