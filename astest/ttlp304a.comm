_F( CHAM_GD = TEMP_12, NOEUD = 'N66', NOM_CMP = 'TEMP', VALE = -16.9401, PRECISION = 1.E-2),
_F( CHAM_GD = TEMP_12, NOEUD = 'N70', NOM_CMP = 'TEMP', VALE = -16.6964, PRECISION = 1.E-2),
#
#                           ECART ABSOLU
