  _F( CHAM_GD = TEMP_12, NOEUD = 'N63', NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
                            VALE = -17.3991, PRECISION = 5.E-2),
  _F( CHAM_GD = TEMP_12, NOEUD = 'N66', NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
                            VALE = -16.9401, PRECISION = 5.E-2),
  _F( CHAM_GD = TEMP_12, NOEUD = 'N70', NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
                            VALE = -16.6964, PRECISION = 5.E-2))

    )

