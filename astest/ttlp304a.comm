_F( CHAM_GD = TEMP_12, NOEUD = 'N36', NOM_CMP = 'TEMP', VALE = -16.3269, PRECISION = 1.E-2),
_F( CHAM_GD = TEMP_12, NOEUD = 'N40', NOM_CMP = 'TEMP', VALE = -15.9049, PRECISION = 1.E-2),
#
#                           ECART ABSOLU
#
  _F( CHAM_GD = TEMP_12, NOEUD = 'N33', NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
                            VALE = -17.1218, PRECISION = 5.E-2),
  _F( CHAM_GD = TEMP_12, NOEUD = 'N36', NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
                            VALE = -16.3269, PRECISION = 5.E-2),
  _F( CHAM_GD = TEMP_12, NOEUD = 'N40', NOM_CMP = 'TEMP',   CRITERE = 'ABSOLU',
