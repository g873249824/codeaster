           REFERENCE='AUTRE_ASTER',
           PRECISION=1.0E-4,
           VALE_CALC=1.20588907396E+16,
           VALE_REFE=1.20598944073E+16,
           NOM_PARA='PRE1',
           TYPE_TEST='MAX',
           TABLE=DEP1,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=1.0E-4,
