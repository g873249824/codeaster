DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
####################################
#   OPERATIONS SUR LE MAILLAGE
#
####################################
MAIL=LIRE_MAILLAGE(FORMAT='MED',)
                  
MAIL=DEFI_GROUP( reuse=MAIL,
                 MAILLAGE=MAIL,
                 CREA_GROUP_NO=(
                      _F( GROUP_MA = 'BAS'),
                      _F( GROUP_MA = 'HAUT'))
                     )


MODELE=AFFE_MODELE(   MAILLAGE=MAIL,
                      AFFE=_F(  TOUT = 'OUI',
                      PHENOMENE = 'MECANIQUE',
                      MODELISATION = '3D_HM')
                     )

LS_N=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y-0.2');

FISS=DEFI_FISS_XFEM(MAILLAGE=MAIL,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LS_N,),
                    INFO=1,
                   );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELE,
                         FISSURE=FISS,
                         INFO=1,
                        );

####################################
#  LISTE DES INSTANTS DE CALCUL
####################################
LI=DEFI_LIST_REEL(   DEBUT=0.,INTERVALLE=(
                       _F(  JUSQU_A = 1.E0,  NOMBRE = 1),
                       _F(  JUSQU_A = 5.E0,  NOMBRE = 1),
                       _F(  JUSQU_A = 1.E1,  NOMBRE = 1),
                       _F(  JUSQU_A = 5.E1,  NOMBRE = 1),
                       _F(  JUSQU_A = 1.E2,  NOMBRE = 1),
                       _F(  JUSQU_A = 5.E2,  NOMBRE = 1),
                       _F(  JUSQU_A = 1.E3,  NOMBRE = 1),
                       _F(  JUSQU_A = 5.E3,  NOMBRE = 1),
                       _F(  JUSQU_A = 1.E4,  NOMBRE = 1))
                     )

ARCHI=DEFI_LIST_REEL(    DEBUT=0.,INTERVALLE=(
                       _F(  JUSQU_A = 1.E0,  NOMBRE = 1),
                       _F(  JUSQU_A = 5.E0,  NOMBRE = 1),
                       _F(  JUSQU_A = 1.E1,  NOMBRE = 1),
                       _F(  JUSQU_A = 5.E1,  NOMBRE = 1),
                       _F(  JUSQU_A = 1.E2,  NOMBRE = 1),
                       _F(  JUSQU_A = 5.E2,  NOMBRE = 1),
                       _F(  JUSQU_A = 1.E3,  NOMBRE = 1))
                 )

############################################

VISCOLIQ=DEFI_CONSTANTE(    VALE=1.E-3)
DVISCOL=DEFI_CONSTANTE(    VALE=0.)
DCONDLI = DEFI_CONSTANTE ( VALE = 0.   )
CONDGAZ = DEFI_CONSTANTE ( VALE = 0.02 )
DCONDGA = DEFI_CONSTANTE ( VALE = 0.   )

PERMGAZ=DEFI_CONSTANTE(    VALE=1.)
DPERGSA=DEFI_CONSTANTE(    VALE=0.)
DPERGPG=DEFI_CONSTANTE(    VALE=0.)

KINT=DEFI_CONSTANTE(    VALE=1.E-17)
UN=DEFI_CONSTANTE(    VALE=1.)
ZERO=DEFI_CONSTANTE(    VALE=0.)


THMALP1 = DEFI_CONSTANTE(VALE=0.000100)

MATERIAU=DEFI_MATERIAU(     THM_LIQU=_F(
   RHO = 1000.,
   UN_SUR_K = 1.0E-17,
   ALPHA = THMALP1,
   CP = 2.85E6,
   VISC = UN,
   D_VISC_TEMP = ZERO,),
                              THM_DIFFU=_F(
   RHO = 1.6E3,
   BIOT_COEF = 1.,
   SATU_PRES = UN,
   D_SATU_PRES = ZERO,
   PESA_X = 0.,
   PESA_Y = 0.,
   PESA_Z = 0.,
   PERM_IN = KINT,),
                              THM_INIT=_F(
   PRE1 = 0.,
   PORO = 0.4,),
                               COMP_THM = 'LIQU_SATU',
                               ELAS=_F(
   E = 225.E6,
   NU = 0.,
   RHO = 2000.,
   ALPHA = 8.E-6)
   )

CHMAT0=AFFE_MATERIAU(   MAILLAGE=MAIL,
                        MODELE=MODELEK,
                        AFFE=_F( TOUT = 'OUI',
                        MATER = MATERIAU))

CH_FLU1=AFFE_CHAR_MECA(   MODELE=MODELEK,
                          FLUX_THM_REP=(
                          _F(
                                  GROUP_MA = 'HAUT',
                                  FLUN_HYDR1 = 0.005),
                          _F(
                                  GROUP_MA = 'BAS',
                                  FLUN_HYDR1 = 0.0),),
                                   );


CHARHO=AFFE_CHAR_MECA(   MODELE=MODELEK,
                          DDL_IMPO=(_F(  TOUT = 'OUI',
                                    DX = 0.,
                                    DZ = 0.,
                                    DY = 0.,
                                    H1X = 0.,
                                    H1Y = 0.,
                                    H1PRE1=0.,
                                    H1Z = 0.,),),
                                   )


RESU0=STAT_NON_LINE(
                MODELE=MODELEK,
                CHAM_MATER=CHMAT0,
                EXCIT=(
                             _F(
                CHARGE = CHARHO),
                             _F(
                CHARGE = CH_FLU1)),
                         COMPORTEMENT=_F( PARM_THETA=1.,
                RELATION = 'KIT_HM',
                RELATION_KIT = ('ELAS','LIQU_SATU','HYDR_UTIL',),
                ITER_INTE_MAXI = 5),
                         INCREMENT=_F(
                LIST_INST = LI),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 121),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 120),
                         ARCHIVAGE=_F(
                LIST_INST = ARCHI)
              )

MA_XFEM=POST_MAIL_XFEM(MODELE = MODELEK)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_HM',),),
                    );
       
RES_X1=POST_CHAM_XFEM(MODELE_VISU = MOD_VISU,
                      RESULTAT    = RESU0,
                     );

#IMPR_RESU(FORMAT='MED',UNITE=80, RESU=_F(RESULTAT=RES_X1,));    



DEP1=POST_RELEVE_T(ACTION=_F(INTITULE='P_SUP',
                             NOEUD=('NO1','NO3','NO6','NO8',),
                             RESULTAT=RESU0,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=8,
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',
                            ),
                  );

DEP2=POST_RELEVE_T(ACTION=_F(INTITULE='P_INF',
                             NOEUD=('NO13','NO15','NO18','NO20',),
                             RESULTAT=RESU0,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=8,
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',
                            ),
                  );

IMPR_TABLE(TABLE=DEP1,);


IMPR_TABLE(TABLE=DEP2,);

