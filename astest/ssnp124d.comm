                           PRES_REP = _F(  MAILLE = 'M3',
                                           PRES   = 2.0E6)
                        )

CHAR_UM4 = AFFE_CHAR_MECA( MODELE   = MODELE,
                           PRES_REP = _F(  MAILLE = 'M4',
                                           PRES = 2.0E6)
