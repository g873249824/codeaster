#       (AXIS_FOURIER ET PESANTEUR)
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(FORMAT='MED',   )

#
# DEFINITION DES GROUPES DE NOEUDS
MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'ENCAST'),
                       _F(  GROUP_MA = 'CONT_PR'),
                       _F(  GROUP_MA = ( 'B',  'SECT_MED', )))
                   )

#
# DEFINITION DU MODELE
MODELE=AFFE_MODELE(    MAILLAGE=MAIL,
                         AFFE=_F(  TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS_FOURIER')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 2.1E11,  NU = 0.3,
                                 ALPHA = 1.E-5,  RHO = 7800.)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(    MAILLAGE=MAIL,
                          AFFE=_F(  TOUT = 'OUI',
                                 MATER = ACIER)  )

F_Y=DEFI_FONCTION(    NOM_PARA='Y',
                        VALE=( 0.,     10000.,
                               0.24,   10000., )
                    )

# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
CLIM=AFFE_CHAR_MECA(    MODELE=MODELE,
                            DDL_IMPO=_F(  GROUP_NO = 'ENCAST',
                                       DX = 0.,  DY = 0.,  DZ = 0.)  )

#   GRAVITE
CHAR_GR=AFFE_CHAR_MECA(     MODELE=MODELE,
                              PESANTEUR=_F(GRAVITE=13.873,
                                           DIRECTION=(1.,  0.,  1.,),),
                          )

#   PRESSION
CHAR_PR=AFFE_CHAR_MECA_F(   MODELE=MODELE,
                              PRES_REP=_F(  GROUP_MA = 'CONT_PR',
                                         PRES = F_Y)
                          )

# RESOLUTION DES 2 CAS DE CHARGE INDEPENDANTS
DEF0=MACRO_ELAS_MULT(MODELE=MODELE,
                     CHAM_MATER=CHMAT,
                     CHAR_MECA_GLOBAL=CLIM,CAS_CHARGE=(
                     _F(MODE_FOURIER = 1,
                        TYPE_MODE = 'SYME',
                        CHAR_MECA = CHAR_GR,
                        OPTION = 'SANS' ),
                     _F(MODE_FOURIER = 1,
                        TYPE_MODE = 'SYME',
                        CHAR_MECA = CHAR_PR,
                        OPTION = 'SANS' ))
                     )

U0_GR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                      RESULTAT=DEF0,
                      NUME_ORDRE=1,
                      NOM_CHAM='DEPL'
                  )

U0_PR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                      RESULTAT=DEF0,
                      NUME_ORDRE=2,
                      NOM_CHAM='DEPL'
                  )

#
# CALCUL DES CONTRAINTES

DEF0=CALC_CHAMP(reuse=DEF0,RESULTAT=DEF0,CONTRAINTE=('SIGM_ELNO'))


SIG0_GR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELNO_SIEF_R',
                      RESULTAT=DEF0,
                      NUME_ORDRE=1,
                      NOM_CHAM='SIGM_ELNO'
                  )

SIG0_PR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELNO_SIEF_R',
                      RESULTAT=DEF0,
                      NUME_ORDRE=2,
                      NOM_CHAM='SIGM_ELNO'
                  )

# RECOMBINAISONS DE FOURIER EN DEPLACEMENTS ET CONTRAINTES
# POUR TETA = 0. (GRAVITE) ET TETA = 0. (PRESSION)
# POUR CHAQUE CAS DE CHARGE SEPAREMENT

RU0_GR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='DEPL',
           AFFE=_F( CHAM_GD=U0_GR,NUME_MODE=1,TYPE_MODE='SYME'),)

RU0_PR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='DEPL',
           AFFE=_F( CHAM_GD=U0_PR,NUME_MODE=1,TYPE_MODE='SYME'),)

RSIG0_GR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='SIGM_ELNO',
           AFFE=_F( CHAM_GD=SIG0_GR,NUME_MODE=1,TYPE_MODE='SYME'),)

RSIG0_PR=CREA_RESU(OPERATION='AFFE',TYPE_RESU='FOURIER_ELAS',NOM_CHAM='SIGM_ELNO',
           AFFE=_F( CHAM_GD=SIG0_PR,NUME_MODE=1,TYPE_MODE='SYME'),)

CO_FOUR1=COMB_FOURIER(   RESULTAT=RU0_GR,
                         NOM_CHAM=( 'DEPL',),
                         ANGLE=( 0., ),
                        )

CO_FOUR2=COMB_FOURIER(   RESULTAT=RU0_PR,
                         NOM_CHAM=( 'DEPL',),
                         ANGLE=( 0., ),
                        )

CO_FOUR3=COMB_FOURIER(   RESULTAT=RSIG0_GR,
                         NOM_CHAM=( 'SIGM_ELNO',),
                         ANGLE=( 0., ),
                        )

CO_FOUR4=COMB_FOURIER(   RESULTAT=RSIG0_PR,
                         NOM_CHAM=( 'SIGM_ELNO',),
                         ANGLE=( 0., ),
                        )

U_GR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='NOEU_DEPL_R',
             RESULTAT=CO_FOUR1,ANGLE=0.,NOM_CHAM='DEPL')

U_PR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='NOEU_DEPL_R',
             RESULTAT=CO_FOUR2,ANGLE=0.,NOM_CHAM='DEPL')

SIG_GR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='ELNO_SIEF_R',
             RESULTAT=CO_FOUR3,ANGLE=0.,NOM_CHAM='SIGM_ELNO')

SIG_PR=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='ELNO_SIEF_R',
             RESULTAT=CO_FOUR4,ANGLE=0.,NOM_CHAM='SIGM_ELNO')


# COMBINAISON DES 2 CAS DE CHARGE

CO_FOUR5=COMB_FOURIER(   RESULTAT=DEF0,
                         NOM_CHAM=( 'DEPL', 'SIGM_ELNO',),
                         ANGLE=( 0., ),
                        )

U=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='NOEU_DEPL_R',
             RESULTAT=CO_FOUR5,ANGLE=0.,NOM_CHAM='DEPL')


SIG=CREA_CHAMP(OPERATION='EXTR',TYPE_CHAM='ELNO_SIEF_R',
             RESULTAT=CO_FOUR5,ANGLE=0.,NOM_CHAM='SIGM_ELNO',)

#
TEST_RESU(CHAM_NO=(_F(NOEUD='N249',
                      NOM_CMP='DX',
                      PRECISION=1.E-2,
                      CHAM_GD=U_GR,
                      VALE_CALC=3.54105397E-07,
                      VALE_REFE=3.566E-07,
                      REFERENCE='ANALYTIQUE',),
                   _F(NOEUD='N87',
                      NOM_CMP='DX',
                      PRECISION=1.E-2,
                      CHAM_GD=U_GR,
                      VALE_CALC= 3.53584190E-07,
                      VALE_REFE=3.566E-07,
                      REFERENCE='ANALYTIQUE',),
                   _F(NOEUD='N249',
                      CRITERE='ABSOLU',
                      NOM_CMP='DY',
                      CHAM_GD=U_GR,
                      VALE_REFE=0.0,
                      VALE_CALC=0.0,
                      REFERENCE='ANALYTIQUE',),
                   _F(NOEUD='N87',
                      CRITERE='ABSOLU',
                      NOM_CMP='DY',
                      CHAM_GD=U_GR,
                      VALE_REFE=0.0,
                      VALE_CALC=0.0,
                      REFERENCE='ANALYTIQUE',),
                   ),
          )

