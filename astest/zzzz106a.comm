DEBUT(CODE=_F(NOM='ZZZZ106A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(INFO=2, )
MA1=LIRE_MAILLAGE(UNITE=14 )

MA=DEFI_GROUP(  reuse=MA,   MAILLAGE=MA,
                CREA_GROUP_MA=_F(  NOM = 'GXXXXXX_TOUT', TOUT = 'OUI'))

MA=DEFI_GROUP(  reuse=MA,   MAILLAGE=MA,
                CREA_GROUP_MA=( _F(  NOM = 'GXXXXXX_GD0',
                                     OPTION = 'SPHERE',
                                     GROUP_NO_CENTRE = 'GXXXXXX_GRN1',
                                     RAYON = 4.0,
                                     CRIT_NOEUD ='AU_MOINS_UN',),
                                _F(  NOM = 'GXXXXXX_GD1',
                                     OPTION = 'SPHERE',
                                     GROUP_NO_CENTRE = 'GXXXXXX_GRN1',
                                     RAYON =  4.0,
                                     CRIT_NOEUD ='TOUS',),
                                _F(  NOM = 'GXXXXXX_GD2',
                                   OPTION = 'SPHERE',
                                   GROUP_NO_CENTRE = 'GXXXXXX_GRN1',
                                   RAYON = 4.0,
                                   CRIT_NOEUD ='MAJORITE',),
                                _F(  NOM = 'GXXXXXX_GC1',
                                     OPTION = 'CYLINDRE',
                                     NOEUD_CENTRE = 'N1',
                                     RAYON = 4.,
                                     ANGL_NAUT = (0.,90.,),
                                     CRIT_NOEUD ='AU_MOINS_UN',),
                                _F(  NOM = 'GXXXXXX_GC2',
                                       OPTION = 'CYLINDRE',
                                       NOEUD_CENTRE = 'N1',
                                       RAYON = 4.,
                                       ANGL_NAUT = (0.,90.,),
                                       CRIT_NOEUD ='TOUS',),
                               _F(  NOM = 'GXXXXXX_GC3',
                                       OPTION = 'CYLINDRE',
                                       NOEUD_CENTRE = 'N1',
                                       RAYON = 4.,
                                       ANGL_NAUT = (0.,90.,),
                                       CRIT_NOEUD ='MAJORITE',),
                                _F(  NOM = 'GXXXXXX_GT1',
                                       OPTION = 'BANDE',
                                       NOEUD_CENTRE = 'N1',
                                       DIST = 4.,
                                       VECT_NORMALE = (0.,1.,0.,),
                                       CRIT_NOEUD ='AU_MOINS_UN',),
                                _F(  NOM = 'GXXXXXX_GT2',
                                       OPTION = 'BANDE',
                                       NOEUD_CENTRE = 'N1',
                                       DIST = 4.,
                                       VECT_NORMALE = (0.,1.,0.,),
                                       CRIT_NOEUD ='TOUS',),
                                _F(  NOM = 'GXXXXXX_GT3',
                                       OPTION = 'BANDE',
                                       NOEUD_CENTRE = 'N1',
                                       DIST = 4.,
                                       VECT_NORMALE = (0.,1.,0.,),
                                       CRIT_NOEUD ='MAJORITE',),
                                _F(  NOM = 'GXXXXXX_GM4',
                                       OPTION = 'FACE_NORMALE',
                                       VECT_NORMALE = (0.,0.,1.,)),),
