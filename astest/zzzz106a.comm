#--------------------------------------------------------------------
#
#   ON TESTE LA NULLITE DES CHAMPS DE DEPLACEMENTS
#

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU3,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU4,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU5,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU6,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU7,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU8,
                   NOM_CHAM='DEPL',
                   NOEUD='N18',
                   NOM_CMP='DZ',
                   VALE_CALC= 0.0E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-10,),
                ),
          )

FIN()
