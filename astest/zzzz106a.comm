                                _F(  NOM = 'GXXXXXX_GN1',
                                       OPTION = 'ENV_SPHERE',
                                       NOEUD_CENTRE = 'N1',
                                       RAYON = 2.5,
                                       PRECISION = 0.1),
                                _F(  NOM = 'GXXXXXX_GN2',
                                       OPTION = 'ENV_CYLINDRE',
                                       NOEUD_CENTRE = 'N1',
                                       RAYON = 2.5,
                                       VECT_NORMALE = (0.,0.,1.,),
                                       PRECISION = 0.1),
                                _F(  NOM = 'GXXXXXX_GN3',
                                       OPTION = 'PLAN',
                                       NOEUD_CENTRE = 'N14',
                                       VECT_NORMALE = (1.,0.,0.,),
                                       PRECISION = 0.1),
                                ))

# -------  VALIDATION DE : DEFI_GROUP / INCLUSION :

# le group_ma ABCD de MA1 ne contient qu'1 quadrangle (ABCD) tel que
# seuls 2 triangles TR11A et TR11B y sont totalement inclus :
DEFI_GROUP(reuse=MA,   MAILLAGE=MA,
             CREA_GROUP_NO=  _F(  NOM = 'GXXXXXX_TR11AB', OPTION = 'INCLUSION',
                                  MAILLAGE_INCL = MA1,
                                  GROUP_MA_INCL = 'GXXXXXX_ABCD',
                                  GROUP_MA      = 'GXXXXXX_CARRE11',
                                  CAS_FIGURE    = '2D'))

DEFI_GROUP(reuse=MA,   MAILLAGE=MA,
            CREA_GROUP_MA=_F( NOM = 'GXXXXXX_TR11AB',OPTION='APPUI',
                              TYPE_APPUI='TOUT', GROUP_NO='GXXXXXX_TR11AB',))


# -------  VALIDATION DE : DEFI_GROUP / DETR_GROUP_XX -----------

MA=DEFI_GROUP(  reuse=MA,   MAILLAGE=MA,
                DETR_GROUP_NO=(_F(NOM=('GXXXXXX_GN1','GXXXXXX_GN32'),),
                               _F(NOM=('GXXXXXX_GN3','GXXXXXX_GN58'),),),
                DETR_GROUP_MA=_F(NOM=('GXXXXXX_GM1','GXXXXXX_GM3','GXXXXXX_GM58','GXXXXXX_GM2'),),
