DEBUT(CODE=_F(NOM='SSLS133C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#YG : Module d'Young, L: longueur, b: largeur, h1: epaisseur initiale, 
#h12: epaisseur a mi longueur, h2: epaisseur finale, F: effort

YG=2.E11
L=100.
F=1.

MAT=DEFI_MATERIAU(ELAS=_F(E=YG,
                          NU=0.0,),);

b=100.
h1=10.
h12=6.25
h2=5. 

MAIL31=LIRE_MAILLAGE(FORMAT='MED');

MAIL31=MODI_MAILLAGE(reuse=MAIL31,MAILLAGE=MAIL31,
ORIE_SHB=_F(GROUP_MA='GEOTOT'),)

MAIL31=DEFI_GROUP(reuse =MAIL31, MAILLAGE=MAIL31,
                CREA_GROUP_NO=(_F(GROUP_MA='SUENCAS',),
                               _F(GROUP_MA='SUDEPIM',),
                               _F(GROUP_MA='LI15',),
                               _F(GROUP_MA='LI21',),
                               _F(GROUP_MA='LI87',),),);

CHMAT31=AFFE_MATERIAU(MAILLAGE=MAIL31,
                  AFFE=_F(TOUT='OUI',MATER=MAT,),);

MODMEC31=AFFE_MODELE(MAILLAGE=MAIL31,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB8',),);


CHARG31=AFFE_CHAR_MECA(MODELE=MODMEC31,
                      DDL_IMPO=(_F(GROUP_MA=('SUENCAS'),
                                   DX=0.0,),
                                _F(GROUP_MA='LI15',
                                   DY=0.0,),
                                _F(GROUP_MA='LI21',
                                   DZ=0.0,),),
                      FORCE_ARETE=(_F(GROUP_MA='LI87',
                                      FZ=F/b,),),
                           );

RESU31=MECA_STATIQUE(MODELE=MODMEC31,
                    CHAM_MATER=CHMAT31,
                    EXCIT=_F(CHARGE=CHARG31,),
                    );

RESU31=CALC_ELEM(reuse=RESU31, RESULTAT=RESU31,

                    OPTION=('SIEF_ELNO','EQUI_ELGA_SIGM','EQUI_ELNO_SIGM'),
                    );


#Deplacements en x = L, Calcul exacte effectue sous Maple
Uz=.4712388984e-9;


TEST_RESU(RESU=(
            _F(  RESULTAT = RESU31, NOM_CHAM='DEPL', PRECISION = 9.E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = Uz),

            _F(   RESULTAT = RESU31, NOM_CHAM='DEPL',  PRECISION = 9.E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = Uz),
              ),)

TEST_RESU(RESU=(
            _F(  RESULTAT = RESU31, NOM_CHAM='DEPL', REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = 5.1212226115707e-10),

            _F(   RESULTAT = RESU31, NOM_CHAM='DEPL', REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = 5.1212226115597e-10),
              ),)

FIN()
