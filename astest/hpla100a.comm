              GROUP_NO = 'D',   NOM_CMP = 'SIYY',  VALE = 8.00000E-4),

            _F(  RESULTAT = DEPLROT,  CRITERE = 'RELATIF',   PRECISION = 2.E-5,
            NUME_ORDRE = 1,     MAILLE = 'M1',  NOM_CHAM = 'SIGM_ELNO_DEPL',
              GROUP_NO = 'A',    NOM_CMP = 'SIYY',  VALE = 0.99488E-3),
