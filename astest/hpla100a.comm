#PRE_GIBI()

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

MOTHER=AFFE_MODELE(MAILLAGE=MA,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='THERMIQUE',
                           MODELISATION='PLAN',),);

MAT=DEFI_MATERIAU(ELAS=_F(E=2.0E+5,
                          NU=0.3,
                          RHO=8.E-6,
                          ALPHA=1.E-5,),);

MATHER=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,
                             RHO_CP=0.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

CHMATHE=AFFE_MATERIAU(MAILLAGE=MA,
                      AFFE=_F(TOUT='OUI',
                              MATER=MATHER,),);

# ------------------------------------------------------------------

CHTHE=AFFE_CHAR_THER(MODELE=MOTHER,
                     TEMP_IMPO=(_F(GROUP_NO='BC',
                                   TEMP=0.5,),
                                _F(GROUP_NO='DA',
                                   TEMP=-0.5,),),);

CHTH2=AFFE_CHAR_THER(MODELE=MOTHER,
                     TEMP_IMPO=(_F(GROUP_NO='BC',
                                   TEMP=0.1,),
                                _F(GROUP_NO='DA',
                                   TEMP=0.1,),),);

TEMPLIN=THER_LINEAIRE(MODELE=MOTHER,
                      CHAM_MATER=CHMATHE,
                      EXCIT=_F(CHARGE=CHTHE,),);

TEMPLI2=THER_LINEAIRE(MODELE=MOTHER,
                      CHAM_MATER=CHMATHE,
                      EXCIT=_F(CHARGE=CHTH2,),);

# ------------------------------------------------------------------

CHPES=AFFE_CHAR_MECA(MODELE=MO,
                     PESANTEUR=_F(GRAVITE=10.0,
                                  DIRECTION=(0.,-1.,0.,),),
                     DDL_IMPO=_F(GROUP_NO='F',
                                 DY=0.,),
                     PRES_REP=_F(GROUP_MA='HAUT',
                                 PRES=-8.E-4,),);

CHROT=AFFE_CHAR_MECA(MODELE=MO,
                     ROTATION=_F(GROUP_MA='ZONE1',
                                 MAILLE=
                                 ('M17','M18','M19','M20','M21','M22','M23','M24','M25',
                                          'M26','M27','M28','M29','M30','M31','M32',),
                                 VITESSE=1.,
                                 AXE=(0.,1.,0.,),),
                     DDL_IMPO=(_F(GROUP_NO='BAS',
                                  DY=0.,),
                               _F(GROUP_NO='HAUT',
                                  DY=0.,),),);

IMPR_CO(CONCEPT=_F(NOM=CHROT,),);

CHDIL=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=_F(GROUP_NO='TOUT',
                                 DY=0.,),);

CHDI2=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='BAS',
                                  DY=0.,),
                               _F(GROUP_NO='HAUT',
                                  DY=0.,),),);

CHMTM=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),
                    AFFE_VARC=_F(TOUT='OUI',
                                 NOM_VARC='TEMP',
                                 EVOL=TEMPLIN,
                                 NOM_CHAM='TEMP',
                                 VALE_REF=0.0,),);

CHMTM2=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(TOUT='OUI',
                             MATER=MAT,),
                     AFFE_VARC=_F(TOUT='OUI',
                                  NOM_VARC='TEMP',
                                  EVOL=TEMPLI2,
                                  NOM_CHAM='TEMP',
                                  VALE_REF=0.0,),);

# ------------------------------------------------------------------

DEPLPES=MECA_STATIQUE(MODELE=MO,
                      CHAM_MATER=CHMAT,
                      EXCIT=_F(CHARGE=CHPES,),);

DEPLPES=CALC_ELEM(reuse =DEPLPES,
                  RESULTAT=DEPLPES,
                  OPTION=('SIGM_ELNO','EPME_ELNO','EPSI_ELNO',),);

DEPLROT=MECA_STATIQUE(MODELE=MO,
                      CHAM_MATER=CHMAT,
                      EXCIT=_F(CHARGE=CHROT,),);

DEPLROT=CALC_ELEM(reuse =DEPLROT,
                  RESULTAT=DEPLROT,
                  OPTION=('SIGM_ELNO','EPME_ELNO','EPSI_ELNO',),);

DEPLDIL=MECA_STATIQUE(MODELE=MO,
                      CHAM_MATER=CHMTM,
                      EXCIT=_F(CHARGE=CHDIL,),);

DEPLDIL=CALC_ELEM(reuse =DEPLDIL,
                  RESULTAT=DEPLDIL,
                  OPTION=('SIGM_ELNO','EPME_ELNO','EPSI_ELNO',),);

DEPLDI2=MECA_STATIQUE(MODELE=MO,
                      CHAM_MATER=CHMTM2,
                      EXCIT=_F(CHARGE=CHDI2,),);

DEPLDI2=CALC_ELEM(reuse =DEPLDI2,
                  RESULTAT=DEPLDI2,
                  OPTION=('SIGM_ELNO','EPME_ELNO','EPSI_ELNO',),);

#---------------------------------------------------------------------

T_POT=POST_ELEM(ENER_POT=_F(TOUT='OUI',),
                CHARGE=CHDIL,
                RESULTAT=DEPLDIL,);

IMPR_TABLE(TABLE=T_POT,);

