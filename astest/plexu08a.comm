DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),IMPR_MACRO='OUI',ERREUR=_F(ERREUR_F='EXCEPTION',),
      DEBUG=_F(HIST_ETAPE='OUI'));

MPa = 1000000.0;


# ------------------- Lecture du maillage

MAILLAGE=LIRE_MAILLAGE(    UNITE=20 )
DEFI_GROUP(MAILLAGE = MAILLAGE, reuse=MAILLAGE, CREA_GROUP_MA=_F(MAILLE ='SG001003', NOM = 'GRSG3'))
#

MODELE=AFFE_MODELE(
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = ('GROUPE_____VOILE',),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'Q4GG'),
                     _F(  GROUP_MA = 'GROUPE__CABLE001',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE')
                            ) )

#
EP=6.0E-2
CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       COQUE=(_F(  GROUP_MA = 'GROUPE_____VOILE',
                               EPAIS = EP,
                               COQUE_NCOU = 1,
                               MODI_METRIQUE = 'NON',
                               ),
                               ),
                       BARRE=_F(  GROUP_MA = 'GROUPE__CABLE001',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 1.5E-4, ))
                               )



# ------------------- Definition des materiaux

BETON = DEFI_MATERIAU(ELAS=_F(
                    E = 30000*MPa,
                    NU = 0.3,
                    RHO = 2500,
                   ),
                    BPEL_BETON=_F()
                  );
FSIGM=DEFI_FONCTION(
               NOM_PARA='EPSI',
               PROL_DROITE='LINEAIRE',
               VALE=(
               5E-6,    1.E6,
               5E-4,    1.E6,
               )
             )

ACIER = DEFI_MATERIAU(ELAS=_F(
                    E = 200000*MPa,
                    NU = 0.3,
                    RHO = 7500,
                   ),
                   TRACTION=_F(SIGM=FSIGM),
                   BPEL_ACIER=_F(  F_PRG = 1.77E+9,
                              FROT_COURB = 0.,
                              FROT_LINE = 0.)
                  );

CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAILLAGE,AFFE=(
                       _F(  GROUP_MA = ('GROUPE_____VOILE',),
                              MATER = BETON),
                       _F(  GROUP_MA = 'GROUPE__CABLE001',
                              MATER = ACIER)
                              )
                              )



CABLEPR=DEFI_CABLE_BP(            MODELE=MODELE,
                                  CHAM_MATER=CHMATER,
                                   CARA_ELEM=CARAELEM,
                              GROUP_MA_BETON='GROUPE_____VOILE',
                                  DEFI_CABLE=_F(
                               GROUP_MA = 'GROUPE__CABLE001',
                               NOEUD_ANCRAGE = ( 'NC001001',  'NC001005', ),),
                               TYPE_ANCRAGE = ( 'ACTIF',   'ACTIF',  ),
                                TENSION_INIT=2.0E+5,
                               RECUL_ANCRAGE=0. )

#

CHARMECA=AFFE_CHAR_MECA(
                      MODELE=MODELE,DDL_IMPO=(
                    _F(  GROUP_NO='ENCAST',
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,
                               DRX = 0.,
                               DRY = 0.,
                               DRZ = 0.),
                               ),
                RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'NON',
                               RELA_CINE = 'OUI'),
                        INFO=1 )

CHARPRES=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                FORCE_COQUE=_F(PRES = -1.E7,GROUP_MA = 'GROUPE_____VOILE',),
                        INFO=1 )
#
# -----------------------------------------------------------
# DEFINITION DE LA FORCE D'IMPACT LINEAIRE
# -----------------------------------------------------------

FONCCB=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,    0,
                           1e-5,  2e-4,
                           2e-4,  0.01,
                           5e-4,  0.1,
                           1e-3,  0.25,
                           2e-3,  0.7,
                           3e-3,  0.0,
                           ),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     );
TAB1 = CO('TAB1')

U = CALC_EUROPLEXUS(
   MODELE=MODELE,
   CHAM_MATER=CHMATER,
   COMPORTEMENT =(_F(
                  RELATION = 'ELAS',
                  GROUP_MA = ('GROUPE_____VOILE'),
                  ),
                  _F(
                  RELATION = 'VMIS_ISOT_TRAC',
                  #RELATION = 'ELAS',
                  GROUP_MA = ('GROUPE__CABLE001',),
                 ),
              ),
   CARA_ELEM=CARAELEM,
   EXCIT=(_F(CHARGE=CHARPRES,FONC_MULT=FONCCB,),
          _F(CHARGE=CHARMECA,),
         ),
   LANCEMENT ='OUI',
   CALCUL = _F(TYPE_DISCRETISATION  ='UTIL',
               INST_INIT = 0,
               INST_FIN  = 2e-3,
               NMAX      = 100,
               PASFIX    = 2e-5,
               ),
   ARCHIVAGE   = _F(PAS_NBRE=10,),
   PAS_NBRE_COURBE=1,
   TABLE_COURBE    = TAB1,
   COURBE = (_F(
           NOM_CHAM = 'SIEF_ELGA',
           NOM_CMP  = 'N',
           NUM_GAUSS = 1,
           GROUP_MA = 'GRSG3',
           NOM_COURBE = 'N_GRSG3'
           ),
           _F(
           NOM_CHAM = 'VARI_ELGA',
           NOM_CMP  = 'V3',
           NUM_GAUSS = 1,
           GROUP_MA = 'GRSG3',
           NOM_COURBE = 'V3_GRSG3'
           ),
   ),
   );
IMPR_TABLE(TABLE=TAB1)

#IMPR_RESU(RESU=_F(RESULTAT=U,NUME_ORDRE=(20,50,100),NOM_CHAM=('DEPL','VITE','ACCE','SIEF_ELGA')))
IMPR_RESU(RESU=_F(RESULTAT=U,NOM_CHAM=('VARI_ELGA'), GROUP_MA='GRSG3'))


TEST_TABLE(TABLE=TAB1,
           NOM_PARA='N_GRSG3',
           TYPE_TEST='MAX',
           REFERENCE = 'ANALYTIQUE',
           VALE_REFE = 1.E6,
           VALE_CALC = 1.E6,
           )

TEST_RESU(RESU=_F(NUME_ORDRE = 80,
                  RESULTAT=U,
                  NOM_CHAM='SIEF_ELGA',
                  MAILLE='SG001003',
                  NOM_CMP='N',
                  POINT = 1,
                  VALE_CALC=150.0,
                  ),
          )


TEST_RESU(RESU=_F(NUME_ORDRE = 90,
                  RESULTAT=U,
                  NOM_CHAM='SIEF_ELGA',
                  MAILLE='SG001003',
                  NOM_CMP='N',
                  POINT = 1,
                  VALE_CALC=150.0,
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE = 100,
                  RESULTAT=U,
                  NOM_CHAM='SIEF_ELGA',
                  MAILLE='SG001003',
                  NOM_CMP='N',
                  POINT = 1,
                  VALE_CALC=150.,
                  ),
          )
# référence prise dans la table
TEST_RESU(RESU=_F(NUME_ORDRE = 100,
                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=U,
                  NOM_CHAM='VARI_ELGA',
                  MAILLE='SG001003',
                  NOM_CMP='V1',
                  POINT = 1,
                  VALE_CALC=1.62139511288E-05,
                  VALE_REFE=1.62140E-05,
                  ),
          )



FIN()
