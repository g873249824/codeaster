# SSLS112A
# PLAQUE DE STRATIFIE RECTANGULAIRE EXCENTREE
# CALCUL 1 : 1 QUADRICOUCHE , CE CALCUL SERT DE REFERENCE
# CALCUL 2 : 2 BICOUCHES EXCENTRES
# MODELISATION DKQ
#             67 NOEUDS  50 MAILLES QUAD4
#----------------------------------------------------------------------
#POINTS:  A4                          A3
#           -------------------------
#           -                       -
#           -                       -
#           -                       -
#           -                       -
#           -------------------------
#         A1                          A2
#
# ______________________________________________________________________
#
#


DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
#
#

MA1=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED',);

MA1=DEFI_GROUP(reuse =MA1,
               MAILLAGE=MA1,
               CREA_GROUP_MA=_F(NOM='TOUT',
                                TOUT='OUI',),);

MA=CREA_MAILLAGE(MAILLAGE=MA1,
                 CREA_MAILLE=_F(NOM='PLAQ2',
                                  GROUP_MA='PLAQ1',
                                  PREF_MAILLE='MAIL',),);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(GROUP_MA='PLAQ1',
                                NOM='PLAQ1',),
                             _F(GROUP_MA='PLAQ2',
                                NOM='PLAQ2',),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='DKT',),);

MO1=AFFE_MODELE(MAILLAGE=MA,
                AFFE=_F(GROUP_MA='PLAQ1',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='DKT',),);

EP=AFFE_CARA_ELEM(MODELE=MO,
                  COQUE=(_F(GROUP_MA='TOUT',
                            COQUE_NCOU=2,
                            EPAIS=0.20000000000000001,),
                         _F(GROUP_MA='PLAQ1',
                            EPAIS=0.20000000000000001,
                            COQUE_NCOU=2,
                            ANGL_REP=(0.0,0.0),
                            EXCENTREMENT=0.10000000000000001,
                            INER_ROTA='OUI',),
                         _F(GROUP_MA='PLAQ2',
                            EPAIS=0.20000000000000001,
                            ANGL_REP=(0.0,0.0),
                            EXCENTREMENT=-0.10000000000000001,
                            COQUE_NCOU=2,
                            INER_ROTA='OUI',),),);

EP1=AFFE_CARA_ELEM(MODELE=MO1,
                   COQUE=_F(GROUP_MA='TOUT',
                            EPAIS=0.40000000000000002,
                            COQUE_NCOU=4,
                            ANGL_REP=(0.0,0.0),),);

MAT1A=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=2.E10,
                                 E_T=2.E10,
                                 NU_LT=0.29999999999999999,
                                 G_LT=2.E9,),);

MAT1B=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=1.5000000000E10,
                                 E_T=1.5000000000E10,
                                 NU_LT=0.29999999999999999,
                                 G_LT=1.500000000E9,),);

MAT1C=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=2.E10,
                                 E_T=2.E10,
                                 NU_LT=0.29999999999999999,
                                 G_LT=2.E9,),);

MAT1D=DEFI_MATERIAU(ELAS_ORTH=_F(E_L=1.5000000000E10,
                                 E_T=1.5000000000E10,
                                 NU_LT=0.29999999999999999,
                                 G_LT=1.500000000E9,),);
#
#


MU=DEFI_COMPOSITE(COUCHE=(_F(EPAIS=0.10000000000000001,
                             MATER=MAT1D,
                             ORIENTATION=0.0,),
                          _F(EPAIS=0.10000000000000001,
                             MATER=MAT1C,
                             ORIENTATION=0.0,),
                          _F(EPAIS=0.10000000000000001,
                             MATER=MAT1B,
                             ORIENTATION=0.0,),
                          _F(EPAIS=0.10000000000000001,
                             MATER=MAT1A,
                             ORIENTATION=0.0,),),);

MU1=DEFI_COMPOSITE(COUCHE=(_F(EPAIS=0.10000000000000001,
                              MATER=MAT1B,
                              ORIENTATION=0.0,),
                           _F(EPAIS=0.10000000000000001,
                              MATER=MAT1A,
                              ORIENTATION=0.0,),),);

MU2=DEFI_COMPOSITE(COUCHE=(_F(EPAIS=0.10000000000000001,
                              MATER=MAT1D,
                              ORIENTATION=0.0,),
                           _F(EPAIS=0.10000000000000001,
                              MATER=MAT1C,
                              ORIENTATION=0.0,),),);

CHMUL=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MU,),);

CHMU2=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='PLAQ1',
                             MATER=MU1,),
                          _F(GROUP_MA='PLAQ2',
                             MATER=MU2,),),);

CHME=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='A1',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,
                                 DRX=0.0,
                                 DRY=0.0,
                                 DRZ=0.0,),
                              _F(GROUP_NO='A2',
                                 DX=0.0,
                                 DY=0.0,),),
                    FORCE_NODALE=_F(GROUP_NO='A3',
                                    FZ=-1000.0,),);

CHME1=AFFE_CHAR_MECA(MODELE=MO1,
                     DDL_IMPO=(_F(GROUP_NO='A1',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),
                               _F(GROUP_NO='A2',
                                  DX=0.0,
                                  DY=0.0,),),
                     FORCE_NODALE=_F(GROUP_NO='A3',
                                     FZ=-1000.0,),);
# ______________________________________________________________________
#
#                         RESOLUTION
# ______________________________________________________________________
#
#
#  2 BICOUCHES
#
DEP=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHMU2,
                   CARA_ELEM=EP,
                   EXCIT=_F(CHARGE=CHME,),
                  );

DEP=CALC_CHAMP(reuse=DEP,RESULTAT=DEP,CONTRAINTE=('SIGM_ELNO','EFGE_ELNO'))


DEP1INF=POST_CHAMP(RESULTAT=DEP,
                   EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=1,
                                   NIVE_COUCHE='INF',),);
DEP2SUP=POST_CHAMP(RESULTAT=DEP,
                   EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=2,
                                   NIVE_COUCHE='SUP',),);
IMPR_RESU(RESU=_F(RESULTAT=DEP2SUP,NOM_CHAM='SIGM_ELNO',
                   NUME_ORDRE=1,MAILLE=('M10',)))

IMPR_RESU(RESU=_F(RESULTAT=DEP1INF,NOM_CHAM='SIGM_ELNO',
                   NUME_ORDRE=1,MAILLE=('MAILM10',)))



# ______________________________________________________________________
#
#
#
#  QUADRICOUCHE
#
DEPQ=MECA_STATIQUE(MODELE=MO1,
                   CHAM_MATER=CHMUL,
                   CARA_ELEM=EP1,
                   EXCIT=_F(CHARGE=CHME1,),
                  );

DEPQ=CALC_CHAMP(reuse=DEPQ,RESULTAT=DEPQ,CONTRAINTE=('SIGM_ELNO','EFGE_ELNO'))


#
# COUCHE 1
#
DEPQ1INF=POST_CHAMP(RESULTAT=DEPQ,
                    EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=1,
                                   NIVE_COUCHE='INF',),);
IMPR_RESU(RESU=_F(RESULTAT=DEPQ1INF,NOM_CHAM='SIGM_ELNO',
                   NUME_ORDRE=1,MAILLE=('M10',)))

#
# COUCHE 4
#
DEPQ4SUP=POST_CHAMP(RESULTAT=DEPQ,
                    EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=4,
                                   NIVE_COUCHE='SUP',),);
IMPR_RESU(RESU=_F(RESULTAT=DEPQ4SUP,NOM_CHAM='SIGM_ELNO',
                   NUME_ORDRE=1,MAILLE=('M10',)))


