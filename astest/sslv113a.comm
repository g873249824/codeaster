# EN PRESSION INTERNE ET EXTERNE SUR 2 MATERIAUX
#
# MODELISATION 1 : ELEMENT MECANIQUE AXISYMETRIQUE QUAD8
# MAILLAGE       : GIBI > ASTER

DEBUT(CODE=_F(
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

M=LIRE_MAILLAGE(FORMAT='MED',);

MO=AFFE_MODELE(MAILLAGE=M,
               
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

young = DEFI_FONCTION(NOM_PARA='X',VALE=(0.90, 2.0,
                                         1.51, 2.0,
                                         1.52, 1.0, 
                                         2.10, 1.0),)
nu = DEFI_CONSTANTE(VALE=0.3)
alpha = DEFI_CONSTANTE(VALE=0.)
zero = DEFI_CONSTANTE(VALE=0.)

MA1=DEFI_MATERIAU(ELAS_FO=_F(E=young,
                          NU=nu,
                          ALPHA=alpha,TEMP_DEF_ALPHA=0.),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=(_F(GROUP_MA='DOM',
                          MATER=MA1,),
                       ),);

F1 = FORMULE(VALE="""
1.15385*(2*-0.98097+0.91333)+2*0.76923*(-0.98097
                -(-1.11741/R**2))
                """,
             NOM_PARA='R',)

F2 = FORMULE(VALE="""
