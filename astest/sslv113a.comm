                  FACE_IMPO=(_F(GROUP_MA='GRMA1',
                                DY=0.,),
                             _F(GROUP_MA='GRMA2',
                                DY=0.91333,),),
                  FORCE_CONTOUR=(_F(GROUP_MA='GRMA3',
                                    FX=1.,),
                                 _F(GROUP_MA='GRMA4',
                                    FX=-2.,),),);

LISTR=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);
                                   
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0 ,0.0 ,
                          1.0 ,1.0 ,),);

RESU=STAT_NON_LINE(MODELE=MO, 
                   CHAM_MATER=CM,
                   COMPORTEMENT=_F(TOUT='OUI', RELATION='ELAS'),
                   INCREMENT=_F(LIST_INST=LISTR),
                   EXCIT=_F(CHARGE=CH,FONC_MULT=RAMPE),);


RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 TOUT_ORDRE='OUI',
                 OPTION='ERME_ELEM',);

