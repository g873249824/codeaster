DEBUT(CODE=_F(VISU_EFICAS='NON',NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'),)

# MODELIDATION A : X-FEM 3D

# PARTIE 1 : RAFFINEMENT DE MAILLAGE

# CE FICHIER EST LA VERSION CORRIGEE DU 4EME EXERCICE DES TPS
# POUR LA PREPARATION DES TPS, FOURNIR EGALEMENT UNE VERSION A TROUS DE CE FICHIER :
# - REMPLACER LA VALEUR DE nb_calc PAR UNE VALEUR BIDON
# - REMPLACER LA VALEUR DE crit PAR UNE VALEUR BIDON

# nbre d'iterations
nb_calc = 6

# initialisation
MA    = [None]*(nb_calc+1)
MO    = [None]*nb_calc
CHERR = [None]*nb_calc
FISS  = [None]*nb_calc

num_calc=0   

crit = [1., 1, 1, 0.2, 0.1, 0.1]

MA[num_calc]=LIRE_MAILLAGE(FORMAT='MED');

for num_calc in range(0,nb_calc) :
   
   MO[num_calc]=AFFE_MODELE(MAILLAGE=MA[num_calc],
                            AFFE=_F(GROUP_MA='CUBE',
                                    PHENOMENE='MECANIQUE',
                                    MODELISATION='3D',),);
  
   # definition de la fissure semi elliptique                     
   FISS[num_calc]=DEFI_FISS_XFEM(MODELE=MO[num_calc],  
                                 DEFI_FISS=_F(FORM_FISS      = 'ELLIPSE',
                                              DEMI_GRAND_AXE = 2.,
                                              DEMI_PETIT_AXE = 2.,
                                              CENTRE         = (0.,0.,0.),
                                              VECT_X         = (1.,0.,0.), 
                                              VECT_Y         = (0.,1.,0.),
                                              ),
                                 )
      
   # evaluation des formules           
   CHERR[num_calc]=RAFF_XFEM(FISSURE=FISS[num_calc])
   
   # definition du type du maillage n+1 --> pour dire que MAILLAGE_NP1 est un type 'maillage'
   MA[num_calc+1]=CO('MA_%d' % (num_calc+1))
   
   MACR_ADAP_MAIL(ADAPTATION         = 'RAFFINEMENT',
                  CHAM_GD            = CHERR[num_calc],
                  CRIT_RAFF_PE       = crit[num_calc],  
                  USAGE_CMP          = 'RELATIF',
                  MAILLAGE_N         = MA[num_calc],
                  MAILLAGE_NP1       = MA[num_calc+1])
   
   # Impression des maillages
   #IMPR_RESU(FORMAT='MED',UNITE=81+num_calc,RESU=_F(MAILLAGE=MA[num_calc+1]));

# impression du dernier maillage
IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(MAILLAGE=MA[num_calc+1]));
    
FIN()
