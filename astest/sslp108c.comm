# ARLEQUIN : SUBSTITUTION 2D/2D INCOMPATIBLE.
#            MAILLAGES DE DENSITES DIFFERENTES

DEBUT(CODE=_F(NOM='SSLP108C',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

MA=LIRE_MAILLAGE(UNITE=20,
                 FORMAT='MED',);

MA=MODI_MAILLAGE(reuse=MA,
                   MAILLAGE=MA,
                   MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                  GROUP_NO_FOND='P0',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

MAT=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                          NU=0.3,),
                  ECRO_LINE=_F(D_SIGM_EPSI=3.,
                               SY=2800000000.,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

BL=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=_F(GROUP_MA='L23',
                              LIAISON='ENCASTRE',),);

CH=AFFE_CHAR_MECA(MODELE=MO,
                  FORCE_CONTOUR=(_F(GROUP_MA='L12',
                                    FX=1.E8,
                                    FY=1.E8,),
                                 _F(GROUP_MA='L34',
                                    FX=-1.E8,
                                    FY=-1.E8,),),);
##################################################


# 1. Creation des relations de couplage Arlequin
#    entre les groupes de mailles
#    couronne et fissure (maillage fin)
#    et la plaque (maillage grossier)
#    Collage sur le modele fin
##################################################


AR=AFFE_CHAR_MECA(MODELE=MO,
                  ARLEQUIN=_F(GROUP_MA_1=('COUR','FISS',),
                              GROUP_MA_2='PLAQ',
                              GROUP_MA_COLL='COUR',
                              COLLAGE='GROUP_MA_1',
                              POIDS_1=0.99999,),);
##################################


# 2. CALCUL STATIQUE NON LINEAIRE
##################################


LIST=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=(_F(JUSQU_A=5.,
                                  NOMBRE=10,),
                                _F(JUSQU_A=10.,
                                  NOMBRE=10,),));

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=AR,),
                          _F(CHARGE=CH),
                          _F(CHARGE=BL,),),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
                   INCREMENT=_F(LIST_INST=LIST,),);
                   
                   
############################


# 3. POST-TRAITEMENT GENERAL
#    => deplacements
#    => contraintes
############################


RESU=CALC_ELEM(reuse =RESU,
               RESULTAT=RESU,
               OPTION=('EQUI_ELNO_SIGM','SIEF_ELNO_ELGA','EPSP_ELNO','VARI_ELNO_ELGA',),);

RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION=('EQUI_NOEU_SIGM','SIEF_NOEU_ELGA','EPSP_NOEU','VARI_NOEU_ELGA',),);

LIGAM=INTE_MAIL_2D(MAILLAGE=MA,
                   DEFI_CHEMIN=_F(GROUP_MA='LPOP11',),);

BHAUT=INTE_MAIL_2D(MAILLAGE=MA,
                   DEFI_CHEMIN=_F(GROUP_MA='L12',),);

SIYY_LIG=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='SIYY_LIG',
                                 RESULTAT=RESU,
                                 NOM_CHAM='SIEF_NOEU_ELGA',
                                 INST=10.0,
                                 CHEMIN=LIGAM,
                                 NOM_CMP='SIYY',
                                 MOYE_NOEUD='NON',),
                       INFO=2,);

DEPL_H=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='DEPL_H',
                               RESULTAT=RESU,
                               NOM_CHAM='DEPL',
                               INST=10.0,
                               CHEMIN=BHAUT,
                               NOM_CMP='DY',),
                     INFO=2,);
############################################


# 4. POST-TRAITEMENT MECANIQUE DE LA RUPTURE
#    => calcul de G, K1, K2, G_IRWIN
############################################


DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RESU,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

FOND1=DEFI_FOND_FISS(MAILLAGE=MA,
                     FOND_FISS=_F(GROUP_NO='P0',),
                     NORMALE=(0.0,-1.0,0.0,),);

THETA01=CALC_THETA(MODELE=MO,
                   DIRECTION=(-1.0,0.0,0.0,),
                   THETA_2D=_F(GROUP_NO='P0',
                               MODULE=1.0,
                               R_INF=1e-05,
                               R_SUP=0.00014,),);

GK01=CALC_G(THETA=_F(THETA=THETA01,),
            RESULTAT=RESU,
            INST=10.0,
            EXCIT=(_F(CHARGE=BL,),
                   _F(CHARGE=CH,),
                   _F(CHARGE=AR,),),
            SYME_CHAR='SANS',
            COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
            OPTION='CALC_G',
            INFO=1,);

GK02=CALC_G(THETA=_F(THETA=THETA01,
                     FOND_FISS=FOND1,),
            RESULTAT=RESU,
            INST=10.0,
            EXCIT=(_F(CHARGE=BL,),
                   _F(CHARGE=CH,),
                   _F(CHARGE=AR,),),
            SYME_CHAR='SANS',
            COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
            OPTION='CALC_K_G',
            INFO=1,);
#########################


# 5. TESTS NON REGRESSION
#    => deplacements
