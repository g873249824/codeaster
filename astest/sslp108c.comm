# ARLEQUIN : SUBSTITUTION 2D/2D INCOMPATIBLE.
#            MAILLAGES DE DENSITES DIFFERENTES

DEBUT(CODE=_F(NOM='SSLP108C',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

MAIL=LIRE_MAILLAGE(UNITE=20,
                   FORMAT='MED',);

MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                  GROUP_NO_FOND='P0',),);

MODARL=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);

MATERI=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                             NU=0.3,),
                     ECRO_LINE=_F(D_SIGM_EPSI=3.,
                                  SY=2800000000.,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=MATERI,),);

BLOC=AFFE_CHAR_MECA(MODELE=MODARL,
                    DDL_IMPO=_F(GROUP_MA='L23',
                                LIAISON='ENCASTRE',),);

CHAREX=AFFE_CHAR_MECA(MODELE=MODARL,
                      FORCE_CONTOUR=(_F(GROUP_MA='L12',
                                        FX=1.E8,
                                        FY=1.E8,),
                                     _F(GROUP_MA='L34',
                                        FX=-1.E8,
                                        FY=-1.E8,),),);

##################################################
# 1. Creation des relations de couplage Arlequin
#    entre les groupes de mailles
#    couronne et fissure (maillage fin)
#    et la plaque (maillage grossier)
#    Collage sur le modele fin
##################################################

ARLEQ=AFFE_CHAR_MECA(MODELE=MODARL,
                     ARLEQUIN=_F(GROUP_MA_1=('COUR','FISS',),
                                 GROUP_MA_2='PLAQ',
                                 GROUP_MA_COLL='COUR',
                                 COLLAGE='GROUP_MA_1',
                                 POIDS_1=0.99999,),);

##################################
# 2. CALCUL STATIQUE NON LINEAIRE
##################################

LIST=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=(_F(JUSQU_A=5.,
                                   NOMBRE=10,),
                                _F(JUSQU_A=10.,
                                   NOMBRE=10,),));

RESU=STAT_NON_LINE(MODELE=MODARL,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=ARLEQ,),
                          _F(CHARGE=CHAREX),
                          _F(CHARGE=BLOC,),),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
                   INCREMENT=_F(LIST_INST=LIST,),);
 
############################
# 3. POST-TRAITEMENT GENERAL
#    => deplacements
#    => contraintes
############################

RESU=CALC_ELEM(reuse =RESU,
               RESULTAT=RESU,
               OPTION=('EQUI_ELNO_SIGM','SIEF_ELNO','EPSP_ELNO','VARI_ELNO_ELGA',),);

RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION=('EQUI_NOEU_SIGM','SIEF_NOEU','EPSP_NOEU','VARI_NOEU_ELGA',),);

LIGAM=INTE_MAIL_2D(MAILLAGE=MAIL,
                   DEFI_CHEMIN=_F(GROUP_MA='LPOP11',),);

BHAUT=INTE_MAIL_2D(MAILLAGE=MAIL,
                   DEFI_CHEMIN=_F(GROUP_MA='L12',),);

SIYY_LIG=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='SIYY_LIG',
                                 RESULTAT=RESU,
                                 NOM_CHAM='SIEF_NOEU',
                                 INST=10.0,
                                 CHEMIN=LIGAM,
                                 NOM_CMP='SIYY',
                                 MOYE_NOEUD='NON',),);

DEPL_H=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='DEPL_H',
                               RESULTAT=RESU,
                               NOM_CHAM='DEPL',
                               INST=10.0,
                               CHEMIN=BHAUT,
                               NOM_CMP='DY',),);

########################################################
# 4. POST-TRAITEMENT MECANIQUE DE LA RUPTURE
# Attention : on est sur un modele Arlequin !
#             Il faut donc restreindre le resultat RESU
#             au groupe de mailles du patch (RESU_RES).
########################################################

IMPR_RESU(FORMAT='MED',
          UNITE=81,
          RESTREINT=_F(GROUP_MA=('COUR','FISS',),
                       TOUT_GROUP_NO='OUI',),
          RESU=_F(RESULTAT=RESU,NOM_CHAM=('DEPL','SIEF_ELGA','EPSP_ELNO','VARI_ELNO_ELGA',)))

MA_RES=LIRE_MAILLAGE(UNITE=81,
                     FORMAT='MED',
                     VERI_MAIL=_F(VERIF='OUI',),);

MA_RES=MODI_MAILLAGE(reuse =MA_RES,
                     MAILLAGE=MA_RES,
                     MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                    GROUP_NO_FOND='P0',),);

MO_RES=AFFE_MODELE(MAILLAGE=MA_RES,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);

MAT_RES=AFFE_MATERIAU(MAILLAGE=MA_RES,
                      AFFE=_F(TOUT='OUI',
                              MATER=MATERI,),);

RESU_RES=LIRE_RESU(TYPE_RESU='EVOL_NOLI',
                   FORMAT='MED',
                   MODELE=MO_RES,
                   CHAM_MATER=MAT_RES,
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
                   UNITE=81,
                   FORMAT_MED=(
                   _F(NOM_CHAM='DEPL',
                      NOM_CHAM_MED='&RESUR1_DEPL____________________',),
                   _F(NOM_CHAM='SIEF_ELGA',
                      NOM_CHAM_MED='&RESUR1_SIEF_ELGA_______________',),
                   _F(NOM_CHAM='EPSP_ELNO',
                      NOM_CHAM_MED='&RESUR1_EPSP_ELNO_______________',),
                   _F(NOM_CHAM='VARI_ELNO_ELGA',
                      NOM_CHAM_MED='&RESUR1_VARI_ELNO_ELGA__________',),),
                   INST=10.0,);

FOND1=DEFI_FOND_FISS(MAILLAGE=MA_RES,
                     FOND_FISS=_F(GROUP_NO='P0',),
                     NORMALE=(0.0,-1.0,0.0,),);

THETA01=CALC_THETA(MODELE=MO_RES,
                   DIRECTION=(-1.0,0.0,0.0,),
                   THETA_2D=_F(GROUP_NO='P0',
                               MODULE=1.0,
                               R_INF=1e-05,
                               R_SUP=0.00014,),);

###################################################
# 5. On peut post-traiter sur le modele restreint :
#    => calcul de G, K1, K2, G_IRWIN
###################################################

GK01=CALC_G(THETA=_F(THETA=THETA01,),
            RESULTAT=RESU_RES,
            INST=10.0,
            SYME_CHAR='SANS',
            COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
            OPTION='CALC_G',);

GK02=CALC_G(THETA=_F(THETA=THETA01,
                     FOND_FISS=FOND1,),
            RESULTAT=RESU_RES,
            INST=10.0,
            SYME_CHAR='SANS',
            COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
            OPTION='CALC_K_G',);

#########################
# 6. TESTS NON REGRESSION
#    => deplacements
#    => contraintes
#########################

TEST_TABLE(TABLE=DEPL_H,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=0.0023,),
           NOM_PARA='DY',
           VALE=-8.1928913165022e-07,
           PRECISION=8e-08,
           REFERENCE='NON_REGRESSION',
           VERSION='10.00.19',);

TEST_TABLE(TABLE=SIYY_LIG,
