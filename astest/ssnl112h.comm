                     MODELE=MOD,
                 CHAM_MATER=CHMTM,
                 CARA_ELEM=CARBA,EXCIT=(
                _F(  CHARGE = LIAISON),),
                  LIST_INST=LINST,
               )

CALC_CHAMP(reuse=UELAS,RESULTAT=UELAS,LIST_INST=LINST,CONTRAINTE=('EFGE_ELNO'))


CALC_CHAMP(reuse=UELAS,  FORCE='FORC_NODA',
                        RESULTAT=UELAS)

TEST_RESU(RESU=(_F(INST=1.0,
