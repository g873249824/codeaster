CALC_ELEM( reuse= DEPLDIL ,RESULTAT= DEPLDIL ,
                  OPTION=('EFGE_ELNO_DEPL','SIGM_ELNO_DEPL',),
                  REPE_COQUE =_F( NUME_COUCHE=1,
                                  NIVE_COUCHE='INF',),
                         )


DEPLDIL2=MECA_STATIQUE(
                  MODELE=MO,
              CHAM_MATER=CHMAT1,
