                _F(
                TOUT = 'OUI',
                NOM_CMP = 'TEMP_INF',
                VALE = 0.),

                _F(
                TOUT = 'OUI',
                NOM_CMP = 'TEMP',
                VALE = 0.),

                _F(
                TOUT = 'OUI',
                NOM_CMP = 'TEMP_SUP',
                VALE = 0.))
                )

