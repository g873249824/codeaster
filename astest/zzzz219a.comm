DEBUT(CODE=_F( NOM = 'ZZZZ219A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#----------------------------------------------------------------------------------
# Dans ce test tres simple, tous les deplacements des 6 noeuds du maillage
# sont imposes. On peut donc facilement verifier que les C.L. sont bien respectees.
#
#          S4
#  N5  +---------+ N6
#      !         !
#  S5  !         ! S3
#      !         !
#  N3  +---------+ N4
#      !         !
#  S6  !         ! S2
#      !         !
#  N1  +---------+ N2
#          S1
#
#----------------------------------------------------------------------------------

MA=LIRE_MAILLAGE(  )

MO=AFFE_MODELE(MAILLAGE=MA,AFFE=_F(TOUT = 'OUI', PHENOMENE = 'MECANIQUE', MODELISATION = 'D_PLAN') )

MAT1=DEFI_MATERIAU( ELAS=_F(  E = 1.2, NU = 0.3, ),)

CHMAT=AFFE_MATERIAU( MAILLAGE=MA, AFFE=  _F(TOUT = 'OUI', MATER = MAT1,),)

CHMEC=AFFE_CHAR_MECA(  MODELE=MO,
             FACE_IMPO=( _F(  GROUP_MA = 'GAUCHE', DX=1, DY=2.),
                         _F(  GROUP_MA = 'BAS',  DNOR = 3., SANS_MAILLE=('S5','S6'),),
                         _F(  GROUP_MA = 'HAUT', DNOR = 5., SANS_GROUP_MA= 'GAUCHE', SANS_MAILLE=('S5','S2'),),),
             DDL_IMPO=(  _F(  NOEUD=('N2','N4','N6'),   DX=7,  ),
                         _F(  NOEUD='N4'          ,     DY=13, ),
                         _F(  NOEUD='N6'          ,     DX=11, ),),
             )

MSLIN=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHMAT,EXCIT=_F(CHARGE = CHMEC,), )


TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N1', NOM_CMP='DX', VALE=  1.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N1', NOM_CMP='DY', VALE=  2.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N2', NOM_CMP='DX', VALE=  7.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N2', NOM_CMP='DY', VALE= -3.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N3', NOM_CMP='DX', VALE=  1.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N3', NOM_CMP='DY', VALE=  2.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N4', NOM_CMP='DX', VALE=  7.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N4', NOM_CMP='DY', VALE= 13.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N5', NOM_CMP='DX', VALE=  1.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N5', NOM_CMP='DY', VALE=  2.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N6', NOM_CMP='DX', VALE= 11.00 ),)
TEST_RESU(RESU=  _F(  RESULTAT = MSLIN,    NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
       REFERENCE = 'ANALYTIQUE', NOEUD='N6', NOM_CMP='DY', VALE=  5.00 ),)

FIN( )
