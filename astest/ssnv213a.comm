CH12=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=_F(GROUP_NO=('S_AVT',),
                                DY=0.0,),);

CH20=AFFE_CHAR_MECA_F(MODELE=MO,
                      PRES_REP=_F(GROUP_MA='S_SUP',
                                  PRES=FORC1,),);


#attention si fluage=0 revoir la liste des temps
