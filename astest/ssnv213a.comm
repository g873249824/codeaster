                      INTERVALLE=(_F(JUSQU_A=0.01,
                                     NOMBRE=1),
                                  _F(JUSQU_A=2.0,
                                     NOMBRE=1),
                                  _F(JUSQU_A=500.0,
                                     NOMBRE=498,),),);

L_SAUV=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=500.0,
                                     NOMBRE=50,),),);

