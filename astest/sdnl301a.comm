DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#----- GRANDEURS PHYSIQUE: UNITE MKS ---

KN=1.0E+5

CN=.28

RAYON=.00795

EPAIS=.00115

AMPL=41.38

FORCEP=8.276 /2;

FORCEM=-8.276 /2;

OMEGA=251.2 # (#40 Hz)

TDEB=.17

TFIN=.25

DELT=.5E-5

ACIER=DEFI_MATERIAU(ELAS=_F(E = 2.07E11, NU = 0.3, RHO = 7870.,
                            AMOR_ALPHA = 1.79E-5, AMOR_BETA = .1526));

#-----   MODELE ----------------

POUTRE=LIRE_MAILLAGE(FORMAT='MED',);

POUTRE=DEFI_GROUP(reuse=POUTRE,
                  MAILLAGE=POUTRE,
                  CREA_GROUP_MA=_F(NOM = 'TOUT',
                                   TOUT = 'OUI'));

MO=AFFE_MODELE(MAILLAGE=POUTRE,
               AFFE=_F(TOUT        = 'OUI',
                       PHENOMENE   = 'MECANIQUE',
                       MODELISATION= 'POU_D_T'));

SINO = FORMULE(NOM_PARA='INST',
               VALE    ='sin (OMEGA *INST)');

LINST=DEFI_LIST_REEL(DEBUT=0.,
                     INTERVALLE=_F(JUSQU_A= TFIN,
                                   PAS    = .0001));

SINOT=CALC_FONC_INTERP(FONCTION=SINO,
                       LIST_PARA=LINST,
                       NOM_PARA='INST');

CARELEM=AFFE_CARA_ELEM(MODELE=MO,
                       POUTRE=_F(GROUP_MA= 'TOUT',
                                 SECTION = 'CERCLE',
                                 CARA    = ('R','EP'),
                                 VALE    = (RAYON, EPAIS)));

CHMAT=AFFE_MATERIAU(MAILLAGE=POUTRE,
                    AFFE=_F(TOUT = 'OUI',
                            MATER= ACIER));

CLIMIT=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=(_F(TOUT ='OUI',
                                   DZ=.0, DRX=.0, DRY=.0),
                                _F(NOEUD='N1',
                                   DX=.0, DY=.0, DRZ=.0)));

FORCE=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_NODALE=(_F(FY = FORCEP,
                                      NOEUD = ('N3','N4','N5',
                                               'N6','N7','N8','N9',
                                               'N10','N11','N12','N13',
                                               'N27','N28','N29','N30',
                                               'N31','N32','N33','N34',
                                               'N35','N36','N37',)),
                                   _F(FY = FORCEM,
                                      NOEUD = ('N15','N16','N17','N18',
                                               'N19','N20','N21','N22',
                                               'N23','N24','N25',
                                               'N39','N40','N41','N42',
                                               'N43','N44','N45','N46',
                                               'N47','N48','N49',))));

BUTE=DEFI_OBSTACLE(TYPE='PLAN_Y');

# ASSEMBLAGE MATRICE

MELER=CALC_MATR_ELEM(MODELE    =MO,
                     OPTION    ='RIGI_MECA',
                     CARA_ELEM =CARELEM,
                     CHAM_MATER=CHMAT,
                     CHARGE    =CLIMIT);

MELEM=CALC_MATR_ELEM(MODELE    =MO,
                     OPTION    ='MASS_MECA',
                     CARA_ELEM =CARELEM,
                     CHAM_MATER=CHMAT,
                     CHARGE    =CLIMIT);

MELEA=CALC_MATR_ELEM(MODELE    =MO,
                     OPTION    ='AMOR_MECA',
                     CARA_ELEM =CARELEM,
                     MASS_MECA =MELEM,
                     RIGI_MECA =MELER,
                     CHAM_MATER=CHMAT);

# CHARGE: CLIMIT);

VELEM=CALC_VECT_ELEM(CHARGE=FORCE, OPTION='CHAR_MECA');

NDDL=NUME_DDL(MATR_RIGI=MELER);

MATR=ASSE_MATRICE(MATR_ELEM=MELER, NUME_DDL=NDDL);

MATM=ASSE_MATRICE(MATR_ELEM=MELEM, NUME_DDL=NDDL);

MATA=ASSE_MATRICE(MATR_ELEM=MELEA, NUME_DDL=NDDL);

VECT=ASSE_VECTEUR(VECT_ELEM=VELEM, NUME_DDL=NDDL);

# CALCUL MODAL :30 MODES
# (jusqu'a 4800 Hz)

MODE=CALC_MODES(MATR_RIGI=MATR,
                OPTION='PLUS_PETITE',
                CALC_FREQ=_F(NMAX_FREQ=30,
                             ),
                MATR_MASS=MATM,
                )

#enrichissement de la base pour restitution des contraintes
MODE=CALC_CHAMP(reuse=MODE,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CARELEM,
                   RESULTAT=MODE,
                   CONTRAINTE=('SIPO_ELNO','EFGE_ELNO','EFGE_ELGA',),);

NDDLGEN=NUME_DDL_GENE(BASE=MODE);

# PROJECTION DANS LA BASSE MODALE

RIGIPROJ=PROJ_MATR_BASE(BASE         =MODE,
                        NUME_DDL_GENE=NDDLGEN,
                        MATR_ASSE    =MATR);

MASSPROJ=PROJ_MATR_BASE(BASE         =MODE,
                        NUME_DDL_GENE=NDDLGEN,
                        MATR_ASSE    =MATM);

AMORPROJ=PROJ_MATR_BASE(BASE         =MODE,
                        NUME_DDL_GENE=NDDLGEN,
                        MATR_ASSE    =MATA);

VECTPROJ=PROJ_VECT_BASE(BASE         =MODE,
                        VECT_ASSE    =VECT,
                        NUME_DDL_GENE=NDDLGEN,
                        TYPE_VECT    ='FORC');

TRANGENE=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='GENE',
                         SCHEMA_TEMPS=_F(SCHEMA='DIFF_CENTRE',),
                         MATR_MASS=MASSPROJ,
                         MATR_RIGI=RIGIPROJ,
                         MATR_AMOR=AMORPROJ,
                         INCREMENT=_F(INST_INIT= 0.,
                                      INST_FIN = TFIN,
                                      PAS      = DELT),
                         ARCHIVAGE=_F(PAS_ARCH  =10),
                         EXCIT=_F(VECT_ASSE_GENE= VECTPROJ,
                                  FONC_MULT= SINOT),
                         COMPORTEMENT=(
                                _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N14',
                                  JEU      = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (.609, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN),
                               _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N26',
                                  JEU      = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (1.218, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN),
                               _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N38',
                                  JEU = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (1.827, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN),
                               _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N2',
                                  JEU      = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (2.436, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN)),
                          INFO=1,
                          IMPRESSION=_F(NIVEAU  = 'DEPL_LOC',
                          #              TOUT='OUI',
                          #              INST_INIT= 0.01,
                                        INST_FIN= 0.01),);

POST=POST_DYNA_MODA_T(RESU_GENE=TRANGENE,
                      CHOC=_F(INST_INIT= TDEB,
                              INST_FIN = TFIN));

IMPR_TABLE(TABLE=POST);

TEST_TABLE(VALE_CALC=23.9833137772,
           NOM_PARA='RMS_T_TOTAL',
           TABLE=POST,
           FILTRE=(_F(NOM_PARA='CALCUL',
                      VALE_K='FORCE_NORMALE',),
                   _F(NOM_PARA='NOEUD',
                      VALE_K='N2',),
                   ),
           )

# ON PASSE EN SDVERI=NON LE TEMPS DE LA REST_GENE_PHYS
# A CAUSE DU TEMPS CPU TROP IMPORTANT (FACTEUR +40)
DEBUG(SDVERI='NON');

RESPHY1 = REST_GENE_PHYS(RESU_GENE =TRANGENE,
                         TOUT_ORDRE='OUI',
                         NOM_CHAM  =('DEPL','ACCE','SIPO_ELNO','EFGE_ELNO',),
                   )

RESPHY1=CALC_CHAMP(reuse=RESPHY1,
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CARELEM,
                   RESULTAT=RESPHY1,
                   CONTRAINTE=('EFGE_ELGA',),);

DEBUG(SDVERI='OUI');

# RECU_FONCTION/RESULTAT=RESPHY1 (pour comparaison REST_GENE_PHYS/POST_GENE_PHYS)
SVY_N1=RECU_FONCTION(RESULTAT=RESPHY1,
                  NOM_CHAM='SIPO_ELNO',
                  NOM_CMP='SVY',
                   MAILLE='M1',
                  NOEUD='N1',);

MFZ_N1=RECU_FONCTION(RESULTAT=RESPHY1,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='MFZ',
                   MAILLE='M1',
                  NOEUD='N1',);

DY_N3=RECU_FONCTION(RESULTAT=RESPHY1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  NOEUD='N3',);

ADRZ_N3=RECU_FONCTION(RESULTAT=RESPHY1,
                  NOM_CHAM='ACCE',
                  NOM_CMP='DRZ',
                  NOEUD='N3',);

VY_G1=RECU_FONCTION(RESULTAT=RESPHY1,
                  NOM_CHAM='EFGE_ELGA',
                  NOM_CMP='VY',
                   MAILLE='M1',
                  POINT=1,
                 );

#par POST_GENE_PHYS:
T_SIPO=POST_GENE_PHYS(RESU_GENE=TRANGENE,
                      OBSERVATION=(
                                    _F(NOM_CHAM = 'SIPO_ELNO',
                                       NOM_CMP=('SVY','SMFZ',),
                                       MAILLE='M1',
                                       ),
                                    _F(NOM_CHAM = 'EFGE_ELNO',
                                       NOM_CMP=('VY','MFZ',),
                                       MAILLE='M1',
                                       ),
                                    _F(NOM_CHAM = 'DEPL',
                                       NOEUD='N3',
                                       ),
                                    _F(NOM_CHAM = 'ACCE',
                                       NOEUD='N3',
                                       ),
                                    _F(NOM_CHAM = 'EFGE_ELGA',
                                       NOM_CMP=('VY','MFZ',),
                                       MAILLE='M1',
                                       ),
                                   ),
                       )

#IMPR_TABLE(TABLE=T_SIPO)

SVY_N1X = RECU_FONCTION(TABLE=T_SIPO,
                      PARA_X='INST',PARA_Y='VALE_R',
                      FILTRE=(_F(NOM_PARA='NOM_CMP', VALE_K='SVY',),
                              _F(NOM_PARA='NOM_CHAM', VALE_K='SIPO_ELNO',),
                              _F(NOM_PARA='NOEUD', VALE_K='N1',),
                              _F(NOM_PARA='MAILLE', VALE_K='M1',),
                             ),
           )

MFZ_N1X = RECU_FONCTION(TABLE=T_SIPO,
                      PARA_X='INST',PARA_Y='VALE_R',
                      FILTRE=(_F(NOM_PARA='NOM_CMP', VALE_K='MFZ',),
                              _F(NOM_PARA='NOM_CHAM', VALE_K='EFGE_ELNO',),
                              _F(NOM_PARA='NOEUD', VALE_K='N1',),
                              _F(NOM_PARA='MAILLE', VALE_K='M1',),
                             ),
           )

DY_N3X = RECU_FONCTION(TABLE=T_SIPO,
                      PARA_X='INST',PARA_Y='VALE_R',
                      FILTRE=(_F(NOM_PARA='NOM_CMP', VALE_K='DY',),
                              _F(NOM_PARA='NOEUD', VALE_K='N3',),
                              _F(NOM_PARA='NOM_CHAM', VALE_K='DEPL',),
                             ),
           )

ADRZ_N3X = RECU_FONCTION(TABLE=T_SIPO,
                      PARA_X='INST',PARA_Y='VALE_R',
                      FILTRE=(_F(NOM_PARA='NOM_CMP', VALE_K='DRZ',),
                              _F(NOM_PARA='NOEUD', VALE_K='N3',),
                              _F(NOM_PARA='NOM_CHAM', VALE_K='ACCE',),
                             ),
           )

VY_G1X = RECU_FONCTION(TABLE=T_SIPO,
                      PARA_X='INST',PARA_Y='VALE_R',
                      FILTRE=(_F(NOM_PARA='NOM_CMP', VALE_K='VY',),
                              _F(NOM_PARA='POINT', VALE_K='PT 1',),
                              _F(NOM_PARA='MAILLE', VALE_K='M1',),
                             ),
           )

DIF_SVY  = FORMULE(VALE='(SVY_N1(t)-SVY_N1X(t))**2', NOM_PARA = 't',)
DIF_MFZ  = FORMULE(VALE='(MFZ_N1(t)-MFZ_N1X(t))**2', NOM_PARA = 't',)
DIF_DY  = FORMULE(VALE='(DY_N3(t)-DY_N3X(t))**2', NOM_PARA = 't',)
DIF_ADRZ  = FORMULE(VALE='(ADRZ_N3(t)-ADRZ_N3X(t))**2', NOM_PARA = 't',)
DIF_VY  = FORMULE(VALE='(VY_G1(t)-VY_G1X(t))**2', NOM_PARA = 't',)

#pour limiter le temps calcul, on ne fait le calcul que sur 10 points
echant = 10
pas = TFIN/echant
inst_list = [i*pas for i in range(echant+1)]

D_SVY = CALC_FONC_INTERP(FONCTION = DIF_SVY,
                         VALE_PARA = inst_list,
                         NOM_PARA='INST',)

D_MFZ = CALC_FONC_INTERP(FONCTION = DIF_MFZ,
                         VALE_PARA = inst_list,
                         NOM_PARA='INST',)

D_DY = CALC_FONC_INTERP(FONCTION = DIF_DY,
                         VALE_PARA = inst_list,
                         NOM_PARA='INST',)

D_ADRZ = CALC_FONC_INTERP(FONCTION = DIF_ADRZ,
                         VALE_PARA = inst_list,
                         NOM_PARA='INST',)

D_VY = CALC_FONC_INTERP(FONCTION = DIF_VY,
                         VALE_PARA = inst_list,
                         NOM_PARA='INST',)

ERR_SVY  = CALC_FONCTION(INTEGRE=_F(FONCTION = D_SVY))
ERR_MFZ  = CALC_FONCTION(INTEGRE=_F(FONCTION = D_MFZ))
ERR_DY  = CALC_FONCTION(INTEGRE=_F(FONCTION = D_DY))
ERR_ADRZ  = CALC_FONCTION(INTEGRE=_F(FONCTION = D_ADRZ))
ERR_VY  = CALC_FONCTION(INTEGRE=_F(FONCTION = D_VY))

TEST_FONCTION(VALEUR=(_F(FONCTION  = ERR_SVY  , VALE_PARA = inst_list[-1],
                         VALE_CALC = 0.0      , VALE_REFE = 0.0 ,
                         CRITERE   = 'ABSOLU' , REFERENCE='AUTRE_ASTER',), ),)

TEST_FONCTION(VALEUR=(_F(FONCTION  = ERR_MFZ  , VALE_PARA = inst_list[-1],
                         VALE_CALC = 0.0      , VALE_REFE = 0.0 ,
                         CRITERE   = 'ABSOLU' , REFERENCE='AUTRE_ASTER',), ),)

TEST_FONCTION(VALEUR=(_F(FONCTION  = ERR_DY  , VALE_PARA = inst_list[-1],
                         VALE_CALC = 0.0      , VALE_REFE = 0.0 ,
                         CRITERE   = 'ABSOLU' , REFERENCE='AUTRE_ASTER',), ),)

TEST_FONCTION(VALEUR=(_F(FONCTION  = ERR_ADRZ  , VALE_PARA = inst_list[-1],
                         VALE_CALC = 0.0      , VALE_REFE = 0.0 ,
                         CRITERE   = 'ABSOLU' , REFERENCE='AUTRE_ASTER',), ),)

TEST_FONCTION(VALEUR=(_F(FONCTION  = ERR_VY  , VALE_PARA = inst_list[-1],
                         VALE_CALC = 0.0      , VALE_REFE = 0.0 ,
                         CRITERE   = 'ABSOLU' , REFERENCE='AUTRE_ASTER',), ),)

dN25_Y1a = RECU_FONCTION(RESULTAT= RESPHY1,
                         NOM_CHAM= 'DEPL',
                         NOEUD   = 'N2',
                         INTERPOL= 'LIN',
                         NOM_CMP = 'DY');

dN25_Y1b = RECU_FONCTION(RESU_GENE= TRANGENE,
                         NOM_CHAM = 'DEPL',
                         NOEUD    = 'N2',
                         INTERPOL = 'LIN',
                         NOM_CMP  = 'DY');


# ---- PRISE EN COMPTE DES MODES STATIQUES DANS LA BASE MODALE
#      =======================================================

MODE_STA = MODE_STATIQUE(MATR_RIGI   = MATR,
                         MATR_MASS   = MATM,
                         FORCE_NODALE=_F(NOEUD=('N14','N26','N38','N2'),
                                         AVEC_CMP=('DX','DY',)),
                         INFO=1);

BASEMODA = DEFI_BASE_MODALE(RITZ =(_F(MODE_MECA=MODE,
                                      NMAX_MODE=30),
                                   _F(MODE_INTF=MODE_STA)),
                            NUME_REF=NDDL,
                            INFO=2);

#IMPR_CO(CHAINE='BASEMODA           .REFE');
#IMPR_CO(CHAINE='MODE_STA           .REFE');
#IMPR_CO(CHAINE='MODE               .REFE');

NDDLGEN2 = NUME_DDL_GENE(BASE= BASEMODA,
                         STOCKAGE= 'PLEIN');

# MATRICES GENERALISEES
# ---------------------
# PROJECTION DANS LA BASSE MODALE

PROJ_BASE(BASE=BASEMODA,
          MATR_ASSE_GENE=(_F(MATRICE  =CO('RIGIPRO2'),
                             MATR_ASSE=MATR),
                          _F(MATRICE  =CO('MASSPRO2'),
                             MATR_ASSE=MATM),
                          _F(MATRICE  =CO('AMORPRO2'),
                             MATR_ASSE=MATA)),
          STOCKAGE='PLEIN');

VECTPRO2=PROJ_VECT_BASE(BASE         =BASEMODA,
                        VECT_ASSE    =VECT,
                        NUME_DDL_GENE=NDDLGEN2,
                        TYPE_VECT    ='FORC');

TRANGEN2=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='GENE',
                         SCHEMA_TEMPS=_F(SCHEMA='DIFF_CENTRE',),
                         MATR_MASS=MASSPRO2,
                         MATR_RIGI=RIGIPRO2,
                         MATR_AMOR=AMORPRO2,

                         INCREMENT=_F(INST_INIT= 0.,
                                      INST_FIN = TFIN,
                                      PAS      = DELT),
                         ARCHIVAGE=_F(PAS_ARCH=10),
                         EXCIT=_F(VECT_ASSE_GENE = VECTPRO2,
                                  FONC_MULT = SINOT),
                         COMPORTEMENT=(
                               _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N14',
                                  JEU      = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (.609, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN),
                               _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N26',
                                  JEU      = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (1.218, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN),
                               _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N38',
                                  JEU      = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (1.827, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN),
                               _F(RELATION = 'DIS_CHOC',
                                  NOEUD_1  = 'N2',
                                  JEU      = .406E-3,
                                  OBSTACLE = BUTE,
                                  ORIG_OBST= (2.436, 0.0, 0.0, ),
                                  NORM_OBST= ( 1.,  0., 0., ),
                                  RIGI_NOR = KN,
                                  AMOR_NOR = CN)),
                           INFO=1,
                           IMPRESSION=_F(NIVEAU  = 'DEPL_LOC',
                           #              TOUT='OUI',
                           #             INST_INIT= 0.01,
                                        INST_FIN= 0.01));

POST2=POST_DYNA_MODA_T(RESU_GENE=TRANGEN2,
                      CHOC=_F(INST_INIT= TDEB,
                              INST_FIN = TFIN));
IMPR_TABLE(TABLE=POST2);

TEST_TABLE(VALE_CALC=24.5382986473,
           NOM_PARA='RMS_T_TOTAL',
           TABLE=POST2,
           FILTRE=(_F(NOM_PARA='CALCUL',
                      VALE_K='FORCE_NORMALE',),
                   _F(NOM_PARA='NOEUD',
                      VALE_K='N2',),
                   ),
           )


FIN();
