#          SIMPLEMENT APPUYEE
#---------------------------------------------------------------------
#  - CALCUL QUASI-STATIQUE D'UNE PLAQUE SOUMISE A UNE PRESSION
#    UNIFORMEMENT REPARTIE
#  - MODELISATION AVEC DES ELEMENTS 'DST'(TRIA3)
#---------------------------------------------------------------------
#
DEBUT( CODE=_F( NOM = 'SSLS503C',NIV_PUB_WEB='INTERNET'))

#
#PRE_IDEAS ();
#
MA=LIRE_MAILLAGE( )

#
MO=AFFE_MODELE(   MAILLAGE=MA,
                        AFFE=_F( TOUT = 'OUI',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'DST')
                  )

#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAR_ELE=AFFE_CARA_ELEM(   MODELE=MO,
                             COQUE=_F( GROUP_MA = 'TOUT_ELT',
                                    EPAIS = 0.012,
                                    ANGL_REP = (0.,-48.5,))
                          )

#
#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT=DEFI_MATERIAU(   ELAS_ORTH=_F( E_L = 4.E+10,
                                  E_T = 0.16E+10,
                                  G_LT = 8.E+08,
                                  G_TN = 3.2E+08,
                                  NU_LT = 0.25)
                     )

#
MULTI=DEFI_COQU_MULT( COUCHE=(  _F( EPAIS = 0.004,
                                  MATER = MAT,
                                  ORIENTATION = 0.),
                          _F( EPAIS = 0.004,
                                  MATER = MAT,
                                  ORIENTATION = 90.),
                          _F( EPAIS = 0.004,
                                  MATER = MAT,
                                  ORIENTATION = 0.))
                        )

#
CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MA,
                                AFFE=_F( TOUT = 'OUI',
                                      MATER = MULTI)
                          )

#
#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------
#
CHA=AFFE_CHAR_MECA(        MODELE=MO,DDL_IMPO=(
                           _F( GROUP_NO = 'COTEBC',
                                          DY = 0.),
                           _F( GROUP_NO = 'COTECD',
                                          DY = 0.)),LIAISON_OBLIQUE=(
                    _F( GROUP_NO = 'COTEAD',
                                          DY = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,)),
                    _F( GROUP_NO = 'COTEAD',
                                          DRX = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,)),
                    _F( GROUP_NO = 'COTEAB',
                                          DX = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,)),
                    _F( GROUP_NO = 'COTEAB',
                                          DRY = 0.,
                                    ANGL_NAUT = (0., -48.5, 90.,))),
                        FORCE_COQUE=_F( TOUT = 'OUI',
                                     PRES = -3000.)
                      )

#
#---------------------------------------------------------------------
#                         RESOLUTION
#---------------------------------------------------------------------
#
RESU=MECA_STATIQUE(       MODELE=MO,
                        CHAM_MATER=CHAM_MAT,
                         CARA_ELEM=CAR_ELE,
                             EXCIT=_F( CHARGE = CHA)
                      )


#COUCHE 2 : CALCUL DES CONTRAINTES
##################################
SIG_C2_S=CALC_ELEM(       RESULTAT=RESU,
                          MODELE=MO,
                          NUME_ORDRE=1,
                          OPTION='SIGM_ELNO_DEPL',
                          CHAM_MATER=CHAM_MAT,
                          CARA_ELEM=CAR_ELE,
                          NUME_COUCHE=2,
                          NIVE_COUCHE='SUP'
                          )

#
SIG_C2_M=CALC_ELEM(       RESULTAT=RESU,
                          MODELE=MO,
                          CHAM_MATER=CHAM_MAT,
                          CARA_ELEM=CAR_ELE,
                          NUME_ORDRE=1,
                          OPTION='SIGM_ELNO_DEPL',
                          NUME_COUCHE=2,
                          NIVE_COUCHE='MOY'
                          )

#
#COUCHE 3 : CALCUL DES CONTRAINTES
##################################
SIG_C3_S=CALC_ELEM(       RESULTAT=RESU,
                          MODELE=MO,
                          CHAM_MATER=CHAM_MAT,
                          CARA_ELEM=CAR_ELE,
                          NUME_ORDRE=1,
                          OPTION='SIGM_ELNO_DEPL',
                          NUME_COUCHE=3,
                          NIVE_COUCHE='SUP'
                          )


#
TEST_RESU(
     RESU=_F( RESULTAT = RESU,               NOM_CHAM = 'DEPL',
           GROUP_NO = 'A',                  NOM_CMP = 'DY',
           NUME_ORDRE = 1, REFERENCE='ANALYTIQUE',
           VALE = 0.01507,           PRECISION = 0.01)
           )

#
TEST_RESU(RESU=(
_F( RESULTAT = SIG_C3_S,  NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA11', GROUP_NO = 'A', NOM_CMP = 'SIXX',REFERENCE='SOURCE_EXTERNE',
    VALE = 2.4216E+07,         PRECISION = 0.07),
_F( RESULTAT = SIG_C3_S, NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA12', GROUP_NO = 'A', NOM_CMP = 'SIXX',REFERENCE='SOURCE_EXTERNE',
    VALE = 2.4216E+07,         PRECISION = 0.175),
_F( RESULTAT = SIG_C2_S, NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA11', GROUP_NO = 'A', NOM_CMP = 'SIYY',REFERENCE='SOURCE_EXTERNE',
    VALE = 5.7810E+06,         PRECISION = 0.175),
_F( RESULTAT = SIG_C2_S, NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA12', GROUP_NO = 'A', NOM_CMP = 'SIYY',REFERENCE='SOURCE_EXTERNE',
    VALE = 5.7810E+06,         PRECISION = 0.24),
_F( RESULTAT = SIG_C3_S, NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA61', GROUP_NO = 'C', NOM_CMP = 'SIXY',REFERENCE='SOURCE_EXTERNE',
    VALE = 1.2825E+06,         PRECISION = 0.04),
_F( RESULTAT = SIG_C3_S, NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA62', GROUP_NO = 'C', NOM_CMP = 'SIXY',REFERENCE='SOURCE_EXTERNE',
    VALE = 1.2825E+06,         PRECISION = 0.04),
           
_F( RESULTAT = SIG_C2_M, NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA72', GROUP_NO = 'D', NOM_CMP = 'SIXZ',REFERENCE='SOURCE_EXTERNE', 
    VALE = -2.3525E+05,        PRECISION = 0.36),
           
_F( RESULTAT = SIG_C2_M, NUME_ORDRE=1, NOM_CHAM='SIGM_ELNO_DEPL',
    MAILLE = 'MA1',  GROUP_NO = 'B', NOM_CMP = 'SIYZ', REFERENCE='SOURCE_EXTERNE',
    VALE = 8.8950E+04,         PRECISION = 0.25),
           ))

#
FIN()
