#  DETAILS : MODELISATION : MULTI-COUCHES
#                          (DKT- ENDO_ISOT_BETON+2*GRILLE ACIER)
#                                           VS.
#                           DKT- BETON_PR+2*GRILLE ACIER)
#            MAILLAGE     : TRIA3
#            CHARGEMENT   : CYCLIQUE DISTORSION ET CISAILLEMENT

DEBUT(CODE=_F(
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))
EP = 0.1;

D0 = 0.00033;

t1 = 1.0;

t2 = (3 * t1);

t3 = (4.0 * t1);

dt = 0.05;

fx = 500000.0;


MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='A1A2',),
                               _F(GROUP_MA='A4A3',),
                               _F(GROUP_MA='A2A4',),
                               _F(GROUP_MA='A3A1',),
                               _F(DIFFE=('BORDS','A1',),
                                  NOM='CL_IMPOS',),),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='DALLE',
                                      VECT_NORM=(0.,0.,1.,),
                                      GROUP_NO='A1',),);

MA=CREA_MAILLAGE(MAILLAGE=MAIL,
                 CREA_MAILLE=(_F(NOM='ACIER1L',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A1L',),
                                _F(NOM='ACIER2L',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A2L',),
                                _F(NOM='ACIER1T',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A1T',),
                                _F(NOM='ACIER2T',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='A2T',),
                                _F(NOM='DALLE_G',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='G',),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               INFO=1,
               AFFE=(_F(GROUP_MA=('DALLE','A1A2','A4A3','A2A4','A3A1',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='DKT',),
                     _F(GROUP_MA='ACIER1L',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),
                     _F(GROUP_MA='ACIER1T',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),
                     _F(GROUP_MA='ACIER2L',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),
                     _F(GROUP_MA='ACIER2T',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='GRILLE_EXCENTRE',),),);

EB = 32308000000.0;

BETON_E=DEFI_MATERIAU(ELAS=_F(E=EB,
                              NU=0.2,
                              RHO=2500.0,),
                      BETON_ECRO_LINE=_F(D_SIGM_EPSI=(-0.2 * EB),
                                         SYT=3.4E6,),
                      BETON_REGLE_PR=_F(D_SIGM_EPSI=(-0.2 * EB),
                                        SYT=3.4E6,
                                        SYC  = 32.308e6,
                                        EPSC = 0.002,
                                        N = 2
                                        ),                   );                   

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.E11,
                            NU=0.2,
                            RHO=7800.0,
                            AMOR_ALPHA=3.7771128018011707E-05,
                            AMOR_BETA=2.6476993459625175,),
                    ECRO_LINE=_F(D_SIGM_EPSI=3.E8,
                                 SY=5.70000000E8,),
                    INFO=1,);


MATER=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='DALLE',
                             MATER=BETON_E,),
                          _F(GROUP_MA='ACIER1L',
                             MATER=ACIER,),
                          _F(GROUP_MA='ACIER1T',
                             MATER=ACIER,),
                          _F(GROUP_MA='ACIER2L',
                             MATER=ACIER,),
                          _F(GROUP_MA='ACIER2T',
                             MATER=ACIER,),),);

CAEL=AFFE_CARA_ELEM(MODELE=MO,
                    INFO=1,
                    COQUE=_F(GROUP_MA='DALLE',
                             EPAIS=EP,
                             ANGL_REP=(0.0,0.0,),
                             COQUE_NCOU=1,
                             INER_ROTA='OUI',
                             MODI_METRIQUE='NON',),
                    GRILLE=(_F(GROUP_MA='ACIER1L',
                               SECTION=8.0E-4,
                               ANGL_REP=(0.0,0.0,),
                               EXCENTREMENT=(-(EP) * 0.4),
                               COEF_RIGI_DRZ=1.E-10,),
                            _F(GROUP_MA='ACIER1T',
                               SECTION=8.0E-4,
                               ANGL_REP=(90.0,0.0,),
                               EXCENTREMENT=(-(EP) * 0.4),
                               COEF_RIGI_DRZ=1.E-10,),
                            _F(GROUP_MA='ACIER2L',
                               SECTION=8.0E-4,
                               ANGL_REP=(0.0,0.0,),
                               EXCENTREMENT=(EP * 0.4),
                               COEF_RIGI_DRZ=1.E-10,),
                            _F(GROUP_MA='ACIER2T',
                               SECTION=8.0E-4,
                               ANGL_REP=(90.0,0.0,),
                               EXCENTREMENT=(EP * 0.4),
                               COEF_RIGI_DRZ=1.E-10,),),);

UX_IMPOS = FORMULE(VALE='D0*Y',
                   NOM_PARA='Y',);

UY_IMPOS = FORMULE(VALE='D0*X',
                   NOM_PARA='X',);

COND_0=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=_F(GROUP_NO='A1',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),);

COND_1=AFFE_CHAR_MECA_F(MODELE=MO,
                        DDL_IMPO=_F(GROUP_NO='CL_IMPOS',
                                    DX=UX_IMPOS,
                                    DY=UY_IMPOS,),);

chf1=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                         t1,1.,
                         t2,-1.,
                         t3,0.,
                         ),PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',);

inst1=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=t3,
                                   PAS=dt,),);

DEFLIST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                    LIST_INST=inst1,),
                       ECHEC=_F(ACTION        = 'DECOUPE',
                                SUBD_METHODE  = 'MANUEL',
                                SUBD_PAS=4,
                                SUBD_PAS_MINI=0.0001,),);

RESU_R=STAT_NON_LINE(MODELE=MO,
                     CHAM_MATER=MATER,
                     CARA_ELEM=CAEL,
                     EXCIT=(_F(CHARGE=COND_0,
                               FONC_MULT=chf1,),
                            _F(CHARGE=COND_1,
                               FONC_MULT=chf1,),),
                     COMPORTEMENT=(_F(RELATION='ENDO_ISOT_BETON',
                                    GROUP_MA='DALLE',),
                                _F(RELATION='GRILLE_ISOT_LINE',
                                   GROUP_MA=('ACIER1L','ACIER2L','ACIER1T','ACIER2T',),),),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  PRECISION=0.1,),
                     NEWTON=_F(REAC_ITER=1,),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=25,),);

RESU_R=CALC_CHAMP(reuse =RESU_R,RESULTAT=RESU_R,PRECISION=1.E-3,
             FORCE=('FORC_NODA',),)

RESU_G=STAT_NON_LINE(MODELE=MO,
                     CHAM_MATER=MATER,
                     CARA_ELEM=CAEL,
                     EXCIT=(_F(CHARGE=COND_0,
                               FONC_MULT=chf1,),
                            _F(CHARGE=COND_1,
                               FONC_MULT=chf1,),),
                     COMPORTEMENT=(_F(RELATION='BETON_REGLE_PR',
                                    GROUP_MA='DALLE',),
                                _F(RELATION='GRILLE_ISOT_LINE',
                                   GROUP_MA=('ACIER1L','ACIER2L','ACIER1T','ACIER2T',),),),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  PRECISION=0.1,),
                     NEWTON=_F(REAC_ITER=1,),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=25,),);

RESU_G=CALC_CHAMP(reuse =RESU_G,RESULTAT=RESU_G,PRECISION=1.E-3,
             FORCE=('FORC_NODA',),
             )

TFN_B_R=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='B_FN',
                                RESULTAT=RESU_R,
                                NOM_CHAM='FORC_NODA',
                                GROUP_NO='B',
                                NOM_CMP=('DX','DY',),),);

TFN_B_G=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='B_FNG',
                                RESULTAT=RESU_G,
                                NOM_CHAM='FORC_NODA',
                                GROUP_NO='B',
                                NOM_CMP=('DX','DY',),),);

FORM_A = FORMULE(VALE='abs(DX+DY)',
                 NOM_PARA=('DX','DY',),);

FORM_V = FORMULE(VALE='(DX*DX+DY*DY)**0.5*(DX+DY)/abs(DX+DY)',
                 NOM_PARA=('DX','DY',),);

DIF_REL = FORMULE(VALE='(VG-V)/V',
                  NOM_PARA=('V','VG',),);

TFN_B_R=CALC_TABLE(reuse =TFN_B_R,
                   TABLE=TFN_B_R,
                   ACTION=_F(OPERATION='EXTR',
                             NOM_PARA=('INST','DX','DY',),),);

TFN_B_R=CALC_TABLE(reuse =TFN_B_R,
                   TABLE=TFN_B_R,
                   ACTION=_F(OPERATION='OPER',
                             FORMULE=FORM_A,
                             NOM_PARA='DI',),);

TFN_B_R=CALC_TABLE(reuse =TFN_B_R,
                   TABLE=TFN_B_R,
                   ACTION=_F(OPERATION='FILTRE',
                             NOM_PARA='DI',
                             CRIT_COMP='NE',
                             VALE=0.0,),);

TFN_B_R=CALC_TABLE(reuse =TFN_B_R,
                   TABLE=TFN_B_R,
                   ACTION=_F(OPERATION='SUPPRIME',
                             NOM_PARA='DI',),);

TFN_B_R=CALC_TABLE(reuse =TFN_B_R,
                   TABLE=TFN_B_R,
                   ACTION=(_F(OPERATION='OPER',
                              FORMULE=FORM_V,
                              NOM_PARA='V',),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DX','FX',),),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DY','FY',),),),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='EXTR',
                             NOM_PARA=('INST','DX','DY',),),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='OPER',
                             FORMULE=FORM_A,
                             NOM_PARA='DI',),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='FILTRE',
                             NOM_PARA='DI',
                             CRIT_COMP='NE',
                             VALE=0.0,),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=_F(OPERATION='SUPPRIME',
                             NOM_PARA='DI',),);

TFN_B_G=CALC_TABLE(reuse =TFN_B_G,
                   TABLE=TFN_B_G,
                   ACTION=(_F(OPERATION='OPER',
                              FORMULE=FORM_V,
                              NOM_PARA='VG',),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DX','FXG',),),
                           _F(OPERATION='RENOMME',
                              NOM_PARA=('DY','FYG',),),),);

TFN_B=CALC_TABLE(TABLE=TFN_B_R,
                 ACTION=_F(OPERATION='COMB',
                           TABLE=TFN_B_G,
                           NOM_PARA='INST',),);

TFN_B=CALC_TABLE(reuse =TFN_B,
                 TABLE=TFN_B,
                 ACTION=_F(OPERATION='FILTRE',
                           NOM_PARA='V',
                           CRIT_COMP='NE',
                           VALE=0.0,),);

TFN_B=CALC_TABLE(reuse =TFN_B,
                 TABLE=TFN_B,
                 ACTION=(_F(OPERATION='OPER',
                            FORMULE=DIF_REL,
                            NOM_PARA='VG_V',),
                         # _F(OPERATION='COMB',
                            # TABLE=TVAR_G,
                            # NOM_PARA='INST',)
                            ),);

# IMPR_TABLE(TABLE=TFN_B,);

#----- DISTORSION POSITIF - PHASE ELASTIQUE -----

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.2,
           VALE_CALC=0.187689471727,
           VALE_REFE=0.0,
           NOM_PARA='VG_V',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.25,),
           )

#----- DISTORSION POSITIF - PHASE ENDOMMAGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.1,
           VALE_CALC=0.0997431026625,
           VALE_REFE=0.0,
           NOM_PARA='VG_V',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

#----- DISTORSION POSITIF - PHASE DECHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.4,
           VALE_CALC=0.30842720555,
           VALE_REFE=0.0,
           NOM_PARA='VG_V',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.5,),
           )

#----- DISTORSION NEGATIF - PHASE CHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.1,
           VALE_CALC=0.0997431026625,
           VALE_REFE=0.0,
           NOM_PARA='VG_V',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,),
           )

#----- DISTORSION NEGATIF - PHASE DECHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.4,
           VALE_CALC=0.30842720555,
           VALE_REFE=0.0,
           NOM_PARA='VG_V',
           TABLE=TFN_B,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.5,),
           )

         
FIN();
