#
# 5.3.2. Les tractions

CH2_1=AFFE_CHAR_MECA_F(MODELE=MODE_1,
                       FORCE_CONTOUR=(_F(GROUP_MA='GMP2',
                                         FX=F1,
                                         FY=F2,),
                                      _F(GROUP_MA='GMP3',
                                         FX=F3,
                                         FY=F4,),),);
#
# 5.4. Resolution

RESU_1=MECA_STATIQUE(MODELE=MODE_1,
                     CHAM_MATER=MATE_1,
                     EXCIT=(_F(CHARGE=CH1_1,),
                            _F(CHARGE=CH2_1,),),);
#
# 5.5. Calcul de l'indicateur d'erreur

RESU_1=CALC_CHAMP(reuse=RESU_1,RESULTAT=RESU_1,CONTRAINTE=('SIGM_ELNO'))


RESU_1=CALC_ERREUR(reuse =RESU_1,
                   RESULTAT=RESU_1,
                   OPTION=('ERME_ELEM',),
);
#
# 5.6. Calcul de la contrainte a comparer

RESU_1=CALC_CHAMP(reuse =RESU_1,
               RESULTAT=RESU_1,
               CONTRAINTE='SIGM_NOEU',);
#
# 5.7. Impressions pour creer la base du cas-test homard sfr_mix_2d_zzzz121d_d1 a l'iteration 1
##IMPR_RESU(
##FORMAT='MED',
##UNITE=81,
##RESU=_F(RESULTAT=RESU_1,),
##);

#
# 6. Adaptation du maillage de 1 vers 2
#    On en profite pour tester des mises a jour de champs aux noeuds et aux elements
#    a un moment complique : des triangles deviennent des quadrangles et reciproquement.

MACR_ADAP_MAIL(MAILLAGE_N=M_1,
               MAILLAGE_NP1=CO('M_2'),
               ADAPTATION='RAFF_DERA',
               RESULTAT_N=RESU_1,
               NOM_CHAM='ERME_ELEM',
               NOM_CMP='ERREST',
               CRIT_RAFF_PE=0.6,
               CRIT_DERA_PE=0.2,
               MAILLAGE_FRONTIERE=ARC,
               MAJ_CHAM=_F(CHAM_MAJ=CO('DEPL_1_2'),
                           TYPE_CHAM='NOEU_DEPL_R',
                           RESULTAT=RESU_1,
                           NOM_CHAM='DEPL',
                           NUME_ORDRE=1,),
               QUALITE='OUI',);
#
# 7. Calcul sur le maillage 2
#
# 7.1. Mise en place du materiau

MATE_2=AFFE_MATERIAU(MAILLAGE=M_2,
                     AFFE=_F(TOUT='OUI',
                             MATER=MATER1,),);
#
# 7.2. Le modele

MODE_2=AFFE_MODELE(MAILLAGE=M_2,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);
#
# 7.3. Chargement
# 7.3.1. Les bords de symetrie et la partie libre

CH1_2=AFFE_CHAR_MECA(MODELE=MODE_2,
                     FACE_IMPO=(_F(GROUP_MA='GMP1',
                                   DY=0.,),
                                _F(GROUP_MA='GMP4',
                                   DX=0.,),),
                     PRES_REP=_F(GROUP_MA=('GMP5',), PRES=0.0,),);
#
# 7.3.2. Les tractions

CH2_2=AFFE_CHAR_MECA_F(MODELE=MODE_2,
                       FORCE_CONTOUR=(_F(GROUP_MA='GMP2',
                                         FX=F1,
                                         FY=F2,),
                                      _F(GROUP_MA='GMP3',
                                         FX=F3,
                                         FY=F4,),),);
#
# 7.4. Resolution

RESU_2=MECA_STATIQUE(MODELE=MODE_2,
                     CHAM_MATER=MATE_2,
                     EXCIT=(_F(CHARGE=CH1_2,),
                            _F(CHARGE=CH2_2,),),);
#
# 7.5. Calcul des contraintes

RESU_2=CALC_CHAMP(reuse=RESU_2,RESULTAT=RESU_2,CONTRAINTE=('SIGM_ELNO'))


RESU_2=CALC_ERREUR(reuse =RESU_2,
                   RESULTAT=RESU_2,
                   OPTION=('ERME_ELEM',),
);
#
# 7.6. Calcul de la contrainte a comparer

RESU_2=CALC_CHAMP(reuse =RESU_2,
               RESULTAT=RESU_2,
               CONTRAINTE='SIGM_NOEU',);
#
# 7.7. Impressions pour creer la base du cas-test homard sfr_mix_2d_zzzz121d_d1 a l'iteration 2
##IMPR_RESU(
##FORMAT='MED',
##UNITE=82,
##RESU=_F(RESULTAT=RESU_2,),
##);

#
# 8. Adaptation du maillage de 2 vers 3

MACR_ADAP_MAIL(MAILLAGE_N=M_2,
               MAILLAGE_NP1=CO('M_3'),
               ADAPTATION='RAFF_DERA',
               RESULTAT_N=RESU_2,
               NOM_CHAM='ERME_ELEM',
               NOM_CMP='ERREST',
               CRIT_RAFF_PE=0.01,
               CRIT_DERA_PE=0.8,
               MAILLAGE_FRONTIERE=ARC,
               QUALITE='OUI',);
#
# 9. Calcul sur le maillage 3
#
# 9.1. Mise en place du materiau

MATE_3=AFFE_MATERIAU(MAILLAGE=M_3,
                     AFFE=_F(TOUT='OUI',
                             MATER=MATER1,),);
#
# 9.2. Le modele

MODE_3=AFFE_MODELE(MAILLAGE=M_3,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);
#
# 9.3. Chargement
# 9.3.1. Les bords de symetrie et la partie libre

CH1_3=AFFE_CHAR_MECA(MODELE=MODE_3,
                     FACE_IMPO=(_F(GROUP_MA='GMP1',
                                   DY=0.,),
                                _F(GROUP_MA='GMP4',
                                   DX=0.,),),
                     PRES_REP=_F(GROUP_MA=('GMP5',), PRES=0.0,),);
#
# 9.3.2. Les tractions

CH2_3=AFFE_CHAR_MECA_F(MODELE=MODE_3,
                       FORCE_CONTOUR=(_F(GROUP_MA='GMP2',
                                         FX=F1,
                                         FY=F2,),
                                      _F(GROUP_MA='GMP3',
                                         FX=F3,
                                         FY=F4,),),);
#
# 9.4. Resolution

RESU_3=MECA_STATIQUE(MODELE=MODE_3,
                     CHAM_MATER=MATE_3,
                     EXCIT=(_F(CHARGE=CH1_3,),
                            _F(CHARGE=CH2_3,),),);
#
# 9.5. Calcul des contraintes

RESU_3=CALC_CHAMP(reuse=RESU_3,RESULTAT=RESU_3,CONTRAINTE=('SIGM_ELNO'))

#
# 9.6. Calcul de la contrainte a comparer

RESU_3=CALC_CHAMP(reuse =RESU_3,
               RESULTAT=RESU_3,
               CONTRAINTE='SIGM_NOEU',);
#
# N. Tests de non regression

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='GNP20',
                   RESULTAT=RESU_1,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=0.83646560000000003,
                   
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=1,
                   GROUP_NO='GNP20',
                   RESULTAT=RESU_2,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=0.50229053000000001,
                   
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=1,
                   GROUP_NO='GNP20',
                   RESULTAT=RESU_3,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=0.51272200999999995,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=1,
                   GROUP_NO='GNP21',
                   RESULTAT=RESU_1,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=1.5763891000000001,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=1,
                   GROUP_NO='GNP21',
                   RESULTAT=RESU_2,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=1.590414,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=1,
                   GROUP_NO='GNP21',
                   RESULTAT=RESU_3,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=1.7902384,
                   TOLE_MACHINE=9.9999999999999995E-08,
