                   ),
                ),
          CHAM_NO=_F(CRITERE='RELATIF',
                     NOM_CMP='DX',
                     GROUP_NO='GNP21',
                     CHAM_GD=DEPL_1_2,
                     VALE_CALC=7.7330000000000003E-06,
                     ),
          )

