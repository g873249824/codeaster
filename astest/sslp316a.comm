# RESPONSABLE DELMAS J.DELMAS
# TEST V3.02.316
# CHARGEMENT EN DEPLACEMENT
# MAILLAGE : QUAD LINEAIRE
# COMPORTEMENT ELASTIQUE C_PLAN

DEBUT(CODE=_F(NOM='SSLP316A',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

e = 100000.0;
nu = 0.0;

ACIER=DEFI_MATERIAU(ELAS=_F(E=e,NU=nu,RHO=7800.0,),);

lx = 1.0;
ly = 1.0;

# a : profondeur de la fissure  
# h : hauteur du plan de fissure
a = 0.5;
h = 0.5;

LN = FORMULE(VALE='Y-h',NOM_PARA=('X','Y',),);

LT = FORMULE(VALE='-X+a',NOM_PARA=('X','Y',),);

R = FORMULE(VALE='sqrt((X-0.5)**2+(Y-0.5)**2)',NOM_PARA=('X','Y',),);

T = FORMULE(VALE='atan2(Y-0.5,-X+0.5)',NOM_PARA=('X','Y',),);

coef = ((1 + nu) / (e * sqrt((2.0 * pi))));

# CHAMPS DE DEPLACEMENT

OUAUX1 = FORMULE(VALE='-1.*coef*sqrt(R(X,Y))*cos(T(X,Y)/2.)*(3.-4.*nu-cos(T(X,Y)))',NOM_PARA=('X','Y',),);

UAUX2 = FORMULE(VALE=' coef*sqrt(R(X,Y))*sin(T(X,Y)/2.)*(3.-4.*nu-cos(T(X,Y)))',NOM_PARA=('X','Y',),);

UAUX1 = FORMULE(VALE='-1.* OUAUX1(X,Y)',NOM_PARA=('X','Y',),);

H1YSUP = FORMULE(VALE='coef*sqrt(R(X,0.5))*(4.-4.*nu)',NOM_PARA=('X','Y',),);

# INSTANTS

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.0,
                                    NOMBRE=3,),);

PRE_GIBI();

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,                                                                                    
              MAILLAGE=MA,                                                                                  
              CREA_GROUP_NO=(_F(GROUP_MA=('LIGSUP','LIGINF','LIGGAU','LIGDRO'),),
                             _F(NOM='NHSUP',OPTION='ENV_SPHERE',POINT=(lx,(11+1.)/(2.*11)),RAYON=1/(4.*11),PRECISION=1/(4.*11)),
                             _F(NOM='NHINF',OPTION='ENV_SPHERE',POINT=(lx,(11-1.)/(2.*11)),RAYON=1/(4.*11),PRECISION=1/(4.*11)),
                             _F(NOM='ENVELTOU',UNION=('LIGSUP','LIGINF','LIGGAU','LIGDRO',),),
                             _F(NOM='ENVELCLA',DIFFE=('ENVELTOU','NHSUP','NHINF',),),),);

MOD=AFFE_MODELE(MAILLAGE=MA,
                AFFE=_F(GROUP_MA='SURF',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='C_PLAN',),);

FISS=DEFI_FISS_XFEM(MODELE=MOD,
                    DEFI_FISS=_F(FONC_LT=LT,
                                 FONC_LN=LN,),);

MODK=MODI_MODELE_XFEM(MODELE_IN=MOD,
                      FISSURE=FISS,);

CHXFM=AFFE_CHAR_MECA(MODELE=MODK,
                     LIAISON_XFEM='OUI',);

MAT=AFFE_MATERIAU(MAILLAGE=MA,
                  MODELE=MODK,
                  AFFE=_F(GROUP_MA='SURF',
                          MATER=ACIER,),);

CH2=AFFE_CHAR_MECA_F(MODELE=MODK,
                     DDL_IMPO=(_F(GROUP_NO='ENVELCLA',
                                  DX=OUAUX1,
                                  DY=UAUX2,),
                               _F(GROUP_NO='NHSUP',
                                  DX =OUAUX1,
                                  DY = UAUX2,
                                  H1X=OUAUX1,
                                  H1Y=H1YSUP,),
                               _F(GROUP_NO='NHINF',
                                  DX =OUAUX1,
                                  DY = UAUX2,
                                  H1X=UAUX1,
                                  H1Y=H1YSUP,),),);

UTOT=STAT_NON_LINE(MODELE=MODK,
                   CHAM_MATER=MAT,
                   EXCIT=(_F(CHARGE=CHXFM,),
                          _F(CHARGE=CH2,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='SURF',),
                   INCREMENT=_F(LIST_INST=L_INS1,
                                INST_FIN=1.0,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),);

UTOT=CALC_ELEM(reuse =UTOT,
               RESULTAT=UTOT,
               OPTION=('SIEF_ELNO','ERME_ELEM',),);

TEST_RESU(RESU=(_F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO',
                   NOM_CMP='SIXX',
                   TYPE_TEST='MAX',
                   VALE=1.42,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.16',),
                _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO',
                   NOM_CMP='SIXX',
                   TYPE_TEST='MIN',
                   VALE=4.90e-2,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.16',),
                 _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO',
                   NOM_CMP='SIXY',
                   TYPE_TEST='MAX',
                   VALE=1.46,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.16',),
                _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO',
                   NOM_CMP='SIXY',
                   TYPE_TEST='MIN',
                   VALE=-1.28,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.16',),
                 _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_SENO_SEGA',
                   NOM_CMP='X4',
                   TYPE_TEST='MAX',
                   VALE=2.44e-1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.16',),
                _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_SENO_SEGA',
                   NOM_CMP='X4',
                   TYPE_TEST='MIN',
                   VALE=-6.98e-1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.16',),
                 _F(RESULTAT=UTOT,
                    INST=1.0,
                    NOM_CHAM='ERME_ELEM',
                    NOM_CMP='ERREST',
                    TYPE_TEST='MAX',
                    VALE=5.73e-4,
                    CRITERE='RELATIF',
                    PRECISION=0.005,
                    REFERENCE='NON_REGRESSION',
                    VERSION='9.03.16',),
                 _F(RESULTAT=UTOT,
                    INST=1.0,
                    NOM_CHAM='ERME_ELEM',
                    NOM_CMP='ERREST',
                    TYPE_TEST='MIN',
                    VALE=3.69e-6,
                    CRITERE='RELATIF',
                    PRECISION=0.005,
                    REFERENCE='NON_REGRESSION',
                    VERSION='9.03.16',),),);

FIN();
