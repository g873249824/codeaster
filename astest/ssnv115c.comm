DEBUT(CODE=_F(NOM='SSNV115C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',);

M=DEFI_GROUP(reuse =M,
             MAILLAGE=M,
             CREA_GROUP_MA=_F(NOM='TOUT',
                              TOUT='OUI',),);

M=DEFI_GROUP(reuse =M,
             MAILLAGE=M,
             CREA_GROUP_NO=(_F(GROUP_MA='SURF',),
                            _F(GROUP_MA='BOUT1',),
                            _F(GROUP_MA='BOUT2',),),);

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='DKT',),);

CARAC=AFFE_CARA_ELEM(MODELE=MO,
                     COQUE=_F(GROUP_MA='TOUT',
                              EPAIS=0.05,
                              COQUE_NCOU=7,),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=2000.0,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=200.0,
                                 SY=100.0,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=ACIER,),);

CH_LA=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='SURF',
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,),
                               _F(GROUP_NO='BOUT1',
                                  DX=0.0,
                                  DY=0.0,
                                  DRZ=0.0,),
                               _F(GROUP_NO='BOUT1',
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,),),);

CH_FO=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_NODALE=_F(GROUP_NO='BOUT2',
                                     FX=0.25,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0,),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=10,),);

L_INST1=DEFI_LIST_REEL(DEBUT=0.1,
                       INTERVALLE=_F(JUSQU_A=1.0,
                                     NOMBRE=9,),);
# STAT_NON_LINE 'VMIS_ISOT_LINE'
#-------------------------------

U2=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CM,
                 CARA_ELEM=CARAC,
                 EXCIT=(_F(CHARGE=CH_FO,
                           FONC_MULT=RAMPE,),
                        _F(CHARGE=CH_LA,),),
                 COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                              RESI_INTE_RELA=1e-08,
                              ITER_INTE_MAXI=30,),
                 INCREMENT=_F(LIST_INST=L_INST,
                              NUME_INST_FIN=2,),
                 NEWTON=_F(MATRICE='ELASTIQUE',),
                 CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                ITER_GLOB_MAXI=30),);

U2=STAT_NON_LINE(reuse =U2,
                 MODELE=MO,
                 CHAM_MATER=CM,
                 CARA_ELEM=CARAC,
                 EXCIT=(_F(CHARGE=CH_FO,
                           FONC_MULT=RAMPE,),
                        _F(CHARGE=CH_LA,),),
                 COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                              RESI_INTE_RELA=1e-08,
                              ITER_INTE_MAXI=30,),
                 ETAT_INIT=_F(EVOL_NOLI=U2,
                              NUME_ORDRE=2,),
                 INCREMENT=_F(LIST_INST=L_INST,
                              NUME_INST_FIN=10,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=2,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=1e-06),);

U2=CALC_CHAMP(reuse =U2,
           RESULTAT=U2,
           LIST_INST=L_INST1,
           FORCE='REAC_NODA',
           );

TEST_RESU(RESU=(_F(NUME_ORDRE=10,
                   GROUP_NO='X',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=0.027034997,
                   VALE_REFE=0.02743,
                   PRECISION=0.02,),
                _F(NUME_ORDRE=10,
                   GROUP_NO='X',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-0.288716248,
                   VALE_REFE=-0.28039999999999998,
                   PRECISION=0.035000000000000003,),
                _F(NUME_ORDRE=10,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2,
                   NOM_CHAM='REAC_NODA',
                   NOEUD='N1',
                   NOM_CMP='DX',
                   VALE_CALC=-0.250000000,
                   VALE_REFE=-0.25,
                   PRECISION=1.E-3,),
                ),
          )

FIN();
