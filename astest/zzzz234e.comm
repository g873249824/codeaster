                                        VALE = ( 2.6389E8, 2.6389E8,),),
                                    _F(  GROUP_MA = 'TOUT',
                                         CARA = 'K_T_D_L',
                                         # K = EA/L
                                        VALE = ( 2.6389E8, 2.6389E8,),),
