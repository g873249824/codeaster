                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'B',
                   NOM_CMP    = 'DX',
                   VALE       = -7.5789154571981E-06,
                   
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '10.2.20',
                   PRECISION  = 1E-8,
