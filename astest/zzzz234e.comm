           CRITERE = 'ABSOLU',        PRECISION = 0., REFERENCE='NON_REGRESSION',);


TEST_RESU(RESU=(_F(RESULTAT   = RE_2DTRL,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'B',
                   NOM_CMP    = 'DY',
                   VALE       = -4.1684035014589E-06,
                   
