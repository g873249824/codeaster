                                      MOMENT_REFE = CoefRef,
                                     ),
   );

TB_2DITL = RECU_TABLE(CO=RE_2DITL,NOM_PARA=('ITER_GLOB'),)

IMPR_TABLE(TABLE=TB_2DITL)
TEST_TABLE(CRITERE='ABSOLU',
