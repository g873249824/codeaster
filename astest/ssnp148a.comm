#CONTRAINTES AVEC ENDO_HETEROGENE
#

DEBUT( CODE=_F(
        NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),
        PAR_LOT='OUI',DEBUG=_F(SDVERI='OUI'));

from math import *

# AFFECTATION DE LA GEOMETRIE


INSTEL=DEFI_LIST_REEL(INTERVALLE=(_F(JUSQU_A=1,NOMBRE=1),
                                   ),
                      DEBUT=0,
                      );

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = INSTEL),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS=4,
                                 SUBD_PAS_MINI=0.000001,),)

PRE_GMSH();
MAILIN=LIRE_MAILLAGE();
MAIL=CREA_MAILLAGE(MAILLAGE=MAILIN,
                   LINE_QUAD=_F(TOUT='OUI'),
                   );
MAIL=DEFI_GROUP(reuse=MAIL,
                CREA_GROUP_MA=(_F(GROUP_MA='GM41',NOM='gauchem'),
                               _F(GROUP_MA='GM42',NOM='droitem'),
                               _F(GROUP_MA='GM43',NOM='postm'),
                               _F(GROUP_MA='GM40',NOM='pointgm'),
                               _F(GROUP_MA='GM61',NOM='restm'),
                                ),

                CREA_GROUP_NO=(_F(GROUP_MA='gauchem',NOM='gauchen'),
                               _F(GROUP_MA='droitem',NOM='droiten'),
                               _F(GROUP_MA='pointgm',NOM='pointgn'),
                               _F(GROUP_MA='postm',NOM='postn'),
                               _F(GROUP_MA='restm',NOM='restn'),
                               _F(GROUP_MA='GM70',NOM='sifg'),
                               _F(GROUP_MA='GM71',NOM='sifd'),
                               ),
                MAILLAGE=MAIL,
                );


# DEFINITION DU COMPORTEMENT DES MATERIAUX

ACIER=DEFI_MATERIAU(
                ELAS=_F(       #PARAMETRES ELASTIQUES
                E = 20.0E9,
                NU = 0.25),

                ENDO_HETEROGENE=_F(   #PARAMETRES ENDO-FRAGILE
                SY = 1.0E18,
                WEIBULL = 2.0,
                KI = 1.0E9,
                EPAI = 1.0,
                GR = 121.0,
                          ),

               NON_LOCAL=_F(
               LONG_CARA = 0.020),
                 );


MAT=AFFE_MATERIAU(
              MAILLAGE=MAIL,
                AFFE=(_F(
                GROUP_MA = 'restm',
                MATER = ACIER),
                  )
              )

# CHOIX DU MODELE (GRAD_SIGM)


MO_MECA=AFFE_MODELE(
               MAILLAGE=MAIL,
                AFFE=(_F(
                TOUT='OUI',
                PHENOMENE = 'MECANIQUE',
                MODELISATION = 'D_PLAN_GRAD_SIGM'),
                  ),
                );


# DEFINITION DU CHARGEMENT

#FIXATION DE LA POUTRE
LIAISON=AFFE_CHAR_MECA(
                   MODELE=MO_MECA,
                   DDL_IMPO=(
                      _F(GROUP_NO = 'gauchen',DX=0.0),
                      _F(GROUP_NO = 'pointgn',DY=0.0),
                            ),
                );

LIST_PA=DEFI_LIST_REEL(VALE=(0.,1.0),
                       );
LIST_PR=DEFI_LIST_REEL(VALE=(0.0,-1.0E4),
                       );
fdx= DEFI_FONCTION(  NOM_PARA = 'INST',
  VALE_PARA=LIST_PA,
  VALE_FONC=LIST_PR,
  PROL_DROITE='CONSTANT',
  PROL_GAUCHE='CONSTANT',
  INTERPOL=('LIN','LIN'),
  )

TRACTEL=AFFE_CHAR_MECA_F(MODELE=MO_MECA,
                       PRES_REP=(_F(GROUP_MA='droitem',PRES=fdx),),
                        );
#
#DEFINITION DES CHAMPS INITIAUX
#
#CHAMPS DE POSITION AUX NOEUDS
#
CHXN=CREA_CHAMP(OPERATION='EXTR', TYPE_CHAM='NOEU_GEOM_R',
      NOM_CHAM='GEOMETRIE', MAILLAGE=MAIL, INFO=1)

#CHAMPS DE POSITION AU PTS DE GAUSS
CHXG=CREA_CHAMP(OPERATION='DISC', TYPE_CHAM='ELGA_GEOM_R',
      MODELE=MO_MECA,   CHAM_GD=CHXN,  INFO=1 )


#DEFINITION DE LA FONCTION D'ENDOMMAGEMENT PROJETEE (CRENEAUX)

b=0.5 # le b du .geo
n=175.0 # le n du .geo
l=0.5 # le b du .geo
ll=2.75 # ll+lll du .geo
nd=125.0 # longueur de la fissure en elems (longfidd/hauteur = nd/n)
g=ll+(l/2)-((l/(2.0*n))*0.99999)
d=ll+(l/2)+((l/(2.0*n))*0.99999)
ah=0.99999*b*nd/n
ab=0.99999*b*(1-nd/n)


def hv1(x,y) :
        if (x>=g) and (x<=d) and (y<=ah) : return 0.999999999
        else : return 0.

def hv2(x,y) :
        if (x>=g) and (x<=d) and (y<=ah) : return 2.0
        else : return 0.

def hv3(x,y) :
        if (x>=g) and (x<=d) and (y<=ah) : return 0.0
        else : return 0.

VARI1 = FORMULE(NOM_PARA=('X','Y'),VALE='hv1(X,Y)')
VARI2 = FORMULE(NOM_PARA=('X','Y'),VALE='hv2(X,Y)')
VARI3 = FORMULE(NOM_PARA=('X','Y'),VALE='hv3(X,Y)')

#CREATION DU CHAMPS DE VARIABLE INTERNES AUX PTS DE GAUSS (6 COMPOSANTES)

VA1=CREA_CHAMP(OPERATION='AFFE', TYPE_CHAM='ELGA_NEUT_F',
     MODELE=MO_MECA, INFO=2,PROL_ZERO='OUI',
     AFFE=_F(
      GROUP_MA = 'restm', NOM_CMP = ('X1','X2','X3'),
              VALE_F = (VARI1,VARI2,VARI3),),);

VA2=CREA_CHAMP(OPERATION='EVAL', TYPE_CHAM='ELGA_NEUT_R', INFO=1,
                CHAM_F=VA1,  CHAM_PARA=( CHXG,)   )

VAINI=CREA_CHAMP(OPERATION='ASSE', TYPE_CHAM='ELGA_VARI_R',
      MODELE=MO_MECA,
      INFO=1, PROL_ZERO='OUI',
      ASSE=_F( GROUP_MA = 'restm',   CHAM_GD = VA2,
            NOM_CMP = ('X1','X2','X3'),
            NOM_CMP_RESU = ('V1','V2','V12')))



#CALCUL

EVOEL=STAT_NON_LINE(SOLVEUR=_F(METHODE='MULT_FRONT',),
                        CHAM_MATER=MAT,
                        ETAT_INIT=_F(VARI=VAINI,),
                        MODELE=MO_MECA,
                        EXCIT=(
                             _F(CHARGE = LIAISON),
                             _F(CHARGE = TRACTEL),
                             ),
                        COMPORTEMENT=_F(
                            RELATION = 'ENDO_HETEROGENE',
                            DEFORMATION = 'PETIT'),
                       INCREMENT=_F(LIST_INST=DEFLIST,
                                    ),
                       NEWTON =_F(MATRICE='TANGENTE',
                            REAC_INCR = 1,
                            REAC_ITER=1,
                                  ),
                       CONVERGENCE = (_F(ITER_GLOB_MAXI=30,
                                        RESI_GLOB_RELA=1.E-4,
                                     ),),
                       );

#
# RECUPERATION DES CONTRAINTES REGULARISEES
#
SBARG=POST_RELEVE_T(ACTION=(_F(INTITULE='sbarg',
                               GROUP_NO='sifg',
                               RESULTAT=EVOEL,
                               NOM_CHAM=('DEPL'),
                               NOM_CMP='SIXX',
                               INST = 1.,
                               OPERATION= 'EXTRACTION',
                               PRECISION=9.99999E-04,
                               ),
                         ),
                  );
SBARD=POST_RELEVE_T(ACTION=(_F(INTITULE='sbard',
                               GROUP_NO='sifd',
                               RESULTAT=EVOEL,
                               NOM_CHAM=('DEPL'),
                               NOM_CMP='SIXX',
                               OPERATION= 'EXTRACTION',
                               INST = 1.,
                               PRECISION=9.99999E-04,
                               ),
                             ),
                           );

SIGG=CALC_TABLE(TABLE=SBARG,
                         TITRE = ' ',
                         ACTION=(_F(OPERATION='RENOMME',NOM_PARA=('SIXX','SIXX_G'),),
                                 ),
                       );

SIGD=CALC_TABLE(TABLE=SBARD,
                         TITRE = ' ',
                         ACTION=(_F(OPERATION='RENOMME',NOM_PARA=('SIXX','SIXX_D'),),
                                 ),
                       );
SIGC=CALC_TABLE(TABLE=SIGD,
                         TITRE = ' ',
                         ACTION=(
                                 _F(OPERATION='COMB',TABLE=SIGG,NOM_PARA='INST'),
                                 ),
                       );

#===============================================
## Calcul de SIF par la solution analytique
#===============================================
pi=3.14159
# longueur de la fissure :
lf=nd/n*b
#
pia=pi*lf
cc=pia/(2*b)
ki2=pia/cos(cc)
KIA=sqrt(ki2)*1.E4

# Calcul de SIF par la contrainte regularisee
lca=0.02
pilc=pi*lca
ck=1.7434 #(5*pi)/(6*1.2254)
ckl=ck*(sqrt(pilc))
#KIC=ckl*(SIXX_G+SIXX_D)/2.
#RKI=KIA/KIC
#ECARTKI=200.0*abs(KIA-KIC)/(KIA+KIC)



F_FIC = FORMULE(NOM_PARA=('SIXX_G','SIXX_D'),VALE='KIA/(ckl*(SIXX_G+SIXX_D)/2.)')
SIGOFC=CALC_TABLE(TABLE=SIGC,
                  TITRE = ' ',
                  ACTION=(
                  _F(OPERATION='OPER',FORMULE=F_FIC,NOM_PARA=('F_FIC')),
                        ),
                       );

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-2,
           VALE_CALC=0.990211668,
           VALE_REFE=1.0,
           NOM_PARA='F_FIC',
           TABLE=SIGOFC,
           FILTRE=_F(PRECISION=1.E-2,
                     NOM_PARA='INST',
                     VALE=1.0,),
           )

FIN();
