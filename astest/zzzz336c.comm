DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI',),)

# ON TESTE LA METHODE SOUS_POINT DE PROJ_CHAMP POUR DES ELEMENTS POU_D_EM
# UN CHAMP DE TEMPERATURE EST PROJETEE POUR UN CALCUL MECANIQUE
# LES SOLUTIONS DE REFERENCE SONT UN AUTRE CALCUL ASTER AVEC DES 3D
# ET UNE REFERENCE ANALYTIQUE



# CALCUL MONO-MATERIAU : REFERENCE 3D
# ===================================

LINST =  DEFI_LIST_REEL(VALE=(0.,1.,2.))


# Thermique 3D
MATH=LIRE_MAILLAGE(FORMAT='MED')
MOTHER=AFFE_MODELE(MAILLAGE=MATH,
                 AFFE=_F(GROUP_MA='POUTRE',PHENOMENE='THERMIQUE',MODELISATION='3D'))

# DEFINITION DE LA FONCTION TEMPERATURE
# -------------------------------------

FTEMP = FORMULE(NOM_PARA=('Z','INST'),VALE='(-200*Z*+20)*INST')

# CONSTRUCTION DES CHAMPS DE GEOMETRIE
# ------------------------------------

CHXN=CREA_CHAMP(OPERATION='EXTR',
                TYPE_CHAM='NOEU_GEOM_R',
                NOM_CHAM='GEOMETRIE',
                MAILLAGE=MATH,
                )

# AFFECTATION DES FONCTIONS
# -------------------------


TEMP_F=CREA_CHAMP(OPERATION='AFFE',
                 TYPE_CHAM='NOEU_NEUT_F',
                 MAILLAGE=MATH,
                 AFFE=_F( TOUT = 'OUI',
                          NOM_CMP = ('X1',),
                          VALE_F = (FTEMP,)
                         )
                )

# CONTRUCTION D'UN RESULTAT DE TYPE EVOL_THER
# -------------------------------------------

l_inst=[0.0,1.0,2.0,]


TEMP={}
l_affe_temp=[]

for i,inst in enumerate(l_inst):


    INSTN=CREA_CHAMP(OPERATION='AFFE',
                TYPE_CHAM='NOEU_INST_R',
                MAILLAGE=MATH,
                AFFE=_F( TOUT = 'OUI',
                            NOM_CMP = ('INST',),
                            VALE =inst,
                        )
                )

    TEMP2=CREA_CHAMP(OPERATION='EVAL',
                    TYPE_CHAM='NOEU_NEUT_R',
                    CHAM_F=TEMP_F,
                    CHAM_PARA=( CHXN,INSTN,)   )

    TEMP[i]=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='NOEU_TEMP_R',
                    MAILLAGE=MATH,
                    ASSE=_F( TOUT = 'OUI',
                            CHAM_GD = TEMP2,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('TEMP',),
                        )
                )
    DETRUIRE(CONCEPT=_F(NOM=(TEMP2,INSTN,)))

    l_affe_temp.append(_F( CHAM_GD = TEMP[i],  MODELE = MOTHER, INST = inst,))


RESUTHER = CREA_RESU( OPERATION = 'AFFE',
                      TYPE_RESU = 'EVOL_THER',
                      NOM_CHAM  = 'TEMP',
                      AFFE = l_affe_temp )

# MATERIAUX

BETON=DEFI_MATERIAU(ELAS=_F(E=3.E10, NU=0.2, ALPHA=1.E-5))
ACIER=DEFI_MATERIAU(ELAS=_F(E=2.E11, NU=0.3, ALPHA=2.E-5))


# CALCUL MECANIQUE 3D : REFERENCE
#--------------------------------

MOD_3D=AFFE_MODELE(MAILLAGE=MATH,
                 AFFE=_F(GROUP_MA='POUTRE',PHENOMENE='MECANIQUE',MODELISATION='3D'))

CHMA_3D=AFFE_MATERIAU(MAILLAGE=MATH,
                    AFFE=(_F(GROUP_MA='BAS' ,MATER=BETON),
                          _F(GROUP_MA='HAUT',MATER=BETON),
                          ),
                    AFFE_VARC = _F( GROUP_MA = 'POUTRE', NOM_VARC = 'TEMP', EVOL=RESUTHER,VALE_REF=0.),
                                  )


CL_3D=AFFE_CHAR_MECA(MODELE=MOD_3D,
                    DDL_IMPO=(_F(GROUP_NO=('ENC_0','ENC_1'),DZ=0.),
                              _F(GROUP_NO=('ENC_0'),        DX=0.),
                              _F(GROUP_NO=('P0','P1'),        DY=0.),
                            )
                    )

U_3D=STAT_NON_LINE(MODELE=MOD_3D,
                 CHAM_MATER=CHMA_3D,
                 EXCIT=(_F(CHARGE=CL_3D,),
                                             ),
                 COMPORTEMENT=_F(GROUP_MA=('POUTRE',),RELATION='ELAS'),
                 INCREMENT=_F(LIST_INST=LINST),
                 );


IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT = U_3D, NOM_CHAM=('DEPL',), GROUP_NO = 'N_MIL'))
#IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT = U_3D, NOM_CHAM=('SIEF_ELGA',),))

# CONSTRUCTION DU MODELE MECANIQUE A SOUS-POINTS
# ----------------------------------------------


MAPOU=LIRE_MAILLAGE(FORMAT="ASTER",UNITE=18,)

MOPOU=AFFE_MODELE(MAILLAGE=MAPOU,
                  AFFE=_F(GROUP_MA = 'POUTRE', PHENOMENE='MECANIQUE', MODELISATION='POU_D_TGM',),
                  );

GF=DEFI_GEOM_FIBRE( INFO=1,
                    FIBRE = (_F(GROUP_FIBRE='SBET',
                                COOR_AXE_POUTRE = (0.,0.,),
                                VALE =( -0.075, -0.025, 0.0025,
                                        -0.075, -0.075, 0.0025,
                                        -0.025, -0.025, 0.0025,
                                        -0.025, -0.075, 0.0025,
                                         0.025, -0.025, 0.0025,
                                         0.025, -0.075, 0.0025,
                                         0.075, -0.025, 0.0025,
                                         0.075, -0.075, 0.0025,
                                       ),),
                             _F(GROUP_FIBRE='SACI',
                                COOR_AXE_POUTRE = (0.,0.,),
                                VALE =( -0.075,  0.025, 0.0025,
                                        -0.075,  0.075, 0.0025,
                                        -0.025,  0.025, 0.0025,
                                        -0.025,  0.075, 0.0025,
                                         0.025,  0.025, 0.0025,
                                         0.025,  0.075, 0.0025,
                                         0.075,  0.025, 0.0025,
                                         0.075,  0.075, 0.0025,
                                       ),),
                             ),
                   )

POUCA=AFFE_CARA_ELEM( MODELE=MOPOU,
                        INFO=1,
                        POUTRE=_F(  GROUP_MA=('POUTRE'),
                                    SECTION='RECTANGLE',
                                    CARA=('HY','HZ'),
                                    VALE=(0.2,0.2), ),
                        ORIENTATION=_F( GROUP_MA=('POUTRE'),
                                        CARA='ANGL_VRIL',
                                        VALE=0.0,
                                        ),
                        GEOM_FIBRE=GF,
                        MULTIFIBRE=(_F( GROUP_MA=('POUTRE'), PREC_AIRE=1.0E-06,  PREC_INERTIE=2.55E-01, GROUP_FIBRE=('SBET','SACI') ), )
                    );

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,MATER_SECT=BETON,
                  MULTIFIBRE=(_F(GROUP_FIBRE='SBET',MATER=BETON,RELATION='ELAS',),
                              _F(GROUP_FIBRE='SACI',MATER=BETON,RELATION='ELAS',),
                             ),
                     )

# PROJECTION DU CHAMP THERMIQUE
# -----------------------------

RESU_SP=PROJ_CHAMP(PROJECTION='OUI',METHODE='SOUS_POINT',
                   MODELE_1 = MOTHER,
                   MODELE_2   = MOPOU,
                   RESULTAT=RESUTHER,
                   CARA_ELEM=POUCA,
                   )

CHMA_POU=AFFE_MATERIAU(MAILLAGE=MAPOU,
                    AFFE=(_F(GROUP_MA='POUTRE' ,MATER=(BETON,ACIER)),
                          ),
                    AFFE_VARC = _F( GROUP_MA = 'POUTRE', NOM_VARC = 'TEMP', EVOL=RESU_SP,VALE_REF=0.),
                    AFFE_COMPOR=_F(GROUP_MA = 'POUTRE',COMPOR=COMPF),
                                  )

CL_POU=AFFE_CHAR_MECA(MODELE=MOPOU,
                   DDL_IMPO=(_F(GROUP_NO=('P1','P2'),DZ=0.,DY=0.),
                             _F(GROUP_NO=('P1'),     DX=0., DRX = 0., DRZ=0.),)
                                 )

U_POU=STAT_NON_LINE(MODELE=MOPOU,
                 CARA_ELEM = POUCA,
                 CHAM_MATER=CHMA_POU,
                 EXCIT=(_F(CHARGE=CL_POU,),
                                             ),
                 COMPORTEMENT=_F(GROUP_MA=('POUTRE',),RELATION='MULTIFIBRE'),
                 INCREMENT=_F(LIST_INST=LINST),
                 );

CALC_CHAMP(reuse=U_POU,RESULTAT=U_POU,DEFORMATION=('EPVC_ELGA','EPVC_ELNO'))


IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT = U_POU, NOM_CHAM='DEPL', GROUP_NO = 'P3'))



TEST_RESU(RESU=(_F(INST=2.,
                   RESULTAT=U_3D,
                   NOM_CHAM='DEPL',
                   GROUP_NO='N_MIL',
                   NOM_CMP='DZ',
                   VALE_CALC=-4.01394722938527E-02,
                   ),
               ))


TEST_RESU(RESU=(_F(INST=2.,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U_POU,
                   NOM_CHAM='DEPL',
                   GROUP_NO='P3',
                   NOM_CMP='DZ',
                   VALE_CALC=-4.00000000000267E-02,
                   VALE_REFE=-4.01394722938527E-02,
                   PRECISION=4.E-2,),
               ))

TEST_RESU(RESU=(
                _F(INST=2.,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U_POU,
                   NOM_CHAM='EPVC_ELGA',
                   MAILLE='M1',
                   POINT = 1,
                   SOUS_POINT = 8,
                   NOM_CMP='EPTHER_L',
                   VALE_CALC=0.006,
                   VALE_REFE=0.006,
                   ),
               ))



# CALCUL BI-MATERIAU : REFERENCE ANALYTIQUE
# =========================================


# DEFINITION DE LA FONCTION TEMPERATURE
# -------------------------------------

FTEMP_H = FORMULE(NOM_PARA=('INST'),VALE='20*INST')
FTEMP_B = FORMULE(NOM_PARA=('INST'),VALE='40*INST')


# AFFECTATION DES FONCTIONS
# -------------------------


TEMP_F_H=CREA_CHAMP(OPERATION='AFFE',
                 TYPE_CHAM='NOEU_NEUT_F',
                 MAILLAGE=MATH,
                 AFFE=_F( GROUP_MA = 'HAUT',
                          NOM_CMP = ('X1',),
                          VALE_F = (FTEMP_H,)
                         )
                )

TEMP_F_B=CREA_CHAMP(OPERATION='AFFE',
                 TYPE_CHAM='NOEU_NEUT_F',
                 MAILLAGE=MATH,
                 AFFE=_F( GROUP_MA = 'BAS',
                          NOM_CMP = ('X1',),
                          VALE_F = (FTEMP_B,)
                         )
                )

# CONSTRUCTION DU MODELE MECANIQUE A SOUS-POINTS
# ----------------------------------------------

COMPF2=DEFI_COMPOR(GEOM_FIBRE=GF,MATER_SECT=BETON,
                  MULTIFIBRE=(_F(GROUP_FIBRE='SBET',MATER=BETON,RELATION='ELAS',),
                              _F(GROUP_FIBRE='SACI',MATER=ACIER,RELATION='ELAS',),
                             ),
                     )
# CONTRUCTION DE DEUX RESULTATS DE TYPE EVOL_THER
# -----------------------------------------------

l_inst=[0.0,1.0,2.0,]


TEMPSP={}
l_affe_temp=[]


TEMP0=CREA_CHAMP(OPERATION='AFFE',
                TYPE_CHAM='NOEU_NEUT_R',
                MAILLAGE=MATH,
                AFFE=_F( TOUT = 'OUI',
                            NOM_CMP = ('X1',),
                            VALE =0.0,
                        )
                )


for i,inst in enumerate(l_inst):


    INSTN=CREA_CHAMP(OPERATION='AFFE',
                TYPE_CHAM='NOEU_INST_R',
                MAILLAGE=MATH,
                AFFE=_F( TOUT = 'OUI',
                            NOM_CMP = ('INST',),
                            VALE =inst,
                        )
                )

    TEMP2_H=CREA_CHAMP(OPERATION='EVAL',
                    TYPE_CHAM='NOEU_NEUT_R',
                    CHAM_F=TEMP_F_H,
                    CHAM_PARA=( INSTN,)   )
    TEMP2_B=CREA_CHAMP(OPERATION='EVAL',
                    TYPE_CHAM='NOEU_NEUT_R',
                    CHAM_F=TEMP_F_B,
                    CHAM_PARA=( INSTN,)   )

    TEMP_H=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='NOEU_TEMP_R',
                    MAILLAGE=MATH,
                    ASSE=(_F( TOUT='OUI',
                            CHAM_GD = TEMP0,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('TEMP',),),
                          _F( GROUP_MA = 'HAUT',
                            CHAM_GD = TEMP2_H,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('TEMP',),
                          )
                        )
                )

    TEMP_B=CREA_CHAMP(OPERATION='ASSE',
                    TYPE_CHAM='NOEU_TEMP_R',
                    MAILLAGE=MATH,
                    ASSE=(_F( TOUT='OUI',
                            CHAM_GD = TEMP0,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('TEMP',),),
                          _F( GROUP_MA = 'BAS',
                            CHAM_GD = TEMP2_B,
                            NOM_CMP = ('X1',),
                            NOM_CMP_RESU = ('TEMP',),
                          )
                        )
                )

    TEMPSP_H=PROJ_CHAMP(PROJECTION='OUI',METHODE='SOUS_POINT',
                   MODELE_1 = MOTHER,
                   MODELE_2   = MOPOU,
                   CHAM_GD = TEMP_H ,
                   CARA_ELEM=POUCA,
                   )
    TEMPSP_B=PROJ_CHAMP(PROJECTION='OUI',METHODE='SOUS_POINT',
                   MODELE_1 = MOTHER,
                   MODELE_2   = MOPOU,
                   CHAM_GD = TEMP_B ,
                   CARA_ELEM=POUCA,
                   )

    TEMPSP[i]=CREA_CHAMP(OPERATION = 'ASSE', MODELE   = MOPOU, TYPE_CHAM = 'ELGA_TEMP_R',
                         ASSE = (_F(TOUT = 'OUI', CHAM_GD = TEMPSP_H, CUMUL = 'OUI'),
                                 _F(TOUT = 'OUI', CHAM_GD = TEMPSP_B, CUMUL = 'OUI'),
                         ),
                        )

    DETRUIRE(CONCEPT=_F(NOM=(TEMP2_H,TEMP2_B,TEMP_H,TEMP_B,INSTN,TEMPSP_H,TEMPSP_B)))

    l_affe_temp.append(_F( CHAM_GD = TEMPSP[i],  MODELE = MOPOU, INST = inst,))



RESU_SP2 = CREA_RESU( OPERATION = 'AFFE',
                      TYPE_RESU = 'EVOL_THER',
                      NOM_CHAM  = 'TEMP',
                      AFFE = l_affe_temp )



CHMA_PO2=AFFE_MATERIAU(MAILLAGE=MAPOU,
                    AFFE=(_F(GROUP_MA='POUTRE' ,MATER=(BETON,ACIER)),
                          ),
                    AFFE_VARC = (
                                _F( TOUT = 'OUI',  NOM_VARC = 'TEMP', EVOL=RESU_SP2,VALE_REF=0.),
                                ),
                    AFFE_COMPOR=_F(GROUP_MA = 'POUTRE',COMPOR=COMPF2),
                                  )

U_POU2=STAT_NON_LINE(MODELE=MOPOU,
                 CARA_ELEM = POUCA,
                 CHAM_MATER=CHMA_PO2,
                 EXCIT=(_F(CHARGE=CL_POU,),
                                             ),
                 COMPORTEMENT=_F(GROUP_MA=('POUTRE',),RELATION='MULTIFIBRE'),
                 INCREMENT=_F(LIST_INST=LINST),
                 );

TEST_RESU(RESU=(_F(INST=2.,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U_POU2,
                   NOM_CHAM='DEPL',
                   GROUP_NO='P3',
                   NOM_CMP='DZ',
                   VALE_CALC=0.E0,
                   VALE_REFE=0.E0,
                   PRECISION=1.E-6,
                   CRITERE ='ABSOLU'),
                _F(INST=2.,
                   RESULTAT=U_POU2,
                   NOM_CHAM='DEPL',
                   GROUP_NO='P3',
                   NOM_CMP='DX',
                   VALE_CALC=8.0E-04,
                   ),
               ))
# BLOCAGE DES DEUX EXTREMITES

CL_POU3=AFFE_CHAR_MECA(MODELE=MOPOU,
                   DDL_IMPO=(_F(GROUP_NO=('P1','P2'),DX=0.,DZ=0.,DY=0.,DRX = 0.,DRY=0., DRZ=0.),
                             )
                                 )


U_POU3=STAT_NON_LINE(MODELE=MOPOU,
                 CARA_ELEM = POUCA,
                 CHAM_MATER=CHMA_PO2,
                 EXCIT=(_F(CHARGE=CL_POU3,),
                                             ),
                 COMPORTEMENT=_F(GROUP_MA=('POUTRE',),RELATION='MULTIFIBRE'),
                 INCREMENT=_F(LIST_INST=LINST),
                 );

CALC_CHAMP(reuse=U_POU3,RESULTAT=U_POU3,DEFORMATION=('EPVC_ELGA'))

IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT=U_POU3, NOM_CHAM='SIEF_ELGA', INST = 2.0, MAILLE = 'M1'))


TEST_RESU(RESU=(
                _F(INST=2.,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U_POU3,
                   NOM_CHAM='SIEF_ELGA',
                   MAILLE='M10',
                   POINT=1,
                   SOUS_POINT=1,
                   NOM_CMP='SIXX',
                   VALE_CALC=-2.400000000E+07,
                   VALE_REFE=-2.400000000E+07,
                   CRITERE = 'ABSOLU',
                   PRECISION=1.E-4,
                   ),
                _F(INST=2.,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U_POU3,
                   NOM_CHAM='SIEF_ELGA',
                   MAILLE='M10',
                   POINT=1,
                   SOUS_POINT=9,
                   NOM_CMP='SIXX',
                   VALE_CALC=-1.600000000E+08,
                   VALE_REFE=-1.600000000E+08,
                   CRITERE = 'ABSOLU',
                   PRECISION=1.E-4,
                   ),
               ))

TEST_RESU(RESU=(
                _F(INST=2.,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U_POU3,
                   NOM_CHAM='EPVC_ELGA',
                   MAILLE='M1',
                   POINT = 1,
                   SOUS_POINT = 8,
                   NOM_CMP='EPTHER_L',
                   VALE_CALC=8E-4,
                   VALE_REFE=8E-4,
                   ),
               ))


FIN()
