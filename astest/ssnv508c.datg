*----------------------------------------
*construction des droites et des surfaces 
*----------------------------------------

AB = DROIT NX A B;
BA = inverse(AB);
BC = DROIT NY B C;
CD = DROIT NX C D;
DA = DROIT NY D A;

S5= DALL AB BC CD DA;
V1 = 0. 0.  LZ;
