A1D1 = inverse(D1A1);
V2 = 0. LY  0.;

S4 = DALL A1B1 B1B BA AA1;
S3 = S4 PLUS V2;



D1D = DROIT NZ D1 D;
S2 = DALL AA1 A1D1 D1D DA;

V3 = LX 0. 0.;
S1=S2 PLUS V3;


S  = S1 et S2 et S3 et S4 et S5 et S6;
ELIM 0.0001 S;

*----------------------
