                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DY',
                      PRECISION=1.E-2,
                      CHAM_GD=DEP1,
                      VALE_CALC=-0.180831473,
                      VALE_REFE=-0.17999999999999999,),
                   ),)

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=DEP,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N27',
                   NOM_CMP='MFZ',
                   VALE_CALC=-10.000000000,
                   VALE_REFE=-10.0,
                   PRECISION=1.E-2,
                   MAILLE='S1',),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=DEP,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N29',
                   NOM_CMP='MFZ',
