                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-2,
                   MAILLE='S2',),
                ),
          )

DEP=CALC_CHAMP(reuse =DEP,
            RESULTAT=DEP,
            FORCE='FORC_NODA',
            TOUT='OUI',);

TABLEAU1=POST_RELEVE_T(ACTION=_F(INTITULE='MOMENT',
                                 GROUP_NO='GRNO1',
                                 RESULTAT=DEP,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE=('DX','DY','DZ',),
                                 MOMENT=('DRX','DRY','DRZ',),
                                 POINT=(0.0,0.0,0.0,),
                                 REPERE='GLOBAL',
                                 OPERATION='EXTRACTION',),);

TABLEAU2=POST_RELEVE_T(ACTION=_F(INTITULE='MOMENT',
                                 GROUP_NO='GRNO2',
                                 RESULTAT=DEP,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE=('DX','DY','DZ',),
                                 MOMENT=('DRX','DRY','DRZ',),
                                 POINT=(10.0,0.0,0.0,),
                                 REPERE='GLOBAL',
                                 OPERATION='EXTRACTION',),);

TABLEAU3=POST_RELEVE_T(ACTION=_F(INTITULE='MOMENT',
                                 GROUP_NO='GRNO3',
                                 RESULTAT=DEP,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE=('DX','DY','DZ',),
                                 MOMENT=('DRX','DRY','DRZ',),
                                 POINT=(20.0,0.0,0.0,),
                                 REPERE='GLOBAL',
                                 OPERATION='EXTRACTION',),);




# validation (non-regression) de CALC_FERRAILLAGE sur COQUE_3D :
