                                      NOEUD_2=('N21','N25',),
                                      DDL_1='DY',
                                      COEF_MULT_1=1.0,
                                      DDL_2='DY',
                                      COEF_MULT_2=-1.0,
                                      COEF_IMPO=F0,),
                                   _F(NOEUD_1=('N4','N16',),
                                      NOEUD_2=('N21','N25',),
                                      DDL_1='DZ',
                                      COEF_MULT_1=1.0,
                                      DDL_2='DZ',
                                      COEF_MULT_2=-1.0,
                                      COEF_IMPO=F0,),
                                   _F(NOEUD_1=('N5','N11','N17',),
                                      GROUP_NO_2='GRNO3',
                                      DDL_1='DX',
                                      COEF_MULT_1=1.0,
                                      DDL_2='DRZ',
                                      COEF_MULT_2=0.5,
                                      COEF_IMPO=F0,),
                                   _F(NOEUD_1=('N3','N15','N10',),
                                      GROUP_NO_2='GRNO3',
                                      DDL_1='DX',
                                      COEF_MULT_1=1.0,
                                      DDL_2='DRZ',
                                      COEF_MULT_2=-0.5,
                                      COEF_IMPO=F0,),),
                    LIAISON_ELEM=(_F(OPTION='COQ_POU',
                                     GROUP_MA_1='GRMA3',
                                     GROUP_NO_2='C',
                                     CARA_ELEM=CARA,
                                     AXE_POUTRE=(1.0,0.0,0.0,),),
                                  _F(OPTION='3D_POU',
                                     GROUP_MA_1='FACE1',
                                     GROUP_NO_2='ORIG',),),
                    FORCE_NODALE=_F(NOEUD='N29',
                                    FY=FM1,),
                    INFO=2,);

