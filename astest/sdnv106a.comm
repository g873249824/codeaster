                NEWTON=_F(MATRICE = 'TANGENTE',REAC_ITER = 1),
                CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                               ITER_GLOB_MAXI = 10),
                CRIT_STAB = _F(TYPE='FLAMBEMENT',
                               COEF_DIM_ESPACE=5,LIST_INST = L_INSTC,OPTION='BANDE',
