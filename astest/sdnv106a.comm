                MODE_VIBR = _F(OPTION='BANDE',FREQ=(0.1,0.5),MATR_RIGI='ELASTIQUE',LIST_INST=L_INSTM),
                RECH_LINEAIRE=_F(METHODE='MIXTE',),
             );

