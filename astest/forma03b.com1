               MODELE=MO,
               CHAM_MATER=CM,);

LH=INTE_MAIL_2D(MAILLAGE=M,
                DEFI_CHEMIN=_F(GROUP_MA='haut',),);

FHAUT=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
