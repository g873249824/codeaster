# CAS_TEST__:SSNV128U
# TEST DU GRECO CALCUL 3D
# MODELE
# MAILLAGE : ELEMENT 3D HEXAEDRE A 8 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)

DEBUT(CODE=_F(NOM='SSNV128U',
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))

#......................................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 3D HEXA8
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#......................................................................

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI',),FORMAT='MED',);

#

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(GROUP_MA='SCONTAR',
                                NOM='NCONTAR',),
                             _F(GROUP_MA='VPLAQ',
                                NOM='NPLAQ',),
                             _F(DIFFE=('NPLAQ','NCONTAR',),
                                NOM='NPLAQZ',),),);

#

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

#

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=(_F(GROUP_MA='SPRESV',),
                               _F(GROUP_MA='SPRESH',),),);

#

MATPLA=DEFI_MATERIAU(ELAS=_F(E=2.1E11,
                             NU=0.2,),);

#

MATBAT=DEFI_MATERIAU(ELAS=_F(E=1.0E16,
                             NU=0.0,),);

#

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='VPLAQ',
                             MATER=MATPLA,),
                          _F(GROUP_MA='VBATI',
                             MATER=MATBAT,),),);

#

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA='SENCA',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,),
                              _F(GROUP_MA='SBLBAX',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,),
                              _F(GROUP_MA='SBLOCX',
                                 DX=0.0,),
                              _F(GROUP_MA='LBLOCY',
                                 DY=0.0,),
                              _F(GROUP_NO='NPLAQZ',
                                 DZ=0.0,),),
                    PRES_REP=_F(GROUP_MA='SPRESV',
                                PRES=5.E07,),);

CHA2=AFFE_CHAR_MECA(MODELE=MO,
                    PRES_REP=_F(GROUP_MA='SPRESH',
                                PRES=5.E07,),);

#

CHA4=DEFI_CONTACT(MODELE=MO,
                  FORMULATION    = 'CONTINUE',
                  FROTTEMENT     = 'COULOMB',
                  ALGO_RESO_GEOM = 'POINT_FIXE',
                  ALGO_RESO_CONT = 'POINT_FIXE', 
                  ALGO_RESO_FROT = 'NEWTON', 
                  REAC_GEOM      = 'CONTROLE',
                  NB_ITER_GEOM   = 2,
                  ZONE=_F(GROUP_MA_ESCL='SCONTAR',
                          GROUP_MA_MAIT='SBATIR',
                          INTEGRATION='AUTO',
                          COEF_FROT     = 1.E5,
                          COEF_CONT=100,
                          USURE='ARCHARD',
                          K=1.0E-12,
                          H=1.0,
                          COULOMB=1.0,),);

#

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,0.04,1.0));
#

LINST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=(_F(JUSQU_A = 0.01, NOMBRE = 15),
                                  _F(JUSQU_A = 0.04, NOMBRE = 10),),) 

L_INST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                  LIST_INST=LINST,),
                     ECHEC=_F(EVENEMENT='ERREUR',
                              ACTION='DECOUPE',
                              SUBD_METHODE = 'MANUEL',
                              SUBD_PAS=5,
                              SUBD_NIVEAU=5,),);

#

F2=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                       0.01,1.0,
                       0.02,0.0,
                       0.03,-1.0,
                       0.04,0.0,
                       ),);

#
#-----------------------------------------------------------

U4M=STAT_NON_LINE(MODELE=MO,
                  INFO=1,
                  CHAM_MATER=CHMAT,
                  EXCIT=(_F(CHARGE=CHA1,
                            FONC_MULT=RAMPE,),
                         _F(CHARGE=CHA2,
                            FONC_MULT=F2,),),
                  CONTACT=CHA4,
                  COMP_ELAS=_F(RELATION='ELAS',),
                  INCREMENT=_F(LIST_INST=L_INST,
                               INST_FIN=0.04,),
                  NEWTON=_F(REAC_ITER=1,),
                  CONVERGENCE=_F(RESI_GLOB_RELA=1.0E-5,
                                 ITER_GLOB_MAXI=20,
                                 ARRET='OUI',),);

#
#
# TEST DE NON REGRESSION

TEST_RESU(RESU=(_F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-4.7791921716242E7,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N4',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-4.3225643134287998E7,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N10',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-4.7870662495189004E7,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N30',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-5.0017718189496003E7,
                   TOLE_MACHINE=1.E-3,
                   ),
                _F(INST=0.040000000000000001,
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOEUD='N32',
                   NOM_CMP='LAGS_C',
                   VALE_CALC=-1.0002560270083E8,
                   TOLE_MACHINE=1.E-3,
                   ),
                ),
          )

PRFUS4=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='USURE',
                               RESULTAT=U4M,
                               NOM_CHAM='VALE_CONT',
                               NUME_ORDRE=4,
                               GROUP_NO='NCONTAR',
                               NOM_CMP='HN',),);

LAGSC4=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='LAMBDA',
                               RESULTAT=U4M,
                               NOM_CHAM='DEPL',
                               NUME_ORDRE=4,
                               GROUP_NO='NCONTAR',
                               NOM_CMP='LAGS_C',),);

IMPR_TABLE(TABLE=PRFUS4,);

IMPR_TABLE(TABLE=LAGSC4,);

#
#-----------------------------------------------------------
#
#

FIN();

