DEBUT(CODE=_F(NOM='SSLP303A',
              NIV_PUB_WEB='INTERNET',),);

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

#
# DEFINITION DES GROUPES DE NOEUDS

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='ENCAST',),
                               _F(GROUP_MA='LEF',),
                               _F(GROUP_MA='SURF',),
                               _F(GROUP_MA='BORD_CH',),),);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_MA=_F(NOM='VOLUM',
                                 OPTION='APPUI',
                                 GROUP_NO='SURF',
                                 TYPE_APPUI='AU_MOINS_UN',),);

#
# DEFINITION DU MODELE

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);

#
# DEFINITION DU MATERIAU

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.10000000000E11,
                            NU=0.29999999999999999,
                            RHO=7950.0,
                            ALPHA=1.0000000000000001E-05,),);

#
# ATTRIBUTION DU MATERIAU

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
#
# CALCUL DE LA MAISSE TESTE DEFI_GROUP : APPUI

MASTOT=POST_ELEM(MASS_INER=_F(TOUT='OUI',),
                 MODELE=MODELE,
                 CHAM_MATER=CHMAT,);

MASVOL=POST_ELEM(MASS_INER=_F(GROUP_MA='VOLUM',),
                 MODELE=MODELE,
                 CHAM_MATER=CHMAT,);

# LA VALEUR DE LA MASSE EN VOLUM EST UN TIERS DE LA MASSE TOTALE

TEST_TABLE(TABLE=MASVOL,
           NOM_PARA='MASSE',
           VALE=39.75,
           REFERENCE='AUTRE_ASTER',);

CHARGT=AFFE_CHAR_MECA(MODELE=MODELE,
                      DDL_IMPO=_F(GROUP_NO='ENCAST',
                                  DX=0.0,
                                  DY=0.0,),
                      FORCE_CONTOUR=_F(GROUP_MA='BORD_CH',
                                       FY=1.70000E5,),);

#
# RESOLUTION

DEF=MECA_STATIQUE(MODELE=MODELE,
                  CHAM_MATER=CHMAT,
                  EXCIT=_F(CHARGE=CHARGT,),);

#
# RECUPERATION DU CHAMP DE DEPLACEMENTS

U=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
             OPERATION='EXTR',
             RESULTAT=DEF,
             NOM_CHAM='DEPL',
             NUME_ORDRE=1,);

#
# CALCUL DES CONTRAINTES

DEF=CALC_CHAMP(reuse=DEF,RESULTAT=DEF,CONTRAINTE=('SIGM_ELNO'))


#
# TEST_RESU DU DEPLACEMENT
#

TEST_RESU(CHAM_NO=(_F(CHAM_GD=U,
                      NOEUD='N455',
                      NOM_CMP='DY',
                      VALE=0.129,
                      PRECISION=0.004,
                      REFERENCE='ANALYTIQUE',),
                   _F(CHAM_GD=U,
                      NOEUD='N756',
                      NOM_CMP='DY',
                      VALE=0.129,
                      PRECISION=0.004,
                      REFERENCE='ANALYTIQUE',),),);

#
# TEST_RESU DE LA CONTRAINTE
#

TEST_RESU(RESU=(_F(RESULTAT=DEF,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXX',
                   MAILLE='M1',
                   NOEUD='N1',
                   VALE=204000000.0,
                   PRECISION=0.023,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXX',
                   MAILLE='M101',
                   NOEUD='N201',
                   VALE=102000000.0,
                   PRECISION=0.005,
                   REFERENCE='ANALYTIQUE',),),);

FIN();

