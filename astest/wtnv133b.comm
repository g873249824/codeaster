                                GROUP_NO = 'C',
                                RESULTAT = U1,
                                NOM_CHAM = 'SIGM_NOEU',
                                INVARIANT = 'OUI',
                                OPERATION= 'EXTRACTION'));

TAB_VARI=POST_RELEVE_T(ACTION=_F(INTITULE= 'VARI_NOEU',
                                GROUP_NO = 'C',
                                RESULTAT = U1,
                                NOM_CHAM = 'VARI_NOEU',
                                NOM_CMP  = ('V1','V4','V8','V23','V32'),
