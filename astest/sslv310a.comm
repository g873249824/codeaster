                        THETA_3D=_F(  TOUT = 'OUI',
                                   MODULE_FO = MOD_T1,
                                   R_INF_FO = RAY_INF3,
                                   R_SUP_FO = RAY_SUP3)
                    )

G0_C3=CALC_G(          OPTION='CALC_G_GLOB',
                       RESULTAT=CHAMDEPL,
                       THETA=_F(THETA=THETA3),
                       )

#
# CALCUL DE G LOCAL AVEC THETA_LEGENDRE
#
GLO3_LE4=CALC_G(               RESULTAT=CHAMDEPL,
                               THETA=_F(FOND_FISS=FISS1,
                                        R_INF_FO=RAY_INF3,
                                        R_SUP_FO=RAY_SUP3),
                               LISSAGE=_F( DEGRE=4),
                         )

GLOC3=CALC_G(                 RESULTAT=CHAMDEPL,
                              THETA=_F(FOND_FISS=FISS1,
                                       R_INF_FO=RAY_INF3,
                                       R_SUP_FO=RAY_SUP3,),
                              LISSAGE=_F(DEGRE=4,),
                         )

