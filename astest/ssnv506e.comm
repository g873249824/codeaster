                     VALE=1.0),
           VALE=-10190213,
           PRECISION=0.15,
           REFERENCE='SOURCE_EXTERNE',);

#TESTS DE NON-REGRESSION

TEST_RESU(RESU=_F(RESULTAT=U,
                  NOEUD='N295',
                  NUME_ORDRE=63,
                  NOM_CHAM='VALE_CONT',
                  NOM_CMP='RN',
                  VALE=1.1058333934147E+06,
                  PRECISION=0.1,
                  CRITERE='RELATIF',
                  REFERENCE='AUTRE_ASTER',
                  VERSION='8.0.11',),);

TEST_RESU(RESU=_F(RESULTAT=U,
                  NOEUD='N294',
                  NUME_ORDRE=63,
                  NOM_CHAM='VALE_CONT',
                  NOM_CMP='RN',
                  VALE=0.0,
                  PRECISION=1.E-12,
                  CRITERE='ABSOLU',
                  REFERENCE='AUTRE_ASTER',
                  VERSION='8.0.11',),);




