# MODELISATION 2D AXIS : COUPLAGE FLUAGE PROPRE UMLV ET ENDO_ISTO_BETON
# ---------------------------------------------------------------------

DEBUT(CODE=_F(NOM='SSNV169B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE( )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=(_F( GROUP_MA = 'L_INF'),
                            _F( GROUP_MA = 'L_SUP'),
                            _F( GROUP_MA = 'L_GCH'),
                            _F( GROUP_MA = 'L_DRT'),),
            )

MO=AFFE_MODELE( MAILLAGE=M,
                AFFE=_F( TOUT = 'OUI',
               PHENOMENE = 'MECANIQUE',
          MODELISATION = 'AXIS')
              )

FONC1=DEFI_FONCTION( NOM_PARA='SECH',
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 1.0, 1.0,)
                   )

E=DEFI_FONCTION( NOM_PARA='TEMP',
                 PROL_DROITE='LINEAIRE',
                 PROL_GAUCHE='LINEAIRE',
                 VALE=(0.0, 31000., 1.0, 31000.,)
               )

NU=DEFI_FONCTION( NOM_PARA='TEMP',
                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE',
                  VALE=(0.0,0.2,1.0,0.2,)
                ),

ALPHA=DEFI_CONSTANTE(VALE=0.);

BETON=DEFI_MATERIAU( ELAS_FO =_F( E = E,
                                  NU = NU ,
                                  ALPHA = ALPHA ,TEMP_DEF_ALPHA=0.,
                                  FONC_DESORP = FONC1
                                  ),
                     BETON_ECRO_LINE=_F( SYT = 3.0,
                                    D_SIGM_EPSI = -6000.,
                                    SYC = 30.),
                     BETON_UMLV_FP =_F( K_RS   = 1.2E+5,
                                        ETA_RS = 2.21E+10,
                                        K_IS   = 6.22E+4,
                                        ETA_IS = 4.16E+10,
                                        K_RD   = 3.86E+4,
                                        ETA_RD = 6.19E+10,
                                        ETA_ID = 1.64e+12,
                                      ),
                  )

SECH=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=M,
                        AFFE=_F( TOUT = 'OUI',
                              NOM_CMP = 'TEMP',
                              VALE = 1.)
                       )

TEMP=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=M,
                         AFFE=_F(
                               TOUT = 'OUI',
                               NOM_CMP = 'TEMP',
                               VALE = 20.)
                       )

CM=AFFE_MATERIAU(  MAILLAGE=M,
                   AFFE=_F( TOUT = 'OUI',MATER = BETON,),
                   AFFE_VARC=(_F(TOUT='OUI',CHAM_GD=SECH,NOM_VARC='SECH',VALE_REF=1),
                              _F(TOUT='OUI',CHAM_GD=TEMP,NOM_VARC='TEMP',VALE_REF=20),),);


# rap_pic : pourcentage de la force au pic
rap_pic=.6

FORCE=DEFI_FONCTION( NOM_PARA='INST',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 1., 3.*rap_pic/4., 31536010., 3.*rap_pic/4)
                   )

CH10=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'L_INF',
                                  DY       = 0.
                               )
                    )

CH12=AFFE_CHAR_MECA( MODELE=MO,
                     LIAISON_UNIF=_F( GROUP_NO = 'L_DRT',
                                      DDL = 'DX'
                                )
                   )
CH2=AFFE_CHAR_MECA_F( MODELE=MO,
                      FORCE_NODALE=_F( GROUP_NO = 'L_SUP',
                                       FY = FORCE),
            LIAISON_UNIF=_F( GROUP_NO = 'L_SUP',
                                      DDL = 'DY'
                                )
                    )


heure=3600.
jour=24.*heure
annee=365.25*jour
mois=annee/12.
L_INST=DEFI_LIST_REEL(  DEBUT=-1.0,INTERVALLE=(
             _F( JUSQU_A = 0.0,       NOMBRE = 1),
             _F( JUSQU_A = 1.0,       NOMBRE = 1),
             _F( JUSQU_A = annee, NOMBRE = 120),)
           )


U=STAT_NON_LINE( MODELE=MO,
                 CHAM_MATER=CM,
                 EXCIT=(
                   _F(CHARGE = CH10),
                   _F(CHARGE = CH12),
                   _F(CHARGE = CH2)
                 ),
       COMP_INCR=_F(
                 RELATION = 'KIT_DDI',
                 RELATION_KIT=('BETON_UMLV_FP','ENDO_ISOT_BETON'),
                                ITER_INTE_MAXI = 1000),
                 INCREMENT=_F(LIST_INST = L_INST,
                              NUME_INST_FIN = 104),
                 NEWTON=_F(PREDICTION = 'TANGENTE',
                           MATRICE = 'TANGENTE',
                           REAC_ITER = 1),
                 CONVERGENCE=_F(RESI_GLOB_MAXI = 1.E-6,
                                ITER_GLOB_MAXI = 25,)
              )
U=CALC_CHAMP(reuse = U,
            RESULTAT=U,
             DEFORMATION=('EPFP_ELGA'))

TEST_RESU(RESU=(_F(NUME_ORDRE=104,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIYY',
                   VALE_CALC=1.7999998174062,
                   VALE_REFE=1.8,
                   PRECISION=1.E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=104,
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V5',
                   VALE_CALC=3.1088075131078E-05,
                   MAILLE='M1',),
                _F(NUME_ORDRE=104,
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V22',
                   VALE_CALC=0.15469914255124,
                   MAILLE='M1',),
                _F(NUME_ORDRE=104,
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='EPFP_ELGA',
                   NOM_CMP='EPYY',
                   VALE_CALC=5.5715360870327E-05,
                   MAILLE='M1',),
                ),
          )

FIN()
#
