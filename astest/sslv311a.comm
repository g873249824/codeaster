# lissage de lagrange Regu
GLO4=CALC_G(RESULTAT=CHAMDEP2,
            OPTION='CALC_K_G',
            THETA=_F(FOND_FISS=FISS,
                     R_INF_FO=RAY_INF1,
                     R_SUP_FO=RAY_SUP1,),
            LISSAGE=_F(LISSAGE_THETA='LAGRANGE_REGU',
                       LISSAGE_G='LAGRANGE_REGU',),
            )

