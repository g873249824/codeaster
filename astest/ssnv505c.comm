DEBUT(CODE=_F(NOM='SSNV505C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# ---------------------  MAILLAGE ----------------
MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_NO=(_F(TOUT_GROUP_MA='OUI'),
                                 ),);

# -------------------- MODELE ------------------
MODE=AFFE_MODELE(MAILLAGE=MAILL1,
                 VERIF='MAILLE',
                 AFFE=_F(GROUP_MA='STRUCT',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='D_PLAN',),);

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                   MAILLAGE=MAILL1,
                   ORIE_PEAU_2D=_F(GROUP_MA=('SMAI','SESC'),),
                   );

# ------------- DEFINITON DU MATERIAU -----------
MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,
                              NU=0.3333,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=_F(GROUP_MA='STRUCT',
                         MATER=MPOUTRE,),);

# ---------- CONDITIONS AUX LIMITES --------------
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='LA3A',
                                     DX=0.0,DY=0.0,),
                                  _F(GROUP_NO='LB3B',
                                     DX=0.0,DY=0.0,),
                                  ),);

#CH1=AFFE_CHAR_MECA(MODELE=MODE,
#                   FORCE_NODALE=(_F(GROUP_NO='C',
#                                    FY=-2578.18,),
#                                 ),);

CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=(_F(GROUP_NO='C',
                                DY=-800.,),
                             ),);

CH2 = DEFI_CONTACT(MODELE       = MODE,
                   FORMULATION  = 'DISCRETE',

                   ZONE=(
                        _F(
                            GROUP_MA_MAIT='SMAI',
                            GROUP_MA_ESCL='SESC',
                            SANS_GROUP_NO=('B3','A'),
                         ),

                       ),
                  )


RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=50,),),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=25,),),);

U=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                MODELE=MODE,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  = CH2,
                COMP_ELAS=_F(RELATION='ELAS',
                             DEFORMATION='GROT_GDEP',
                             GROUP_MA='STRUCT',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=1.,
                             ),
                NEWTON=_F(REAC_ITER=1,
                          ),
                CONVERGENCE=_F(ITER_GLOB_MAXI=30),
                ARCHIVAGE=_F(LIST_INST=L_ARCH),
                INFO=1,);

U=CALC_CHAMP(reuse =U,
          RESULTAT=U,
          FORCE='FORC_NODA',
          );


TAB_DEPL=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO='C',
                                 RESULTAT=U,
                                 NOM_CHAM='DEPL',
                                 NOM_CMP='DY',
                                 OPERATION='EXTRACTION',),);
TAB_REAC=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('LA3A','LB3B'),
                                 RESULTAT=U,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);
TEST_TABLE(
           VALE_CALC=43.566800000000001,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.20000000000000001,),
           )

TEST_TABLE(
           VALE_CALC=169.59800000000001,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.40000000000000002,),
           )

TEST_TABLE(
           VALE_CALC=322.85023572807,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.59999999999999998,),
           )

TEST_TABLE(
           VALE_CALC=267.16199999999998,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.80000000000000004,),
           )

TEST_TABLE(
           VALE_CALC=309.62299999999999,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

FIN();
