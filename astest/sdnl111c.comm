# Etude du choc de deux poutres maillees 3D
# en traction compression.
#
DEBUT( CODE=_F( NOM ='SDNL111C',NIV_PUB_WEB='INTERNET'),)

MAIL=LIRE_MAILLAGE()

MAIL=DEFI_GROUP( reuse=MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'),
               )

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
       AFFE=(
             _F(GROUP_MA=('POU1','POU2'),
                PHENOMENE='MECANIQUE',
                MODELISATION='3D',),
             ),)


MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_3D=(
                                 _F(GROUP_MA='CNT_POU1',),
                                 _F(GROUP_MA='CNT_POU2',),
                                  ),)          
             

MATER=DEFI_MATERIAU(ELAS=_F(E=2.E11,NU=0.3,RHO=7800.,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(
                          _F(GROUP_MA=('POU1'), 
                             MATER=MATER,),
                          _F(GROUP_MA=('POU2'), 
                             MATER=MATER,),
                          ),)


# *******************************************

CONDLIM=AFFE_CHAR_MECA(MODELE=MODELE,
                       DDL_IMPO=(
                                 _F(TOUT='OUI',
                                    DX=0.,DY=0.),
                                  _F(GROUP_NO='CL_POU2',
                                     DZ=0.,),),
                                 )

VITINI=CREA_CHAMP(   OPERATION='AFFE', 
                     TYPE_CHAM='NOEU_DEPL_R',
                     MAILLAGE=MAIL,
                     AFFE=(_F( GROUP_NO = 'POU1',
                               NOM_CMP = 'DZ',
                               VALE = -1.0),
                           _F( GROUP_NO = 'POU2',
                                NOM_CMP = 'DZ',
                                VALE = 0.0),
                                ),)



                                    
# *******************************************
CONTACT=AFFE_CHAR_MECA(MODELE=MODELE,
                       CONTACT=(
                                 _F(
                                    METHODE='CONTRAINTE',
                                    RECHERCHE='NOEUD_BOUCLE',
                                    GROUP_MA_MAIT='CNT_POU1',
                                    GROUP_MA_ESCL='CNT_POU2',),
                        ),)

                        
# ************************************************************
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A=10.E-4,
                                    PAS=1.E-6,
                                    ),)


RESU=DYNA_NON_LINE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                EXCIT=(
                       _F(CHARGE=CONTACT,),
                       _F(CHARGE=CONDLIM,),
                       ),
                ETAT_INIT=_F(VITE= VITINI),       
                COMP_INCR=_F(RELATION='ELAS',
                             DEFORMATION='PETIT_REAC',),                
                INCREMENT=_F(LIST_INST=L_INST,
                             SUBD_METHODE='UNIFORME', SUBD_PAS=3,
                             SUBD_PAS_MINI=1.E-8,),
                SCHEMA_TEMPS=_F(SCHEMA='HHT',
                                FORMULATION='DEPLACEMENT',
                                ALPHA=-0.1,),
                NEWTON=_F(MATRICE='TANGENTE',),
                SOLVEUR=_F(SYME='OUI',
                           METHODE='MULT_FRONT',),)


##############################################
# TEST DE LA CONFORMITE AVEC LA SOLUTION DE REFERENCE ANALYTIQUE
##############################################

TEST_RESU(
      RESU=(
            _F(
                  RESULTAT = RESU,
                  INST = 2.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = -1.05E-4,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU,
                  INST = 4.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = -1.55E-4,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU,
                  INST = 6.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = -5.54E-5,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU,
                  INST = 8.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = 9.92E-5,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU,
                  INST = 10.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = 2.99E-4,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
                  ),)
#
RESU2=DYNA_NON_LINE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                EXCIT=(
                       _F(CHARGE=CONTACT,),
                       _F(CHARGE=CONDLIM,),
                       ),
                ETAT_INIT=_F(VITE= VITINI),       
                COMP_INCR=_F(RELATION='ELAS',
                             DEFORMATION='PETIT_REAC',),                
                INCREMENT=_F(LIST_INST=L_INST,
                             SUBD_METHODE='UNIFORME', SUBD_PAS=3,
                             SUBD_PAS_MINI=1.E-8,),
                SCHEMA_TEMPS=_F(SCHEMA='HHT',
                                FORMULATION='DEPLACEMENT',
                                ALPHA=-0.3,MODI_EQUI='OUI',),
                NEWTON=_F(MATRICE='TANGENTE',),
                SOLVEUR=_F(SYME='OUI',
                           METHODE='MULT_FRONT',),)


##############################################
# TEST DE LA CONFORMITE AVEC LA SOLUTION DE REFERENCE ANALYTIQUE
##############################################

TEST_RESU(
      RESU=(
            _F(
                  RESULTAT = RESU2,
                  INST = 2.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = -1.05E-4,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU2,
                  INST = 4.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = -1.55E-4,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU2,
                  INST = 6.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = -5.54E-5,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU2,
                  INST = 8.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = 9.92E-5,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = RESU2,
                  INST = 10.0E-4,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'N1',
                  NOM_CMP = 'DZ',
                  VALE = 2.99E-4,
                  PRECISION = (0.05, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
                  ),)
#

FIN();
