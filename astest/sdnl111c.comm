# Etude du choc de deux poutres maillees 3D
# en traction compression.
#
DEBUT( CODE=_F( NOM ='SDNL111C',NIV_PUB_WEB='INTERNET'),)

MAIL=LIRE_MAILLAGE(FORMAT='MED',)

MAIL=DEFI_GROUP( reuse=MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'),
               )

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
       AFFE=(
             _F(GROUP_MA=('POU1','POU2'),
                PHENOMENE='MECANIQUE',
                MODELISATION='3D',),
             ),)


MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_3D=(
                                 _F(GROUP_MA='CNT_POU1',),
                                 _F(GROUP_MA='CNT_POU2',),
                                  ),)          
             

MATER=DEFI_MATERIAU(ELAS=_F(E=2.E11,NU=0.3,RHO=7800.,
                       AMOR_ALPHA=1.E-7, AMOR_BETA=1.E-3,  
                     ),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(
                          _F(GROUP_MA=('POU1'), 
                             MATER=MATER,),
                          _F(GROUP_MA=('POU2'), 
                             MATER=MATER,),
                          ),)


# *******************************************

CONDLIM=AFFE_CHAR_MECA(MODELE=MODELE,
                       DDL_IMPO=(
                                 _F(TOUT='OUI',
                                    DX=0.,DY=0.),
                                 _F(GROUP_NO='CL_POU2',
                                    DZ=0.,),),
                                 )

VITINI=CREA_CHAMP(   OPERATION='AFFE', 
                     TYPE_CHAM='NOEU_DEPL_R',
                     MAILLAGE=MAIL,
                     AFFE=(_F( TOUT='OUI',
                               NOM_CMP = ('DX','DY','DZ'),
                               VALE = (0.0,0.0,0.0)),
                           _F( GROUP_NO = 'POU1',
                               NOM_CMP = 'DZ',
                               VALE = -1.0),
                           _F( GROUP_NO = 'POU2',
                               NOM_CMP = 'DZ',
                               VALE = 0.0),
                                ),)



                                    
# *******************************************

CONTACT = DEFI_CONTACT(MODELE      = MODELE,
                       FORMULATION = 'DISCRETE',
                       ZONE =_F(
                             GROUP_MA_MAIT='CNT_POU1',
                             GROUP_MA_ESCL='CNT_POU2',
                             ),
                      );


                        
# ************************************************************
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A=10.E-4,
                                    PAS=1.E-6,
                                    ),)


RESU=DYNA_NON_LINE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                EXCIT=(
                       _F(CHARGE=CONDLIM,),
                       ),
                CONTACT = CONTACT,       
                ETAT_INIT=_F(VITE= VITINI),       
                COMP_INCR=_F(RELATION='ELAS',
                             DEFORMATION='PETIT_REAC',),                
                INCREMENT=_F(LIST_INST=L_INST,),
                SCHEMA_TEMPS=_F(SCHEMA='HHT',
                                FORMULATION='DEPLACEMENT',
                                ALPHA=-0.1,),
                NEWTON=_F(MATRICE='TANGENTE',),
                SOLVEUR=_F(SYME='OUI',
                           METHODE='MULT_FRONT',),)


##############################################
# TEST DE LA CONFORMITE AVEC LA SOLUTION DE REFERENCE ANALYTIQUE
##############################################

TEST_RESU(RESU=(_F(GROUP_NO='N1',
                   INST=2.0000000000000001E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-1.04999990E-04,
                   VALE_REFE=-1.05E-4,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=4.0000000000000002E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-1.55408246E-04,
                   VALE_REFE=-1.55E-4,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=5.9999999999999995E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-5.54082648E-05,
                   VALE_REFE=-5.5399999999999998E-05,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=8.0000000000000004E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC= 9.43019347E-05,
                   VALE_REFE=9.9199999999999999E-05,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC= 2.94450601E-04,
                   VALE_REFE=2.99E-4,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                ),
          )

#
RESU2=DYNA_NON_LINE(MODELE=MODELE,
                CHAM_MATER=CHMAT,INFO=2,
                EXCIT=(
                       _F(CHARGE=CONDLIM,),
                       ),
                CONTACT = CONTACT,    
                ETAT_INIT=_F(VITE= VITINI),       
                COMP_INCR=_F(RELATION='ELAS',
                             DEFORMATION='PETIT_REAC',),                
                INCREMENT=_F(LIST_INST=L_INST,),
                SCHEMA_TEMPS=_F(SCHEMA='HHT',
                                FORMULATION='DEPLACEMENT',
                                ALPHA=-0.3,MODI_EQUI='OUI',),
                NEWTON=_F(MATRICE='TANGENTE',),
                SOLVEUR=_F(SYME='OUI',
                           METHODE='MULT_FRONT',),)


##############################################
# TEST DE LA CONFORMITE AVEC LA SOLUTION DE REFERENCE ANALYTIQUE
##############################################

TEST_RESU(RESU=(_F(GROUP_NO='N1',
                   INST=2.0000000000000001E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-1.04999990E-04,
                   VALE_REFE=-1.05E-4,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=4.0000000000000002E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-1.55408463E-04,
                   VALE_REFE=-1.55E-4,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=5.9999999999999995E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-5.54082021E-05,
                   VALE_REFE=-5.5399999999999998E-05,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=8.0000000000000004E-4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC= 9.55802140E-05,
                   VALE_REFE=9.9199999999999999E-05,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                _F(GROUP_NO='N1',
                   INST=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC= 2.95565708E-04,
                   VALE_REFE=2.99E-4,
                   CRITERE='RELATIF',
                   PRECISION=0.050000000000000003, TOLE_MACHINE=(0.050000000000000003, 9.9999999999999995E-08),),
                ),
          )

#

FIN();
