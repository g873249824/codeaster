NOM_CHAM='TEMP', RESULTAT=TEMPE,
                           INST=0.6   )

TEMP_07=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
                           INST=0.7   )

TEMP_08=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
