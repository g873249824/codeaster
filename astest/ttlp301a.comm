_F( CHAM_GD = TEMP_11,  NOEUD = 'N1', NOM_CMP = 'TEMP', VALE = 3.120, PRECISION = 2.E-2),
_F( CHAM_GD = TEMP_12,   NOEUD = 'N1', NOM_CMP = 'TEMP', VALE = 3.248, PRECISION = 2.E-2))

           )

