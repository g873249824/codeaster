DEBUT(  CODE=_F(  NOM = 'TTLP301A',NIV_PUB_WEB='INTERNET')
      )

#
# LECTURE DU MAILLAGE --------------------------------------------------
#


MAIL=LIRE_MAILLAGE( )

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ----------------------------
#

# ----- MATERIAU


MATER=DEFI_MATERIAU(   THER=_F(  LAMBDA = 0.1,  RHO_CP = 1.0)
                       )

#
# AFFECTATIONS: MATERIAU, MODELE, CHARGEMENT ---------------------------
#


CHMAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',  MATER = MATER)
                        )

MOTH=AFFE_MODELE(     MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',
                                  MODELISATION = 'PLAN',
                                  PHENOMENE = 'THERMIQUE')
                        )

CHTH=AFFE_CHAR_THER(   MODELE=MOTH,
                            FLUX_REP=_F(  GROUP_MA = 'GRP_FLU',  FLUN = 1.0),
                             ECHANGE=_F(  GROUP_MA = 'GRP_ECH',
                                       COEF_H = 1.0,  TEMP_EXT = 0.0)
                         )

#
# DEFINITION DE LA STRATEGIE DE CALCUL --------------------------------
#


LTMPS=DEFI_LIST_REEL(   DEBUT=0.,INTERVALLE=(
                            _F(  JUSQU_A = 0.2,   NOMBRE = 10),
                            _F(  JUSQU_A = 1.2,   NOMBRE = 10))
                         )

#
# RESOLUTION ----------------------------------------------------------
#

TEMPE=THER_LINEAIRE(  MODELE=MOTH,
                          ETAT_INIT=_F(  VALE = 0.E0),
                          INCREMENT=_F(  LIST_INST = LTMPS),
                          CHAM_MATER=CHMAT,
                          EXCIT=_F( CHARGE = CHTH)
                       )

#
# ANALYSE DES RESULTATS -----------------------------------------------
#

TEMP_01=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
                           INST=0.1   )

TEMP_02=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
                           INST=0.2   )

