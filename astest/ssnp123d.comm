DEBUT(CODE=_F(NOM='SSNP123D', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),);

# DEFINITION DU PROBLEME
#=======================

MA1=LIRE_MAILLAGE(FORMAT='MED',);

MA=CREA_MAILLAGE(MAILLAGE=MA1,
                 LINE_QUAD=_F(TOUT='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D_INCO',),);

MATERIAU=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                               NU=0.3,),
                       ECRO_LINE=_F(D_SIGM_EPSI=1000.0,
                                    SY=200.0,),);

CH_MATER=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATERIAU,),);

SYMETRIE=AFFE_CHAR_MECA(MODELE=MO,
                        FACE_IMPO=(_F(GROUP_MA=('LCD_FACE','LBC_FACE',),
                                      DY=0.0,),
                                   _F(GROUP_MA=('LDE_FACE','LEF_FACE',),
                                      DX=0.0,),),);

APPUI=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=_F(TOUT='OUI',
                                   DZ=0.0,),);

TRACTION=AFFE_CHAR_MECA(MODELE=MO,
                        FACE_IMPO=_F(GROUP_MA='LFG_FACE',
                                     DY=0.1,),);

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=5,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0, 0.0,
                          1.0, 1.0,),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =INSTANTS ),
                        ECHEC=_F(EVENEMENT     = 'ERREUR',
                                 ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS      = 4,
                                 SUBD_PAS_MINI = 0.001),)

# CALCUL
#========

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CH_MATER,
                   EXCIT=(_F(CHARGE=TRACTION,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=SYMETRIE,),
                          _F(CHARGE=APPUI,),
                         ),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
                   INCREMENT=_F(LIST_INST=DEFLIST),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=INSTANTS,),);

# POST TRAITEMENT
#=================

RESU=CALC_CHAMP(reuse=RESU,RESULTAT=RESU,CONTRAINTE=('SIGM_ELNO'))


RESU=CALC_ERREUR(reuse =RESU,

               RESULTAT=RESU,
               OPTION=('ERME_ELEM',
                       'ERME_ELNO',
                      ),
               );

RESU=CALC_CHAMP(reuse =RESU,
             RESULTAT=RESU,
             CONTRAINTE='SIGM_NOEU',);

ECHAN=INTE_MAIL_3D(MAILLAGE=MA,
                   DEFI_SEGMENT=_F(ORIGINE=(0.0,1.0,0.0,),
                                   EXTREMITE=(5.0,1.0,0.0,),),
                   PRECISION=0.00001,
                   INFO=1,);

S_T=POST_RELEVE_T(ACTION=_F(INTITULE='S_EXT',
                            CHEMIN=ECHAN,
                            RESULTAT=RESU,
                            NOM_CHAM='SIGM_NOEU',
                            INST=1.0,
                            CRITERE='RELATIF',
                            NOM_CMP='SIYY',
                            REPERE='GLOBAL',
                            MOYE_NOEUD='OUI',
                            OPERATION='EXTRACTION',),
                  INFO=1,);

IMPR_TABLE(TABLE=S_T,);


