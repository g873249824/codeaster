# PLAQUE INFINIE SOUMISE A DES COUPLES DE FLUXS
# ANTISYMETRIQUES (MODELISATION COQUE THERMIQUE SEG36)
#

DEBUT(  CODE=_F( NOM = 'TPLS100B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(  )

M=DEFI_GROUP( reuse=M,   MAILLAGE=M,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(
                       MAILLAGE=M,
                      AFFE=_F( TOUT = 'OUI',PHENOMENE = 'THERMIQUE',
                                      MODELISATION = 'COQUE_PLAN'))

EPANT=AFFE_CARA_ELEM( MODELE=MO,
                           COQUE=_F(
       GROUP_MA = 'TOUT',
EPAIS = 4.) )

MAANT=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0,LAMBDA = 4.5))

CMANT=AFFE_MATERIAU(  MAILLAGE=M, AFFE=_F( TOUT = 'OUI',MATER = MAANT))

VAL0=DEFI_CONSTANTE(   VALE=0.)

VAL30=DEFI_CONSTANTE(   VALE=30.)

VALM30=DEFI_CONSTANTE(   VALE=-30.)

CHANTF=AFFE_CHAR_THER_F(
           MODELE=MO,
          TEMP_IMPO=_F( TOUT = 'OUI',TEMP = VAL0),
          FLUX_REP=_F( GROUP_MA = 'GRSD2',FLUN_SUP = VAL30,FLUN_INF = VALM30))

MEANT=CALC_MATR_ELEM(
                    MODELE=MO,CHARGE=CHANTF,CHAM_MATER=CMANT,
                      CARA_ELEM=EPANT, OPTION='RIGI_THER'    )

VEANT=CALC_VECT_ELEM(  CHARGE=CHANTF,
                            OPTION='CHAR_THER' )

NUANT=NUME_DDL(   MATR_RIGI=MEANT )

KA=ASSE_MATRICE(   MATR_ELEM=MEANT,  NUME_DDL=NUANT )

FA=ASSE_VECTEUR(   VECT_ELEM=VEANT, NUME_DDL=NUANT  )

KA=FACTORISER( reuse=KA,   MATR_ASSE=KA)

TEMPA=RESOUDRE(   MATR=KA,    CHAM_NO=FA )

TEST_RESU(CHAM_NO=(
      _F( CHAM_GD = TEMPA, NOEUD = 'N201',NOM_CMP = 'TEMP_SUP',
                         VALE = 13.3321,  PRECISION = 9.E-5),
              _F( CHAM_GD = TEMPA, NOEUD = 'N176',NOM_CMP = 'TEMP_SUP',
                         VALE = 13.2565,  PRECISION = 8.E-6),
              _F( CHAM_GD = TEMPA, NOEUD = 'N161',NOM_CMP = 'TEMP_SUP',
                         VALE = 12.7462,  PRECISION = 3.E-4),
              _F( CHAM_GD = TEMPA, NOEUD = 'N1', NOM_CMP = 'TEMP_SUP',
                         VALE = 6.6666,  PRECISION = 1.0E-4),
              _F( CHAM_GD = TEMPA, NOEUD = 'N46', NOM_CMP = 'TEMP_SUP',
                         VALE = 0.07679, PRECISION = 1.8E-3))
                                         )

# MEME CAS TEST AVEC THER_LINEAIRE


CHANT=AFFE_CHAR_THER(
           MODELE=MO,
          TEMP_IMPO=_F( TOUT = 'OUI',TEMP = 0.),
          FLUX_REP=_F( GROUP_MA = 'GRSD2',FLUN_SUP = 30., FLUN_INF = -30.))

TEMPA2=THER_LINEAIRE(
               MODELE=MO,
               CHAM_MATER=CMANT,    CARA_ELEM=EPANT,
              EXCIT=_F(  CHARGE = CHANT) )

TEST_RESU(RESU=(
        _F( RESULTAT = TEMPA2, NOEUD = 'N201',NOM_CMP = 'TEMP_SUP',
            NUME_ORDRE = 0,
            NOM_CHAM = 'TEMP',   VALE = 13.3321,  PRECISION = 9.E-5),
              _F( RESULTAT = TEMPA2, NOEUD = 'N176',NOM_CMP = 'TEMP_SUP',
            NUME_ORDRE = 0,
            NOM_CHAM = 'TEMP',   VALE = 13.2565,  PRECISION = 8.E-6),
              _F( RESULTAT = TEMPA2, NOEUD = 'N161',NOM_CMP = 'TEMP_SUP',
            NUME_ORDRE = 0,
            NOM_CHAM = 'TEMP',   VALE = 12.7462,  PRECISION = 3.E-4),
              _F( RESULTAT = TEMPA2, NOEUD = 'N1', NOM_CMP = 'TEMP_SUP',
            NUME_ORDRE = 0,
            NOM_CHAM = 'TEMP',   VALE = 6.6666,  PRECISION = 1.0E-4),
              _F( RESULTAT = TEMPA2, NOEUD = 'N46', NOM_CMP = 'TEMP_SUP',
            NUME_ORDRE = 0,
            NOM_CHAM = 'TEMP',   VALE = 0.07679, PRECISION = 1.8E-3))
                                                                  )

FIN()
