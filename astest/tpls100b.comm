# PLAQUE INFINIE SOUMISE A DES COUPLES DE FLUXS
# ANTISYMETRIQUES (MODELISATION COQUE THERMIQUE SEG36)
#

DEBUT(  CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',  )

M=DEFI_GROUP( reuse=M,   MAILLAGE=M,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(
                       MAILLAGE=M,
                      AFFE=_F( TOUT = 'OUI',PHENOMENE = 'THERMIQUE',
                                      MODELISATION = 'COQUE_PLAN'))

EPANT=AFFE_CARA_ELEM( MODELE=MO,
                           COQUE=_F(
       GROUP_MA = 'TOUT',
EPAIS = 4.) )

MAANT=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0,LAMBDA = 4.5))

CMANT=AFFE_MATERIAU(  MAILLAGE=M, AFFE=_F( TOUT = 'OUI',MATER = MAANT))

VAL0=DEFI_CONSTANTE(   VALE=0.)

VAL30=DEFI_CONSTANTE(   VALE=30.)

VALM30=DEFI_CONSTANTE(   VALE=-30.)

CHANTF=AFFE_CHAR_THER_F(
           MODELE=MO,
          TEMP_IMPO=_F( TOUT = 'OUI',TEMP_MIL= VAL0),
          FLUX_REP=_F( GROUP_MA = 'GRSD2',FLUN_SUP = VAL30,FLUN_INF = VALM30))

MEANT=CALC_MATR_ELEM(
                    MODELE=MO,CHARGE=CHANTF,CHAM_MATER=CMANT,
                      CARA_ELEM=EPANT, OPTION='RIGI_THER'    )

VEANT=CALC_VECT_ELEM(  CHARGE=CHANTF,
                            OPTION='CHAR_THER' )

NUANT=NUME_DDL(   MATR_RIGI=MEANT )

KA=ASSE_MATRICE(   MATR_ELEM=MEANT,  NUME_DDL=NUANT )

FA=ASSE_VECTEUR(   VECT_ELEM=VEANT, NUME_DDL=NUANT  )

KA=FACTORISER( reuse=KA,   MATR_ASSE=KA)

TEMPA=RESOUDRE(   MATR=KA,    CHAM_NO=FA )

TEST_RESU(CHAM_NO=(_F(NOEUD='N201',
                      NOM_CMP='TEMP_SUP',
                      CHAM_GD=TEMPA,
                      REFERENCE='ANALYTIQUE',
                      VALE_REFE=13.3321,
                      VALE_CALC=13.331018048501,
                      PRECISION=1.E-3,),
                   _F(NOEUD='N176',
                      NOM_CMP='TEMP_SUP',
                      CHAM_GD=TEMPA,
                      REFERENCE='ANALYTIQUE',
                      VALE_CALC=13.25650073213,
                      VALE_REFE=13.2565,
                      PRECISION=1.E-3,),
                   _F(NOEUD='N161',
                      NOM_CMP='TEMP_SUP',
                      CHAM_GD=TEMPA,
                      REFERENCE='ANALYTIQUE',
                      VALE_CALC=12.746259086106,
                      VALE_REFE=12.7462,
                      PRECISION=1.E-3,),
                   _F(NOEUD='N1',
                      NOM_CMP='TEMP_SUP',
                      CHAM_GD=TEMPA,
                      REFERENCE='ANALYTIQUE',
                      VALE_CALC=6.6666666666667,
                      VALE_REFE=6.6666666666667,
                      PRECISION=1.E-3,),
                   _F(NOEUD='N46',
                      NOM_CMP='TEMP_SUP',
                      PRECISION=1.8E-3,
                      CHAM_GD=TEMPA,
                      VALE_CALC=0.076832601203197,
                      VALE_REFE=0.07679,
                      REFERENCE='ANALYTIQUE',),
                   ),
          )

# MEME CAS TEST AVEC THER_LINEAIRE


CHANT=AFFE_CHAR_THER(
           MODELE=MO,
          TEMP_IMPO=_F( TOUT = 'OUI',TEMP_MIL= 0.),
          FLUX_REP=_F( GROUP_MA = 'GRSD2',FLUN_SUP = 30., FLUN_INF = -30.))

TEMPA2=THER_LINEAIRE(
               MODELE=MO,
               CHAM_MATER=CMANT,    CARA_ELEM=EPANT,
              EXCIT=_F(  CHARGE = CHANT) )

TEST_RESU(RESU=(_F(NUME_ORDRE=0,
                   RESULTAT=TEMPA2,
                   NOM_CHAM='TEMP',
                   NOEUD='N201',
                   NOM_CMP='TEMP_SUP',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=13.3321,
                   VALE_CALC=13.331018048501,
                   PRECISION=1.E-3,
                   ),
                _F(NUME_ORDRE=0,
                   RESULTAT=TEMPA2,
                   NOM_CHAM='TEMP',
                   NOEUD='N176',
                   NOM_CMP='TEMP_SUP',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=13.2565,
                   VALE_CALC=13.25650073213,
                   PRECISION=1.E-3,
                   ),
                _F(NUME_ORDRE=0,
                   RESULTAT=TEMPA2,
                   NOM_CHAM='TEMP',
                   NOEUD='N161',
                   NOM_CMP='TEMP_SUP',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=12.7462,
                   VALE_CALC=12.746259086106,
                   PRECISION=1.E-3,
                   ),
                _F(NUME_ORDRE=0,
                   RESULTAT=TEMPA2,
                   NOM_CHAM='TEMP',
                   NOEUD='N1',
                   NOM_CMP='TEMP_SUP',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=6.6666666666667,
                   VALE_CALC=6.6666666666667,
                   PRECISION=1.E-4,
                   ),
                _F(NUME_ORDRE=0,
                   RESULTAT=TEMPA2,
                   NOM_CHAM='TEMP',
                   NOEUD='N46',
                   NOM_CMP='TEMP_SUP',
                   VALE_CALC=0.076832601203197,
                   VALE_REFE=0.07679,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=1.8E-3,),
                ),
          )

FIN()
