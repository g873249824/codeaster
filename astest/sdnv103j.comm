DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),
IGNORE_ALARM=('ELEMENTS4_74'),DEBUG=_F(SDVERI='OUI'))

MA1=LIRE_MAILLAGE(FORMAT='MED',);

MA=CREA_MAILLAGE(MAILLAGE=MA1,
                 QUAD_LINE=_F(TOUT='OUI',),);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(TOUT_GROUP_MA='OUI',),
                             _F(GROUP_MA='SZZ',
                                NOM='MILIEU',
                                CRIT_NOEUD='MILIEU',),
                             _F(DIFFE=('SXX','MILIEU',),
                                NOM='SXX1',),
                             _F(DIFFE=('SYY','MILIEU',),
                                NOM='SYY1',),),);

MODE=AFFE_MODELE(MAILLAGE=MA,
                 VERIF='MAILLE',
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION=('3D','3D_SI',),),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=_F(GROUP_MA=('SZZ','RIGID',),),
                 INFO=2,);

ACIER=DEFI_MATERIAU(ELAS=_F(E=117.E3,
                            NU=0.35,
                            RHO=8.930E-9,),
                    ECRO_LINE=_F(D_SIGM_EPSI=100.,
                                 SY=400.,),);

# pour ce test, il faut que l'elasticite soit incrementale
ACIERE=DEFI_MATERIAU(ELAS=_F(E=117.E3,
                            NU=0.35,
                            RHO=8.930E-9,),
                    ECRO_LINE=_F(D_SIGM_EPSI=100.,
                                 SY=400.E6,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(
                    _F(GROUP_MA=('VOL',),MATER=ACIER,),
                    _F(GROUP_MA=('VOL_RIG',),MATER=ACIERE,),
                            ));


CH=AFFE_CHAR_MECA(MODELE=MODE,
                  DDL_IMPO=(_F(GROUP_MA='RIGID',
                               DX=0.,
                               DY=0.,
                               DZ=0.,),
                            _F(GROUP_MA='VOL_RIG',
                               DX=0.,
                               DY=0.,
                               DZ=0.,),
                            _F(GROUP_NO='SYY1',
                               DY=0.,),
                            _F(GROUP_NO='SXX1',
                               DX=0.,),),
                  INFO=1,);


CHCONT  = DEFI_CONTACT(MODELE         = MODE,
                       FORMULATION    = 'DISCRETE',
                       REAC_GEOM      = 'CONTROLE',
                       NB_ITER_GEOM   = 1,
                       FROTTEMENT     = 'COULOMB',

                       ZONE =_F(
                             ALGO_CONT      = 'LAGRANGIEN',
                             ALGO_FROT      = 'PENALISATION',
                             COULOMB        = 0.25,
                             E_T            = 1.E5,
                             APPARIEMENT    = 'MAIT_ESCL',
                             GROUP_MA_MAIT  = 'RIGID',
                             GROUP_MA_ESCL  = 'SZZ',
                             COEF_MATR_FROT = 0.4,
                               ),
                      );


VIT_0=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                 OPERATION='AFFE',
                 MODELE=MODE,

                 AFFE=(
                 _F(TOUT='OUI',
                    NOM_CMP=('DX','DY','DZ',),
                    VALE=(0.,0.,-227000.,),),
                 _F(GROUP_NO='VOL_RIG',
                    NOM_CMP=('DX','DY','DZ',),
                    VALE=(0.,0.,0.,),),
                 _F(GROUP_NO='RIGID',
                    NOM_CMP=('DX','DY','DZ',),
                    VALE=(0.,0.,0.,),),),);

L_INST=DEFI_LIST_REEL(DEBUT=-1.E-5,
                      INTERVALLE=(_F(JUSQU_A=-2.5E-7,
                                     NOMBRE=1,),
                                  _F(JUSQU_A=2.5E-7,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=80.E-6,
                                     NOMBRE=316,),),);

L_SAUV=DEFI_LIST_REEL(DEBUT=-1.E-5,
                      INTERVALLE=(_F(JUSQU_A=2.5E-7,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=80.E-6,
                                     NOMBRE=316,),),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST),
                        ECHEC=_F(EVENEMENT='ERREUR',
                                 ACTION='DECOUPE',
                                 SUBD_METHODE='MANUEL',
                                 SUBD_PAS=2,
                                 SUBD_PAS_MINI=1.E-30,),);
#
#  On teste la sous-division du pas de temps
#  On en profite aussi pour avoir un schema en temps plus adapte
#
# pour ce test, il faut que l'elasticite soit incrementale
RESU=DYNA_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CH,),
                   CONTACT = CHCONT,
                   COMPORTEMENT=(_F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='PETIT_REAC',
                                TOUT='OUI',),
                              _F(RELATION='VMIS_ISOT_LINE',
                                 DEFORMATION='PETIT',
                                 GROUP_MA='VOL_RIG',),),
                   ETAT_INIT=_F(VITE=VIT_0,),
                   INCREMENT=_F(LIST_INST=DEFLIST,
                                INST_FIN=2.5E-7,),
#  Ancien schema en temps :
#                    SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
#                                 FORMULATION='DEPLACEMENT',
#                                 BETA=0.49,GAMMA=0.9,),
#
#  Schema "optimal" car on manque d'amortissement en lineaire :
                   SCHEMA_TEMPS=_F(SCHEMA='HHT',
                                FORMULATION='DEPLACEMENT',
                                MODI_EQUI='NON',ALPHA=-0.6,),
                   NEWTON=_F(
                             REAC_INCR=1,
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   SOLVEUR=_F(SYME='OUI',),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
#  On force le sous-decoupage du pas de temps :
#                                  ITER_GLOB_MAXI=20,
                                  ITER_GLOB_MAXI=5,
                                  ARRET='OUI',),
                   );

VITE_A2=POST_RELEVE_T(ACTION=_F(INTITULE='VITE EN A2',
                                GROUP_NO='A2',
                                RESULTAT=RESU,
                                NOM_CHAM='VITE',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

DEPL_A1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN A1',
                                GROUP_NO='A1',
                                RESULTAT=RESU,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

DEPL_B1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN B1',
                                GROUP_NO='B1',
                                RESULTAT=RESU,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);
#####################################################
#####################################################

# TESTS DE DEPLACEMENTS, VITESSES ET ACCELERATIONS

TEST_RESU(RESU=(_F(GROUP_NO='A2',
                   INST=2.5E-07,
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-2.3267499999985,
                   CRITERE='RELATIF',
                   ),
                _F(GROUP_NO='A2',
                   INST=2.5E-07,
                   RESULTAT=RESU,
                   NOM_CHAM='VITE',
                   NOM_CMP='DZ',
                   VALE_CALC=-2.2699999997981E5,
                   CRITERE='RELATIF',
                   ),
                ),
          )

# TESTS SUR LA VALEUR DU JEU ET DU FROTTEMENT

TABJEU=POST_RELEVE_T(ACTION=_F(INTITULE='MESSAGE',
                               GROUP_NO='B1',
                               RESULTAT=RESU,
                               NOM_CHAM='VALE_CONT',
                               TOUT_CMP='OUI',
                               OPERATION='EXTRACTION',),);
# JEU

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.010529710650703,

           NOM_PARA='JEU',
           TABLE=TABJEU,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.5E-07,),
           )

IMPR_TABLE(TABLE=TABJEU,);

FIN();
