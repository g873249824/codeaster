DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
BIDON=DEFI_CONSTANTE(    VALE=1.)

UN=DEFI_CONSTANTE(    VALE=1.)

ZERO=DEFI_CONSTANTE(    VALE=0.)

VISCOLIQ=DEFI_CONSTANTE(    VALE=1.E-3)

VISCOGAZ=DEFI_CONSTANTE(    VALE=1.8E-5)

DVISCOL=DEFI_CONSTANTE(    VALE=0.)

DVISCOG=DEFI_CONSTANTE(    VALE=0.)

KINT=DEFI_CONSTANTE(    VALE=1.E-18)

#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED', )

MODELE=AFFE_MODELE(     MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = '3D_HM',
                                   )
                          )

MAILLAGE=MODI_MAILLAGE( reuse=MAILLAGE,   MAILLAGE=MAILLAGE,
                       ORIE_PEAU_3D=_F(  GROUP_MA = ('HAUT', 'DEVANT', 'DROITE',),  )
                     )
MAILLAGE = DEFI_GROUP( reuse        = MAILLAGE,
                          MAILLAGE     = MAILLAGE,
                          CREA_GROUP_MA=_F(NOM='ROCHE',TOUT='OUI',),
                          CREA_GROUP_NO=_F(GROUP_MA='ROCHE',), );

SOL=DEFI_MATERIAU(  ELAS=_F(  E = 7.74E6,
                                NU = 0.3,
                                RHO = 2500.,
                                ALPHA = 1.E-5,),
                    CAM_CLAY=_F(MU=6.E6,
                                PORO=0.66,
                                LAMBDA=0.25,
                                KAPA=0.05,
                                M=0.9,
                                PRES_CRIT=3.E5,
                                KCAM= 0.,
                                PTRAC=0.),
                     COMP_THM = 'LIQU_SATU',
                     THM_LIQU=_F(
                                RHO = 1000.,
                                UN_SUR_K = 0.0,
                                ALPHA = 1.E-4,
                                CP = 4180.,
                                VISC = VISCOLIQ,
                                D_VISC_TEMP = DVISCOL,
                                        ),
                              THM_GAZ=_F(
                                MASS_MOL = 28.96E-3,
                                CP = 1000.,
                                VISC = VISCOGAZ,
                                D_VISC_TEMP = DVISCOG,
                                  ),
                              THM_VAPE_GAZ=_F(
                                MASS_MOL = 18.E-3,
                                CP = 1870.,
                                VISC = BIDON,
                                D_VISC_TEMP = BIDON,
                                 ),
                              THM_DIFFU=_F(
                                R_GAZ = 8.315,
                                RHO = 2400.,
                                CP = 800.,
                                BIOT_COEF = 1.,
                                SATU_PRES = UN,
                                D_SATU_PRES = ZERO,
                                PESA_X = 0.,
                                PESA_Y = 0.,
                                PESA_Z = 0.,
                                PERM_IN = KINT,
                                PERM_LIQU = UN,
                                D_PERM_LIQU_SATU = ZERO,
                                PERM_GAZ = UN,
                                D_PERM_SATU_GAZ = ZERO,
                                D_PERM_PRES_GAZ = ZERO,
                                 ),
                              THM_INIT=_F(
                                TEMP = 293.,
                                PRE1 = 1.0,
                                PRE2 = 0.1E6,
                                PORO = 0.66,
                                PRES_VAPE = 2269.8,
                                )
                           )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',    MATER = SOL,  )
                          )

PR_LATE=AFFE_CHAR_MECA(    MODELE=MODELE,
                              PRES_REP=_F(   GROUP_MA = ( 'DEVANT', 'DROITE', ),
                                          PRES = 1.,
                                    )
                       )

PR_V=AFFE_CHAR_MECA(    MODELE=MODELE,
                             PRES_REP=_F(   GROUP_MA = 'HAUT',    PRES = 1.,
                                    )
                        )

DEP_SYM=AFFE_CHAR_MECA(    MODELE=MODELE,FACE_IMPO=(
                              _F(  GROUP_MA = 'BAS',    DZ = 0.,   ),
                              _F(  GROUP_MA = 'DERRIERE',    DX = 0.,   ),
                              _F(  GROUP_MA = 'GAUCHE',    DY = 0.,   ))
                           )

P0=AFFE_CHAR_MECA(    MODELE=MODELE,
                               DDL_IMPO=_F(GROUP_NO='ROCHE',   PRE1 = 0.,  )
                           )

EVOL_HYD=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                         VALE=( 0.0,  0.0,
                               1.0,  100.E3,
                               3.0,   200.E3,
                               6.0,   300.E3,)
                       )

TEMPS1=DEFI_LIST_REEL(   DEBUT=0.,
                            INTERVALLE=_F(  JUSQU_A = 1.,      NOMBRE = 1,  ),
                         )

TEMPS2=DEFI_LIST_REEL(   DEBUT=1.,INTERVALLE=(
                            _F(  JUSQU_A = 3.,    NOMBRE = 5,  ),
                            _F(  JUSQU_A = 6.,    NOMBRE = 5,  ),
                            _F(  JUSQU_A = 8.,    NOMBRE = 100,  ),
                            _F(  JUSQU_A = 15.,    NOMBRE = 20,  ),
                            _F(  JUSQU_A = 20.,    NOMBRE = 20,  ),)
                         )

DEFLIST2 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =TEMPS2 ),
                        ECHEC=_F(EVENEMENT     = 'ERREUR',
                                 ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS      = 10,
                                 SUBD_PAS_MINI = 0.001,
                                ),)

#CALCUL ELASTIQUE

U1=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = PR_LATE,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = PR_V,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = DEP_SYM,
                ), _F(
                CHARGE = P0,
                )),
                         COMPORTEMENT=_F(
                RELATION = 'KIT_HM',
                RELATION_KIT = (
                  'ELAS',        'LIQU_SATU',
                  'HYDR_UTIL',  ),
                ),
                         INCREMENT=_F(
                LIST_INST = TEMPS1,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                )
              )
RES1=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                OPERATION='EXTR',
                RESULTAT=U1,
                NOM_CHAM='SIEF_ELGA',
                INST=1.0,);

#CALCUL HYDROSTATIQUE

U1=STAT_NON_LINE(reuse=U1,
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = PR_LATE,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = PR_V,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = DEP_SYM,
                ), _F(
                CHARGE = P0,
                )),
                      ETAT_INIT=_F(SIGM=RES1,),
                         COMPORTEMENT=_F(
                RELATION = 'KIT_HM',
                RELATION_KIT = ('CAM_CLAY','LIQU_SATU','HYDR_UTIL',),
                ITER_INTE_MAXI=100
                ),
                       NEWTON=_F(
                     MATRICE='TANGENTE',
                     REAC_ITER=1,
                     ),
                         INCREMENT=_F(
                INST_INIT = 1.,
                INST_FIN=6.,
                LIST_INST = TEMPS2,
                ),
                       CONVERGENCE=_F(
                       ITER_GLOB_MAXI=50,
                       RESI_GLOB_RELA = 1.E-6,
                )
              )


IMPR_RESU(RESU=_F(MAILLAGE=MAILLAGE,
                  RESULTAT=U1,
                  NOM_CHAM=('DEPL'),
                  PRECISION = 1.E-10,
                  INST = 6.,),);

#***********************************************************************
#  TRIAXIAL
#***********************************************************************

DEP_V=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  GROUP_MA = 'HAUT',    DZ = 1.,   )
                           )

EVOL_DEV=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                         VALE=(
                                6.0,  -6.22547E-03,
                                8.0,  -2.57468E-02,
                                15.0,  -3.57468E-02,
                                20.0,  -6.57468E-02,

                                ),)

U1=STAT_NON_LINE(reuse=U1,
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = DEP_SYM,
                ), _F(
                CHARGE = PR_LATE,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = DEP_V,
                FONC_MULT = EVOL_DEV,
                )),
                         COMPORTEMENT=_F(
                RELATION = 'KIT_HM',
                RELATION_KIT = ('CAM_CLAY','LIQU_SATU','HYDR_UTIL',),
                ITER_INTE_MAXI=20,
                ),
                         ETAT_INIT=_F(
                EVOL_NOLI = U1,
                ),
                         INCREMENT=_F(
                LIST_INST = DEFLIST2,
                INST_INIT = 6.,
                              ),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER=1,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 10,
                )
              )

U1=CALC_CHAMP(reuse=U1,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=U1)


U1=CALC_CHAMP(  reuse=U1,     RESULTAT=U1,
                       CONTRAINTE='SIGM_NOEU',VARI_INTERNE='VARI_NOEU',
                )

#-------------------------
#    TEST_RESU AU POINT A
#-------------------------

TEST_RESU(RESU=(_F(INST=3.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIXX',
                   VALE_CALC=-2.E5,
                   ),
                _F(INST=6.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIXX',
                   VALE_CALC=-3.E5,
                   ),
                _F(INST=15.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIXX',
                   VALE_CALC=-2.10000E5,
                   ),
                _F(INST=20.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIXX',
                   VALE_CALC=-2.10000E5,
                   ),
                _F(INST=3.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIYY',
                   VALE_CALC=-2.E5,
                   ),
                _F(INST=6.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIYY',
                   VALE_CALC=-3.E5,
                   ),
                _F(INST=15.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIYY',
                   VALE_CALC=-2.10000E5,
                   ),
                _F(INST=20.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIYY',
                   VALE_CALC=-2.10000E5,
                   ),
                _F(INST=3.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIZZ',
                   VALE_CALC=-2.E5,
                   ),
                _F(INST=6.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIZZ',
                   VALE_CALC=-3.E5,
                   ),
                _F(INST=15.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIZZ',
                   VALE_CALC=-4.80000E5,
                   ),
                _F(INST=20.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIZZ',
                   VALE_CALC=-4.80000E5,
                   ),
                _F(INST=20.0,
                   RESULTAT=U1,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='PRE1',
                   VALE_CALC=9.E4,
                   ),
                ),
          )

FIN () ;
