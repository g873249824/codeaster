#

DEBUT(CODE=_F(NOM='SSNV127B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='OUI'))



SSNV127B=LIRE_MAILLAGE(FORMAT='MED',);
#
#


SSNV127B=DEFI_GROUP(reuse =SSNV127B,
                    MAILLAGE=SSNV127B,
                    CREA_GROUP_NO=(_F(GROUP_MA='CC4',),
