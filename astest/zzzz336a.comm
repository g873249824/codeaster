# ON TESTE LA METHODE SOUS_POINT DE PROJ_CHAMP POUR DES ELEMENTS DKT ET GRILLE
# LA TEMPERATURE D'UN CALCUL THERMIQUE 3D LINEAIRE STATIONNAIRE EST PROJETEE POUR UN CALCUL MECANIQUE
# LA SOLUTION DE REFERENCE EST UN AUTRE CALCUL ASTER OU LA EMPERATURE ETAIT DONNEE AVEC PREP_VRC1

import math

LINST =  DEFI_LIST_REEL(VALE=(0.,1.))


# Thermique 3D
MATH=LIRE_MAILLAGE(UNITE=19)
MOTH=AFFE_MODELE(MAILLAGE=MATH,
                 AFFE=_F(TOUT='OUI',PHENOMENE='THERMIQUE',MODELISATION='3D_DIAG'))
RHO_CP = 0.
LAMBDA = 2.0
BETON_TH = DEFI_MATERIAU(THER=_F(LAMBDA = LAMBDA,RHO_CP = RHO_CP))
MAT_TH = AFFE_MATERIAU(MAILLAGE = MATH,
                    AFFE =_F(GROUP_MA = 'POU3D' , MATER = BETON_TH,))


TIMP = AFFE_CHAR_THER(MODELE = MOTH,
                            TEMP_IMPO =( _F(GROUP_MA =('SINF'), TEMP = 50.),
                                                       _F(GROUP_MA =('SSUP'), TEMP = 20.),
                                                         ))
RESUTH = THER_LINEAIRE(MODELE=MOTH,
                     CHAM_MATER = MAT_TH,
                     EXCIT      = _F(CHARGE=TIMP),
                     ETAT_INIT=_F(VALE = 20.),
                     INCREMENT=_F(LIST_INST = LINST,),
                                  )

# Mecanique dalle
MA0=LIRE_MAILLAGE()
MA0=MODI_MAILLAGE(reuse=MA0,
                  MAILLAGE=MA0,
                                ORIE_NORM_COQUE=_F(GROUP_MA='SDAL',VECT_NORM=(0., 0., 1.),GROUP_NO='C1'))
MA1=CREA_MAILLAGE(MAILLAGE=MA0,
                  CREA_MAILLE=_F(NOM='ARMA',GROUP_MA='SDAL',PREF_MAILLE='A'))
IMPR_RESU(RESU=_F(MAILLAGE=MA1))
MO1=AFFE_MODELE(MAILLAGE=MA1,
                AFFE=(_F(GROUP_MA='SDAL',PHENOMENE='MECANIQUE',MODELISATION='DKT'),
                                    _F(GROUP_MA='ARMA',PHENOMENE='MECANIQUE',MODELISATION='GRILLE_EXCENTRE')
                              ))
# epaisseur dalle = 10 cm
epai=0.1
# phi 8, esp = 20 cm, enrobage = 2.5 cm
d=0.08
esp=0.2
sa=(math.pi*d*d/4.)/esp
enro=2.5e-2
exc=-epai/2.+enro

CA1=AFFE_CARA_ELEM(MODELE=MO1,
                   COQUE=_F(GROUP_MA='SDAL',EPAIS=epai,COQUE_NCOU=10),
                                 GRILLE=_F(GROUP_MA='ARMA',SECTION=sa,EXCENTREMENT=exc,ANGL_REP=(0.,0.)),
                                 )

BETON=DEFI_MATERIAU(ELAS=_F(E=3.E10, NU=0.2, ALPHA=1.E-5))
ACIER=DEFI_MATERIAU(ELAS=_F(E=2.E11, NU=0.3, ALPHA=2.E-5))

CL1=AFFE_CHAR_MECA(MODELE=MO1,
                   DDL_IMPO=(_F(GROUP_MA=('APP1','APP2'),DZ=0.),
                                           _F(GROUP_NO=('C1'),DY=0.),
                                           _F(GROUP_MA=('APP1'),DX=0.),)
                                 )


# chargement thermique par projection du calcul 3D
TEMP_SP2=PROJ_CHAMP(PROJECTION='OUI',METHODE='SOUS_POINT',
                   MAILLAGE_1 = MATH,
                   MODELE_2   = MO1,
                   RESULTAT   = RESUTH,
                                 NOM_CHAM   = 'TEMP',
                   CARA_ELEM  = CA1,
                   )

CHMA2=AFFE_MATERIAU(MAILLAGE=MA1,
