                                        _F(GROUP_MA='ARMA',MATER=ACIER),),
                    AFFE_VARC = _F( TOUT = 'OUI', NOM_VARC = 'TEMP', EVOL=TEMP_SP2,VALE_REF=20.),
                                  )

U2=STAT_NON_LINE(MODELE=MO1,
                 CHAM_MATER=CHMA2,
                 CARA_ELEM=CA1,
                 EXCIT=(_F(CHARGE=CL1,),
