                CREA_GROUP_NO=(_F(GROUP_MA='DROITE'),
                               _F(NOEUD='N7' , NOM = 'H'),
                               _F(NOEUD='N58', NOM = 'I'),
                               _F(NOEUD='N60', NOM = 'J'),))

