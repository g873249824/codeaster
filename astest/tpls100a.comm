DEBUT(  CODE=_F( NOM = 'TPLS100A',NIV_PUB_WEB='INTERNET'))

M=LIRE_MAILLAGE( )

M=DEFI_GROUP( reuse=M,   MAILLAGE=M,
              CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE( MAILLAGE=M, 
                AFFE=_F( TOUT = 'OUI',PHENOMENE = 'THERMIQUE',
                         MODELISATION = 'COQUE'))

EPANT=AFFE_CARA_ELEM( MODELE=MO,
                      COQUE=_F( GROUP_MA = 'TOUT',
                                EPAIS = 4.) )

MAANT=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0,LAMBDA = 4.5))

EPANT2=AFFE_CARA_ELEM( MODELE=MO,
                       COQUE=_F( GROUP_MA = 'TOUT',
                                 EPAIS = 4.,  
                                 ANGL_REP = (30.,0.,)) )

#
# DEFINITION DU MATERIAU HOMOGENEISE MAANT2 ANALOGUE AU 
# MATERIAU ISOTROPE MAANT.
# LES COEFFICIENTS DE CE MATERIAU ONT ETE CALCULES AVEC DES
# POLYNOMES DE LAGRANGE.
#

MAANT2=DEFI_MATERIAU(  THER_COQUE=_F( 
   COND_LMM = 9.6,  COND_TMM = 9.6,  COND_LMP = 1.2,   COND_TMP = 1.2,
   COND_LPP = 2.4,  COND_TPP = 2.4,  COND_LSI = -0.6,  COND_TSI = -0.6,
   COND_NMM = 6.0,   COND_NMP = -3.0,  COND_NPP = 2.625,   COND_NSI = 0.375,
   CMAS_MM = 2.133333333,         CMAS_MP = 0.2666666667,    
   CMAS_PP = 0.533333333,         CMAS_SI = -0.1333333333)
                          )

CMANT=AFFE_MATERIAU(  MAILLAGE=M, AFFE=_F( TOUT = 'OUI',MATER = MAANT))

CMANT2=AFFE_MATERIAU(  MAILLAGE=M, AFFE=_F( TOUT = 'OUI',MATER = MAANT2))

VAL0=DEFI_CONSTANTE(   VALE=0.)

VAL30=DEFI_CONSTANTE(   VALE=30.)

VALM30=DEFI_CONSTANTE(   VALE=-30.)

CHANTF=AFFE_CHAR_THER_F(  MODELE=MO,
                          TEMP_IMPO=_F( TOUT = 'OUI',TEMP = VAL0),
                          FLUX_REP=_F( GROUP_MA = 'GRSD2',FLUN_SUP = VAL30,FLUN_INF = VALM30))

TEMPE=THER_LINEAIRE(MODELE=MO,
                    CARA_ELEM=EPANT,
                    CHAM_MATER=CMANT,
                    EXCIT=_F(CHARGE = CHANTF),
                    );
      
TEMPE=CALC_ELEM( reuse=TEMPE,RESULTAT=TEMPE,
                 REPE_COQUE=_F(NIVE_COUCHE='SUP',),
                 OPTION='FLUX_ELNO_TEMP',  );


MEANT2=CALC_MATR_ELEM(  MODELE=MO,CHARGE=CHANTF,CHAM_MATER=CMANT2,
                        CARA_ELEM=EPANT2, OPTION='RIGI_THER'    )

VEANT2=CALC_VECT_ELEM(  CHARGE=CHANTF,  OPTION='CHAR_THER' )

NUANT2=NUME_DDL(   MATR_RIGI=MEANT2 )

KA2=ASSE_MATRICE(   MATR_ELEM=MEANT2,  NUME_DDL=NUANT2 )

FA2=ASSE_VECTEUR(   VECT_ELEM=VEANT2, NUME_DDL=NUANT2  )

KA2=FACT_LDLT( reuse=KA2,   MATR_ASSE=KA2)

TEMPA3=RESO_LDLT(   MATR_FACT=KA2,    CHAM_NO=FA2 )

