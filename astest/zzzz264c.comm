DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),)

delta   = 0.02
pas     = delta
lorth   = 10*delta
lreg    = delta
seuil   = 0.0001
lim_fpz = 0.0001

inst = 0.100


# -------------------------
# LECTURE MAILLAGE
#
MAIL = LIRE_MAILLAGE( UNITE  = 80,
                      FORMAT = 'MED')

# --------------------------------------
# LECTURE RESULTAT MED
#                             
EVOL = LIRE_RESU( TYPE_RESU  = 'EVOL_NOLI',
                  FORMAT     = 'MED',
                  INST       = inst,
                  UNITE      = 80,
                  MAILLAGE   = MAIL,
                  FORMAT_MED = (_F( NOM_CHAM_MED ='EVOL1___DEPL',
                                    NOM_CHAM    = 'DEPL',
                                    ),
                                 _F( NOM_CHAM_MED ='EVOL1___VARI_NOEU',
                                     NOM_CHAM    = 'VARI_NOEU',
                                     NOM_CMP     = ('V4',),
                                     NOM_CMP_MED = ('V4',),),
                                     ),)


#~ MAIL0=CREA_MAILLAGE(MAILLAGE=MAIL, RESTREINT=_F(GROUP_MA='FACECENT'), )
#~ 
#~ MO1B=AFFE_MODELE(MAILLAGE=MAIL0,
               #~ AFFE=_F(TOUT='OUI',
                       #~ PHENOMENE='MECANIQUE',
                       #~ MODELISATION='D_PLAN',),);
                       #~ 
#~ EVOL1=EXTR_RESU(RESULTAT=EVOL, RESTREINT=_F(MODELE=MO1B,)); 
#~ 
#~ IMPR_RESU(FORMAT='MED',UNITE=81,RESU=_F(RESULTAT=EVOL1,NOM_CHAM=('DEPL','VARI_NOEU'),) )


# --------------------------------------
# TRAITEMENT DE LA FISSURE
#
MAFI = POST_ENDO_FISS( TABLE      = CO('TAB_FISS'),
                       RESULTAT   = EVOL,
                       INST       = inst,
                       OUVERTURE  = 'OUI',
                       NOM_CHAM   = 'VARI_NOEU',
                       NOM_CMP    = 'V4',
                       RECHERCHE  = _F( TOUT      = 'OUI',
                                        LONG_ORTH = lorth,
                                        PAS       = pas,
                                        LONG_REG  = lreg,
                                        BORNE_MIN = seuil,
                                        BORNE_MAX = lim_fpz,
                                      ),)

# --------------------------------------
# TESTS 
#

DIST = FORMULE(VALE     = 'abs(COORY)',
               NOM_PARA = ('COORY'),)

# On filtre les tables pour enlever les points proches su bord
#   ou on sait que la procedure marche moins bien.
x_ini = 0.06 + 2*pas
x_fin = 0.94 - 2*pas

TAB_FISS = CALC_TABLE(reuse = TAB_FISS,
                      TABLE = TAB_FISS,
                      ACTION= (_F( OPERATION = 'FILTRE',
                                   NOM_PARA  = 'COORX',
                                   CRIT_COMP = 'GE',
                                   VALE      = x_ini,),
                               _F( OPERATION = 'FILTRE',
                                   NOM_PARA  = 'COORX',
                                   CRIT_COMP = 'LE',
                                   VALE      = x_fin,),
                               _F( OPERATION = 'OPER',
                                   FORMULE   = DIST,
                                   NOM_PARA  = 'DIST',)
                               ),
                      )

# Test analytique sur les coordonnees du chemin de fissuration
TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.029999999999999999,
           VALE_CALC= 2.58453100E-03,
           VALE_REFE=0.0,
           NOM_PARA='DIST',
           TYPE_TEST='MAX',
           TABLE=TAB_FISS,)

# Test analytique sur l'ouverture de fissure
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.10000000000000001,
           VALE_CALC= 1.98633433E-04,
           VALE_REFE=2.0000000000000001E-4,
           NOM_PARA='OUVERTURE',
           TYPE_TEST='MIN',
           TABLE=TAB_FISS,)

# Test de non-regression sur les coordonnees du chemin de fissuration
TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='NON_DEFINI',
           PRECISION=1.E-2,
           VALE_CALC= 2.58453100E-03,
           VALE_REFE=0.0,
           NOM_PARA='DIST',
           TYPE_TEST='MAX',
           TABLE=TAB_FISS,
           )

# Test de non-regression sur l'ouverture de fissure
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='NON_DEFINI',
           PRECISION=1.E-2,
           VALE_CALC= 1.98633433E-04,
           VALE_REFE=2.0000000000000001E-4,
           NOM_PARA='OUVERTURE',
           TYPE_TEST='MIN',
           TABLE=TAB_FISS,
           )

