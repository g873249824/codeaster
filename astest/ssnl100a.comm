#    ESSAI DE POULIES SUR DEUX PORTEES DE CABLE.
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA1=LIRE_MAILLAGE()

MA1=DEFI_GROUP( reuse=MA1,   MAILLAGE=MA1,
                CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO1=AFFE_MODELE(   MAILLAGE=MA1,
                   AFFE=(_F( GROUP_MA = 'MCABLE', PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'CABLE'),
                         _F( GROUP_MA = 'MCABPOUL', PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'CABLE_POULIE'))
                        )

MAT1=DEFI_MATERIAU(   ELAS=_F(  E = 5.E7,  NU=0.,  RHO = 30.),
                      CABLE=_F(EC_SUR_E = 1.E0),
                        )

MAT2=DEFI_MATERIAU(    ELAS=_F(  E = 5.E7,  NU = 0.3,  RHO = 30.)
                        )

CHMAT1=AFFE_MATERIAU(  MAILLAGE=MA1,
                       AFFE=(_F( GROUP_MA = ('MCABLE',),   MATER = MAT1),
                             _F( GROUP_MA = ('MCABPOUL',), MATER = MAT2))
                        )

CHA1=AFFE_CHAR_MECA(  MODELE=MO1,
                      DDL_IMPO=(_F( GROUP_NO = 'PORTEE',    DY = 0.        ),
                                _F( NOEUD = 'N1',  DX = 0.,         DZ = 0.),
                                _F( NOEUD = 'N14', DX = 0.,         DZ = 0.),
                                _F( NOEUD = 'N24', DX = 0.,         DZ = 0.),
                                _F( NOEUD = 'N25',                  DZ = 0.))
                        )

CHA2=AFFE_CHAR_MECA(  MODELE=MO1,
                      DDL_IMPO=(_F( GROUP_NO = 'PORTEE',     DY = 0.         ),
                                _F( NOEUD = 'N1',  DX =  0.,          DZ = 0.),
                                _F( NOEUD = 'N14', DX =  0.,          DZ = 0.),
                                _F( NOEUD = 'N24', DX =  0.,          DZ = 0.),
                                _F( NOEUD = 'N25', DX =-10., DY = 0., DZ = 0.))
                        )

CHA3=AFFE_CHAR_MECA(  MODELE=MO1,
                      FORCE_NODALE=(_F( NOEUD = 'N12', FZ = -1.E3),
                                    _F( NOEUD = 'N26', FZ = -1.E3),
                                    _F( NOEUD = 'N25', FX =  1.E3))
                        )

CHA4=AFFE_CHAR_MECA(  MODELE=MO1,
                      PESANTEUR=_F(GRAVITE=1.,
                                   DIRECTION=(0.,0.,-1.,),),
                        )

CARA1=AFFE_CARA_ELEM(  MODELE=MO1,
                       CABLE=_F( GROUP_MA = 'TOUT',
                                 SECTION = 1.0, N_INIT=5000.0)
                        )

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
                        INTERVALLE=_F( JUSQU_A = 2., NOMBRE = 2)
                         )

RESU=STAT_NON_LINE(     MODELE=MO1,
                    CHAM_MATER=CHMAT1,
                     CARA_ELEM=CARA1,
                         EXCIT=(_F( CHARGE = CHA1),
                                _F( CHARGE = CHA3)),
                     COMPORTEMENT=(_F( GROUP_MA = 'MCABLE',
                                    RELATION = 'CABLE',
                                    DEFORMATION = 'GROT_GDEP'),
                                _F( GROUP_MA = 'MCABPOUL',
                                    RELATION = 'ELAS',
                                    DEFORMATION = 'GROT_GDEP')),
                     INCREMENT= _F( LIST_INST = L_INST,
                                    NUME_INST_FIN = 1),
                   CONVERGENCE= _F( RESI_GLOB_RELA = 1.E-8,
                                    ITER_GLOB_MAXI = 100),
                        NEWTON= _F( REAC_ITER = 1)
              )

RESU=STAT_NON_LINE( reuse=RESU,
                        MODELE=MO1,
                    CHAM_MATER=CHMAT1,
                     CARA_ELEM=CARA1,
                         EXCIT=(_F( CHARGE = CHA2,
                                    TYPE_CHARGE = 'FIXE_CSTE'),
                                _F( CHARGE = CHA4,
                                    TYPE_CHARGE = 'SUIV')),
                     ETAT_INIT= _F( EVOL_NOLI = RESU,
                                    NUME_ORDRE = 1),
                     COMPORTEMENT=(_F( GROUP_MA = 'MCABLE',
                                    RELATION = 'CABLE',
                                    DEFORMATION = 'GROT_GDEP'),
                                _F( GROUP_MA = 'MCABPOUL',
                                    RELATION = 'ELAS',
                                    DEFORMATION = 'GROT_GDEP')),
                     INCREMENT= _F( LIST_INST = L_INST,
                                    NUME_INST_INIT = 1,
                                    NUME_INST_FIN  = 2),
                   CONVERGENCE= _F( RESI_GLOB_RELA = 1.E-8,
                                    ITER_GLOB_MAXI = 200),
                        NEWTON= _F( REAC_ITER = 1)
              )

RESU=CALC_CHAMP(reuse=RESU,RESULTAT=RESU,CONTRAINTE=('SIEF_ELNO'),VARI_INTERNE=('VARI_ELNO'))


TEST_RESU(RESU=(_F(NUME_ORDRE=2,
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOEUD='N6',
                   NOM_CMP='DZ',
                   VALE_CALC=-13.962397569,
                   VALE_REFE=-13.93,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                _F(NUME_ORDRE=2,
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOEUD='N19',
                   NOM_CMP='DZ',
                   VALE_CALC=-13.895279337,
                   VALE_REFE=-13.93,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                ),
          )

FIN()
