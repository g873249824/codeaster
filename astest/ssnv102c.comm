#----------------------------------------------------------------------
#    CAS TEST VPCS PLAQUE CARREE EN TRACTION CISAILLEMENT PROPORTIONNEL
#                   ESSAI AVEC CHARGE EN RAMPE A 88 N (0.17#)
#----------------------------------------------------------------------
#        UNITES : NEWTON , MILLIMETRE , SECONDE (N,MM,S,->MPA)
#----------------------------------------------------------------------
#
#               Y       A
#               I<------------->                     A = 1 MM
#                     FACE1XZ                                2
#                                                    S = 1 MM
#               V   NO1        A   NO2
#              <O>-------------O>
#              /I             /I                F(T)
#       NO5  V/ I       NO6 A/ I                 A
#           <O>-+-----------O> I              22NI____
#            I  I           I  I                 I   /I
#  FACEYZ    I  I           I  I      FACE1YZ    I  / I
#            I  I           I  I                 I /  I
#            I  V   NO3     I  A  NO4            I/   I
#            I<<O-----------I-<O      - X        +----+----> T
#            I /            I /                       1S
#       NO7  V/        NO8  A/
#          <<O-------------<O
#
#          /       FACEXZ
#         Z               SEUIL ELASTIQUE : F = 64.8    MPA (=RO(1-M))
#                          EN TRACTION      U = 3.24E-4 MM
#
#
#----------------------------------------------------------------------
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

CUBE=LIRE_MAILLAGE(    )

TROISD=AFFE_MODELE(            MAILLAGE=CUBE,
                                    AFFE=_F(   PHENOMENE = 'MECANIQUE',
                                            MODELISATION = '3D',
                                            TOUT = 'OUI'))

R_0_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     72.,
                                 1000.,  72.,      ))

ALPHA_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     .3,
                                 1000.,  .3,       ))

M_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     .1,
                                 1000.,  .1,       ))

A_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     312.,
                                 1000.,  312.,       ))

B_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     30.,
                                 1000.,  30.,       ))

C1_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     -.012,
                                 1000.,  -.012,      ))

C_INF_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     .065,
                                 1000.,  .065,       ))

S_T=DEFI_FONCTION(      NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     450.,
                                 1000.,  450.,      ))

ACIER=DEFI_MATERIAU(            ELAS=_F(   E = 200000.,
                                            NU = .3,
                                            ALPHA = 0.),

                               TAHERI_FO=_F(   R_0 = R_0_T,
                                            ALPHA = ALPHA_T,
                                            M = M_T,
                                            A = A_T,
                                            B = B_T,
                                            C1 = C1_T,
                                            C_INF = C_INF_T,
                                            S = S_T))

CHT0=CREA_CHAMP(OPERATION='AFFE', TYPE_CHAM='CART_TEMP_R', MAILLAGE=CUBE,
           AFFE=_F(  TOUT='OUI',   NOM_CMP = 'TEMP',   VALE = 0.E0,  ) )
MAT=AFFE_MATERIAU(  MAILLAGE=CUBE,
                    AFFE=_F(  MAILLE = 'MA1', MATER = ACIER),
                    AFFE_VARC= _F( TOUT = 'OUI', NOM_VARC = 'TEMP', CHAM_GD=CHT0,VALE_REF=20.),
                                           )

LINST=DEFI_LIST_REEL(              DEBUT=0.,INTERVALLE=(
                              _F( JUSQU_A = .35, NOMBRE = 1),
                                         _F( JUSQU_A = 1.,  NOMBRE = 5))
                                         )

TR_CS=AFFE_CHAR_MECA(
                    MODELE=TROISD,DDL_IMPO=(
                _F(  NOEUD = 'NO4', DX = 0.,DY = 0.),
                         _F(  NOEUD = 'NO8', DX = 0.,DY = 0.,DZ = 0.),
                         _F(  NOEUD = 'NO2', DX = 0.),
                         _F(  NOEUD = 'NO6', DX = 0.)),FORCE_NODALE=(
            _F(  NOEUD = ('NO1', 'NO5', 'NO3', 'NO7',),       FY = -1.),
                         _F(  NOEUD = ('NO3', 'NO7',),  FX = -2.),
                         _F(  NOEUD = ('NO2', 'NO6',),  FY = 1.))
                                                                     )

COEF=DEFI_FONCTION(
                              NOM_PARA='INST',
                            PROL_DROITE='LINEAIRE',
                           PROL_GAUCHE='LINEAIRE',
                                  VALE=( 0.,  0.,
                                         1.,  22.,
                                       ))

SOLNL=STAT_NON_LINE(
               TITRE='TEST TRACTION_CISAILLEMENT MODELE SAID',
                            MODELE=TROISD,
                        CHAM_MATER=MAT,
                             EXCIT=_F(
                CHARGE = TR_CS,
                FONC_MULT = COEF,
                TYPE_CHARGE = 'FIXE_CSTE'),
                         COMPORTEMENT=_F(
                RELATION = 'VISC_TAHERI',
                DEFORMATION = 'PETIT',
                ITER_INTE_MAXI = 20,
                ITER_INTE_PAS = 10,
                TOUT = 'OUI'),
                         INCREMENT=_F(
                LIST_INST = LINST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_INCR = 1,
                REAC_ITER = 10),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 20,
                RESI_GLOB_RELA = 1.E-6),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 4,
                RHO_MAX=200.,
                RESI_LINE_RELA = 1.E-3)
              )

SOLNL=CALC_CHAMP(reuse=SOLNL,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=SOLNL)


#

TEST_RESU(RESU=(_F(NUME_ORDRE=6,
                   PARA='INST',
                   RESULTAT=SOLNL,
                   VALE_CALC=1.0,),
                _F(NUME_ORDRE=6,
                   PARA='ITER_GLOB',
                   VALE_CALC_I=4,
                   RESULTAT=SOLNL,
                   CRITERE='ABSOLU',),
                ),
          )

SOLNL=CALC_CHAMP(reuse=SOLNL,DEFORMATION=('EPSI_ELNO'),RESULTAT=SOLNL)


SOLNL=CALC_CHAMP(reuse=SOLNL,DEFORMATION=('EPSP_ELGA'),RESULTAT=SOLNL)


SOLNL=CALC_CHAMP(reuse=SOLNL,DEFORMATION=('EPSP_ELNO'),RESULTAT=SOLNL)


TEST_RESU(RESU=(_F(NUME_ORDRE=6,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO1',
                   NOM_CMP='EPXX',
                   REFERENCE='ANALYTIQUE',
                   VALE_CALC=0.017213885632237,
                   VALE_REFE=0.01721,
                   PRECISION=1.E-3,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=6,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO1',
                   NOM_CMP='EPXY',
                   REFERENCE='ANALYTIQUE',
                   VALE_CALC=0.025732828808352,
                   VALE_REFE=0.02573,
                   PRECISION=1.E-3,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=6,
                   POINT=1,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPXX',
                   REFERENCE='ANALYTIQUE',
                   VALE_CALC=0.016773885766970,
                   VALE_REFE=0.01678,
                   PRECISION=1.E-3,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=6,
                   POINT=1,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPXY',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=0.02515,
                   VALE_CALC=0.025160828960176,
                   PRECISION=1.E-3,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=6,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELNO',
                   NOEUD='NO1',
                   NOM_CMP='EPXX',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=0.01678,
                   VALE_CALC=0.016773886023194,
                   PRECISION=1.E-3,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=6,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELNO',
                   NOEUD='NO1',
                   NOM_CMP='EPXY',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=0.02515,
                   VALE_CALC=0.025160829216076,
                   PRECISION=1.E-3,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=6,
                   RESULTAT=SOLNL,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='NO1',
                   NOM_CMP='V1',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=0.03356,
                   VALE_CALC=0.033547772227673,
                   PRECISION=1.E-3,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=6,
                   RESULTAT=SOLNL,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='NO1',
                   NOM_CMP='V2',
                   REFERENCE='ANALYTIQUE',
                   VALE_REFE=176.0,
