# champ SIGM_ELNO (2D) :  GROUP_MA='CUBE'
# apres interpolation aux noeuds
TEST_TABLE(VALE_CALC=2.7160736351218E+11,
           NOM_PARA='VALE_NORM',
           TABLE=TBB6,);

FIN();
