DEBUT(CODE=_F(NOM='SSLS108C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=MODI_MAILLAGE(reuse=MAIL,MAILLAGE=MAIL,
ORIE_SHB=_F(GROUP_MA='VOLTOT'),)
#ORIE_SHB=_F(GROUP_MA='TOUT'),)

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='ENCAS',),
                               _F(GROUP_MA='BOUT',),),);

MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB',),);

MAT=DEFI_MATERIAU(ELAS=_F(E=29E+6,
                          NU=0.22,),);

CHM=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          MATER=MAT,),);


CHARGT=AFFE_CHAR_MECA(MODELE=MODMECA,
                      DDL_IMPO=(_F(GROUP_MA=('ENCAS'),
                                   DX=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DY=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DZ=0.0,),),
                      FORCE_NODALE=(_F(GROUP_NO = 'A1',
                                      FZ=0.5,),
                                    _F(GROUP_NO = 'A2',
                                      FZ=0.5,),),);

RESU1=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHM,
                    EXCIT=_F(CHARGE=CHARGT,),
                    );

RESU1=CALC_CHAMP(reuse =RESU1,
                 CONTRAINTE=('SIGM_ELNO'),
                RESULTAT=RESU1,);

CHARGT2=AFFE_CHAR_MECA(MODELE=MODMECA,
                      DDL_IMPO=(_F(GROUP_MA=('ENCAS'),
                                   DX=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DY=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DZ=0.0,),),
                      FORCE_NODALE=(_F(GROUP_NO = 'A1',
                                      FY=0.5,),
                                    _F(GROUP_NO = 'A2',
                                      FY=0.5,),),);

RESU2=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHM,
                    EXCIT=_F(CHARGE=CHARGT2,),
                    );

RESU2=CALC_CHAMP(reuse =RESU2,
                 CONTRAINTE=('SIGM_ELNO'),
                RESULTAT=RESU2,);


TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='A1',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.71754005E-03,
                   VALE_REFE=-1.72E-3,
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A2',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC= 5.40959803E-03,
                   VALE_REFE=5.4200000000000003E-3,
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A1',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC= 1.72648040E-03,
                   VALE_REFE=1.75E-3,
                   PRECISION=0.02,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='A2',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-1.71754005E-03,
                   VALE_REFE=-1.72E-3,
                   PRECISION=1.E-2,),
                ),
          )

FIN()
