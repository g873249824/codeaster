                                SECTION = 'RECTANGLE',
                                CARA=('HY', 'HZ'),
                                VALE =(0.035449077018110321,0.035449077018110321),
                                VARI_SECT = 'CONSTANT',
                               ),
                      ORIENTATION = _F(GROUP_MA=('POUT1','POUT2',),CARA = 'ANGL_VRIL',VALE= 0.),
                      DISCRET = (
                                 _F(
                                    GROUP_MA = ('MASS1', 'MASS2'),
                                    CARA     = 'M_T_D_N',
                                    VALE     = 1000.,
                                    ),
                                 _F(
                                    GROUP_MA = ('SUPP2',),
                                    CARA     = 'K_T_D_N',
                                    VALE     = (0.,75398.,0.,),
                                    ),
                                 _F(
                                    GROUP_MA = ('SUPP2',),
                                    CARA     = 'A_T_D_N',
                                    VALE     = (0.,100.,0.,),
                                    ),
                                 _F(
                                    GROUP_MA = ('SUPP1',),
                                    CARA     = 'K_T_D_L',
                                    VALE     = (75398.0,0.,0.,),
                                    REPERE = 'LOCAL',
                                    ),
                                 ),
                     INFO=1,);




U2 = CALC_EUROPLEXUS(
   LANCEMENT = 'OUI',
   MODELE=MO,
   CHAM_MATER=MATER,
   COMPORTEMENT =(_F(
                  RELATION = 'ELAS',
                  GROUP_MA = ('POUT1','POUT2',),
                 ),
              ),
   CARA_ELEM=ELEM2,
   FONC_PARASOL = _F(
                     GROUP_MA =('SUPP2'),
                     NFKT = NFKT,
                     NFAT = NFKT,
                    ),
   EXCIT=(_F(CHARGE=BLOQ,),
          _F(CHARGE=FORC,FONC_MULT=F_FORC,),
         ),
   CALCUL = _F(TYPE_DISCRETISATION  ='AUTO',
               CSTAB     = 0.8,
               INST_INIT = 0,
               INST_FIN  = 1.0234,
               NMAX      = 2000000,
               ),
   ARCHIVAGE   = _F(INST=2.562047E-01,),
   );



TEST_RESU(
   RESU=(
        _F(
          RESULTAT   = U2,
          #INST       = 2.562047E-01,
          NUME_ORDRE = 23469,
          NOM_CHAM   = 'DEPL',
          NOM_CMP    = 'DY',
          GROUP_NO   = 'P_1_0_0',
          REFERENCE  = 'SOURCE_EXTERNE',
          VALE_CALC  = 0.0129519151791,
          VALE_REFE  = 1.29492E-02,),
        _F(
          RESULTAT   = U2,
          #INST       = 2.562047E-01,
