          REFERENCE  = 'SOURCE_EXTERNE',
          VALE_CALC  = 0.0129519702163,
          VALE_REFE  = 1.29492E-02,),
    )
  );




ELEM2 = AFFE_CARA_ELEM(MODELE = MO,
