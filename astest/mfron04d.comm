                                DEFORMATION = 'GDEF_LOG',
                                RESI_INTE_MAXI=1e-10,
                        ),

                INCREMENT=_F(LIST_INST = L_INST),
                NEWTON=_F(REAC_ITER = 1),
                CONVERGENCE=_F( RESI_GLOB_RELA = 1.E-6,ITER_GLOB_MAXI = 50),
              )


