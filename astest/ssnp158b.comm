                                    LIST_INST=INST,
                                    PAS_MINI=0.001,));

# -----------------------------------------------------------------------------
#  RESOLUTION PAS 0->10
# -----------------------------------------------------------------------------
RESUNL=STAT_NON_LINE(MODELE=MO,
                     CHAM_MATER=CM,
                     EXCIT=(_F(CHARGE=CH1,
                               FONC_MULT=F_DEPL,),),
                     COMPORTEMENT=_F(RELATION='VMIS_ISOT_TRAC',),
                     ARCHIVAGE=_F(LIST_INST=INST),
                     METHODE='NEWTON_KRYLOV',
                     SOLVEUR=_F(METHODE='GCPC',
