DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),);


NbIncr=20
DT=10./NbIncr
Depl=0.2

MA=LIRE_MAILLAGE(FORMAT='MED',);
MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

# -----------------------------------------------------------------------------
