                     VALE=(0.0,  0.0,
                           10.0, 1.0,
                           ),);

# DISCRETISATION EN TEMPS
INST=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=_F(JUSQU_A=10.0,
                                  NOMBRE=NbIncr,),);

