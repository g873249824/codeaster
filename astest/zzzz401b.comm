# Verification des renumerotateurs des solveurs lineaires directs
# en parallèle MPI
#########################################################################
# VERIFICATION DES RENUMEROTEURS DE SOLVEUR DIRECT.
# VERSION PARALLELE MPI SUR 2 PROCS.
#########################################################################

DEBUT(  CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
MA=LIRE_MAILLAGE(FORMAT='MED')
DEFI_GROUP(reuse=MA, MAILLAGE=MA, CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'))
MO=AFFE_MODELE(MAILLAGE=MA, AFFE=_F(TOUT='OUI',PHENOMENE='MECANIQUE',MODELISATION='3D'))
MAT=DEFI_MATERIAU(   ELAS=_F( E = 1.E+05,  NU = 0.3, RHO=9800.)  )
CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MA,  AFFE=_F( TOUT = 'OUI', MATER = MAT) )
CHAR=AFFE_CHAR_MECA( MODELE=MO,
       DDL_IMPO=  _F( GROUP_MA = 'BASE1',  DZ = 0., DY = 0.),
       PESANTEUR=_F(GRAVITE=9.81,DIRECTION=(0., 0., -1.)))
CHARCI=AFFE_CHAR_CINE( MODELE=MO, MECA_IMPO=  _F( GROUP_NO = 'BASE1',  DX = 10.))
L1=DEFI_LIST_REEL(VALE=(1., 2.))
iinfo=1

# 1: Calcul avec MULT_FRONT
#---------------------------
solver='MULT_FRONT'
ordering='MD'
RESU11=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU11,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='MDA'
RESU12=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU12,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='METIS'
RESU13=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU13,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)

# 2: Calcul avec LDLT
#--------------------
solver='LDLT'
ordering='RCMK'
RESU21=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU21,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)

# 3: Calcul avec MUMPS
#---------------------
solver='MUMPS'
ordering='AMD'
#PRETRAITEMENT='SANS' afin que MUMPS ne substitue pas un autre renuméroteur
RESU31=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU31,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='AMF'
RESU32=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU32,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='PORD'
RESU33=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU33,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='METIS'
RESU34=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU34,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='QAMD'
RESU35=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU35,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='SCOTCH'
RESU36=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU36,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='AUTO'
RESU37=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=iinfo,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU37,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)

ordering='PARMETIS'
RESU38=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=2,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU38,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)
ordering='PTSCOTCH'
RESU39=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=2,
                    EXCIT=( _F(CHARGE = CHAR),_F(CHARGE = CHARCI)),
                    SOLVEUR=_F(METHODE=solver, RENUM=ordering, PRETRAITEMENTS='SANS'),)
TEST_RESU(RESU=_F(NUME_ORDRE=1,GROUP_NO='C',RESULTAT=RESU39,NOM_CHAM='DEPL',NOM_CMP='DZ',
                  VALE_CALC=-41.60757189956,),)

FIN(INFO_RESU='NON');
