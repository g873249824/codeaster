DEBUT(  CODE=_F(  NOM = 'TTLV01B',NIV_PUB_WEB='INTERNET') )

#
# LECTURE DU MAILLAGE --------------------------------------------------
#


MAIL=LIRE_MAILLAGE(FORMAT='MED', )

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ----------------------------
#

# ----- MATERIAU


MATER=DEFI_MATERIAU(   THER=_F(  LAMBDA = 48.822,  RHO_CP = 4816800.) )

#
# AFFECTATIONS: MATERIAU, MODELE, CHARGEMENT ---------------------------
#


CHMAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',  MATER = MATER)
                        )

MOTH=AFFE_MODELE(     MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',
                                  MODELISATION = 'AXIS',
                                  PHENOMENE = 'THERMIQUE')
                        )

CHTH=AFFE_CHAR_THER(   MODELE=MOTH,
                             ECHANGE=_F(  GROUP_MA = 'FACE_EXT',
                                       COEF_H = 232.5,  TEMP_EXT = 1000.)
                         )

#
# DEFINITION DE LA STRATEGIE DE CALCUL ---------------------------------
#


LTMPS=DEFI_LIST_REEL(   DEBUT=0.,INTERVALLE=(
                            _F(  JUSQU_A = 100.,   NOMBRE = 8),
                            _F(  JUSQU_A = 300.,   NOMBRE = 8),
                            _F(  JUSQU_A = 700.,   NOMBRE = 8),
                            _F(  JUSQU_A = 1400.,   NOMBRE = 7),
                            _F(  JUSQU_A = 2400.,   NOMBRE = 5))
                         )

#
# RESOLUTION -----------------------------------------------------------
#

TEMPE=THER_LINEAIRE(  MODELE=MOTH,
                          ETAT_INIT=_F(  VALE = 20.E0),
                          INCREMENT=_F(  LIST_INST = LTMPS),
                          CHAM_MATER=CHMAT,
                          EXCIT=_F( CHARGE = CHTH)
                       )

