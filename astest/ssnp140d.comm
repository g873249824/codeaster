                         CRITERE='ABSOLU',
                         VALE_PARA=8.0000000000000004E-4,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=9.9999999999999998E-13,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=-1.33226762955e-15,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=1.1999999999999999E-3,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=9.9999999999999998E-13,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=3.5527136788e-15,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=1.6000000000000001E-3,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=9.9999999999999998E-13,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=-1.15463194561e-14,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=2.E-3,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=9.9999999999999998E-13,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=-0.543750803384,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=3.2000000000000002E-3,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=0.54500000000000004,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=-3.5452667449,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=4.0000000000000001E-3,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=3.5459999999999998,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=0.572380651762,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=6.0000000000000001E-3,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=0.57299999999999995,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=0.631081279895,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=8.0000000000000002E-3,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=0.63200000000000001,
                         FONCTION=DIFF,),
                      _F(VALE_CALC=0.665792750495,
                         VALE_REFE=0.0,
                         CRITERE='ABSOLU',
                         VALE_PARA=1.E-2,
                         REFERENCE='AUTRE_ASTER',
                         PRECISION=0.66700000000000004,
                         FONCTION=DIFF,),
                      ),
              )

# CALCUL DU CRITERE DES MOINDRES CARRES ENTRE LES DEUX COURBES
# ON INTEGRE LA DIFF DES DEUX COURBES ET ON SOMME LES TERMES
INT=CALC_FONCTION(INTEGRE=(
                          _F( FONCTION = DIFF,),
                          ) );

TINT=CREA_TABLE(FONCTION= _F(FONCTION = DIFF,
                             PARA = ('INST','DY')),);

TEST_TABLE(
           VALE_CALC=117.6284793894,
           NOM_PARA='DY',
           TYPE_TEST='SOMM_ABS',
           TABLE=TINT,
           )

FIN();
