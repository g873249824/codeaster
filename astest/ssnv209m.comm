# SURFACE RIGIDE, MODELE1_63 PEUT ETRE IGNOREE
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),IGNORE_ALARM='MODELE1_63',DEBUG=_F(SDVERI='OUI'))

#......................................................
#  CALCUL TEST ELEMENTS FINIS XFEM ET CONTACT 2D TRI6
#  PLAQUE EN CONTACT FROTTANT SUR BATI RIGIDE
#  AVEC XFEM
#......................................................
#
# La fissure coincide avec les faces du maillage
#
#----------------------------------------------
#                MAILLAGE
#----------------------------------------------

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'));

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO= (_F(GROUP_MA='SURF'),
                                    _F(GROUP_MA='LIG1',
                                       NOM='NLIG1')),
                    CREA_GROUP_MA= (_F(OPTION='BANDE',
                                       NOM='PLAQ',
                                       GROUP_NO_CENTRE='P4',
                                       VECT_NORMALE= (0.,1.),
                                       DIST=0.0399),
                                    _F(OPTION='BANDE',
                                       NOM='BAT',
                                       GROUP_NO_CENTRE='P1',
                                       VECT_NORMALE= (0.,1.),
                                       DIST=0.0099)),
                    INFO=1);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(_F(OPTION='ENV_SPHERE',
                                      NOM='PPR',
                                      POINT=(0.03875,0.),
                                      RAYON=0.000001,
                                      PRECISION=0.00001),
                                   _F(NOM='NFISSU',
                                      OPTION='PLAN',
                                      POINT=(0.,0.),
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=5E-4)),
                    INFO=1);

MAILLAG1=MODI_MAILLAGE(reuse=MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_2D=(_F(GROUP_MA = 'LIG2'),
                                     _F(GROUP_MA = 'LIG3'),
                                     _F(GROUP_MA = 'LIG4')));

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=(_F(GROUP_MA=('LIG3'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('LIG1'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('SURF'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('LIG2'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'),
                           _F(GROUP_MA=('LIG4'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN')),
                     INFO=1);

# FORME ANALYTIQUE POUR LA FISSURE

LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y');

FISS=DEFI_FISS_XFEM(MAILLAGE=MAILLAG1,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN),
                    INFO=1);

# MODELE XFEM

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='STANDARD',
                         INFO=1);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=1.3E11
nu=0.2
rho=7800.

CHABNO=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_R',
                  OPERATION='EXTR',
                  FISSURE=FISS,
                  NOM_CHAM='LNNO');

NU_F=DEFI_FONCTION(NOM_PARA='NEUT1',
                   VALE=(-1.0,0.0,-1.E-9,0.0,1.E-9,nu,1.0,nu));

E_F=DEFI_FONCTION(NOM_PARA='NEUT1',
                  VALE=(-1.0,1.E16,-1.E-9,1.E16,1.E-9,E,1.0,E));

MATVAR=DEFI_MATERIAU(ELAS_FO=_F(E=E_F,NU=NU_F,RHO=rho));

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('PLAQ'),
                                MATER=MATVAR),
                             _F(GROUP_MA=('BAT'),
                                MATER=MATVAR)),
                       AFFE_VARC=_F(NOM_VARC='NEUT1',
                                    CHAM_GD=CHABNO));

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------


CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      FROTTEMENT     = 'COULOMB',
                      ITER_CONT_MAXI = 4,
                      ITER_FROT_MAXI = 20,
                      REAC_GEOM      = 'SANS',ELIM_ARETE     = 'ELIM',
                      ZONE=(
                            _F(
                               FISS_MAIT      = FISS,
                               INTEGRATION    = 'NCOTES',
                               CONTACT_INIT   = 'OUI',
                               COULOMB        = 1.0,
                               ALGO_CONT      = 'STANDARD',
                               COEF_CONT      = 100.,
                               ALGO_FROT      = 'STANDARD',
                               COEF_FROT      = 100.,
                               ALGO_LAGR      = 'VERSION2',
                             ),
                         ),
                   );

CHCINE=AFFE_CHAR_CINE(MODELE=MODELEK,
#                     ENCASTREMENT DU BAS
                      MECA_IMPO=(_F(GROUP_NO='NLIG1',
                                    DX=0.,
                                    DY=0.)));

CHMECA=AFFE_CHAR_MECA(MODELE=MODELEK,
#                     FORCE IMPOSEE EN A
                      PRES_REP=(_F(GROUP_MA = 'LIG3',
                                   PRES = 5.E7)));

def pr(y) :
   if y  < 1e-15:  return 0.E07
   if y  > 1e-15:  return 15.E7
   if y == 1e-15:  return 0.

FCTPRES = FORMULE(VALE='pr(Y)',
                  NOM_PARA=('X','Y'));

CHAF=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                      PRES_REP=_F(GROUP_MA = ('LIG2','LIG4'),
                                  PRES=FCTPRES));

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0));

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=1.0,NOMBRE=1));


# LES SYSTEMES LINEAIRES SONT DIFFICILES A RESOUDRE.
# ON DEVERROUILLE LA DETECTION DE SINGULARITE. LE PROCESSUS DE NEWTON
# CORRIGE LES EVENTUELLES DERIVES.
UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA1,
                    EXCIT=(
                           _F(CHARGE=CHCINE),
                           _F(CHARGE=CHMECA,
                              FONC_MULT=RAMPE),
                           _F(CHARGE=CHAF,
                              FONC_MULT=RAMPE),
                           ),
                    CONTACT  =CTXFEM,
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',
                                 ),
                    INCREMENT=_F(LIST_INST=L_INS1,
                                 INST_FIN=1.0,
                                 ),
                    CONVERGENCE=_F(ARRET='OUI',
                                   ITER_GLOB_MAXI=40,
                                   RESI_GLOB_RELA=1.E-6,
                                   ),
                    SOLVEUR=_F(METHODE='MUMPS',

                               NPREC=-1,
                               ),
                    NEWTON=_F(REAC_ITER=1),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                    );

#-----------------------------------------------------------
#             POST-TRAITEMENT POUR LA VISUALISATION
#-----------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK)

MA_XFEM=DEFI_GROUP(reuse=MA_XFEM,
                   MAILLAGE=MA_XFEM,
                   CREA_GROUP_NO=(_F(OPTION='ENV_SPHERE',
                                     NOM='PPA',
                                     POINT=(0.,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPB',
                                     POINT=(0.00125,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPC',
                                     POINT=(0.005,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPD',
                                     POINT=(0.0075,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
                                  _F(OPTION='ENV_SPHERE',
                                     NOM='PPE',
                                     POINT=(0.01125,0.),
                                     RAYON=0.000001,
                                     PRECISION=0.00001),
#                                  _F(NOM='NFISSU',
#                                     OPTION='PLAN',
#                                     POINT=(0.,0.),
#                                     VECT_NORMALE=(0.,1.),
#                                     PRECISION=1.E-6),
                                   ),
                   INFO=1);

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='D_PLAN'))

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        )

#----------------------------------------------
#                   POST RELEVE
#----------------------------------------------

DEP_NFI=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                INST=1.0,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                     TABLE = DEP_NFI,
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]')));

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                     TABLE = DEP_NFI,
                     ACTION=_F(OPERATION='TRI',
                               NOM_PARA='COOR_X',
                               ORDRE='CROISSANT'));

IMPR_TABLE(TABLE=DEP_NFI);

