# ssnl106l.para = tps_job 60 mem_job 128Mo ncpus 1 liste_test R
# VERSION DYNA_NON_LINE (QUASI_STATIQUE)


DEBUT(CODE=_F(NOM='SSNL106L', NIV_PUB_WEB='INTERNET'))

#------- MAILLAGE FILAIRE DE LA POUTRE

MA=LIRE_MAILLAGE()

#------- MAILLAGE DE LA SECTION RECTANGULAIRE

MA_RECT=LIRE_MAILLAGE(FORMAT='MED',UNITE=21,INFO=2)

#------- MAILLAGE DE LA SECTION CIRCULAIRE

MA_CERC=LIRE_MAILLAGE(FORMAT='MED',UNITE=22)

GF=DEFI_GEOM_FIBRE(INFO=1,
                   SECTION =( _F( GROUP_FIBRE='S_RECT',
                                  MAILLAGE_SECT = MA_RECT, TOUT_SECT = 'OUI',
                                  COOR_AXE_POUTRE = (0., 0.,),),
                              _F( GROUP_FIBRE='S_CERC',
                                  MAILLAGE_SECT = MA_CERC, TOUT_SECT = 'OUI',
                                  COOR_AXE_POUTRE = (0., 0.,),),
                             )
                   )

MO=AFFE_MODELE(        MAILLAGE=MA,
     AFFE=_F( TOUT = 'OUI', PHENOMENE = 'MECANIQUE',MODELISATION = 'POU_D_TGM')
                        )

CAREL=AFFE_CARA_ELEM(   MODELE=MO,POUTRE=(
          # SECTION CERCLE :
           _F(  GROUP_MA = 'GC1',   SECTION = 'CERCLE',
          CARA = (   'R',   ), VALE = (  0.1,)),
          # SECTION RECTANGLE  :
           _F(  GROUP_MA = 'GR1',   SECTION = 'RECTANGLE',
          CARA = (   'HY', 'HZ',  ), VALE = (  0.2,  0.1, ))),
                  GEOM_FIBRE=GF,
                  MULTIFIBRE=(_F(GROUP_MA='GC1',GROUP_FIBRE='S_CERC'),
                              _F(GROUP_MA='GR1',GROUP_FIBRE='S_RECT'),
                     )
                  )

# ------ TRACTION SIMPLE AVEC ECROUISSAGE ------------------------------
# AFFECTE SUR LA SECTION CERCLE

MATC2=DEFI_MATERIAU(   ELAS=_F( E = 2.E11, NU = 0.3,
                              RHO=7850.0),
           ECRO_LINE=_F( D_SIGM_EPSI = 2.E9, SY = 150.E6),
                        )

# AFFECTE SUR LA SECTION RECTANGLE

MATR3=DEFI_MATERIAU(   ELAS=_F( E = 2.E11, NU = 0.3,
                              RHO=7850.0),
           ECRO_LINE=_F( D_SIGM_EPSI = 0., SY = 150.E6),
                        )

COMPPMFA=DEFI_COMPOR(GEOM_FIBRE=GF,
                    MATER_SECT=MATR3,
                    MULTIFIBRE=(
                                 _F(GROUP_FIBRE='S_RECT', MATER=MATR3, RELATION='VMIS_ISOT_LINE'),
                               ),
                      )
COMPPMFB=DEFI_COMPOR(GEOM_FIBRE=GF,
                    MATER_SECT=MATC2,
                    MULTIFIBRE=(
                                 _F(GROUP_FIBRE='S_CERC', MATER=MATC2, RELATION='VMIS_ISOT_LINE'),
                                  ),
                      )
CHMAT1=AFFE_MATERIAU(   MAILLAGE=MA,
                        AFFE=(_F( GROUP_MA = 'GR1', MATER = MATR3),
                              _F( GROUP_MA = 'GC1', MATER = MATC2)),
                   AFFE_COMPOR= (
                                  _F( GROUP_MA = 'GR1', COMPOR = COMPPMFA),
                                  _F( GROUP_MA = 'GC1', COMPOR = COMPPMFB)))



CHART=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
     _F( GROUP_NO = 'A', DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     _F( GROUP_NO = 'B', DX = 0.75E-3))
    )

FONCT=DEFI_FONCTION(   NOM_PARA='INST',
                      VALE=( 0., 0., 10., 10., ) )

#

LINST=DEFI_LIST_REEL(DEBUT=0.,INTERVALLE=(
               _F( JUSQU_A = 1., NOMBRE = 1),
               _F( JUSQU_A = 2., NOMBRE = 10),
               _F( JUSQU_A = 3., NOMBRE = 10))
)

ACCE0 = CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',OPERATION='AFFE',MODELE=MO,
                   AFFE=_F(TOUT='OUI',NOM_CMP=('DX', 'DY', 'DZ', 'DRX', 'DRY', 'DRZ' , 'GRX'),
                                      VALE = (0., 0., 0.,0., 0., 0., 0.,),),);

REST=DYNA_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHMAT1,
                         CARA_ELEM=CAREL,
#  On impose explicitement une acceleration initiale nulle car la matrice de masse etant singuliere
#  a cause du modele POU_D_TGM (DDL de gauchissement sans masse), on ne peut calculer une acceleration non nulle :
                         ETAT_INIT=_F(ACCE=ACCE0,),
                             EXCIT=_F(
                CHARGE = CHART,
                FONC_MULT = FONCT),COMP_INCR=(
                         _F(
                RELATION = 'MULTIFIBRE',

                ITER_INTE_MAXI = 500,
                GROUP_MA = 'GC1'),
                         _F(
                RELATION = 'MULTIFIBRE',

                ITER_INTE_MAXI = 500,
                GROUP_MA = 'GR1')),
                         INCREMENT=_F(
                LIST_INST = LINST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
                                FORMULATION='DEPLACEMENT',),
              )

REST=CALC_ELEM( reuse=REST,
                            OPTION=(
                  'SIEF_ELNO',),
                          RESULTAT=REST,
              )
REST=CALC_ELEM( reuse=REST,
                     RESULTAT=REST,
                       OPTION='DEGE_ELNO'    )
#IMPR_RESU(RESU=_F(RESULTAT=REST,NUME_ORDRE=21,NOM_CHAM=('DEGE_ELNO','VARI_ELGA','SIEF_ELNO')))

TEST_RESU(RESU=(_F( RESULTAT = REST,
                 NUME_ORDRE = 11,
                 NOM_CHAM = 'SIEF_ELNO',
                 NOEUD = 'R3',
                 MAILLE = 'SR2',
                 NOM_CMP = 'N',
                 VALE = 3.0E+6, REFERENCE = 'ANALYTIQUE',
                 PRECISION = 1.E-3),
           _F( RESULTAT = REST,
                 NUME_ORDRE = 21,
                 NOM_CHAM = 'SIEF_ELNO',
                 NOEUD = 'R3',
                 MAILLE = 'SR2',
                 NOM_CMP = 'N',
                 VALE = 3.E+6, REFERENCE = 'ANALYTIQUE',
                 PRECISION = 1.E-3),
           _F( RESULTAT = REST,
                 NUME_ORDRE = 11,
                 NOM_CHAM = 'SIEF_ELNO',
                 NOEUD = 'C3',
                 MAILLE = 'SC2',
                 NOM_CMP = 'N',
                 VALE = 4.822395E+6, REFERENCE = 'ANALYTIQUE',
                 PRECISION = 2.5E-2),
           _F( RESULTAT = REST,
                 NUME_ORDRE = 21,
                 NOM_CHAM = 'SIEF_ELNO',
                 NOEUD = 'C3',
                 MAILLE = 'SC2',
                 NOM_CMP = 'N',
                 VALE = 4.870141E+06, REFERENCE = 'ANALYTIQUE',
                 PRECISION = 2.5E-2),
           _F( RESULTAT = REST,
                 NUME_ORDRE = 21,
                 NOM_CHAM = 'DEGE_ELNO',
                 NOEUD = 'C3',
                 MAILLE = 'SC2',
                 NOM_CMP = 'EPXX',
                 VALE = 2.25000E-03, REFERENCE = 'ANALYTIQUE',
                 PRECISION = 1.E-3),
           _F( RESULTAT = REST,
                 NUME_ORDRE = 21,
                 NOM_CHAM = 'VARI_ELGA',
                 POINT = 1,
                 SOUS_POINT = 1,
                 MAILLE = 'SR2',
                 NOM_CMP = 'V1',
                 VALE = 1.5E-3, REFERENCE = 'NON_REGRESSION',
                 PRECISION = 2.E-1),
           _F( RESULTAT = REST,
                 NUME_ORDRE = 21,
                 NOM_CHAM = 'VARI_ELGA',
                 POINT = 1,
                 SOUS_POINT = 1,
                 MAILLE = 'SC2',
                 NOM_CMP = 'V1',
                 VALE = 1.5E-3, REFERENCE = 'ANALYTIQUE',
                 PRECISION = 1.5E-2))
          )


FIN()
