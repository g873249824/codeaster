               POINT = 1,
               MAILLE='M1294',
               NOM_CMP = 'NUEST',
               VALE = 1.2725,
               REFERENCE = 'NON_REGRESSION',VERSION = '11.0.30'),
             ),
            );

TEST_RESU(RESU=(
           _F( RESULTAT = RESU,
               INST = 1.,
               NOM_CHAM = 'ERME_ELNO',
               CRITERE = 'RELATIF',
               PRECISION = 0.0001,
               NOEUD= 'N5',
               MAILLE='M1294',
