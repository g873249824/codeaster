           NOM_PARA='SIYY',
           TABLE=S_T,
           FILTRE=_F(NOM_PARA='COOR_X',
                     VALE=3.8079700000000001,),
           )

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.08,
           VALE_CALC=168.85858339317,
