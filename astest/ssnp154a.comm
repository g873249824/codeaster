DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),IGNORE_ALARM='CONTACT3_16',DEBUG=_F(SDVERI='OUI'))

MESH=LIRE_MAILLAGE(FORMAT='MED',);

MESH=MODI_MAILLAGE(reuse =MESH,
                   MAILLAGE=MESH,
                   ORIE_PEAU_2D=(_F(GROUP_MA='Top_B',),
                                 _F(GROUP_MA='Bottom_C',),),);

MESH=DEFI_GROUP(reuse=MESH,
                  MAILLAGE=MESH,
                  CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'),);

MAT_cy=DEFI_MATERIAU(ELAS=_F(E=210000.0,
                             NU=0.3,),);

MAT_bl=DEFI_MATERIAU(ELAS=_F(E=70000.0,
                             NU=0.3,),);

FONC=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                         1.0,1.0,
                         ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                     NOMBRE=2,),);

MO=AFFE_MODELE(MAILLAGE=MESH,
                 AFFE=(_F(GROUP_MA=('Block',),
                         PHENOMENE='MECANIQUE',
                         MODELISATION=('D_PLAN',),),
                       _F(GROUP_MA=('Cylinder',),
                         PHENOMENE='MECANIQUE',
                         MODELISATION=('D_PLAN',),),
                         ),
                         );

MA=AFFE_MATERIAU(MAILLAGE=MESH,
                   AFFE=(_F(GROUP_MA='Block',
                            MATER=MAT_bl,),
                         _F(GROUP_MA='Cylinder',
                            MATER=MAT_cy,),),);

CH_LIM=AFFE_CHAR_CINE(MODELE=MO,
                    MECA_IMPO=(_F(GROUP_NO='Axe',
                                 DX=0.0,),
                              _F(GROUP_NO='Encast',
                                 DX=0.0,
                                 DY=0.0,),),);

FORCE=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_NODALE=_F(GROUP_NO='Force',
                                    FY=-17500.0,),);

CONT=DEFI_CONTACT(MODELE=MO,
                  FORMULATION='CONTINUE',
                  ALGO_RESO_CONT = 'POINT_FIXE',
                  ALGO_RESO_GEOM = 'POINT_FIXE',
                  ZONE=_F(
                          GROUP_MA_MAIT='Top_B',
                          GROUP_MA_ESCL='Bottom_C',
                          CONTACT_INIT='OUI',
                          INTEGRATION='GAUSS',
                          ),);

MESH=DEFI_GROUP(reuse =MESH,
                  MAILLAGE=MESH,
                  CREA_GROUP_NO=(_F(OPTION='NOEUD_ORDO',
                                   NOM='Bottom_n',
                                   GROUP_MA='Bottom_C',
                                   GROUP_NO_ORIG='C',),
                                 _F(OPTION='NOEUD_ORDO',
                                   NOM='Top_n',
                                   GROUP_MA='Top_B',
                                   GROUP_NO_ORIG='B',),),);

RESU=STAT_NON_LINE(MODELE=MO,
                     CHAM_MATER=MA,
                     EXCIT=(_F(CHARGE=CH_LIM,),
                            _F(CHARGE=FORCE,
                               FONC_MULT=FONC,),),
                     CONTACT=CONT,
                     COMPORTEMENT=_F(RELATION='ELAS',
                                  DEFORMATION='GROT_GDEP',),
                     INCREMENT=_F(LIST_INST=L_INST,),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=50,),
                     SOLVEUR=_F(),
                     NEWTON=_F(REAC_ITER=1,),);

RESU=CALC_CHAMP(reuse=RESU,RESULTAT=RESU,CONTRAINTE=('SIEF_ELNO'))


RESU=CALC_CHAMP(reuse =RESU,
               RESULTAT=RESU,
               CONTRAINTE='SIEF_NOEU',FORCE=('FORC_NODA','REAC_NODA'));

IMPR_RESU(FORMAT='MED',
          UNITE=80,
          RESU=_F(RESULTAT=RESU,
                  NOM_CHAM=('DEPL','FORC_NODA',),),);

IMPR_RESU(FORMAT='MED',
          UNITE=81,
          RESU=_F(RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP=('DX','DY',),),);

MOINS_UN=FORMULE(NOM_PARA='LAGS_C',VALE='1.0*LAGS_C',);

RELE=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='Pression',
                               RESULTAT=RESU,
                               NOM_CHAM='DEPL',
                               INST=1.0,
                               GROUP_NO='Bottom_n',
                               NOM_CMP='LAGS_C',),);

RELE=CALC_TABLE(reuse=RELE,
                TABLE=RELE,
                ACTION=(_F(OPERATION='OPER',
                           NOM_PARA='MLAGS_C',
                           FORMULE=MOINS_UN,),),);

PRES=RECU_FONCTION(TABLE=RELE,
                     PARA_X='ABSC_CURV',
                     PARA_Y='MLAGS_C',);

# ===========================================

# champ de contraintes de Cauchy aux noeuds
p_appr_F=CALC_PRESSION(MAILLAGE=MESH,
                             RESULTAT=RESU,
                                GROUP_MA=('Bottom_C','Top_B',),
                            INST=1.0,
                    );


depl=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RESU,
                NOM_CHAM='DEPL',
                INST=1.0,
                );
mdepl=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='COMB',
                COMB=_F(CHAM_GD=depl,
                        COEF_R=-1.0,),
                );

MESH=MODI_MAILLAGE(reuse=MESH,
                   MAILLAGE=MESH,
                   DEFORME=_F(OPTION='TRAN',
                              DEPL=depl,),);

MESH=MODI_MAILLAGE(reuse=MESH,
                   MAILLAGE=MESH,
                   DEFORME=_F(OPTION='TRAN',
                              DEPL=depl,),);

# abscisse curviligne sur la deformee
CALC_PSF=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='P_S',
                                CHAM_GD=p_appr_F,
                                GROUP_NO='Bottom_n',
                                TOUT_CMP='OUI',),);

# abscisse curviligne sur la deformee
CALC_PMF=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='P_M',
                                CHAM_GD=p_appr_F,
                                GROUP_NO='Top_n',
                                TOUT_CMP='OUI',),);

MESH=MODI_MAILLAGE(reuse=MESH,
                   MAILLAGE=MESH,
                   DEFORME=_F(OPTION='TRAN',
                              DEPL=mdepl,),);

PS_FONCF=RECU_FONCTION(TABLE=CALC_PSF,
                      PARA_X='ABSC_CURV',
                      PARA_Y='LAGS_C',);

PM_FONCF=RECU_FONCTION(TABLE=CALC_PMF,
                      PARA_X='ABSC_CURV',
                      PARA_Y='LAGS_C',);

# ===========================================

# Construction de la solution analytique
a=6.21
p0=3585.37
p=FORMULE(NOM_PARA='X',VALE='-1.0*p0*sqrt(1-(X/a)*(X/a))',)
absc=DEFI_LIST_REEL(DEBUT=0,
                    INTERVALLE=(_F(JUSQU_A=5,
                                  NOMBRE=100,),
                                _F(JUSQU_A=a,
                                  NOMBRE=100,),
                                ));
SOL=CALC_FONC_INTERP(FONCTION=p,
                     NOM_PARA='X',
                     LIST_PARA=absc)

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=29,
              COURBE=(_F(FONCTION=SOL,LEGENDE='Solution analytique',MARQUEUR=0,),
                      _F(FONCTION=PRES,LEGENDE='Pression de contact LAGS_C',MARQUEUR=0,),
                      _F(FONCTION=PS_FONCF,LEGENDE='Pression extrapolee esclave CALC_PRESSION',MARQUEUR=0,),
                      _F(FONCTION=PM_FONCF,LEGENDE='Pression extrapolee maitre CALC_PRESSION',MARQUEUR=0,),
                      ),
              TITRE='Lineaire sans frottement',
              BORNE_X=(0.0,8.0,),
              BORNE_Y=(-5000.0,10.0,),
              ECHELLE_X='LIN',
              ECHELLE_Y='LIN',
              GRILLE_X=1,
              GRILLE_Y=500,
              LEGENDE_X='Densite surfacique (N/mm2)',
              LEGENDE_Y='Abscisse curviligne (mm)',);


# ===========================================
# Tests

TEST_RESU(RESU=(_F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.306904040,
                   VALE_REFE=-1.3069040401076999,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIEF_NOEU',
                   NOM_CMP='SIYY',
                   VALE_CALC=-3.80049495E+03,
                   VALE_REFE=-3800.4949548282998,
                   ),
                ),
          )

FIN();
