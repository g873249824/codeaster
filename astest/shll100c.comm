                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='DX',
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=1,
                   VALE_CALC_C=(-7.8466047322167004e-08+9.6156749722208995e-10j),
