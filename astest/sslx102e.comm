# MODELISATION TUYAU

DEBUT(CODE=_F(NOM='SSLX102E',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M1=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

M1=DEFI_GROUP( reuse=M1,   MAILLAGE=M1,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MA=CREA_MAILLAGE(     MAILLAGE=M1,
       MODI_MAILLE=_F(  OPTION = 'SEG3_4', TOUT = 'OUI')
)

MA=DEFI_GROUP( reuse=MA,   INFO=2,
                MAILLAGE=MA,
           CREA_GROUP_NO=_F(  NOM = 'AD',
OPTION = 'NOEUD_ORDO',
GROUP_MA = 'AD',
#NOEUD_ORIG : N296
#NOEUD_EXTR : N297
GROUP_NO_ORIG = 'A',
GROUP_NO_EXTR = 'D')
                   )

MO=AFFE_MODELE(        MAILLAGE=MA, INFO=2,
     AFFE=_F(  TOUT = 'OUI',   PHENOMENE = 'MECANIQUE',
     MODELISATION = 'TUYAU_3M')
               )

CAREL=AFFE_CARA_ELEM(   MODELE=MO, INFO=2,
          POUTRE=_F( GROUP_MA = 'TOUT', SECTION = 'CERCLE',TUYAU_NCOU=1,
           CARA = (   'R',  'EP', ), VALE = (  0.10959,  0.00818, )),
ORIENTATION=_F( GROUP_NO = 'D', CARA = 'GENE_TUYAU', VALE = (0., 0., 1.,))
                              )

MAT=DEFI_MATERIAU(   ELAS=_F( E = 2.E11, NU = 0.3,  RHO = 7900.),
                   ECRO_LINE=_F( D_SIGM_EPSI = 12.5E9, SY = 200.E6)
                   )

CHMAT=AFFE_MATERIAU(   MAILLAGE=MA,
                       AFFE=_F( TOUT = 'OUI', MATER = MAT))

#FLEXION PLANE

CHAR1=AFFE_CHAR_MECA(  MODELE=MO,
     DDL_IMPO=_F( GROUP_NO = 'A', DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     FORCE_NODALE=_F( GROUP_NO = 'D', MZ = 17.E3)
    )

#RESU1 = MECA_STATIQUE (MODELE: MO CHAM_MATER : CHMAT CARA_ELEM : CAREL
#                        EXCIT: ( CHARGE: CHAR1 )  );

RAMPE=DEFI_FONCTION(  NOM_PARA='INST',
            PROL_GAUCHE='LINEAIRE', PROL_DROITE='LINEAIRE',
            VALE=(0.0,0.0,1.0,1.0,) )

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
                INTERVALLE=_F( JUSQU_A = 1.,   NOMBRE = 1) )

RESU1=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CAREL,
                             EXCIT=_F(
                CHARGE = CHAR1,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F( RELATION = 'ELAS', ),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6)
              )

RESU1=CALC_CHAMP(reuse=RESU1,CONTRAINTE=('EFGE_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=RESU1)


TEST_RESU(
           RESU=_F( RESULTAT = RESU1,
                 NUME_ORDRE = 1,
                 NOM_CHAM = 'DEPL',
                 GROUP_NO = 'D',
                 NOM_CMP = 'DY',
# REF 20C20D3COU16SEC
                 VALE = 1.86137E-02,
                 PRECISION = 1.E-2)
                    )

# FLEXION HORS PLAN

CHAR2=AFFE_CHAR_MECA(  MODELE=MO,
     DDL_IMPO=_F( GROUP_NO = 'A', DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     FORCE_NODALE=_F( GROUP_NO = 'D', MY = 17.E3)
    )

RESU2=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CAREL,
                             EXCIT=_F(
                CHARGE = CHAR2,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F( RELATION = 'ELAS', ),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6)
              )

RESU2=CALC_CHAMP(reuse=RESU2,CONTRAINTE=('EFGE_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=RESU2)


TEST_RESU(
           RESU=_F( RESULTAT = RESU2,
                 NUME_ORDRE = 1,
                 NOM_CHAM = 'DEPL',
                 GROUP_NO = 'D',
                 NOM_CMP = 'DZ',
# REF 20C20D3COU16SEC
                 VALE = -1.535E-02,
                 PRECISION = 1.E-2)
                    )



MASSEAB=POST_ELEM(   MODELE=MO,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'AB')
     )

MASSEBC=POST_ELEM(   MODELE=MO,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'BC')
     )

TEST_TABLE( TABLE=MASSEAB,
                NOM_PARA='MASSE',
                #FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
               REFERENCE='ANALYTIQUE',
                VALE=51.40365,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASSEBC,
                NOM_PARA='MASSE',
               # FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                VALE=20.5226,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASSEAB,
                NOM_PARA='CDG_Y',
                #FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                VALE=0.905,   PRECISION=1.E-7)

TEST_TABLE( TABLE=MASSEBC,
                NOM_PARA='CDG_X',
               # FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                VALE=0.099197,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASSEBC,
                NOM_PARA='CDG_Y',
               # FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                VALE=0.099197,   PRECISION=1.E-5)

# TEST DES NOEUDS ORDONNES PAR DEFI_GROUP

EVOL=POST_RELEVE_T(    ACTION=_F( INTITULE = 'DEPL',
  GROUP_NO = 'AD',  RESULTAT = RESU1, NOM_CHAM = 'DEPL',
  TOUT_CMP = 'OUI',  OPERATION = 'EXTRACTION'))

TEST_TABLE( TABLE=EVOL,
                NOM_PARA='ABSC_CURV',
       FILTRE=(_F( NOM_PARA = 'NOEUD',
               VALE_K = 'NS1'),
               _F(NOM_PARA='NUME_ORDRE',VALE_I=1),),
               REFERENCE='NON_REGRESSION',
                VALE=2.46696E+00  )

TEST_TABLE( TABLE=EVOL,
                NOM_PARA='ABSC_CURV',
       FILTRE=(_F( NOM_PARA = 'NOEUD',
               VALE_K = 'NS18'),
               _F(NOM_PARA='NUME_ORDRE',VALE_I=1),),
               REFERENCE='NON_REGRESSION',
                VALE=2.06041E-01  )

TEST_TABLE( TABLE=EVOL,
                NOM_PARA='ABSC_CURV',
       FILTRE=(_F( NOM_PARA = 'NOEUD',
               VALE_K = 'N37'),
               _F(NOM_PARA='NUME_ORDRE',VALE_I=1),),
               REFERENCE='ANALYTIQUE',
                VALE=2.87909288E+00  )


#----------------------------------------------------------------------------
# Validation EFGE_ELGA pour les elements de tuyau apres STAT_NON_LINE :
#----------------------------------------------------------------------
RESU2=CALC_CHAMP(reuse=RESU2,RESULTAT=RESU2,CONTRAINTE=('EFGE_ELGA'))

TEST_RESU( RESU= _F( RESULTAT = RESU2, NOM_CHAM = 'EFGE_ELGA', INST = 1., PRECISION=1.E-9,
             MAILLE = 'M3', POINT = 3, NOM_CMP = 'MFY', VALE =  -1.70000E+04 , REFERENCE = 'NON_REGRESSION'))

RESU2B=CALC_CHAMP(RESULTAT= RESU2, CONTRAINTE=('EFGE_ELGA'), )
TEST_RESU( RESU= _F( RESULTAT = RESU2B, NOM_CHAM = 'EFGE_ELGA', INST = 1., PRECISION=1.E-9,
             MAILLE = 'M3', POINT = 3, NOM_CMP = 'MFY', VALE =  -1.70000E+04 , REFERENCE = 'NON_REGRESSION'))
#----------------------------------------------------------------------------
FIN()
