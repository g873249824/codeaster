# MODELISATION TUYAU

DEBUT(CODE=_F(NOM='SSLX102E',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M1=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

M1=DEFI_GROUP( reuse=M1,   MAILLAGE=M1,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MA=CREA_MAILLAGE(     MAILLAGE=M1,
       MODI_MAILLE=_F(  OPTION = 'SEG3_4', TOUT = 'OUI')
)

MA=DEFI_GROUP( reuse=MA,   INFO=2,
                MAILLAGE=MA,
           CREA_GROUP_NO=_F(  NOM = 'AD',
OPTION = 'NOEUD_ORDO',
GROUP_MA = 'AD',
#NOEUD_ORIG : N296
#NOEUD_EXTR : N297
GROUP_NO_ORIG = 'A',
GROUP_NO_EXTR = 'D')
                   )

MO=AFFE_MODELE(        MAILLAGE=MA, INFO=2,
     AFFE=_F(  TOUT = 'OUI',   PHENOMENE = 'MECANIQUE',
     MODELISATION = 'TUYAU_3M')
               )

CAREL=AFFE_CARA_ELEM(   MODELE=MO, INFO=2,
          POUTRE=_F( GROUP_MA = 'TOUT', SECTION = 'CERCLE',TUYAU_NCOU=1,
           CARA = (   'R',  'EP', ), VALE = (  0.10959,  0.00818, )),
ORIENTATION=_F( GROUP_NO = 'D', CARA = 'GENE_TUYAU', VALE = (0., 0., 1.,))
                              )

MAT=DEFI_MATERIAU(   ELAS=_F( E = 2.E11, NU = 0.3,  RHO = 7900.),
                   ECRO_LINE=_F( D_SIGM_EPSI = 12.5E9, SY = 200.E6)
                   )

CHMAT=AFFE_MATERIAU(   MAILLAGE=MA,
                       AFFE=_F( TOUT = 'OUI', MATER = MAT))

#FLEXION PLANE

CHAR1=AFFE_CHAR_MECA(  MODELE=MO,
     DDL_IMPO=_F( GROUP_NO = 'A', DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     FORCE_NODALE=_F( GROUP_NO = 'D', MZ = 17.E3)
    )

#RESU1 = MECA_STATIQUE (MODELE: MO CHAM_MATER : CHMAT CARA_ELEM : CAREL
#                        EXCIT: ( CHARGE: CHAR1 )  );

RAMPE=DEFI_FONCTION(  NOM_PARA='INST',
            PROL_GAUCHE='LINEAIRE', PROL_DROITE='LINEAIRE',
            VALE=(0.0,0.0,1.0,1.0,) )

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
                INTERVALLE=_F( JUSQU_A = 1.,   NOMBRE = 1) )

RESU1=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CAREL,
                             EXCIT=_F(
                CHARGE = CHAR1,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F( RELATION = 'ELAS', ),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6)
              )

RESU1=CALC_CHAMP(reuse=RESU1,CONTRAINTE=('EFGE_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=RESU1)


TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  RESULTAT=RESU1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=0.018540399,
                  VALE_REFE=0.0186137,
                  REFERENCE='NON_DEFINI',
                  PRECISION=1.E-2,),
          )

# FLEXION HORS PLAN

CHAR2=AFFE_CHAR_MECA(  MODELE=MO,
     DDL_IMPO=_F( GROUP_NO = 'A', DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     FORCE_NODALE=_F( GROUP_NO = 'D', MY = 17.E3)
    )

RESU2=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CAREL,
                             EXCIT=_F(
                CHARGE = CHAR2,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F( RELATION = 'ELAS', ),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6)
              )

RESU2=CALC_CHAMP(reuse=RESU2,CONTRAINTE=('EFGE_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=RESU2)


TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  RESULTAT=RESU2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=-0.015314626,
                  VALE_REFE=-0.015350000000000001,
                  REFERENCE='NON_DEFINI',
                  PRECISION=1.E-2,),
          )

MASSEAB=POST_ELEM(   MODELE=MO,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'AB')
     )

MASSEBC=POST_ELEM(   MODELE=MO,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'BC')
     )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.0000000000000001E-05,
           VALE_CALC=51.403651317,
           VALE_REFE=51.403649999999999,
           NOM_PARA='MASSE',
           TABLE=MASSEAB,)

TEST_TABLE(
           VALE_CALC=20.522600000000001,
           TOLE_MACHINE=1.0000000000000001E-05,
           NOM_PARA='MASSE',
           TABLE=MASSEBC,)

TEST_TABLE(
           VALE_CALC=0.90500000000000003,
           TOLE_MACHINE=9.9999999999999995E-08,
           NOM_PARA='CDG_Y',
           TABLE=MASSEAB,)

TEST_TABLE(
           VALE_CALC=0.099196999999999994,
           TOLE_MACHINE=1.0000000000000001E-05,
           NOM_PARA='CDG_X',
           TABLE=MASSEBC,)

TEST_TABLE(
           VALE_CALC=0.099196999999999994,
           TOLE_MACHINE=1.0000000000000001E-05,
           NOM_PARA='CDG_Y',
           TABLE=MASSEBC,)

# TEST DES NOEUDS ORDONNES PAR DEFI_GROUP

EVOL=POST_RELEVE_T(    ACTION=_F( INTITULE = 'DEPL',
  GROUP_NO = 'AD',  RESULTAT = RESU1, NOM_CHAM = 'DEPL',
  TOUT_CMP = 'OUI',  OPERATION = 'EXTRACTION'))

TEST_TABLE(
           VALE_CALC=2.4669599999999998,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='ABSC_CURV',
           TABLE=EVOL,
           FILTRE=(_F(NOM_PARA='NOEUD',
                      VALE_K='NS1',),
                   _F(NOM_PARA='NUME_ORDRE',
                      VALE_I=1,),
                   ),
           )

TEST_TABLE(
           VALE_CALC=0.206041,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='ABSC_CURV',
           TABLE=EVOL,
           FILTRE=(_F(NOM_PARA='NOEUD',
                      VALE_K='NS18',),
                   _F(NOM_PARA='NUME_ORDRE',
                      VALE_I=1,),
                   ),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=2.879038154,
           VALE_REFE=2.87909288,
           NOM_PARA='ABSC_CURV',
           TABLE=EVOL,
           FILTRE=(_F(NOM_PARA='NOEUD',
                      VALE_K='N37',),
                   _F(NOM_PARA='NUME_ORDRE',
                      VALE_I=1,),
                   ),
           )

#----------------------------------------------------------------------------
# Validation EFGE_ELGA pour les elements de tuyau apres STAT_NON_LINE :
#----------------------------------------------------------------------
RESU2=CALC_CHAMP(reuse=RESU2,RESULTAT=RESU2,CONTRAINTE=('EFGE_ELGA'))

TEST_RESU(RESU=_F(INST=1.0,
                  POINT=3,
                  RESULTAT=RESU2,
                  NOM_CHAM='EFGE_ELGA',
                  NOM_CMP='MFY',
                  VALE_CALC=-1.7000E4,
                  MAILLE='M3',),
          )

RESU2B=CALC_CHAMP(RESULTAT= RESU2, CONTRAINTE=('EFGE_ELGA'), )
TEST_RESU(RESU=_F(INST=1.0,
                  POINT=3,
                  RESULTAT=RESU2B,
                  NOM_CHAM='EFGE_ELGA',
                  NOM_CMP='MFY',
                  VALE_CALC=-1.7000E4,
                  MAILLE='M3',),
          )

#----------------------------------------------------------------------------
FIN()
