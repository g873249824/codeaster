          _F( RESULTAT = lmod[3], PARA = 'FREQ',
                CRITERE = 'RELATIF', PRECISION = 2.E-3,
                NUME_ORDRE = 4,  VALE = 1.9600E+02, REFERENCE='AUTRE_ASTER',),
          _F( RESULTAT = lmod[3], PARA = 'AMOR_REDUIT',
