                                      NUME_INST_FIN = 5),
                              NEWTON=_F( REAC_ITER = 1,
                                      MATRICE = 'TANGENTE'),
                         CONVERGENCE=_F(
                                      ITER_GLOB_MAXI = 30,
                                      RESI_GLOB_RELA = 1.E-6)
                        )

# premiere valeur de ETA_PILOTAGE de reference
