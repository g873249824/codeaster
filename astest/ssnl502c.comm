#

#

RESULT=CALC_CHAMP(reuse=RESULT,CONTRAINTE=('SIEF_ELNO'),RESULTAT=RESULT)


RESULT= CALC_CHAMP   (reuse=RESULT ,
                  FORCE='FORC_NODA',
                  RESULTAT   = RESULT,
                  TOUT_ORDRE = 'OUI',
                             );


ETA=RECU_FONCTION (   RESULTAT = RESULT,
                          NOM_PARA_RESU = 'ETA_PILOTAGE',
                          TOUT_ORDRE = 'OUI')

DEPZ=RECU_FONCTION (   RESULTAT = RESULT,
                          NOM_CMP = 'DZ',
                          NOM_CHAM = 'DEPL',
                          GROUP_NO = 'B',
                          TOUT_ORDRE = 'OUI')

DEPX=RECU_FONCTION (   RESULTAT = RESULT,
                          NOM_CMP = 'DX',
                          NOM_CHAM = 'DEPL',
                          GROUP_NO = 'B',
                          TOUT_ORDRE = 'OUI')

MDEPZ=CALC_FONCTION(COMB= _F(  FONCTION = DEPZ, COEF = -1.)  )

IMPR_FONCTION ( UNITE=29,FORMAT='XMGRACE',
              COURBE=(_F(FONC_Y=ETA, FONC_X = MDEPZ),
                      _F(FONC_Y=ETA, FONC_X = DEPX),),
              TITRE='Dx et Dz en fonction de P/Pcr'
               )


# A PARTIR DES FONCTIONS
#     - ETA  = F(INST,ETA_PILOTAGE),
#     - DEPX = F(INST,DX)
#     - DEPZ = F(INST,DZ)
# ON CREE LES FONCTIONS
#     - DX  = F(ETA),
#     - DZ  = F(ETA),

T_ETA  = CREA_TABLE(FONCTION=_F(FONCTION=ETA),)
T_DEPZ = CREA_TABLE(FONCTION=_F(FONCTION=DEPZ),)
T_DEPX = CREA_TABLE(FONCTION=_F(FONCTION=DEPX),)

TAB_COMB=CALC_TABLE(TABLE=T_ETA,
                    ACTION=(_F(OPERATION='COMB', TABLE=T_DEPZ, NOM_PARA='INST'),
                            _F(OPERATION='COMB', TABLE=T_DEPX, NOM_PARA='INST'),),)

IMPR_TABLE(TABLE=TAB_COMB)

DX_ETA = RECU_FONCTION(TABLE=TAB_COMB, PARA_X='ETA_PILO', PARA_Y='DX', INTERPOL='LIN',
                       FILTRE=_F(NOM_PARA='ETA_PILO', CRIT_COMP='NE', VALE=0.),
                       PROL_DROITE='CONSTANT', PROL_GAUCHE='CONSTANT',)
DZ_ETA = RECU_FONCTION(TABLE=TAB_COMB, PARA_X='ETA_PILO', PARA_Y='DZ',INTERPOL='LIN',
                       FILTRE=_F(NOM_PARA='ETA_PILO', CRIT_COMP='NE', VALE=0.),
                       PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',)

IMPR_FONCTION ( COURBE= _F(FONCTION=DX_ETA,),)

IMPR_FONCTION ( COURBE= _F(FONCTION=DZ_ETA,),)

