#---------------------------------------------------------------------
#- CALCUL DE LA STABILITE D'UNE POUTRE CONSOLE SOUMISE A UN EFFORT DE
#  COMPRESSION A UNE EXTREMITE.
#- VALIDATION DANS LE DOMAINE QUASI-STATIQUE NON-LINEAIRE GEOMETRIQUE
#  EN GRANDS DEPLACEMENTS ET GRANDES ROTATIONS EN PRESENCE
#  D'INSTABILITE (FLAMBEMENT D'EULER)
#- MODELISATION AVEC DES ELEMENTS 'POU_D_T_GD' (SEG2)
#---------------------------------------------------------------------
# person_in_charge: patrick.massin at edf.fr
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

#

MAIL=LIRE_MAILLAGE()


MAIL=DEFI_GROUP(reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(
                   _F( GROUP_MA = 'TOUT_ELT'),
                 ))

#
MO=AFFE_MODELE(   MAILLAGE=MAIL,
                        AFFE=(_F(GROUP_MA='TOUT_ELT',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'POU_D_T_GD'),)
                  )

#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAREL=AFFE_CARA_ELEM(   MODELE=MO,
                  ORIENTATION =_F( GROUP_MA = 'TOUT_ELT',
                     CARA='ANGL_VRIL', VALE=(0.,),),
                     POUTRE=_F( GROUP_MA = 'TOUT_ELT' ,
 SECTION = 'GENERALE',
                CARA = ( 'A',   'IY',    'IZ',    'AY',    'AZ',    'JX', ),
VALE = (  0.0003375, 5.69531E-10,  1.58203E-07, 1.0, 1.0,  2.19201E-09,  ),),
               INFO=2,
                        )

CAREL2=AFFE_CARA_ELEM(   MODELE=MO,
                  ORIENTATION =_F( GROUP_MA = 'TOUT_ELT',
                     CARA='ANGL_VRIL', VALE=(0.,),),
                     POUTRE=_F( GROUP_MA = 'TOUT_ELT' ,
                    SECTION = 'RECTANGLE',
                CARA = ( 'HY',   'HZ',    ),
                VALE = (  0.075, 0.0045  ),),
               INFO=2,
                        )
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT=DEFI_MATERIAU(   ELAS=_F( E = 2.E+11,
                             NU = 0.3 ,  ALPHA = 0.)
                     )

#
CHMAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                             AFFE=_F( TOUT = 'OUI', MATER = MAT)
                       )

#
#---------------------------------------------------------------------
#                        CHARGEMENTS
#---------------------------------------------------------------------
#
CON_LIM=AFFE_CHAR_MECA(     MODELE=MO,
                            DDL_IMPO=(_F( GROUP_NO = 'A',
                                      DX = 0., DZ = 0.,
                                      DRX = 0., DRY = 0., DRZ = 0.),
                           _F( GROUP_NO = 'TOUT_ELT',
                                      DY = 0.),
                               ),
                          )

#
CHARGE=AFFE_CHAR_MECA(         MODELE=MO,
                            FORCE_NODALE=_F( GROUP_NO = 'B',
                                         FZ = -1124.21)
                          )

PERTUR=AFFE_CHAR_MECA(         MODELE=MO,
                            FORCE_NODALE=_F( GROUP_NO = 'B',
#                                         FX = 1.12421)
                                         FX = 0.112421)
                          )

F_PERT2=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                          PROL_GAUCHE='EXCLU',
                                 VALE=( 0.,     0.,
                                        0.84,   0.,
                                        0.85,   1.,
                                        0.95,  1.,
                                        0.96,  0.,
                                        2.,     0.,
                                       )
                        )
F_PERT=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                          PROL_GAUCHE='EXCLU',
                                 VALE=( 0.,     0.,
                                        0.94,   0.,
                                        0.95,   1.,
                                        1.009,   1.,
                                        1.01,   0.,
                                        2.,     0.,
                                       )
                        )

F_CHARGE=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                          PROL_GAUCHE='EXCLU',
                                 VALE=( 0.,   0.,
                                        2.,   2.,
                                       )
                        )

#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------
#
LIS_INST=DEFI_LIST_REEL(       DEBUT=0.0,INTERVALLE=(
                           _F( JUSQU_A = 0.94, NOMBRE = 1),
                           _F( JUSQU_A = 0.95, NOMBRE = 1),
                           _F( JUSQU_A = 1.009, NOMBRE = 10),
                           _F( JUSQU_A = 1.01, NOMBRE = 1),
#
# DEPLACEMENT IMPOSE U=0.015 : ON COMPLETE LE DEPLACEMENT OBTENU
# PRECEDEMMENT QUI EST DE W =-1.01816E-02
                          _F( JUSQU_A = 2., NOMBRE = 198),
)
                           )

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = LIS_INST),
                        ECHEC=_F(ACTION='DECOUPE',
                                 SUBD_METHODE='MANUEL',
                                 SUBD_NIVEAU=4,
                                 SUBD_PAS=4,
                                 SUBD_PAS_MINI = 0.00000001),)

