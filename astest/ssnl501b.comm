#---------------------------------------------------------------------
#  - CALCUL QUASI-STATIQUE D'UNE POUTRE CONSTITUEE D'UN MATERIAU
#    ELASTIQUE PARFAITEMENT PLASTIQUE
#  - MODELISATION AVEC DES ELEMENTS 'DKT'(TRIA3)
#---------------------------------------------------------------------
#
DEBUT( CODE=_F( NOM = 'SSNL501B',NIV_PUB_WEB='INTERNET'))

MAIL_B=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'), )
MAIL_B=DEFI_GROUP(reuse =MAIL_B,
              MAILLAGE=MAIL_B,
              CREA_GROUP_NO=(_F(GROUP_MA='LFE',),),);
#
MOD_B=AFFE_MODELE(   MAILLAGE=MAIL_B,
                           AFFE=_F( TOUT = 'OUI',
                                 PHENOMENE = 'MECANIQUE',
                                 MODELISATION = 'DKT')
                     )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAR_EL_B=AFFE_CARA_ELEM(   MODELE=MOD_B,
                              COQUE=_F( GROUP_MA = 'TOUT_ELT',COQUE_NCOU=3,
                                     EPAIS = 0.025)
                           )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT_B=DEFI_MATERIAU(        ELAS=_F( E = 2.E+11,
                                    NU = 0.3),
                         ECRO_LINE=_F( D_SIGM_EPSI = 0.,
                                    SY = 2.35E+08)
                       )

#
CH_MAT_B=AFFE_MATERIAU(   MAILLAGE=MAIL_B,
                                AFFE=_F( TOUT = 'OUI', MATER = MAT_B)
                          )

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------
#
CHAR0=AFFE_CHAR_MECA(     MODELE=MOD_B,
                           DDL_IMPO=(
                          _F( GROUP_MA = 'LAB',
                                    DX = 0., DY = 0., DZ = 0.,
                                    DRX = 0., DRY = 0., DRZ = 0.),
                          _F( GROUP_MA = 'LFE',
                                    DRZ = 0.)),
                   LIAISON_OBLIQUE=_F( GROUP_NO = 'LFE',
                                    DX = 0.,
                                    ANGL_NAUT = (135., 0., 0.,))
                        )
#
PRES_LIM=AFFE_CHAR_MECA(         MODELE=MOD_B,
                              FORCE_COQUE=_F( TOUT = 'OUI',
                                           PRES = -261111.11)
                            )

#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------
#
LIS_INST=DEFI_LIST_REEL(        DEBUT=0.,INTERVALLE=(
                             _F( JUSQU_A = 10., NOMBRE = 20),
                             _F( JUSQU_A = 15., NOMBRE = 20),
                             _F( JUSQU_A = 25., NOMBRE = 30))
                           )

RESULTAT=STAT_NON_LINE(      MODELE=MOD_B,
                            CHAM_MATER=CH_MAT_B,
                             CARA_ELEM=CAR_EL_B,EXCIT=(
                                 _F( CHARGE = CHAR0),
                                       _F( CHARGE = PRES_LIM,
                                        TYPE_CHARGE = 'FIXE_PILO')),
                             COMP_INCR=_F( RELATION = 'VMIS_ISOT_LINE',
                                           ITER_INTE_MAXI = 30,
               ),
                             INCREMENT=_F( LIST_INST = LIS_INST),

                                       NEWTON=_F(
                                        MATRICE = 'TANGENTE',
                                        REAC_ITER = 1),

                                       CONVERGENCE=_F( RESI_GLOB_RELA = 1.E-6,
                                        ITER_GLOB_MAXI = 30),
                              PILOTAGE=_F( TYPE = 'DDL_IMPO',
                                        GROUP_NO = 'E',
                                        NOM_CMP = 'DX',
                                        COEF_MULT = -213.9708)
                          )


TABLE_DX=POST_RELEVE_T(
                          ACTION=_F( GROUP_NO = 'E',
                          INTITULE = 'TAB_DX',
                          RESULTAT = RESULTAT,
                          NOM_CHAM = 'DEPL',
                          NOM_CMP = 'DX',
                          TOUT_ORDRE = 'OUI',
                          OPERATION = 'EXTRACTION')
                          )

IMPR_TABLE(  TABLE=TABLE_DX)

TABLE_DY=POST_RELEVE_T( ACTION=_F( GROUP_NO = 'E',
                          INTITULE = 'TAB_DY',
                          RESULTAT = RESULTAT,
                          NOM_CHAM = 'DEPL',
                          NOM_CMP = 'DY',
                          TOUT_ORDRE = 'OUI',
                          OPERATION = 'EXTRACTION')
                          )

IMPR_TABLE(  TABLE=TABLE_DY)

DEPX=RECU_FONCTION (   RESULTAT = RESULTAT,
                          NOM_CMP = 'DX',
                          NOM_CHAM = 'DEPL',
                          GROUP_NO = 'E',
                          TOUT_ORDRE = 'OUI')

DEPY=RECU_FONCTION (   RESULTAT = RESULTAT,
                          NOM_CMP = 'DY',
                          NOM_CHAM = 'DEPL',
                          GROUP_NO = 'E',
                          TOUT_ORDRE = 'OUI')

ETA=RECU_FONCTION (   RESULTAT = RESULTAT,
                          NOM_PARA_RESU = 'ETA_PILOTAGE',
                          TOUT_ORDRE = 'OUI')

IMPR_FONCTION ( UNITE=25, UNITE_DIGR=26,FORMAT='AGRAF',
               COURBE=_F( FONCTION = DEPX),
    TITRE='Depx en fonction du temps'
              )

IMPR_FONCTION (UNITE=27, UNITE_DIGR=28, FORMAT='AGRAF',
               COURBE=_F( FONCTION = ETA),
    TITRE='Eta en fonction du temps'
              )

IMPR_FONCTION (UNITE=29, UNITE_DIGR=30,FORMAT='AGRAF',
               COURBE=_F( FONCTION = DEPY),
    TITRE='Depy en fonction du temps'
              )


DEPL=CREA_CHAMP(     RESULTAT=RESULTAT,
                      OPERATION='EXTR',
                      TYPE_CHAM='NOEU_DEPL_R',
                       NOM_CHAM='DEPL',
                     NUME_ORDRE=1
                   )

#
#---------------------------------------------------------------------
#                        VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(_F(NUME_ORDRE=30,
                   PARA='ETA_PILOTAGE',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESULTAT,
                   VALE_CALC=1.138588835,
                   VALE_REFE=1.0,
                   PRECISION=0.14999999999999999,),
                _F(NUME_ORDRE=40,
                   PARA='ETA_PILOTAGE',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESULTAT,
                   VALE_CALC=1.139900652,
                   VALE_REFE=1.0,
                   PRECISION=0.14999999999999999,),
                ),
          )

#
FIN()
#
#
