#############################################################
ENER_TOT=POST_ELEM(     RESULTAT=UP,

                        ENER_TOTALE=_F(  TOUT = 'OUI')
                      )


ENER_TO2=POST_ELEM(     RESULTAT=UP2,

                        ENER_TOTALE=_F(  TOUT = 'OUI')
                      )

TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=1.1196472921423,

           NOM_PARA='TOTALE',
           TABLE=ENER_TO2,
           FILTRE=_F(NOM_PARA='INST',
