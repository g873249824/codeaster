# FROTTEMENT AVEC X-FEM QUADRATIQUES
# ssnv508b.para = tps_job 60 mem_job 512Mo ncpus 1 liste_test S


DEBUT(CODE=_F(NOM='SSNV508B', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
              
# CETTE MODELISATION TESTE LA DEFORMEE DE L'INTERFACE SUR UN MAILLAGE
# QUADRATIQUE, LES MAILLES ETANT COUPEES PAR L'INTERFACE.

#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------
# precision
n = 1000.0;

# dimensions de la structure
lx = 2.0;
ly = 3.0;


PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=MODI_MAILLAGE(reuse =MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_2D=_F(GROUP_MA=('LPRESD','LPRESG','LPRESV',),),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(_F(NOEUD='N366',
                                      NOM='A',),
                                   _F(NOEUD='N91',
                                      NOM='B',),
                                   _F(NOEUD='N1',
                                      NOM='C',),
                                   _F(GROUP_MA='MTOT',
                                      NOM='SURF',),
                                    ),
                                  );

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MO=AFFE_MODELE(MAILLAGE=MAILLAG1,
               AFFE=_F(GROUP_MA=('MTOT','LPRESG','LPRESD','LPRESV',),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);


LN = FORMULE(VALE='Y',
             NOM_PARA=('X','Y',),);

FISS=DEFI_FISS_XFEM(MODELE=MO,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),
                    GROUP_MA_ENRI='MTOT',
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO,
                         FISSURE=FISS,
                         INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      INFO=1,);
#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
E = 10000000000.0;

nu = 0.2;

rho = 7800.0;


ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=rho,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MO,
                       AFFE=_F(GROUP_MA='MTOT',
                               MATER=ACIER,
                               ),);
#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

# --- CONDITIONS LIMITES DE DIRICHLET

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                  DDL_IMPO=(_F(GROUP_NO='A',
                               DX=0.0,),
                            _F(GROUP_NO='B',
                               DX=0.0,
                               DY=0.0,),
                            _F(GROUP_NO='C',
                               DY=0.0,),
                               ),
                  INFO=1,);

d0 = 1.E-05 ;
d2 = -2.5 * 1.E-06 ;

def deplv(x) :
   if x<0.  :  return d2*x*x + d0
   if x>0.  :  return d2*x*x + d0
   if x==0. :  return d2*x*x + d0

FCTDEPLV = FORMULE(VALE='deplv(X)',
                  NOM_PARA=('X','Y',),);
CH2=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                  FACE_IMPO=_F(GROUP_MA='LPRESV',
                               DY=FCTDEPLV),);

# --- CONDITIONS LIMITES DE NEUMANN

p2=100000.;
p1=2.*E*d2;

def pr(y) :
   if y<0.  :  return 0.
   if y>0.  :  return  p1*y + p2
   if y==0.  :  return  0.
FCTPRES = FORMULE(VALE='pr(Y)',
                  NOM_PARA=('X','Y',),);
FCTCIS = FORMULE(VALE='0.',
                  NOM_PARA=('X','Y',),);

CH3=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     PRES_REP=_F(GROUP_MA=('LPRESD','LPRESG',),
                                 PRES=FCTPRES,
                                 CISA_2D=FCTCIS),
                                  );

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH1,),
                           _F(CHARGE=CH2,),
                           _F(CHARGE=CH3,),),
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='MTOT',),
                    INCREMENT=_F(LIST_INST=L_INS,
                                 INST_FIN=1.0,),
                    NEWTON=_F(REAC_ITER=1,),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1E-6,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',),
                    INFO=1,);

#----------------------------------------------
#             IMPRESSION
#----------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE = MODELEK)

IMPR_RESU(FORMAT='RESULTAT',RESU=_F(MAILLAGE = MA_XFEM,))

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),),
                              );  

RES_X1=POST_CHAM_XFEM(MODELE_VISU = MOD_VISU,
                        RESULTAT  = UTOT1,);

IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_X1,));

#----------------------------------------------
#                  TESTS
#----------------------------------------------

DEP1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU',),
                                RESULTAT=RES_X1,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);

DEP2 = CALC_TABLE(TABLE = DEP1,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^M]',),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'DY',
                               CRIT_COMP = 'NE',
                               VALE      = 0,),),);

#DY=8.02*1.E-6 - 2.5*1.E-6*x*x
DYMIN=8.02*1.E-6 - 2.5*1.E-6
DYMAX=8.02*1.E-6

TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=DYMIN,
           CRITERE='ABSOLU',
           PRECISION=1.0E-6,
           REFERENCE='ANALYTIQUE',);


TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=DYMAX,
           CRITERE='ABSOLU',
           PRECISION=1.0E-6,
           REFERENCE='ANALYTIQUE',);

#DX=-p2*x/E
DXMIN=-p2/E
DXMAX=p2/E

TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=DXMAX,
           CRITERE='ABSOLU',
           PRECISION=1.0E-6,
           REFERENCE='ANALYTIQUE',);


TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=DXMIN,
           CRITERE='ABSOLU',
           PRECISION=1.0E-6,
           REFERENCE='ANALYTIQUE',);


TEST_RESU(RESU=(_F(  RESULTAT=RES_X1,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DX',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 1.8750833333334E-03,PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.12'),
                _F(  RESULTAT=RES_X1,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DY',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 4.2588145833333E-03,PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.12'),),)


IMPR_TABLE(TABLE=DEP2,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON',);

FIN();
