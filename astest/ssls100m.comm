ZERO=DEFI_CONSTANTE(    VALE=0.0 )

VAL_PRES=DEFI_CONSTANTE(     VALE=1. )

VAL_CHF3=DEFI_CONSTANTE(    VALE=-1. )

PRES=AFFE_CHAR_MECA_F(   MODELE=MODEL,
                    FORCE_COQUE=_F(  TOUT = 'OUI',   PRES = VAL_PRES)   )

CHF3=AFFE_CHAR_MECA_F(   MODELE=MODEL,
                    FORCE_COQUE=_F(  TOUT = 'OUI',   F3 = VAL_CHF3)   )

CHPE=AFFE_CHAR_MECA(   MODELE=MODEL,
                      PESANTEUR=_F(GRAVITE= 10.,
                                   DIRECTION=( 0., 0., -1.,), ),)

CARELEM=AFFE_CARA_ELEM(   MODELE=MODEL,
                               COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.1) )

DEP=MACRO_ELAS_MULT(MODELE=MODEL,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARELEM,
                    CHAR_MECA_GLOBAL=CINE,
                    CAS_CHARGE=(_F(NOM_CAS='CHARGE1',
                                   CHAR_MECA=PRES,),
                                _F(NOM_CAS='CHARGE2',
                                   CHAR_MECA=CHF3,),
                                _F(NOM_CAS='CHARGE3',
                                   CHAR_MECA=CHPE,),),);

