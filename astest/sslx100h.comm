CHME2=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA='GAUCHE',
                                 DX=0.0,DY=0.0,),
                              _F(GROUP_MA='POUTRE',
                                 DZ=0.0,DRX=0.0,DRY=0.0,),),
                    LIAISON_ELEM=_F(OPTION='2D_POU',
                                    GROUP_MA_1='DROIT',
                                    GROUP_NO_2='G',),
                    FORCE_NODALE=_F(GROUP_NO='EXTREM',
                                    FY=-1.0,),);
                                    

RES2=MECA_STATIQUE(MODELE=MO,
                  CHAM_MATER=CHMA,
                  CARA_ELEM  = CARELEM,
                  EXCIT=_F(CHARGE=CHME2,),);

IMPR_RESU(RESU=_F(RESULTAT=RES2,NOM_CHAM='DEPL'))

DEP2=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RES2,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

TEST_RESU(CHAM_NO=(
                   _F(CHAM_GD=DEP2,
                      NOEUD='N12',
                      NOM_CMP='DY',
                      VALE=-0.00546875,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=3.E-04,),
                   _F(CHAM_GD=DEP2,
                      NOEUD='N13',
