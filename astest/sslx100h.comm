DEBUT(CODE=_F(NOM='SSLX100H',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(GROUP_MA='MAIL2D',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='C_PLAN',),
                     _F(GROUP_MA='POUTRE',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='POU_D_E',),),);

MAT=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                          NU=0.,
                          ALPHA=0.0,),);

CHMA=AFFE_MATERIAU(MAILLAGE=MA,
                   AFFE=_F(TOUT='OUI',
                           MATER=MAT,),);

CARELEM=AFFE_CARA_ELEM(MODELE=MO,
                       POUTRE=_F(GROUP_MA='POUTRE',SECTION='RECTANGLE',CARA= ( 'HZ',  'HY', ),
                                     VALE=(1.0,1.0),),
                       ORIENTATION=_F(CARA='VECT_Y',VALE=(0.,1.,0.),GROUP_MA='POUTRE',))

# TRACTION
# --------
CHME=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA='GAUCHE',
                                 DX=0.0,DY=0.0,),
                              _F(GROUP_MA='POUTRE',
                                 DZ=0.0,DRX=0.0,DRY=0.0,),),
                    LIAISON_ELEM=_F(OPTION='2D_POU',
                                    GROUP_MA_1='DROIT',
                                    GROUP_NO_2='G',),
                    FORCE_NODALE=_F(GROUP_NO='EXTREM',
                                    FX=100000.0,),);
 
RES=MECA_STATIQUE(MODELE=MO,
                  CHAM_MATER=CHMA,
                  CARA_ELEM  = CARELEM,
                  EXCIT=_F(CHARGE=CHME,),);

DEP=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RES,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

TEST_RESU(CHAM_NO=(
                   _F(CHAM_GD=DEP,
                      NOEUD='N12',
                      NOM_CMP='DX',
                      VALE=1.25,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=1.E-08,),
                   _F(CHAM_GD=DEP,
                      NOEUD='N13',
