                      NOM_CMP='DY',
                      VALE=-0.08,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=1.E-04,),
                   _F(CHAM_GD=DEP2,
                      NOEUD='N66',
                      NOM_CMP='DY',
