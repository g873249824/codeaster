                   _F(CHAM_GD=DEP,
                      NOEUD='N67',
                      NOM_CMP='DX',
                      VALE=15.0,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=1.E-08,),),)

# FLEXION
