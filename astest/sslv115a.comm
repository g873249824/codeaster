DEBUT(CODE=_F(NOM='SSLV115A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM=('CALCULEL3_47'))

# <CALCULEL3_47> AVEC CALC_PRECONT, ON PASSE CONSCIEMMENT DE LA LDC 'SANS'
# A UNE AUTRE LOI DE COMPORTEMENT

MAILLAGE=LIRE_MAILLAGE(UNITE=20,);

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MAILLAGE = DEFI_GROUP( reuse=MAILLAGE, MAILLAGE=MAILLAGE, INFO=1,
                CREA_GROUP_MA= _F( NOM = 'GXXXXXX_TOUT',
                                   TOUT = 'OUI', ),
                CREA_GROUP_NO=(_F( NOM = 'GXXXXXX_GRNO1',
                                   OPTION = 'TUNNEL',
                                   GROUP_MA = 'GXXXXXX_VOILE',
                                   GROUP_MA_AXE = 'GXXXXXX_CABLE001',
#                                   NOEUD_ORIG = 'NC001001',
                                   RAYON = 3. ,
                                 ),
                               _F( NOM = 'GXXXXXX_GRNO2',
                                   OPTION = 'TUNNEL',
                                   MAILLE = 'toto1',
                                   MAILLE_AXE = ('SG001001','SG001002'),
                                   GROUP_NO_ORIG = 'GXXXXXX_NC1',
                                   LONGUEUR = 0.75 ,
                                   RAYON = 3. ,
                                 ),
                               _F( NOM = 'GXXXXXX_TOUT',
                                   GROUP_MA = 'GXXXXXX_TOUT',
                                 ),
                               ),
               ) ;

MODELE=AFFE_MODELE(AFFE=(_F(GROUP_MA='GXXXXXX_VOILE',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                         _F(GROUP_MA='GXXXXXX_CABLE001',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='BARRE',),),
                   MAILLAGE=MAILLAGE,);

CARAELEM=AFFE_CARA_ELEM(MODELE=MODELE,
                        BARRE=_F(SECTION='GENERALE',
                                 GROUP_MA='GXXXXXX_CABLE001',
                                 CARA=('A'),
                                 VALE=(0.00015),),);

BETON=DEFI_MATERIAU(BPEL_BETON=_F(),
                    THER=_F(LAMBDA=1.00,RHO_CP=1),
                    ELAS=_F(NU=0.0,
                            ALPHA=1.E-05,
                            RHO=3.0,
                            E=3.E10,),);

ACIER=DEFI_MATERIAU(BPEL_ACIER=_F(),
                    ELAS=_F(NU=0.0,
                            RHO=2.E4,
                            ALPHA=1.E-05,
                            E=2.1E11,),);

CHMATER=AFFE_MATERIAU(AFFE=(_F(MATER=BETON,
                               GROUP_MA='GXXXXXX_VOILE',),
                            _F(MATER=ACIER,
                               GROUP_MA='GXXXXXX_CABLE001',),),
                      MAILLAGE=MAILLAGE,);

CABLEPR=DEFI_CABLE_BP(CHAM_MATER=CHMATER,
                      MODELE=MODELE,
                      DEFI_CABLE=_F(NOEUD_ANCRAGE=('NC001001','NC001005'),
                                    GROUP_MA='GXXXXXX_CABLE001',),
                      TYPE_ANCRAGE=('ACTIF','ACTIF'),
                      GROUP_MA_BETON='GXXXXXX_VOILE',
                      TENSION_INIT=2.E5,
                      RECUL_ANCRAGE=0.0,
                      CARA_ELEM=CARAELEM,
                      INFO=1,);

CHARMECA=AFFE_CHAR_MECA(MODELE=MODELE,
                        DDL_IMPO=(_F(NOEUD='NB001001',
                                     DY=0.0,),
                                  _F(DX=0.0,
                                     GROUP_NO='GXXXXXX_VGAUCH',),
                                  _F(DZ=0.0,
                                     GROUP_NO='GXXXXXX_VINF',),),)

CHARMEZ=AFFE_CHAR_MECA(MODELE=MODELE,
                       PESANTEUR=_F(GRAVITE=10.0,
                                    DIRECTION=(0.0,0.0,-1.0),),);

CHARMEX=AFFE_CHAR_MECA(MODELE=MODELE,
                       PESANTEUR=_F(GRAVITE=10.0,
                                    DIRECTION=(-1.0,0.0,0.0),),);

LINST=DEFI_LIST_REEL(VALE=(0.0,1.0),);

EVOLNOLI=CALC_PRECONT( CHAM_MATER=CHMATER,
                       MODELE=MODELE,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                      RESI_GLOB_RELA=1.E-05,),
                       COMP_INCR=_F(TOUT='OUI',
                                    RELATION='ELAS',
                                    DEFORMATION='PETIT',),
                       INCREMENT=_F(LIST_INST=LINST,),
                       CARA_ELEM=CARAELEM,
                       SOLVEUR=_F(METHODE='MULT_FRONT',),
                       EXCIT=(
                              _F(CHARGE=CHARMECA,),
                              _F(CHARGE=CHARMEZ,
                                 ),),
                       CABLE_BP=( CABLEPR ,),
                       INFO=1,);

EVOLNOLI=CALC_CHAMP(reuse =EVOLNOLI,
                    FORCE='REAC_NODA',
                    RESULTAT=EVOLNOLI,);


EVOLNO2=CALC_PRECONT(CHAM_MATER=CHMATER,
                      MODELE=MODELE,
                      CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                     RESI_GLOB_RELA=1.E-05,),
                      COMP_INCR=_F(TOUT='OUI',
                                   RELATION='ELAS',
                                   DEFORMATION='PETIT',),
                      INCREMENT=_F(LIST_INST=LINST,),
                      CARA_ELEM=CARAELEM,
                      SOLVEUR=_F(METHODE='MULT_FRONT',),
                      EXCIT=(
                           _F(CHARGE=CHARMECA,),
                             _F(CHARGE=CHARMEX,
                                ),),
                       CABLE_BP=( CABLEPR ,),
                      INFO=1,);

EVOLNO2=CALC_CHAMP(reuse =EVOLNO2,
                   FORCE='REAC_NODA',
                   RESULTAT=EVOLNO2,);

FORC_DZ=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='FORCDZ',
                                GROUP_NO=('GXXXXXX_VOILE','GXXXXXX_CABLE001'),
                                RESULTANTE='DZ',
                                NUME_ORDRE=2,
                                RESULTAT=EVOLNOLI,
                                NOM_CHAM='REAC_NODA',),);


FORC2_DX=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='FORCDX',
                                 GROUP_NO=('GXXXXXX_VOILE','GXXXXXX_CABLE001'),
                                 RESULTANTE='DX',
                                 RESULTAT=EVOLNO2,
                                 NUME_ORDRE=2,
                                 NOM_CHAM='REAC_NODA',),);


TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-10,
           VALE_CALC=132.0,
           VALE_REFE=132.0,
           NOM_PARA='DZ',
           TABLE=FORC_DZ,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-10,
           VALE_CALC=132.0,
           VALE_REFE=132.0,
           NOM_PARA='DX',
           TABLE=FORC2_DX,)

# CONTRAINTES
TEST_RESU(RESU=(_F(NUME_ORDRE=2,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   RESULTAT=EVOLNOLI,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC=-1.66666667E+05,
                   VALE_REFE=-1.6666666666700001E5,
                   CRITERE='RELATIF',
                   PRECISION=1.E-3,
                   MAILLE='toto1',),
                _F(NUME_ORDRE=2,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   RESULTAT=EVOLNOLI,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='N',
                   VALE_CALC= 2.00000000E+05,
                   VALE_REFE=2.E5,
                   CRITERE='RELATIF',
                   PRECISION=1.E-3,
                   MAILLE='SG001001',),
                ),
          )

# DEFORMATION DANS LE BETON

EVOLNOLI=CALC_CHAMP(reuse=EVOLNOLI,
                   RESULTAT=EVOLNOLI,
                    DEFORMATION=('EPSI_ELNO'))

TEST_RESU(RESU=_F(NUME_ORDRE=2,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOLNOLI,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='NB001001',
                  NOM_CMP='EPXX',
                  VALE_CALC=-5.55555556E-06,
                  VALE_REFE=-5.5555555555999996E-06,
                  CRITERE='RELATIF',
                  PRECISION=1.0E-05,
                  MAILLE='toto1',),
          )

# -----------------------------------------------------------------------------------------------

FIN();
