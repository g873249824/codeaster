DEBUT(CODE=_F(NOM='SSLV115A',NIV_PUB_WEB='INTERNET'),);

MAILLAGE=LIRE_MAILLAGE(UNITE=20,);

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MODELE=AFFE_MODELE(AFFE=(_F(GROUP_MA='VOILE',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                         _F(GROUP_MA='CABLE001',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='BARRE',),),
                   MAILLAGE=MAILLAGE,);

CARAELEM=AFFE_CARA_ELEM(MODELE=MODELE,
                        BARRE=_F(SECTION='GENERALE',
                                 GROUP_MA='CABLE001',
                                 CARA=('A'),
                                 VALE=(0.00015),),);

BETON=DEFI_MATERIAU(BPEL_BETON=_F(),
                    ELAS=_F(NU=0.0,
                            RHO=3.0,
                            E=3.E10,),);

ACIER=DEFI_MATERIAU(BPEL_ACIER=_F(FROT_LINE=0.0,
                                  SY=1.770000000E9,
                                  FROT_COURB=0.0,),
                    ELAS=_F(NU=0.0,
                            RHO=2.E4,
                            E=2.10000000000E11,),);

CHMATER=AFFE_MATERIAU(AFFE=(_F(MATER=BETON,
                               GROUP_MA='VOILE',),
                            _F(MATER=ACIER,
                               GROUP_MA='CABLE001',),),
                      MAILLAGE=MAILLAGE,);

CABLEPR=DEFI_CABLE_BP(CHAM_MATER=CHMATER,
                      MODELE=MODELE,
                      DEFI_CABLE=_F(TYPE_ANCRAGE=('ACTIF','ACTIF'),
                                    NOEUD_ANCRAGE=('NC001001','NC001005'),
                                    GROUP_MA='CABLE001',),
                      GROUP_MA_BETON='VOILE',
                      TENSION_INIT=2.E5,
                      RECUL_ANCRAGE=0.0,
                      CARA_ELEM=CARAELEM,);

CHARMECA=AFFE_CHAR_MECA(DDL_IMPO=(_F(NOEUD='NB001001',
                                     DY=0.0,),
                                  _F(DX=0.0,
                                     GROUP_NO='VGAUCH',),
                                  _F(DZ=0.0,
                                     GROUP_NO='VINF',),),
                        INFO=2,
                        MODELE=MODELE,
                        RELA_CINE_BP=_F(RELA_CINE='OUI',
                                        SIGM_BPEL='OUI',
                                        CABLE_BP=CABLEPR,),);

CHARMEZ=AFFE_CHAR_MECA(MODELE=MODELE,
                       PESANTEUR=(10.0,0.0,0.0,-1.0),);

CHARMEX=AFFE_CHAR_MECA(MODELE=MODELE,
                       PESANTEUR=(10.0,-1.0,0.0,0.0),);

LINST=DEFI_LIST_REEL(VALE=(0.0,1.0),);

F=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,0.0,1.0,1.0),);

EVOLNOLI=STAT_NON_LINE(CHAM_MATER=CHMATER,
                       MODELE=MODELE,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                      RESI_GLOB_RELA=1.E-05,),
                       COMP_INCR=_F(TOUT='OUI',
                                    RELATION='ELAS',
                                    DEFORMATION='PETIT',),
                       INCREMENT=_F(LIST_INST=LINST,),
                       CARA_ELEM=CARAELEM,
                       SOLVEUR=_F(METHODE='MULT_FRONT',),
                       EXCIT=(_F(CHARGE=CHARMECA,),
                              _F(CHARGE=CHARMEZ,
                                 FONC_MULT=F,),),
                       INFO=2,);

EVOLNOLI=CALC_NO(reuse =EVOLNOLI,
                 CARA_ELEM=CARAELEM,
                 CHAM_MATER=CHMATER,
                 OPTION='REAC_NODA',
                 RESULTAT=EVOLNOLI,
                 EXCIT=(_F(CHARGE=CHARMECA,),
                        _F(CHARGE=CHARMEZ,
                           FONC_MULT=F,),),
                 MODELE=MODELE,);

EVOLNO2=STAT_NON_LINE(CHAM_MATER=CHMATER,
                      MODELE=MODELE,
                      CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                     RESI_GLOB_RELA=1.E-05,),
                      COMP_INCR=_F(TOUT='OUI',
                                   RELATION='ELAS',
                                   DEFORMATION='PETIT',),
                      INCREMENT=_F(LIST_INST=LINST,),
                      CARA_ELEM=CARAELEM,
                      SOLVEUR=_F(METHODE='MULT_FRONT',),
                      EXCIT=(_F(CHARGE=CHARMECA,),
                             _F(CHARGE=CHARMEX,
                                FONC_MULT=F,),),
                      INFO=2,);

EVOLNO2=CALC_NO(reuse =EVOLNO2,
                CARA_ELEM=CARAELEM,
                CHAM_MATER=CHMATER,
                OPTION='REAC_NODA',
                RESULTAT=EVOLNO2,
                EXCIT=(_F(CHARGE=CHARMECA,),
                       _F(CHARGE=CHARMEX,
                          FONC_MULT=F,),),
                MODELE=MODELE,);

FORC_DZ=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='FORCDZ',
                                GROUP_NO=('VOILE','CABLE001'),
                                NOM_CMP='DZ',
                                RESULTAT=EVOLNOLI,
                                NOM_CHAM='REAC_NODA',),);

FORC2_DZ=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='FORCDZ',
                                 GROUP_NO=('VOILE','CABLE001'),
                                 RESULTANTE='DZ',
                                 RESULTAT=EVOLNOLI,
                                 NOM_CHAM='REAC_NODA',),);

FORC2_DX=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='FORCDX',
                                 GROUP_NO=('VOILE','CABLE001'),
                                 RESULTANTE='DX',
                                 RESULTAT=EVOLNO2,
                                 NOM_CHAM='REAC_NODA',),);

DEPL_DZ=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='DEPLDZ',
                                GROUP_NO=('VOILE','CABLE001'),
                                NOM_CMP='DZ',
                                RESULTAT=EVOLNOLI,
                                NOM_CHAM='DEPL',),);

IMPR_TABLE(TABLE=FORC2_DZ,);

IMPR_TABLE(TABLE=FORC2_DX,);

TEST_TABLE(TABLE=FORC2_DZ,
           NOM_PARA='DZ',
           VALE=132.0,);

TEST_TABLE(TABLE=FORC2_DX,
           NOM_PARA='DX',
           VALE=132.0,);

FIN();
