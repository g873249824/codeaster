#---------------------------------------------------------------------
#  - CALCUL DE MOYENNE DE LA DEFORMATION
#    JAUGE VIRTUELLE
#  - RESULTATS ISSUS DE DYNA_VIBRA
#---------------------------------------------------------------------
#
import numpy as NP

DEBUT( CODE=_F( NOM = 'SDLV131C',NIV_PUB_WEB='INTERNET'),
       DEBUG=_F(SDVERI='OUI'),
       IMPR_MACRO='NON',
       PAR_LOT='OUI',
       ERREUR=_F(ERREUR_F='EXCEPTION',),
     )

force = 1.E3
young = 2.1E11
poisson = 0.3
rho = 7800.
longueur = 1.
section = 0.3*0.2

POUTRE=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'), );

MODEL=AFFE_MODELE(MAILLAGE=POUTRE,
                  AFFE=_F( GROUP_MA = 'TOUT_ELT',
                           PHENOMENE = 'MECANIQUE',
                           MODELISATION = '3D',),);

POUTRE=DEFI_GROUP(reuse =POUTRE,
                MAILLAGE=POUTRE,
                CREA_GROUP_NO=(
                             _F(GROUP_MA='AY',NOM='AY',),
                             _F(GROUP_MA='AZ',NOM='AZ',),
                             _F(GROUP_MA='SURF2',NOM='SURF2',),
                              ),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
ACIER=DEFI_MATERIAU(ELAS=_F( E = young,
                             NU = poisson,
                             RHO = rho),);

MAT=AFFE_MATERIAU( MAILLAGE=POUTRE,
                   AFFE=_F( GROUP_MA = 'TOUT_ELT', MATER = ACIER)
                 );

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------
#
CHAR0=AFFE_CHAR_MECA( MODELE=MODEL,
                      DDL_IMPO=(  # traction - compression
                               _F( GROUP_NO = 'AY',DY=0.,),
                               _F( GROUP_NO = 'AZ',DZ=0.,),
                        ),
                      FACE_IMPO=( # encastrement
                               _F( GROUP_MA = 'SURF1',DX=0.,),
                        ),
                      LIAISON_UNIF=( # pour la force de traction
                               _F( GROUP_NO = 'SURF2',DDL=('DX',)),
                        ),
                      );
#
#---------------------------------------------------------------------
#                     RESOLUTION
#---------------------------------------------------------------------
#
M_EL_RIG=CALC_MATR_ELEM( MODELE=MODEL,
                         CHAM_MATER=MAT,
                         CHARGE=CHAR0,
                         OPTION='RIGI_MECA'
                           );

M_EL_MAS=CALC_MATR_ELEM( MODELE=MODEL,
                         CHAM_MATER=MAT,
                         CHARGE=CHAR0,
                         OPTION='MASS_MECA'
                           );

NU=NUME_DDL( MATR_RIGI=M_EL_RIG)
M_AS_RIG=ASSE_MATRICE( MATR_ELEM=M_EL_RIG,   NUME_DDL=NU)
M_AS_MAS=ASSE_MATRICE( MATR_ELEM=M_EL_MAS,   NUME_DDL=NU)

rota = 90.
rota_rad = rota*NP.pi/180.
cosinus = NP.cos(rota_rad)
sinus = NP.sin(rota_rad)

CHF=AFFE_CHAR_MECA(MODELE=MODEL,
                   FORCE_FACE=(_F(GROUP_MA='SURF2',FX=force),
                                  ),)

dynatrans = 1
if dynatrans:
  omega0_2 = 3*young/(rho*longueur*longueur)
  tfin = 1.
  nb_inst = 10
  LINST=DEFI_LIST_REEL(DEBUT=0.,
                        INTERVALLE=_F( JUSQU_A = tfin, NOMBRE = nb_inst))

  RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0,0,tfin,tfin),
                          PROL_DROITE='LINEAIRE',);

  DYNATRAN=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='PHYS',
                           MATR_MASS=M_AS_MAS, MATR_RIGI=M_AS_RIG,
                           INCREMENT=_F(LIST_INST=LINST,),
                           SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',),
                           MODELE = MODEL, CHAM_MATER = MAT, # modele obligatoire a cause de charge
                           EXCIT=(_F(FONC_MULT=RAMPE,CHARGE=CHF,),),
                       )

  CALC_CHAMP(reuse=DYNATRAN,RESULTAT=DYNATRAN,DEFORMATION=('EPSI_ELGA','EPSI_ELNO'))


  DYNATRAN=CALC_CHAMP( reuse=DYNATRAN, RESULTAT=DYNATRAN,
                    DEFORMATION='EPSI_NOEU'
                     )

projection = 0
if projection:
# MESURE
    MAILMESU=LIRE_MAILLAGE(UNITE=22)

    MODLMESU=AFFE_MODELE( MAILLAGE=MAILMESU,
                          AFFE=(
                              _F( MAILLE = ('P3','P4','P5','P6','L1','L2','L3',),
                                  PHENOMENE = 'MECANIQUE',
                                  MODELISATION = 'DIS_T'),
                                ),)

    OBSJAU = OBSERVATION(
                   RESULTAT = DYNATRAN,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODLMESU,
                   PROJECTION = 'OUI',
                   TOUT_ORDRE = 'OUI',
                   NOM_CHAM = 'EPSI_NOEU',
                   EPSI_MOYENNE = (  # pour JAUGE : une occ par jauge
                              _F( GROUP_MA='SURF3',SEUIL_VARI=(0.1,),MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF4',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF5',SEUIL_VARI=0.1,MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF6',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              ),
                   MODI_REPERE = _F(
                                    TYPE_CHAM = 'TENS_3D',
                                    GROUP_MA=('P4','P6',),  # sur le modele mesure (rotation 90 degre)
                                    REPERE = 'DIR_JAUGE',  VECT_X = (cosinus,sinus,0.),  VECT_Y = (-sinus,cosinus,0.),
                              ),
                   FILTRE =(
                              _F( GROUP_NO = 'P3',
                                NOM_CHAM = 'EPSI_NOEU',
                                DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P4',
                                NOM_CHAM = 'EPSI_NOEU',
                                DDL_ACTIF = ('EPXX','EPYY',),),
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P6',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPYY',),),
                              ),
                  );

    OBSMXT = OBSERVATION(
                   RESULTAT = DYNATRAN,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODLMESU,
                   PROJECTION = 'OUI',
                   TOUT_ORDRE = 'OUI',
                   NOM_CHAM = ('EPSI_NOEU','DEPL'),
                   FILTRE =(
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = ('EPSI_NOEU',),
                                  DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = ('DEPL'),
                                DDL_ACTIF = ('DX','DZ',),),
                              ),
                  );

else:
    OBSJAU = OBSERVATION(
                   RESULTAT = DYNATRAN,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODEL,
                   PROJECTION = 'NON',
                   TOUT_ORDRE = 'OUI',
                   MATR_RIGI=M_AS_RIG,
                   MATR_MASS=M_AS_MAS,
                   NOM_CHAM = 'EPSI_NOEU',
                   EPSI_MOYENNE = (  # pour JAUGE : une occ par jauge
                              _F(
                                 GROUP_MA=('SURF3',),
                                 SEUIL_VARI=(0.1,),
                                 MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),
                                 ),
                              _F( GROUP_MA='SURF4',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF5',SEUIL_VARI=0.1,MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF6',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              ),
                   MODI_REPERE = (
                               _F( TYPE_CHAM = 'TENS_3D',
                                    GROUP_MA=('SURF4','SURF6',), # (rotation 90 degre)
                                    REPERE = 'DIR_JAUGE',  VECT_X = (cosinus,sinus,0.),  VECT_Y = (-sinus,cosinus,0.),),
                              ),
                   FILTRE = (
                              _F( GROUP_NO = 'P3',
                                  NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPZZ',),
                                 ),
                              _F( GROUP_NO = 'P4',
                                  NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPYY',),
                                 ),
                              _F( GROUP_NO = 'P5',
                                  NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPZZ',),
                                 ),
                              _F( GROUP_NO = 'P6',
                                  NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPYY',),
                                 ),
                             ),
                  );

    OBSMXT = OBSERVATION(
                   RESULTAT = DYNATRAN,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODEL,
                   PROJECTION = 'NON',
                   TOUT_ORDRE = 'OUI',
                   MATR_RIGI=M_AS_RIG,
                   MATR_MASS=M_AS_MAS,
                   NOM_CHAM = ('EPSI_NOEU','DEPL'),
                   FILTRE =(
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = ('EPSI_NOEU',),
                                  DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = ('DEPL'),
                                DDL_ACTIF = ('DX','DZ',),),
                              ),
                  );

#---------------------------------------------------------------------
#                     VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#

if dynatrans:
  omega0=NP.sqrt(omega0_2)
  deltal = 3.*force*(tfin-NP.sin(omega0*tfin)/omega0)/(rho*longueur*omega0_2)
  exx = deltal/longueur
  eyy = -poisson*exx
  ezz = -poisson*exx

  TEST_RESU(RESU=(_F(GROUP_NO='P5',
                     INST=tfin,
                     REFERENCE='ANALYTIQUE',
                     RESULTAT=OBSJAU,
                     NOM_CHAM='EPSI_NOEU',
                     NOM_CMP='EPXX',
                     VALE_CALC= 4.76190959E-09,
                     VALE_REFE=exx,
                     CRITERE='RELATIF',
                     PRECISION=0.02,),
                  _F(GROUP_NO='P5',
                     INST=tfin,
                     REFERENCE='ANALYTIQUE',
                     RESULTAT=OBSJAU,
                     NOM_CHAM='EPSI_NOEU',
                     NOM_CMP='EPZZ',
                     VALE_CALC=-1.42857276E-09,
                     VALE_REFE=ezz,
                     CRITERE='RELATIF',
                     PRECISION=0.02,),
# rotation 90 degres : EPXX <-> EPYY
                  _F(GROUP_NO='P6',
                     INST=tfin,
                     REFERENCE='ANALYTIQUE',
                     RESULTAT=OBSJAU,
                     NOM_CHAM='EPSI_NOEU',
                     NOM_CMP='EPYY',
                     VALE_CALC= 4.76190906E-09,
                     VALE_REFE=exx,
                     CRITERE='RELATIF',
                     PRECISION=0.02,),
                  _F(GROUP_NO='P6',
                     INST=tfin,
                     REFERENCE='ANALYTIQUE',
                     RESULTAT=OBSJAU,
                     NOM_CHAM='EPSI_NOEU',
                     NOM_CMP='EPXX',
                     VALE_CALC=-1.42857276E-09,
                     VALE_REFE=eyy,
                     CRITERE='RELATIF',
                     PRECISION=0.02,),
# observation mixte
                  _F(GROUP_NO='P5',
                     INST=tfin,
                     REFERENCE='ANALYTIQUE',
                     RESULTAT=OBSMXT,
                     NOM_CHAM='EPSI_NOEU',
                     NOM_CMP='EPXX',
                     VALE_CALC= 4.76190907E-09,
                     VALE_REFE=exx,
                     CRITERE='RELATIF',
                     PRECISION=0.02,),
                  _F(GROUP_NO='P5',
                     INST=tfin,
                     REFERENCE='ANALYTIQUE',
                     RESULTAT=OBSMXT,
                     NOM_CHAM='DEPL',
                     NOM_CMP='DX',
                     VALE_CALC= 4.52383302E-09,
                     VALE_REFE=deltal,
                     CRITERE='RELATIF',
                     PRECISION=0.059999999999999998,),
                  ),
            )

FIN();
