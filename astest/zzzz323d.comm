# MODELISATION 3D

MAILLAGE=LIRE_MAILLAGE(    UNITE=20 )

#
MODELE=AFFE_MODELE(
                   MAILLAGE=MAILLAGE,
                   AFFE=(
                         _F(  TOUT='OUI',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = '3D',
                           ),
                        )
                  )

#
CARAELEM=AFFE_CARA_ELEM(
                        MODELE=MODELE,
                        MASSIF=(_F(MAILLE='HEX1',
                                   ANGL_REP=(45.0,45.0,90.0)
                                  ),
                                _F(MAILLE='HEX2',
                                   ORIG_AXE=(100.0,0.5,0.5),
                                   ANGL_AXE=(0.0,-45.0),
                                  )
                               ),
                       )

# IMPRESSION AU FORMAT ASTER
