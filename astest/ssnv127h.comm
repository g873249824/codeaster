DEBUT(CODE=_F( NOM = 'SSNV127H', NIV_PUB_WEB='INTERNET'));

#PRE_GIBI();

#_____________________________________________________________________ 
#                                                       
#                    LECTURE DE SSNV127H.MGIB           
#_____________________________________________________________________ 
#                                                       

SSNV127H=LIRE_MAILLAGE(FORMAT='MED',);

SSNV127H=DEFI_GROUP(reuse =SSNV127H,
                    MAILLAGE=SSNV127H,
                    CREA_GROUP_NO=(_F(GROUP_MA='CC4',),
                                   _F(GROUP_MA='AD1',),
                                   _F(GROUP_MA='AD2',),
                                   _F(GROUP_MA='AD3',),
                                   _F(GROUP_MA='BD3',),
                                   _F(GROUP_MA='AD4',),
                                   _F(GROUP_MA='BD4',),
                                   _F(GROUP_MA='BCC2',
                                      OPTION='NOEUD_ORDO',
                                      NOM='RELEVE',
                                      GROUP_NO_ORIG='A1',
                                      GROUP_NO_EXTR='B2',),),);

# POUR ELIMINER LES NOEUDS COMMUNS AUX SURFACES DE CONTACT
SSNV127H=DEFI_GROUP(reuse=SSNV127H,
                    MAILLAGE=SSNV127H,
                    CREA_GROUP_NO=(_F(GROUP_MA='BCC1',),
                                   _F(GROUP_MA='BCC2',),
                                   _F(INTERSEC=('BCC1','BCC2',),
                                      NOM='BCC_SGN'),),)

#_____________________________________________________________________ 
#
#                    DEFINITION DU MODELE               
#_____________________________________________________________________ 
#

MO=AFFE_MODELE(MAILLAGE=SSNV127H,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

SSNV127H=MODI_MAILLAGE(reuse =SSNV127H,
                       MAILLAGE=SSNV127H,
                       ORIE_PEAU_2D=_F(GROUP_MA='BCC1',),
                      );

SSNV127H=MODI_MAILLAGE(reuse =SSNV127H,
                       MAILLAGE=SSNV127H,
                       ORIE_PEAU_2D=_F(GROUP_MA='BCC2',),
                      );
#_____________________________________________________________________ 
#                                                       
#                    DEFINITION DES MATERIAUX           
#_____________________________________________________________________ 
#                                                       

MAT1=DEFI_MATERIAU(ELAS=_F(E=2.10000000000E11,
                           NU=0.29999999999999999,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=SSNV127H,
                    AFFE=_F(GROUP_MA='SURFF',
                            MATER=MAT1,),);
#_____________________________________________________________________ 
#                                                       
#              AFFECTATION DES CONDITIONS AUX LIMITES            
#                       ET DU CHARGEMENT                
#_____________________________________________________________________ 
#                                                       

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='CC4',
                                 DX=0.0,
                                 DY=0.0,),
                              _F(GROUP_NO='AD1',
                                 DX=0.0,),
                              _F(GROUP_NO='AD2',
                                 DX=0.0,),
                              _F(GROUP_NO='AD3',
                                 DX=0.0,),
                              _F(GROUP_NO='AD4',
                                 DX=0.0,),
                              _F(GROUP_NO='BD3',
                                 DX=0.0,),
                              _F(GROUP_NO='BD4',
                                 DX=0.0,),),
                    FORCE_NODALE=_F(GROUP_NO='O2',
                                    FY=-9.37500E5,),);


CHA2 = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    FROTTEMENT     = 'COULOMB',
                    REAC_GEOM      = 'CONTROLE',
                    NB_ITER_GEOM   = 3,
                    ITER_CONT_MAXI = 25, 
                    ITER_FROT_MAXI = 6,
                    ALGO_RESO_FROT = 'POINT_FIXE',
                    ZONE =(
                           _F(
                             GROUP_MA_ESCL = 'BCC2',
                             GROUP_MA_MAIT = 'BCC1',
                             SANS_GROUP_NO = 'BCC_SGN',
                             COULOMB       = 0.4,
                             ALGO_CONT     = 'STANDARD',
                             COEF_CONT     = 10.,
                             ALGO_FROT     = 'STANDARD',
                             COEF_FROT     = 10.,                             
                             ),                            
                          ),
                   );
                               
#_____________________________________________________________________ 
#                                                       
#               DEFINITION DES FONCTIONS TEMPORELLES    
#_____________________________________________________________________ 
#                                                       

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0,),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);
                                    
#_____________________________________________________________________ 
#                                                       
#                            RESOLUTION                 
#_____________________________________________________________________ 
#                                                       

U=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE=CHA1,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  = CHA2,       
                COMP_ELAS=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20),
                );

U=CALC_ELEM(reuse =U,
            OPTION=('SIGM_ELNO'),
            RESULTAT=U,);

TPRESS=POST_RELEVE_T(ACTION=_F(INTITULE='PRESS',
                              GROUP_NO='RELEVE',
                              RESULTAT=U,
                              INST=1.,
                              NOM_CHAM='SIGM_ELNO',
                              NOM_CMP='SIXX',
                              REPERE='POLAIRE',
                              OPERATION='EXTRACTION',),);


TCONT=POST_RELEVE_T(ACTION=_F(INTITULE='CONT',
                              GROUP_NO='RELEVE',
                              RESULTAT=U,
                              INST=1.,
                              NOM_CHAM='VALE_CONT',
                              TOUT_CMP='OUI',
                              OPERATION='EXTRACTION',),);
                              
IMPR_TABLE(TABLE=TPRESS)
IMPR_TABLE(TABLE=TCONT)

#_____________________________________________________________________ 
#                                                       
#                  TRACER DE COURBES INTERACTIF
#_____________________________________________________________________ 
#                                                       

COS    = FORMULE( NOM_PARA=('COOR_X','COOR_Y'),
                  VALE='-180.*atan2(COOR_X,COOR_Y)/pi')

TPRESS = CALC_TABLE(TABLE=TPRESS,reuse=TPRESS,
                    ACTION=_F(OPERATION='OPER',
                              FORMULE=COS,
                              NOM_PARA='COS'),)

TPRES  = CALC_TABLE(TABLE=TPRESS,
                    ACTION=_F(OPERATION='FILTRE',
                              NOM_PARA='COS',
                              CRIT_COMP='LE',
                              VALE=180.),)

IMPR_TABLE(TABLE=TPRESS)

CONT  = FORMULE(NOM_PARA=('CONT'),
                VALE='-1.E6*CONT')

TCONT = CALC_TABLE(TABLE=TCONT,reuse=TCONT,
                   ACTION=(_F(OPERATION='OPER',
                              FORMULE=CONT,
                              NOM_PARA='INDICATEUR'),
                           _F(OPERATION='OPER',
                              FORMULE=COS,
                              NOM_PARA='COS')))

TCON  = CALC_TABLE(TABLE=TCONT,
                   ACTION=_F(OPERATION='FILTRE',
                             NOM_PARA='COS',
                              CRIT_COMP='LE',
                             VALE=180.),)

IMPR_TABLE(TABLE=TCONT)

IMPR_TABLE(
   UNITE=30,
   FORMAT='XMGRACE',
   TABLE=TPRES,
   NOM_PARA=('COS','SIXX'),
   LEGENDE='Pression',
   LEGENDE_X = 'ABS',
   LEGENDE_Y = 'PRESSION',
   BORNE_X   = (0.,90.),
   BORNE_Y   = (2.E6,-2.E7),
   GRILLE_X  = 10.,
   GRILLE_Y  = 2.E6,
   COULEUR=6,
)

