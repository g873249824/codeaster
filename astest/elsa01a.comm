         REFERENCE = 'NON_REGRESSION'),
 _F( FONCTION = D_XVAN, PRECISION = 0.1E-2,
         VALE_PARA = 0.86,  VALE_REFE = -2.6988226449943E-02,
         REFERENCE = 'NON_REGRESSION'),
 _F( FONCTION = D_XVAN, PRECISION = 0.1E-2,
         VALE_PARA = 1.06,  VALE_REFE = 8.1639914363308E-02,
         REFERENCE = 'NON_REGRESSION'))
            )

FIN()
