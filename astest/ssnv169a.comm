# MODELISATION 3D : COUPLAGE FLUAGE PROPRE UMLV ET ENDO_ISTO_BETON
# ----------------------------------------------------------------

DEBUT(CODE=_F(NOM='SSNV169A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED', )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=(_F( GROUP_MA = 'S_INF'),
                             _F( GROUP_MA = 'S_SUP'),
                             _F( GROUP_MA = 'S_GCH'),
                             _F( GROUP_MA = 'S_DRT'),
                             _F( GROUP_MA = 'S_AVT'),
                             _F( GROUP_MA = 'S_ARR'),
            ),)

MO=AFFE_MODELE( MAILLAGE=M,
                AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = '3D')
              )

FONC1=DEFI_FONCTION( NOM_PARA='SECH',
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 1.0, 1.0,)
                   )

E=DEFI_FONCTION( NOM_PARA='TEMP',
                 PROL_DROITE='LINEAIRE',
                 PROL_GAUCHE='LINEAIRE',
                 VALE=(0.0, 31000., 1.0, 31000.,)
               )

NU=DEFI_FONCTION( NOM_PARA='TEMP',
                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE',
                  VALE=(0.0,0.2,1.0,0.2,)
                ),

ALPHA=DEFI_CONSTANTE(VALE=0.);

BETON=DEFI_MATERIAU( ELAS_FO =_F( E = E,
                                  NU = NU ,
                                  ALPHA = ALPHA ,TEMP_DEF_ALPHA=0.,
                                  FONC_DESORP = FONC1
                                  ),
                     BETON_ECRO_LINE=_F( SYT = 3.0,
                                    D_SIGM_EPSI = -6000.,
                                    SYC = 30.),
                     BETON_UMLV_FP =_F( K_RS   = 1.2E+5,
                                        ETA_RS = 2.21E+10,
                                        K_IS   = 6.22E+4,
                                        ETA_IS = 4.16E+10,
                                        K_RD   = 3.86E+4,
                                        ETA_RD = 6.19E+10,
                                        ETA_ID = 1.64e+12,
                                      ), )

SECH=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=M,
                        AFFE=_F( TOUT = 'OUI',
                              NOM_CMP = 'TEMP',
                              VALE = 1.)
                       )

TEMP=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=M,
                         AFFE=_F(
                               TOUT = 'OUI',
                               NOM_CMP = 'TEMP',
                               VALE = 20.)
                       )

CM=AFFE_MATERIAU(  MAILLAGE=M,
                   AFFE=_F( TOUT = 'OUI',MATER = BETON,),
                   AFFE_VARC=(_F(TOUT='OUI',CHAM_GD=SECH,NOM_VARC='SECH',VALE_REF=1.),
                             _F(TOUT='OUI',CHAM_GD=TEMP,NOM_VARC='TEMP',VALE_REF=20.),),);


# rap_pic : pourcentage de la force au pic
rap_pic=.6

FORCE=DEFI_FONCTION( NOM_PARA='INST',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 1., 3.*rap_pic/4., 31536010., 3.*rap_pic/4)
                   )

CH10=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'S_INF',
                                  DZ       = 0.
                               )
                    )
CH11=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'S_GCH',
                                  DX       = 0.
                                )
                   )

CH12=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'S_AVT',
                                 DY       = 0.
                               ),
                  )

CH20=AFFE_CHAR_MECA_F( MODELE=MO,
                      LIAISON_UNIF=_F( GROUP_NO = 'S_DRT',
                                       DDL      = 'DX'
                                     ),
                     )
CH21=AFFE_CHAR_MECA_F( MODELE=MO,
                      LIAISON_UNIF=_F( GROUP_NO = 'S_ARR',
                                       DDL      = 'DY'
                                     )
                     )
CH22=AFFE_CHAR_MECA_F( MODELE=MO,
                      FORCE_NODALE=_F( GROUP_NO = 'S_SUP',
                                   FZ = FORCE)
                    )

heure=3600.
jour=24.*heure
annee=365.25*jour
mois=annee/12.
L_INST=DEFI_LIST_REEL(  DEBUT=-1.0,INTERVALLE=(
             _F( JUSQU_A = 0.0,       NOMBRE = 1),
             _F( JUSQU_A = 1.0,       NOMBRE = 1),
             _F( JUSQU_A = annee, NOMBRE = 120),)
           )


U=STAT_NON_LINE( MODELE=MO,
                 CHAM_MATER=CM,EXCIT=( _F(CHARGE = CH10),
                             _F(CHARGE = CH11),
                   _F(CHARGE = CH12),
                   _F(CHARGE = CH20),
                   _F(CHARGE = CH21),
                   _F(CHARGE = CH22),
                 ),
       COMP_INCR=_F(
                 RELATION = 'KIT_DDI',
                 RELATION_KIT=('BETON_UMLV_FP','ENDO_ISOT_BETON'),),
                 INCREMENT=_F(LIST_INST = L_INST,
                              NUME_INST_FIN = 104,
                              ),
                 NEWTON=_F(PREDICTION = 'TANGENTE',
                 MATRICE = 'TANGENTE',
                           REAC_ITER = 1),
                 CONVERGENCE=_F(RESI_GLOB_MAXI=1.E-6)
                                )

U=CALC_CHAMP(reuse = U,
            RESULTAT=U,
             DEFORMATION=('EPFP_ELGA'))

U104 = CREA_CHAMP(
             TYPE_CHAM   = 'NOEU_DEPL_R',
             OPERATION   = 'EXTR',
             RESULTAT    = U,
             NOM_CHAM    = 'DEPL',
             NUME_ORDRE  = 104,
             )

V104 = CREA_CHAMP(
             TYPE_CHAM   = 'ELGA_VARI_R',
             OPERATION   = 'EXTR',
             RESULTAT    = U,
             NOM_CHAM    = 'VARI_ELGA',
             NUME_ORDRE  = 104,
             )

CHAMELG=CALC_CHAM_ELEM(    MODELE=MO,
                             OPTION='COOR_ELGA' )

TEST_RESU(CHAM_ELEM=_F(REFERENCE='ANALYTIQUE',
                       POINT=1,
                       NOM_CMP='W',
                       PRECISION=1.0000000000000001E-09,
                       MAILLE='M1',
                       CHAM_GD=CHAMELG,
                       VALE_CALC=0.1250000000,
                       VALE_REFE=0.125,
                       TOLE_MACHINE=1.0000000000000001E-09,),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=104,
                   REFERENCE='ANALYTIQUE',
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIZZ',
                   VALE_CALC=1.799999817,
                   VALE_REFE=1.8,
                   PRECISION=1.E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=104,
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V7',
                   VALE_CALC=3.1088099999999998E-05,
                   TOLE_MACHINE=1.E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=104,
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V22',
                   VALE_CALC=0.154698,
                   TOLE_MACHINE=1.E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=104,
                   POINT=1,
                   RESULTAT=U,
                   NOM_CHAM='EPFP_ELGA',
                   NOM_CMP='EPZZ',
                   VALE_CALC=5.5715299999999998E-05,
                   TOLE_MACHINE=1.E-4,
                   MAILLE='M1',),
                ),
          )

FIN()
#
