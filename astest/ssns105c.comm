#-----------------------------------------------------------------------

MAIL=LIRE_MAILLAGE(   )

MAILTOT=CREA_MAILLAGE(MAILLAGE      = MAIL,
                      CREA_GROUP_MA = _F(
                               NOM         = 'GEOBB',
                               GROUP_MA    = 'GEOB',
                               PREF_MAILLE = 'B'))

#
# -----------------------------------------------------
# DEFINITION DES MATERIAUX
# -----------------------------------------------------
#

ACIER=DEFI_MATERIAU(
            ELAS=_F(  E = 2.E11,   NU = 0.0),
            ECRO_LINE=_F( D_SIGM_EPSI = 2.E10,   SY = 2.E11, )
                          )

BETON=DEFI_MATERIAU(
            ELAS=_F(  E = 2.E10,   NU = 0.2),
                          )

#
# -------------------------------------------------
# AFFECTATION DES MATERIAUX
# -------------------------------------------------
#
MATAF=AFFE_MATERIAU(    MAILLAGE=MAILTOT,
                          AFFE=(_F(  GROUP_MA = ('GEOB','GEOBB'),
                                 MATER = ACIER,),
                                _F(  GROUP_MA = ('GEO',),
                                 MATER = BETON,),
                               )
                      )

#
# -------------------------------------------------
# AFFECTATION DES MODELES
# -------------------------------------------------
#
LEMOD=AFFE_MODELE(    MAILLAGE=MAILTOT,     
                        AFFE=(_F(  GROUP_MA = ('GEOB','GEOBB'),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = 'GRILLE_EXCENTRE'),
                              _F(  GROUP_MA = ('GEO',),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = '3D'),
                             )
                      )

# -------------------------------------------------
# AFFECTATION DES CARACTERISTIQUES
# -------------------------------------------------
#
LACAR=AFFE_CARA_ELEM(    MODELE=LEMOD,
                       GRILLE=(_F( GROUP_MA = 'GEOB',
                                SECTION = .1,
                                ANGL_REP = (0., 0.0,),
                                EXCENTREMENT = 0.0,
                                COEF_RIGI_DRZ=0.,
                              ),
                              _F( GROUP_MA = 'GEOBB',
                                SECTION = .1,
                                ANGL_REP = (90.0, 0.0,),
                                EXCENTREMENT = 0.0,
                              ))
                        )

ZCHM=AFFE_CHAR_MECA(
             MODELE=LEMOD,
             DDL_IMPO=(
                       _F(  NOEUD = ('NO1','NO2','NO3','NO4'),
                          DZ = 0.,
                       ),
                       _F(  NOEUD = ('NO2','NO3','NO5','NO8'),
#                          DZ = 0.,
                            DRY = 0.,
                       DRX = 0.,      DRZ = 0.,
                       ),
                       _F(  NOEUD = ('NO1','NO2','NO5','NO6'),
                          DY = 0.,
                       ),
                       _F(  NOEUD = ('NO1','NO4','NO5','NO8'),
                          DX = 0.,
                       ),
                       _F(  NOEUD = ('NO3','NO4','NO7','NO8'),
                          DY = 1.,
                       ),
                       _F(  NOEUD = ('NO3','NO2','NO6','NO7'),
                          DX = 1.,
                       ),)
                       )

f_inst=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,  0.0,  10.,  10.),);

#
# ----------------------------------------------
# CALCUL
# ----------------------------------------------
#


ZLIST=DEFI_LIST_REEL(   DEBUT=0.,
             INTERVALLE=(_F( JUSQU_A = 1.0,  NOMBRE = 1, ),
                         _F( JUSQU_A = 2.0,  NOMBRE = 1, ),
                         _F( JUSQU_A =10.0,  NOMBRE = 1, ),),
           )

LEDEP=STAT_NON_LINE(
                            MODELE=LEMOD,
                        CHAM_MATER=MATAF,
                         CARA_ELEM=LACAR,
                             EXCIT=_F(
                CHARGE = ZCHM,
                FONC_MULT=f_inst
                ),
                COMPORTEMENT=(_F(GROUP_MA=('GEOB','GEOBB'),
                              RELATION = 'GRILLE_ISOT_LINE'),
                           _F(GROUP_MA=('GEO',),
                              RELATION = 'ELAS',)
                ),
                         INCREMENT=_F(
                LIST_INST = ZLIST,
                ),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_MAXI = 1.E-3,
                ITER_GLOB_MAXI = 30,
                ),
              )


LEDEP=CALC_CHAMP( reuse = LEDEP,
