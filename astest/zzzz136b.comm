CHARGE=AFFE_CHAR_MECA(    MODELE=MO,
                              FACE_IMPO=_F(  GROUP_MA = 'P3P4',
                                          DNOR = 0.),
                               DDL_IMPO=_F(  GROUP_MA = 'P1P2',
                                          DY = 0.),
