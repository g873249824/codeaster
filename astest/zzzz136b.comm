                           AFFE=_F(  TOUT = 'OUI',
                                  PHENOMENE = 'MECANIQUE',
                                  MODELISATION = 'D_PLAN')
                   )

MAT=DEFI_MATERIAU(     ELAS=_F(  E = 1.E+07,
                                   NU = 0.3)
                       )

CH_MAT=AFFE_MATERIAU(    MAILLAGE=MA,
                                 AFFE=_F(  TOUT = 'OUI',
                                        MATER = MAT) 
