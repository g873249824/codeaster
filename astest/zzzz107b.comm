DEBUT(CODE=_F(NOM='ZZZZ107B',
              NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='NON',),
      PAR_LOT='NON')

MA=LIRE_MAILLAGE();


MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOM='GM12',
                                GROUP_MA='GM12',),
                             ),);   


MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

# On reoriente les group_ma GM11 et GM12 qui 
# interviennent dans le contact car leurs normales
# doivent absolument etre sortantes
MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=(_F(GROUP_MA=('GM11')),
                               _F(GROUP_MA=('GM12'))),
                 );


MAT1=DEFI_MATERIAU(ELAS=_F(E=2.E5,
                           NU=0.3,),
                   ECRO_LINE=_F(D_SIGM_EPSI=5000.0,
                                SY=300.0,),
                                );

MAT2=DEFI_MATERIAU(ELAS=_F(E=2.E9,
                           NU=0.3,),);


CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA=('GM15'),
                             MATER=MAT1,),
                          _F(GROUP_MA=('GM17',),
                             MATER=MAT2,),
                             ),);

CHA=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_MA='GM14',
                                DX= 0.0,
                                DY= 0.0,),
                             _F(GROUP_MA=('GM9','GM11',),
                                DX=0.0,
                                DY=-0.4,),
                             _F(GROUP_MA=('GM10',),
                                DY=-0.4,),
                                ),
                   CONTACT=_F(APPARIEMENT='MAIT_ESCL',
                              GROUP_MA_MAIT='GM11',
                              GROUP_MA_ESCL='GM12',
                              NORMALE='MAIT_ESCL',
                              RECHERCHE = 'NOEUD_BOUCLE',
                              ),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=200,),);

F=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,  0.0,
                      0.8,  1.0,
                      1.0,  0.98),
                      );

is_ok = 0
try:
  RESU=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                     MODELE=MO,
                     CHAM_MATER=CHMAT,
                     EXCIT=_F(CHARGE=CHA,
                              FONC_MULT=F,),
                     COMP_INCR=(
                                 _F(RELATION='VMIS_ISOT_LINE',
                                 DEFORMATION='SIMO_MIEHE',
                                 GROUP_MA='GM15',
                                 ),
                                 _F(RELATION='ELAS',
                                 GROUP_MA='GM17',),
                                    ),
                     CONVERGENCE=(_F(ITER_GLOB_MAXI=20,)),
                     INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=1.,
                                 SUBD_METHODE='UNIFORME', SUBD_PAS=4,
                                 SUBD_PAS_MINI=1.E-4,
                                 SUBD_COEF_PAS_1=1.0,),
                     NEWTON=_F(MATRICE='TANGENTE',
                              PREDICTION='ELASTIQUE',
                              REAC_ITER=1,),
                     ARCHIVAGE=_F(LIST_INST=L_INST,
                                 ARCH_ETAT_INIT='OUI',),);
except aster.ArretCPUError, msg:
   is_ok = 1

TAB1=CREA_TABLE(LISTE=(_F(PARA='TEST',TYPE_K='K8',LISTE_K='VALEUR  ',),
                     _F(PARA='BOOLEEN',LISTE_I=is_ok,),
                     ),
               )
TEST_TABLE(TABLE=TAB1,
         FILTRE=_F(NOM_PARA='TEST',VALE_K='VALEUR  ',),
         NOM_PARA='BOOLEEN',
         VALE_I=1,
         REFERENCE='ANALYTIQUE',
         )

FIN();
