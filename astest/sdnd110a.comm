# CAS_TEST__:SDND110A
#   FICHIER DE COMMANDE ASTER DU CAS-TEST DE MASSE AVEC
#   FORCE LAME FLUIDE
#

DEBUT(CODE=_F(NOM='SDND110A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#

POUTRE=LIRE_MAILLAGE( )

POUTRE=DEFI_GROUP( reuse=POUTRE,   MAILLAGE=POUTRE,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

#

MODELE=AFFE_MODELE(   MAILLAGE=POUTRE,
                         AFFE=_F( TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                        MODELISATION = 'DIS_T'))

#
CHAMPCAR=AFFE_CARA_ELEM(  MODELE=MODELE,DISCRET=(
      _F(
       GROUP_MA = 'TOUT',
 CARA = 'K_T_D_N', VALE = (1.E-4, 1.E0, 1.E0,)),
      _F(
       GROUP_MA = 'TOUT',
   CARA = 'M_T_D_N', VALE = 1000.))
                         )

#
CHARGE=AFFE_CHAR_MECA(  MODELE=MODELE,DDL_IMPO=(
             _F( TOUT = 'OUI',  DZ = 0.0),
             _F( NOEUD = 'NO2',   DX = 0.0, DY = 0.0),
             _F( NOEUD = 'NO1',   DY = 0.0))
                      )

#
FORCE=AFFE_CHAR_MECA(  MODELE=MODELE,
                     FORCE_NODALE=_F( NOEUD = 'NO1',  FX = 1.)
                       )

#
RIGIELEM=CALC_MATR_ELEM(  MODELE=MODELE,  CHARGE=CHARGE,
                         CARA_ELEM=CHAMPCAR,
                         OPTION='RIGI_MECA')

#
MASSELEM=CALC_MATR_ELEM(  MODELE=MODELE,  CHARGE=CHARGE,
                         CARA_ELEM=CHAMPCAR,
                         OPTION='MASS_MECA')

#
VECTELEM=CALC_VECT_ELEM(   CHARGE=CHARGE,  OPTION='CHAR_MECA')

VECTEL1=CALC_VECT_ELEM(   CHARGE=FORCE,   OPTION='CHAR_MECA')

#
#
NUMEROTA=NUME_DDL(   MATR_RIGI=RIGIELEM )

#
MATRRIGI=ASSE_MATRICE(MATR_ELEM=RIGIELEM,NUME_DDL=NUMEROTA)

#
MATRMASS=ASSE_MATRICE(MATR_ELEM=MASSELEM,NUME_DDL=NUMEROTA)

#
VECTASS=ASSE_VECTEUR(   VECT_ELEM=VECTEL1,  NUME_DDL=NUMEROTA)

#
MODES=MODE_ITER_SIMULT(   MATR_RIGI=MATRRIGI,  MATR_MASS=MATRMASS,
                         CALC_FREQ=_F( OPTION = 'CENTRE',
                        NMAX_FREQ = 1,
                        FREQ = ( 0.1, )),
)


#
DEPLPHY1=CREA_CHAMP( OPERATION='AFFE', PROL_ZERO='OUI', TYPE_CHAM='NOEU_DEPL_R',
                            MAILLAGE=POUTRE,AFFE=(
                 _F( TOUT = 'OUI',
                    NOM_CMP = ('DX','DY',), VALE = (0.E0, 0., )),
                 _F( NOEUD = 'NO1',
                    NOM_CMP = ('DX','DY',), VALE = (-0.1,  0., ))),
          CHAM_NO=VECTASS
                          )

#
DEPLPHY2=CREA_CHAMP( OPERATION='AFFE', PROL_ZERO='OUI', TYPE_CHAM='NOEU_DEPL_R',
                            MAILLAGE=POUTRE,AFFE=(
                 _F( TOUT = 'OUI',
                    NOM_CMP = ('DX','DY',), VALE = (0.E0, 0., )),
                 _F( NOEUD = 'NO1',
                    NOM_CMP = ('DX','DY',), VALE = (-1.,  0., ))),
          CHAM_NO=VECTASS
                          )

#
PROJ_BASE(  BASE=MODES,
            STOCKAGE='DIAG',
            MATR_ASSE_GENE=(
             _F(
                             MATRICE = CO("MASSEGEN"),
                             MATR_ASSE = MATRMASS),
             _F(
                             MATRICE = CO("RIGIDGEN"),
                             MATR_ASSE = MATRRIGI)),VECT_ASSE_GENE=(
             _F(
                             VECTEUR = CO("VITINI1"),
                             VECT_ASSE = DEPLPHY1,
                             TYPE_VECT = 'VITE'),
             _F(
                             VECTEUR = CO("VITINI2"),
                             VECT_ASSE = DEPLPHY2,
                             TYPE_VECT = 'VITE'))
)

#
PLANZ=DEFI_OBSTACLE(   TYPE='BI_PLAN_Z' )

PLANY=DEFI_OBSTACLE(   TYPE='BI_PLAN_Y' )

#
# PREMIER CAS PROFIL POISEUILLE V=0.1 M/S
#
DYNAMOD1=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='GENE',
                        SCHEMA_TEMPS=_F(SCHEMA='EULER',),
                       MATR_MASS=MASSEGEN,
                       MATR_RIGI=RIGIDGEN,
                       AMOR_MODAL=_F(AMOR_REDUIT=( 0.0, ),),
                       ETAT_INIT=_F( VITE = VITINI1),
                       INCREMENT=_F( INST_INIT = 0.,
                                  INST_FIN = 0.190,
                                  PAS = 2.5E-5),
                      ARCHIVAGE=_F(  PAS_ARCH = 10),
                      PARA_LAME_FLUI=_F(
                       NMAX_ITER=20,
                       RESI_RELA=1.E-5,),
                      CHOC=_F(
                               NOEUD_1 = 'NO1',
                               NOEUD_2 = 'NO2',
                               INTITULE = 'LIAISON',
                               OBSTACLE = PLANZ,
                               NORM_OBST = (0., 0., 1.,),
                               ANGL_VRIL = 0.,
                               DIST_1 = 0.497,
                               DIST_2 = 0.497,
                               RIGI_NOR = 2.E10,
                               RIGI_TAN = 2.E10,
                               FROTTEMENT='COULOMB',
                               COULOMB = 0.,
                               LAME_FLUIDE = 'OUI',
                               ALPHA = -0.0833,
                               BETA = 0.19992,
                               CHI = -0.9996E-6,
                               DELTA = 0.)
                          )

#
LISTER1=DEFI_LIST_REEL(  DEBUT=0.02,
                    INTERVALLE=_F( JUSQU_A = 0.2, NOMBRE = 9)
                            )

#
DYNATRAN=REST_GENE_PHYS(
                    RESU_GENE=DYNAMOD1,
                    LIST_INST=LISTER1,
                    NOM_CHAM='DEPL',
                    INTERPOL='LIN'
                            )

#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DYNATRAN,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='SOURCE_EXTERNE',
                  VALE_REFE=-1.98593E-3,
                  VALE_CALC=-1.9858218275146E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=2,
                  RESULTAT=DYNATRAN,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='SOURCE_EXTERNE',
                  VALE_REFE=-3.91819E-3,
                  VALE_CALC=-3.9181870864042E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=3,
                  RESULTAT=DYNATRAN,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='SOURCE_EXTERNE',
                  VALE_REFE=-5.61048E-3,
                  VALE_CALC=-5.6106795786378E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=9,
                  RESULTAT=DYNATRAN,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='SOURCE_EXTERNE',
                  VALE_REFE=-5.90398E-3,
                  VALE_CALC=-5.9034659818518E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
# DEUXIEME CAS PROFIL UNIFORME V=0.1 M/S
#
DYNAMOD2=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='GENE',
                        SCHEMA_TEMPS=_F(SCHEMA='EULER',),
                       MATR_MASS=MASSEGEN,
                       MATR_RIGI=RIGIDGEN,
                       AMOR_MODAL=_F(AMOR_REDUIT=( 0.0, ),),
                       ETAT_INIT=_F( VITE = VITINI1),
                       INCREMENT=_F( INST_INIT = 0.,
                                  INST_FIN = 0.190,
                                  PAS = 2.5E-5),
                      ARCHIVAGE=_F(  PAS_ARCH = 10),
                      CHOC=_F(
                               NOEUD_1 = 'NO1',
                               NOEUD_2 = 'NO2',
                               INTITULE = 'LIAISON',
                               OBSTACLE = PLANZ,
                               NORM_OBST = (0., 0., 1.,),
                               ANGL_VRIL = 0.,
                               DIST_1 = 0.497,
                               DIST_2 = 0.497,
                               RIGI_NOR = 2.E10,
                               RIGI_TAN = 2.E10,
                               FROTTEMENT='COULOMB',
                               COULOMB = 0.,
                               LAME_FLUIDE = 'OUI',
                               ALPHA = -0.0833,
                               BETA = 0.1666,
                               CHI = 0.,
                               DELTA = 0.)
                          )

#
##
DYNATRA2=REST_GENE_PHYS(
                    RESU_GENE=DYNAMOD2,
                    LIST_INST=LISTER1,
                    NOM_CHAM='DEPL',
                    INTERPOL='LIN'
                            )

#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DYNATRA2,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='ANALYTIQUE',
                  VALE_REFE=-1.98828E-3,
                  VALE_CALC=-1.9882750835701E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=2,
                  RESULTAT=DYNATRA2,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='ANALYTIQUE',
                  VALE_REFE=-3.93216E-3,
                  VALE_CALC=-3.9321630771097E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=3,
                  RESULTAT=DYNATRA2,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='ANALYTIQUE',
                  VALE_REFE=-5.66658E-3,
                  VALE_CALC=-5.6669079604426E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
TEST_RESU(RESU=_F(NUME_ORDRE=9,
                  RESULTAT=DYNATRA2,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  REFERENCE='ANALYTIQUE',
                  VALE_REFE=-5.99946E-3,
                  VALE_CALC=-5.9991365922366E-03,
                  PRECISION=1.E-3,
                  ),
          )

#
FIN()
