CA=AFFE_CARA_ELEM(   MODELE=MO,
                       COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.01)  )

ASSEMBLAGE( MODELE=MO, CARA_ELEM=CA,
                 CHARGE=CH, CHAM_MATER=CHMAT,
                 NUME_DDL=CO("NUM"),
                 MATR_ASSE=(
                         _F(  MATRICE = CO("MATR"),   OPTION = 'RIGI_MECA'),
