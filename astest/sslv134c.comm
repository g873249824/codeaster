# QUART DE FISSURE. TEST DE POST_K1_K2_K3. MAILLAGE BARSOUM

DEBUT(   CODE=_F(  NOM = 'SSLV134C',NIV_PUB_WEB='INTERNET') )

PRE_GIBI()

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA, 
                     CREA_GROUP_NO=_F( 
                      GROUP_MA = ('LP8P9', 'LP8P9S', 'LP0P11', 'LP0P8', 'LP0P8S',))
                 )

MO=AFFE_MODELE(  MAILLAGE=MA,
                      AFFE=_F(  TOUT = 'OUI',
                             PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'AXIS') )

MAT=DEFI_MATERIAU(ELAS=_F(  E = 2.E11,
                             NU = 0.3,
                             ALPHA = 0.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',
                              MATER = MAT) )

SYMETRIE=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
                _F(  GROUP_NO = ('LP8P9', 'LP8P9S',), DX = 0.),
                         _F(  GROUP_NO = 'LP0P11',  DY = 0.))
                         )

PRESSION=AFFE_CHAR_MECA(    MODELE=MO,FORCE_CONTOUR=(
                  _F(  GROUP_MA = 'LP9P10',  FY = 1.E6),
                  _F(  GROUP_MA = 'LP9P10S', FY = -1.E6))
                          )

CHAMDEPL=MECA_STATIQUE(        MODELE=MO,
                              CHAM_MATER=CHMAT,EXCIT=(
                              _F( CHARGE = PRESSION),
                              _F( CHARGE = SYMETRIE))
                           )

DEP=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                 NOM_CHAM='DEPL',  RESULTAT=CHAMDEPL,  NUME_ORDRE=1  )

CHAMDEPL=CALC_ELEM( reuse=CHAMDEPL,      
                           OPTION='SIGM_ELNO_DEPL',
                         RESULTAT=CHAMDEPL )

FISS=DEFI_FOND_FISS(    MAILLAGE=MA,
                            FOND_FISS=_F( GROUP_NO = ('P0',)),
                            NORMALE=(0., 1., 0.,)
                       )

RSUP=0.5

RINF=0.2

# VALEUR DE REFERENCE MILIEU INFINI : G=145.6
# POUR UN RADIAN

GREF=23.17296

GLOC=11.58648

THETA01=CALC_THETA(    MODELE=MO,
                         THETA_2D=_F(  GROUP_NO = ('P0',),
                                    MODULE = 1.,
                                    R_INF = RINF,
                                    R_SUP = RSUP),
                         DIRECTION=(1., 0., 0.,)
                     )

G0=CALC_G        (       OPTION='CALC_G',
                         RESULTAT=CHAMDEPL,
                         NUME_ORDRE = 1,
                         THETA=_F(THETA=THETA01),
                         SYME_CHAR='SANS'
                     )

TEST_TABLE(       TABLE=G0,
                  NOM_PARA='G',
                      VALE=GREF,
                 PRECISION=0.017        )

# POST_K1_K2_K3


MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,CREA_GROUP_NO=(
                   _F(  NOM = 'L1INF',
                                   OPTION = 'SEGM_DROI_ORDO',
                                   PRECISION = 1.E-3,
                                   CRITERE = 'ABSOLU',
                                   GROUP_NO = 'LP0P8S',
                                   GROUP_NO_ORIG = 'P0',
                                   GROUP_NO_EXTR = 'P8'),
                   _F(  NOM = 'L1SUP',
                                   OPTION = 'SEGM_DROI_ORDO',
                                   PRECISION = 1.E-3,
                                   CRITERE = 'ABSOLU',
                                   GROUP_NO = 'LP0P8',
                                   GROUP_NO_ORIG = 'P0',
                                   GROUP_NO_EXTR = 'P8'))
                 )

DEPH1=POST_RELEVE_T( ACTION=_F(  INTITULE = 'TAB2',
                                GROUP_NO = 'L1SUP',
                                CHAM_GD = DEP,
#                                RESULTAT = CHAMDEPL,
#                                NOM_CHAM = 'DEPL',
                                NOM_CMP = ('DX', 'DY',),
                                OPERATION = 'EXTRACTION')
                      )

IMPR_TABLE( TABLE= DEPH1 )

DEPB1=POST_RELEVE_T( ACTION=_F(  INTITULE = 'TAB1',
                                GROUP_NO = 'L1INF',
                                CHAM_GD = DEP,
#                                RESULTAT = CHAMDEPL,
#                                NOM_CHAM = 'DEPL',
                                NOM_CMP = ('DX', 'DY',),
                                OPERATION = 'EXTRACTION')
                      )

TABL_K1=POST_K1_K2_K3(  MODELISATION='AXIS',
                        TABL_DEPL_SUP=DEPH1,
                        TABL_DEPL_INF=DEPB1,
                        INFO=2,
                        MATER=MAT,
                        ABSC_CURV_MAXI=0.539,
                        VECT_K1=(0., 1., 0.,)
                     )

IMPR_TABLE( TABLE=TABL_K1 )

K1=1.595769E6

# ---------------

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 1),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='K1_MAX',
                           VALE=K1,
                      PRECISION=0.017           )

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 1),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='K1_MIN',
                           VALE=K1,
                      PRECISION=0.007           )

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 1),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='G_MAX',
                           VALE=GLOC,
                      PRECISION=0.031           )

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 1),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='G_MIN',
                           VALE=GLOC,
                      PRECISION=0.004           )

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 2),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='K1_MAX',
                           VALE=K1,
                      PRECISION=0.006          )

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 2),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='K1_MIN',
                           VALE=K1,
                      PRECISION=0.08           )

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 2),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='G_MAX',
                           VALE=GLOC,
                      PRECISION=0.012           )

TEST_TABLE(       TABLE=TABL_K1,
                        FILTRE=_F(  NOM_PARA = 'METHODE',
                                 VALE_I = 2),
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='G_MIN',
                           VALE=GLOC,
                      PRECISION=0.13           )

FIN()
