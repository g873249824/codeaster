                              TYPE_CHARGE='FIXE_CSTE',),),
                    COMP_INCR=(_F(RELATION='POLYCRISTAL',
                                  COMPOR=COMPORP,
                                  DEFORMATION='PETIT',
                                  TOUT='OUI',),),
                    INCREMENT=(_F(LIST_INST=LINST,
