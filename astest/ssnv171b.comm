SOLNLP=CALC_ELEM(reuse =SOLNLP,
                 RESULTAT=SOLNLP,
                 OPTION=('EPSI_ELGA_DEPL','EPSP_ELGA',),);
                 
IMPR_RESU(RESU=_F(RESULTAT=SOLNLP))
