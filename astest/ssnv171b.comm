DEBUT(CODE=_F(NOM='SSNV171B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),)



ACIER=DEFI_MATERIAU(ELAS=_F(E=145200.0,
                            NU=0.3,),
                    MONO_VISC1=_F(N=10.0,
                                  K=40.0,
                                  C=1.0,),
                    MONO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H=0.0,),
                    MONO_CINE1=_F(D=36.68,),);

CHARGE=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,140.0,),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);


COMPOR1=DEFI_COMPOR( ROTA_RESEAU='POST',
                     MONOCRISTAL=(_F(MATER=ACIER,
                                    ECOULEMENT='MONO_VISC1',
                                    ECRO_ISOT='MONO_ISOT1',
                                    ECRO_CINE='MONO_CINE1',
                                    ELAS='ELAS',
                                    FAMI_SYST_GLIS='OCTAEDRIQUE',),),);

TFIN = 1.5;

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.87,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=TFIN,
                                    NOMBRE=300,),),);
DEFLIST1 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = LINST),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS=10,
                                 SUBD_NIVEAU=10),)
MONOIMPR=SIMU_POINT_MAT(INFO=1,
                   COMPORTEMENT=_F(RELATION='MONOCRISTAL',
                                COMPOR=COMPOR1,
                                ALGO_INTE='NEWTON_RELI',
                                ),
                   MASSIF=_F(ANGL_REP=30.0),
                   NEWTON=_F(REAC_ITER=1,),
                   MATER      = ACIER,
                   INCREMENT=_F(LIST_INST=DEFLIST1),
                   ARCHIVAGE=_F(LIST_INST=LINST,PRECISION=1.E-6),
                   SIGM_IMPOSE=_F(SIXX=CHARGE),
                        );

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=1.E-6,
           VALE_CALC=210.0,
           VALE_REFE=210.0,
           NOM_PARA='SIXX',
           TABLE=MONOIMPR,
           FILTRE=_F(PRECISION=1.E-06,
                     NOM_PARA='INST',
                     VALE=1.5,),
           )

