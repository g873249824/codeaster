                                  FRAC_VOL=1.0,
                                  ANGL_REP=(30.,0.,0.)),),
                    LOCALISATION='BETA',DA=0.,DL=0.,
                  #  LOCALISATION='BZ',
                    );

#IMPR_CO(CO=COMPORP)

