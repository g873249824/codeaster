DEBUT(CODE=_F(NOM='FORMA03C',
              NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='OUI',),
      IGNORE_ALARM=('FACTOR_20' ),
              );

# IGNORE_ALARM=('FACTOR_20' ): LA MATRICE EST SINGULIERE (CHARGE LIMITE)
# LE BUT EST PEDAGOGIQUE: ON MONTRE QUE LA MATRICE EST SINGULIERE POUR
# ESTIMER CETTE CHARGE LIMITE PAR DECOUPE AUTOMATIQUE DU PAS DE TEMPS
# PUIS ON UTILISE LE PILOTAGE

# LECTURE DU MAILLAGE
MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_2D=_F(GROUP_MA='haut',),);
                   
# AFFECTATION DU MODELE (CONTRAINTES PLANES)
MODE=AFFE_MODELE(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='C_PLAN',),);

# COURBE DE TRACTION
CTRAC = LIRE_FONCTION(UNITE=21,
                      NOM_PARA='EPSI',
                      PROL_DROITE='CONSTANT',)

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=29,
              COURBE=_F(FONCTION=CTRAC,),
              TITRE='Courbe de traction',
              LEGENDE_X='Deformation',
              LEGENDE_Y='Contrainte (MPa)',);
              
              
# DEFINITION DU MATERIAU
ACIER =DEFI_MATERIAU(ELAS=_F(E=200000.0,
                         NU=0.3,),
                 TRACTION=_F(SIGM=CTRAC,),);

CHMATE=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=_F(TOUT='OUI',
                             MATER=ACIER,),);

# CONDITIONS DE SYMETRIE (QUART DE PLAQUE)
CHSYME=AFFE_CHAR_CINE(MODELE=MODE,
                      MECA_IMPO=(_F(GROUP_MA='bas',
                                 DY=0.0,),
                                 _F(GROUP_MA='gauche',
                                 DX=0.0,),),
                                );
                                
# CHARGEMENT EN FORCE
CHFORC=AFFE_CHAR_MECA(MODELE=MODE,
                      FORCE_CONTOUR=_F(GROUP_MA='haut',
                                       FY=1.,),);

# RAMPE APPLICATION CHARGEMENT
RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                           1000.0,1000.0,
                           ),);
                           

    
########################################################################
#
# PREMIER CALCUL : RECHERCHE MANUELLE DE LA CHARGE LIMITE
#
########################################################################

# DISCRETISATION EN TEMPS
TFIN  = 243.0;
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=TFIN,
                                     NOMBRE=10,),
                                  ),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST,),
                        ECHEC=_F(SUBD_METHODE='EXTRAPOLE',
                                 SUBD_PAS=2,
                                 SUBD_NIVEAU=20,),)

# CALCUL                                                    
RESUNL=STAT_NON_LINE(MODELE=MODE,
                     CHAM_MATER=CHMATE,
                     EXCIT=(_F(CHARGE=CHSYME,),
                            _F(CHARGE=CHFORC,
                               FONC_MULT=RAMPE,),),
                     COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                     INCREMENT=_F(LIST_INST=DEFLIST,
                                  INST_FIN=TFIN,
                                  ),            
                     NEWTON=_F(REAC_ITER=1,),
                     SOLVEUR=_F(STOP_SINGULIER='DECOUPE',),
                     );

########################################################################
#
# PREMIER CALCUL : RECHERCHE MANUELLE DE LA CHARGE LIMITE
#
# EXEMPLE DE GESTION DE LA LISTE AUTOMATIQUE
#
########################################################################

# DISCRETISATION EN TEMPS
TFIN  = 243.0;

# EN GESTION AUTOMATIQUE, ON IMPOSE SEULEMENT LE 1ER PAS DE TEMPS
L_AUTO=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=    50,NOMBRE= 1),
                                  _F(JUSQU_A=  TFIN,NOMBRE= 1),
                                  ),);

DEFAUTO=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='AUTO',
                                    LIST_INST = L_AUTO,
                                    PAS_MINI=1.e-6,
                                    PAS_MAXI=100.)
                      )
                      
# CALCUL
RESUAU=STAT_NON_LINE(MODELE=MODE,
                     CHAM_MATER=CHMATE,
                     EXCIT=(_F(CHARGE=CHSYME,),
                            _F(CHARGE=CHFORC,
                               FONC_MULT=RAMPE,),),
                     COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                     INCREMENT=_F(LIST_INST=DEFAUTO,
                                  INST_FIN=TFIN,
                                  ),
                     NEWTON=_F(REAC_ITER=1,),
                     );

########################################################################
#
# POST-TRAITEMENTS STANDARDS
#
########################################################################

# INTERPOLATION DES CONTRAINTES AUX NOEUDS
RESUNL=CALC_ELEM(reuse =RESUNL,
                 RESULTAT=RESUNL,
                 OPTION=('SIEF_ELNO',),);

RESUNL=CALC_NO(reuse =RESUNL,
               RESULTAT=RESUNL,
               OPTION=('SIEF_NOEU',),);
          
# INTERPOLATION DES VARIABLES INTERNES AUX NOEUDS
RESUNL=CALC_ELEM(reuse =RESUNL,
                 RESULTAT=RESUNL,
                 OPTION=('VARI_ELNO'),);
                              
RESUNL=CALC_NO(reuse =RESUNL,
               RESULTAT=RESUNL,
               OPTION='VARI_NOEU',);
 
########################################################################
#
# IMPRESSION DES RESULTATS
#
########################################################################

IMPR_RESU(FORMAT='MED',
          RESU=_F(RESULTAT=RESUNL,),);

#########################################################################
#
# DEUXIEME CALCUL : CALCUL AVEC PILOTAGE
#
########################################################################

# DISCRETISATION EN TEMPS
TFINPI  = 6.0;
L_INST2=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=TFINPI,
                                     NOMBRE=30,),
                                  ),);

DEFLIST2 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST2,),
                        ECHEC=_F(SUBD_METHODE='EXTRAPOLE',
                                 SUBD_PAS=4,
                                 SUBD_NIVEAU=5,),)

# CALCUL 

RESUPI=STAT_NON_LINE(MODELE=MODE,
                     CHAM_MATER=CHMATE,
                     EXCIT=(_F(CHARGE=CHSYME,),
                            _F(CHARGE=CHFORC,
                               TYPE_CHARGE='FIXE_PILO',),),
                     COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                     INCREMENT=_F(LIST_INST=DEFLIST2,
                                  INST_FIN=TFINPI,),
                     NEWTON=_F(REAC_ITER=1,),
                     PILOTAGE=_F(TYPE='DDL_IMPO',
                                 COEF_MULT=1.0,
                                 GROUP_NO='G',
                                 NOM_CMP='DY',),
                     SOLVEUR=_F(STOP_SINGULIER='DECOUPE',),
                     );


########################################################################
#
# POST-TRAITEMENTS STANDARDS
#
########################################################################

# INTERPOLATION DES CONTRAINTES AUX NOEUDS
RESUPI=CALC_ELEM(reuse =RESUPI,
                 RESULTAT=RESUPI,
                 OPTION=('SIEF_ELNO',),);

RESUPI=CALC_NO(reuse =RESUPI,
               RESULTAT=RESUPI,
               OPTION=('SIEF_NOEU',),);
          
# INTERPOLATION DES VARIABLES INTERNES AUX NOEUDS
RESUPI=CALC_ELEM(reuse =RESUPI,
                 RESULTAT=RESUPI,
                 OPTION=('VARI_ELNO'),);
                              
RESUPI=CALC_NO(reuse =RESUPI,
               RESULTAT=RESUPI,
               OPTION='VARI_NOEU',);


########################################################################
#
# COURBE FORCE APPLIQUEE  EN FONCTION DU TEMPS (ETA_PILOTAGE)
#
########################################################################


ETA_PILO=RECU_FONCTION(RESULTAT=RESUPI,
                       TOUT_ORDRE='OUI',
                       NOM_PARA_RESU='ETA_PILOTAGE',);

IMPR_FONCTION(FORMAT='TABLEAU',
              COURBE=_F(FONCTION=ETA_PILO,),
              UNITE=8,);

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=81,
              COURBE=_F(FONCTION=ETA_PILO,),
              TITRE=' Fy(ETA_PILOTAGE) fonction du temps',
              LEGENDE_X='Temps',
              LEGENDE_Y='ETA_PILOTAGE',);

########################################################################
#
# IMPRESSION DES RESULTATS
#
########################################################################

IMPR_RESU(FORMAT='MED',
          RESU=_F(RESULTAT=RESUPI,),);

# TESTS - CALCUL SANS PILOTAGE       
TEST_RESU(RESU=_F(RESULTAT=RESUNL,
                  INST=TFIN,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='SIYY',
                  GROUP_NO='G',
                  VALE=243.04226576028,
                  PRECISION=1E-8,
                  REFERENCE='NON_REGRESSION',),);
                  
TEST_RESU(RESU=_F(RESULTAT=RESUNL,
                  INST=TFIN,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='SIYY',
                  GROUP_NO='G',
                  VALE=243.,
                  PRECISION=0.0003,
                  REFERENCE='ANALYTIQUE',),);
                                    
# TESTS - CALCUL AVEC PILOTAGE  
TEST_RESU(RESU=_F(RESULTAT=RESUPI,
                  INST=TFINPI,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='SIYY',
                  GROUP_NO='G',
                  VALE=243.,
                  PRECISION=0.003,
                  REFERENCE='ANALYTIQUE',),);
   
TEST_RESU(RESU=_F(RESULTAT=RESUPI,
                  INST=TFINPI,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='SIYY',
                  GROUP_NO='G',
                  VALE=243.04100195057,
                  PRECISION=0.003,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=RESUPI,
                  INST=TFINPI,
                  NOM_CHAM='SIEF_NOEU',
                  NOM_CMP='SIYY',
                  GROUP_NO='G',
                  VALE=243.56838375054,
                  PRECISION=1E-8,
                  REFERENCE='NON_REGRESSION',),);  
                    
TEST_FONCTION(VALEUR=_F(FONCTION=ETA_PILO,
                        VALE_PARA=TFINPI,
                        VALE_REFE=243.,
                        PRECISION=0.003,
                        REFERENCE='ANALYTIQUE',
                        ),);
                                                       
TEST_FONCTION(VALEUR=_F(FONCTION=ETA_PILO,
                        VALE_PARA=TFINPI,
                        VALE_REFE=243.535356,
                        PRECISION=1E-8,
                        REFERENCE='NON_REGRESSION',
                        ),);
          
FIN();
