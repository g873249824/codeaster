    DETRUIRE(CONCEPT=_F(NOM=(INIVAR,FONCH,PESHAU,PESBAS,
                             DEPL_IN,DEPL_PJ,FX,FY,FZ,
                             CH_FONC,CH_EVAL,CH_GEOM,DXN,DYN,DZN,
                             CH_DEPL,CH_BID,),),);

