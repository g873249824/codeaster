DEBUT(CODE=_F(NOM='SDLS111A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAILLA=LIRE_MAILLAGE(FORMAT='MED',UNITE=20);

MODELE=AFFE_MODELE(MAILLAGE=MAILLA,
                   AFFE=_F(GROUP_MA='CALCUL',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='DKT'));

MATER=DEFI_MATERIAU(ELAS=_F(E=210000000000.0,
                            NU=0.3,
                            RHO=7800));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLA,
                       AFFE=_F(GROUP_MA='CALCUL',
                               MATER=MATER));

PARAM=AFFE_CARA_ELEM(MODELE=MODELE,
                     COQUE=_F(GROUP_MA='CALCUL',
                              EPAIS=0.01));
#
#
#//////////////////////////////////////////////////////////////////////
#
#     SOUS-STRUCTURE LIBRE - INTERFACE DE TYPE CRAIG-BAMPTON
#
#//////////////////////////////////////////////////////////////////////

CHARGE_L=AFFE_CHAR_MECA(MODELE=MODELE,
                        DDL_IMPO=(_F(GROUP_MA='TOUT',
                                     DRZ=0),
                                  _F(GROUP_NO='BASE',
                                     DX=0,
                                     DY=0,
                                     DZ=0,
                                     DRX=0,
                                     DRY=0)));

MERIME_L=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=PARAM,
                        CHARGE=CHARGE_L);

MEMAME_L=CALC_MATR_ELEM(OPTION='MASS_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=PARAM,
                        CHARGE=CHARGE_L);

NUMDDL_L=NUME_DDL(MATR_RIGI=MERIME_L);

RAID_L=ASSE_MATRICE(MATR_ELEM=MERIME_L,
                    NUME_DDL=NUMDDL_L);

MASS_L=ASSE_MATRICE(MATR_ELEM=MEMAME_L,
                    NUME_DDL=NUMDDL_L);

MODES_L1=MODE_ITER_SIMULT(MATR_RIGI=RAID_L,
                          MATR_MASS=MASS_L,
                          CALC_FREQ=_F(OPTION='BANDE',
                                       FREQ=(10.0,1000.0)));


MODES_L2=MODE_ITER_SIMULT(MATR_RIGI=RAID_L,
                          MATR_MASS=MASS_L,
                          CALC_FREQ=_F(OPTION='BANDE',
                                       FREQ=(1000.0,2000.0)));


MODES_L3=MODE_ITER_SIMULT(MATR_RIGI=RAID_L,
                          MATR_MASS=MASS_L,
                          CALC_FREQ=_F(OPTION='BANDE',
                                       FREQ=(2000.0,3000.0)));

#
#
#//////////////////////////////////////////////////////////////////////
#
#     SOUS-STRUCTURE CENTRALE - INTERFACE DE TYPE CRAIG-BAMPTON
#
#//////////////////////////////////////////////////////////////////////

CHARGE_M=AFFE_CHAR_MECA(MODELE=MODELE,
                        DDL_IMPO=(_F(GROUP_MA='TOUT',
                                     DRZ=0),
                                  _F(GROUP_NO='DROITE',
                                     DX=0,
                                     DY=0,
                                     DZ=0,
                                     DRX=0,
                                     DRY=0),
                                  _F(GROUP_NO='GAUCHE',
                                     DX=0,
                                     DY=0,
                                     DZ=0,
                                     DRX=0,
                                     DRY=0),
                                  _F(GROUP_NO='BASE',
                                     DX=0,
                                     DY=0,
                                     DZ=0,
                                     DRX=0,
                                     DRY=0)));

MERIME_M=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=PARAM,
                        CHARGE=CHARGE_M);

MEMAME_M=CALC_MATR_ELEM(OPTION='MASS_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=PARAM,
                        CHARGE=CHARGE_M);

NUMDDL_M=NUME_DDL(MATR_RIGI=MERIME_M);

RAID_M=ASSE_MATRICE(MATR_ELEM=MERIME_M,
                    NUME_DDL=NUMDDL_M);

MASS_M=ASSE_MATRICE(MATR_ELEM=MEMAME_M,
                    NUME_DDL=NUMDDL_M);

MODES_M1=MODE_ITER_SIMULT(MATR_RIGI=RAID_M,
                          MATR_MASS=MASS_M,
                          CALC_FREQ=_F(OPTION='BANDE',
                                       FREQ=(10.0,1000.0)));


MODES_M2=MODE_ITER_SIMULT(MATR_RIGI=RAID_M,
                          MATR_MASS=MASS_M,
                          CALC_FREQ=_F(OPTION='BANDE',
                                       FREQ=(1000.0,2000.0)));


MODES_M3=MODE_ITER_SIMULT(MATR_RIGI=RAID_M,
                          MATR_MASS=MASS_M,
                          NMAX_ITER_SOREN=25,
                          CALC_FREQ=_F(OPTION='BANDE',
                                       FREQ=(2000.0,3000.0)));

#
#
#//////////////////////////////////////////////////////////////////////
#
#     SOUS-STRUCTURE ENCASTREE - INTERFACE DE TYPE CRAIG-BAMPTON
