                    NUME_DDL=NUMDDL_E);

MASS_E=ASSE_MATRICE(MATR_ELEM=MEMAME_E,
                    NUME_DDL=NUMDDL_E);

MODE_E=MODE_ITER_SIMULT(MATR_RIGI=RAID_E,
                        MATR_MASS=MASS_E,
                        CALC_FREQ=_F(NMAX_FREQ=25),
                        VERI_MODE=_F(STOP_ERREUR='NON'));

#
#
#//////////////////////////////////////////////////////////////////////
#
#     DEFINITION DES INTERFACES
#
#//////////////////////////////////////////////////////////////////////

LINT1_CL=DEFI_INTERF_DYNA(NUME_DDL=NUMDDL_L,
                          INTERFACE=_F(NOM='BASE_L',
                                       TYPE='CRAIGB',
                                       GROUP_NO='BASE',
                                       MASQUE='DRZ'));

LINT2_CL=DEFI_INTERF_DYNA(NUME_DDL=NUMDDL_M,
                          INTERFACE=(_F(NOM='DROITE_M',
                                        TYPE='CRAIGB',
                                        GROUP_NO='DROIT2',
                                        MASQUE='DRZ'),
                                     _F(NOM='GAUCHE_M',
                                        TYPE='CRAIGB',
                                        GROUP_NO='GAUCH2',
                                        MASQUE='DRZ'),
                                     _F(NOM='BASE_M',
                                        TYPE='CRAIGB',
                                        GROUP_NO='BASE',
                                        MASQUE='DRZ')));

LINT3_CL=DEFI_INTERF_DYNA(NUME_DDL=NUMDDL_E,
                          INTERFACE=_F(NOM='DROITE_E',
                                       TYPE='CRAIGB',
                                       GROUP_NO='DROITE',
                                       MASQUE='DRZ'));

#
#
#//////////////////////////////////////////////////////////////////////
#
#     CALCUL DE LA BASE MODALE DES SOUS-STRUCTURES
#
#//////////////////////////////////////////////////////////////////////

BAMO1_CL=DEFI_BASE_MODALE(CLASSIQUE=_F(INTERF_DYNA=LINT1_CL,
                                       MODE_MECA=(MODES_L1,MODES_L2,MODES_L3),
                                       NMAX_MODE=20));

BAMO2_CL=DEFI_BASE_MODALE(CLASSIQUE=_F(INTERF_DYNA=LINT2_CL,
                                       MODE_MECA=(MODES_M1,MODES_M2,MODES_M3),
                                       NMAX_MODE=20));

BAMO3_CL=DEFI_BASE_MODALE(CLASSIQUE=_F(INTERF_DYNA=LINT3_CL,
                                       MODE_MECA=MODE_E,
                                       NMAX_MODE=20));


#
#
#//////////////////////////////////////////////////////////////////////
#
#     CALCUL DES MACRO-ELEMENTS
#
#//////////////////////////////////////////////////////////////////////

MACEL1=MACR_ELEM_DYNA(BASE_MODALE=BAMO1_CL);

MACEL2=MACR_ELEM_DYNA(BASE_MODALE=BAMO2_CL);

MACEL3=MACR_ELEM_DYNA(BASE_MODALE=BAMO3_CL);


#
#
#//////////////////////////////////////////////////////////////////////
#
#     CALCUL SUR MODELE GENERALISE
#
#//////////////////////////////////////////////////////////////////////

MODEGENE=DEFI_MODELE_GENE(SOUS_STRUC=(
                                      _F(NOM='TRI1',
                                         MACR_ELEM_DYNA=MACEL1,
                                         TRANS=(0.,0.0,0.),
                                         ANGL_NAUT=(0.0,0.0,0.0)),
                                      _F(NOM='TRI2',
                                         MACR_ELEM_DYNA=MACEL2,
                                         TRANS=(0.,0.0,0.),
                                         ANGL_NAUT=(0.0,0.0,180.0)),
                                      _F(NOM='TRI3',
                                         MACR_ELEM_DYNA=MACEL3,
                                         TRANS=(-0.5,-0.866025,0.),
                                         ANGL_NAUT=(0.,0.,0.)),
                                      _F(NOM='TRI4',
                                         MACR_ELEM_DYNA=MACEL3,
                                         TRANS=(1.5,-0.866025,0.),
                                         ANGL_NAUT=(0.0,180.0,0.0)),
                                         ),
                          LIAISON=(
                                   _F(SOUS_STRUC_1='TRI1',
                                      INTERFACE_1='BASE_L',
                                      SOUS_STRUC_2='TRI2',
                                      INTERFACE_2='BASE_M'),
                                   _F(SOUS_STRUC_1='TRI2',
                                      INTERFACE_1='GAUCHE_M',
                                      SOUS_STRUC_2='TRI3',
                                      INTERFACE_2='DROITE_E'),
                                  _F(SOUS_STRUC_2='TRI4',
                                      INTERFACE_2='DROITE_E',
                                      SOUS_STRUC_1='TRI2',
                                      INTERFACE_1='DROITE_M'),
                                       ),
                          VERIF=_F(PRECISION=1e-06,
                                   CRITERE='RELATIF'));

NUMEGE=NUME_DDL_GENE(MODELE_GENE=MODEGENE,
                     STOCKAGE='LIGN_CIEL');



RIGIGENE=ASSE_MATR_GENE(NUME_DDL_GENE=NUMEGE,
                        OPTION='RIGI_GENE');


MASSGENE=ASSE_MATR_GENE(NUME_DDL_GENE=NUMEGE,
                        OPTION='MASS_GENE');

RESGEN=MODE_ITER_SIMULT(MATR_RIGI=RIGIGENE,
                        MATR_MASS=MASSGENE,
                        SOLVEUR=_F(METHODE='MUMPS',
   ),
                        CALC_FREQ=_F(OPTION='BANDE',
                                     FREQ=(0.0,250.0)));



SKEL=DEFI_SQUELETTE(MODELE_GENE=MODEGENE,
                    RECO_GLOBAL=_F(TOUT='OUI'),
                    TOUT='OUI',
                    SOUS_STRUC=(
                                _F(NOM='TRI1',
                                   GROUP_MA='CALCUL'),
                                _F(NOM='TRI2',
                                   GROUP_MA='CALCUL'),
                                _F(NOM='TRI3',
                                   GROUP_MA='CALCUL'),
                                _F(NOM='TRI4',
                                   GROUP_MA='CALCUL')));

MODPHY=REST_SOUS_STRUC(RESU_GENE=RESGEN,
                      TOUT_ORDRE='OUI',
                      TOUT_CHAM='OUI',
                      SQUELETTE=SKEL,
                      );

