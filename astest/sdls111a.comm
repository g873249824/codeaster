                                     DRZ=0),
                                  _F(GROUP_NO='DROITE',
                                     DX=0,
                                     DY=0,
                                     DZ=0,
                                     DRX=0,
                                     DRY=0),
                                  _F(GROUP_NO='BLOQUE',
                                     DX=0,
                                     DY=0,
                                     DZ=0,
                                     DRX=0,
                                     DRY=0)));

MERIME_E=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=PARAM,
                        CHARGE=CHARGE_E);

MEMAME_E=CALC_MATR_ELEM(OPTION='MASS_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=PARAM,
                        CHARGE=CHARGE_E);

NUMDDL_E=NUME_DDL(MATR_RIGI=MERIME_E);

