DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

from math import *

# LECTURE ET CREATION DES MAILLAGES

MESH_LIN = LIRE_MAILLAGE(FORMAT = 'MED')

MESH_QUA = CREA_MAILLAGE(MAILLAGE  = MESH_LIN,
                         LINE_QUAD = _F(TOUT='OUI'));

MESH_Q9  = CREA_MAILLAGE(MAILLAGE=MESH_QUA,
                         MODI_MAILLE=_F(OPTION='QUAD8_9',
                                        PREF_NOEUD='NM',
                                        GROUP_MA='BULK_QUAD'))

# Closed-form solution

T0   = 0.0
w    = 2.0
tmax = 1.0
Tmax = T0*exp(-w*tmax) + (1-exp(-w*tmax))/w

# s(T) = 2 - 2*w * T

SOURCE = DEFI_FONCTION(NOM_PARA    = 'TEMP',
                       VALE        = (0,2, 1,2-2*w),
                       PROL_DROITE = 'LINEAIRE',
                       PROL_GAUCHE = 'LINEAIRE',
                      )
  
# Usual data setting

LAMBDA = DEFI_CONSTANTE(VALE = 0)
RHO_CP = DEFI_CONSTANTE(VALE = 2)

FAKE_MAT = DEFI_MATERIAU(THER_NL = _F(LAMBDA=LAMBDA, RHO_CP=RHO_CP),
                        )
# Computation
  
TIME = DEFI_LIST_REEL(DEBUT      =  0.0,
                      INTERVALLE = (_F(JUSQU_A = tmax, NOMBRE = 100),),
                     )
#===================================================================
# Verification des mailles de type TRIA3 avec une modelisation PLAN
#===================================================================

MAT_T3 = AFFE_MATERIAU(MAILLAGE = MESH_LIN,
                       AFFE     = _F(GROUP_MA = 'BULK_TRIA', MATER = FAKE_MAT),
                       )
  
# Initial temperature field
  
INI_T_T3 = CREA_CHAMP(OPERATION ='AFFE', 
                      TYPE_CHAM ='NOEU_TEMP_R',
                      MAILLAGE  = MESH_LIN,
                      AFFE      =_F(TOUT='OUI', NOM_CMP='TEMP', VALE=T0)
                     )

MODEL_T3 = AFFE_MODELE(MAILLAGE = MESH_LIN,
                       AFFE     = _F(GROUP_MA     = 'BULK_TRIA',
                                     PHENOMENE    = 'THERMIQUE',
                                     MODELISATION = 'PLAN'),
                      )
  
# Non linear heating source

HEAT_T3 = AFFE_CHAR_THER_F( MODELE  = MODEL_T3,
                            SOUR_NL = _F(GROUP_MA='BULK_TRIA', SOUR=SOURCE),    
                          )

EVOL_T3 = THER_NON_LINE( MODELE      = MODEL_T3,
                         INCREMENT   = _F(LIST_INST = TIME),
                         CHAM_MATER  = MAT_T3,
                         EXCIT       = (_F(CHARGE = HEAT_T3),),
                         ETAT_INIT   = _F(CHAM_NO = INI_T_T3),
                         CONVERGENCE = _F(RESI_GLOB_MAXI = 1.E-9),
                         NEWTON      = _F(REAC_ITER = 1),
                       )

# Result checking (middle of the rod)

TEST_RESU(RESU=_F(GROUP_NO='PROBE',
                  INST=1.0,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOL_T3,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.432147394089,
                  VALE_REFE=0.43233235838169365,),
          )
