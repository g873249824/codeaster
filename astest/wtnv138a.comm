#                AVEC COUPLAGE HYDRAULIQUE  CONFINEMENT DE 8 MPa
# ======================================================================
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),PAR_LOT='OUI',DEBUG=_F(SDVERI='OUI'))
#
BIDON=DEFI_CONSTANTE(    VALE=1.)

UN=DEFI_CONSTANTE(    VALE=1.)

ZERO=DEFI_CONSTANTE(    VALE=0.)

VISCOLIQ=DEFI_CONSTANTE(    VALE=1.E-3)

VISCOGAZ=DEFI_CONSTANTE(    VALE=1.8E-5)

DVISCOL=DEFI_CONSTANTE(    VALE=0.)

DVISCOG=DEFI_CONSTANTE(    VALE=0.)

KINT=DEFI_CONSTANTE(    VALE=1.E-18)

#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED', )

MODELE=AFFE_MODELE(     MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = '3D_HM',
                                   )
                          )

MAILLAGE=MODI_MAILLAGE( reuse=MAILLAGE,   MAILLAGE=MAILLAGE,
                       ORIE_PEAU_3D=_F(  GROUP_MA = ('HAUT', 'DEVANT', 'DROITE',),  )
                     )
MAILLAGE = DEFI_GROUP( reuse        = MAILLAGE,
                          MAILLAGE     = MAILLAGE,
                          CREA_GROUP_MA=(_F(NOM='ROCHE',
                                            TOUT='OUI',)),
                          CREA_GROUP_NO=(_F(NOM='ROCHES',
                                            CRIT_NOEUD='SOMMET',
                                            GROUP_MA='ROCHE'),),);

SOL=DEFI_MATERIAU(    ELAS=_F(  E     = 1500.E6,
                                RHO   = 2500.,
                                NU    = 0.27,
                                ALPHA = 1.E-5,
                              ),
                                VISC_DRUC_PRAG =_F(    PREF        =   0.1E6,
                                             A           =   1.5E-12,
                                             N           =   4.5,
                                             P_PIC       =   0.015,
                                             P_ULT       =   0.028,
                                             ALPHA_0     =   0.065,
                                             ALPHA_PIC   =   0.26,
                                             ALPHA_ULT   =   0.091,
                                             R_0         =   1.3021E6,
                                             R_PIC       =   6.24808E6,
                                             R_ULT       =   1.30808E6,
                                             BETA_0      =   -0.15,
                                             BETA_PIC    =    0.,
                                             BETA_ULT    =    0.13, ),
                               COMP_THM = 'LIQU_SATU',
                               THM_LIQU=_F(
                                RHO = 1000.,
#                               UN_SUR_K : 0.5E-9
                                UN_SUR_K = 0.0,
                                ALPHA = 1.E-4,
                                CP = 4180.,
                                VISC = VISCOLIQ,
                                D_VISC_TEMP = DVISCOL,
                                        ),
                              THM_GAZ=_F(
                                MASS_MOL = 28.96E-3,
                                CP = 1000.,
                                VISC = VISCOGAZ,
                                D_VISC_TEMP = DVISCOG,
                                  ),
                              THM_VAPE_GAZ=_F(
                                MASS_MOL = 18.E-3,
                                CP = 1870.,
                                VISC = BIDON,
                                D_VISC_TEMP = BIDON,
                                 ),
                              THM_DIFFU=_F(
                                R_GAZ = 8.315,
                                RHO = 2400.,
                                CP = 800.,
#                               BIOT_COEF : 0.7
                                BIOT_COEF = 1.,
                                SATU_PRES = UN,
                                D_SATU_PRES = ZERO,
                                PESA_X = 0.,
                                PESA_Y = 0.,
                                PESA_Z = 0.,
                                PERM_IN = KINT,
                                PERM_LIQU = UN,
                                D_PERM_LIQU_SATU = ZERO,
                                PERM_GAZ = UN,
                                D_PERM_SATU_GAZ = ZERO,
                                D_PERM_PRES_GAZ = ZERO,
                                 ),
                              THM_INIT=_F(
                                TEMP = 293.,
                                PRE1 = 1.0,
                                PRE2 = 0.1E6,
                                PORO = 0.14,
                                PRES_VAPE = 2269.8,
                                )
                           )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',    MATER = SOL,  )
                          )

PR_LATE=AFFE_CHAR_MECA(    MODELE=MODELE,
                              PRES_REP=_F(   GROUP_MA = ( 'DEVANT', 'DROITE', ),
                                          PRES = 1.,
                                    )
                       )

PR_V=AFFE_CHAR_MECA(    MODELE=MODELE,
                             PRES_REP=_F(   GROUP_MA = 'HAUT',    PRES = 1.,
                                    )
                        )

DEP_SYM=AFFE_CHAR_MECA(    MODELE=MODELE,FACE_IMPO=(
                              _F(  GROUP_MA = 'BAS',    DZ = 0.,   ),
                              _F(  GROUP_MA = 'DERRIERE',    DX = 0.,   ),
                              _F(  GROUP_MA = 'GAUCHE',    DY = 0.,   ))
                           )

P0=AFFE_CHAR_MECA(    MODELE=MODELE,
                               DDL_IMPO=_F( GROUP_NO='ROCHES',   PRE1 = 0.,  )
                           )

EVOL_HYD=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                         VALE=( 0.0,  0.0,
                                7000.0,  8.E6,)
                       )

TEMPS1=DEFI_LIST_REEL(   DEBUT=0.,
                            INTERVALLE=_F(  JUSQU_A = 7000.,     NOMBRE = 1,  )
                         )

U1=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = PR_LATE,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = PR_V,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = DEP_SYM,
                ), _F(
                CHARGE = P0,
                )),
                         COMPORTEMENT=_F(
                RELATION = 'KIT_HM',
                RELATION_KIT = (
                  'VISC_DRUC_PRAG', 'LIQU_SATU',
                  'HYDR_UTIL',  ),
                ),
                         INCREMENT=_F(
                LIST_INST = TEMPS1,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                )
              )

U1=CALC_CHAMP(reuse=U1,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=U1)


U1=CALC_CHAMP( reuse=U1,
                            CONTRAINTE='SIGM_NOEU',VARI_INTERNE='VARI_NOEU',
                          RESULTAT=U1,
              )

TEMPS2=DEFI_LIST_REEL(   DEBUT=7000.,
                         INTERVALLE=(_F(  JUSQU_A = 8000.,       NOMBRE = 100, ),
                                     _F(  JUSQU_A = 9000.,       NOMBRE = 100, ),
                                     _F(  JUSQU_A = 10000.,      NOMBRE = 100, ),
                                     _F(  JUSQU_A = 11000.,      NOMBRE = 100, ),
                                     _F(  JUSQU_A = 12000.,      NOMBRE = 100, ),
                                     _F(  JUSQU_A = 13000.,      NOMBRE = 100, ),
                                                        ))

ARCHI=DEFI_LIST_REEL(   DEBUT=7000.,
                         INTERVALLE=(_F(  JUSQU_A = 8000.,     NOMBRE = 10, ),
                                     _F(  JUSQU_A = 9000.,     NOMBRE = 10, ),
                                     _F(  JUSQU_A = 10000.,    NOMBRE = 10, ),
                                     _F(  JUSQU_A = 11000.,    NOMBRE = 10, ),
                                     _F(  JUSQU_A = 12000.,    NOMBRE = 10, ),
                                     _F(  JUSQU_A = 13000.,    NOMBRE = 10, ),

                   ))

DEP_V=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  GROUP_MA = 'HAUT',    DZ = 1.,   )
                           )

EVOL_DEV=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                         VALE=(  7000.0,     -2.4533E-3,
                                 8000.0,     -0.01,
                                 9000.0,     -0.02,
                                10000.0,     -0.03,
                                11000.0,     -0.04,
                                12000.0,     -0.05,
                                13000.0,     -0.06,
                       ), )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************


U2=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = DEP_SYM,
                ), _F(
                CHARGE = PR_LATE,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = DEP_V,
                FONC_MULT = EVOL_DEV,
                )),
                         COMPORTEMENT=_F(
                RELATION = 'KIT_HM',
                RELATION_KIT = (
                  'VISC_DRUC_PRAG',        'LIQU_SATU',
                  'HYDR_UTIL',  ),
                ),
                         ETAT_INIT=_F(
                EVOL_NOLI = U1,
                NUME_ORDRE = 1,
                ),
                         INCREMENT=_F(
                LIST_INST = TEMPS2,
                INST_FIN = 13000.),
                ARCHIVAGE = _F(LIST_INST=ARCHI),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER=1,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 30,
                )
              )

U2=CALC_CHAMP(reuse=U2,CRITERES=('SIEQ_ELNO'),VARI_INTERNE=('VARI_ELNO'),CONTRAINTE=('SIGM_ELNO'),RESULTAT=U2)


U2=CALC_CHAMP(  reuse=U2,
             RESULTAT=U2,
             CONTRAINTE='SIGM_NOEU',CRITERES='SIEQ_NOEU',VARI_INTERNE='VARI_NOEU'
          )


TFIN1 = 7000.
TFIN2 = 13000.

TEST_RESU(RESU=(_F(INST=7000.0,
                   RESULTAT=U1,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='DX',
                   VALE_CALC=-2.4533333333333E-3,
                   ),
                _F(INST=1.3000E4,
                   RESULTAT=U2,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='DX',
                   VALE_CALC=0.02632,
                   ),
                ),
          )

TEST_RESU(RESU=(_F(INST=7000.0,
                   RESULTAT=U1,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIYY',
                   VALE_CALC=-8.E6,
                   ),
                _F(INST=1.3000E4,
                   RESULTAT=U2,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='NO8',
                   NOM_CMP='SIYY',
                   VALE_CALC=-1.4279826468604E7,
                   ),
                ),
          )

FIN()
