                   VALE_REFE=14.1271,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(NUME_ORDRE=15,
                   RESULTAT=SOLNL,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N1',
