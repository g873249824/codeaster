DEBUT(CODE=_F(NOM='SSLS09C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'), MEMOIRE=_F(DYNAMIQUE=400,))

MAIL=LIRE_MAILLAGE();
#
# DEFINITION DES GROUPES DE NOEUDS

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='APPUI',),
                               _F(GROUP_MA='LEF',),
                               _F(GROUP_MA='LCD',),),);
#
# DEFINITION DU MODELE

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='AXIS_INCO',),);
#
# DEFINITION DU MATERIAU

ACIER=DEFI_MATERIAU(ELAS=_F(E=210000000000.0,
                            NU=0.3,
                            RHO=8000.0,
                            ALPHA=1e-05,),);
#
# ATTRIBUTION DU MATERIAU

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);
#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS

CLIM=AFFE_CHAR_MECA(MODELE=MODELE,
                    DDL_IMPO=_F(GROUP_NO='APPUI',
                                DY=0.0,),);
#

F_Y=DEFI_FONCTION(NOM_PARA='Y',
                  VALE=(0.0,-78500.0,3.0,-78500.0,6.0,-78500.0,),);

CHARGT=AFFE_CHAR_MECA_F(MODELE=MODELE,
                        FORCE_INTERNE=_F(TOUT='OUI',
                                         FY=F_Y,),);

CH_PESA=AFFE_CHAR_MECA(MODELE=MODELE,
                       PESANTEUR=(9.8125,0.0,-1.0,0.0,),);
#
# RESOLUTION
#DEF=MECA_STATIQUE(    MODELE=MODELE,
#                        CHAM_MATER=CHMAT,EXCIT=(
#                        _F(  CHARGE = CLIM),
#                              _F(  CHARGE = CHARGT))
#                    )

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0,),);
# CALCUL
#========

DEF=STAT_NON_LINE(MODELE=MODELE,
         SOLVEUR=_F(METHODE='MUMPS',PARALLELISME='DISTRIBUE_MD',PCENT_PIVOT=900.),
                  CHAM_MATER=CHMAT,
                  EXCIT=(_F(CHARGE=CH_PESA,
                            FONC_MULT=RAMPE,),
                         _F(CHARGE=CLIM,),),
                  COMP_ELAS=_F(RELATION='ELAS',),
                  INCREMENT=_F(LIST_INST=INSTANTS,),
                  NEWTON=_F(REAC_ITER=1,),);

DEF=CALC_ELEM(reuse =DEF,
              MODELE=MODELE,
              CHAM_MATER=CHMAT,
              RESULTAT=DEF,
              OPTION=('SIEF_ELNO_ELGA','ERRE_ELEM_SIGM','ERRE_ELNO_ELEM',),
              EXCIT=(
              _F(CHARGE=CLIM,),
              _F(CHARGE=CH_PESA,
                 FONC_MULT=RAMPE,),),);
