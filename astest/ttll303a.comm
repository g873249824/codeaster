SOURCE=DEFI_CONSTANTE(   VALE=1.E6)

#

MATE=DEFI_MATERIAU( THER=_F(  LAMBDA = 100.E0,
                                   RHO_CP = 7000.E0))

#
# AFFECTATIONS: MATERIAU, MODELE, CHARGEMENT ----------------------
#

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                 AFFE=_F(  TOUT = 'OUI', MATER = MATE) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
               AFFE=_F( TOUT = 'OUI', MODELISATION = 'PLAN',
                                   PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(MODELE=MOTH,
          TEMP_IMPO=_F( GROUP_NO = 'NOE_IMP', TEMP = TEMP_IMP),
             SOURCE=_F( TOUT = 'OUI', SOUR = SOURCE)
             )

#
# DEFINITION DE LA STATEGIE DE CALCUL -----------------------------
#

LR8=DEFI_LIST_REEL(        DEBUT=0.,
                   INTERVALLE=_F(    JUSQU_A = 0.50,    NOMBRE = 50)
                  )

#
# RESOLUTION ------------------------------------------------------
#

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
                      TEMP_INIT=_F( VALE = 0.E0),
                      INCREMENT=_F( LIST_INST = LR8),
                       CHAM_MATER=CHMAT,
                          EXCIT=_F( CHARGE = CHTH)
