DEBUT(CODE=_F(NOM='SSNP143A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),PAR_LOT='OUI',DEBUG=_F(SDVERI='OUI'))

######## PARAMETRES #############
tfin   = 3
nbpas  = tfin
nbarch = tfin

# PARAMETRES MECANIQUES DES PLOTS (VALEURS TESTS)
young = 3.E12
poiss = 0.25
rho_b = 2400
ap    = 9.81
h     = 10.

# PARAMETRES DES JOINTS (VALEURS TESTS)
k_n    = 1E12
sig_m  = 0.0
alpha  = 1.
penrup = 0.2
av_cla = 1
i_clav = 2.0
ap_cla = tfin
eps    = 1.E-5

# CLAVAGE EN FONCTION DU TEMPS
cla_ins=DEFI_FONCTION(NOM_PARA='INST',VALE=( 0.,-1. , i_clav-eps,-1 , i_clav,1. , i_clav+eps,-1 ,  tfin,-1.));
# CLAVAGE EN FONCTION DE L'ESPACE
cla_y = FORMULE(NOM_PARA=('Y',),VALE='5.0E4');
# CLAVAGE FONCTION DU TEMPS ET DE L'ESPACE
fonc_cla = FORMULE(NOM_PARA=('Y','INST'),VALE='cla_y(Y)*cla_ins(INST)');

##################################

MA=LIRE_MAILLAGE(FORMAT='MED');

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,

  CREA_GROUP_MA = (
      _F(NOM = 'JOINT',    GROUP_MA = 'G_2D_14'),
      _F(NOM = 'BARRAGE',  GROUP_MA = 'G_2D_15'),
      _F(NOM = 'FONDATIO',GROUP_MA = 'G_1D_16'),
      _F(NOM = 'JOINT_G',  GROUP_MA = 'G_1D_17'),
      _F(NOM = 'JOINT_D',  GROUP_MA = 'G_1D_18'),

          ),
  CREA_GROUP_NO = (
      _F(NOM = 'JOINT_G', OPTION = 'NOEUD_ORDO',   GROUP_MA = 'JOINT_G'),
      _F(NOM = 'JOINT_D', OPTION = 'NOEUD_ORDO',   GROUP_MA = 'JOINT_D'),
          ),
    );

MA=MODI_MAILLAGE(reuse =MA,
                   MAILLAGE=MA,
                   ORIE_FISSURE=_F(GROUP_MA='JOINT'),
                   INFO=1)


MO=AFFE_MODELE(MAILLAGE=MA,
               VERIF='MAILLE',
               INFO=1,
               AFFE=(_F(GROUP_MA=('BARRAGE',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN',),
                     _F(GROUP_MA='JOINT',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='PLAN_JOINT',),),);



# MATERIAU DES PLOTS
MATPLO=DEFI_MATERIAU(ELAS=_F(
                              E   = young,
                              NU  = poiss,
                              RHO = rho_b),)

# MATERIAU JOINT
MATJOI=DEFI_MATERIAU(
                  JOINT_MECA_RUPT=_F(K_N=k_n,
                                     K_T=k_n,
                                     SIGM_MAX=sig_m,
                                     PENA_RUPTURE=penrup,
                                     ALPHA=alpha,
                                     PRES_CLAVAGE=fonc_cla,
                          ),)

CM=AFFE_MATERIAU(MAILLAGE=MA,
                 AFFE=(
                      _F(GROUP_MA='BARRAGE',MATER = MATPLO),
                      _F(GROUP_MA='JOINT',  MATER = MATJOI))
                 );

# CONDITION AUX LIMITES SUR LA TERRE
CL_TER=AFFE_CHAR_MECA(MODELE=MO,
                       FACE_IMPO=_F(GROUP_MA=('FONDATIO',),DX=0.0,DY=0.0,),);

# CHAMPS DE PESANTEUR
CH_PES = AFFE_CHAR_MECA(
  MODELE = MO,
  FORCE_INTERNE = (_F(GROUP_MA=('BARRAGE',),FX=0.0, FY=-rho_b*ap)));

# CREATION DE LISTE D'INSTANT
L_INST=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=tfin, NOMBRE=nbpas,),);
L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=tfin, NOMBRE=nbarch,),);

# CALCUL
U_1=STAT_NON_LINE(MODELE=MO,
                  CHAM_MATER=CM,
                  EXCIT=(_F(CHARGE=CL_TER),
                         _F(CHARGE=CH_PES),),
                  COMP_INCR=(_F(RELATION='JOINT_MECA_RUPT',
                                GROUP_MA='JOINT',),
                             _F(RELATION='ELAS',
                                GROUP_MA=('BARRAGE')),),
                  INCREMENT=_F(LIST_INST=L_INST,
                               INST_FIN=tfin,),
                  NEWTON=_F(MATRICE='TANGENTE',
                            REAC_ITER=1,),
                  CONVERGENCE=_F(ITER_GLOB_MAXI=900,
                                 RESI_GLOB_RELA=1E-5),
                  SOLVEUR=_F(METHODE='MUMPS',),
                  ARCHIVAGE = _F(LIST_INST = L_ARCH,),
                  );


############# POST TRAITEMENT #################

U_1=CALC_CHAMP(reuse =U_1,

              RESULTAT=U_1,
               VARI_INTERNE=('VARI_ELNO'),
              GROUP_MA='JOINT');

U_1=CALC_CHAMP(reuse =U_1,

              RESULTAT=U_1,
               CONTRAINTE=('SIEF_ELNO'),
              GROUP_MA='JOINT');


U_1=CALC_CHAMP(reuse =U_1,
            RESULTAT=U_1,
            VARI_INTERNE='VARI_NOEU',CONTRAINTE='SIEF_NOEU',
            GROUP_MA='JOINT');


# AVANT CLAVAGE
################

# OUVERTURE ET VARIABLE INTERNE DE CLAVAGE
VAR_AV=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='SAUT_N',
                               RESULTAT=U_1,
                               NOM_CHAM='VARI_NOEU',
                               INST=av_cla,
                               GROUP_NO='JOINT_D',
                               NOM_CMP=('V7','V10'),),);


# CONTRAINTE NORMALE DANS LE JOINT
SIG_AV=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='SIG_N',
                               RESULTAT=U_1,
                               NOM_CHAM='SIEF_NOEU',
                               INST=av_cla,
                               GROUP_NO = 'JOINT_D',
                               NOM_CMP=('SIGN')),);


# APRES CLAVAGE
################

# OUVERTURE ET VARIABLE INTERNE DE CLAVAGE
VAR_AP=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='SAUT_N',
                               RESULTAT=U_1,
                               NOM_CHAM='VARI_NOEU',
                               INST=ap_cla,
                               GROUP_NO='JOINT_D',
                               NOM_CMP=('V7','V10')),);



# CONTRAINTE NORMALE DANS LE JOINT
SIG_AP=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='SIG_N',
                               RESULTAT=U_1,
                               NOM_CHAM='SIEF_NOEU',
                               INST=ap_cla,
                               GROUP_NO = 'JOINT_D',
                               NOM_CMP=('SIGN')),);



################### TESTS COMPARAISON GEFDYN ####################

TEST_TABLE(REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.059999999999999998,
           VALE_CALC= 6.02102542E-07,
           VALE_REFE=6.3799999999999997E-07,
           NOM_PARA='V7',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=2.0,),
           )

TEST_TABLE(REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.070000000000000007,
           VALE_CALC= 2.00318942E-06,
           VALE_REFE=2.1399999999999998E-06,
           NOM_PARA='V7',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=5.0,),
           )

TEST_TABLE(REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.070000000000000007,
           VALE_CALC= 3.62260622E-06,
           VALE_REFE=3.8800000000000001E-06,
           NOM_PARA='V7',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=8.0,),
           )

################ TESTS NON REGRESSION ###########################

TEST_TABLE(
           VALE_CALC=6.0216299999999996E-07,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='V7',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=2.0,),
           )

TEST_TABLE(
           VALE_CALC=6.5216300000000001E-07,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='V10',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=2.0,),
           )

TEST_TABLE(
           VALE_CALC=-4.9999900000000001E4,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='SIGN',
           TABLE=SIG_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=2.0,),
           )

TEST_TABLE(
           VALE_CALC=1.7436900000000001E-06,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='V7',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=4.5,),
           )

TEST_TABLE(
           VALE_CALC=1.79369E-06,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='V10',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=4.5,),
           )

TEST_TABLE(
           VALE_CALC=-4.9999699999999997E4,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='SIGN',
           TABLE=SIG_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=4.5,),
           )

TEST_TABLE(
           VALE_CALC=3.89115E-06,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='V7',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=8.5,),
           )

TEST_TABLE(
           VALE_CALC=3.9411500000000004E-06,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='V10',
           TABLE=VAR_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=8.5,),
           )

TEST_TABLE(
           VALE_CALC=-4.9999199999999997E4,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='SIGN',
           TABLE=SIG_AP,
           FILTRE=_F(NOM_PARA='COOR_Y',
                     VALE=8.5,),
           )

IMPR_TABLE(TABLE=VAR_AV,FORMAT='TABLEAU',NOM_PARA=('COOR_Y','V7','V10'),);
IMPR_TABLE(TABLE=SIG_AV,FORMAT='TABLEAU',NOM_PARA=('COOR_Y','SIGN',),);
IMPR_TABLE(TABLE=VAR_AP ,FORMAT='TABLEAU',NOM_PARA=('COOR_Y','V7','V10'),);
IMPR_TABLE(TABLE=SIG_AP, FORMAT='TABLEAU',NOM_PARA=('COOR_Y','SIGN',),);

FIN()
