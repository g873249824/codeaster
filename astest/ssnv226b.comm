DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),)


CUBE=LIRE_MAILLAGE(FORMAT="ASTER",);

PRECIS=1.E-6

CUBE=DEFI_GROUP(reuse =CUBE,
              MAILLAGE=CUBE,
              DETR_GROUP_NO=_F(NOM=('GAUCHE','ARRIERE','HAUT','BAS',),),
              CREA_GROUP_NO=(_F(OPTION='PLAN',
                                NOM='BAS',
                                POINT=(0.0,0.0,0.0,),
                                VECT_NORMALE=(0.0,0.0,1.0,),
                                PRECISION=PRECIS,),
                             _F(OPTION='PLAN',
                                NOM='HAUT',
                                POINT=(0.0,0.0,1.0,),
                                VECT_NORMALE=(0.0,0.0,1.0,),
                                PRECISION=PRECIS,),
                             _F(OPTION='PLAN',
                                NOM='GAUCHE',
                                POINT=(0.0,0.0,0.0,),
                                VECT_NORMALE=(0.0,1.0,0.0,),
                                PRECISION=PRECIS,),
                             _F(OPTION='PLAN',
                                NOM='ARRIERE',
                                POINT=(0.0,0.0,0.0,),
                                VECT_NORMALE=(1.0,0.0,0.0,),
                                PRECISION=PRECIS,),),);



SY = 750.E6;
E=2.E11
Eps0=SY/E

CTRAC=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(Eps0, SY,  1., 1500.E6))

ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                               NU=0.3,
                               RHO=7800.,
                             ALPHA=0.0,),
                   CRIT_RUPT=_F(SIGM_C=7.8E8,
                                COEF=10000.,),
                   TRACTION=_F(SIGM=CTRAC,),
                                 );

TROISD=AFFE_MODELE(MAILLAGE=CUBE,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);



MAT=AFFE_MATERIAU(MAILLAGE=CUBE,
                  AFFE=_F(TOUT='OUI',
                          MATER=ACIER,),);

TRAC=AFFE_CHAR_MECA(MODELE=TROISD,
                     DDL_IMPO=(
                     _F(GROUP_NO='ARRIERE',
                                 DX=0.0,),
                        _F(GROUP_NO='BAS',
                                 DZ=0.0,),
                       _F(GROUP_NO='GAUCHE',
                                 DY=0.0,),
                       _F(GROUP_NO='HAUT',
                                   DZ=1.0),
                                   ),)
##chargement
EpsMax=0.05
viteps=1.e-4
tmax=EpsMax/viteps

EPSZZ=DEFI_FONCTION(
                      NOM_PARA='INST',  NOM_RESU='EPSI',
                       VALE=(  0. , 0., tmax , EpsMax),
                       PROL_DROITE='LINEAIRE',
                        )



LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=tmax,
                                    NOMBRE=200,),
                                    ),);

DEFLIST1=DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST=LINST,),)


SOLNL=STAT_NON_LINE( MODELE=TROISD,
                     CHAM_MATER=MAT,
                     EXCIT=_F(CHARGE=TRAC,FONC_MULT=EPSZZ),
                     INCREMENT=_F(LIST_INST=DEFLIST1),
                     COMPORTEMENT=_F(RELATION='VMIS_ISOT_TRAC',
                                  POST_ITER='CRIT_RUPT',
                                  ),
                     NEWTON=_F(REAC_ITER=1,PREDICTION='ELASTIQUE',
                              ),
                     SUIVI_DDL= (
                               _F(NOM_CMP    ='V1',
                                  NOM_CHAM   ='VARI_ELGA',
                                  TOUT='OUI',
                                  EVAL_ELGA  ='MAX',
                                  EVAL_CHAM  ='MAX',),
                               _F(NOM_CMP    ='V8',
                                  NOM_CHAM   ='VARI_ELGA',
                                  TOUT='OUI',
                                  EVAL_ELGA  ='MAX',
                                  EVAL_CHAM  ='MAX',),
                               _F(NOM_CMP    ='SIZZ',
                                  NOM_CHAM   ='SIEF_ELGA',
                                  TOUT='OUI',
                                  EVAL_ELGA  ='MAX',
                                  EVAL_CHAM  ='MAX',),
                                  ),
                    );



SOLNL=CALC_CHAMP(reuse =SOLNL,
                 RESULTAT=SOLNL,
                 DEFORMATION=('EPSI_ELGA','EPSP_ELGA',),);


# Calcul des moyennes

INT_SI=POST_ELEM(MODELE=TROISD,
                   RESULTAT=SOLNL,
                   INTEGRALE=_F(NOM_CHAM='SIEF_ELGA',
                                NOM_CMP=('SIXX','SIYY','SIZZ'),
                                TOUT='OUI',
                                TYPE_MAILLE='3D'),)

IMPR_TABLE(TABLE=INT_SI)

INT_EP=POST_ELEM(MODELE=TROISD,
                   RESULTAT=SOLNL,
                   INTEGRALE=_F(NOM_CHAM='EPSI_ELGA',
                                NOM_CMP=('EPXX','EPYY','EPZZ'),
                                TOUT='OUI',
                                TYPE_MAILLE='3D'),)

IMPR_TABLE(TABLE=INT_EP)





F_SI=RECU_FONCTION(TABLE=INT_SI,
                           PARA_X='INST',
                           PARA_Y='INTE_SIZZ',);

F_EP=RECU_FONCTION(TABLE=INT_EP,
                           PARA_X='INST',
                           PARA_Y='INTE_EPZZ',);


IMPR_FONCTION(
FORMAT ='XMGRACE',
UNITE=40,
#PILOTE='INTERACTIF',
COURBE=_F(FONC_X=F_EP, FONC_Y=F_SI))



INT_VI=POST_ELEM(MODELE=TROISD,
                   RESULTAT=SOLNL,
                   INTEGRALE=_F(NOM_CHAM='VARI_ELGA',
                                NOM_CMP=('V1','V3','V4','V5','V6','V7','V8'),
                                TOUT='OUI',
                                TYPE_MAILLE='3D'),)

IMPR_TABLE(TABLE=INT_VI)


TEST_FONCTION(VALEUR=_F(VALE_REFE=7.80113E8,
                        VALE_CALC=7.80113E8,
                        REFERENCE='ANALYTIQUE',
                        VALE_PARA=437.5,
                        NOM_PARA='INST',
                        FONCTION=F_SI,
                        ),
              )

TEST_FONCTION(VALEUR=_F(VALE_REFE=5000.0,
                        VALE_CALC=5000.0,
                        REFERENCE='ANALYTIQUE',
                        VALE_PARA=440.0,
                        NOM_PARA='INST',
                        FONCTION=F_SI,
                        ),
              )

FIN()
