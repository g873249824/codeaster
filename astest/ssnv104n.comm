DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA0=LIRE_MAILLAGE(FORMAT='MED',)

MA0=MODI_MAILLAGE(reuse=MA0,
                  MAILLAGE=MA0,
                  ORIE_PEAU_2D=(_F(GROUP_MA='CON1',),
                                _F(GROUP_MA='CON2',),),);

MA=CREA_MAILLAGE(MAILLAGE=MA0,
                 DECOUPE_LAC=_F(GROUP_MA_ESCL=('CON2'),),);

MO=AFFE_MODELE(
                           MAILLAGE=MA,
                          AFFE=_F(
                                TOUT = 'OUI',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'AXIS')
                        )

#

MAT=DEFI_MATERIAU(
                            ELAS=_F(
                                  E = 20000.0,
                                  NU = 0.3,
                                  ALPHA = 0.01)
                          )

#

CHMAT=AFFE_MATERIAU(
                            MAILLAGE=MA,
                           AFFE=_F(
                                 TOUT = 'OUI',
                                 MATER = MAT)
                           )

#
CHAR=AFFE_CHAR_MECA(
                     MODELE=MO,DDL_IMPO=(
                     _F(  GROUP_NO = 'SPHSUP',  DY = -2.0),
                              _F(  GROUP_NO = 'SPHINF',  DY = 2.0),
                              _F(  GROUP_NO = 'AXE',     DX = 0.0)),

                   )


CHA2=DEFI_CONTACT(MODELE=MO,
                  FORMULATION='CONTINUE',
                  ALGO_RESO_CONT = 'NEWTON',
                  ALGO_RESO_GEOM = 'NEWTON',
                  LISSAGE='OUI',
                  ZONE=_F(ALGO_CONT='LAC',
                          GROUP_MA_MAIT='CON1',
                          GROUP_MA_ESCL='CON2',
                          CONTACT_INIT='OUI',
                          ),);


RAMPE=DEFI_FONCTION(   NOM_PARA='INST',
            PROL_GAUCHE='LINEAIRE',   PROL_DROITE='LINEAIRE',
            VALE=(0.0,0.0,1.0,1.0,) )

#

L_INST=DEFI_LIST_REEL( DEBUT=0.0,
                       INTERVALLE=_F( JUSQU_A = 1., NOMBRE = 10)
                           )
#

RESU2=STAT_NON_LINE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    EXCIT=(
                           _F(CHARGE=CHAR,
                            FONC_MULT=RAMPE,),),
                    CONTACT=CHA2,
                    COMPORTEMENT=_F(RELATION='ELAS',
                                    DEFORMATION='PETIT',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=50,
                                RESI_GLOB_RELA=1.E-6),
                    SOLVEUR=_F(ELIM_LAGR='NON',
                            METHODE='MUMPS',
                            RESI_RELA=1.E-6,
                            PCENT_PIVOT= 30),
                        NEWTON=_F(REAC_ITER=1,),);


RESU2=CALC_CHAMP(reuse=RESU2,CONTRAINTE=('SIGM_ELNO'),RESULTAT=RESU2)

RESU2=CALC_CHAMP(reuse =RESU2,CONTRAINTE='SIGM_NOEU',RESULTAT=RESU2)


SIELNO2=CREA_CHAMP(TYPE_CHAM='NOEU_SIEF_R', OPERATION='EXTR', RESULTAT=RESU2,
                  NOM_CHAM='SIGM_NOEU', INST=1.,);


# TEST DE LA CONFORMITE AVEC LA SOLUTION DE REFERENCE ANALYTIQUE


TEST_RESU(CHAM_NO=_F(CRITERE='RELATIF',
                     REFERENCE='ANALYTIQUE',
                     NOM_CMP='SIYY',
                     GROUP_NO='G1_1',
                     PRECISION=0.05,
                     CHAM_GD=SIELNO2,
                     VALE_CALC=-2912.06041838,
                     VALE_REFE=-2798.3,),
          )

TEST_RESU(CHAM_NO=_F(CRITERE='RELATIF',
                     REFERENCE='ANALYTIQUE',
                     NOM_CMP='SIYY',
                     GROUP_NO='G1_2',
                     PRECISION=0.05,
                     CHAM_GD=SIELNO2,
                     VALE_CALC=-2911.29604194,
                     VALE_REFE=-2798.3,),
          )

FIN()
