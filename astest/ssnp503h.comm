# CONTACT-FROTTANT EN GRANDS GLISSEMENTS
# AVEC XFEM POUR DES FISSURES HORIZONTALES
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

MAILLAG2=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
Y=3

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,Y,0.0),
                                       VECT_NORMALE = (0.0,1.0,0.0),
                                       PRECISION = 1E-1)))
MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU2',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,6,0.0),
                                       VECT_NORMALE = (0.0,1.0,0.0),
                                       PRECISION = 1E-1)))

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOLU'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D')));

LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y-2.5');

LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-Y+6.5');

FISS1=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1));

FISS2=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN2));

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='OUI',
                         INFO=1);


CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      FROTTEMENT     = 'COULOMB',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 10,
                      ITER_GEOM_MAXI = 10,
                      REAC_GEOM      = 'AUTOMATIQUE',ELIM_ARETE     = 'ELIM',
                      ZONE=(
                            _F(
                               FISS_MAIT    = FISS1,
                               INTEGRATION  = 'NOEUD',
                               CONTACT_INIT = 'OUI',
                               COULOMB      = 0.5,
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,
                               ALGO_FROT    = 'STANDARD',
                               COEF_FROT    = 100.,
                               ALGO_LAGR    = 'VERSION2',
                               TOLE_PROJ_EXT =0.1,
                             ),
                            _F(
                               FISS_MAIT    = FISS2,
                               INTEGRATION  = 'NOEUD',
                               CONTACT_INIT = 'OUI',
                               COULOMB      = 0.5,
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,
                               ALGO_FROT    = 'STANDARD',
                               COEF_FROT    = 100.,
                               ALGO_LAGR    = 'VERSION2',
                               TOLE_PROJ_EXT =0.1,
                             ),
                         ),
                   );


E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER));

DEPX=2.00
DEPY=1.E-3
DEPZ=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SINF',DY=DEPY,DZ=0.0),
                             _F(GROUP_MA='SSUP',DY=-DEPY,DZ=0.0),
                             _F(GROUP_MA=('L1','L2','L3','L4'),
                                DX=0.0),
                             ),
                   INFO=1);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SCOTE',
                               DX=DEPX,DZ=DEPZ)),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                   LIAISON_DDL=(_F(NOEUD=('N9','N9'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N29','N29'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N45','N45'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N65','N65'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N9','N9'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPZ),
                               _F(NOEUD=('N29','N29'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPZ),
                               _F(NOEUD=('N45','N45'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPZ),
                               _F(NOEUD=('N65','N65'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPZ)),
                    INFO=1);

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(_F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2)),
                    CONTACT  = CTXFEM,
                    NEWTON=_F(REAC_ITER=1),
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 GROUP_MA='VOLU'),
                    INCREMENT=_F(LIST_INST=L_INST),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06
                                    )),
                    SOLVEUR=_F(METHODE='MUMPS',
   NPREC=-1),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                    INFO=1);

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]

# Les dernieres valeurs sont multipliees par 1.5 car en XFEM, les facettes
# de contact sont des triangles alors qu'en methode continue ce sont
# des quadrangles. Cet artifice permet de retrouver exactement la
# valeur calculee par la methode continue.
vale = [[0.0,-7.4394583764921E+03,-5.5601370655444E+03,-5.7894273952134E+03,-3.8723294509650E+03*1.5],
        [0.0,-9.9465543792087E+03,-1.1457450319764E+04,-1.0644746695273E+04,-8.1152552633727E+03*1.5],
        [0.0,0.0,-2.0437613658808E+04,-1.6583379369957E+04,-1.1024368301123E+04*1.5],
        [0.0,0.0,-1.8510255493553E+04,-2.1051213541175E+04,-1.6537310657554E+04*1.5]]

VALE_CALC=[
[ 0.0E+00,
 -7439.464576505,
 -5560.1137597783,
 -5789.4376373649,
 -5808.4928066125,],
[ 0.0E+00,
 -9946.9278039094,
 -11457.634442624,
 -10645.173992008,
 -12173.296230105,],
[ 0.0E+00,
 0.0E+00,
 -20438.787331975,
 -16584.583081565,
 -16537.570248897,],
[ 0.0E+00,
 0.0E+00,
 -18513.17282699,
 -21053.131834701,
 -24807.937248773,],
]

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO='NFISSU',
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2'),
                                         OPERATION='EXTRACTION'));
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              NOM_PARA=('NOEUD','INST','COOR_X','COOR_Z','LAGS_C','LAGS_F1','LAGS_F2'),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X'),
                     ORDRE='CROISSANT'));

   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-10
        critere = 'ABSOLU'
      else:
        precision = 1.0E-2
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE_CALC=VALE_CALC[i-1][j], VALE_REFE=vale[i-1][j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=(_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU'),
                        _F(NOM_PARA='COOR_Z',
                           CRIT_COMP='EQ',
                           VALE=0.0,
                           PRECISION=0.01,
                           CRITERE='ABSOLU')));

MA_XFEM=POST_MAIL_XFEM(
                       MODELE=MODELEK,
                       PREF_NOEUD_X = 'nx',
                       PREF_NOEUD_M = 'nm',
                       PREF_NOEUD_P = 'np',
                       PREF_MAILLE_X = 'mx');

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                   AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D'))

RES_XFEM=POST_CHAM_XFEM(
                  MODELE_VISU= MOD_VISU,
                  RESULTAT=UTOT1,
                  INFO =1)

#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=RES_XFEM,NOM_CHAM='DEPL',TYPE_CHAM='VECT_3D',NOM_CMP=('DX','DY','DZ')))),

FIN();
