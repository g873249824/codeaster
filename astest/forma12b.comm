# RESPONSABLE BOYERE E.BOYERE

DEBUT(CODE=_F(NOM='FORMA12B',
              NIV_PUB_WEB='INTERNET',),);

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_MA=_F(NOM='TOUT',
                               TOUT='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='POU_D_E',),);

CARELEM=AFFE_CARA_ELEM(MODELE=MO,
                       POUTRE=_F(GROUP_MA='TOUT',
                                 SECTION='RECTANGLE',
                                 CARA=('HY','HZ',),
                                 VALE=(0.05,0.05,),),);

SY=150.E6

HY=0.05
HZ=0.05


NP= HY*HZ*SY
MEY= SY*HY*HZ*HZ/6.;
MPY= 1.5*MEY ;

MAT=DEFI_MATERIAU(ELAS=_F(E=210000000000.0,
                          NU=0.3,
                          RHO=7800.0,),
                  ECRO_LINE=_F(D_SIGM_EPSI=90000000000.0,
                               SY=150000000.0,),
                  VMIS_POUTRE=_F(NP=NP,
                                 MEY=MEY,
                                 MPY=MPY,
                                 CAY=0.84,
                                 CBY=0.002,
                                 MEZ=MEY,
                                 MPZ=MPY,
                                 CAZ=0.84,
                                 CBZ=0.002,
                                 MPX=10000000000.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

CH1=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=_F(NOEUD='A',
                               DX=0.0,
                               DY=0.0,
                               DZ=0.0,
                               DRX=0.0,
                               DRY=0.0,
                               DRZ=0.0,),);
# ------------------------------------------------------------------
# MODELISATION
# CALCUL DES MATRICES ELEMENTAIRES DE RIGIDITE ET DE MASSE
#
#
#

MELR1=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                     MODELE=MO,
                     CHAM_MATER=CHMAT,
                     CARA_ELEM=CARELEM,
                     CHARGE=CH1,);

MELM1=CALC_MATR_ELEM(OPTION='MASS_MECA',
                     MODELE=MO,
                     CHAM_MATER=CHMAT,
                     CARA_ELEM=CARELEM,
                     CHARGE=CH1,);
# NUMEROTATION
#
#
#

NUM1=NUME_DDL(MATR_RIGI=MELR1,);
# ASSEMBLAGE DES MATRICES DE RIGIDITE ET DE MASSE
#
#
#

MATASSR1=ASSE_MATRICE(MATR_ELEM=MELR1,
                      NUME_DDL=NUM1,);

MATASSM1=ASSE_MATRICE(MATR_ELEM=MELM1,
                      NUME_DDL=NUM1,);
#--------------------------------------------------------------------
# ANALYSE MODALE
#
#
#

FREQ=MODE_ITER_SIMULT(MATR_A=MATASSR1,
                      MATR_B=MATASSM1,
                      METHODE='SORENSEN',
                      CALC_FREQ=_F(OPTION='PLUS_PETITE',
                                   NMAX_FREQ=11,),
                      TITRE='1/ METHODE DE SORENSEN',);
# TEST_RESU UNIQUEMENT POUR FAIRE CAS TEST

TEST_RESU(RESU=_F(RESULTAT=FREQ,
                  NUME_ORDRE=1,
                  PARA='FREQ',
                  VALE=167.638,
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='7.3.19',),);

EXCIT=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_NODALE=_F(NOEUD='B',
                                     FY=10000.0,),);

EVOLF = FORMULE(VALE='sin(121*INST)',
                NOM_PARA='INST',);

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=0.2,
                                   PAS=0.001,),);

FELEM=CALC_VECT_ELEM(OPTION='CHAR_MECA',
                     CHARGE=EXCIT,
                     CARA_ELEM=CARELEM,);

FASSE=ASSE_VECTEUR(VECT_ELEM=FELEM,
                   NUME_DDL=NUM1,);

MACRO_PROJ_BASE(BASE=FREQ,
                NB_VECT=5,
                MATR_ASSE_GENE=(_F(MATRICE=CO('MASSGENE'),
                                   MATR_ASSE=MATASSM1,),
                                _F(MATRICE=CO('RIGIGENE'),
                                   MATR_ASSE=MATASSR1,),),
                VECT_ASSE_GENE=_F(VECTEUR=CO('FEGENE'),
                                  VECT_ASSE=FASSE,),);

DYNAEL=DYNA_NON_LINE(MODELE=MO,
                     CHAM_MATER=CHMAT,
                     CARA_ELEM=CARELEM,
                     EXCIT=(_F(CHARGE=CH1,),
                            _F(CHARGE=EXCIT,
                               FONC_MULT=EVOLF,),),
                     AMOR_MODAL=_F(MODE_MECA=FREQ,
                                   AMOR_REDUIT=(0.02,0.02,0.03,0.04,0.05,),),
                     COMP_INCR=_F(RELATION='ELAS',
                                  TOUT='OUI',),
                     INCREMENT=_F(LIST_INST=LINST,),
                     SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',FORMULATION='DEPLACEMENT'),
                     NEWTON=_F(MATRICE='TANGENTE',
                               REAC_ITER=1,),
                     CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                    ITER_GLOB_MAXI=50,
                                    ARRET='OUI',),
                     ARCHIVAGE=_F(LIST_INST=LINST,
                                  ARCH_ETAT_INIT='OUI',),);

DTYBEL=RECU_FONCTION(RESULTAT=DYNAEL,
                     LIST_INST=LINST,
                     PRECISION=1e-07,
                     NOM_CHAM='DEPL',
                     NOM_CMP='DY',
                     NOEUD='B',);

TEST_FONCTION(VALEUR=_F(FONCTION=DTYBEL,
                        VALE_PARA=0.1,
                        VALE_REFE=-0.001789867,
                        PRECISION=0.001,
                        REFERENCE='NON_REGRESSION',),);

DYNANL=DYNA_NON_LINE(MODELE=MO,
                     CHAM_MATER=CHMAT,
                     CARA_ELEM=CARELEM,
                     EXCIT=(_F(CHARGE=CH1,),
                            _F(CHARGE=EXCIT,
                               FONC_MULT=EVOLF,),),
                     AMOR_MODAL=_F(MODE_MECA=FREQ,
                                   AMOR_REDUIT=(0.02,0.02,0.03,0.04,0.05,),),
                     COMP_INCR=_F(RELATION='VMIS_POU_LINE',
                                  TOUT='OUI',
                                  ITER_INTE_MAXI=500,
                                  ),
                     INCREMENT=_F(LIST_INST=LINST,
                                  EVOLUTION='CHRONOLOGIQUE',
                                  SUBD_METHODE='UNIFORME',
                                  SUBD_COEF_PAS_1=1.0,
                                  SUBD_PAS=5,
                                  SUBD_PAS_MINI=1e-05,),
                     SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',FORMULATION='DEPLACEMENT'),
                     NEWTON=_F(
                               MATRICE='TANGENTE',
                               REAC_ITER=1,),
                     CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                    ITER_GLOB_MAXI=50,
                                    ARRET='OUI',),
                     ARCHIVAGE=_F(LIST_INST=LINST,
                                  ARCH_ETAT_INIT='OUI',),);

DTYBNL=RECU_FONCTION(RESULTAT=DYNANL,
                     LIST_INST=LINST,
                     PRECISION=1e-07,
                     NOM_CHAM='DEPL',
                     NOM_CMP='DY',
                     NOEUD='B',);

TEST_FONCTION(VALEUR=_F(FONCTION=DTYBNL,
                        VALE_PARA=0.1,
                        VALE_REFE=-0.00143,
                        PRECISION=0.1,
                        REFERENCE='NON_REGRESSION',),);

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=38,
              COURBE=(_F(FONCTION=DTYBEL,),
                      _F(FONCTION=DTYBNL,)))

FIN();
