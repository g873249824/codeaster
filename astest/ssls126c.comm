DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

# Carateristiques de la dalle

# Epaisseur
EP = 0.12;

# Taux de ferraillage
AX = 0.0007854;

# Taux de ferraillage
AY = 0.0007854;

# Distance grille/surface moyenne de la dalle
EZ = 0.038;

# Distance normalisee du ferraillage par rapport
# au feuillet moyen
R0 = (EZ / (0.5 * EP));

# Caracteristiques du beton
# Module d'Young
EB = 35700000000.0;

# Coefficient de poisson vaut 0.0
NUB = 0.0;

# Caracteristiques de l'acier (armatures)
# Module d'Young
E = 2.1e+11;

# Coefficient de poisson
NU = 0.0;


MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='LCONTY',),
                               _F(GROUP_MA='LCONTX',),
                               _F(GROUP_MA='LSYMY',),
                               _F(GROUP_MA='LSYMX',),),);

MOD=AFFE_MODELE(MAILLAGE=MAIL,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='Q4GG',),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='DALLE',
                                      VECT_NORM=(0.0,0.0,1.0,),
                                      GROUP_NO='A1',),);

MAT=DEFI_MATERIAU(ELAS=_F(E=EB,
                          NU=0.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

CARA_ELE=AFFE_CARA_ELEM(MODELE=MOD,
                        COQUE=_F(GROUP_MA='MESH',
                                 EPAIS=EP,
                                 ANGL_REP=(0.0,0.0,),),);

COND_LIM=AFFE_CHAR_MECA(MODELE=MOD,
                        DDL_IMPO=(_F(GROUP_NO='LCONTY',
                                     DZ=0.0,),
                                  _F(GROUP_NO='LSYMX',
                                     DY=0.0,
                                     DRX=0.0,),
                                  _F(GROUP_NO='LSYMY',
                                     DX=0.0,
                                     DRY=0.0,),),);

PRES=AFFE_CHAR_MECA(MODELE=MOD,
                    FORCE_COQUE=_F(GROUP_MA='DALLE',
                                   PRES=-10000.0,),);

CH_FO=DEFI_FONCTION(
                    NOM_PARA='INST',
                    VALE=(0.0 ,0.0 ,
                          1.0 ,1.0 ,),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

INST=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=_F(JUSQU_A=1.0,
                                  NOMBRE=2,),);

RESU=STAT_NON_LINE(MODELE=MOD,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CARA_ELE,
                   EXCIT=(_F(CHARGE=COND_LIM,),
                          _F(CHARGE=PRES,
                             FONC_MULT=CH_FO,),),
                   COMPORTEMENT=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=INST,),);

RESU=CALC_CHAMP(reuse =RESU,
                RESULTAT=RESU,
                CONTRAINTE=('EFGE_ELNO','EFGE_ELGA',),
                DEFORMATION=('DEGE_ELNO','DEGE_ELGA',),
                VARI_INTERNE='VARI_ELNO',);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  GROUP_NO='A1',
                  CRITERE='RELATIF',
                  VALE_REFE=0.0002658,
                  PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',
                  VALE_CALC=0.000267385577441,),);

