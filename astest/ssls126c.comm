                  RESULTAT=RESU,
                  NOM_CHAM='DEGE_ELGA',
                  NOM_CMP='KXX',
                  VALE_CALC= 7.220475609981E-04,
                  CRITERE='RELATIF',
                  MAILLE='M266',),);

TEST_RESU(RESU=_F(NUME_ORDRE=1,
