               NOM_CHAM = 'DEGE_ELNO',
               CRITERE = 'RELATIF',
               GROUP_NO = 'A1',
               MAILLE='M266',
               NOM_CMP = 'KXX',
               VALE_CALC = 4.9465321713326E-04,),
