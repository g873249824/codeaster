                  RESULTAT=RESU,
                  NOM_CHAM='EFGE_ELGA',
                  NOM_CMP='MXX',
                  VALE_CALC= 3963.0385644587,
                  CRITERE='RELATIF',
                  MAILLE='M266',),);

TEST_RESU(RESU=_F(NUME_ORDRE=1,
