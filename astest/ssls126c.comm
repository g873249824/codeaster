RESU=CALC_FERRAILLAGE(reuse =RESU,
                      RESULTAT=RESU,
                      INST=1.0,
                      TYPE_COMB='ELU',
                      AFFE=_F(TOUT='OUI',
                              ENROBG=0.04,
                              SIGM_ACIER=500000000.0,
                              SIGM_BETON=21000000.0,
