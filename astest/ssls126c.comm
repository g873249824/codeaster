                  NOM_CMP='DZ',
                  GROUP_NO='A1',
                  CRITERE='RELATIF',
                  VALE_REFE=0.0002658,
                  PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',
                  VALE_CALC=0.000267385577441,),);

TEST_RESU(RESU=(_F(RESULTAT=RESU1,
