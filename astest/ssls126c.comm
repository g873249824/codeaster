                  NOM_CMP='KXX',
                  MAILLE='M266',
                  POINT=1,
                  CRITERE='RELATIF',
                  VALE_CALC=0.000784426794909,),);

TEST_RESU(RESU=_F(RESULTAT=RESU_UTI,
                  NUME_ORDRE=2,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='MXX',
                  MAILLE='M266',
                  POINT=1,
                  CRITERE='RELATIF',
                  VALE_CALC=4032.58126727,),);

CARA_EL1=AFFE_CARA_ELEM(MODELE=MOD,
                        COQUE=_F(GROUP_MA='MESH',
                                 EPAIS=EP,
                                 ANGL_REP=(33.0,12.0,),),);

RESU1=STAT_NON_LINE(MODELE=MOD,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARA_EL1,
                    EXCIT=(_F(CHARGE=COND_LIM,),
                           _F(CHARGE=PRES,
                              FONC_MULT=CH_FO,),),
                    COMPORTEMENT=_F(RELATION='ELAS',),
                    INCREMENT=_F(LIST_INST=INST,),);

RESU1=CALC_CHAMP(reuse =RESU1,
                 RESULTAT=RESU1,
                 CONTRAINTE=('EFGE_ELNO','EFGE_ELGA',),
                 DEFORMATION=('DEGE_ELNO','DEGE_ELGA',),
                 VARI_INTERNE='VARI_ELNO',);

TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  NUME_ORDRE=2,
