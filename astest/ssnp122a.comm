               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_INCO_GD'));

TRACT= DEFI_FONCTION(
  NOM_PARA = 'EPSI',
  VALE = (
    0.002,   400,
    1.002,  2400),
  PROL_DROITE = 'LINEAIRE');

ACIER = DEFI_MATERIAU(
  ELAS       = _F(E = 200000,  NU = 0.3),
  TRACTION   = _F(SIGM = TRACT),
  ROUSSELIER = _F(D = 2,  SIGM_1 = 500,  PORO_INIT = 0.01),
  NON_LOCAL  = _F(
                #   LONG_CARA=0.1,
                   C_GONF=1
                  ));

CHP_MAT=AFFE_MATERIAU(MAILLAGE=MA,
                      AFFE=_F(TOUT='OUI',
                              MATER=ACIER));

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.5,
                                     NOMBRE=50)));

U_CHAR=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,0.0,1.5,1.5),
                     PROL_DROITE='CONSTANT');

CHA_L=AFFE_CHAR_MECA(MODELE=MO_L,
                      DDL_IMPO=(_F(GROUP_NO='AB',DY=0.0),
                                _F(GROUP_NO='A',DX=0.0),
                                _F(GROUP_NO='CD',DY=1.0)));
# CHA_NL=AFFE_CHAR_MECA(MODELE=MO_NL,
#                       DDL_IMPO=(_F(GROUP_NO='AB',DY=0.0),
#                                 _F(GROUP_NO='A',DX=0.0),
#                                 _F(GROUP_NO='CD',DY=1.0)));
CHA_NLG=AFFE_CHAR_MECA(MODELE=MO_NLG,
                      DDL_IMPO=(_F(GROUP_NO='AB',DY=0.0),
                                _F(GROUP_NO='A',DX=0.0),
                                _F(GROUP_NO='CD',DY=1.0)));

U_L=STAT_NON_LINE(MODELE=MO_L,
                CHAM_MATER=CHP_MAT,
                EXCIT=_F(CHARGE=CHA_L,
                         FONC_MULT=U_CHAR),
                COMP_INCR=_F(RELATION='ROUSSELIER',
                             DEFORMATION='SIMO_MIEHE',
                             RESI_INTE_RELA=1.E-06,
                             ITER_INTE_MAXI=15),
                INCREMENT=_F(LIST_INST=L_INST),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1),
                RECH_LINEAIRE=_F(ITER_LINE_MAXI=5),
                CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
                               ITER_GLOB_MAXI=15));

