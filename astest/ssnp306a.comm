                        )

CH_U4  = AFFE_CHAR_CINE( MODELE   = MO,
                         MECA_IMPO= ( _F(NOEUD ='N1', DX = 0.,),),
                        )


EVOL   = STAT_NON_LINE( MODELE     = MO,INFO=2,
                        CHAM_MATER = CHMAT,
                        EXCIT      = (_F( CHARGE       = CH_U1,
                                          FONC_MULT    = COEF1),
                                      _F( CHARGE       = CH_U2,
