DEBUT(CODE=_F(NOM='SSNP118D',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='OUI'))


MA=LIRE_MAILLAGE( )

IMPR_RESU(RESU=_F(MAILLAGE=MA),FORMAT='GMSH',UNITE=19)

MO=AFFE_MODELE(

                MAILLAGE=MA,
                VERIF='MAILLE',

                AFFE= (
                _F(
                   MAILLE = ('CUBE','DROITE'),
                   PHENOMENE = 'MECANIQUE',
                   MODELISATION = 'D_PLAN'
                     ),
                 _F(
                   MAILLE = 'CUBEDEG',
                   PHENOMENE = 'MECANIQUE',
                   MODELISATION = 'PLAN_JOINT'
                   )
                 )
              )


MAT=DEFI_MATERIAU(

     ELAS=_F(
              E =  0.5,
              NU = 0.0),
     RUPT_FRAG = _F(
                     GC = 0.9,
                     SIGM_C = 1.1,
                     PENA_ADHERENCE = 0.001
                    )
            )


CM=AFFE_MATERIAU(

               MAILLAGE=MA,
               AFFE=_F(
                       TOUT = 'OUI',
                       MATER = MAT),
              )

LIAISON=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(MAILLE='GAUCHE2', DX=0, DY=0),
    )
  )

# CHARGEMENT MODE I PUR
TRAC_1=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(MAILLE='DROITE', DX=2.16506350946110, DY= 1.250),
    )
  )

# CHARGEMENT MODE II PUR
TRAC_2=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
     _F(MAILLE='HAUT', DX=-1.250, DY= 2.16506350946110),
     _F(MAILLE='BAS',  DX=-1.250, DY= 2.16506350946110),
    )
  )

L_INST=DEFI_LIST_REEL(
                     DEBUT   = -1.,
     INTERVALLE=(
      _F( JUSQU_A = 0,       NOMBRE = 1 ),
      _F( JUSQU_A = 0.0001,  NOMBRE = 1 ),
      _F( JUSQU_A = 1.7,     NOMBRE = 17 ),
      )
     )

L_INST2=DEFI_LIST_REEL(

                     DEBUT   = 0.0001,
     INTERVALLE=(
      _F( JUSQU_A = 1.7,     NOMBRE = 17 ),
      )
     )


FCT = DEFI_FONCTION(
  NOM_PARA = 'INST',
  VALE     = (-1, 0, 0, 1.E-8),
  PROL_DROITE = 'LINEAIRE'
  )

#----------------------------------------------------------------------------------
# CALCUL AVEC CHARGEMENT EN MODE I
#-----------------------------------------------------------------------------------

U_1=STAT_NON_LINE(
  SOLVEUR=_F(METHODE='MUMPS'),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON ),
    _F(CHARGE = TRAC_1, FONC_MULT = FCT),
  ),
  COMPORTEMENT  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG'),
          _F(RELATION = 'ELAS',
             MAILLE = 'CUBE'),
          ),
  INCREMENT  = _F(LIST_INST = L_INST, INST_FIN = 0.),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 20),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  )

STAT_NON_LINE(reuse = U_1,
  SOLVEUR=_F(METHODE='MUMPS'),
  ETAT_INIT  = _F(EVOL_NOLI = U_1),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON ),
    _F(CHARGE = TRAC_1 , TYPE_CHARGE = 'FIXE_PILO'),
  ),
  PILOTAGE   = _F(
    SELECTION = 'RESIDU',
    TYPE = 'PRED_ELAS',
    MAILLE = 'CUBEDEG',
    COEF_MULT = 1,
    ETA_PILO_R_MIN = 0.,
    ),
  COMPORTEMENT  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG'),
          _F(RELATION = 'ELAS',
             GROUP_MA = 'G_CUBE'),
          ),
  INCREMENT  = _F(LIST_INST = L_INST),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 5),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  ARCHIVAGE  = _F(LIST_INST = L_INST2)
  )

#----------------------------------------------------------------------------------
# CALCUL AVEC CHARGEMENT EN MODE II
#-----------------------------------------------------------------------------------

U_2=STAT_NON_LINE(
  SOLVEUR=_F(METHODE='MUMPS'),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON ),
    _F(CHARGE = TRAC_2, FONC_MULT = FCT),
  ),
  COMPORTEMENT  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG'),
          _F(RELATION = 'ELAS',
             MAILLE = 'CUBE'),
          ),
  INCREMENT  = _F(LIST_INST = L_INST, INST_FIN = 0.),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 20),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  )

STAT_NON_LINE(reuse = U_2,
  SOLVEUR=_F(METHODE='MUMPS'),
  ETAT_INIT  = _F(EVOL_NOLI = U_2),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON  ),
    _F(CHARGE = TRAC_2 , TYPE_CHARGE = 'FIXE_PILO'),
  ),
  PILOTAGE   = _F(
    SELECTION = 'RESIDU',
    TYPE = 'PRED_ELAS',
    MAILLE = 'CUBEDEG',
    COEF_MULT = 1,
    ETA_PILO_R_MIN = 0.,
    ),
  COMPORTEMENT  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG'),
          _F(RELATION = 'ELAS',
             GROUP_MA = 'G_CUBE'),
          ),
  INCREMENT  = _F(LIST_INST = L_INST),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 5),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  ARCHIVAGE  = _F(LIST_INST = L_INST2)
  )



#----------------------------------------------------------------------------------
# TESTS CALCUL MODE I
#-----------------------------------------------------------------------------------

F_DX_1=RECU_FONCTION( RESULTAT=U_1,
              NOM_CHAM='DEPL',NOM_CMP='DX',GROUP_NO='TOUTSEUL')
T_DX_1=CREA_TABLE(FONCTION=_F(FONCTION=F_DX_1))
IMPR_TABLE(TABLE=T_DX_1)


F_ETA_1=RECU_FONCTION( RESULTAT=U_1,
              NOM_PARA_RESU='ETA_PILOTAGE')
T_ETA_1=CREA_TABLE(FONCTION=_F(FONCTION=F_ETA_1))
IMPR_TABLE(TABLE=T_ETA_1)


F_SIGN_1=RECU_FONCTION( RESULTAT=U_1, NOM_CHAM = 'SIEF_ELGA',
          NOM_CMP = 'SIGN',
          MAILLE  = 'CUBEDEG',POINT=1)
T_SIGN_1=CREA_TABLE(FONCTION=_F(FONCTION=F_SIGN_1))
IMPR_TABLE(TABLE=T_SIGN_1)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=1.472017048,
           VALE_REFE=1.4720169999999999,
           NOM_PARA='DX',
           TABLE=T_DX_1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.6000000000000001,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC=0.679895551,
           VALE_REFE=0.67989555099999999,
           NOM_PARA='ETA_PILO',
           TABLE=T_ETA_1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.6000000000000001,),
           )

TEST_RESU(RESU=_F(GROUP_NO='TOUTSEUL',
                  INST=0,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=U_1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC= 2.16506351E-08,
                  VALE_REFE=2.1650600000000001E-08,),
          )

TEST_RESU(RESU=_F(INST=1.6000059,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=U_1,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SIXX',
                  VALE_CALC=0.092762954,
                  VALE_REFE=0.0927629539,
                  MAILLE='CUBE',),
          )

TEST_RESU(RESU=_F(INST=1.000041,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=U_1,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SIGN',
                  VALE_CALC=0.548877353,
                  VALE_REFE=0.54887554999999999,
                  MAILLE='CUBEDEG',),
          )

TEST_RESU(RESU=_F(INST=0.4000765,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=U_1,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SITX',
                  VALE_CALC=-1.17587985E-14,
                  VALE_REFE=0.0,
                  CRITERE='ABSOLU',
                  MAILLE='CUBEDEG',),
          )

#----------------------------------------------------------------------------------
# TESTS CALCUL MODE II
#-----------------------------------------------------------------------------------

F_DY_2=RECU_FONCTION( RESULTAT=U_2,
              NOM_CHAM='DEPL',NOM_CMP='DY',GROUP_NO='TOUTSEUL')
T_DY_2=CREA_TABLE(FONCTION=_F(FONCTION=F_DY_2))
IMPR_TABLE(TABLE=T_DY_2)


F_ETA_2=RECU_FONCTION( RESULTAT=U_2,
              NOM_PARA_RESU='ETA_PILOTAGE')
T_ETA_2=CREA_TABLE(FONCTION=_F(FONCTION=F_ETA_2))
IMPR_TABLE(TABLE=T_ETA_2)


F_SIGN_2=RECU_FONCTION( RESULTAT=U_2, NOM_CHAM = 'SIEF_ELGA',
          NOM_CMP = 'SIGN',
          MAILLE  = 'CUBEDEG',POINT=1)
T_SIGN_2=CREA_TABLE(FONCTION=_F(FONCTION=F_SIGN_2))
IMPR_TABLE(TABLE=T_SIGN_2)


F_SITX_2=RECU_FONCTION( RESULTAT=U_2, NOM_CHAM = 'SIEF_ELGA',
          NOM_CMP = 'SITX',
          MAILLE  = 'CUBEDEG',POINT=1)
T_SITX_2=CREA_TABLE(FONCTION=_F(FONCTION=F_SITX_2))
IMPR_TABLE(TABLE=T_SITX_2)


TEST_RESU(RESU=_F(INST=1.000041,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=U_2,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SIGN',
                  VALE_CALC=-2.77490088E-13,
                  VALE_REFE=0.0,
                  CRITERE='ABSOLU',
                  MAILLE='CUBEDEG',),
          )

TEST_RESU(RESU=_F(INST=1.200029,
                  REFERENCE='ANALYTIQUE',
                  POINT=2,
                  RESULTAT=U_2,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SITX',
                  VALE_CALC=0.433148729,
                  VALE_REFE=0.43314871999999999,
                  MAILLE='CUBEDEG',),
          )

IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=U_1,NOM_CHAM='DEPL',TYPE_CHAM='VECT_2D',NOM_CMP=('DX','DY')))),
IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=U_2,NOM_CHAM='DEPL',TYPE_CHAM='VECT_2D',NOM_CMP=('DX','DY')))),


FIN()
