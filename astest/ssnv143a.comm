#----------------------------------------------------------------------
#   TEST MODELE BETON_DOUBLE_DP 3D (DOUBLE CRITERE DE DRUCKER PRAGER)
#   EN DEPLACEMENT IMPOSE
#   ESSAI EN TRACTION (XX-ZZ)
#   CHARGE (AVEC RESULTATS DE REFERENCE) ET DECHARGE (NON REGRESSION)
#
#----------------------------------------------------------------------
#                ELEMENT 3D CUBE A 8 NOEUDS
#              ETAT HOMOGENE CONTRAINTE DEFORMATION
#       UNITES : NEWTON , MILLIMETRE , SECONDE (N,MM,S,->MPA)
#
#
#               Z       A                     A = 1 MM
#               L<------------->                      2
#                                             S = 1 MM
#                / N5            N8
#              >O--------------O
#              /L             /L
#         N6  / L         N7 / L
#           >O--+-----------O  L
#            L  L           L  L
#            L  L     S     L  L
#            L  L           L  L
#            L  L/ N1       L  L N4
#            L >O-----------L--O-> Y
#            L /V           L /
#         N2 L/          N3 L/
#           >O--------------O
#
#          /
#         X
#
#
#----------------------------------------------------------------------
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

CUBE=LIRE_MAILLAGE(    )

TROISD=AFFE_MODELE(            MAILLAGE=CUBE,
                                    AFFE=_F(   PHENOMENE = 'MECANIQUE',
                                            MODELISATION = '3D',
                                            MAILLE = 'MA1'))

E=DEFI_FONCTION(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                          PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      37000.,
                                20.E0,      32000.,
                               400.E0,      15000.,
                               800.E0,       5000., )
                      )

NU=DEFI_FONCTION(   NOM_PARA='SECH',  PROL_DROITE='LINEAIRE',
                                         PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      .18,
                               800.E0,      .18, )
                      )

ALPHA=DEFI_FONCTION(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                         PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      1.E-5,
                               800.E0,      1.E-5,)
                     )

FC=DEFI_FONCTION(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                         PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      40.,
                               400.E0,      40.,
                               800.E0,      15., )
                      )

FT=DEFI_FONCTION(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                         PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      4.,
                               400.E0,      4.,
                               800.E0,      1.5, )
                      )

BETA=DEFI_FONCTION(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                         PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      1.16,
                               800.E0,      1.16, )
                      )

GC=DEFI_FONCTION(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                        PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      10.,
                               800.E0,      10., )
                      )

GT=DEFI_FONCTION(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                         PROL_GAUCHE='LINEAIRE',
                        VALE=(   0.E0,      0.100,
                               800.E0,      0.100, )
                      )

BETON=DEFI_MATERIAU(           ELAS_FO=_F( E = E,
                                           NU = NU,
                                           K_DESSIC = 1.E-5,
                                           ALPHA = ALPHA,
                                           TEMP_DEF_ALPHA=0.0),
                          BETON_DOUBLE_DP=_F( F_C = FC,
                                           F_T = FT,
                                           COEF_BIAX = BETA,
                                           ENER_COMP_RUPT = GC,
                                           ENER_TRAC_RUPT = GT,
                                           COEF_ELAS_COMP = 33.33333333,
                                           ECRO_COMP_P_PIC = 'LINEAIRE',
                                           ECRO_TRAC_P_PIC = 'LINEAIRE')
                             )
CHTEMP1=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R',
MAILLAGE=CUBE,
                              AFFE=_F( NOM_CMP = 'TEMP',
                                    TOUT = 'OUI',
                                    VALE = 20.0)
                                   )

CHTEMP2=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R',
MAILLAGE=CUBE,
                              AFFE=_F( NOM_CMP = 'TEMP',
                                    TOUT = 'OUI',
                                    VALE = 0.0)
                                   )


SECH=CREA_RESU(  OPERATION='AFFE', TYPE_RESU='EVOL_THER',
                    NOM_CHAM='TEMP',AFFE=(
                     _F(  INST = 0.,   CHAM_GD = CHTEMP1),
                             _F(  INST = 2.5,  CHAM_GD = CHTEMP2))
                  )

TEMP=CREA_RESU(  OPERATION='AFFE', TYPE_RESU='EVOL_THER',
                    NOM_CHAM='TEMP',AFFE=(
                     _F(  INST = 0.,   CHAM_GD = CHTEMP2),
                             _F(  INST = 2.5,  CHAM_GD = CHTEMP1))
                  )

MATBET=AFFE_MATERIAU(           MAILLAGE=CUBE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = BETON),
                                    AFFE_VARC=(_F(TOUT='OUI',
                                                 EVOL=SECH,
                                                 NOM_VARC="SECH",
                                                 NOM_CHAM='TEMP',
                                                 VALE_REF=20),
                                              _F(TOUT='OUI',
                                                 EVOL=TEMP,
                                                 NOM_VARC='TEMP',
                                                 NOM_CHAM='TEMP',
                                                 VALE_REF=0),),
                         )

COND_LIM=AFFE_CHAR_MECA(
                      MODELE=TROISD,
                      DDL_IMPO=(
                    _F(  GROUP_NO = 'FACEXY',  DZ = 0.),
                    _F(  GROUP_NO = 'FACEYZ',  DX = 0.),
                    _F(  NOEUD = 'N1',         DY = 0.),
                             _F(  NOEUD = 'N2',         DY = 0.))
                             )

TRAC=AFFE_CHAR_MECA(
                      MODELE=TROISD,DDL_IMPO=(
                    _F(  GROUP_NO = 'FACE1XY',  DZ = 0.02),
                    _F(  GROUP_NO = 'FACE1YZ',  DX = 0.01))

                            )

LINST=DEFI_LIST_REEL(              DEBUT=0.,INTERVALLE=(

                           _F(  JUSQU_A = 1.,   NOMBRE = 20),
                           _F(  JUSQU_A = 1.5,  NOMBRE = 10),
                           _F(  JUSQU_A = 2.5,  NOMBRE = 10))
                         )

COEF_TR=DEFI_FONCTION(
                              NOM_PARA='INST',
                                  VALE=(   0.,    0.,
                                           1.5,   1.5,
                                           2.5,   0.5,  )
                        )

SOLNL=STAT_NON_LINE(
                             TITRE='TEST LOI BETON_DOUBLE_DP',
                            MODELE=TROISD,EXCIT=(
                             _F(
                CHARGE = COND_LIM),
                             _F(
                CHARGE = TRAC,
                FONC_MULT = COEF_TR)),
                         INCREMENT=_F(
                LIST_INST = LINST),
                        CHAM_MATER=MATBET,
                         COMPORTEMENT=_F(
                RELATION = 'BETON_DOUBLE_DP',
                DEFORMATION = 'PETIT',
                ITER_INTE_MAXI = 10,
                RESI_INTE_RELA = 1.E-6,
                ITER_INTE_PAS = 0,
                TOUT = 'OUI'),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_INCR = 1,
                REAC_ITER = 5),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 20,
                RESI_GLOB_RELA = 1.E-6)
              )

SOLNL=CALC_CHAMP(reuse=SOLNL,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=SOLNL)


SOLNL=CALC_CHAMP(reuse=SOLNL,DEFORMATION=('EPSI_ELNO'),RESULTAT=SOLNL)


