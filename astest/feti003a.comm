# DE CORPS RIGIDES ET DIRICHLETS SIMPLES (DDL_IMPO,FACE_IMPO)
# ======================================================================
# PLAQUE 2D MODELISEE VIA 24 TRIA3 ET DECOUPEE EN 4 SOUS-DOMAINES
# HOMOGENES AVEC UN NOEUD CENTRAL DE MULTIPLICITE 4.
# TEST INFORMATIQUE DU SOLVEUR FETI-1 AVEC 6 MODE DE CORPS RIGIDES
# (SOUS-DOMAINES 2 ET 4 FLOTTANTS)
# TROIS DIRICHLET SIMPLE (DDL_IMPO + FACE_IMPO) VIA AFFE_CHAR_MECA
# DONT UN SCINDE (CH4) ENTRE DEUX SOUS-DOMAINES (1 ET 3).
# DIRICHLET HOMOGENE ET INHOMOGENE (POUR TESTER LE SCALING DES LAGRANGES)
# PARALLELE: VALEUR CONSEILLEE 2 PROCS.
# ======================================================================
# RESPONSABLE BOITEAU O.BOITEAU
DEBUT(CODE=_F(NOM='FETI003A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'))

MATER=DEFI_MATERIAU(
           ELAS=_F(E = 180000., NU = 0.30, ALPHA = 15.E-6, RHO = 7700.,))

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

MODM=AFFE_MODELE(MAILLAGE=MAIL,
                 AFFE=(_F(GROUP_MA = 'STRU',PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'D_PLAN',   ),))

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(TOUT='OUI',MATER=MATER),))

CH1=AFFE_CHAR_MECA(MODELE=MODM,
                    PRES_REP=(_F(GROUP_MA=('DDLI1'),PRES = 2000.,),),)

CH2=AFFE_CHAR_MECA(MODELE=MODM,
                    FACE_IMPO=(_F(MAILLE=('M25'),DY = -1.,),),)

CH3=AFFE_CHAR_MECA(MODELE=MODM,
                    DDL_IMPO=(_F( GROUP_NO=('DIRI1'),DX=0.0,),),)

CH4=AFFE_CHAR_MECA(MODELE=MODM,
                    DDL_IMPO=(_F( GROUP_NO=('DIRI13'),DX=2.0, DY=-3.0),),)

SDFETI=DEFI_PART_OPS(NOM='SD',
                     MODELE=MODM,
                     INFO=1,
                     DEFI=(_F(GROUP_MA = 'FETI1', ),
                           _F(GROUP_MA = 'FETI2', ),
                           _F(GROUP_MA = 'FETI3', GROUP_MA_BORD = 'B5',),
                           _F(GROUP_MA = 'FETI4', GROUP_MA_BORD = 'B6',),
                           ),
                     EXCIT=(_F(CHARGE=CH1),
                            _F(CHARGE=CH2),
                            _F(CHARGE=CH3),
                            _F(CHARGE=CH4),),
                    );

#IMPR_CO(CONCEPT=_F(NOM=SDFETI));

