# -----------------------------------------------------
# CYLINDRE AXISYMETRIQUE EN FLUAGE : FLUAGE DE GRANGER POU_D_EM
# -----------------------------------------------------

DEBUT(CODE=_F(NOM='SSNV142C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='POU_D_EM',),);

PRE_GIBI(UNITE_GIBI=18,
         UNITE_MAILLAGE=21,);

MASEC=LIRE_MAILLAGE(UNITE=21,);

IMPR_RESU(FORMAT='GMSH',
          UNITE=37,
          RESU=_F(MAILLAGE=MASEC,),);

GF=DEFI_GEOM_FIBRE(
   SECTION =_F(GROUP_FIBRE='SECF',
             GROUP_MA_SECT='SURF1',
             MAILLAGE_SECT=MASEC,
            COOR_AXE_POUTRE=(0.,0.,),),)

CARA=AFFE_CARA_ELEM(MODELE=MO,
                    INFO=2,
                    POUTRE=_F(GROUP_MA='TOUT',
                              SECTION='GENERALE',
                              CARA=('A','IY','IZ','JX','RY','RZ','RT',),
                              VALE=(0.02,0.166666666666e-4,0.666666666666e-4,4.5776e-5,0.1,0.05,0.0892632,),),
                    ORIENTATION=_F(GROUP_MA='TOUT',
                                   CARA='ANGL_VRIL',
                                   VALE=0.0,),
                    GEOM_FIBRE=GF,
                    MULTIFIBRE=_F(GROUP_MA='TOUT',
                                 GROUP_FIBRE='SECF'),);

E=DEFI_CONSTANTE(VALE=31000,);

NU=DEFI_CONSTANTE(VALE=0.2,);

ALPHA=DEFI_CONSTANTE(VALE=0.,);

FONC1=DEFI_FONCTION(NOM_PARA='SECH',
                    VALE=(0.0,0.0,
1.,1.,
),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

BETON=DEFI_MATERIAU(ELAS_FO=_F(E=E,
                               NU=NU,
                               TEMP_DEF_ALPHA=20.0,
                               ALPHA=ALPHA,
                               FONC_DESORP=FONC1,),
                   GRANGER_FP_INDT=_F(J1=3.226e-5,
                                       J2=6.452e-5,
                                       TAUX_1=432000.,
                                       TAUX_2=4320000.,),);

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,MATER_SECT=BETON,
                  MULTIFIBRE=_F(GROUP_FIBRE='SECF',MATER=BETON,
                                RELATION='GRANGER_FP_INDT',
                                ALGO_1D='DEBORST',)
                     )

SECH=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(
                TOUT='OUI',
                NOM_CMP='TEMP',
                VALE=1.,),);

SECH1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=_F(
                CHAM_GD=SECH,
                INST=0.,),);

TEMP=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(
                TOUT='OUI',
                NOM_CMP='TEMP',
                VALE=20.,),);

TEMP1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=_F(
                CHAM_GD=TEMP,
                INST=0.,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=BETON,
                         ),
                 AFFE_VARC=(_F(TOUT='OUI',
                              EVOL=SECH1,
                              NOM_VARC="SECH",
                              NOM_CHAM='TEMP',
                              VALE_REF=1),
                             _F(TOUT='OUI',
                              EVOL=TEMP1,
                              NOM_VARC="TEMP",
                              NOM_CHAM='TEMP',
                              VALE_REF=20.),) ,
                 AFFE_COMPOR=_F(TOUT='OUI',COMPOR=COMPF),
);


FORCE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,
10.,-20.*0.02,
31536010.,-20.*0.02,
),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

CH1=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_NO='A',
                                LIAISON='ENCASTRE',),
                             _F(GROUP_NO='B',
                                DY=0.,
                                DZ=0.,),),);

CH2=AFFE_CHAR_MECA_F(MODELE=MO,
                     FORCE_NODALE=_F(GROUP_NO='B',
                                     FX=FORCE,),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=10.0,
                                     NOMBRE=1,),
                                  _F(JUSQU_A=31536010.,
                                     NOMBRE=73,),),);
#DELTAT=432000=5J

U=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CM,
                CARA_ELEM=CARA,
                EXCIT=(_F(CHARGE=CH1,),
                       _F(CHARGE=CH2,),),
                COMP_INCR=_F(RELATION='MULTIFIBRE',
#                             ITER_INTE_MAXI=1000,
                          ),
                INCREMENT=_F(LIST_INST=L_INST,
                             NUME_INST_FIN=74,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=1.E-6,
                               ITER_GLOB_MAXI=25,),);

U=CALC_CHAMP(reuse =U,
            RESULTAT=U,
             CONTRAINTE=('SIEF_ELNO'),);

U=CALC_NO(reuse =U,
          RESULTAT=U,
          OPTION='SIEF_NOEU',);
# S=0.02

TEST_RESU(RESU=(_F(RESULTAT=U,
                   NUME_ORDRE=74,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   GROUP_NO='B',
                   VALE=-0.00257977,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=U,
                   NUME_ORDRE=74,
                   NOM_CHAM='SIEF_NOEU',
                   NOM_CMP='N',
                   GROUP_NO='B',
                   VALE=-0.4,
                   REFERENCE='ANALYTIQUE',),),);

FIN();
#
