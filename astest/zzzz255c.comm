DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),
         DEBUG=_F(SDVERI='OUI'),)

MaiPlate=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

plate=AFFE_MODELE(MAILLAGE=MaiPlate,
                   AFFE=(_F(GROUP_MA=('ALL',),
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),),);
nbcalc= 3
nbcalc= nbcalc +2
rayon = 250.
avance = 25.

MAX =  [None]*(nbcalc)
MAT =  [None]*(nbcalc)
FF =  [None]*(nbcalc)
MODEL =  [None]*(nbcalc)
CHLT =  [None]*(nbcalc)
TABLST =  [None]*(nbcalc)
#***********************************
# DEFINITION DE LA FISSURE INITIALE (CERCLE)
#***********************************
MAX[0]=CO('MAX_0')
MAT[0]=CO('MAT_0')
PROPA_FISS(METHODE_PROPA='INITIALISATION',
         MAIL_STRUC=MaiPlate,
         MAIL_FISS = MAX[0],
         MAIL_TOTAL = MAT[0],
         FORM_FISS='ELLIPSE',
         INFO=2,
         CENTRE = (0., 0., 0.),
         DEMI_GRAND_AXE =rayon,
         DEMI_PETIT_AXE =rayon,
         VECT_X         =(1.,0.,0.),
         VECT_Y         =(0.,1.,0.),
         ANGLE_ORIG        = 0.,
         ANGLE_EXTR        = 360.,
         GROUP_MA_FISS='FISS',
         GROUP_MA_FOND='FOND',
         NB_POINT_FOND = 21,
         );


#***********************************
# PROPAGATION DE LA FISSURE (K1 = CONSTANTE)
#***********************************

TABK= LIRE_TABLE(UNITE=38, NUME_TABLE = 1 , FORMAT='ASTER')
E=205000.0E6
nu=0.3
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho,),);

for i in range(0,(nbcalc-1)) :


 MODEL[i]=AFFE_MODELE(MAILLAGE=MAT[i],
                   AFFE=(_F(GROUP_MA=('ALL',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),),);
                            
 FF[i]=DEFI_FISS_XFEM(DEFI_FISS=_F(GROUP_MA_FISS='FISS_'+str(i),
                              GROUP_MA_FOND='FOND_'+str(i),),
                   MAILLAGE=MAT[i],
                   INFO=1
                   );
                   
 if ( i != nbcalc ) :
   MAX[i+1]=CO('MAX_%d'%(i+1))
   MAT[i+1]=CO('MAT_%d'%(i+1))
   PROPA_FISS( METHODE_PROPA='MAILLAGE',
            LOI_PROPA=_F(LOI='PARIS',
                        M=1.,
                        C=1.,
                        MATER=ACIER,
                         )  ,
            FISSURE=_F(FISS_ACTUELLE=FF[i],
                 MAIL_ACTUEL =  MAX[i],
                 MAIL_PROPAGE = MAX[i+1] ,
                 GROUP_MA_FISS='FISS',
                 GROUP_MA_FOND='FOND',    
                 TABLE=TABK,
                  ),
            COMP_LINE=_F(COEF_MULT_MINI=0.,                                 
                         COEF_MULT_MAXI=1.),   
            CRIT_ANGL_BIFURCATION='PLAN',
            MAIL_STRUC=MaiPlate,
            MAIL_TOTAL = MAT[i+1],
            DA_MAX=25,
            ITERATION = i+1,
            INFO=2,);

                   
#****************************
# TEST TABLE (VERIFICATION INFORMATIQUE)
#****************************

CHLT[i]=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_R',
                OPERATION='EXTR',
                NOM_CHAM='LTNO',
                FISSURE=FF[i],);

TABLST[i] = POST_RELEVE_T(ACTION=_F(INTITULE='Test LST',
                                 GROUP_NO=('A','B','C','D','E','F','G','H',),
                                 CHAM_GD=CHLT[i],
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION',),);


d1 = 500.*sqrt(2.)- rayon - (nbcalc-2)*avance

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='NON_DEFINI',
           PRECISION=0.050000000000000003,
           VALE_CALC=392.316396739,
           VALE_REFE=382.10678118654755,
           NOM_PARA='X1',
           TYPE_TEST='MAX',
           TABLE=TABLST_3,
           )

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='NON_DEFINI',
           PRECISION=0.050000000000000003,
           VALE_CALC=392.316396739,
           VALE_REFE=382.10678118654755,
           NOM_PARA='X1',
           TYPE_TEST='MIN',
           TABLE=TABLST_3,
           )

FIN();
