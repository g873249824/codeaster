                  CRITERE='RELATIF',
                  MAILLE='M20',
                  ),
          )

#--------------------------------------------------------------------------------
#
#  TEST SUR LES CONTRAINTES AU POINT B

TEST_RESU(RESU=_F(INST=1.0,
                  RESULTAT=RESU,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N20',
                  NOM_CMP='SIYY',
                  VALE_CALC=439.88472897788,
                  CRITERE='RELATIF',
                  MAILLE='M20',
                  ),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  RESULTAT=RESU,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N20',
