# MAILLAGE G1, MATERIAU M3
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED', )

MO=AFFE_MODELE(     MAILLAGE=MA,
                                 AFFE=_F(  TOUT = 'OUI',
                                        PHENOMENE = 'MECANIQUE',
                                        MODELISATION = 'C_PLAN')
                        )

MAT1=DEFI_MATERIAU(    ELAS=_F(  E = 2.E12,
                                     NU = 0.30,
                                     ALPHA = 0.)
                          )

#
MAT2=DEFI_MATERIAU(    ELAS=_F(  E = 5.E11,
                                     NU = 0.30,
                                     ALPHA = 0.)
                          )

#
CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,AFFE=(
                                  _F(  GROUP_MA = 'VTOT1',
                                         MATER = MAT1),
                                  _F(  GROUP_MA = 'LPB1PA1',
                                         MATER = MAT1),
                                  _F(  GROUP_MA = 'LPA1PA',
                                         MATER = MAT1),
                                  _F(  GROUP_MA = 'LPBPB1',
                                         MATER = MAT1),
                                  _F(  GROUP_MA = 'VTOT2',
                                         MATER = MAT2),
                                  _F(  GROUP_MA = 'LPA2PB2',
                                         MATER = MAT2),
                                  _F(  GROUP_MA = 'LPAPA2',
                                         MATER = MAT2),
                                  _F(  GROUP_MA = 'LPB2PB',
                                         MATER = MAT2))
                         )

#
MA=DEFI_GROUP( reuse=MA,        MAILLAGE=MA,
                     CREA_GROUP_NO=_F(  GROUP_MA = ( 'LPB1PA1',  'LPA2PB2',
                                                  'LPA1PA',   'LPBPB1',
                                                  'LPAPA2',   'LPB2PB',
                                                ))
                 )

#
CH=AFFE_CHAR_MECA(           MODELE=MO,DDL_IMPO=(
                                    _F(  GROUP_NO = 'LPA1PA',
                                               DX = 0.),
                                             _F(  GROUP_NO = 'LPAPA2',
                                               DX = 0.),
                                             _F(  GROUP_NO = 'PA',
                                               DY = 0.)),LIAISON_UNIF=(
                                _F(  GROUP_NO = 'LPBPB1',
                                                DDL = 'DX'),
                                             _F(  GROUP_NO = 'LPB2PB',
                                                DDL = 'DX')),FORCE_CONTOUR=(
                               _F(  GROUP_MA = 'LPB1PA1',
                                               FY = 75.E6),
                                             _F(  GROUP_MA = 'LPA2PB2',
                                               FY = -75.E6))
                           )

#
CHAMDEPL=MECA_STATIQUE(        MODELE=MO,
                              CHAM_MATER=CHMAT,
                                   EXCIT=_F( CHARGE = CH)
                          )

#
FOND1=DEFI_FOND_FISS(    MAILLAGE=MA,
                            FOND_FISS=_F( GROUP_NO = ('P0',)),
                      )

#
THETA01=CALC_THETA(       MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 3.57125E-4,
                                     R_SUP = 7.14250E-4),
                         DIRECTION=(-0.707106781, 0.707106781, 0.,)
                     )

#
G01=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(THETA=THETA01),
                     )

#
GK01=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F(  R_INF = 3.57125E-4,
                              R_SUP = 7.14250E-4,
                              FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )

#
THETA02=CALC_THETA(       MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 7.1425E-4,
                                     R_SUP = 1.071375E-3),
                         DIRECTION=(-0.707106781, 0.707106781, 0.,)
                     )

#
G02=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(THETA=THETA02),
                     )

#
GK02=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F( R_INF = 7.1425E-4,
                             R_SUP = 1.071375E-3,
                             FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )

#
THETA03=CALC_THETA(       MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 1.071375E-3,
                                     R_SUP = 7.1425E-3),
                         DIRECTION=(-0.707106781, 0.707106781, 0.,)
                     )

#
G03=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(THETA=THETA03),
                     )

#
GK03=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F(   R_INF = 1.071375E-3,
                               R_SUP = 7.1425E-3,
                               FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )

#
THETA04=CALC_THETA(       MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 3.57125E-4,
                                     R_SUP = 7.1425E-3),
                         DIRECTION=(-0.707106781, 0.707106781, 0.,)
                     )

#
G04=CALC_G(        RESULTAT=CHAMDEPL,
                   THETA=_F(THETA=THETA04),
                     )

#
GK04=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F(  R_INF = 3.57125E-4,
                              R_SUP = 7.1425E-3,
                              FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )

#
GK05=CALC_G(       RESULTAT=CHAMDEPL,
                   THETA=_F( FOND_FISS=FOND1),
                   OPTION='CALC_K_G'
                      )

