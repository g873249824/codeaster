### TEST HEXA27/Face 

Mailh27=CREA_MAILLAGE(MAILLAGE=Mailh8,
                    HEXA20_27=_F(TOUT='OUI',
                                        PREF_NOEUD='NN',
                                        PREF_NUME=1,),
                    );
 
Mailh82=CREA_MAILLAGE(MAILLAGE=Mailh27,
                    DECOUPE_LAC=_F(GROUP_MA_ESCL='Face_2',),);


IMPR_RESU(FORMAT='MED', UNITE=80, RESU=_F(MAILLAGE=Mailh82),)

TEST_RESU(MAILLAGE=(
