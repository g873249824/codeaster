DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',), DEBUG=_F(SDVERI='OUI',), INFO=1,);

Mail=LIRE_MAILLAGE(UNITE=20,
                   FORMAT='MED',);

### TEST HEXA20/Edge 

Mailh8=CREA_MAILLAGE(MAILLAGE=Mail,
                   LINE_QUAD=_F(TOUT='OUI',),);


###TEST HEXA8/Face

Mail12=CREA_MAILLAGE(MAILLAGE=Mail,
                   DECOUPE_LAC=_F(GROUP_MA_ESCL='Face_2',
                                  DECOUPE_HEXA="HEXA",),);


IMPR_RESU(FORMAT='MED', UNITE=80, RESU=_F(MAILLAGE=Mail12),)


TEST_RESU(MAILLAGE=(
       _F( REFERENCE='ANALYTIQUE', MAILLAGE=Mail12,
           CARA='NB_MAILLE', VALE_REFE_I=28, VALE_CALC_I=28,),
       _F( REFERENCE='ANALYTIQUE', MAILLAGE=Mail12,
           CARA='NB_NOEUD', VALE_REFE_I=16, VALE_CALC_I=16,),))

Mail12b=CREA_MAILLAGE(MAILLAGE=Mail,
                      DECOUPE_LAC=_F(GROUP_MA_ESCL='Face_2',),);


TEST_RESU(MAILLAGE=(
       _F( REFERENCE='ANALYTIQUE', MAILLAGE=Mail12b,
           CARA='NB_MAILLE', VALE_REFE_I=26, VALE_CALC_I=26,),
       _F( REFERENCE='ANALYTIQUE', MAILLAGE=Mail12b,
           CARA='NB_NOEUD', VALE_REFE_I=9, VALE_CALC_I=9,),))



### TEST HEXA20/Face

Mailh20=CREA_MAILLAGE(MAILLAGE=Mailh8,
                   DECOUPE_LAC=_F(GROUP_MA_ESCL='Face_2',
                                  DECOUPE_HEXA="HEXA",),);


IMPR_RESU(FORMAT='MED', UNITE=80, RESU=_F(MAILLAGE=Mailh20),)


TEST_RESU(MAILLAGE=(
       _F( REFERENCE='ANALYTIQUE', MAILLAGE=Mailh20,
           CARA='NB_MAILLE', VALE_REFE_I=28, VALE_CALC_I=28,),
       _F( REFERENCE='ANALYTIQUE', MAILLAGE=Mailh20,
           CARA='NB_NOEUD', VALE_REFE_I=48, VALE_CALC_I=48,),))

