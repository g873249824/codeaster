DEBUT(  CODE=_F( NOM = 'TPLA300A',NIV_PUB_WEB='INTERNET'))

#
# LECTURE DU MAILLAGE ----------------------------------------------
#

MAILL=LIRE_MAILLAGE(FORMAT='MED',  )

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ------------------------
#

MATE=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0, LAMBDA = 0.04))

#
# AFFECTATIONS: MATERIAU  MODELE  CHARGEMENT ----------------------
#

MODEL=AFFE_MODELE( 
                  MAILLAGE=MAILL,
                     AFFE=_F( TOUT = 'OUI',  PHENOMENE = 'THERMIQUE',
                                       MODELISATION = 'PLAN'))

CHMATE=AFFE_MATERIAU(   MAILLAGE=MAILL,
                    AFFE=_F( TOUT = 'OUI',  MATER = MATE))

CHARGE=AFFE_CHAR_THER(
                      MODELE=MODEL,
                  TEMP_IMPO=_F( GROUP_NO = 'NOE_IMP',  TEMP = 0.E0),
                     SOURCE=_F( TOUT = 'OUI', SOUR = 1.E0)
                  )

#
# RESOLUTION ------------------------------------------------------
#


TEMP=THER_LINEAIRE( 
                      MODELE=MODEL,
                  CHAM_MATER=CHMATE,
                      EXCIT=_F(  CHARGE = CHARGE) )

#
# ANALYSE DES RESULTATS -------------------------------------------
#
TPR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMP, NUME_ORDRE=0)

