DEBUT(CODE=_F(NOM='SSNV195D', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAILLAG1=LIRE_MAILLAGE();

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(_F(NOM='SURFQUAD',
                                     GROUP_MA='SURF'),
                _F(NOM='PMED',
                                      OPTION ='ENV_SPHERE',
                  POINT  = (0.0,25.0),
                  RAYON = 1E-3,
                                      PRECISION = 2E-3,
                  ),
                  _F(NOM='PHAU',
                     OPTION ='ENV_SPHERE',
                     POINT  = (0.0,50.0),
                     RAYON = 1E-3,
                     PRECISION = 2E-3,
                   ),
                  )
                 );

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                CREA_GROUP_NO =(
                             _F(
                                NOM = 'LIGMED',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'PMED',
                                VECT_NORMALE = (0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'LIGSUP',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'PHAU',
                                VECT_NORMALE = (0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                     )
                   );
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=(_F(GROUP_MA=('SURF'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',
                              ),
                           _F(GROUP_MA=('LIGINF'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',
                              )));


LN1=FORMULE(NOM_PARA=('X','Y'),VALE='Y-12.5');

LN2=FORMULE(NOM_PARA=('X','Y'),VALE='Y-37.5');

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN1),
                    INFO=1);

FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN2),
                    INFO=1);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         INFO=1);

E=100.0E6
nu=0.0
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('SURF','LIGINF'),
                                MATER=ACIER,
                                ));

DEPX=1.E-3
DEPY=2.E-3
ZERO=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIGINF',
                                          DX=0.0,
                                          DY=-DEPY),
                             _F(GROUP_NO='LIGSUP',
                                          DX=0.0,
                                          DY=DEPY),
                  _F(GROUP_NO='LIGMED',
                                          DX=0.0,
                                          DY=0.0)),
                    INFO=1);


UTOT1=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(
                          _F(CHARGE=CH1),
                          ),
                   SOLVEUR=_F(   
                            METHODE='MUMPS'),
                   INFO=2,
                   ); 

# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN'))

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)

#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));

# -------------------------------------------------------------   
#            TEST_TABLE
# -------------------------------------------------------------   


DEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=DEP,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON');

# POUR LES DEUX FISSURES

TEST_TABLE(TABLE=DEP,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');


# POUR LA PREMIERE FISSURE

DEP1P = CALC_TABLE(TABLE = DEP,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^P]'),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'COOR_Y',
                               CRIT_COMP = 'LT',
                               VALE      = 25)));

TEST_TABLE(TABLE=DEP1P,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=-DEPY,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP1P,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=-DEPY,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE');

DEP1M = CALC_TABLE(TABLE = DEP,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^M]'),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'COOR_Y',
                               CRIT_COMP = 'LT',
                               VALE      = 25)));

TEST_TABLE(TABLE=DEP1M,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP1M,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');


# POUR LA DEUXIEME FISSURE

DEP2P = CALC_TABLE(TABLE = DEP,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^P]'),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'COOR_Y',
                               CRIT_COMP = 'GT',
                               VALE      = 25)));

TEST_TABLE(TABLE=DEP2P,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP2P,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

DEP2M = CALC_TABLE(TABLE = DEP,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^M]'),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'COOR_Y',
                               CRIT_COMP = 'GT',
                               VALE      = 25)));

TEST_TABLE(TABLE=DEP2M,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=DEPY,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP2M,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=DEPY,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE');




FIN();
