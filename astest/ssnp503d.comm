# CONTACT-FROTTANT EN GRANDS GLISSEMENTS
# AVEC XFEM POUR DES FISSURES HORIZONTALES
#

DEBUT(CODE=_F(NOM='SSNP503D',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

PRE_GIBI();

typma=2

if (typma==1):
  MAILLAG2=LIRE_MAILLAGE(INFO=1,);
  Y=3

if (typma==2):
  MAILLAG1=LIRE_MAILLAGE(INFO=1,);
  MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='SURF',),);
  Y=2.5

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,Y,),
                                       VECT_NORMALE = (0.0,1.0),
                                       PRECISION = 1E-1,),),)

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('SURF',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN_XFEM_CONT',),),);             

LN1=FORMULE(NOM_PARA=('X','Y'),VALE='Y-2.5');

LN2=FORMULE(NOM_PARA=('X','Y'),VALE='-Y+6.5');

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1,),
                     GROUP_MA_ENRI='SURF');

FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN2,),
                     GROUP_MA_ENRI='SURF');

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='OUI',
                         INFO=1,);

                         
CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      REAC_GEOM      = 'AUTOMATIQUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 10,
                      ITER_GEOM_MAXI = 10,
                      FROTTEMENT     = 'COULOMB',
                      ZONE = (_F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.0,   
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',   
                                 COEF_ECHELLE  = 1.,
                                 FISS_MAIT     = FISS1,
                                 ALGO_LAGR     = 'VERSION1',
                              ),
                             _F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.0,   
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',   
                                 COEF_ECHELLE  = 1.,
                                 FISS_MAIT     = FISS2,
                                 ALGO_LAGR     = 'VERSION1',
                              ),
                            )
                    );                          


E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER,
                                ),);

DEPX=2.00
DEPY=1.E-3
ZERO=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIGINF',
                                DY=DEPY,),
                             _F(GROUP_MA='LIGSUP',
                                DY=-DEPY,),                        
                             _F(GROUP_NO=('P1','P2','P3','P4'),
                                DX=0.0,),                              
                             ),
                   INFO=1,);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(NOEUD=('N21','N22','N23','N24',),
                                DX=DEPX,
                                ),
                             ),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                   LIAISON_DDL=(_F(NOEUD=('N20','N20','N21','N21',),
                                   DDL=('DCX','H1X','DCX','H1X',),
                                   COEF_MULT=(.5,0.5,0.5,.5),
                                   COEF_IMPO=DEPX,),
                                _F(NOEUD=('N24','N24','N25','N25',),
                                   DDL=('DCX','H1X','DCX','H1X',),
                                   COEF_MULT=(.5,0.5,0.5,.5),
                                   COEF_IMPO=DEPX,),
                                ),
                    INFO=1,);               

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      CONTACT_XFEM=CTXFEM,
                      LIAISON_XFEM='OUI',
                      INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,      
                    CONTACT = CTXFEM,        
                    EXCIT=(
                           _F(CHARGE=CHXFEM,),                                    
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2),),  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06,),),
                    NEWTON=_F(REAC_ITER=1,),
                    SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=10,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]

vale = [[0.0,-7.1041387832239E+03,-5.5751916294848E+03,-5.6055420096676E+03,-3.6920555634806E+03],
        [0.0,-9.1709838813643E+03,-1.1435732074163E+04,-1.0391561917541E+04,-7.7671507432153E+03],
        [0.0,0.0,-1.9485288504693E+04,-1.6399582621920E+04,-1.0605892575698E+04],
        [0.0,0.0,-1.7065781429260E+04,-2.0872173661117E+04,-1.6114705916606E+04]]

#vale = [[0.0,-7.2381997723673E+03,-5.4928433477996E+03,-5.5947441906363E+03,-3.6379529728764E+03],
#        [0.0,-9.3916134615046E+03,-1.1352700359925E+04,-1.0340028138765E+04,-7.6451891437089E+03],
#        [0.0,0.0,-1.9702365044462E+04,-1.6378559150242E+04,-1.0278180761557E+04],
#        [0.0,0.0,-1.7447691288661E+04,-2.0847840754336E+04,-1.5703134828960E+04]]
        
for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO=('NFISSU'),
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1',),
                                         OPERATION='EXTRACTION',),);
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              FORMAT_C='MODULE_PHASE',
              NOM_PARA=('NOEUD','INST','COOR_X','LAGS_C','LAGS_F1',),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X',),
                     ORDRE='CROISSANT',),);
   
   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-10
        critere = 'ABSOLU'
      else:
        precision = 1.0E-4 
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE=vale[i-1] [j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU',),);

FIN();
