                             ALGO_CONT      = 'PENALISATION',
                             ALGO_FROT      = 'PENALISATION',
                             COULOMB        = 0.,
                             E_N            = 1.E10,
                             E_T            = 1.E16,
                             GROUP_MA_ESCL  = 'FACEINF',
                             GROUP_MA_MAIT  = 'SSOL',
                             DIST_ESCL      = F_DIST_E,
                               ),
