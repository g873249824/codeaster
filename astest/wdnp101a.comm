                      INST = t_fin,
                      INFO=1,);

    SIG[n]=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                      OPERATION='EXTR',
                      RESULTAT=U[n],
                      NOM_CHAM='SIEF_ELGA',
                      INST = t_fin,
                      INFO=1,);

    VAR[n]=CREA_CHAMP(TYPE_CHAM='ELGA_VARI_R',
                OPERATION='EXTR',
                RESULTAT=U[n],
                NOM_CHAM='VARI_ELGA',
                INST = t_fin,
