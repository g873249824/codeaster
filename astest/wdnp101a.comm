                      ASSE=(_F(GROUP_MA=list_group_ma[:n+1],
                               CHAM_GD=DEP[n-1])));

    INISIG=CREA_CHAMP(INFO=1,
                      TYPE_CHAM='ELGA_SIEF_R',
                      OPERATION='ASSE',
                      MODELE=MOD[n],
                      PROL_ZERO='OUI',
                      ASSE=(_F(GROUP_MA=list_group_ma[n+1],
                               CHAM_GD=SIEF,
                               CUMUL='OUI',
                               COEF_R=1.0),
                            _F(GROUP_MA=list_group_ma[:n+1],
                               CHAM_GD=SIG[n-1],
                               CUMUL='OUI',
                               COEF_R=1.0),                 
                            ),
                      );

    CART1 = CREA_CHAMP(INFO = 1,
                   TYPE_CHAM='ELGA_NEUT_R',
                   OPERATION='AFFE',
                   MODELE=MOD[n],
                   PROL_ZERO='OUI',
