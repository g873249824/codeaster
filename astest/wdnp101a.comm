                            _F(GROUP_MA=list_group_m2[0:nbcouche],
                               CHAM_GD=SIEFCO,
                               CUMUL='OUI',
                               COEF_R=1.0),
                               
                            ),
                      );
                            
CARTCO = CREA_CHAMP(INFO = 1,
