from Numeric import *


# Nombre de couches
# (dont la couche elastique correspondant au BEDROCK)
nbcouche  = 11    ;

UN        = 1.0   ;
ZERO      = 0.0   ;
P_PELOUSE = 1.E+2 ;

YOUNG = 1.E+9 ;          # [Pa]
NU    = 0.3   ;
