                               ITER_INTE_PAS=-10,
                               RELATION_KIT=('ELAS','LIQU_SATU','HYDR_UTIL'),
                               GROUP_MA=list_group_ma[0]),
                            _F(
                               ITER_INTE_MAXI=50,
                               ITER_INTE_PAS=-10,
                               RELATION='KIT_HM',
                               RELATION_KIT=('HUJEUX','LIQU_SATU','HYDR_UTIL'),
                               GROUP_MA=list_group_ma[1]),
                            ),
