# Calcul par une méthode directe,
# en construisant les matrices assemblées
# fréquence par fréquence


DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'),);


# LECTURE DU MAILLAGE AU FORMAT MED

MAIL=LIRE_MAILLAGE(UNITE=20,
                   FORMAT='MED',);


# DEFINITION DE LA MODELISATION PLAQUE

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=(_F(GROUP_MA=('DESSUS','DESSOUS',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='DKT',),
                         _F(GROUP_MA=('VOLUME',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),),);


# DEFINITION DES MATERIAUX ELASTIQUES (A PROPRIETES CONSTANTES)

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.1E11,
                            NU=0.3,
                            RHO=7800,
                            AMOR_HYST=0.002,),);

ALU=DEFI_MATERIAU(ELAS=_F(E=7.0E10,
                          NU=0.3,
                          RHO=2700,
                          AMOR_HYST=0.001,),);

# DEFINITION DU MATERIAU VISCOELASTIQUE A 1 HZ

VISCO0=DEFI_MATERIAU(ELAS=_F(E=23.2E6,
                             NU=0.45,
                             RHO=1200,
                             AMOR_HYST=1.1,),);

# AFFECTATION DES MATERIAUX AUX ELEMENTS PLAQUES DU MAILLAGE

CHMAT0=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='DESSOUS',
                             MATER=ACIER,),
                          _F(GROUP_MA='DESSUS',
                             MATER=ALU,),
                          _F(GROUP_MA='VOLUME',
                             MATER=VISCO0,),),);

# DEFINITION ET AFFECTATION DES PROPRIETES DES PLAQUES

CARAPLAQ=AFFE_CARA_ELEM(MODELE=MODELE,
                        COQUE=(_F(GROUP_MA='DESSOUS',
                                  EPAIS=1E-3,
                                  EXCENTREMENT=0.5E-3,
                                  INER_ROTA='OUI',),
                               _F(GROUP_MA='DESSUS',
                                  EPAIS=0.5E-3,
                                  EXCENTREMENT=0.25E-3,
                                  INER_ROTA='OUI',),),);

# DEFINITION DES CONDITIONS D'ENCASTREMENT

CONDLIM=AFFE_CHAR_MECA(MODELE=MODELE,
                       DDL_IMPO=_F(GROUP_MA='ENCAS',
                                   LIAISON='ENCASTRE',),);

# DEFINITION DU CHARGEMENT

EXCIT=AFFE_CHAR_MECA(MODELE=MODELE,
                     FORCE_NODALE=_F(GROUP_NO='A',
                                     FZ=1.,),);


# CALCUL DES MATRICES ASSEMBLEES K ET M ET DU VECTEUR CHARGEMENT ASSEMBLE
# M EST CONSTANTE

MASSE=CALC_MATR_ELEM(OPTION='MASS_MECA',
                     MODELE=MODELE,
                     CHAM_MATER=CHMAT0,
                     CARA_ELEM=CARAPLAQ,
                     CHARGE=CONDLIM,);

RIGIDIT0=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHMAT0,
                        CARA_ELEM=CARAPLAQ,
                        CHARGE=CONDLIM,);

RIGIHYS0=CALC_MATR_ELEM(OPTION='RIGI_MECA_HYST',
                        MODELE=MODELE,
                        CHARGE=CONDLIM,
                        CHAM_MATER=CHMAT0,
                        CARA_ELEM=CARAPLAQ,
                        RIGI_MECA=RIGIDIT0,);

FORCE=CALC_VECT_ELEM(OPTION='CHAR_MECA',
                     CHARGE=EXCIT,);

NUMEDDL=NUME_DDL(MATR_RIGI=RIGIDIT0,);

ASSEM=ASSE_MATRICE(MATR_ELEM=MASSE,
                   NUME_DDL=NUMEDDL,);

ASSEK0=ASSE_MATRICE(MATR_ELEM=RIGIDIT0,
                    NUME_DDL=NUMEDDL,);

ASSEKC0=ASSE_MATRICE(MATR_ELEM=RIGIHYS0,
                     NUME_DDL=NUMEDDL,);

ASSEF=ASSE_VECTEUR(VECT_ELEM=FORCE,
                   NUME_DDL=NUMEDDL,);


# CALCUL DE LA REPONSE DU SYSTEME A 1 HZ

DYNA0=DYNA_VIBRA(TYPE_CALCUL='HARM',
                 BASE_CALCUL='PHYS',
                 MATR_MASS=ASSEM,
                 MATR_RIGI=ASSEKC0,
                 FREQ=1.,
                 TOUT_CHAM='OUI',
                 EXCIT=_F(VECT_ASSE=ASSEF,
                          COEF_MULT=1.0,),);


###########################################################

# DEFINITION DU MATERIAU VISCOELASTIQUE A 100 HZ

VISCO1=DEFI_MATERIAU(ELAS=_F(E=203E6,
                             NU=0.45,
                             RHO=1200,
                             AMOR_HYST=0.6,),);

CHMAT1=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=(_F(GROUP_MA='DESSOUS',
                              MATER=ACIER,),
                           _F(GROUP_MA='DESSUS',
                              MATER=ALU,),
                           _F(GROUP_MA='VOLUME',
                              MATER=VISCO1,),),);


# CALCUL DES MATRICES ASSEMBLEES K ET M ET DU VECTEUR CHARGEMENT ASSEMBLE
# Rq : on ne recalcule pas M car elle est constante

RIGIDIT1=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHMAT1,
                        CARA_ELEM=CARAPLAQ,
                        CHARGE=CONDLIM,);

RIGIHYS1=CALC_MATR_ELEM(OPTION='RIGI_MECA_HYST',
                        MODELE=MODELE,
                        CHARGE=CONDLIM,
                        CHAM_MATER=CHMAT1,
                        CARA_ELEM=CARAPLAQ,
                        RIGI_MECA=RIGIDIT1,);

# les conditions aux limites ne changent pas
# donc la numérotation des DDL non plus

ASSEK1=ASSE_MATRICE(MATR_ELEM=RIGIDIT1,
                    NUME_DDL=NUMEDDL,);

ASSEKC1=ASSE_MATRICE(MATR_ELEM=RIGIHYS1,
                     NUME_DDL=NUMEDDL,);


# CALCUL DE LA REPONSE DU SYSTEME A 100 HZ

DYNA1=DYNA_VIBRA(TYPE_CALCUL='HARM',
                 BASE_CALCUL='PHYS',
                 MATR_MASS=ASSEM,
                 MATR_RIGI=ASSEKC1,
                 FREQ=100.,
                 TOUT_CHAM='OUI',
                 EXCIT=_F(VECT_ASSE=ASSEF,
                          COEF_MULT=1.0,),);


###########################################################


# DEFINITION DES MATERIAUX A 500 HZ

VISCO2=DEFI_MATERIAU(ELAS=_F(E=348E6,
                             NU=0.45,
                             RHO=1200,
                             AMOR_HYST=0.4,),);

CHMAT2=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=(_F(GROUP_MA='DESSOUS',
                              MATER=ACIER,),
                           _F(GROUP_MA='DESSUS',
                              MATER=ALU,),
                           _F(GROUP_MA='VOLUME',
                              MATER=VISCO2,),),);

# CALCUL DES MATRICES ASSEMBLEES K ET M ET DU VECTEUR CHARGEMENT ASSEMBLE
# Rq : on ne recalcule pas M car elle est constante

RIGIDIT2=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHMAT2,
                        CARA_ELEM=CARAPLAQ,
                        CHARGE=CONDLIM,);

RIGIHYS2=CALC_MATR_ELEM(OPTION='RIGI_MECA_HYST',
                        MODELE=MODELE,
                        CHARGE=CONDLIM,
                        CHAM_MATER=CHMAT2,
                        CARA_ELEM=CARAPLAQ,
                        RIGI_MECA=RIGIDIT2,);

# les conditions aux limites ne changent pas
# donc la numérotation des DDL non plus

ASSEK2=ASSE_MATRICE(MATR_ELEM=RIGIDIT2,
                    NUME_DDL=NUMEDDL,);

ASSEKC2=ASSE_MATRICE(MATR_ELEM=RIGIHYS2,
                     NUME_DDL=NUMEDDL,);


# CALCUL DE LA REPONSE DU SYSTEME A 500 HZ

DYNA2=DYNA_VIBRA(TYPE_CALCUL='HARM',
                 BASE_CALCUL='PHYS',
                 MATR_MASS=ASSEM,
                 MATR_RIGI=ASSEKC2,
                 FREQ=500.,
                 TOUT_CHAM='OUI',
                 EXCIT=_F(VECT_ASSE=ASSEF,
                          COEF_MULT=1.0,),);

###########################################################


TEST_RESU(RESU=(_F(RESULTAT=DYNA0,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   GROUP_NO='A',
                   NOM_CMP='DZ',
                   VALE_CALC_C=3.84063122275e-04 - 8.77803614739e-05j ),
                _F(RESULTAT=DYNA1,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   GROUP_NO='A',
                   NOM_CMP='DZ',
                   VALE_CALC_C=-1.1663671537e-04 - 9.6134604316e-06j ),
                _F(RESULTAT=DYNA2,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   GROUP_NO='A',
                   NOM_CMP='DZ',
                   VALE_CALC_C=-1.302768494e-05 - 1.65977932083e-06j ),
           ),);





###################################################################
# CALCUL ETAPE PAR ETAPE SANS UTILISER LA MACRO-COMMANDE DYNA_VISCO
# Le code ci-dessous est donné dans un but pédagogique, il présente
# la syntaxe à utiliser si on devait calculer la réponse du système
# en de nombreuses fréquences, grâce à une boucle Python,
# et visualiser l'amplitude de la réponse harmonique
# Rq : il faut mettre PAR_LOT='NON' dans DEBUT
###################################################################
calcul_complet = False

if calcul_complet == True:
    from math import exp, log
    
    # DEFINITION DE LA DEPENDANCE EN FREQUENCE DES PROPRIETES DES MATERIAUX VISCOELASTIQUES
    
    list_f=DEFI_LIST_REEL(VALE=(1.,10.,50.,100.,500.,1000.,1500.),);
    
    list_E=DEFI_LIST_REEL(VALE=(23.2E6,58.E6,145.E6,203.E6,348.E6,435.E6,464.E6),);
    
    list_eta=DEFI_LIST_REEL(VALE=(1.1,0.85,0.7,0.6,0.4,0.35,0.34),);                            
    
    # DEFINITION DES FREQUENCES DE CALCUL DE LA REPONSE HARMONIQUE
    listfr=DEFI_LIST_REEL(DEBUT=1.,
                          INTERVALLE=(_F(JUSQU_A=500.,
                                         PAS=1.,),),);
    
    
    # CALC DE LA REPONSE POUR LA PREMIERE FREQUENCE
    dyna=DYNA_VIBRA(TYPE_CALCUL='HARM',
                    BASE_CALCUL='PHYS',
                    MATR_MASS=ASSEM,
                    MATR_RIGI=ASSEKC0,
                    FREQ=1.,
                    TOUT_CHAM='OUI',
                    EXCIT=_F(VECT_ASSE=ASSEF,
                             COEF_MULT=1.0,),);
    
    # BOUCLE SUR LES FREQUENCES DEMANDEES (A PARTIR DE LA DEUXIEME)
    for i in range (1,len(listfr.Valeurs())):
    
        f = listfr.Valeurs()[i]   # fréquence courante
    
        ind_i = list_f.Valeurs().index(next(x for x in list_f.Valeurs() if x>f))-1
    
        x1 = list_f.Valeurs()[ind_i]     # valeur inférieure la plus proche
        x2 = list_f.Valeurs()[ind_i+1]   # valeur inférieure la plus proche
        
        y1 = list_E.Valeurs()[ind_i]
        y2 = list_E.Valeurs()[ind_i+1]
        E = exp(log(y1)+(log(f)-log(x1))*(log(y2)-log(y1)) / (log(x2)-log(x1)))
    
        y1 = list_eta.Valeurs()[ind_i]
        y2 = list_eta.Valeurs()[ind_i+1]
        eta = exp(log(y1)+(log(f)-log(x1))*(log(y2)-log(y1)) / (log(x2)-log(x1)))
    
    
        mat_visc=DEFI_MATERIAU(ELAS=_F(E=E,
                                       NU=0.45,
                                       RHO=1200,
                                       AMOR_HYST=eta,),);
    
        chmat=AFFE_MATERIAU(MAILLAGE=MAIL,
                             AFFE=(_F(GROUP_MA='DESSOUS',
                                      MATER=ACIER,),
                                   _F(GROUP_MA='DESSUS',
                                      MATER=ALU,),
                                   _F(GROUP_MA='VOLUME',
                                      MATER=mat_visc,),),);
        
        # CALCUL DES MATRICES ASSEMBLEES K ET M ET DU VECTEUR CHARGEMENT ASSEMBLE
        
        # Rq : ici, il n'est pas nécessaire de recalculer la matrice de masse à chaque fréquence
        # car la masse volumique est indépendante de la fréquence.
        # On le fait pour présenter un cas plus général
        M_elem=CALC_MATR_ELEM(OPTION='MASS_MECA',
                              MODELE=MODELE,
                              CHAM_MATER=chmat,
                              CARA_ELEM=CARAPLAQ,
                              CHARGE=CONDLIM,);
        
        K_elem=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                              MODELE=MODELE,
                              CHAM_MATER=chmat,
                              CARA_ELEM=CARAPLAQ,
                              CHARGE=CONDLIM,);
        
        Kh_elem=CALC_MATR_ELEM(OPTION='RIGI_MECA_HYST',
                               MODELE=MODELE,
                               CHARGE=CONDLIM,
                               CHAM_MATER=chmat,
                               CARA_ELEM=CARAPLAQ,
                               RIGI_MECA=K_elem,);
        
        for_elem=CALC_VECT_ELEM(OPTION='CHAR_MECA',
                                CHARGE=EXCIT,);
        
        num_ddl=NUME_DDL(MATR_RIGI=K_elem,);
        
        M_asse=ASSE_MATRICE(MATR_ELEM=M_elem,
                            NUME_DDL=num_ddl,);
        
        K_asse=ASSE_MATRICE(MATR_ELEM=K_elem,
                            NUME_DDL=num_ddl,);
        
        Kh_asse=ASSE_MATRICE(MATR_ELEM=Kh_elem,
                             NUME_DDL=num_ddl,);
        
        for_asse=ASSE_VECTEUR(VECT_ELEM=for_elem,
                            NUME_DDL=num_ddl,);
        

        # CALCUL DE LA REPONSE DU SYSTEME A LA FREQUENCE f
        
        dyna=DYNA_VIBRA(reuse=dyna,
                        RESULTAT=dyna,
                        TYPE_CALCUL='HARM',
                        BASE_CALCUL='PHYS',
                        MATR_MASS=M_asse,
                        MATR_RIGI=Kh_asse,
                        FREQ=f,
                        TOUT_CHAM='OUI',
                        EXCIT=_F(VECT_ASSE=for_asse,
                                 COEF_MULT=1.0,),);
    
        DETRUIRE(CONCEPT=_F(NOM=(mat_visc, chmat, M_elem, K_elem, Kh_elem, for_elem, M_asse, K_asse, Kh_asse, for_asse)));
        if i != len(listfr.Valeurs())-1:
            DETRUIRE(CONCEPT=_F(NOM=num_ddl));
    
    TEST_RESU(RESU=(
                    _F(RESULTAT=dyna,
                       NUME_ORDRE=1,
                       NOM_CHAM='DEPL',
                       GROUP_NO='A',
                       NOM_CMP='DZ',
                       VALE_CALC_C=3.84063122275e-04 - 8.77803614739e-05j ),
                    _F(RESULTAT=dyna,
                       NUME_ORDRE=100,
                       NOM_CHAM='DEPL',
                       GROUP_NO='A',
                       NOM_CMP='DZ',
                       VALE_CALC_C=-1.1663671537e-04 - 9.6134604316e-06j ),
                    _F(RESULTAT=dyna,
                       NUME_ORDRE=500,
                       NOM_CHAM='DEPL',
                       GROUP_NO='A',
                       NOM_CMP='DZ',
                       VALE_CALC_C=-1.302768494e-05 - 1.65977932083e-06j ),
                    ),);
    
    dz_A=RECU_FONCTION(RESULTAT=dyna,
                       NOM_CHAM='DEPL',
                       NOM_CMP='DZ',
                       GROUP_NO='A')
    
    dz_A_abs=CALC_FONCTION(EXTRACTION=_F(FONCTION=dz_A,
                                         PARTIE='MODULE'),);
    
    IMPR_FONCTION(FORMAT='XMGRACE',
                  UNITE=29,
                  COURBE=_F(FONCTION=dz_A_abs))

FIN();
