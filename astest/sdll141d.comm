                   RESULTAT=MODES,
                   VALE_CALC= 1.96427913E+03,
                   VALE_REFE=1963.9595999999999,),
                ),
          )

OM=10000.

GYOM=COMB_MATR_ASSE(COMB_R=(_F(MATR_ASSE=GYASS, COEF_R=OM,),
                            _F(MATR_ASSE=AMOR, COEF_R=1.,),))


MODEG=CALC_MODES(MATR_RIGI=RIGIDITE,
                 VERI_MODE=_F(SEUIL=2.E-3,
                              ),
                 OPTION='CENTRE',
                 CALC_FREQ=_F(NMAX_FREQ=30,
                              FREQ=1.,
                              ),
                 MATR_AMOR=GYOM,
                 MATR_MASS=MASSE,
                 SOLVEUR_MODAL=_F(METHODE='QZ',
                                  ),
                 )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
