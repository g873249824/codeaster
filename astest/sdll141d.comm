                   VALE_CALC= 1.13239181E+03,
                   VALE_REFE=1132.3345999999999,),
                _F(NUME_ORDRE=7,
                   PARA='FREQ',
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=MODEG,
                   VALE_CALC= 1.91639580E+03,
                   VALE_REFE=1916.0764999999999,),
                _F(NUME_ORDRE=9,
                   PARA='FREQ',
