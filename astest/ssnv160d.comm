                    CHAM_MATER=CHM0,
                    EXCIT=(_F(CHARGE=CHA0,),
                           _F(CHARGE=CHA1,),),
                    COMP_ELAS=_F(RELATION='ELAS',),
                    INCREMENT=_F(LIST_INST=LI1,
                                 INST_INIT=0.0,
                                 INST_FIN=1.E2,),
                    NEWTON=_F(MATRICE='ELASTIQUE',
                              REAC_ITER=20,),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=10),
                    SOLVEUR=_F(METHODE='LDLT',),);

############################################
#
#  EXTRACTION DES CONTRAINTES
