#
#

#   OPERATIONS SUR LE MAILLAGE
#
#

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='AB',),
                               _F(GROUP_MA='BC',),
                               _F(GROUP_MA='CD',),
                               _F(GROUP_MA='DA',),),);

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='AXIS',),);

MAIL = MODI_MAILLAGE(reuse=MAIL,
                     MAILLAGE=MAIL,
                     ORIE_PEAU_2D=(_F(GROUP_MA='BC',),
                                   _F(GROUP_MA='CD',),),);

