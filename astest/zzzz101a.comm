IXX_Q4P=IXX_QUA4 + (MAS_QUA4 * ((2. - 3.)*(2. - 3.) + (7. - 2.)*(7. - 2.)))

IYY_Q4P=IYY_QUA4 + (MAS_QUA4 * ((2.5 - 4.)*(2.5 - 4.) + (7. - 2.)*(7. - 2.)))

IZZ_Q4P=IZZ_QUA4 + (MAS_QUA4 * ((2. - 3.)*(2. - 3.)+ (2.5 - 4.)*(2.5 - 4.)))

