                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                _F(PARA='FREQ',
                   NUME_MODE=4,
                   RESULTAT=DYNAM,
                   VALE_CALC= 1.49308600E+03,
                   VALE_REFE=1452.0,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                _F(PARA='FREQ',
                   NUME_MODE=5,
