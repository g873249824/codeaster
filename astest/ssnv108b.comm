#
# MODELISATION : ELEMENTS X-FEM 2D
#

DEBUT(CODE=_F(
              NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'));

PRE_GMSH()

MA1=LIRE_MAILLAGE();

MA1=DEFI_GROUP(reuse =MA1,
               MAILLAGE=MA1,
               CREA_GROUP_MA =_F(NOM='VOL',GROUP_MA='GM1'),
               CREA_GROUP_NO =(_F(NOM='A', GROUP_MA='GM2'),
                               _F(NOM='B', GROUP_MA='GM3'),
                               _F(NOM='BP',GROUP_MA='GM4'),
                               _F(NOM='NTEST',GROUP_MA='GM5'),
                               ),
               );

MODELEIN=AFFE_MODELE(MAILLAGE=MA1,
                     AFFE=_F(GROUP_MA='VOL',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),
                     );

LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y');
LT=FORMULE(NOM_PARA=('X','Y'),VALE='X-29.78');

FISS=DEFI_FISS_XFEM(MAILLAGE=MA1,
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,);

FONC1=DEFI_FONCTION(NOM_PARA='EPSI',
                    VALE=
                    (0.001707,350.0,
                     0.002   ,394.0,
                     0.002333,450.5,
                     0.002667,491.5,
                     0.003   ,518.8,
                     0.003333,534.4,
                     0.004   ,546.1,
                     0.004667,551.9,
                     0.008   ,565.6,
                     0.016   ,590.0,
                     0.024   ,614.3,
                     0.035   ,643.6,
                     0.045   ,663.1,
                     0.055   ,669.0,
                     0.075   ,674.8,
                     0.1     ,676.0,
                     1.0     ,680.0,
                     100.0   ,700.0,),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

MAT=DEFI_MATERIAU(ELAS=_F(E=205000.0,
                          NU=0.3,
                          RHO=0.0,
                          ALPHA=0.0,),
                  TRACTION=_F(SIGM=FONC1,),
                  );

CHMAT=AFFE_MATERIAU(MAILLAGE=MA1,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,
                            ),);


uy = 1.
CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                  DDL_IMPO=(
#                           _F(GROUP_NO='A',DX=0.,DY=0.),
                            _F(GROUP_NO='B',DX=0.,DY=uy),
                            _F(GROUP_NO='BP',DX=0.,DY=-uy),),);

RAMPE=DEFI_FONCTION(  NOM_PARA='INST',
                        VALE=(0.,0.,1.,1.,),
                        PROL_DROITE='LINEAIRE'
                      )

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=5,),);

U=STAT_NON_LINE(MODELE=MODELEK,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE=CH,FONC_MULT = RAMPE),
                       ),
                COMPORTEMENT=_F(RELATION='ELAS_VMIS_TRAC',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
#               CONVERGENCE=_F(SIGM_REFE=0.055,
#                              RESI_REFE_RELA=1.0,
#                              ITER_GLOB_MAXI=25,),
            );

# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),)

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = U,
                        INFO          = 2)

#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));

# -------------------------------------------------------------
#                           TESTS
# -------------------------------------------------------------


TABDEP= POST_RELEVE_T ( ACTION =_F(INTITULE   = 'DEPLACEMENT_FISSURE',
                                   GROUP_NO   = 'NTEST',
                                   RESULTAT   = U,
                                   NOM_CHAM   = 'DEPL',
                                   NOM_CMP    = ( 'DX','DY',),
                                   OPERATION  = 'EXTRACTION',),);

IMPR_TABLE(TABLE=TABDEP,);

# CETTE VALEUR A ETE OBTENUE PAR LA MODELISATION A EN VERSION 9.0.13
DEPREF=-7.8967073390778E-01

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.16,
           VALE_CALC=-0.909097075,
           VALE_REFE=-0.78967073390777998,
           NOM_PARA='DY',
           TABLE=TABDEP,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

# on double de non regression
TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=-0.90909707489146996,

           NOM_PARA='DY',
           TABLE=TABDEP,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

FIN();
