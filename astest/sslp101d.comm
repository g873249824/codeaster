# PLAQUE FISSUREE EN TRACTION (MAILLAGE ALIBABA )
# CALCUL DE L'INTEGRALE G SUR DIFFERENTES COURONNES D'INTEGRATION
# AVEC L OPTION LAGRANGIENNE ALPHA=0 PUIS ALPHA=1

DEBUT(CODE=_F( NOM = 'SSLP101D',NIV_PUB_WEB='INTERNET'))

MA=LIRE_MAILLAGE(  )

MO=AFFE_MODELE(  MAILLAGE=MA, 
      AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = 'D_PLAN'))

THETA1=CALC_THETA(   MODELE=MO,  THETA_2D=_F(  NOEUD = 'N668', MODULE = 1.,
                                            R_INF = 22.04078,
                                            R_SUP = 30.),
                                              DIRECTION=( 1., 0., 0.,)
                   )

THETA2=CALC_THETA(   MODELE=MO,  THETA_2D=_F(  NOEUD = 'N668', MODULE = 1.,
                                            R_SUP = 22.04078,
                                            R_INF = 15.3061094),
                                              DIRECTION=( 1., 0., 0.,)
                   )

THETA3=CALC_THETA(   MODELE=MO,  THETA_2D=_F(  NOEUD = 'N668', MODULE = 1.,
                                            R_INF = 9.79591846,
                                            R_SUP = 15.3061094),
                                              DIRECTION=( 1., 0., 0.,)
                   )

THETA4=CALC_THETA(   MODELE=MO,  THETA_2D=_F(  NOEUD = 'N668', MODULE = 1.,
                                            R_SUP = 9.79591846,
                                            R_INF = 5.510197639),
                                              DIRECTION=( 1., 0., 0.,)
                   )

THETA5=CALC_THETA(   MODELE=MO,  THETA_2D=_F(  NOEUD = 'N668', MODULE = 1.,
                                            R_INF = 2.448979377,
                                            R_SUP = 5.510197639),
                                              DIRECTION=( 1., 0., 0.,)
                   )

THETA6=CALC_THETA(   MODELE=MO,  THETA_2D=_F(  NOEUD = 'N668', MODULE = 1.,
                                            R_SUP = 2.448979377,
                                            R_INF = 0.61225116),
                                              DIRECTION=( 1., 0., 0.,)
                   )

MAT=DEFI_MATERIAU(ELAS=_F(  E = 200000., NU = 0.3, RHO = 0., ALPHA = 0.))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,AFFE=_F(  TOUT = 'OUI',MATER = MAT))

CH=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=( 
            _F(  GROUP_NO = 'GRNM5', DY = 0.),_F(  NOEUD = 'N451', DX = 0.)),
            FORCE_CONTOUR=_F(  GROUP_MA = 'GRMA1', FY = 1.) )

FCONT  = FORMULE(NOM_PARA=('X','Y'),VALE='1.')

CHFONC=AFFE_CHAR_MECA_F(  MODELE=MO, 
                            FORCE_CONTOUR=_F(  GROUP_MA = 'GRMA1',
                                             FY = FCONT)    )

ME1=CALC_MATR_ELEM(         MODELE=MO,
                                CHARGE=CH,
                            CHAM_MATER=CHMAT,
                               OPTION='RIGI_MECA'   )

ME2=CALC_MATR_ELEM(         MODELE=MO,
                                CHARGE=CH,
                            CHAM_MATER=CHMAT,
                           PROPAGATION=1.0,
                                 THETA=THETA1,
                               OPTION='RIGI_MECA_LAGR'   )

#

VE=CALC_VECT_ELEM(         CHARGE=CH,
                            CHAM_MATER=CHMAT,
                               OPTION='CHAR_MECA'   )

NU1=NUME_DDL(            MATR_RIGI=ME1          )

K1=ASSE_MATRICE(        MATR_ELEM=ME1,
                              NUME_DDL=NU1)

F1=ASSE_VECTEUR(        VECT_ELEM=VE,
                              NUME_DDL=NU1          )

K1=FACT_LDLT(      reuse=K1,     MATR_ASSE=K1           )

DEP1=RESO_LDLT(           MATR_FACT=K1,
                               CHAM_NO=F1           )

NU2=NUME_DDL(            MATR_RIGI=ME2          )

K2=ASSE_MATRICE(        MATR_ELEM=ME2,
                              NUME_DDL=NU2)

F2=ASSE_VECTEUR(        VECT_ELEM=VE,
                              NUME_DDL=NU2          )

K2=FACT_LDLT(      reuse=K2,     MATR_ASSE=K2           )

DEP2=RESO_LDLT(           MATR_FACT=K2,
                               CHAM_NO=F2           )

#
#            ALPHA= 0.  OPTION CALC_G PAR DEFAUT
#
G1=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP1,   THETA=THETA1,
                    EXCIT=_F(CHARGE=CHFONC,),
                       COMP_ELAS=_F( RELATION = 'ELAS',
                                  DEFORMATION = 'PETIT'),
                   SYME_CHAR='SYME',  CHAM_MATER=CHMAT )

G2=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP1,   THETA=THETA2,
                       COMP_ELAS=_F( RELATION = 'ELAS',
                                  DEFORMATION = 'PETIT'),
                    EXCIT=_F(CHARGE=CH,), SYME_CHAR='SYME',  CHAM_MATER=CHMAT )

G3=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP1,   THETA=THETA3,
                       COMP_ELAS=_F( RELATION = 'ELAS',
                                  DEFORMATION = 'PETIT'),
                    EXCIT=_F(CHARGE=CH,), SYME_CHAR='SYME',  CHAM_MATER=CHMAT )

G4=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP1,   THETA=THETA4,
                       COMP_ELAS=_F( RELATION = 'ELAS',
                                  DEFORMATION = 'PETIT'),
                    EXCIT=_F(CHARGE=CH,), SYME_CHAR='SYME',  CHAM_MATER=CHMAT )

G5=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP1,   THETA=THETA5,
                     EXCIT=_F(CHARGE=CHFONC,),
                        COMP_ELAS=_F( RELATION = 'ELAS',
                                   DEFORMATION = 'PETIT'),
                    SYME_CHAR='SYME',  CHAM_MATER=CHMAT )

G6=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP1,   THETA=THETA6,
                       COMP_ELAS=_F( RELATION = 'ELAS',
                                   DEFORMATION = 'PETIT'),
                    EXCIT=_F(CHARGE=CH,), SYME_CHAR='SYME',  CHAM_MATER=CHMAT )

#
#            ALPHA= 1.
#
GLAG=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP2,  THETA=THETA1,  EXCIT=_F(CHARGE=CH,),
                     SYME_CHAR='SYME',PROPAGATION=1., CHAM_MATER=CHMAT,
                      OPTION='CALC_G_LAGR')

GLAGF=CALC_G_THETA_T(  MODELE=MO,  DEPL=DEP2,  THETA=THETA1,
                       EXCIT=_F(CHARGE=CHFONC,),
                     SYME_CHAR='SYME',PROPAGATION=1., CHAM_MATER=CHMAT,
                      OPTION='CALC_G_LAGR')

#
#       COMPARAISONS AUX VALEURS DE REFERENCE
#


TEST_TABLE(       TABLE=G1,
                       NOM_PARA='G',
                           VALE=2.1015E-3,
                      PRECISION=5.E-3   )

TEST_TABLE(       TABLE=G2,
                       NOM_PARA='G',
                           VALE=2.1015E-3,
                      PRECISION=5.E-3   )

TEST_TABLE(       TABLE=G3,
                       NOM_PARA='G',
                           VALE=2.1015E-3,
                      PRECISION=5.E-3   )

TEST_TABLE(       TABLE=G4,
                       NOM_PARA='G',
                           VALE=2.1015E-3,
                      PRECISION=5.E-3   )

TEST_TABLE(       TABLE=G5,
                       NOM_PARA='G',
                           VALE=2.1015E-3,
                      PRECISION=5.E-3   )

TEST_TABLE(       TABLE=G6,
                       NOM_PARA='G',
                           VALE=2.1015E-3,
                      PRECISION=1.E-2   )

TEST_TABLE(       TABLE=GLAG,
                       NOM_PARA='G',
                           VALE=2.2610E-3,
                      PRECISION=5.E-3   )

TEST_TABLE(       TABLE=GLAGF,
                       NOM_PARA='G',
                           VALE=2.2610E-3,
                      PRECISION=5.E-3   )

FIN( )
