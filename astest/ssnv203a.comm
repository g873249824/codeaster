# CETTE MODELISATION TESTE L'APPLICATION D'UNE PRESSION (CONSTANTE ET AFFINE) 
# SUR LES LEVRES D'UNE FISSURE X-FEM EN 2D (EN FAIT, C'EST PLUTOT UNE INTERFACE)


#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------
# precision
n = 1000.0;

# dimensions de la structure
lx = 1.0;
ly = 1.0;

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURF'),
                                   _F(GROUP_MA='LIGB'),
                                   _F(GROUP_MA='LIGH'),),
                    INFO=2,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='SURF'),);

#IMPR_RESU(FORMAT='RESULTAT',RESU=_F(MAILLAGE=MAILLAG2))

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MO=AFFE_MODELE(MAILLAGE=MAILLAG2,
               AFFE=_F(GROUP_MA=('SURF',),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_XFEM',),);

LN = FORMULE(VALE='Y-0.5',NOM_PARA=('X','Y',),);
LT = FORMULE(VALE='X-10.',NOM_PARA=('X','Y',),);
#LT = FORMULE(VALE='X-0.5',NOM_PARA=('X','Y',),);

FISS=DEFI_FISS_XFEM(MODELE=MO,
                    DEFI_FISS=_F(FONC_LT=LT,
                                 FONC_LN=LN,),
                    GROUP_MA_ENRI='SURF',
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO,
                         FISSURE=FISS,
                         INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      INFO=1,);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
E = 10000000000.0;
nu = 0.;
rho = 7800.0;

ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=rho,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MO,
                       AFFE=_F(GROUP_MA='SURF',
                               MATER=ACIER,
                               TEMP_REF=20.0,),);

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                  DDL_IMPO=(_F(GROUP_NO='LIGB',DX=0.,DY=0.,),
                            _F(GROUP_NO='LIGH',DX=0.,DY=0.,),),
                  INFO=1,);


# pour tester option CHAR_MECA_PRES_R
CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   PRES_REP=_F(FISSURE=FISS,PRES=10000.)
                   )

# pour tester option CHAR_MECA_PRES_F
FCTPRES = FORMULE(VALE='X*10000.',
                  NOM_PARA=('X',),);

CH2=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     PRES_REP=_F(FISSURE=FISS,PRES=FCTPRES)
                   )

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=3.0,
                                   NOMBRE=3,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH,),
                           _F(CHARGE=CH1,),),
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INS,
                                 INST_FIN=1.0,),
                    NEWTON=_F(REAC_ITER=1,),
                    INFO=1,);

UTOT2=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH,),
                           _F(CHARGE=CH2,),),
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INS,
                                 INST_FIN=1.0,),
                    NEWTON=_F(REAC_ITER=1,),
                    INFO=1,);

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                       MODELE        = MODELEK)

#IMPR_RESU(FORMAT='RESULTAT',RESU=_F(MAILLAGE = MA_XFEM,))
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(MAILLAGE = MA_XFEM,))

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),),
                              );  

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'))


IMPR_RESU(FORMAT='GMSH',UNITE=82,RESU=_F(RESULTAT=RES_XFEM,));

DEP1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='DEPLE',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             GROUP_NO='SURF',
                             TOUT_CMP='OUI',),);

DEP2=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='DEPLE',
                             RESULTAT=UTOT2,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             GROUP_NO='SURF',
                             TOUT_CMP='OUI',),);


IMPR_TABLE(TABLE=DEP1,);
IMPR_TABLE(TABLE=DEP2,);



# POUR LE CHARGEMENT 1

