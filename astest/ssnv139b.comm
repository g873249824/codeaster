# MAILLAGE     : MEC3QU9H
#

MATERI_1=DEFI_MATERIAU( ELAS=_F(  E = 2.1E6,
                                  NU = 0.333333333333333) )

#

MAILLA_1=LIRE_MAILLAGE(FORMAT='MED',   )

MAILLA_1=DEFI_GROUP( reuse=MAILLA_1,   MAILLAGE=MAILLA_1,
                           CREA_GROUP_MA=_F(  NOM = 'TOUT',  TOUT = 'OUI') )

#

CH_MAT_1=AFFE_MATERIAU(  MAILLAGE=MAILLA_1,
                            AFFE=_F(  TOUT = 'OUI',   MATER = MATERI_1) )

#

MODELE_1=AFFE_MODELE(  MAILLAGE=MAILLA_1, VERIF='MAILLE',
                     AFFE=_F( TOUT = 'OUI', MODELISATION = 'COQUE_3D',
                                PHENOMENE = 'MECANIQUE') )

#

CHARGE_1=AFFE_CHAR_MECA(MODELE=MODELE_1,
                 DDL_IMPO=_F( GROUP_NO = 'G_41',  DX = 0., DY = 0., DZ = 0.,
                                            DRX = 0., DRY = 0., DRZ = 0.),
                  FORCE_NODALE=_F(  GROUP_NO = 'G_10',  FZ = -1.E0)
                  )

#

CARAC_1=AFFE_CARA_ELEM(   MODELE=MODELE_1,
                               COQUE=_F( GROUP_MA = 'TOUT',
                                         EPAIS = 0.5)  )
#

D_FONC=DEFI_FONCTION(    NOM_PARA='INST',
                                VALE=( 0.,   0.,
                                       500., 500.,  )  )

#
#

L_INST=DEFI_LIST_REEL(   DEBUT=0.0,INTERVALLE=(
                            _F(  JUSQU_A = 250., NOMBRE = 10),
                            _F(  JUSQU_A = 500., NOMBRE = 3))
                         )

#

DEFLIST1=DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST=L_INST,
                                     METHODE='AUTO',
                                     PAS_MINI=1.0E-12),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS      = 4,
                                 SUBD_PAS_MINI = 1e-7),
                        ADAPTATION=_F(EVENEMENT='SEUIL'),
                        );

NONLIN=STAT_NON_LINE(       MODELE=MODELE_1,
                        CHAM_MATER=CH_MAT_1,
                         CARA_ELEM=CARAC_1,
                             EXCIT=_F(
                CHARGE = CHARGE_1,
                FONC_MULT = D_FONC),
                         COMPORTEMENT=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'GROT_GDEP'),
                         INCREMENT=_F(
                LIST_INST = DEFLIST1,),
                            NEWTON=_F(
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 12,
                RESI_GLOB_RELA = 1.E-6)
