                VALE=125.8150,),

                _F(RESULTAT=MODEZ,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=3,
                PARA='FREQ',
                PRECISION=.004,
                VALE=479.0191,),

                _F(RESULTAT=MODEZ,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=4,
                PARA='FREQ',
                PRECISION=.02,
                VALE=503.2598,),

                _F(RESULTAT=MODEZ,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=5,
                PARA='FREQ',
                PRECISION=.02,
                VALE=1077.7931,),

                _F(RESULTAT=MODEZ,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=6,
                PARA='FREQ',
                PRECISION=.03,
                VALE=1132.3346,),

                _F(RESULTAT=MODEZ,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=7,
                PARA='FREQ',
                PRECISION=.03,
                VALE=1916.0765,),

                _F(RESULTAT=MODEZ,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=9,
                PARA='FREQ',
                PRECISION=.02,
                VALE=2013.0393,),))

TEST_RESU(RESU=(_F(RESULTAT=MODEG,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=1,
                PARA='FREQ',
                PRECISION=.004,
                VALE=119.7548),

                _F(RESULTAT=MODEG,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=2,
                PARA='FREQ',
                PRECISION=.009,
                VALE=125.8150,),

                _F(RESULTAT=MODEG,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=3,
                PARA='FREQ',
                PRECISION=.004,
                VALE=479.0191,),

                _F(RESULTAT=MODEG,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=4,
                PARA='FREQ',
                PRECISION=.02,
                VALE=503.2598,),

                _F(RESULTAT=MODEG,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=5,
                PARA='FREQ',
                PRECISION=.02,
                VALE=1077.7931,),

                _F(RESULTAT=MODEG,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=6,
                PARA='FREQ',
