#                                        CARA='ANGL_NAUT',
#                                        VALE=(45.,0.,0.))
#                                     )
                        )
                                 
                                 
# CONDITIONS AUX LIMITES 

BLOQUAGE=AFFE_CHAR_MECA(MODELE=MODELE,
                        DDL_IMPO=(
                                 _F(GROUP_NO=('PALIER_A','PALIER_B'),
                                     DX=0, DY=0, DZ=0,),
                                     ),
                        LIAISON_OBLIQUE=(
                                 _F(GROUP_NO=('PALIER_A','PALIER_B'),
                                         DRX=0.,
                                         ANGL_NAUT=(45.,0.,0.)),
                                    )
                          )
                                    
#MATRICES ASSEMBLEES K, M ET C

MACRO_MATR_ASSE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARELEM,
                CHARGE=BLOQUAGE,
                NUME_DDL=CO('NUMEDDL'),
                MATR_ASSE=(_F(MATRICE=CO('RIGIDITE'),
                              OPTION='RIGI_MECA',),
                           _F(MATRICE=CO('MASSE'),
                              OPTION='MASS_MECA',),
                           _F(MATRICE=CO('AMOR'),
                              OPTION='AMOR_MECA',),),);


GYELEM=CALC_MATR_ELEM(OPTION='MECA_GYRO',
                      MODELE=MODELE,
                      CHAM_MATER=CHMAT,
                      CARA_ELEM=CARELEM,);


GYASS=ASSE_MATRICE(MATR_ELEM=GYELEM,
                   NUME_DDL=NUMEDDL,);

                    
MODES=MODE_ITER_SIMULT(MATR_A=RIGIDITE,
                       MATR_B=MASSE,
                       CALC_FREQ=_F(OPTION='BANDE',
                       FREQ=(3.,3000.)));

                                  
TEST_RESU(RESU=(_F(RESULTAT=MODES,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=1,
                PARA='FREQ',
                PRECISION=.003,
                VALE=122.7475,),

                _F(RESULTAT=MODES,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=3,
                PARA='FREQ',
                PRECISION=.009,
                VALE=490.9899,),

                _F(RESULTAT=MODES,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=5,
                PARA='FREQ',
                PRECISION=.02,
                VALE=1104.7273,),

                _F(RESULTAT=MODES,
                REFERENCE='SOURCE_EXTERNE',
                NUME_ORDRE=7,
                PARA='FREQ',
                PRECISION=.04,
                VALE=1963.9596,),))



OM=10000.

GYOM=COMB_MATR_ASSE(COMB_R=(_F(MATR_ASSE=GYASS, COEF_R=OM,),
                            _F(MATR_ASSE=AMOR, COEF_R=1.,),))


MODEG=MODE_ITER_SIMULT(MATR_A=RIGIDITE,
                       MATR_B=MASSE,
                       MATR_C=GYOM,
                       METHODE='SORENSEN',
                       VERI_MODE=_F(SEUIL=1.E-4),
                       CALC_FREQ=_F(OPTION='CENTRE',
                                     FREQ=1.,
                                     NMAX_FREQ=30),)

