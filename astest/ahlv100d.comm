#  GUIDE D'ONDE A SORTIE ANECHOIQUE CAS 2D
#

DEBUT(CODE=_F( NOM = 'AHLV100D',NIV_PUB_WEB='INTERNET'))

F=500.

PRE_IDEAS()

MAYA=LIRE_MAILLAGE( )

AIR=DEFI_MATERIAU( FLUIDE=_F( RHO = 1.3, CELE_R = 343.))

CHAMPMAT=AFFE_MATERIAU(  MAILLAGE=MAYA,
                                 AFFE=_F( TOUT = 'OUI',  MATER = AIR) )

COLONNE=AFFE_MODELE(  MAILLAGE=MAYA,     VERIF='MAILLE',
                            AFFE=_F( TOUT = 'OUI', MODELISATION = '2D_FLUIDE',
                                     PHENOMENE = 'MECANIQUE') )

#  --- AFIN DE TESTER UN CHARGEMENT AVEC UNE FONCTION ON DECOMPOSE
#      VNOR = 0.014 = 0.010 (AFFE_CHAR_MECA) + 0.004 (AFFE_CHAR_MECA_F)

CHARMECA=AFFE_CHAR_MECA(      MODELE=COLONNE,
                 VITE_FACE=_F( GROUP_MA = 'ENTREE', VNOR = 0.010))

#  --- CHARGEMENT IDENTIQUE MAIS AVEC UNE FONCTION ---

VNOR=DEFI_FONCTION(  NOM_PARA='X',  VALE=(0., 0.004, 1., 0.004,) )

CHARMECF=AFFE_CHAR_MECA_F(      MODELE=COLONNE,
                 VITE_FACE=_F( GROUP_MA = 'ENTREE', VNOR = VNOR))

IMPEMECA=AFFE_CHAR_MECA(      MODELE=COLONNE,
                  IMPE_FACE=_F( GROUP_MA = 'SORTIE', IMPE = 445.9))

MATELE_K=CALC_MATR_ELEM(   MODELE=COLONNE,  CHAM_MATER=CHAMPMAT,
                                OPTION='RIGI_MECA', CHARGE=CHARMECA)

MATELE_M=CALC_MATR_ELEM(   MODELE=COLONNE,  CHAM_MATER=CHAMPMAT,
                                OPTION='MASS_MECA', CHARGE=CHARMECA)

MATELE_I=CALC_MATR_ELEM(   MODELE=COLONNE,  CHAM_MATER=CHAMPMAT,
                                OPTION='IMPE_MECA',   CHARGE=IMPEMECA )

VECTELEM=CALC_VECT_ELEM(   OPTION='CHAR_MECA',  CHAM_MATER=CHAMPMAT,
                                CHARGE=(CHARMECA, CHARMECF,))

NUM=NUME_DDL(  MATR_RIGI=MATELE_K )

MATASK=ASSE_MATRICE(  MATR_ELEM=MATELE_K,   NUME_DDL=NUM )

MATASM=ASSE_MATRICE(  MATR_ELEM=MATELE_M,   NUME_DDL=NUM )

MATASI=ASSE_MATRICE(  MATR_ELEM=MATELE_I,   NUME_DDL=NUM )

VECTASS=ASSE_VECTEUR(  VECT_ELEM=VECTELEM,   NUME_DDL=NUM )

DYNAHARM=DYNA_LINE_HARM( MATR_MASS=MATASM,  MATR_RIGI=MATASK,
                             MATR_IMPE_PHI=MATASI,  NOM_CHAM='DEPL',
                               FREQ=F,
                             EXCIT=_F( VECT_ASSE = VECTASS,  COEF_MULT = 1.,
                                    PUIS_PULS = 1,  PHAS_DEG = 90.))

#  IMPR_RESU (MODELE : COLONNE
#             RESU   :(FORMAT:'RESULTAT'  FICHIER:'RESULTAT'
#                      RESULTAT:DYNAHARM ));


TEST_RESU(RESU=(
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'DEPL',  NUME_ORDRE = 01,
              NOEUD = 'NO1',  NOM_CMP = 'PRES',  VALE_C = ('RI',-6.2426,0.,)),
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'DEPL',  NUME_ORDRE = 01,
              NOEUD = 'NO3', NOM_CMP = 'PRES',  VALE_C = ('RI',-6.2426,0.,)),
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'DEPL',  NUME_ORDRE = 01,
              NOEUD = 'NO151', NOM_CMP = 'PRES',  VALE_C = ('RI',6.02368,1.6387,)),
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'DEPL',  NUME_ORDRE = 01,
              NOEUD = 'NO153',  NOM_CMP = 'PRES',  VALE_C = ('RI',6.02368,1.6387,)))
             )

DEPRES=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_C',
NOM_CHAM='DEPL',  RESULTAT=DYNAHARM,  NUME_ORDRE=01)

CHNODBEL=CALC_CHAM_ELEM(      MODELE=COLONNE,
                              CHAM_MATER=CHAMPMAT,
                               DEPL=DEPRES,
                                  OPTION='PRES_DBEL_DEPL'
                            )

TEST_RESU(CHAM_ELEM=(
             _F( CHAM_GD = CHNODBEL,
              MAILLE = 'MA1',
              NOEUD = 'NO1',   NOM_CMP = 'DB',  VALE = 109.9),
             _F( CHAM_GD = CHNODBEL,
              MAILLE = 'MA1',
              NOEUD = 'NO3',   NOM_CMP = 'DB',  VALE = 109.9),
             _F( CHAM_GD = CHNODBEL,
              MAILLE = 'MA29',
              NOEUD = 'NO151', NOM_CMP = 'DB',  VALE = 109.9),
             _F( CHAM_GD = CHNODBEL,
              MAILLE = 'MA29',
              NOEUD = 'NO153', NOM_CMP = 'DB',  VALE = 109.9))
             )

DYNAHARM=CALC_ELEM( reuse=DYNAHARM,     MODELE=COLONNE,   CHAM_MATER=CHAMPMAT,
                            TOUT_ORDRE='OUI',
                             RESULTAT=DYNAHARM,
                               OPTION='PRES_DBEL_DEPL'
           )

TEST_RESU(RESU=(
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'PRES_DBEL_DEPL', NUME_ORDRE = 01,
              MAILLE = 'MA1',
              NOEUD = 'NO1',  NOM_CMP = 'DB',  VALE = 109.8867),
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'PRES_DBEL_DEPL', NUME_ORDRE = 01,
              MAILLE = 'MA1',
              NOEUD = 'NO3',  NOM_CMP = 'DB',  VALE = 109.8867),
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'PRES_DBEL_DEPL', NUME_ORDRE = 01,
              MAILLE = 'MA29',
              NOEUD = 'NO151', NOM_CMP = 'DB',  VALE = 109.8867),
             _F( RESULTAT = DYNAHARM,  NOM_CHAM = 'PRES_DBEL_DEPL', NUME_ORDRE = 01,
              MAILLE = 'MA29',
              NOEUD = 'NO153', NOM_CMP = 'DB',  VALE = 109.8867))
             )

FIN()
