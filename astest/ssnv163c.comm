DEBUT(CODE=_F(NOM='SSNV163C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE( )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'L_INF')
           )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'L_SUP')
            )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'L_GCH')
            )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'L_DRT')
            )

MO=AFFE_MODELE( MAILLAGE=M,
                AFFE=_F( TOUT = 'OUI',
               PHENOMENE = 'MECANIQUE',
          MODELISATION = 'C_PLAN')
              )

FONC1=DEFI_FONCTION( NOM_PARA='SECH',
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 1.0, 1.0,)
                   )

E=DEFI_FONCTION( NOM_PARA='TEMP',
                 PROL_DROITE='LINEAIRE',
                 PROL_GAUCHE='LINEAIRE',
                 VALE=(0.0, 31000., 1.0, 31000.,)
               )

NU=DEFI_FONCTION( NOM_PARA='TEMP',
                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE',
                  VALE=(0.0,0.2,1.0,0.2,)
                ),

ALPHA=DEFI_CONSTANTE(VALE=0.);

BETON=DEFI_MATERIAU( ELAS_FO =_F( E = E,
                                  NU = NU ,
                                  ALPHA=ALPHA,TEMP_DEF_ALPHA=0.,
                                  FONC_DESORP = FONC1),
                     BETON_UMLV_FP =_F( K_RS   = 2.0E+5,
                                        ETA_RS = 4.0E+10,
                                        K_IS   = 5.0E+4,
                                        ETA_IS = 1.0E+11,
                                        K_RD   = 5.0E+4,
                                        ETA_RD = 1.0E+10,
                                        ETA_ID = 1.0e+11,
                                 ),
                  )

SECH=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=M,
                        AFFE=_F( TOUT = 'OUI',
                              NOM_CMP = 'TEMP',
                              VALE = 1.)
                       )

TEMP=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=M,
                         AFFE=_F(
                               TOUT = 'OUI',
                               NOM_CMP = 'TEMP',
                               VALE = 20.)
                       )
CM=AFFE_MATERIAU(  MAILLAGE=M,
                   AFFE=_F( TOUT = 'OUI',
                            MATER = BETON,),
                   AFFE_VARC=(_F(TOUT='OUI',
                                CHAM_GD=SECH,
                                NOM_VARC='SECH',
                                VALE_REF=1),
                                _F(TOUT='OUI',
                                CHAM_GD=TEMP,
                                NOM_VARC='TEMP',
                                VALE_REF=20.),),);



FORCE=DEFI_FONCTION( NOM_PARA='INST',
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 1., -.5, 31536010., -.50,)
                   )

CH10=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'L_INF',
                                  DY       = 0.
                               )
                    )
CH11=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'L_GCH',
                                  DX       = 0.
                                )
                   )
CH12=AFFE_CHAR_MECA( MODELE=MO,
                     LIAISON_UNIF=_F( GROUP_NO = 'L_DRT',
                                      DDL = 'DX'
                                )
                   )
CH2=AFFE_CHAR_MECA_F( MODELE=MO,
                      FORCE_NODALE=_F( GROUP_NO = 'L_SUP',
                                       FY = FORCE),
            LIAISON_UNIF=_F( GROUP_NO = 'L_SUP',
                                      DDL = 'DY'
                                )
                    )


L_INST=DEFI_LIST_REEL(  DEBUT=-1.0,INTERVALLE=(
             _F( JUSQU_A = 0.0,       NOMBRE = 1),
             _F( JUSQU_A = 1.0,       NOMBRE = 1),
        _F( JUSQU_A = 138629.44, NOMBRE = 10),
             _F( JUSQU_A = 8640000.,  NOMBRE = 10))
           )
#DELTAT=432000=5J


U=STAT_NON_LINE( MODELE=MO,
                 CHAM_MATER=CM,EXCIT=( _F(CHARGE = CH10),
                             _F(CHARGE = CH11),
                   _F(CHARGE = CH12),
                   _F(CHARGE = CH2),
                 ),
       COMP_INCR=_F(RELATION = 'BETON_UMLV_FP',),
                 INCREMENT=_F(LIST_INST = L_INST,
                              NUME_INST_FIN = 22),
                 NEWTON=_F(PREDICTION = 'ELASTIQUE',
                           MATRICE = 'TANGENTE',
            REAC_ITER = 1),
                 CONVERGENCE=_F(RESI_GLOB_MAXI = 1.E-8,
                                ITER_GLOB_MAXI = 25)
              )

U=CALC_CHAMP(reuse=U,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO'),RESULTAT=U)


TEST_RESU(RESU=(_F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPYY',
                   VALE_CALC=0.0,
                   VALE_REFE=0.0,
                   TOLE_MACHINE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=0.0,
                   MAILLE='M1',),
                _F(INST=1.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPYY',
                   VALE_CALC=-3.22581053E-05,
                   VALE_REFE=-3.2258146182608338E-05,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=9.7041E4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPYY',
                   VALE_CALC=-3.86714084E-05,
                   VALE_REFE=-3.8671439652747337E-05,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=1.8389035E6,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPYY',
                   VALE_CALC=-6.08855499E-05,
                   VALE_REFE=-6.0885526755165308E-05,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                _F(INST=8.640000E6,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N3',
                   NOM_CMP='EPYY',
                   VALE_CALC=-1.10047828E-04,
                   VALE_REFE=-1.1004783309409689E-4,
                   PRECISION=5.0000000000000001E-3,
                   MAILLE='M1',),
                ),
          )

FIN()
