                  SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
                                  FORMULATION='DEPLACEMENT',),
                  NEWTON=_F(MATRICE='TANGENTE',
                            REAC_ITER=1,),
                  SOLVEUR=_F(METHODE='MUMPS',
