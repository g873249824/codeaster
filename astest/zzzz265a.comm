                                 ITER_GLOB_MAXI=500,
                                 ),
                  SOLVEUR=_F(METHODE='MULT_FRONT',
                             NPREC=15,),);

LOAD=AFFE_CHAR_MECA(MODELE=MODELE,
                    FORCE_NODALE=_F(GROUP_NO='EXCIT19',
