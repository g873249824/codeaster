#       ELEMENTS DE POUTRE, METHODES LAGRANGIENNE ET CONTINUE
# CAS TEST  SSNV505F
# MODELE
# MAILLAGE : ELEMENT DE POUTRES SEG2 EN MODELISATION 3D
# UNITES   : NEWTON , MILLIMETRE , SECONDE (N,MM,S)
#
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))


# ---------------------  MAILLAGE ----------------
MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_NO=(_F(GROUP_MA=('POU1',), NOM='GROUP1',),
                                 _F(GROUP_MA=('POU2',), NOM='GROUP2',),
                                 ));

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_LIGNE=_F(GROUP_MA=('POU1',),
                                   GROUP_NO='A',
                                   VECT_TANG=(-1.,0.,0.),),
                     INFO=2);


# ---- MODELE : POUTRE DROITE D'EULER ----
MODE=AFFE_MODELE(MAILLAGE=MAILL1,
                 
                 AFFE=(_F(GROUP_MA='POU1',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_E',),
                       _F(GROUP_MA='POU2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_E',),
                       ),);


# ------------- DEFINITON DU MATERIAU -----------
MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,NU=0.33333,RHO=7800.,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=_F(GROUP_MA=('POU1','POU2'),
                         MATER=MPOUTRE,),);

CHAMPCAR=AFFE_CARA_ELEM(MODELE=MODE,
                        INFO=1,
                        POUTRE=(_F(GROUP_MA='POU1',
                                   SECTION='RECTANGLE',
                                   CARA=('HY','HZ',),
                                   VALE=(63.5,10.16,),),
                                _F(GROUP_MA='POU2',
                                   SECTION='RECTANGLE',
                                   CARA=('HY','HZ',),
                                   VALE=(63.5,10.16,),),
                                ),);


# ------------- DEFINITION D'UN CARA_ELEM POUR LE CONTACT -----------
MOD2=AFFE_MODELE(MAILLAGE=MAILL1,
                 AFFE=_F(GROUP_MA='POU2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_E',),);

DEFI_R=AFFE_CARA_ELEM(MODELE=MOD2,
                      POUTRE=_F(GROUP_MA='POU2',
                                SECTION='CERCLE',
                                VARI_SECT='HOMOTHETIQUE',
                                CARA=('R_DEBUT','R_FIN',),
                                VALE=(31.75,31.75,),),);


# ---------- CONDITIONS AUX LIMITES --------------
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO=('A','B',),
                                     DX=0.0,DY=0.0,DZ=0.0,
                                     DRX=0.0,DRY=0.0,DRZ=0.0),
                                  _F(GROUP_NO=('GROUP1','GROUP2'),
                                     DZ=0.0,DRX=0.0,DRY=0.0,),
                                  ),);

CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=(_F(GROUP_NO=('C'),DY=-790.,),),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=50,),),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=25,),),);


# ---- METHODE LAGRANGIENNE ----
F_DIST_M=DEFI_CONSTANTE(VALE=31.75);
F_DIST_E=DEFI_CONSTANTE(VALE=31.75);

CHLAG= DEFI_CONTACT(MODELE         = MODE,
                    FORMULATION    = 'DISCRETE',
                    FROTTEMENT     = 'COULOMB',
                    REAC_GEOM      = 'AUTOMATIQUE',
                    ZONE =(
                           _F(
                              GROUP_MA_MAIT  = 'POU1',
                              GROUP_MA_ESCL  = 'POU2',
                              ALGO_CONT      = 'LAGRANGIEN',
                              ALGO_FROT      = 'LAGRANGIEN',
                              COULOMB        = 0.,
                              COEF_MATR_FROT = 1.,
                              VECT_MAIT      = 'VECT_Y',
                              MAIT_VECT_Y    = (0.,0.,1.,),
                              DIST_MAIT      = F_DIST_M,
                              DIST_ESCL      = F_DIST_E,
                             ),
                          ),
                   );



ULAG=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  = CHLAG,
                COMPORTEMENT=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=1.,),
                NEWTON=_F(REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=40,),
                SOLVEUR=_F(),
                ARCHIVAGE=_F(LIST_INST=L_ARCH),
                INFO=1,);

ULAG=CALC_CHAMP(reuse =ULAG,
          RESULTAT=ULAG,

          FORCE='FORC_NODA',


         );


# ---- TABLEAUX ----
T_REA_LA=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=ULAG,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

T_DEP_LA=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO=('C','A1','B1'),
                                 RESULTAT=ULAG,
                                 NOM_CHAM='DEPL',
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION',),);


#
# ---- METHODE CONTINUE ----
#      ________________

CHCON= DEFI_CONTACT(MODELE         = MODE,
                    FORMULATION    = 'CONTINUE',
                    ALGO_RESO_CONT = 'POINT_FIXE',
                    ALGO_RESO_GEOM = 'POINT_FIXE',
REAC_GEOM      = 'CONTROLE',
                    NB_ITER_GEOM   = 4,
                    ZONE =(
                           _F(
                              GROUP_MA_MAIT  = 'POU1',
                              GROUP_MA_ESCL  = 'POU2',
                              VECT_MAIT      = 'VECT_Y',
                              MAIT_VECT_Y    = (0.,0.,1.,),
                              DIST_MAIT      = F_DIST_M,
                              DIST_ESCL      = F_DIST_E,
                             ),
                          ),
                   );


UCON=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  = CHCON,
                COMPORTEMENT=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=1.,),
                NEWTON=_F(REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=40,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH),
                INFO=1,);

UCON=CALC_CHAMP(reuse =UCON,
          RESULTAT=UCON,

          FORCE='FORC_NODA',


         );


# ---- TABLEAUX ----
T_REA_CO=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=UCON,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

T_DEP_CO=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO=('C','A1','B1'),
                                 RESULTAT=UCON,
                                 NOM_CHAM='DEPL',
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION',),);


# ---- VALIDATION ----
TEST_TABLE(VALE_CALC=307.48396428377,
           NOM_PARA='DY',
           TABLE=T_REA_CO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.2,),
           )

TEST_TABLE(VALE_CALC=1251.4814003345,
           NOM_PARA='DY',
           TABLE=T_REA_CO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4,),
           )

TEST_TABLE(VALE_CALC=3023.8536740096,
           NOM_PARA='DY',
           TABLE=T_REA_CO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6,),
           )

TEST_TABLE(VALE_CALC=4829.4298492022,
           NOM_PARA='DY',
           TABLE=T_REA_CO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8,),
           )

TEST_TABLE(VALE_CALC=6655.9179587886,
           NOM_PARA='DY',
           TABLE=T_REA_CO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

FIN();
