DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',);

modelisa = 'AXIS'
#modelisa = 'C_PLAN'


# ce test est une copie de sslx100d (avec modélisation AXIS) dans lequel on modifie la surface pour qu'elle
# soit différente de 1 (la surface est de 0.5)
# on compare le modèle avec discret+liaison 2D_POU au même modèle sans discret
# et en imposant directement une pression sur les éléments massifs (qui sert de référence)
# Il permet de valider la liaison 2D_POU avec les éléments AXIS
surf_racc=0.5

MO_REF=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(GROUP_MA='MAIL2D',
                        PHENOMENE='MECANIQUE',
                        MODELISATION=modelisa,),
                        ),
                        );

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(GROUP_MA='MAIL2D',
                        PHENOMENE='MECANIQUE',
                        MODELISATION=modelisa,),
                     _F(GROUP_MA='DISCRET',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='2D_DIS_TR',),
                        ),
                        );

MAT=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                          NU=0.0,
                          ALPHA=0.0,),);

CHMA=AFFE_MATERIAU(MAILLAGE=MA,
                   AFFE=_F(TOUT='OUI',
                           MATER=MAT,),);

CARELEM=AFFE_CARA_ELEM(MODELE=MO,
                       DISCRET_2D=_F(REPERE='LOCAL',CARA='K_TR_D_L', GROUP_MA='DISCRET',
                                     VALE=(40.0E5, 2.0E5, 4.0E5),),)

# TRACTION
# --------
CHME_REF=AFFE_CHAR_MECA(MODELE=MO_REF,
                    DDL_IMPO=_F(GROUP_MA='GAUCHE',
                                DX=0.0,
                                DY=0.0,),
                    PRES_REP=_F(PRES=-50000/surf_racc, 
                                #GROUP_MA='DROIT',
                                GROUP_MA='DROIT2',
                                ),
                                    );

CHME=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=_F(GROUP_MA='GAUCHE',
                                DX=0.0,
                                DY=0.0,),
                    LIAISON_ELEM=_F(OPTION='2D_POU',
                                    #GROUP_MA_1='DROIT',
                                    GROUP_MA_1='DROIT2',
                                    GROUP_NO_2='G',),
                    FORCE_NODALE=_F(GROUP_NO='EXTREM',
                                    FX = 50000.,
                                    ),
                                    
                                    );

RES_REF=MECA_STATIQUE(MODELE=MO_REF,
                  CHAM_MATER=CHMA,
                  EXCIT=(_F(CHARGE=CHME_REF,),
                        ),);


CALC_CHAMP(reuse=RES_REF, RESULTAT=RES_REF, FORCE='FORC_NODA')

#IMPR_RESU(FORMAT='RESULTAT', RESU=_F(RESULTAT=RES_REF, NOM_CHAM=('DEPL','FORC_NODA'), GROUP_MA='DROIT'))

TEST_RESU(RESU=(   _F(NUME_ORDRE=1,
                      NOEUD='N125',
                      NOM_CMP='DX',
                      RESULTAT=RES_REF,
                      NOM_CHAM='DEPL',
                      VALE_CALC=7.44677670109,),
                   _F(NUME_ORDRE=1,
                      NOEUD='N129',
                      NOM_CMP='DX',
                      RESULTAT=RES_REF,
                      NOM_CHAM='FORC_NODA',
                      VALE_CALC=125000.0,),
                   ),
          )


RES=MECA_STATIQUE(MODELE=MO,
                  CHAM_MATER=CHMA,
                  CARA_ELEM  = CARELEM,
                  EXCIT=(_F(CHARGE=CHME,),
                        ),);


CALC_CHAMP(reuse=RES, RESULTAT=RES, FORCE='FORC_NODA')

#IMPR_RESU(FORMAT='RESULTAT', RESU=_F(RESULTAT=RES, NOM_CHAM=('DEPL','FORC_NODA'), GROUP_MA='DROIT'))

TEST_RESU(RESU=(   _F(NUME_ORDRE=1,
                      NOEUD='N125',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DX',
                      RESULTAT=RES,
                      NOM_CHAM='DEPL',
                      VALE_CALC=7.44677670111,
                      VALE_REFE=7.44677670109,),
                   _F(NUME_ORDRE=1,
                      NOEUD='N129',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DX',
                      RESULTAT=RES,
                      NOM_CHAM='FORC_NODA',
                      VALE_CALC=125000.0,
                      VALE_REFE=125000.0,),
                   ),
          )

DETRUIRE(CONCEPT=_F(NOM=(CHME_REF,CHME, RES_REF, RES )))

# FLEXION
# -------
CHME_REF=AFFE_CHAR_MECA(MODELE=MO_REF,
                    DDL_IMPO=_F(GROUP_MA='GAUCHE',
                                DX=0.0,
                                DY=0.0,),
                    PRES_REP=_F(CISA_2D=-0.5/surf_racc, 
                                #GROUP_MA='DROIT',
                                GROUP_MA='DROIT2',
                                ),
                                    );

CHME=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=_F(GROUP_MA='GAUCHE',
                                DX=0.0,
                                DY=0.0,),
                    LIAISON_ELEM=_F(OPTION='2D_POU',
                                    #GROUP_MA_1='DROIT',
                                    GROUP_MA_1='DROIT2',
                                    GROUP_NO_2='G',),
                    FORCE_NODALE=_F(GROUP_NO='EXTREM',
                                    FY = -0.5,
                                    ),
                                    
                                    );

RES_REF=MECA_STATIQUE(MODELE=MO_REF,
                  CHAM_MATER=CHMA,
                  EXCIT=(_F(CHARGE=CHME_REF,),
                        ),);


CALC_CHAMP(reuse=RES_REF, RESULTAT=RES_REF, FORCE='FORC_NODA')

#IMPR_RESU(FORMAT='RESULTAT', RESU=_F(RESULTAT=RES_REF, NOM_CHAM=('DEPL','FORC_NODA'), GROUP_MA='DROIT'))

TEST_RESU(RESU=(   _F(NUME_ORDRE=1,
                      NOEUD='N125',
                      NOM_CMP='DY',
                      RESULTAT=RES_REF,
                      NOM_CHAM='DEPL',
                      VALE_CALC=-0.304601213708,),
                   _F(NUME_ORDRE=1,
                      NOEUD='N129',
                      NOM_CMP='DY',
                      RESULTAT=RES_REF,
                      NOM_CHAM='FORC_NODA',
                      VALE_CALC=-1.24999999551,),
                   ),
          )


RES=MECA_STATIQUE(MODELE=MO,
                  CHAM_MATER=CHMA,
                  CARA_ELEM  = CARELEM,
                  EXCIT=(_F(CHARGE=CHME,),
                        ),);


CALC_CHAMP(reuse=RES, RESULTAT=RES, FORCE='FORC_NODA')

#IMPR_RESU(FORMAT='RESULTAT', RESU=_F(RESULTAT=RES, NOM_CHAM=('DEPL','FORC_NODA'), GROUP_MA='DROIT'))

TEST_RESU(RESU=(   _F(NUME_ORDRE=1,
                      NOEUD='N125',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DY',
                      RESULTAT=RES,
                      NOM_CHAM='DEPL',
                      VALE_CALC=-0.304601215347,
                      VALE_REFE=-0.304601213708,),
                   _F(NUME_ORDRE=1,
                      NOEUD='N129',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DY',
                      RESULTAT=RES,
                      NOM_CHAM='FORC_NODA',
                      VALE_CALC=-1.25000002128,
                      VALE_REFE=-1.24999999551,),
                   ),
          )

DETRUIRE(CONCEPT=_F(NOM=(CHME_REF,CHME, RES_REF, RES )))


# MOMENT
# ------
BLOQ_REF=AFFE_CHAR_MECA(MODELE=MO_REF,
                    DDL_IMPO=_F(GROUP_MA='GAUCHE',
                                DX=0.0,
                                DY=0.0,),
                                    );

# le noeud central de la zone de raccord est en y=0.5
FPRES = FORMULE(NOM_PARA='Y', VALE = "96000*(Y-0.5)")

CHME_REF=AFFE_CHAR_MECA_F(MODELE=MO_REF,
                      PRES_REP=_F(PRES=FPRES, GROUP_MA='DROIT2'),
                     )

CHME=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=_F(GROUP_MA='GAUCHE',
                                DX=0.0,
                                DY=0.0,),
                    LIAISON_ELEM=_F(OPTION='2D_POU',
                                    #GROUP_MA_1='DROIT',
                                    GROUP_MA_1='DROIT2',
                                    GROUP_NO_2='G',),
                    FORCE_NODALE=_F(GROUP_NO='EXTREM',
                                    MZ = 1000.,
                                    ),
                                    
                                    );

RES_REF=MECA_STATIQUE(MODELE=MO_REF,
                  CHAM_MATER=CHMA,
                  EXCIT=(_F(CHARGE=CHME_REF,),
                         _F(CHARGE=BLOQ_REF,),
                        ),);


CALC_CHAMP(reuse=RES_REF, RESULTAT=RES_REF, FORCE='FORC_NODA')

#IMPR_RESU(FORMAT='RESULTAT', RESU=_F(RESULTAT=RES_REF, NOM_CHAM=('DEPL','FORC_NODA'), GROUP_MA='DROIT'))

TEST_RESU(RESU=(   _F(NUME_ORDRE=1,
                      NOEUD='N125',
                      NOM_CMP='DY',
                      RESULTAT=RES_REF,
                      NOM_CHAM='DEPL',
                      VALE_CALC=26.9952680974,),
                   _F(NUME_ORDRE=1,
                      NOEUD='N129',
                      NOM_CMP='DX',
                      RESULTAT=RES_REF,
                      NOM_CHAM='FORC_NODA',
                      VALE_CALC=30000.0,),
                   ),
          )


RES=MECA_STATIQUE(MODELE=MO,
                  CHAM_MATER=CHMA,
                  CARA_ELEM  = CARELEM,
                  EXCIT=(_F(CHARGE=CHME,),
                        ),);


CALC_CHAMP(reuse=RES, RESULTAT=RES, FORCE='FORC_NODA')

#IMPR_RESU(FORMAT='RESULTAT', RESU=_F(RESULTAT=RES, NOM_CHAM=('DEPL','FORC_NODA'), GROUP_MA='DROIT'))

TEST_RESU(RESU=(   _F(NUME_ORDRE=1,
                      NOEUD='N125',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DY',
                      RESULTAT=RES,
                      NOM_CHAM='DEPL',
                      VALE_CALC=26.9952681316,
                      VALE_REFE=26.9952680974,),
                   _F(NUME_ORDRE=1,
                      NOEUD='N129',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DX',
                      RESULTAT=RES,
                      NOM_CHAM='FORC_NODA',
                      VALE_CALC=30000.0,
                      VALE_REFE=30000.0,),
                   ),
          )


FIN();
#
