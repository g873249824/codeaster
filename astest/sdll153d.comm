TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   PARA='FREQ',
                   PRECISION=0.02,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=modesFF,
                   VALE_CALC=(663.447369317, ),
                   VALE_REFE=(654.216, )),
                _F(NUME_ORDRE=2,
