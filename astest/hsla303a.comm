                  NOEUD='N1',
                  NOM_CMP='EPYY',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N1',
                  NOM_CMP='EPZZ',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N2',
                  NOM_CMP='EPXX',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N2',
                  NOM_CMP='EPYY',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N2',
                  NOM_CMP='EPZZ',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
