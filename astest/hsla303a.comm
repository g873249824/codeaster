                  VALE_REFE=3.7139999999999999E-3,
                  REFERENCE='NON_DEFINI',
                  PRECISION=0.076999999999999999,
                  MAILLE='M10',),
          )

#
# DEUXIEME CALCUL AVEC DEFORMATIONS INITIALES
#
# DEFINITION DES DEFORM. INIT.
DEF_INIT=AFFE_CHAR_MECA(     MODELE=MODELE,
                             PRE_EPSI=_F(  TOUT = 'OUI',
                             EPXX = 1.2E-3,
                             EPYY = 1.2E-3,
                             EPZZ = 1.2E-3,
                             EPXY = 0.)
                           )

# RESOLUTION
DEF_PR2=MECA_STATIQUE(    MODELE=MODELE,
                            CHAM_MATER=CHMATST,EXCIT=(
                            _F(  CHARGE = CLIM),
                                  _F(  CHARGE = CHAR_PR),
                                  _F(  CHARGE = DEF_INIT))
                     )

#
# RECUPERATION DU CHAMP DE DEPLACEMENT U2
U_PR2=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
RESULTAT=DEF_PR2,
                       NUME_ORDRE=1,
                       NOM_CHAM='DEPL' )

#
DEF_PR2=CALC_CHAMP(reuse=DEF_PR2,RESULTAT=DEF_PR2,DEFORMATION=('EPSI_ELNO'))


#
# TEST_RESU DES DEPLACEMENTS
#
TEST_RESU(CHAM_NO=(_F(NOEUD='N3',
                      NOM_CMP='DY',
                      PRECISION=0.058000000000000003,
                      CHAM_GD=U_PR2,
                      VALE_CALC= 5.19677260E-03,
                      VALE_REFE=4.914E-3,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N4',
                      NOM_CMP='DY',
                      PRECISION=0.058000000000000003,
                      CHAM_GD=U_PR2,
                      VALE_CALC= 5.19677256E-03,
                      VALE_REFE=4.914E-3,
                      REFERENCE='NON_DEFINI',),
                   ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_PR2,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N3',
                  NOM_CMP='EPYY',
                  VALE_CALC= 5.19633732E-03,
                  VALE_REFE=4.914E-3,
                  REFERENCE='NON_DEFINI',
                  PRECISION=0.058000000000000003,
                  MAILLE='M10',),
          )

