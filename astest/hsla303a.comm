                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N3',
                  NOM_CMP='EPXX',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M10',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N3',
                  NOM_CMP='EPYY',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M10',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N3',
                  NOM_CMP='EPZZ',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M10',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N4',
                  NOM_CMP='EPXX',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M10',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N4',
                  NOM_CMP='EPYY',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M10',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_TH,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N4',
                  NOM_CMP='EPZZ',
                  VALE_CALC=1.1999999999999999E-3,
                  TOLE_MACHINE=1.E-4,
                  MAILLE='M10',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF_PR,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N3',
                  NOM_CMP='EPYY',
                  VALE_CALC= 3.99633732E-03,
                  VALE_REFE=3.7139999999999999E-3,
                  REFERENCE='NON_DEFINI',
                  PRECISION=0.076999999999999999,
                  MAILLE='M10',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
