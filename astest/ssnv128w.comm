#
# CAS_TEST__:SSNV128W
# TEST DU GRECO CALCUL 3D
# MODELE
# MAILLAGE : ELEMENT 3D HEXAEDRE A 8 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
# LE GROUPE_NO PP CONTIENT LES NOEUDS OU LA
# REDONDANCE APPARAIT ICI, ON N'UTILISE PAS
# VECT_Y ET VECT_Z PARCEQUE LES DIRECTIONS
# DE GLISSEMENT SONT CONSTRUITES AUTOMATIQUEMENT

# TRAITE EN FORMULATION 'CONTINUE' PAR 'PENALISATION'

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),
IGNORE_ALARM='MODELE1_63',DEBUG=_F(SDVERI='OUI'))

#  CALCUL TEST ELEMENTS FINIS DE CONTACT 3D HEXA8
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE

MA=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'));

MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOM='NCONTA',
                                GROUP_MA='SCONTA'),
                             _F(NOM='PP',
                                UNION=('PPSZ','PPS')),
                             _F(NOM='NPLAQ',
                                GROUP_MA='VPLAQ',),
                             _F(NOM='NBLOCZ',
                                DIFFE=('NPLAQ','NCONTA')),
                             _F(NOM='NENCA',
                                GROUP_MA='SBATI',
                                CRIT_NOEUD='SOMMET')));

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = '3D'));

MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=(_F(GROUP_MA = 'SPRESV'),
                               _F(GROUP_MA = 'SPRESH')));

MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11, NU = 0.2));

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT = 'OUI',
                            MATER = MATPLA));

CHA1=DEFI_CONTACT(MODELE=MO,
                 FORMULATION    = 'CONTINUE',
                 FROTTEMENT     = 'COULOMB',
                 ITER_GEOM_MAXI = 2,
                 ALGO_RESO_FROT = 'POINT_FIXE',
                 ALGO_RESO_GEOM = 'POINT_FIXE',
                 ALGO_RESO_CONT = 'NEWTON',
                 ZONE=_F(
                         GROUP_MA_MAIT = 'SBATI',
                         GROUP_MA_ESCL = 'SCONTA',
                         CONTACT_INIT = 'INTERPENETRE',
                         COULOMB        = 1.0,
                         SANS_GROUP_NO_FR= 'PP',

                         ALGO_CONT='PENALISATION',
                         ALGO_FROT='PENALISATION',

                         COEF_PENA_CONT=1.E+16,
                         COEF_PENA_FROT=1.E+8,
                 ),);
CHA2=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO = 'NENCA',  DX = 0., DY = 0., DZ=0.),
                              _F(GROUP_MA = 'SBLOCX', DX = 0.),
                              _F(GROUP_NO = 'NBLOCZ', DZ = 0.)),
                    PRES_REP=(_F(GROUP_MA = 'SPRESV', PRES = 5.E07),
                              _F(GROUP_MA = 'SPRESH', PRES = 15.E07)));

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0));

LINST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 1));

L_INST=DEFI_LIST_INST(METHODE='MANUEL',
                      DEFI_LIST=_F(LIST_INST=LINST,),
                      ECHEC=_F(EVENEMENT='ERREUR',
                              ACTION='DECOUPE',
                              SUBD_METHODE = 'MANUEL',
                              SUBD_PAS=5,
                              SUBD_NIVEAU=5,),);

#-----------------------------------------------------------

U2M=STAT_NON_LINE(MODELE   = MO,
                CHAM_MATER = CHMAT,
                EXCIT      =(_F(CHARGE = CHA2, FONC_MULT = RAMPE),),
                CONTACT=CHA1,
                COMPORTEMENT  = _F(RELATION = 'ELAS'),
                NEWTON     = _F(REAC_ITER = 1),
                INCREMENT  = _F(LIST_INST = L_INST),
                CONVERGENCE= _F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 30,),);

TEST_RESU(RESU=(_F(GROUP_NO='PPA',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.92177477E-05,
                   VALE_REFE=2.8600000000000001E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPB',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.77463756E-05,
                   VALE_REFE=2.72E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPC',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.31882585E-05,
                   VALE_REFE=2.2799999999999999E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPD',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.01146026E-05,
                   VALE_REFE=1.98E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPE',
                   INST=1.0,
                   REFERENCE='SOURCE_EXTERNE',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.56927123E-05,
                   VALE_REFE=1.5E-05,
                   PRECISION=0.050000000000000003,
                   ),
                ),
          )

#IMPR_RESU(FORMAT='CASTEM',
#          UNITE=37,RESU=_F(MAILLAGE= MA,
#          RESULTAT=U2M));

PRFUS=POST_RELEVE_T(ACTION=_F(INTITULE='USURE',
                             GROUP_NO='NCONTA',
                             RESULTAT=U2M,
                             NOM_CHAM='CONT_NOEU',
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION'));

LAGSC=POST_RELEVE_T(ACTION=_F(INTITULE='LAMBDA',
                             GROUP_NO='NCONTA',
                             RESULTAT=U2M,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP='LAGS_C',
                             OPERATION='EXTRACTION'));

LAGSF1=POST_RELEVE_T(ACTION=_F(INTITULE='LAMBDA',
                             GROUP_NO='NCONTA',
                             RESULTAT=U2M,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP='LAGS_F1',
                             OPERATION='EXTRACTION'));

LAGSF2=POST_RELEVE_T(ACTION=_F(INTITULE='LAMBDA',
                             GROUP_NO='NCONTA',
                             RESULTAT=U2M,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP='LAGS_F2',
                             OPERATION='EXTRACTION'));

POSX=POST_RELEVE_T(ACTION=_F(INTITULE='LAMBDA',
                             GROUP_NO='NCONTA',
                             RESULTAT=U2M,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP='DX',
                             OPERATION='EXTRACTION'));

POSY=POST_RELEVE_T(ACTION=_F(INTITULE='LAMBDA',
                             GROUP_NO='NCONTA',
                             RESULTAT=U2M,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP='DY',
                             OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=PRFUS);
IMPR_TABLE(TABLE=LAGSC);
IMPR_TABLE(TABLE=LAGSF1);
IMPR_TABLE(TABLE=LAGSF2);
IMPR_TABLE(TABLE=POSX);
IMPR_TABLE(TABLE=POSY);

#IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(RESULTAT=U2M));

#IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=U2M));

FIN();
