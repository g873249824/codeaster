             CRITERE='RELATIF',
             VALE_ABS='NON',)

# G3R/GK
TEST_TABLE(  TABLE=T_FIN,
             NOM_PARA='G3R/G_K',
             FILTRE=_F(NOM_PARA='INST',
                       VALE=1.0,
                       ),
             REFERENCE='ANALYTIQUE',
             VALE_CALC=1.01058498945885,
             VALE_REFE=1.0, 
             PRECISION=0.015,
             CRITERE='RELATIF',
             VALE_ABS='NON',)

TEST_TABLE(  TABLE=T_FIN,
             NOM_PARA='G3R/G_K',
             FILTRE=_F(NOM_PARA='INST',
                       VALE=2.0,
                       ),
             REFERENCE='ANALYTIQUE',
             VALE_CALC=1.0057590498390634,
             VALE_REFE=1.0, 
