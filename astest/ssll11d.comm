DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MATER=DEFI_MATERIAU( ELAS=_F(  RHO = 1.E04,  E = 1.962E11, NU = 0.3,
                                 AMOR_ALPHA = 0.1,  AMOR_BETA = 0.1)  )

MA=LIRE_MAILLAGE(  )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                          AFFE=_F(  TOUT = 'OUI',   MATER = MATER)  )

MOD=AFFE_MODELE(   MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'POU_D_TG')  )

E1=0.005

R1=(1.E-04 + pi*E1*E1) / (2.*pi*E1)

E2=0.006

R2=(2.E-04 + pi*E2*E2) / (2.*pi*E2)

CAREL=AFFE_CARA_ELEM(  MODELE=MOD,
                POUTRE=(_F( GROUP_MA = 'ACBC',  SECTION = 'CERCLE',
                            VARI_SECT = 'HOMOTHETIQUE',
                            CARA = ( 'R1', 'R2', 'EP1', 'EP2', ),
                            VALE = (  R2,   R2,   E2,   E2,  )),
                        _F( GROUP_MA = 'CDBD', SECTION = 'CERCLE',
                            VARI_SECT = 'HOMOTHETIQUE',
                            CARA = ( 'R1', 'R2', 'EP1', 'EP2', ),
                            VALE = (  R1,   R1,   E1,   E1,  ))) )

CHADEP=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_DEPL_R',
                   MAILLAGE=MA,
     AFFE=_F(   NOEUD = ('A1','B2',), NOM_CMP = ( 'DY',), VALE = ( 1.,))
                       )

CH=AFFE_CHAR_MECA(    MODELE=MOD,
           FORCE_NODALE=_F( NOEUD = 'D3',     FY = -9810.),DDL_IMPO=(
           _F( TOUT = 'OUI', DZ = 0., DRX = 0., DRY = 0.),
          _F( NOEUD = ('A1', 'B2',    ),  DX = 0.),
          _F( NOEUD = (        'B4',),         DY = 0.)),LIAISON_DDL=(
          _F(  NOEUD = ('A1','B4',),
                        DDL = ('DX','DX',),
                        COEF_MULT = (1.,-1.,),
                        COEF_IMPO = 0.),
          _F(  NOEUD = ('A1','B2',),
                        DDL = ('DY','DY',),
                        COEF_MULT = (1.,-1.,),
                        COEF_IMPO = 0.),
# D3,D4
          _F( NOEUD = ('D3','D4',),
                       DDL = ('DX','DX',),
                       COEF_MULT = (1.,-1.,),
                       COEF_IMPO = 0.),
          _F( NOEUD = ('D3','D4',),
                       DDL = ('DY','DY',),
                       COEF_MULT = (1.,-1.,),
                       COEF_IMPO = 0.),
# C3,C2
          _F( NOEUD = ('C3','C2',),
                       DDL = ('DX','DX',),
                       COEF_MULT = (1.,-1.,),
                       COEF_IMPO = 0.),
          _F( NOEUD = ('C3','C2',),
                       DDL = ('DY','DY',),
                       COEF_MULT = (1.,-1.,),
                       COEF_IMPO = 0.),
# C3,C1
          _F( NOEUD = ('C3','C1',),
                       DDL = ('DX','DX',),
                       COEF_MULT = (1.,-1.,),
                       COEF_IMPO = 0.),
          _F( NOEUD = ('C3','C1',),
                       DDL = ('DY','DY',),
                       COEF_MULT = (1.,-1.,),
                       COEF_IMPO = 0.)),
