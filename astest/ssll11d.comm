                   RESULTAT=DYNHAR0,
                   NOM_CHAM='VITE',
                   NOEUD='D4',
                   NOM_CMP='DY',
                   FREQ=100.0,
                   ),
                _F(VALE_CALC_C=(6.5168380651304+3.6415828960876j),
                   RESULTAT=DYNHAR0,
                   NOM_CHAM='ACCE',
                   NOEUD='D4',
                   NOM_CMP='DX',
                   FREQ=100.0,
