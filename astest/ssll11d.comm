MATELCC=CALC_MATR_ELEM(  MODELE=MOD,           OPTION='AMOR_MECA',
                             RIGI_MECA=MATELCR,    MASS_MECA=MATELCM,
                             CARA_ELEM=CAREL,      CHAM_MATER=CHMAT,
