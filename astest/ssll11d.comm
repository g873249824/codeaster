            _F(  NOEUD = ('C3','C1',),  DDL = ('DX','DX',),
                          COEF_MULT = (1.,-1.,),
                          COEF_IMPO = 0.+0.j),
            _F(  NOEUD = ('C3','C1',),  DDL = ('DY','DY',),
                          COEF_MULT = (1.,-1.,),
                          COEF_IMPO = 0.+0.j))
                    )

MATELCR=CALC_MATR_ELEM(  MODELE=MOD,           OPTION='RIGI_MECA',
                             CARA_ELEM=CAREL,      CHAM_MATER=CHMAT,
