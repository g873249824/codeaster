           VALE_REFE=0.99999938317435999,
           NOM_PARA='PROBA_WEIBULL',
           TABLE=WEIB,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=4,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=1.045823643,
           VALE_REFE=1.0458000000000001,
           NOM_PARA='TX_CROIS_CAVITES',
           TABLE=RET,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=1.045823643,
           VALE_REFE=1.0458000000000001,
           NOM_PARA='TX_CROIS_CAVITES',
           TABLE=RET,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=2,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=1.045823643,
           VALE_REFE=1.0458000000000001,
           NOM_PARA='TX_CROIS_CAVITES',
           TABLE=RET,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=3,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
