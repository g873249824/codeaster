#


MAT1=DEFI_MATERIAU(ELAS=_F(E=2.10000000000E11,
                           NU=0.29999999999999999,),);
#                                                       
#


CHMAT=AFFE_MATERIAU(MAILLAGE=SSNV127D,
                    AFFE=_F(GROUP_MA='SURFF',
                            MATER=MAT1,),);
