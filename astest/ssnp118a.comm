              NOM_CHAM='DEPL',NOM_CMP='DX',GROUP_NO='TOUTSEUL')

T_DX=CREA_TABLE(FONCTION=_F(FONCTION=F_DX))

IMPR_TABLE(TABLE=T_DX)

F_ETAT=RECU_FONCTION( RESULTAT=U,
              NOM_PARA_RESU='ETA_PILOTAGE')

T_ETAT=CREA_TABLE(FONCTION=_F(FONCTION=F_ETAT))

IMPR_TABLE(TABLE=T_ETAT)


TEST_TABLE(TABLE=T_DX,
           FILTRE=(_F(NOM_PARA='INST',
                      VALE=1.90005,),),
           NOM_PARA='DX',
           VALE=2.1676,
           PRECISION=0.001,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=T_ETAT,
           FILTRE=(_F(NOM_PARA='INST',
                      VALE=1.90005,),),
           NOM_PARA='ETA_PILO',
           VALE=1.,
