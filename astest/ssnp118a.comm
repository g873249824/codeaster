  ETAT_INIT  = _F(EVOL_NOLI = U), 
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON  , ),
    _F(CHARGE = TRACTION , TYPE_CHARGE = 'FIXE_PILO'),
