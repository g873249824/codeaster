DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

sigc=3.25
gc=0.150
dc=2*gc/sigc

kni=20.
ouvm=1000.

tfin=200.
pas=20
vchar=0.002

pimp=0.21

coef=1./(vchar/2)

VISCOL=DEFI_CONSTANTE(VALE=1.E-9,);

DVISCOL=DEFI_CONSTANTE(VALE=0.,);

PERML=DEFI_CONSTANTE(VALE=1.E-19,);

####################################
#  LISTE DES INSTANTS DE CALCUL
####################################

LISTINST=DEFI_LIST_REEL(DEBUT = 0.,
                        INTERVALLE=(
                                    _F(JUSQU_A = tfin,
                                      NOMBRE = tfin/pas ,),),);

####################################
#  OPERATIONS SUR LE MAILLAGE
####################################

MA0=LIRE_MAILLAGE(FORMAT='MED');

MA=CREA_MAILLAGE(MAILLAGE = MA0, LINE_QUAD = _F(TOUT='OUI'));

MA = DEFI_GROUP(reuse = MA,
       MAILLAGE = MA,
       CREA_GROUP_MA = (
          _F(NOM = 'BETON', GROUP_MA = 'G_2D_33'),
          _F(NOM = 'BORD', GROUP_MA = 'G_1D_39'),
          _F(NOM = 'I1', GROUP_MA = 'G_2D_34'),
          _F(NOM = 'I2', GROUP_MA = 'G_2D_35'),
          _F(NOM = 'I3', GROUP_MA = 'G_2D_36'),
          _F(NOM = 'FISSURE' , UNION = ('I1','I2','I3',),),
          _F(NOM = 'ENTREE',   GROUP_MA = 'G_1D_41'),
          _F(NOM = 'SORTIE' , GROUP_MA = 'G_1D_42'),
          _F(NOM = 'BORD_F',   GROUP_MA = 'G_1D_40'),
                        ),
        CREA_GROUP_NO=(
           _F(NOM='BORD_F',GROUP_MA='BORD_F',),
           _F(NOM='AG',GROUP_MA='G_0D_37',),
           _F(NOM='AD',GROUP_MA='G_0D_38',),
           _F(NOM='FISSURE',GROUP_MA='FISSURE',),
           _F(NOM='I3',GROUP_MA='I3',),
           _F(NOM='I1',GROUP_MA='I1',),
           _F(NOM='MEMB',DIFFE=('I1','BORD_F'),),
           _F(NOM='OUVERT',DIFFE=('FISSURE','I3','BORD_F','I1'),),
           _F(NOM='FERME',DIFFE=('FISSURE','OUVERT','BORD_F',),),
       ),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_FISSURE=_F(GROUP_MA='FISSURE'),
                 );

####################################
## AFFECTATION DU MODELE
####################################

MO=AFFE_MODELE(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA=('BETON','BORD'),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='D_PLAN_HMS',),
                         _F(GROUP_MA=('FISSURE','ENTREE','SORTIE',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='PLAN_JHMS',),),);

####################################
## ORIENTATION FISSURE
####################################

ORIEN_FI=AFFE_CARA_ELEM(MODELE=MO,
                        MASSIF=(_F(GROUP_MA=('FISSURE'),
                                  ANGL_REP=(0.,0.,0.,),),
                                ),);

####################################
## DEFINITION DES MATERIAUX
####################################

MAT_1=DEFI_MATERIAU(ELAS=_F(E=27500,
                          NU=0.2,
                          ALPHA=0.0,),
                  COMP_THM='LIQU_SATU',
                  THM_INIT=_F(PRE1=0.0,
                                   PORO=0.15,),
                  THM_DIFFU=_F(RHO=2000.0,
                                    BIOT_COEF=0.15,
                                    PESA_X=0.0,
                                    PESA_Y=00.0,
                                    PESA_Z=00.0,
                                    PERM_IN=PERML,),
                  THM_LIQU=_F(RHO=1.,
                                   UN_SUR_K=0.,
                                   VISC=VISCOL,
                                   D_VISC_TEMP=DVISCOL,),
                         );

MAT_2=DEFI_MATERIAU(
                  COMP_THM='LIQU_SATU',
                  THM_INIT=_F(PRE1=0.0,
                                   PORO=1.,),
                  THM_DIFFU=_F(RHO=2000.0,
                                    BIOT_COEF=1.,
                                    PESA_X=0.0,
                                    PESA_Y=00.0,
                                    PESA_Z=00.0,
                                    PERM_IN=PERML,),
                  THM_LIQU=_F(RHO=1.,
                                   UN_SUR_K=0.,
                                   VISC=VISCOL,
                                   D_VISC_TEMP=DVISCOL,),
                  THM_RUPT=_F(OUV_FICT=1.E1,
                              UN_SUR_N=1/3.E-4,
                                   ),
                  RUPT_FRAG=_F(GC=gc,
                               SIGM_C=sigc,
                               PENA_ADHERENCE=1.E-3,),
                         );

MAT_3=DEFI_MATERIAU(
                  COMP_THM='LIQU_SATU',
                  THM_INIT=_F(PRE1=0.0,
                                   PORO=1.,),
                  THM_DIFFU=_F(RHO=2000.0,
                                    BIOT_COEF=1.,
                                    PESA_X=0.0,
                                    PESA_Y=00.0,
                                    PESA_Z=00.0,
                                    PERM_IN=PERML,),
                  THM_LIQU=_F(RHO=1.,
                                   UN_SUR_K=0.,
                                   VISC=VISCOL,
                                   D_VISC_TEMP=DVISCOL,),
                  JOINT_BANDIS=_F(K=kni,
                                  DMAX=ouvm,
                                  GAMMA=0.,),
                         );

CM=AFFE_MATERIAU(MAILLAGE=MA,
                 AFFE=(_F(TOUT='OUI',
                         MATER=MAT_1,),
                       _F(GROUP_MA=('I2','I3'),
                         MATER=MAT_2,),
                       _F(GROUP_MA='I1',
                         MATER=MAT_3,),
                         ),)

####################################
## DEFINITION DES CHARGEMENTS
####################################

FORCE=AFFE_CHAR_MECA(
  MODELE=MO,
  FORCE_NODALE= (
    _F(GROUP_NO='AG',FX=-3.73,FY=-1.,),
    _F(GROUP_NO='AD',FX=3.73,FY=-1.,),
    ),
  );

LIAISON=AFFE_CHAR_MECA(
  MODELE=MO,
  DDL_IMPO= (
    _F(GROUP_MA=('SORTIE',),DX=0.,DY=0.),
    _F(GROUP_NO=('MEMB'),PRE1=0.),
    _F(GROUP_MA=('SORTIE','BORD' ),PRE1=0.),
    ),
  );

SYM=AFFE_CHAR_MECA(
  MODELE=MO,
  LIAISON_DDL= (
    _F(GROUP_NO=('AD','AG',),
    DDL=('DX','DX',),
    COEF_MULT=(1.,1.),
    COEF_IMPO=0.),
    ),
  );

PRESS=AFFE_CHAR_MECA(
  MODELE=MO,
  DDL_IMPO=(
    _F(GROUP_NO  =('OUVERT',),PRE1=pimp,),
  ),);

IMPER=AFFE_CHAR_CINE(
  MODELE=MO,
  MECA_IMPO=(
    _F(GROUP_MA  =('I1',),LH1=0.,),
  ),);

####################################
## INITIALISATION DES CHAMPS
####################################

CHAMNO=CREA_CHAMP(MAILLAGE=MA,
                    OPERATION='AFFE',
                    TYPE_CHAM='NOEU_DEPL_R',
                    AFFE=(
                           _F(TOUT='OUI',
                             NOM_CMP=('DX','DY','PRE1','LH1'),
                             VALE=(0.,0.,0.,0.,),),
                           _F(GROUP_MA=('I1','I2',),
                             NOM_CMP='PRE1',
                             VALE=pimp,),
                            _F(GROUP_MA=('I3','BETON'),
                             NOM_CMP='PRE1',
                             VALE=0.,), ),
                           );

CH_VARI=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='ELGA_VARI_R', MODELE=MO, PROL_ZERO='OUI',
         AFFE=(
           _F( GROUP_MA = 'BETON',   NOM_CMP = 'V3', VALE = 0),
           _F( GROUP_MA = 'I1',      NOM_CMP = 'V3', VALE = 0),
           _F( GROUP_MA = 'I2',      NOM_CMP = ('V1','V2','V3','V4','V5','V11',),
                                      VALE   = ( dc,   1,   2,   1., gc,   0.)),
           _F( GROUP_MA = 'I3',      NOM_CMP = 'V11', VALE =  0),
                    ),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =LISTINST ),
                        ECHEC=_F(
                  EVENEMENT     = 'ERREUR',
                  ACTION        = 'DECOUPE',
                  SUBD_METHODE  = 'MANUEL',
                  SUBD_PAS  = 5,
                  SUBD_PAS_MINI = 1.E-10,
                  SUBD_NIVEAU=12,
                                 ),);

####################################
## STAT NON LINE
####################################

U_1=STAT_NON_LINE(
  MODELE     = MO,
  CHAM_MATER = CM,
  CARA_ELEM  = ORIEN_FI,
  EXCIT      = (
                 _F(CHARGE = LIAISON),
                 _F(CHARGE = IMPER),
                 _F(CHARGE = SYM),
                 _F(CHARGE = PRESS),
                 _F(CHARGE = FORCE,
                    TYPE_CHARGE='FIXE_PILO',),
               ),
  ETAT_INIT=_F(DEPL=CHAMNO,VARI=CH_VARI),
  COMPORTEMENT=(_F(RELATION='KIT_HM',
                RELATION_KIT=('ELAS','LIQU_SATU','HYDR_UTIL',),
               GROUP_MA='BETON',),
             _F(RELATION='KIT_HM',
               RELATION_KIT=('CZM_LIN_REG','LIQU_SATU','HYDR_UTIL',),
               GROUP_MA=('I2',),),
            _F(RELATION='KIT_HM',
               RELATION_KIT=('CZM_EXP_REG','LIQU_SATU','HYDR_UTIL',),
               GROUP_MA=('I3',),),
            _F(RELATION='KIT_HM',
               RELATION_KIT=('JOINT_BANDIS','LIQU_SATU','HYDR_UTIL',),
               GROUP_MA=('I1',),),
                ),
  PILOTAGE=(_F(TYPE='DDL_IMPO',
            GROUP_NO='AD',
            NOM_CMP='DX',
            COEF_MULT=coef,
            ),),
  INCREMENT=_F(LIST_INST=DEFLIST,
               INST_FIN=tfin,),
  NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
  CONVERGENCE=_F(
                 ITER_GLOB_MAXI=50,),
  SOLVEUR = _F(METHODE = 'MUMPS', NPREC=-1),
  );

####################################
## TESTS DE VALIDATION
####################################

U_1=CALC_CHAMP(reuse=U_1,
        TOUT_ORDRE='OUI',
        TOUT='OUI',
        FORCE='FORC_NODA',
        RESULTAT=U_1,);

# TEST FY EN 'AD' A 100S, 150S ET 200S

TEST_RESU(RESU=_F(NUME_ORDRE=5,
                  GROUP_NO='AD',
                  RESULTAT=U_1,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_CALC=-11.864745396066,
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=10,
                  GROUP_NO='AD',
                  RESULTAT=U_1,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_CALC=-2.1004046111322,
                  ),
          )

# TEST PRE1  NS5768 A 140S ET 190S

TEST_RESU(RESU=_F(NUME_ORDRE=7,
                  RESULTAT=U_1,
                  NOM_CHAM='DEPL',
                  NOEUD='NS5768',
                  NOM_CMP='PRE1',
                  VALE_CALC=0.073701877958239,
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=10,
                  RESULTAT=U_1,
                  NOM_CHAM='DEPL',
                  NOEUD='NS5768',
                  NOM_CMP='PRE1',
                  VALE_CALC=0.16785844413280,
                  ),
          )

FIN();
