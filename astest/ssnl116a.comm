     _F(  GROUP_MA = 'GOA',
     PHENOMENE = 'MECANIQUE',
     MODELISATION = 'POU_D_EM'),
     _F(  GROUP_MA = ('GRBAR_T','GRBAR_H','GRBAR_V',),
     PHENOMENE = 'MECANIQUE',
     MODELISATION = 'BARRE'))
     )

