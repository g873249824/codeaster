DEBUT(   CODE=_F( NOM = 'WTNV135A',NIV_PUB_WEB='INTERNET',),PAR_LOT='OUI'  )
#
BIDON=DEFI_CONSTANTE(    VALE=1.)

UN=DEFI_CONSTANTE(    VALE=1.)

ZERO=DEFI_CONSTANTE(    VALE=0.)

VISCOLIQ=DEFI_CONSTANTE(    VALE=1.E-3)

VISCOGAZ=DEFI_CONSTANTE(    VALE=1.8E-5)

DVISCOL=DEFI_CONSTANTE(    VALE=0.)

DVISCOG=DEFI_CONSTANTE(    VALE=0.)

KINT=DEFI_CONSTANTE(    VALE=1.E-18)

#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED', )

MODELE=AFFE_MODELE(     MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = '3D_HM',
                                   )
                          )

MAILLAGE=MODI_MAILLAGE( reuse=MAILLAGE,   MAILLAGE=MAILLAGE,
                       ORIE_PEAU_3D=_F(  GROUP_MA = ('HAUT', 'DEVANT', 'DROITE',),  )
                     )
MAILLAGE = DEFI_GROUP( reuse        = MAILLAGE,
                          MAILLAGE     = MAILLAGE,
                          CREA_GROUP_MA=(_F(NOM='ROCHE',
                                            TOUT='OUI',)),
                          CREA_GROUP_NO=(_F(NOM='ROCHES',
                                            CRIT_NOEUD='SOMMET',
                                            GROUP_MA='ROCHE'),),);

SOL=DEFI_MATERIAU(              ELAS=_F(  E  = 5000.E6,
                                          NU = 0.12  ,
                                          RHO = 2500.,
                                          ALPHA = 0.),
                                LETK =_F(    PA          =   0.1E6,
                                             NELAS       =   0. ,
                                             SIGMA_C     =   12.E6,
                                             H0_EXT      =   1.10292,
                                             GAMMA_CJS   =   0.8,
                                             XAMS        =   0.1,
                                             ETA         =   0.04,
                                             A_0         =   0.25,
                                             A_E         =   0.60,
                                             A_PIC       =   0.4,
                                             S_0         =   0.0005,
                                             M_0         =   0.01,
                                             M_E         =   2.,
                                             M_PIC       =   6.,
                                             M_ULT       =   0.61,
                                             XI_ULT      =   0.365,
                                             XI_E        =   0.028,
                                             XI_PIC      =   0.015,
                                             MV_MAX      =   3.,
                                             XIV_MAX     =   0.0039,
                                             A           =   1.5E-12,
                                             N           =   4.5,
                                             SIGMA_P1    =   57.8E6,
                                             MU0_V       =   0.1,
                                             XI0_V       =   0.3,
                                             MU1         =   0.1,
                                             XI1         =   0.3,  ),
                               COMP_THM = 'LIQU_SATU',
                               THM_LIQU=_F(
                                RHO = 1000.,
#                               UN_SUR_K : 0.5E-9
                                UN_SUR_K = 0.0,
                                ALPHA = 1.E-4,
                                CP = 4180.,
                                VISC = VISCOLIQ,
                                D_VISC_TEMP = DVISCOL,
                                        ),
                              THM_GAZ=_F(
                                MASS_MOL = 28.96E-3,
                                CP = 1000.,
                                VISC = VISCOGAZ,
                                D_VISC_TEMP = DVISCOG,
                                  ),
                              THM_VAPE_GAZ=_F(
                                MASS_MOL = 18.E-3,
                                CP = 1870.,
                                VISC = BIDON,
                                D_VISC_TEMP = BIDON,
                                 ),
                              THM_DIFFU=_F(
                                R_GAZ = 8.315,
                                RHO = 2400.,
                                CP = 800.,
#                               BIOT_COEF : 0.7
                                BIOT_COEF = 1.,
                                SATU_PRES = UN,
                                D_SATU_PRES = ZERO,
                                PESA_X = 0.,
                                PESA_Y = 0.,
                                PESA_Z = 0.,
                                PERM_IN = KINT,
                                PERM_LIQU = UN,
                                D_PERM_LIQU_SATU = ZERO,
                                PERM_GAZ = UN,
                                D_PERM_SATU_GAZ = ZERO,
                                D_PERM_PRES_GAZ = ZERO,
                                 ),
                              THM_INIT=_F(
                                TEMP = 293.,
                                PRE1 = 0.0,
                                PRE2 = 0.1E6,
                                PORO = 0.14,
                                PRES_VAPE = 2269.8,
                                )
                           )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',    MATER = SOL,  )
                          )

PR_LATE=AFFE_CHAR_MECA(    MODELE=MODELE,
                              PRES_REP=_F(   GROUP_MA = ( 'DEVANT', 'DROITE', ),
                                          PRES = 1.,
                                    )
                       )

PR_V=AFFE_CHAR_MECA(    MODELE=MODELE,
                             PRES_REP=_F(   GROUP_MA = 'HAUT',    PRES = 1.,
                                    )
                        )

DEP_SYM=AFFE_CHAR_MECA(    MODELE=MODELE,FACE_IMPO=(
                              _F(  GROUP_MA = 'BAS',    DZ = 0.,   ),
                              _F(  GROUP_MA = 'DERRIERE',    DX = 0.,   ),
                              _F(  GROUP_MA = 'GAUCHE',    DY = 0.,   ))
                           )

P0=AFFE_CHAR_MECA(    MODELE=MODELE,
                               DDL_IMPO=_F( GROUP_NO='ROCHES',   PRE1 = 0.,  )
                           )

EVOL_HYD=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                         VALE=( 0.0,  0.0,
                                7000.0,  5.E6,)
                       )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS1=DEFI_LIST_REEL(   DEBUT=0.,
                           INTERVALLE=(_F(  JUSQU_A = 3600.0,    NOMBRE = 1),
                                       _F(  JUSQU_A = 7000.0,   NOMBRE =  1),
                         ),)

U1=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = PR_LATE,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = PR_V,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = DEP_SYM,
                ), _F(
                CHARGE = P0,
                )),
                         COMP_INCR=_F(
                ALGO_INTE='SPECIFIQUE',
                RELATION = 'KIT_HM',
                RELATION_KIT = (
                  'LETK',        'LIQU_SATU',
                  'HYDR_UTIL',  ),
                ),
                         INCREMENT=_F(
                LIST_INST = TEMPS1,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                )
              )

U1=CALC_CHAMP(reuse=U1,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=U1)


IMPR_RESU(MODELE=MODELE,
          RESU=(_F(RESULTAT=U1,INST=(7000.)),
                   ),);

TEMPS2=DEFI_LIST_REEL(   DEBUT=7000.,
                         INTERVALLE=(_F(  JUSQU_A = 8000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A = 9000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =10000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =11000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =12000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =13000.00,    NOMBRE = 100, ),
                                                        ))

ARCHI=DEFI_LIST_REEL(   DEBUT=7000.,
                         INTERVALLE=(_F(  JUSQU_A = 8000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A = 9000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =10000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =11000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =12000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =13000.00,   NOMBRE = 10, ),
                   ))
DEP_V=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  GROUP_MA = 'HAUT',    DZ = 1.,   )
                           )

EVOL_DEV=DEFI_FONCTION(      NOM_PARA='INST',
                          PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                         VALE=( 7000.0,     -7.6E-04,
                                8000.0,     -0.01,
                                9000.0,     -0.02,
                               10000.0,     -0.03,
                               11000.0,     -0.04,
                               12000.0,     -0.05,
                               13000.0,     -0.06,
                                ) )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =TEMPS2 ,),
                                ECHEC=_F(EVENEMENT     = 'ERREUR',
                                         ACTION        = 'DECOUPE',
                                         SUBD_METHODE  = 'MANUEL',
                                         SUBD_PAS      = 10,
                                         SUBD_PAS_MINI = 0.01,),)


U2=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = DEP_SYM,
                ), _F(
                CHARGE = PR_LATE,
                FONC_MULT = EVOL_HYD,
                ), _F(
                CHARGE = DEP_V,
                FONC_MULT = EVOL_DEV,
                ),_F(
                CHARGE = P0,
                ),
                ),
                         COMP_INCR=_F(
                ALGO_INTE='SPECIFIQUE',
                RELATION = 'KIT_HM',
                RELATION_KIT = (
                  'LETK',        'LIQU_SATU',
                  'HYDR_UTIL',  ),
                ),
                         ETAT_INIT=_F(
                EVOL_NOLI = U1,
                ),
                         INCREMENT=_F(
                LIST_INST = DEFLIST,
                ),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER=1,
                PAS_MINI_ELAS=1.0,
                REAC_ITER_ELAS=100,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 20,
                ITER_GLOB_ELAS = 100,
                )
              )

U2=CALC_CHAMP(reuse=U2,CRITERES=('SIEQ_ELNO'),VARI_INTERNE=('VARI_ELNO'),CONTRAINTE=('SIGM_ELNO'),RESULTAT=U2)


U2=CALC_CHAMP(  reuse=U2,
             RESULTAT=U2,
             CONTRAINTE='SIGM_NOEU',CRITERES='SIEQ_NOEU',VARI_INTERNE='VARI_NOEU'
          )


TFIN1 = 7000.

TFIN2 = 13000.


TEST_RESU(RESU=_F(INST=1.3000E4,
                  RESULTAT=U2,
                  NOM_CHAM='DEPL',
                  NOEUD='NO8',
                  NOM_CMP='DX',
                  VALE_CALC=0.030194228316090,
                  ),
          )

TEST_RESU(RESU=_F(INST=1.3000E4,
                  RESULTAT=U2,
                  NOM_CHAM='SIGM_NOEU',
                  NOEUD='NO8',
                  NOM_CMP='SIZZ',
                  VALE_CALC=-1.194050238299E+07,
                  ),
          )

FIN () ;
