                                 ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS      = 10,
                                 SUBD_NIVEAU   = 4,
                                 SUBD_PAS_MINI = 0.000001),)

try :
     U=STAT_NON_LINE(MODELE=MODELE,
                     CHAM_MATER=CHMAT,
                 EXCIT=( _F( CHARGE=CHAR_DR,  ),
                         _F( CHARGE=CHAR_GA,  ),
                         _F( CHARGE = DEPL_BA, ),
                         _F( CHARGE = DEPL_GA,),
                         _F( CHARGE = DEPL_HA,
                             FONC_MULT = COEF,),),
