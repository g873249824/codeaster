                                          DY = 1.)
                           )

COEF=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                          PROL_GAUCHE='CONSTANT',
                         VALE=( 0.0,    0.00,
                                2.0,   -0.030, )
                       )

TEMPS=DEFI_LIST_REEL(   DEBUT=0.,
