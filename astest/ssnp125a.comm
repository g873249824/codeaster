                                               'HAUT',
                                               'GAUCHE',),
                               PHENOMENE    = 'MECANIQUE',
                               MODELISATION = 'D_PLAN',),),
                           );

MATER1 = DEFI_MATERIAU(ELAS = _F( E  = 5800.0E6,
                                      NU = 0.3,),
                     DRUCK_PRAGER = _F( H     = -200.0E6,
                                   ALPHA = 0.33,
                                   SY = 2.11E6,
                                   P_ULTM = 0.01,
                                   ECROUISSAGE = 'LINEAIRE',),);

