DEBUT( CODE=_F(  NOM = 'SSNL107B',NIV_PUB_WEB='INTERNET') )

PRE_GIBI()

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),)

MA=DEFI_GROUP(reuse=MA,  MAILLAGE=MA,
   CREA_GROUP_NO=_F( GROUP_MA = 'L1')
               )

MO=AFFE_MODELE(
                           MAILLAGE=MA,AFFE=(
                          _F(
                                GROUP_MA = 'S1',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'DKT'),
                          _F(
                                GROUP_MA = 'POUTRE',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'POU_D_E'))
                        )

MAT=DEFI_MATERIAU(
                            ELAS=_F(
                                  E = 200000.0,
                                  NU = 0.3)
                          )

CARA=AFFE_CARA_ELEM(
            MODELE=MO,
            POUTRE=_F(  GROUP_MA = 'POUTRE',
                     SECTION = 'CERCLE',
                     CARA = ( 'R','EP', ),
                     VALE = ( 2.E-3, 2.E-4,)),
           COQUE=_F(  GROUP_MA = 'S1',COQUE_NCOU=19,
                   EPAIS = 0.1)
              )

CHMAT=AFFE_MATERIAU(
                            MAILLAGE=MA,
                           AFFE=_F(
                                 TOUT = 'OUI',
                                 MATER = MAT)
                           )

#

CHA=AFFE_CHAR_MECA(
     MODELE=MO,
      VERI_NORM='NON',DDL_IMPO=(
     _F(  GROUP_NO = 'L1',
                DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     _F(  GROUP_NO = ('P5', 'P6',),
                DX = 0., DY = 0., DZ = -0.2,  DRX = 0., DRY = 0., DRZ = 0.)),CONTACT=(
     _F(
               GROUP_MA_1 = 'COQU1',
               GROUP_MA_2 = 'POUT1',
               DIST_2 = -0.2,
               APPARIEMENT = 'NODAL',
               VECT_NORM_2 = (0., 0., -1.,)),
     _F(
              GROUP_MA_1 = 'COQU2',
              GROUP_MA_2 = 'POUT2',
              APPARIEMENT = 'NODAL',
              VECT_NORM_2 = (0., 0., -1.,)))
             )

#

L_INST=DEFI_LIST_REEL(
                             DEBUT=0.0,
                              INTERVALLE=_F( JUSQU_A = 5., NOMBRE = 5)
                           )

#

FONCM=DEFI_FONCTION(   NOM_PARA='INST',
                      VALE=( 0., 0., 100., 100., ) )

RESU=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CARA,
                             EXCIT=_F(
                CHARGE = CHA,
                FONC_MULT = FONCM),
                         COMP_INCR=_F(
                RELATION = 'ELAS',
               ),
                            NEWTON=_F(
                MATRICE = 'ELASTIQUE'),
                         INCREMENT=_F(
                LIST_INST = L_INST,
                NUME_INST_FIN = 2),
                       CONVERGENCE=_F(
                RESI_GLOB_MAXI = 1.E-6)
              )

RESU=CALC_ELEM( reuse=RESU,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=RESU,
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CARA
              )

TEST_RESU(RESU=(
           _F( RESULTAT = RESU, NOM_CHAM = 'DEPL',
                 NOEUD = 'N45', NOM_CMP = 'DZ',
                 NUME_ORDRE = 2, VALE = -0.19005,   PRECISION = 1.E-3),
           _F( RESULTAT = RESU, NOM_CHAM = 'DEPL',
                 NOEUD = 'N46', NOM_CMP = 'DZ',
                 NUME_ORDRE = 2, VALE = -0.19005,   PRECISION = 1.E-3),
           _F( RESULTAT = RESU, NOM_CHAM = 'SIEF_ELNO_ELGA',
                 NOEUD = 'N3', MAILLE = 'M21', NOM_CMP = 'N',
                 NUME_ORDRE = 2, VALE = -4.75126E-3, PRECISION = 1.E-2),
           _F( RESULTAT = RESU, NOM_CHAM = 'SIEF_ELNO_ELGA',
                 NOEUD = 'N1', MAILLE = 'M22', NOM_CMP = 'N',
                 NUME_ORDRE = 2, VALE = -4.75126E-3, PRECISION = 1.E-2))
          )

FIN()
