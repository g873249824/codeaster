#       METHODE PAR MODELISATION COMPLETE DE LA ROUE

DEBUT(CODE=_F(
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))

MAYA=LIRE_MAILLAGE(FORMAT='MED',);

ELAS=DEFI_MATERIAU(ELAS=_F(E=71000000000.0,
                           NU=0.3,
                           RHO=7820.0,),
                   ECRO_LINE=_F(SY=1.E15,D_SIGM_EPSI=0.),
                           );

MODELE=AFFE_MODELE(MAILLAGE=MAYA,
                   AFFE=_F(GROUP_MA='ALL_EL',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAYA,
                    MODELE=MODELE,
                    AFFE=_F(TOUT='OUI',
                            MATER=ELAS,),);

IMPO=AFFE_CHAR_MECA(MODELE=MODELE,
                    DDL_IMPO=_F(GROUP_NO='BAS_NO',
                                DX=0.0,
                                DY=0.0,
                                DZ=0.0,),);

ROTATION=AFFE_CHAR_MECA(MODELE=MODELE,
                        ROTATION= _F(VITESSE=157.0,
                                     AXE =(0.0,0.0,1.0),
                                     CENTRE=(0.0,0.0,0.0,),),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0,),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

PRECONT=STAT_NON_LINE(MODELE=MODELE,
                      CHAM_MATER=CHMAT,
                      EXCIT=(_F(CHARGE=IMPO,
                                TYPE_CHARGE='FIXE_CSTE',),
                             _F(CHARGE=ROTATION,
                                FONC_MULT=RAMPE,
                                TYPE_CHARGE='SUIV',),),
                      COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',
                                   DEFORMATION='SIMO_MIEHE',),
                      INCREMENT=_F(LIST_INST=L_INST,),
                      NEWTON=_F(MATRICE='TANGENTE',
                                REAC_ITER=1,),
                      CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                     ITER_GLOB_MAXI=30),);

CONTRAIN=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                    OPERATION='EXTR',
                    RESULTAT=PRECONT,
                    NUME_ORDRE=1,
                    NOM_CHAM='SIEF_ELGA',);


ASSEMBLAGE( MODELE=MODELE,
            CHARGE=(IMPO,ROTATION,),
            CHAM_MATER=CHMAT,
            NUME_DDL=CO("NUME"),
            MATR_ASSE=(
                       _F(MATRICE=CO("KASS"),
                          OPTION='RIGI_MECA'),
                       _F(MATRICE=CO("MASS"),
                          OPTION='MASS_MECA'),
                       _F(MATRICE=CO("KASGEO"),
                          OPTION='RIGI_GEOM',
                          SIEF_ELGA=CONTRAIN),
                       _F(MATRICE=CO("KASROT"),
                          OPTION='RIGI_ROTA'),
                       )
)


KASTOT=COMB_MATR_ASSE(COMB_R=(_F(MATR_ASSE=KASGEO,
                                 COEF_R=1.0,),
                              _F(MATR_ASSE=KASROT,
                                 COEF_R=1.0,),
                              _F(MATR_ASSE=KASS,
                                 COEF_R=1.0,),),);

MODES=MODE_ITER_SIMULT(MATR_RIGI=KASTOT,
                       MATR_MASS=MASS,
                       CALC_FREQ=_F(
                       OPTION='PLUS_PETITE',
                       NMAX_FREQ=5,),
                       );


MODES=CALC_CHAMP(reuse=MODES,MODELE=MODELE,CHAM_MATER=CHMAT,RESULTAT=MODES,CONTRAINTE=('SIEF_ELGA'),ENERGIE=('ETOT_ELGA','ETOT_ELNO','ETOT_ELEM'),EXCIT=(_F(CHARGE=IMPO),_F(CHARGE=ROTATION)))


DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=MODES,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

EVOLELAS=CREA_RESU(OPERATION='AFFE',
                   TYPE_RESU='EVOL_ELAS',
                   NOM_CHAM='DEPL',
                   AFFE=_F(
                   CHAM_GD=DEPL,
                   MODELE=MODELE,
                   CHAM_MATER=CHMAT,
                   INST=0.0,),);

EVOLELAS=CALC_CHAMP(reuse=EVOLELAS,RESULTAT=EVOLELAS,CONTRAINTE=('SIEF_ELGA'),ENERGIE=('ETOT_ELGA','ETOT_ELNO','ETOT_ELEM'),EXCIT=(_F(CHARGE=IMPO),_F(CHARGE=ROTATION)))



TEST_RESU(RESU=(_F(PARA='FREQ',
                   NUME_MODE=1,
                   RESULTAT=MODES,
                   VALE_CALC=31.45034893997,),
                _F(PARA='FREQ',
                   NUME_MODE=2,
                   RESULTAT=MODES,
                   VALE_CALC=31.974989217305,),
                _F(PARA='FREQ',
                   NUME_MODE=3,
                   RESULTAT=MODES,
                   VALE_CALC=31.974993085882,),
                _F(PARA='FREQ',
                   NUME_MODE=4,
                   RESULTAT=MODES,
                   VALE_CALC=33.48330404905,),
                _F(PARA='FREQ',
                   NUME_MODE=5,
                   RESULTAT=MODES,
                   VALE_CALC=33.483307096949,),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   POINT=1,
                   RESULTAT=EVOLELAS,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   VALE_CALC=5.0625313830007004,
                   MAILLE='MA225',
                   ),
                _F(NUME_ORDRE=1,
                   POINT=1,
                   RESULTAT=EVOLELAS,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   VALE_CALC=1.8546223993068001E4,
                   MAILLE='MA225',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=EVOLELAS,
                   NOM_CHAM='ETOT_ELNO',
                   NOEUD='NO9584',
                   NOM_CMP='TOTALE',
                   VALE_CALC=75.367826311062998,
                   MAILLE='MA155',
                   ),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   POINT=1,
                   RESULTAT=MODES,
                   NOM_CHAM='ETOT_ELEM',
                   NOM_CMP='TOTALE',
                   VALE_CALC=5.0625313830007004,
                   MAILLE='MA225',
                   ),
                _F(NUME_ORDRE=1,
                   POINT=1,
                   RESULTAT=MODES,
                   NOM_CHAM='ETOT_ELGA',
                   NOM_CMP='TOTALE',
                   VALE_CALC=1.8546223993068001E4,
                   MAILLE='MA225',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=MODES,
                   NOM_CHAM='ETOT_ELNO',
                   NOEUD='NO9584',
                   NOM_CMP='TOTALE',
                   VALE_CALC=75.367826311062998,
                   MAILLE='MA155',
                   ),
                ),
          )

FIN();
