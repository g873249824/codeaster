DEBUT(CODE=_F(NOM='SDNV103E',
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED');

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(TOUT_GROUP_MA='OUI',),
                             _F(OPTION='PLAN',
                                NOM='CYLBAS',
                                POINT=(0.,0.,2.27),
                                VECT_NORMALE=(0,0,1),
                                PRECISION=0.001),),)

MODE=AFFE_MODELE(MAILLAGE=MA,
                 VERIF='MAILLE',
                 AFFE=_F(GROUP_MA=('VOL','VOL_RIG'),
                         PHENOMENE='MECANIQUE',
                         MODELISATION='3D',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=117.E3,
                            NU=0.35,
                            RHO=8.930E-9,),
                    ECRO_LINE=_F(D_SIGM_EPSI=100.,
                                 SY=400.,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(GROUP_MA=('VOL','VOL_RIG'),
                            MATER=ACIER,),);

VIT_0=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                 OPERATION='AFFE',
                 MODELE=MODE,
                 PROL_ZERO='NON',
                 AFFE=(_F(GROUP_MA='VOL',
                          NOM_CMP=('DX','DY','DZ',),
                          VALE=(0.,0.,-227000.,),),
                       _F(GROUP_NO='VOL_RIG',
                          NOM_CMP=('DX','DY','DZ',),
                          VALE=(0.,0.,0.,),),),)

CIMP=DEFI_CONSTANTE(VALE=2.27)

DIST=DEFI_CONSTANTE(VALE=-1)

LIAI_UNI=DEFI_CONTACT( MODELE  = MODE,
                       FORMULATION = 'LIAISON_UNIL',
                       ZONE = _F( GROUP_NO   = ('CYLBAS'),
                                  NOM_CMP    = ('DZ'),
                                  COEF_IMPO  = CIMP,
                                  COEF_MULT  = DIST,),)

L_INST=DEFI_LIST_REEL(DEBUT=-1.E-5,
                      INTERVALLE=(_F(JUSQU_A=-2.5E-7,
                                     NOMBRE=1,),
                                  _F(JUSQU_A=2.5E-7,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=80.E-6,
                                     NOMBRE=316,),),);

L_SAUV=DEFI_LIST_REEL(DEBUT=-1.E-5,
                      INTERVALLE=(_F(JUSQU_A=2.5E-7,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=80.E-6,
                                     NOMBRE=316,),),);

RESU=DYNA_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CHMAT,
                   CONTACT = LIAI_UNI,
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='SIMO_MIEHE',
                                GROUP_MA='VOL',),
                   ETAT_INIT=_F(VITE=VIT_0,),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=2.5E-7,),
                   SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
                                FORMULATION='DEPLACEMENT',
                                BETA=0.49,GAMMA=0.9,),
                   NEWTON=_F(REAC_INCR=1,
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   SOLVEUR=_F(SYME='OUI',),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
                                  ITER_GLOB_MAXI=20,
                                  ARRET='OUI',),
                   );

# DEPLACEMENT VERTICAL POINT A

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   GROUP_NO='A1',
                   VALE=-2.27,
                   CRITERE='RELATIF',
                   PRECISION=1.E-10,
                   REFERENCE='ANALYTIQUE',),));

# VITESSE VERTICALE POINT A

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='VITE',
                   NOM_CMP='DZ',
                   GROUP_NO='A1',
                   VALE=-1.9453030264492E+05,
                   CRITERE='RELATIF',
                   PRECISION=1.E-10,
                   REFERENCE='NON_REGRESSION',
                   VERSION='10.3.16',),));

# DEPLACEMENT VERTICAL POINT B

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   GROUP_NO='B1',
                   VALE=-2.32675,
                   CRITERE='RELATIF',
                   PRECISION=1.E-10,
                   REFERENCE='NON_REGRESSION',
                   VERSION='10.3.16',),),);

# VITESSE VERTICALE POINT B

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='VITE',
                   NOM_CMP='DZ',
                   GROUP_NO='B1',
                   VALE=-2.2700000000001E+05,
                   CRITERE='RELATIF',
                   PRECISION=1.E-10,
                   REFERENCE='ANALYTIQUE',
                   ),),);

# DEPLACEMENT ET VITESSE RADIAL POINT A

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   GROUP_NO='A1',
                   VALE=1.5613650399252E-03,
                   CRITERE='RELATIF',
                   PRECISION=1.E-10,
                   REFERENCE='NON_REGRESSION',
                   VERSION='10.3.16',),
                _F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='VITE',
                   NOM_CMP='DX',
                   GROUP_NO='A1',
                   VALE=-16297.463201374,
                   CRITERE='RELATIF',
                   PRECISION=1.E-10,
                   REFERENCE='NON_REGRESSION',
                   VERSION='10.3.16',),),);


# DEPLACEMENT ET VITESSE RADIAL POINT B

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='B1',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-12,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RESU,
                   INST=2.5e-07,
                   NOM_CHAM='VITE',
                   NOM_CMP='DY',
                   GROUP_NO='B1',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-8,
                   REFERENCE='ANALYTIQUE',),),);



FIN()
