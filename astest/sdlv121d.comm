                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'D_PLAN'),
                         _F(  GROUP_MA = 'CONTOUR',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'D_PLAN_ABSO'))
                        )

