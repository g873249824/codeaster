DEPLPES=MECA_STATIQUE(
                  MODELE=MO,
               CARA_ELEM=CARE,
              CHAM_MATER=CHMAT,
                   EXCIT=_F( CHARGE = CHPES),
                         )

CALC_ELEM( reuse= DEPLPES ,RESULTAT= DEPLPES ,
           OPTION=('EFGE_ELNO_DEPL','SIGM_ELNO_DEPL',),
           NIVE_COUCHE='INF'
                         )

# ------------------------------------------------------------------

LISTIME=DEFI_LIST_REEL(VALE=( 0.0, 1.0, ), )

