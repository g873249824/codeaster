                  MODELE=MO,
              CHAM_MATER=CHMAT,
               CARA_ELEM=CARE,
               COMP_INCR=_F(RELATION='ELAS'),
                   EXCIT=_F( CHARGE = CHDIL),
               INCREMENT=_F(LIST_INST=LISTIME,),
                         )

DEP1=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                 RESULTAT=STATNL1, NOM_CHAM='DEPL', INST=1. )

DEPLDIL=CREA_RESU(  OPERATION='AFFE', TYPE_RESU='EVOL_ELAS',  NOM_CHAM='DEPL',
                    AFFE=_F(  INST = 1.,  CHAM_GD = DEP1,
                              MODELE=MO, CHAM_MATER=CHMAT, CARA_ELEM=CARE, )     )

CALC_ELEM( reuse= DEPLDIL ,RESULTAT= DEPLDIL ,
           OPTION=('EFGE_ELNO_DEPL','SIGM_ELNO_DEPL',),
           EXCIT=_F( CHARGE = CHDIL),
           NIVE_COUCHE='INF'
                         )

STATNL2=STAT_NON_LINE(
                  MODELE=MO,
              CHAM_MATER=CHMATH,
               CARA_ELEM=CARE,
               COMP_INCR=_F(RELATION='ELAS'),
                   EXCIT=_F( CHARGE = CHDIL),
               INCREMENT=_F(LIST_INST=LISTIME,),
                         )

DEP2=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                 RESULTAT=STATNL2, NOM_CHAM='DEPL', INST=1. )

DEPLDILT=CREA_RESU(  OPERATION='AFFE', TYPE_RESU='EVOL_ELAS',  NOM_CHAM='DEPL',
                     AFFE=_F(  INST = 1.,  CHAM_GD = DEP2,
                               MODELE=MO, CHAM_MATER=CHMATH, CARA_ELEM=CARE, )     )

CALC_ELEM( reuse= DEPLDILT ,RESULTAT= DEPLDILT ,
           OPTION=('EFGE_ELNO_DEPL','SIGM_ELNO_DEPL',),
           EXCIT=_F( CHARGE = CHDIL),
           NIVE_COUCHE='INF'
                         )

# ------------------------------------------------------------------

DEPLDI2=MECA_STATIQUE(
                  MODELE=MO,
              CHAM_MATER=CHMAT,
               CARA_ELEM=CARE,
                   EXCIT=_F( CHARGE = CHDI2),
