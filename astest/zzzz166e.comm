DEBUT(CODE=_F(NOM='ZZZZ166E',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
#
PRE_GIBI()
#
MAIL=LIRE_MAILLAGE( )
#
MO=AFFE_MODELE(MAILLAGE=MAIL,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'THERMIQUE',
                       MODELISATION = '3D'))
#
MAT=DEFI_MATERIAU(THER=_F(LAMBDA = 1.0,
                          RHO_CP = 0.0))
#
