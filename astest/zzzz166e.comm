                    AFFE=_F(TOUT = 'OUI',
                            MATER = MAT,
                            ))
#
#-----------------------------------------------------------------
# CHAMP DE TEMPERATURE LINEAIRE => FLUX CONSTANT
# ===============================================
FTEMPL = FORMULE(NOM_PARA=('X','Y','Z'),VALE='2.*X +3.*Y + 4.*Z')
#
CHTEMPL = CREA_CHAMP(OPERATION='AFFE',
                    TYPE_CHAM='NOEU_TEMP_F',
                    MAILLAGE=MAIL,
                    AFFE=_F(TOUT = 'OUI',
                            NOM_CMP = ('TEMP',),
                            VALE_F = (FTEMPL,))
                 )
#
RESUL = CREA_RESU(OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD = CHTEMPL,
                         INST = 1.0,
                         MODELE=MO,
                         CHAM_MATER=CHMAT,)
                )
#
RESUL = CALC_ELEM(reuse=RESUL,
                 RESULTAT=RESUL,
                 OPTION=('FLUX_ELGA','FLUX_ELNO',)
                )
#
