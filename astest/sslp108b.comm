# ARLEQUIN : SUBSTITUTION 2D/2D INCOMPATIBLE.
#            MAILLAGES DE DENSITES DIFFERENTES

DEBUT(CODE=_F(NOM='SSLP108B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI',),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                  GROUP_NO_FOND='P0',),);

MODARL=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);

MATERI=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                             NU=0.3,
                             ALPHA=0.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=MATERI,),);

BLOC=AFFE_CHAR_MECA(MODELE=MODARL,
                    DDL_IMPO=_F(GROUP_MA='L23',
                                LIAISON='ENCASTRE',),);

CHAREX=AFFE_CHAR_MECA(MODELE=MODARL,
                      FORCE_CONTOUR=(_F(GROUP_MA='L12',
                                        FX=1.E8,
                                        FY=1.E8,),
                                     _F(GROUP_MA='L34',
                                        FX=-1.E8,
                                        FY=-1.E8,),),);

##################################################
# 1. Creation des relations de couplage Arlequin
#    entre les groupes de mailles
#    couronne et fissure (maillage fin)
#    et la plaque (maillage grossier)
#    Collage sur le modele fin
##################################################

ARLEQ=AFFE_CHAR_MECA(MODELE=MODARL,
                     ARLEQUIN=_F(GROUP_MA_1=('COUR','FISS',),
                                 GROUP_MA_2='PLAQ',
                                 GROUP_MA_COLL='COUR',
                                 COLLAGE='GROUP_MA_1',
                                 POIDS_1=0.99999,),);

####################
# 2. CALCUL STATIQUE
####################

RESU=MECA_STATIQUE(MODELE=MODARL,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=ARLEQ,),
                          _F(CHARGE=BLOC,),
                          _F(CHARGE=CHAREX,),),);

############################
# 3. POST-TRAITEMENT GENERAL
#    => deplacements
#    => contraintes
############################

RESU=CALC_ELEM(reuse =RESU,
               RESULTAT=RESU,
               OPTION=('EQUI_ELNO_SIGM','SIGM_ELNO_DEPL',),);

RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION=('EQUI_NOEU_SIGM','SIGM_NOEU_DEPL',),);

LIGAM=INTE_MAIL_2D(MAILLAGE=MAIL,
                   DEFI_CHEMIN=_F(GROUP_MA='LPOP11',),);

BHAUT=INTE_MAIL_2D(MAILLAGE=MAIL,
                   DEFI_CHEMIN=_F(GROUP_MA='L12',),);

SIYY_LIG=POST_RELEVE_T(ACTION=_F(INTITULE='SIYY_LIG',
                                 CHEMIN=LIGAM,
                                 RESULTAT=RESU,
                                 NOM_CHAM='SIGM_NOEU_DEPL',
                                 NOM_CMP='SIYY',
                                 MOYE_NOEUD='NON',
                                 OPERATION='EXTRACTION',),);

DEPL_H=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL_H',
                               CHEMIN=BHAUT,
                               RESULTAT=RESU,
                               NOM_CHAM='DEPL',
                               NOM_CMP='DY',
                               OPERATION='EXTRACTION',),);

########################################################
# 4. POST-TRAITEMENT MECANIQUE DE LA RUPTURE
# Attention : on est sur un modele Arlequin !
#             Il faut donc restreindre le resultat RESU
#             au groupe de mailles du patch (RESU_RES).
########################################################

IMPR_RESU(FORMAT='MED',
          UNITE=81,
          RESTREINT=_F(GROUP_MA=('COUR','FISS',),
                       TOUT_GROUP_NO='OUI',),
          RESU=_F(RESULTAT=RESU,
                  NOM_CHAM='DEPL',),);

MA_RES=LIRE_MAILLAGE(UNITE=81,
                     FORMAT='MED',
                     VERI_MAIL=_F(VERIF='OUI',),);

MA_RES=MODI_MAILLAGE(reuse =MA_RES,
                     MAILLAGE=MA_RES,
                     MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                    GROUP_NO_FOND='P0',),);

MO_RES=AFFE_MODELE(MAILLAGE=MA_RES,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);

MAT_RES=AFFE_MATERIAU(MAILLAGE=MA_RES,
                      AFFE=_F(TOUT='OUI',
                              MATER=MATERI,),);

RESU_RES=LIRE_RESU(TYPE_RESU='EVOL_ELAS',
                   FORMAT='MED',
                   MODELE=MO_RES,
                   CHAM_MATER=MAT_RES,
                   UNITE=81,
                   FORMAT_MED=_F(NOM_CHAM='DEPL',
                                 NOM_CHAM_MED='&RESUR1_DEPL____________________',),
                   TOUT_ORDRE='OUI',);

FOND1=DEFI_FOND_FISS(MAILLAGE=MA_RES,
                     FOND_FISS=_F(GROUP_NO='P0',),
                     NORMALE=(0.0,-1.0,0.0,),);

THETA01=CALC_THETA(MODELE=MO_RES,
                   DIRECTION=(-1.0,0.0,0.0,),
                   THETA_2D=_F(GROUP_NO='P0',
                               MODULE=1.0,
                               R_INF=1e-05,
                               R_SUP=0.00014,),);

###################################################
# 5. On peut post-traiter sur le modele restreint :
#    => calcul de G, K1, K2, G_IRWIN
###################################################

GK01=CALC_G(THETA=_F(THETA=THETA01,),
            RESULTAT=RESU_RES,
            SYME_CHAR='SANS',
            OPTION='CALC_G',);

GK02=CALC_G(THETA=_F(THETA=THETA01,
                     FOND_FISS=FOND1,),
            RESULTAT=RESU_RES,
            SYME_CHAR='SANS',
            OPTION='CALC_K_G',);

#########################
# 6. TESTS NON REGRESSION
#    => deplacements
#    => contraintes
#########################

TEST_TABLE(TABLE=DEPL_H,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=2.30000E-03),
           NOM_PARA='DY',
           VALE = -8.1928971860135E-07,
           PRECISION=1.3E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

TEST_TABLE(TABLE=SIYY_LIG,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0.00000E+00),
           NOM_PARA='SIYY',
           VALE = 2.3147057527191E+09,
           PRECISION=2.5E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

TEST_TABLE(TABLE=SIYY_LIG,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=4.00000E-04),
           NOM_PARA='SIYY',
           VALE = 1.6799139069449E+08,
           PRECISION=1.5E-7,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

TEST_TABLE(TABLE=SIYY_LIG,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=8.00000E-04),
           NOM_PARA='SIYY',
           VALE = 1.2776802343333E+08,
           PRECISION=8E-7,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

##############################################################
# 7. TESTS PAR RAPPORT A CALCUL SANS ARLEQUIN (MODELISATION A)
#    => contraintes
##############################################################

TEST_TABLE(TABLE=SIYY_LIG,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0.00000E+00),
           NOM_PARA='SIYY',
           VALE = 2.33237E+09,
           PRECISION=0.008,
           REFERENCE='AUTRE_ASTER');

TEST_TABLE(TABLE=SIYY_LIG,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=4.00000E-04),
           NOM_PARA='SIYY',
           VALE = 1.70803E+08,
           PRECISION=0.02,
           REFERENCE='AUTRE_ASTER');

TEST_TABLE(TABLE=SIYY_LIG,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=8.00000E-04),
           NOM_PARA='SIYY',
           VALE = 1.24032E+08,
           PRECISION=0.04,
           REFERENCE='AUTRE_ASTER');

#################################################
# 8. TESTS NON REGRESSION MECANIQUE DE LA RUPTURE
#    => G, K1, K2, G_IRWIN
#################################################

TEST_TABLE(TABLE=GK01,
           NOM_PARA='G',
           VALE=3.5838791324382E+02,
           PRECISION=4E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

TEST_TABLE(TABLE=GK02,
           NOM_PARA='G',
           VALE=3.5838791324382E+02,
           PRECISION=4E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

TEST_TABLE(TABLE=GK02,
           NOM_PARA='K1',
           VALE=8.1616006312675E+06,
           PRECISION=3E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

TEST_TABLE(TABLE=GK02,
           NOM_PARA='K2',
           VALE=-2.3708427456132E+06,
           PRECISION=8E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

TEST_TABLE(TABLE=GK02,
           NOM_PARA='G_IRWIN',
           VALE=3.6116310094366E+02,
           PRECISION=4E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='9.0.4',);

##############################################################
# 9. TESTS PAR RAPPORT A CALCUL SANS ARLEQUIN (MODELISATION A)
#    => G, K1, K2, G_IRWIN
##############################################################

TEST_TABLE(TABLE=GK01,
           NOM_PARA='G',
           VALE=3.63689E+02,
           PRECISION=0.015,
           REFERENCE='AUTRE_ASTER');

TEST_TABLE(TABLE=GK02,
           NOM_PARA='G',
           VALE=3.63689E+02,
           PRECISION=0.015,
           REFERENCE='AUTRE_ASTER');

TEST_TABLE(TABLE=GK02,
           NOM_PARA='K1',
           VALE=8.16121E+06,
           PRECISION=0.0001,
           REFERENCE='AUTRE_ASTER');

TEST_TABLE(TABLE=GK02,
           NOM_PARA='K2',
           VALE=-2.47739E+06,
           PRECISION=0.05,
           REFERENCE='AUTRE_ASTER');

TEST_TABLE(TABLE=GK02,
           NOM_PARA='G_IRWIN',
