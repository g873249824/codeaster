DEBUT(CODE=_F(NOM='SSLP108B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI',),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                GROUP_NO_FOND='P0',));

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

MAT=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                          NU=0.3,
                          ALPHA=0.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

BL=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=_F(GROUP_MA='L23',
                              LIAISON='ENCASTRE',),);

CH=AFFE_CHAR_MECA(MODELE=MO,
                  FORCE_CONTOUR=(_F(GROUP_MA='L12',
                                    FY=1.E8,
                                     FX=1.E8,),
                                 _F(GROUP_MA='L34',
                                    FY=-1.E8,
                                    FX=-1.E8,),),);

AR=AFFE_CHAR_MECA(MODELE=MO,
                  ARLEQUIN=_F(GROUP_MA_1=('COUR','FISS',),
                              GROUP_MA_2='PLAQ',
                              GROUP_MA_COLL='COUR',
                              COLLAGE='GROUP_MA_1',
                              POIDS_1=0.99999,),);

### CALCUL STATIQUE

RESU=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=AR,),
                          _F(CHARGE=BL,),
                          _F(CHARGE=CH,),),);

### POST-TRAITEMENT GENERAL

RESU=CALC_ELEM(reuse =RESU,
               RESULTAT=RESU,
               OPTION=('EQUI_ELNO_SIGM','SIGM_ELNO_DEPL',),);

RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION=('EQUI_NOEU_SIGM','SIGM_NOEU_DEPL',),);


LIGAM=INTE_MAIL_2D(MAILLAGE=MA,
                   DEFI_CHEMIN=_F(GROUP_MA='LPOP11',),);

BHAUT=INTE_MAIL_2D(MAILLAGE=MA,
                   DEFI_CHEMIN=_F(GROUP_MA='L12',),);


SIYY_LIG=POST_RELEVE_T(ACTION=_F(INTITULE='SIYY_LIG',
                                 CHEMIN=LIGAM,
                                 RESULTAT=RESU,
                                 NOM_CHAM='SIGM_NOEU_DEPL',
                                 NOM_CMP='SIYY',
                                 MOYE_NOEUD='NON',
                                 OPERATION='EXTRACTION',),);

DEPL_H=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL_H',
                               CHEMIN=BHAUT,
                               RESULTAT=RESU,
                               NOM_CHAM='DEPL',
                               NOM_CMP='DY',
                               OPERATION='EXTRACTION',),);

### POST-TRAITEMENT MECA. RUPTURE

DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RESU,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

FOND1=DEFI_FOND_FISS(MAILLAGE=MA,
                     FOND_FISS=_F(GROUP_NO='P0',),
                     NORMALE=(0.0,-1.0,0.0,),);

THETA01=CALC_THETA(MODELE=MO,
                   DIRECTION=(-1.0,0.0,0.0,),
                   THETA_2D=_F(GROUP_NO='P0',
                               MODULE=1.0,
                               R_INF=1e-05,
                               R_SUP=0.00014,),);

GK01=CALC_G(RESULTAT=RESU,
            THETA=_F(THETA=THETA01,),
            EXCIT=(_F(CHARGE=BL,),
                   _F(CHARGE=CH,),
                   _F(CHARGE=AR,),),
            SYME_CHAR='SANS',
            OPTION='CALC_G',
            INFO=1,);

GK02=CALC_G(RESULTAT=RESU,
            THETA=_F(THETA=THETA01,FOND_FISS=FOND1,),
            EXCIT=(_F(CHARGE=BL,),
                   _F(CHARGE=CH,),
                   _F(CHARGE=AR,),),
            SYME_CHAR='SANS',
            OPTION='CALC_K_G',
            INFO=1,);

