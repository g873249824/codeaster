DEBUT(CODE=_F(NOM='SDNV103H',
              NIV_PUB_WEB='INTERNET',),
      IGNORE_ALARM='CONTACT_8',
DEBUG=_F(SDVERI='OUI'))

# IGNORE ALARM: ON UTILISE DES QUAD8 EN CONTACT

MA=LIRE_MAILLAGE(FORMAT='MED',);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(TOUT_GROUP_MA='OUI',),
                             _F(GROUP_MA='SZZ',
                                NOM='MILIEU',
                                CRIT_NOEUD='MILIEU',),
                             _F(DIFFE=('SXX','MILIEU',),
                                NOM='SXX1',),
                             _F(DIFFE=('SYY','MILIEU',),
                                NOM='SYY1',),),);

MODE=AFFE_MODELE(MAILLAGE=MA,
                 VERIF='MAILLE',
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION=('3D',),),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=_F(GROUP_MA=('SZZ','RIGID',),),
                 INFO=1,);

ACIER=DEFI_MATERIAU(ELAS=_F(E=117.E3,
                            NU=0.35,
                            RHO=8.930E-9,),
                    ECRO_LINE=_F(D_SIGM_EPSI=100.,
                                 SY=400.,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(GROUP_MA=('VOL','VOL_RIG',),
                            MATER=ACIER,),);

CH=AFFE_CHAR_MECA(MODELE=MODE,
                  DDL_IMPO=(_F(GROUP_MA='RIGID',
                               DX=0.,
                               DY=0.,
                               DZ=0.,),
                            _F(GROUP_MA='VOL_RIG',
                               DX=0.,
                               DY=0.,
                               DZ=0.,),
                            _F(GROUP_NO='SYY1',
                               DY=0.,),
                            _F(GROUP_NO='SXX1',
                               DX=0.,),),
                  );


CHCONT  = DEFI_CONTACT(MODELE         = MODE,
                       FORMULATION    = 'DISCRETE',
                       REAC_GEOM      = 'CONTROLE',
                       NB_ITER_GEOM   = 2,
                       FROTTEMENT     = 'COULOMB',

                       ZONE =_F(
                             ALGO_CONT      = 'LAGRANGIEN',
                             ALGO_FROT      = 'PENALISATION',
                             COULOMB        = 0.25,
                             E_T            = 1.E5,
                             APPARIEMENT    = 'MAIT_ESCL',
                             GROUP_MA_MAIT  = 'RIGID',
                             GROUP_MA_ESCL  = 'SZZ',
                             COEF_MATR_FROT = 0.4,
                               ),
                      );

VIT_0=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                 OPERATION='AFFE',
                 MODELE=MODE,

                 AFFE=(
                 _F(TOUT='OUI',
                    NOM_CMP=('DX','DY','DZ',),
                    VALE=(0.,0.,-227000.,),),
                 _F(GROUP_NO='VOL_RIG',
                    NOM_CMP=('DX','DY','DZ',),
                    VALE=(0.,0.,0.,),),
                 _F(GROUP_NO='RIGID',
                    NOM_CMP=('DX','DY','DZ',),
                    VALE=(0.,0.,0.,),),),);


NPAS_IMPA   = 11
IFIN   = +1.0E-5


temps=DEFI_LIST_REEL(DEBUT=-1.E-5,
                      INTERVALLE=(_F(JUSQU_A=IFIN,
                                     NOMBRE=NPAS_IMPA,),
                                   ),);



# TEST EN MODE DEFI_LIST_INST MANUEL

DEF_MANU =DEFI_LIST_INST(  DEFI_LIST=_F(LIST_INST = temps,),
                          ECHEC    =(
                            _F(
                               EVENEMENT        = 'ERREUR',
                               ACTION           = 'DECOUPE',
                               SUBD_METHODE     = 'MANUEL',
                              ),
                            _F(
                               EVENEMENT        = 'COLLISION',
                               ACTION           = 'DECOUPE',
                               SUBD_METHODE     = 'AUTO',
                               SUBD_INST = 2.E-8,
                               SUBD_DUREE    = 1.5E-6,
                              ),
                          ),
                        )

RESU1=DYNA_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CH,),
                   CONTACT = CHCONT,
                   COMPORTEMENT=(_F(
                                 RELATION='VMIS_ISOT_LINE',
                                 DEFORMATION='SIMO_MIEHE',
                                 TOUT='OUI',),
                              _F(RELATION='ELAS',
                                 DEFORMATION='PETIT',
                                 GROUP_MA='VOL_RIG',),),
                   ETAT_INIT=_F(VITE=VIT_0,),
                   INCREMENT=_F(LIST_INST=DEF_MANU,),
                   SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',
                                 FORMULATION='DEPLACEMENT',
                                 BETA=0.49,GAMMA=0.9,),
                   NEWTON=_F(REAC_INCR=1,
                             REAC_ITER=1,),
                   SOLVEUR=_F(SYME='OUI',),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
                                  ITER_GLOB_MAXI=20,),
                                );
#
### POST-TRAITEMENTS
#

CONTB1_1=POST_RELEVE_T (ACTION = _F(GROUP_NO='B1',
                                 INTITULE = 'MESSAGE',
                                 RESULTAT = RESU1,
                                 TOUT_CMP='OUI',
                                 NOM_CHAM   = 'VALE_CONT',
                                 OPERATION = 'EXTRACTION'))

DEPLA2_1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN A2',
                                GROUP_NO='A2',
                                RESULTAT=RESU1,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

VITEA2_1=POST_RELEVE_T(ACTION=_F(INTITULE='VITE EN A2',
                                GROUP_NO='A2',
                                RESULTAT=RESU1,
                                NOM_CHAM='VITE',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

DEPLA1_1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN A1',
                                GROUP_NO='A1',
                                RESULTAT=RESU1,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);


VITEA1_1=POST_RELEVE_T(ACTION=_F(INTITULE='VITE EN A1',
                                GROUP_NO='A1',
                                RESULTAT=RESU1,
                                NOM_CHAM='VITE',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

DEPLB1_1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN B1',
                                GROUP_NO='B1',
                                RESULTAT=RESU1,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DZ',
                                OPERATION='EXTRACTION',),);

VITEB1_1=POST_RELEVE_T(ACTION=_F(INTITULE='VITE EN B1',
                                GROUP_NO='B1',
                                RESULTAT=RESU1,
                                NOM_CHAM='VITE',
                                NOM_CMP='DZ',
                                OPERATION='EXTRACTION',),);

#
### TESTS DE DEPLACEMENTS ET VITESSES
#
# Le test est instable suivant les plateformes (dynamique+frottement discret)
# TOLE_MACHINE est relache

TEST_RESU(RESU=(_F(GROUP_NO='B1',
                   INST=1.0E-05,
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=0.75715313319935,
                   TOLE_MACHINE=5.E-5,                  # TODO TOLE_MACHINE
                   ),
                _F(GROUP_NO='A2',
                   INST=1.0E-05,
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                _F(GROUP_NO='B1',
                   INST=1.0E-05,
                   RESULTAT=RESU1,
                   NOM_CHAM='VITE',
                   NOM_CMP='DX',
                   VALE_CALC=78447.636897479,
                   TOLE_MACHINE=5.E-5,                  # TODO TOLE_MACHINE
                   ),
                _F(GROUP_NO='A2',
                   INST=1.0E-05,
                   RESULTAT=RESU1,
                   NOM_CHAM='VITE',
                   NOM_CMP='DY',
                   VALE_CALC=0.0,
                   CRITERE='ABSOLU',
                   ),
                ),
          )

#
### TESTS SUR LE CONTACT
#

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=0.0,
           NOM_PARA='JEU',
           TABLE=CONTB1_1,
           FILTRE=_F(PRECISION=1.E-10,
                     NOM_PARA='INST',
                     VALE=1.0E-05,),
           )

TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=289.96539297039,
           TOLE_MACHINE=5.E-5,                  # TODO TOLE_MACHINE
           NOM_PARA='R',
           TABLE=CONTB1_1,
           FILTRE=_F(PRECISION=2.5E-05,
                     NOM_PARA='INST',
                     VALE=1.0E-05,),
           )

#
### COURBES
#

F_DY_1=RECU_FONCTION( RESULTAT=RESU1,
                      NOM_CHAM='DEPL',
                      GROUP_NO='B1',
                      NOM_CMP='DZ',);
IMPR_FONCTION(FORMAT='XMGRACE',
          UNITE=39,
          COURBE=(
       _F(FONCTION=F_DY_1),),);



F_VY_1=RECU_FONCTION( RESULTAT=RESU1,
                      NOM_CHAM='VITE',
                      GROUP_NO='B1',
                      NOM_CMP='DZ',);
IMPR_FONCTION(FORMAT='XMGRACE',
          UNITE=40,
          COURBE=(
       _F(FONCTION=F_VY_1),),);



# TEST EN MODE DEFI_LIST_INST AUTOMATIQUE

DEF_AUTO = DEFI_LIST_INST(  DEFI_LIST=_F(LIST_INST = temps,
                                       METHODE='AUTO',),
                          ECHEC    =(
                            _F(
                               EVENEMENT        = 'ERREUR',
