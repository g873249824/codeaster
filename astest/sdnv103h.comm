                                OPERATION='EXTRACTION',),);

DEPLA1_2=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN A1',
                                GROUP_NO='A1',
                                RESULTAT=RESU2,
                                NOM_CHAM='DEPL',
