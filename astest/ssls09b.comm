DEBUT(CODE=_F(NOM='SSLS09B', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE()


#
# DEFINITION DU MODELE
MODELE=AFFE_MODELE(    MAILLAGE=MAIL,
                       AFFE=_F(GROUP_MA='SURF',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 2.1E11,  NU = 0.3,
                                 ALPHA = 1.E-5,  RHO = 8000.)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(    MAILLAGE=MAIL,
                          AFFE=_F(  TOUT = 'OUI',
                                 MATER = ACIER)  )

#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
CLIM=AFFE_CHAR_MECA(    MODELE=MODELE,
                           DDL_IMPO=_F(  GROUP_MA = 'APPUI',  DY = 0.)
                        )

#
F_Y=DEFI_FONCTION(    NOM_PARA='Y',
                       VALE=( 0.,   -78500.,
                              3.,   -78500.,
                              6.,   -78500., )
                    )

CHARGT=AFFE_CHAR_MECA_F(    MODELE=MODELE,
                           FORCE_INTERNE=_F(  TOUT = 'OUI',
                                           FY = F_Y)
                          )

#
# RESOLUTION
DEF=MECA_STATIQUE(    MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                        _F(  CHARGE = CLIM),
                              _F(  CHARGE = CHARGT))
                    )

#
# RECUPERATION DU CHAMP DE DEPLACEMENTS
U=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=DEF,
                 NUME_ORDRE=1
                  )

#
# CALCUL DES CONTRAINTES AUX NOEUDS

DEF=CALC_ELEM(reuse =DEF,


              RESULTAT=DEF,
              OPTION='SIGM_ELNO',

);
#
# TEST_RESU DEPLACEMENTS
#
TEST_RESU(CHAM_NO=(   _F(  CHAM_GD = U,
                        NOEUD = 'N452',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001),
              _F(  CHAM_GD = U,
                        NOEUD = 'N504',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001),
              _F(  CHAM_GD = U,
                        NOEUD = 'N503',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001))
          )

#
TEST_RESU(   CHAM_NO=_F(  CHAM_GD = U,
                        NOEUD = 'N51',
                        NOM_CMP = 'DX',
                        REFERENCE='ANALYTIQUE',
                        VALE = 4.49E-7,
                        PRECISION = 0.015)
          )

#
# TEST_RESU CONTRAINTES
#
TEST_RESU(RESU=(   _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N51',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIYY',
                          REFERENCE='ANALYTIQUE',
                          VALE = -3.14E5,
                          PRECISION = 0.0001),
              _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N1',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIXX',
                          REFERENCE='ANALYTIQUE',
                          VALE = 0.,
                          PRECISION = 18.0,
                          CRITERE = 'ABSOLU'),
              _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N101',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIXX',
                          REFERENCE='ANALYTIQUE',
                          VALE = 0.,
                          PRECISION = 36.0,
                          CRITERE = 'ABSOLU'),
              _F(  RESULTAT = DEF,
                          MAILLE = 'M4',
                          NOEUD = 'N51',
                          NUME_ORDRE=1,
                          NOM_CHAM='SIGM_ELNO',
                          NOM_CMP = 'SIXX',
                          REFERENCE='ANALYTIQUE',
                          VALE = 0.,
                          PRECISION = 27.0,
                          CRITERE = 'ABSOLU'))
          )
#--------------------------------------------------------------
#
CH_PESA=AFFE_CHAR_MECA  (  MODELE=MODELE,
                           PESANTEUR=_F(GRAVITE=9.8125,
                                        DIRECTION=(0., -1., 0.,),),
                          )
INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=1,),);
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0, 0.0,
                          1.0, 1.0,),);

DEF_PESA=STAT_NON_LINE(MODELE=MODELE,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(
                             CHARGE=CH_PESA,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=CLIM,),),
                   COMP_ELAS=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=INSTANTS),
                   NEWTON=_F(REAC_ITER=1,),
                    );

DEF_PESA=CALC_ELEM(reuse =DEF_PESA,
               RESULTAT=DEF_PESA,
               OPTION=('SIEF_ELNO',
                       'ERME_ELEM',
                       'ERME_ELNO',
                      ),
               );

IMPR_RESU(RESU=_F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELEM',),
          ) ;
#
# TEST_RESU DEPLACEMENTS
#
TEST_RESU(RESU=(_F(RESULTAT=DEF_PESA,
                        NUME_ORDRE=1,
                        NOM_CHAM='DEPL',
                        NOEUD = 'N452',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001),
               _F(RESULTAT=DEF_PESA,
                        NUME_ORDRE=1,
                        NOM_CHAM='DEPL',
                        NOEUD = 'N504',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001),
               _F(RESULTAT=DEF_PESA,
                        NUME_ORDRE=1,
                        NOM_CHAM='DEPL',
                        NOEUD = 'N503',
                        NOM_CMP = 'DY',
                        REFERENCE='ANALYTIQUE',
                        VALE = -2.99E-6,
                        PRECISION = 0.001)
              )
          )

#
TEST_RESU(RESU=(_F(RESULTAT=DEF_PESA,
                        NUME_ORDRE=1,
                        NOM_CHAM='DEPL',
                        NOEUD = 'N51',
                        NOM_CMP = 'DX',
                        REFERENCE='ANALYTIQUE',
                        VALE = 4.49E-7,
                        PRECISION = 0.015
                   )
                )
         )
