                   CRITERE='ABSOLU',
                   PRECISION=38.0,
                   MAILLE='M4',),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=DEF,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N51',
                   NOM_CMP='SIXX',
                   VALE_CALC=-29.061308653,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=29.100000000000001,
                   MAILLE='M4',),
                ),
          )

#--------------------------------------------------------------

CH_PESA=AFFE_CHAR_MECA(MODELE=MODELE,
                       PESANTEUR=_F(GRAVITE=9.8125,
                                    DIRECTION=(0.,-1.,0.,),),);

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                          1.0,1.0,
                          ),);

DEF_PESA=STAT_NON_LINE(MODELE=MODELE,
                       CHAM_MATER=CHMAT,
                       EXCIT=(_F(CHARGE=CH_PESA,
                                 FONC_MULT=RAMPE,),
                              _F(CHARGE=CLIM,),),
                       COMP_ELAS=_F(RELATION='ELAS',),
                       INCREMENT=_F(LIST_INST=INSTANTS,),
                       NEWTON=_F(REAC_ITER=1,),);

DEF_PESA=CALC_ERREUR(reuse =DEF_PESA,
                     RESULTAT=DEF_PESA,
                     OPTION=('ERME_ELEM','ERME_ELNO',),);

IMPR_RESU(RESU=_F(RESULTAT=DEF_PESA,
                  NOM_CHAM='ERME_ELEM',
                  NUME_ORDRE=1,),);

#
# TEST_RESU DEPLACEMENTS

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=DEF_PESA,
                   NOM_CHAM='DEPL',
                   NOEUD='N452',
                   NOM_CMP='DY',
                   VALE_CALC=-2.98935480E-06,
                   VALE_REFE=-2.9900000000000002E-06,
                   PRECISION=1.E-3,),
