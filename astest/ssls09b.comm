                   VERSION='9.03.15',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='TERMSA',
                   MAILLE='M7',
                   POINT=1,
                   VALE=2.28118E-07,
