#    TEST EN CONTRAINTES

TEST_RESU(RESU=(_F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIEF_ELNO',
                          MAILLE = 'M4',
                          NOEUD = 'N51',
                          NOM_CMP = 'SIYY',
                          REFERENCE='ANALYTIQUE',
                          VALE = -3.14E5,
                          PRECISION = 0.0001),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIEF_ELNO',
                          MAILLE = 'M4',
                          NOEUD = 'N1',
                          NOM_CMP = 'SIXX',
                          REFERENCE='ANALYTIQUE',
                          VALE = 0.,
