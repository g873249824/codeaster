                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.15',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='ERREST',
                   MAILLE='M4',
                   NOEUD='N51',
                   VALE=1.4510785718753E-06,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.15',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='TERMRE',
                   MAILLE='M7',
