                               CHAM_MATER=CH_MAT_1,
                               OPTION='RIGI_MECA' )

MACRO_MATR_ASSE(   MODELE=MODELE_1,     CARA_ELEM=CARAC__1,
                       CHARGE=CINE,         CHAM_MATER=CH_MAT_1,
                      NUME_DDL=CO("NUM"),MATR_ASSE=(
                _F(  MATRICE = CO("MATASSR1"),  OPTION = 'RIGI_MECA'),
