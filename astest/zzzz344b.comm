DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI',),
)

MAILLAGE = LIRE_MAILLAGE( FORMAT = 'MED',)


MODELE = AFFE_MODELE( MAILLAGE = MAILLAGE,
                       AFFE = (_F( GROUP_MA = ('JUPE',),
                                   PHENOMENE = 'MECANIQUE',
                                   MODELISATION = '3D',),
                               _F( GROUP_MA = 'H_5',
                                   PHENOMENE = 'MECANIQUE',
                                   MODELISATION = 'BARRE',),
                               ),
                      )

MATBETON = DEFI_MATERIAU( BPEL_BETON = _F(),
                          ELAS = _F( E = 34000.e6, NU = 0.2, RHO = 2500.,),
                         )

ACIER = DEFI_MATERIAU( ELAS = _F( E = 190000.e6, NU = 0.3, RHO = 7850., ),
                       BPEL_ACIER = _F( F_PRG = 1814.e6, FROT_COURB = 0.17, FROT_LINE = 0.0015, RELAX_1000 = 2.5, MU0_RELAX = 0.43,),
                      )

CHMAT = AFFE_MATERIAU( MAILLAGE = MAILLAGE,
                       AFFE = ( _F( MATER = MATBETON, GROUP_MA = 'JUPE'),
                                _F( MATER = ACIER, GROUP_MA = 'H_5'),
                               ),
                      )

CARELE = AFFE_CARA_ELEM( MODELE = MODELE,
                      BARRE = _F( GROUP_MA = 'H_5', SECTION = 'GENERALE', CARA = 'A', VALE = 4 * 139.e-6),
                     )

CABLE = DEFI_CABLE_BP( MODELE = MODELE,
                       CHAM_MATER = CHMAT,
                       CARA_ELEM = CARELE,
                       GROUP_MA_BETON = 'JUPE',
                       TYPE_ANCRAGE = ('ACTIF','ACTIF'),
                       TENSION_INIT = 4 * 139.e-6 * 1487 * 1e6,
                       RECUL_ANCRAGE = 8.e-3,
                       DEFI_CABLE = _F( GROUP_NO_ANCRAGE = ('H_5_D', 'H_5_F'), GROUP_MA = 'H_5',),
                      )

TABLE = RECU_TABLE(CO=CABLE,
                     NOM_TABLE='CABLE_BP',
                     )

IMPR_TABLE(UNITE=66,   TABLE=TABLE,
               #NOM_PARA=( 'NUME_CABLE','ABSC_CURV','ALPHA','TENSION'),
               PAGINATION='NUME_CABLE'
           )




TEST_TABLE(
           FILTRE =_F(NOM_PARA = 'NOEUD_CABLE',
                    VALE_K ='N1328',
                    ),
           REFERENCE = 'AUTRE_ASTER',
           VALE_CALC=6.532117547203,
           VALE_REFE=6.461066164316,
           NOM_PARA='ALPHA',
           PRECISION = 1.2E-2,
           TABLE=TABLE,)

FIN()
