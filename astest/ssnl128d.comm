#   VISC_IRRA_LOG DEPENDANT DE LA FLUENCE ET DE LA TEMPERATURE

DEBUT(CODE=_F(NIV_PUB_WEB='INTRANET'),DEBUG=_F(SDVERI='OUI'))

# Lecture du maillage
