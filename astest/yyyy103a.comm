                    CARA = 'VECT_Y', VALE = (1.,0.,0.,)),
            _F( GROUP_MA = 'EM',
                    CARA = 'ANGL_NAUT', VALE = (0.,0.,0.,)))
                                                                      )

L_INSTA=DEFI_LIST_REEL(   DEBUT=-0.1,INTERVALLE=(
                             _F( JUSQU_A = 0.0,   NOMBRE = 1),
                             _F( JUSQU_A = 0.250, NOMBRE = 50),
                             _F( JUSQU_A = 2.000, NOMBRE = 175))
