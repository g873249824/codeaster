                    MODELE=MO,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARA,
                    EXCIT=(
                           _F(  CHARGE = CHA1),
                           _F(  CHARGE = CHA2),
                           _F(  CHARGE = CHA4, FONC_MULT = F1),
                           _F(  CHARGE = CHA5, FONC_MULT = F2),
                           _F(  CHARGE = CHA6, FONC_MULT = F3)),
                    INCREMENT=  _F( LIST_INST = L_INSTA),
###CDURAND###                    ARCHIVAGE=  _F( LIST_ARCH = L_ARCHI),
                    HHT=        _F(),
                    ETAT_INIT=  _F( INST = 0.,
                                    EVOL_NOLI = RESU),
                    COMP_ELAS=  _F( GROUP_MA = ('CABLE',),
                                    RELATION = 'CABLE',
                                    DEFORMATION = 'GREEN'),
                    COMP_INCR=( _F( GROUP_MA = ('CHARP',),
                                    RELATION = 'ELAS'),
                                _F( GROUP_MA = ('EM',),
                                    RELATION = 'ELAS')),
                    CONVERGENCE=_F( RESI_GLOB_RELA = 1.E-6,
                                    ITER_GLOB_MAXI = 200),
                    NEWTON=     _F( REAC_ITER = 1),
                    SOLVEUR=    _F( METHODE = 'LDLT',
                                    RENUM = 'RCMK')
                 )

