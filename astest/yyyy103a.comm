                RELATION = 'ELAS')),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 500),
                            NEWTON=_F( 
                REAC_ITER = 1),
                           SOLVEUR=_F( 
                METHODE = 'LDLT',
                RENUM = 'RCMK')
