             CRITERE = 'ABSOLU',
             PARA = 'INST',             VALE = 0.00000E+00),
           _F(  RESULTAT = RESU,           NUME_ORDRE = 1,
             PARA = 'ITER_GLOB',        VALE_I = 0,     
             CRITERE = 'ABSOLU',         PRECISION = 180.),
           _F(  RESULTAT = RESU,           NUME_ORDRE = 1,
             CRITERE = 'ABSOLU',        
