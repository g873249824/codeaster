F1=DEFI_FONC_ELEC( #SIGNAL : 'CONTINU'
         COUR=_F(  INTE_CC_1 = 34600., TAU_CC_1 = 0.070, PHI_CC_1 = 20.,
                INTE_CC_2 = 34600., TAU_CC_2 = 0.070, PHI_CC_2 = 140.,
