DEBUT(CODE=_F(NOM='SSNL103A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#
#

MA1=LIRE_MAILLAGE(  )

MA1=DEFI_GROUP( reuse=MA1,   MAILLAGE=MA1,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO1=AFFE_MODELE(   MAILLAGE=MA1,
                        AFFE=_F( GROUP_MA = 'POUTRE', PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'POU_D_T_GD')
                        )

MAT=DEFI_MATERIAU(   ELAS=_F(  E = 1.,  NU = 0.5,  ALPHA = 0.)
                        )

CHMAT1=AFFE_MATERIAU(   MAILLAGE=MA1,
                          AFFE=_F(  TOUT = 'OUI',  MATER = MAT)
                        )

CHA1=AFFE_CHAR_MECA(   MODELE=MO1,DDL_IMPO=(
                          _F( GROUP_NO = 'POUTRE',   DZ = 0.),
                         _F( NOEUD = 'N1', DX = 0., DY = 0., DRX = 0., DRY = 0., DRZ = 0.))
                        )

CHA2=AFFE_CHAR_MECA(   MODELE=MO1,
                          FORCE_NODALE=_F( NOEUD = 'N6', MZ = 12.566)
                        )

CARA1=AFFE_CARA_ELEM(   MODELE=MO1,
                           POUTRE=_F(
       GROUP_MA = 'TOUT',
 SECTION = 'GENERALE',
                CARA = ( 'A',   'IY',    'IZ',    'AY',    'AZ',    'JX', ),
                VALE = (  1.,    2.,      2.,      0.25,    0.25,    4.,  ))
                        )

L_INST=DEFI_LIST_REEL(   DEBUT=0.0,
                            INTERVALLE=_F( JUSQU_A = 2., NOMBRE = 2)
                         )

RESU=STAT_NON_LINE(
                            MODELE=MO1,
                        CHAM_MATER=CHMAT1,
                         CARA_ELEM=CARA1,EXCIT=(
                             _F(
                CHARGE = CHA1), _F(
                CHARGE = CHA2)),
                         COMP_ELAS=_F(
                RELATION = 'ELAS_POUTRE_GR',
                DEFORMATION = 'GREEN_GR'),
                         INCREMENT=_F(
                LIST_INST = L_INST,
                NUME_INST_FIN = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-8,
                ITER_GLOB_MAXI = 20),
                            NEWTON=_F(
                REAC_ITER = 1)
              )

RESU=CALC_ELEM( reuse=RESU,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=RESU,
              )

#

TEST_RESU(RESU=(
           _F(  RESULTAT = RESU,           NUME_ORDRE = 1,
             PARA = 'INST',             VALE = 1.00000E+00),
           _F(  RESULTAT = RESU,           NUME_ORDRE = 1,
             PARA = 'ITER_GLOB',        VALE_I = 6,
             CRITERE = 'ABSOLU',         PRECISION = 2.),
           _F(  RESULTAT = RESU,           NUME_ORDRE = 1,
             CRITERE = 'ABSOLU',        PRECISION = 1.E-07,
             PARA = 'RESI_GLOB_RELA',   VALE = 0.),
           _F(  RESULTAT = RESU,           NUME_ORDRE = 1,
             CRITERE = 'ABSOLU',        PRECISION = 1.E-07,
             PARA = 'RESI_GLOB',        VALE = 0.),
           _F(  RESULTAT = RESU,           NUME_ORDRE = 1,
             CRITERE = 'ABSOLU',         PRECISION = 2.,
             PARA = 'ITER_LINE',        VALE_I = 0))
           )

#

TEST_RESU(RESU=(
           _F(  RESULTAT = RESU,        NUME_ORDRE = 1,      NOM_CHAM = 'DEPL',
             CRITERE = 'RELATIF',   PRECISION = 1.E-4,
             NOEUD = 'N3',          NOM_CMP = 'DX',    VALE = -0.3),
           _F(  RESULTAT = RESU,        NUME_ORDRE = 1,      NOM_CHAM = 'DEPL',
             CRITERE = 'RELATIF',   PRECISION = 1.E-4,
             NOEUD = 'N4',          NOM_CMP = 'DX',    VALE = -0.7),
           _F(  RESULTAT = RESU,        NUME_ORDRE = 1,      NOM_CHAM = 'DEPL',
             CRITERE = 'RELATIF',   PRECISION = 1.E-4,
             NOEUD = 'N6',          NOM_CMP = 'DX',    VALE = -1.))
           )

#

FIN()
#
