DEBUT(CODE=_F(NOM = 'SSNV128T',NIV_PUB_WEB='INTERNET'));

#......................................................................
#  CALCUL TEST USURE DE TYPE ARCHARD METHODE CONTINUE
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#......................................................................

MA=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'));

MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(GROUP_MA = 'LCONTAR'));

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'D_PLAN'));

MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=(_F(GROUP_MA = 'LPRESV',),
                               _F(GROUP_MA = 'LBATIR',),
                               _F(GROUP_MA = 'LCONTAR',),
                               _F(GROUP_MA = 'LPRESH')));

MATPLA=DEFI_MATERIAU(ELAS=_F(E = 2.1E11,
                             NU = 0.3,));

MATBAT=DEFI_MATERIAU(ELAS=_F(E = 1.E16,
                             NU = 0.0));

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA = 'SPLAQ',
                             MATER    = MATPLA,),
                          _F(GROUP_MA = 'SBATI',
                             MATER    = MATBAT)));

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA = 'LBATIX', DX = 0.0,),
                              _F(GROUP_MA = 'LENCAV', DX = 0.0,
                                                      DY = 0.0,),
                              _F(GROUP_NO = 'PPS',    DY = 0.0,),
                              _F(GROUP_MA = 'LBLOCX', DX = 0.0,)),
                    PRES_REP=(_F(GROUP_MA = 'LPRESV', PRES = 5.E07)));

CHA2=AFFE_CHAR_MECA(MODELE=MO,
                    PRES_REP=_F(GROUP_MA = 'LPRESH', PRES = 5.E07));

CHA4 = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    FROTTEMENT     = 'COULOMB',
                    REAC_FROT      = 'CONTROLE',
                    NB_ITER_FROT   = 4,
                    REAC_GEOM      = 'CONTROLE',
                    NB_ITER_GEOM   = 2,
                    ALGO_RESO_FROT = 'POINT_FIXE',
                    ZONE =(
                           _F(
                             GROUP_MA_MAIT = 'LBATIR',
                             GROUP_MA_ESCL = 'LCONTAR',
                             APPARIEMENT   = 'MAIT_ESCL',
                             NORMALE       = 'MAIT',
                             TOLE_PROJ_EXT = 1.0,
                             SANS_GROUP_NO = ('PPS',),
                             ALGO_CONT     = 'STANDARD',
                             COEF_CONT     = 1.0,
                             ALGO_FROT     = 'STANDARD',
                             COEF_FROT     = 1.E5,                             
                             COULOMB       = 1.0,
                             CONTACT_INIT  = 'INTERPENETRE',
                             SEUIL_INIT    = 0.0,
                             USURE         = 'ARCHARD',
                             K             = 1.0E-12,
                             H             = 1.0,
                             ),                            
                          ),
                   );


RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0));

LINST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 100));
L_INST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                  LIST_INST=LINST,),
                     ECHEC=_F(EVENEMENT='ERREUR',
                              ACTION='DECOUPE',
                              SUBD_METHODE = 'MANUEL',
                              SUBD_PAS=5,
                              SUBD_NIVEAU=5,),);

F2=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,  0.0,
                      0.01, 1,
                      0.02, 0.0,
                      0.03, -1,
                      0.04, 0.0));

#-----------------------------------------------------------

U4M=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE    = CHA1,
                          FONC_MULT = RAMPE),
                       _F(CHARGE    = CHA2,
                          FONC_MULT = F2),
                       ),
                CONTACT  = CHA4,       
                COMP_ELAS=_F(RELATION = 'ELAS'),
                NEWTON=_F(REAC_ITER=1),
                INCREMENT=_F(LIST_INST = L_INST,
                             INST_FIN = 0.04),
                CONVERGENCE=_F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 20,
                               RESI_GLOB_RELA = 1.0E-5),
                INFO=1);
  
# TEST DE NON REGRESSION

TEST_RESU(RESU=(_F(RESULTAT   = U4M,
                   INST       = 0.04,
                   NOM_CHAM   = 'DEPL',
                   NOEUD      = 'N2',
                   NOM_CMP    = 'LAGS_C',
                   VALE       = -9.4259506515042E+05,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '11.0.33',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 0.04,
                   NOM_CHAM   = 'DEPL',
                   NOEUD      = 'N4',
                   NOM_CMP    = 'LAGS_C',
                   VALE       = -1.7553533917221E+06,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '11.0.33',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 0.04,
                   NOM_CHAM   = 'DEPL',
                             NOEUD      = 'N10',
                   NOM_CMP    = 'LAGS_C',
                   VALE       = -1.9839906523786E+06,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '11.0.33',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 0.04,
                   NOM_CHAM   = 'DEPL',
                   NOEUD      = 'N30',
                   NOM_CMP    = 'LAGS_C',
                   VALE       = -2.31615618917E+06,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '11.0.33',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 0.04,
                   NOM_CHAM   = 'DEPL',
                   NOEUD      = 'N32',
                   NOM_CMP    = 'LAGS_C',
                   VALE       = -1.3426590279068E+07,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '11.0.33',
                   )));


PRFUS4=POST_RELEVE_T(ACTION=_F(INTITULE='USURE',
                             GROUP_NO='LCONTAR',
                             RESULTAT=U4M,
                             NOM_CHAM='VALE_CONT',
                             NUME_ORDRE=4,
                             NOM_CMP='HN',
                             OPERATION='EXTRACTION'));

LAGSC4=POST_RELEVE_T(ACTION=_F(INTITULE='LAMBDA',
                             GROUP_NO='LCONTAR',
                             RESULTAT=U4M,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=4,
                             NOM_CMP='LAGS_C',
                             OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=PRFUS4);
IMPR_TABLE(TABLE=LAGSC4);

FIN();
