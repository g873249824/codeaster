DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

M=LIRE_MAILLAGE(FORMAT='MED',);

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

E=DEFI_FONCTION(NOM_PARA='SECH',
                VALE=( 80.,  3.E10,
                      100. , 1.E10),)

NU=DEFI_FONCTION(NOM_PARA='HYDR',
                VALE=(  0.,  0.15,
                        1,   0.25),)

ALPHA=DEFI_CONSTANTE(VALE= 1.E-5)

EPSD0=DEFI_CONSTANTE(VALE=1.E-4,);

AT=DEFI_FONCTION(NOM_PARA='SECH',
                VALE=( 80., 1.,
                      100., 0.8),)

BT=DEFI_FONCTION(NOM_PARA='HYDR',
                VALE=(  0., 10000.,
                        1., 11000.),)

AC   =DEFI_CONSTANTE(VALE = 1.4)

BC   =DEFI_CONSTANTE(VALE = 2000.)

K   =DEFI_CONSTANTE(VALE = 0.7)
Z   =DEFI_CONSTANTE(VALE = 0.)

KAPA=DEFI_CONSTANTE(VALE=1.66E-5)
BETA=DEFI_CONSTANTE(VALE=1.50E-5)

BETON=DEFI_MATERIAU(ELAS_FO=_F(E=E,
                               NU=NU,
                               K_DESSIC =1.66E-5,
                               B_ENDOGE = 1.5E-5,
                               TEMP_DEF_ALPHA = 20.,
                               ALPHA=ALPHA ),
                    MAZARS_FO  =_F(K = K,
                                   EPSD0= EPSD0,
                                   AC   = AC,
                                   AT   = AT,
                                   BC   = BC,
                                   BT   = BT),)

MATFRONT=DEFI_MATERIAU(
                    ELAS_FO=_F(E=E,
                               NU=NU,
                               K_DESSIC =1.66E-5,
                               B_ENDOGE = 1.5E-5,
                               TEMP_DEF_ALPHA = 20.,
                               ALPHA=ALPHA ),
                    Mazars_FO=_F(
                                 YoungModulus = E,
                                 PoissonRatio = NU,
                                 Ac = AC,
                                 At = AT,
                                 Bc = BC,
                                 Bt = BT,
                                 k = K,
                                 ed0 = EPSD0,
                    )
                       );


SECHI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=100.),);

SECHF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=80.),);

SECH1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
               _F(CHAM_GD=SECHI,
                   INST=0.0,),
                _F(CHAM_GD=SECHF,
                   INST=3600.0,),   ),);

TEMPI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=20.0,),);

TEMPF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=120.0,),);

TEMP1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
                _F(CHAM_GD=TEMPI,
                   INST=0.0,),
                _F(CHAM_GD=TEMPF,
                   INST=3600.0,  ),),);

HYDRI=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=0.0,),);

HYDRF=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=1.0,),);

HYDR1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='HYDR_NOEU',
                AFFE=(
                _F(CHAM_GD=HYDRI,
                   INST=0.0,),
                _F(CHAM_GD=HYDRF,
                   INST=3600.0,  ),),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=BETON,),
                 AFFE_VARC=(_F(TOUT='OUI',EVOL=HYDR1,NOM_VARC='HYDR',NOM_CHAM='HYDR_NOEU'),
                            _F(TOUT='OUI',EVOL=SECH1,NOM_VARC='SECH',NOM_CHAM='TEMP',VALE_REF=100),
                            _F(TOUT='OUI',EVOL=TEMP1,NOM_VARC='TEMP',NOM_CHAM='TEMP',VALE_REF=20),),);


CH1=AFFE_CHAR_MECA(MODELE=MO,
                   FACE_IMPO=_F(GROUP_MA='D1',
                                DY=0.0,),);

#  ATTENTION :METTRE  SUFFISAMMENT DE PAS DE TEMPS QUAND E VARIE AVEC LE
#  SECHAGE SINON RESULTATS APPROXIMATIFS OU PROBLEME DE CONVERGENCE
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3600.0,
                                     NOMBRE=20,),)

U=STAT_NON_LINE( MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=_F(CHARGE=CH1,),
                COMPORTEMENT=_F(RELATION='MAZARS'),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=1e-06,
                               ITER_GLOB_MAXI=25,),);
U=CALC_CHAMP(reuse=U,RESULTAT=U,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO','EPSP_ELNO'))


U=CALC_CHAMP(reuse =U,
             RESULTAT = U,

             DEFORMATION=('EPSI_NOEU','EPSP_NOEU',),CONTRAINTE='SIGM_NOEU',VARI_INTERNE='VARI_NOEU')

TEST_RESU(RESU=(_F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.000653,
                   VALE_REFE=6.5300000000000004E-4,
                   PRECISION=5.0000000000000001E-3,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPYY',
                   VALE_CALC=0.000653,
                   VALE_REFE=6.5300000000000004E-4,
                   PRECISION=5.0000000000000001E-3,),
          ))

CMF=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=MATFRONT,),
                 AFFE_VARC=(_F(TOUT='OUI',EVOL=HYDR1,NOM_VARC='HYDR',NOM_CHAM='HYDR_NOEU'),
                            _F(TOUT='OUI',EVOL=SECH1,NOM_VARC='SECH',NOM_CHAM='TEMP',VALE_REF=100),
                            _F(TOUT='OUI',EVOL=TEMP1,NOM_VARC='TEMP',NOM_CHAM='TEMP',VALE_REF=20),),);

MF=STAT_NON_LINE( MODELE=MO,
                CHAM_MATER=CMF,
                EXCIT=_F(CHARGE=CH1,),
                COMPORTEMENT=_F(RELATION='Mazars',
                                RESI_INTE_RELA=1.e-8,
                                  ),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='ELASTIQUE',),
                CONVERGENCE=_F(RESI_GLOB_MAXI=1e-06,
                               ITER_GLOB_MAXI=25,),);
MF=CALC_CHAMP(reuse=MF,RESULTAT=MF,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO','EPSP_ELNO'))


MF=CALC_CHAMP(reuse =MF,
             RESULTAT = MF,

             DEFORMATION=('EPSI_NOEU','EPSP_NOEU',),CONTRAINTE='SIGM_NOEU',VARI_INTERNE='VARI_NOEU')

TEST_RESU(RESU=(_F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MF,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.000653000003038,
                   VALE_REFE=6.5300000000000004E-4,
                   PRECISION=5.0000000000000001E-3,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MF,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPYY',
                   VALE_CALC=0.000653000003038,
                   VALE_REFE=6.5300000000000004E-4,
                   PRECISION=5.0000000000000001E-3,),
                ),
          )


FIN();
