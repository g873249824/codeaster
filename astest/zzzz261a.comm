# person_in_charge: jacques.pellet at edf.fr
DEBUT(  CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#-------------------------------------------------------------------------------------
# description du test :
#----------------------
#  On lit 2 maillages (MA1 et MA2) correspondants au carre [0,1]X[0,1].
#  Leurs discretisations sont differentes.
#
#  On cree sur MA1 un evol_ther "analytique" grace a une formule de temps et de l'espace.
#
#  On projette ensuite cet evol_ther sur MA2 avec les 3 methodes de projection :
#    -'COLLOCATION'
#    -'NUAGE_DEG_0'
#    -'NUAGE_DEG_1'
#
#  On verifie que la temperature projetee au point (0.5, 0.5) est correcte.
#-------------------------------------------------------------------------------------

MA1=LIRE_MAILLAGE(FORMAT='MED',UNITE=19 )

MA2=LIRE_MAILLAGE(FORMAT='MED',UNITE=18 )


# Creation de l'evol_ther "analytique" (TEMP1):
#------------------------------------------------
LINST=DEFI_LIST_REEL( DEBUT=0.,INTERVALLE=  _F( JUSQU_A = 10.,   NOMBRE = 3))

TFONC = FORMULE(NOM_PARA=('X','Y','INST'),VALE=' INST*(1. + 2.*(X-0.5)**2 +3.*(Y-0.25)**2)')

CHFONC=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_F', MAILLAGE=MA1,
              AFFE=_F(  TOUT = 'OUI', NOM_CMP = ('TEMP', ), VALE_F = (TFONC,)))

TEMP1=CREA_RESU(    OPERATION='AFFE', TYPE_RESU='EVOL_THER',  NOM_CHAM='TEMP',
                           AFFE=_F(  LIST_INST = LINST, CHAM_GD = CHFONC))

CHAREF=CREA_CHAMP(MAILLAGE=MA2, OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', INFO=1,
                    AFFE= _F(TOUT='OUI', NOM_CMP='TEMP', VALE=0.0))


# Validation de METHODE='COLLOCATION' :
#--------------------------------
TEMP2=PROJ_CHAMP(RESULTAT=TEMP1,METHODE='COLLOCATION',  MAILLAGE_1=MA1, MAILLAGE_2=MA2, NOM_CHAM='TEMP')
TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP2,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=12.029320988,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

# Validation de METHODE='COLLOCATION' en 2 temps :
#-------------------------------------------------
MATPROJ=PROJ_CHAMP(METHODE='COLLOCATION',  MAILLAGE_1=MA1, MAILLAGE_2=MA2, PROJECTION='NON')
TEMP2B=PROJ_CHAMP(RESULTAT=TEMP1, MATR_PROJECTION=MATPROJ, NOM_CHAM='TEMP')

TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP2B,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=12.029320988,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

# Validation de METHODE='NUAGE_DEG_0' :
#---------------------------------------
TEMP3=PROJ_CHAMP(RESULTAT=TEMP1,METHODE='NUAGE_DEG_0',  MAILLAGE_1=MA1, MAILLAGE_2=MA2, NOM_CHAM='TEMP',  CHAM_NO_REFE=CHAREF)
TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP3,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=12.029809257,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

# Validation de METHODE='NUAGE_DEG_1' :
#---------------------------------------
TEMP4=PROJ_CHAMP(RESULTAT=TEMP1,METHODE='NUAGE_DEG_1',  MAILLAGE_1=MA1, MAILLAGE_2=MA2, NOM_CHAM='TEMP',  CHAM_NO_REFE=CHAREF)
TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP4,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=12.029809257,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

#-------------------------------------------------------------------------------------------------------------------------------
# Validation de differents mots cles :
#-------------------------------------

# VIS_A_VIS/ TOUT_1/2 :
#----------------------
TEMP5=PROJ_CHAMP(RESULTAT=TEMP1,METHODE='COLLOCATION',  MAILLAGE_1=MA1,MAILLAGE_2=MA2, NOM_CHAM='TEMP',
                 VIS_A_VIS=_F(TOUT_1='OUI',TOUT_2='OUI', CAS_FIGURE='2D'))
TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP5,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=12.029320988,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

TEMP6=PROJ_CHAMP(RESULTAT=TEMP1,METHODE='NUAGE_DEG_0',  MAILLAGE_1=MA1,MAILLAGE_2=MA2, NOM_CHAM='TEMP', CHAM_NO_REFE=CHAREF,
                 VIS_A_VIS=_F(TOUT_1='OUI',TOUT_2='OUI', CAS_FIGURE='2D'))
TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP6,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=12.029809257,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

# VIS_A_VIS/ GROUP_MA_1/2 :
#--------------------------
TEMP7=PROJ_CHAMP(RESULTAT=TEMP1,METHODE='COLLOCATION',  MAILLAGE_1=MA1,MAILLAGE_2=MA2, NOM_CHAM='TEMP',
                 VIS_A_VIS=_F(GROUP_MA_1='ABCD',GROUP_MA_2='EFGH', CAS_FIGURE='2D'))
TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP7,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=12.029320988,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

TEMP8=PROJ_CHAMP(RESULTAT=TEMP1,METHODE='NUAGE_DEG_0',  MAILLAGE_1=MA1,MAILLAGE_2=MA2, NOM_CHAM='TEMP', CHAM_NO_REFE=CHAREF,
                 VIS_A_VIS=_F(GROUP_MA_1='ABCD',GROUP_MA_2='EFGH', CAS_FIGURE='2D'))
TEST_RESU(RESU=_F(NUME_ORDRE=4,
                  GROUP_NO='O1',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=TEMP8,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=11.943310870,
                  VALE_REFE=11.875,
                  PRECISION=0.014999999999999999,),
          )

# CHAM_NO isole + METHODE= 'COLLOCATION' / 'NUAGE_DEG_0/1':
#-----------------------------------------------------
CHT1=CREA_CHAMP(OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R', RESULTAT=TEMP1, NOM_CHAM = 'TEMP', NUME_ORDRE=4,)

CHT2=PROJ_CHAMP(CHAM_GD=CHT1,METHODE='COLLOCATION',  MAILLAGE_1=MA1, MAILLAGE_2=MA2)
TEST_RESU(CHAM_NO=_F(REFERENCE='ANALYTIQUE',
                     NOM_CMP='TEMP',
                     GROUP_NO='O1',
                     PRECISION=0.014999999999999999,
                     CHAM_GD=CHT2,
                     VALE_CALC=12.029320988,
                     VALE_REFE=11.875,),
          )

CHT3=PROJ_CHAMP(CHAM_GD=CHT1,METHODE='NUAGE_DEG_0',  MAILLAGE_1=MA1, MAILLAGE_2=MA2, CHAM_NO_REFE=CHAREF)
TEST_RESU(CHAM_NO=_F(REFERENCE='ANALYTIQUE',
                     NOM_CMP='TEMP',
                     GROUP_NO='O1',
                     PRECISION=0.014999999999999999,
                     CHAM_GD=CHT3,
                     VALE_CALC=12.029809257,
                     VALE_REFE=11.875,),
          )

CHT4=PROJ_CHAMP(CHAM_GD=CHT1,METHODE='NUAGE_DEG_1',  MAILLAGE_1=MA1, MAILLAGE_2=MA2, CHAM_NO_REFE=CHAREF)
TEST_RESU(CHAM_NO=_F(REFERENCE='ANALYTIQUE',
                     NOM_CMP='TEMP',
                     GROUP_NO='O1',
                     PRECISION=0.014999999999999999,
                     CHAM_GD=CHT4,
                     VALE_CALC=12.029809257,
                     VALE_REFE=11.875,),
          )

#-----------------------------------------------------------------------------------------------------------
# Suite issue23085 : Comment creer plusieurs POI1 sur les memes noeuds :
#-----------------------------------------------------------------------------------------------------------

# 1) On cree le groupe de mailles GMAB1 contenant autant de mailles POI1 qu'il y a de noeuds dans les
#    groupes A et B.
#    Les mailles POI1 crees ont le meme nom que les noeuds designes.
MA1_1=CREA_MAILLAGE(MAILLAGE=MA1,
                    CREA_POI1 = _F(NOM_GROUP_MA = 'GMAB1',   GROUP_NO = ('A','B'),))

# 2) Pour creer un autre groupe de mailles GMAB2 contenant de nouvelles mailles POI1 sur les memes noeuds :
MA1_2=CREA_MAILLAGE(MAILLAGE=MA1_1,
                 CREA_GROUP_MA=_F(NOM='GMAB2', GROUP_MA='GMAB1', PREF_MAILLE='S',))
IMPR_RESU(RESU=_F(MAILLAGE=MA1_2), UNITE=6)


FIN();
