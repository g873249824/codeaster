                                        FY=-5000.0,),
                                     _F(GROUP_MA='BORD_CH1',
                                        FX=MCOS30E4,
                                        FY=5000.0,),),);
#
# RESOLUTION

DEF=MECA_STATIQUE(MODELE=MODELE,
                  CHAM_MATER=CHMAT,
                  EXCIT=_F(CHARGE=CHARGT,),);
#
# RECUPERATION DU CHAMP DE DEPLACEMENTS

U=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
             OPERATION='EXTR',
             RESULTAT=DEF,
             NOM_CHAM='DEPL',
             NUME_ORDRE=1,);
