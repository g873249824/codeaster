def Pcdesat(PR,SR,n,S) :
      m=1.-1./n
      usn=1./n
      usm=1./m
      S1=(S-SR)/(1.-SR)
      PC=PR*((S1**(-usm)-1.e0)**(usn))
      return(PC)

