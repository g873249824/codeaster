#CAS_TEST__: TTLL01A
# CAS TEST TTLL01A

DEBUT(CODE=_F(NOM='TTLL01A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#
#  CAS 2D AVEC CHOC SOUS FORME DE RAMPE :
#         PAS DE DISCONTINUITE
#


TIMPVAR=DEFI_FONCTION(
       NOM_RESU='TEMP',  NOM_PARA='INST',
       VALE=(0.0E0,    0.0E0,
            1.0E-3,  100.0E0,
            100.,    100.0E0, )
      )

ACIER=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 1.0))

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = 'PLAN',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(MODELE=MOTH,
          TEMP_IMPO=_F( GROUP_NO = 'GRNM14', TEMP = TIMPVAR)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,INTERVALLE=(
       _F(    JUSQU_A = 1.E-3, NOMBRE = 10),
       _F(    JUSQU_A = 1.E-2, NOMBRE = 9),
       _F(    JUSQU_A = 1.E-1, NOMBRE = 9),
       _F(    JUSQU_A = 1.E0,  NOMBRE = 9),
       _F(    JUSQU_A = 2.0,   NOMBRE = 10))
                       )

F=DEFI_CONSTANTE(    VALE=1. )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
          INCREMENT=_F( LIST_INST = LR8, INST_FIN = 2.E-2),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH,  FONC_MULT = F)
             )

LIS=DEFI_LIST_REEL(  VALE=( 0.1,  0.2,  0.7,  2., )  )

TEMPE=THER_LINEAIRE(reuse=TEMPE,        MODELE=MOTH,
                              ETAT_INIT=_F(  EVOL_THER = TEMPE,
                                          INST = 2.E-2),
                              INCREMENT=_F(  LIST_INST = LR8),
                              CHAM_MATER=CHMAT,
                                  EXCIT=_F(  CHARGE = CHTH),
                              ARCHIVAGE=_F(  LIST_INST = LIS)
             )

TEMP_1=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=0.1 )

TEMP_2=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=0.2 )

TEMP_3=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=0.7 )

TEMP_4=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=2.0 )

TEST_RESU(CHAM_NO=(
   # VALEURS DE REFERENCE:
  _F( CHAM_GD = TEMP_1, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 65.48, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_2, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 75.58, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_3, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 93.01, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_4, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 99.72, PRECISION = 1.E-2),

  _F( CHAM_GD = TEMP_1, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 8.09, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_2, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 26.37, PRECISION = 3.E-2),
  _F( CHAM_GD = TEMP_3, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 78.47, PRECISION = 1.E-2),
  _F( CHAM_GD = TEMP_4, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 99.13, PRECISION = 1.E-2),
   # VALEURS TROUVEES JUSQU'A PRESENT:
_F( CHAM_GD = TEMP_1, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 65.294,  PRECISION = 1.E-4),
_F( CHAM_GD = TEMP_2, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 75.814,  PRECISION = 1.E-4),
_F( CHAM_GD = TEMP_3, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 92.867,  PRECISION = 1.E-4),
_F( CHAM_GD = TEMP_4, NOEUD = 'N9', NOM_CMP = 'TEMP', VALE = 99.700,  PRECISION = 1.E-4),

_F( CHAM_GD = TEMP_1, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 8.0356, PRECISION = 1.E-4),
_F( CHAM_GD = TEMP_2, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 25.790, PRECISION = 1.E-4),
_F( CHAM_GD = TEMP_3, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 78.047, PRECISION = 1.E-4),
_F( CHAM_GD = TEMP_4, NOEUD = 'N33', NOM_CMP = 'TEMP', VALE = 99.076, PRECISION = 1.E-4))
    )

#--------------------------------------------------------------------------
# TEST DE L'OPTION MASS_THER SOUS LA COMMANDE CALC_MATR_ELEM


MASSTHER=CALC_MATR_ELEM( MODELE=MOTH,
                       CHAM_MATER=CHMAT,
                       OPTION='MASS_THER', )

TEST_RESU(OBJET=_F(NOM='MASSTHER.ME001     .RESL',
                   S_R=9.2222E-1,
                   REFERENCE = 'NON_REGRESSION',
                   VERSION='9.3.21'),)

FIN()

