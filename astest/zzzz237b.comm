                  VALE_REFE=-4.28571,
                  VALE_CALC=-4.2857143183057,
                  PRECISION=1.E-3,
                  REFERENCE='AUTRE_ASTER',
                  ),
          )

TEST_RESU(RESU=_F(GROUP_NO='B',
                  INST=20.0,
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_REFE=4.28571,
                  VALE_CALC=4.2857142743473,
                  PRECISION=1.E-3,
                  REFERENCE='AUTRE_ASTER',
                  ),
          )

TEST_RESU(RESU=_F(GROUP_NO='A',
                  INST=20.0,
                  RESULTAT=RESU,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_REFE=2.1978000000E10,
                  VALE_CALC=2.1978021987206E10,
                  PRECISION=1.E-3,
                  REFERENCE='AUTRE_ASTER',
                  ),
          )

TEST_RESU(RESU=_F(GROUP_NO='B',
                  INST=20.0,
                  RESULTAT=RESU,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_REFE=2.1978000000E10,
                  VALE_CALC=2.1978021987206E10,
                  PRECISION=1.E-3,
                  REFERENCE='AUTRE_ASTER',
                  ),
          )

#--------------------------

IMPR_RESU(FORMAT='MED',
          RESU=_F(MAILLAGE=MA,
                  RESULTAT=RESU,
                  TOUT='OUI',),);

FIN();
