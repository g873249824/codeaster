                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=RESU,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_CALC= 2.19780220E+10,
                  VALE_REFE=2.1978000000E10,),
          )

TEST_RESU(RESU=_F(GROUP_NO='B',
                  INST=20.0,
                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=RESU,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_CALC= 2.19780220E+10,
                  VALE_REFE=2.1978000000E10,),
          )

# NON REGRESSION
# --------------

TEST_RESU(RESU=_F(GROUP_NO='A',
                  INST=20.0,
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
