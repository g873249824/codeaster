DEBUT(CODE=_F(NOM='ZZZZ237B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',);

# IMPR_RESU(FORMAT='ASTER',
#           UNITE=21,
#           RESU=_F(MAILLAGE=MA,),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=_F(GROUP_MA=('AB','BC','CD','DA','EF','FG','GH','HE',),),);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MAT_DUR=DEFI_MATERIAU(ELAS=_F(E=2.0e+11,
                              NU=0.3,),);

# PAS UTILISE DANS CETTE MODELISATION (ON MODELISE LA CONDITION DE CONTACT
# PAR UNE LIAISON_UNILATER)
MAT_MOU=DEFI_MATERIAU(ELAS=_F(E=2.0e+9,
                              NU=0.3,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='ABCD',
                             MATER=MAT_MOU,),
                          _F(GROUP_MA='EFGH',
                             MATER=MAT_DUR,),
                             ),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(GROUP_MA=('ABCD',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN',),
                     _F(GROUP_MA=('K1','K2',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='2D_DIS_T',),),);

CAR_DIS=AFFE_CARA_ELEM(MODELE=MO,
                       DISCRET_2D=_F(REPERE='LOCAL',
                                     CARA='K_T_D_L',
                                     GROUP_MA=('K1','K2',),
                                     VALE=(10,10,),),);

BLOC_DIS=AFFE_CHAR_CINE(MODELE=MO,
                        MECA_IMPO=_F(GROUP_NO=('R1','R2',),
                                    DX=0,
                                    DY=0,
                                    #DZ=0,
                                    ),);

DEPL=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=_F(GROUP_NO='CD',
                                DY=-1.0,),);


F_IMPO = DEFI_CONSTANTE( VALE = 0.0 )
F_MULT = DEFI_CONSTANTE( VALE = -1.0 )

CONT  = DEFI_CONTACT(MODELE      = MO,
                     FORMULATION = 'LIAISON_UNIL',
                     ZONE        = (
                       _F(
                         GROUP_NO      = ('AB',),
                         NOM_CMP       = ('DY',),
                         COEF_IMPO     = F_IMPO,
                         COEF_MULT     = F_MULT,
                         ),
                     ),
                     )

F_FORC=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                           1.0,1.0,
                           #10.0,10.0,
                           #20.0,0.0,
                           ),PROL_DROITE='LINEAIRE');

L_TPS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=20.0,
                                   NOMBRE=20,),);

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CAR_DIS,
                   EXCIT=(
                          _F(CHARGE=DEPL,
                             FONC_MULT=F_FORC,),
                          _F(CHARGE=BLOC_DIS,),
                          ),
                   CONTACT = CONT,
                   COMP_INCR=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=L_TPS,),
                   #CONVERGENCE=_F(
                   #RESI_GLOB_MAXI=1.0E-06,
                   #),
                   SOLVEUR=_F(SYME='OUI',),
                   INFO=1,
                   );

RESU=CALC_CHAMP(reuse =RESU,
             RESULTAT=RESU,
             FORCE=('FORC_NODA','REAC_NODA'),
             );

T_DEP_HA=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='Deplacements ABCD',
                             RESULTAT=RESU,
                             NOM_CHAM='DEPL',
                             INST=20.0,
                             GROUP_NO='ABCD',
                             TOUT_CMP='OUI',),);

IMPR_TABLE(TABLE=T_DEP_HA,);

T_FN_HAU=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='Forces nodales ABCD',
                             RESULTAT=RESU,
                             NOM_CHAM='FORC_NODA',
                             INST=20.0,
                             GROUP_NO='ABCD',
                             TOUT_CMP='OUI',),);

IMPR_TABLE(TABLE=T_FN_HAU,);


