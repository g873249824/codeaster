DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',);

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

E=DEFI_CONSTANTE(VALE= 3.E10)

NU=DEFI_CONSTANTE(VALE= 0.2)

ALPHA=DEFI_CONSTANTE(VALE= 1.E-5)

FONC1=DEFI_FONCTION( NOM_PARA='SECH',
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 100.0, 1.0,)
                   )

BETON=DEFI_MATERIAU(ELAS_FO=_F(E=E,
                               NU=NU,
                               K_DESSIC=1.66E-5,
                               B_ENDOGE = 1.5E-5,
                               ALPHA=ALPHA,
                               TEMP_DEF_ALPHA=20,
                               FONC_DESORP = FONC1,),
                    BETON_ECRO_LINE=_F( SYT = 4.E6,
                                        D_SIGM_EPSI = -10.E9,
                                        SYC = 53.4E6),
                   BETON_UMLV_FP=_F( K_RS   = 1.E25,
                                     ETA_RS = 1.E25,
                                     K_IS   = 1.E25,
                                     ETA_IS = 1.E25,
                                     K_RD   = 1.E25,
                                     ETA_RD = 1.E25,
                                     ETA_ID = 1.E25 ,)
                       )

SECHI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=100.),);

SECHF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=80.),);

SECH1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
               _F(CHAM_GD=SECHI,
                   INST=0.0,),
                _F(CHAM_GD=SECHF,
                   INST=3600.0,),   ),);

TEMPI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=20.0,),);

TEMPF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=120.0,),);

TEMP1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
                _F(CHAM_GD=TEMPI,
                   INST=0.0,),
                 _F(CHAM_GD=TEMPF,
                   INST=3600.0,),   ),);

HYDRI=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=0.0,),);

HYDRF=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=1.0,),);

HYDR1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='HYDR_NOEU',
                AFFE=(
                _F(CHAM_GD=HYDRI,
                   INST=0.0,),
                _F(CHAM_GD=HYDRF,
                   INST=3600.0,  ),),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=BETON,),
                 AFFE_VARC=(_F(TOUT='OUI',EVOL=HYDR1,NOM_VARC='HYDR',NOM_CHAM='HYDR_NOEU'),
                            _F(TOUT='OUI',EVOL=SECH1,NOM_VARC='SECH',NOM_CHAM='TEMP',VALE_REF=100),
                            _F(TOUT='OUI',EVOL=TEMP1,NOM_VARC='TEMP',NOM_CHAM='TEMP',VALE_REF=20),),);

CH1=AFFE_CHAR_MECA(MODELE=MO,
                   FACE_IMPO=_F(GROUP_MA='D1',
                                DY=0.0,),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3600.0,
                                     NOMBRE=2,  ),);

U=STAT_NON_LINE( MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=_F(CHARGE=CH1,),
                COMPORTEMENT=_F(RELATION = 'KIT_DDI',
                             RELATION_KIT=('BETON_UMLV_FP','ENDO_ISOT_BETON'),
                             ITER_INTE_MAXI=100,),
                 INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=1e-06,
                               ITER_GLOB_MAXI=25,),);
U=CALC_CHAMP(reuse=U,RESULTAT=U,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO','EPSP_ELNO'))


U=CALC_CHAMP(reuse =U,
             RESULTAT = U,
             DEFORMATION=('EPSI_NOEU','EPSP_NOEU'),CONTRAINTE='SIGM_NOEU',VARI_INTERNE='VARI_NOEU')

TEST_RESU(RESU=(_F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPXX',
                   VALE_CALC= 6.53000000E-04,
                   VALE_REFE=6.5300000000000004E-4,
                   PRECISION=5.0000000000000001E-3,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSI_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPYY',
                   VALE_CALC= 6.53000000E-04,
                   VALE_REFE=6.5300000000000004E-4,
                   PRECISION=5.0000000000000001E-3,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='N5',
                   NOM_CMP='SIXX',
                   VALE_CALC= 1.29874064E-07,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_NOEU',
                   NOEUD='N5',
                   NOM_CMP='SIYY',
                   VALE_CALC= 1.58979663E-07,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSP_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPXX',
                   VALE_CALC= 1.67358573E-18,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSP_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPYY',
                   VALE_CALC= 1.87529492E-18,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='VARI_NOEU',
                   NOEUD='N5',
                   NOM_CMP='V22',
                   VALE_CALC= 0.00000000E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                ),
          )

FIN();
