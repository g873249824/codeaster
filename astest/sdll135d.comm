# MODELISATION TUYAU_6M

DEBUT(CODE=_F(NOM='SDLL135D',
              NIV_PUB_WEB='INTERNET',),);

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='TUYAU_6M',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=200000E6,
                            NU=0.29,
                            RHO=7830.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

CARAC=AFFE_CARA_ELEM(MODELE=MODELE,
                     POUTRE=_F(GROUP_MA='TOUT',
                               SECTION='CERCLE',
                               CARA=('R','EP',),
                               VALE=(0.16,0.01,),),);

LISTE=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=2.e-4,
                                   PAS=1e-07,),);

LISTE2=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.2e-4,
                                    PAS=2e-07,),);

# L ENCASTREMENT A L EXTREMITE NE CONCERNE QUE LES
# DDL DE POUTRE POUR POUVOIR COMPARER LES VALEURS
# ANALYTIQUES DE TYPE POUTRE


CL=AFFE_CHAR_MECA(MODELE=MODELE,
                  DDL_IMPO=_F(GROUP_NO='A',
                              DX=0.0,
                              DY=0.0,
                              DZ=0.0,
                              DRX=0.0,
                              DRY=0.0,
                              DRZ=0.0),);


# CHARGEMENT DE TYPE HEAVYSIDE

CHARFX=AFFE_CHAR_MECA(MODELE=MODELE,
                      FORCE_NODALE=_F(GROUP_NO='B',
                                      FX=1.0,),);

CHARMX=AFFE_CHAR_MECA(MODELE=MODELE,
                      FORCE_NODALE=_F(GROUP_NO='B',
                                      MX=1.0,),);

MACRO_MATR_ASSE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARAC,
                CHARGE=CL,
                NUME_DDL=CO('NUMEDDL'),
                MATR_ASSE=(_F(MATRICE=CO('MASSE'),
                              OPTION='MASS_MECA',),
                           _F(MATRICE=CO('RIGID'),
                              OPTION='RIGI_MECA',),),);

# POUR MINIMISER LE TEMPS CPU SEUL LES CALCULS
# PAR DYNA_VIBRA SONT REALISES ( PAS DE DYNA_NON_LINE )

DYNATRAF=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='PHYS',
                       MODELE=MODELE,
                       CHAM_MATER=CHMAT,
                       CARA_ELEM=CARAC,
                       MATR_MASS=MASSE,
                       MATR_RIGI=RIGID,
                       EXCIT=(_F(CHARGE=CHARFX,),
                              _F(CHARGE=CL,),),
                       SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',),
                       ARCHIVAGE=_F(PAS_ARCH=100),
                       INCREMENT=_F(LIST_INST=LISTE,),);


DYNATRAF=CALC_CHAMP(reuse=DYNATRAF,GROUP_MA=('MAILF','MAILI'),RESULTAT=DYNATRAF,CONTRAINTE=('SIEF_ELGA','EFGE_ELNO'))


DYNATRAF=CALC_CHAMP(reuse =DYNATRAF,
                    RESULTAT=DYNATRAF,
                    FORCE='REAC_NODA',
                    MODELE=MODELE,
                    EXCIT=(_F(CHARGE=CHARFX,),
                           _F(CHARGE=CL,),),
                    CHAM_MATER=CHMAT,
                    GROUP_MA=('MAILF','MAILI'),
                    CARA_ELEM=CARAC,);

# TEST SUR LES DEPLACEMENTS

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                 NUME_ORDRE=10,
                 NOM_CHAM='DEPL',
                 NOM_CMP='DX',
                 GROUP_NO='B',
                 PRECISION=0.002,
                 REFERENCE='ANALYTIQUE',
                 VALE=2.59473227925e-10,),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                 NUME_ORDRE=15,
                 NOM_CHAM='DEPL',
                 NOM_CMP='DX',
                 GROUP_NO='B',
                 PRECISION=0.01,
                 REFERENCE='ANALYTIQUE',
                 VALE=3.89209841887e-10,),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                 NUME_ORDRE=20,
                 NOM_CHAM='DEPL',
                 NOM_CMP='DX',
                 PRECISION=0.003,
                 GROUP_NO='B',
                 REFERENCE='ANALYTIQUE',
                 VALE=5.18946455848e-10,),);


# TEST SUR LES REACTIONS NODALES

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                 NUME_ORDRE=10,
                 NOM_CHAM='REAC_NODA',
                 NOM_CMP='DX',
                 GROUP_NO='A',
                 CRITERE='ABSOLU',
                 REFERENCE='ANALYTIQUE',
                 VALE=0.,),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                 NUME_ORDRE=15,
                 NOM_CHAM='REAC_NODA',
                 NOM_CMP='DX',
                 GROUP_NO='A',
                 CRITERE='ABSOLU',
                 REFERENCE='ANALYTIQUE',
                 VALE=0.,),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                 NUME_ORDRE=20,
                 NOM_CHAM='REAC_NODA',
                 NOM_CMP='DX',
                 GROUP_NO='A',
                 PRECISION=0.07,
                 REFERENCE='ANALYTIQUE',
                 VALE=-1.99999819062,),);

# CE TEST VALIDE LA CONTRIBUTION DE M_GAMMA DANS LES REACTIONS NODALES

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                 NUME_ORDRE=20,
                 NOM_CHAM='REAC_NODA',
                 NOM_CMP='DX',
                 GROUP_NO='B',
                 PRECISION=1E-3,
                 CRITERE='ABSOLU',
                 REFERENCE='ANALYTIQUE',
                 VALE=0.,),);


MODE_TRA=MODE_ITER_INV(MATR_A=RIGID,
                  MATR_B=MASSE,
                  CALC_FREQ=_F(
                  OPTION='PROCHE',
                  FREQ=(1263.497,3790.49,6317.484,8844.4477),),);

TEST_RESU(RESU=_F(RESULTAT=MODE_TRA,
                  PARA='FREQ',
                  NUME_ORDRE=1,
                  PRECISION=0.003,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=1263.497),);

# FREQUENCES DES MODES DE TRACTIONS-COMPRESSIONS ( COMPARAISON AVEC DES POUTRES )

TEST_RESU(RESU=_F(RESULTAT=MODE_TRA,
                  PARA='FREQ',
                  NUME_ORDRE=2,
                  PRECISION=0.003,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=3790.49,),);

TEST_RESU(RESU=_F(RESULTAT=MODE_TRA,
                  PARA='FREQ',
                  NUME_ORDRE=3,
                  PRECISION=0.003,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=6317.484),);

TEST_RESU(RESU=_F(RESULTAT=MODE_TRA,
                  PARA='FREQ',
                  NUME_ORDRE=4,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=8844.4477),);

# FREQUENCES DES MODES DE FLEXIONS ( COMPARAISON AVEC POU_D_T )

MODE_FLE=MODE_ITER_INV(MATR_A=RIGID,
                  MATR_B=MASSE,
                  CALC_FREQ=_F(
                  OPTION='PROCHE',
                  FREQ=(269.932,1077.199,2270.705,3249.207,4649.212),),);

TEST_RESU(RESU=_F(RESULTAT=MODE_FLE,
                  PARA='FREQ',
                  NUME_ORDRE=1,
                  PRECISION=0.03,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=269.932),);

TEST_RESU(RESU=_F(RESULTAT=MODE_FLE,
                  PARA='FREQ',
                  NUME_ORDRE=2,
                  PRECISION=0.05,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=1077.199,),);

TEST_RESU(RESU=_F(RESULTAT=MODE_FLE,
                  PARA='FREQ',
                  NUME_ORDRE=3,
                  PRECISION=0.04,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=2270.705),);

TEST_RESU(RESU=_F(RESULTAT=MODE_FLE,
                  PARA='FREQ',
                  NUME_ORDRE=4,
                  PRECISION=0.02,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=3249.207),);

TEST_RESU(RESU=_F(RESULTAT=MODE_FLE,
                  PARA='FREQ',
                  NUME_ORDRE=5,
                  PRECISION=0.005,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',
                  VALE=4649.212),);



MODE_TOR=MODE_ITER_INV(MATR_A=RIGID,
                  MATR_B=MASSE,
                  CALC_FREQ=_F(
                  OPTION='PROCHE',
                  FREQ=(786.619,2359.856,3933.094,5506.331),),);

# FREQUENCES DES MODES DE TORSION ( COMPARAISON AVEC DES POUTRES )

TEST_RESU(RESU=_F(RESULTAT=MODE_TOR,
                 PARA='FREQ',
                 NUME_ORDRE=1,
                 CRITERE='RELATIF',
                 REFERENCE='ANALYTIQUE',
                 VALE=786.619),);

TEST_RESU(RESU=_F(RESULTAT=MODE_TOR,
                 PARA='FREQ',
                 NUME_ORDRE=2,
                 CRITERE='RELATIF',
                 REFERENCE='ANALYTIQUE',
                 VALE=2359.856,),);

TEST_RESU(RESU=_F(RESULTAT=MODE_TOR,
                 PARA='FREQ',
                 NUME_ORDRE=3,
                 CRITERE='RELATIF',
                 REFERENCE='ANALYTIQUE',
                 VALE=3933.094,),);

TEST_RESU(RESU=_F(RESULTAT=MODE_TOR,
                 PARA='FREQ',
                 NUME_ORDRE=4,
                 CRITERE='RELATIF',
                 REFERENCE='ANALYTIQUE',
                 VALE=5506.331),);

# CALCUL DYNAMIQUE AVEC APPLICATION D UNE TORSION

DYNATRAM=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='PHYS',
                        MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                        CARA_ELEM=CARAC,
                        MATR_MASS=MASSE,
                        MATR_RIGI=RIGID,
                        SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',),
                        EXCIT=(_F(CHARGE=CHARMX,),
                               _F(CHARGE=CL,),),
                        INCREMENT=_F(LIST_INST=LISTE2,),
                        ARCHIVAGE=_F(PAS_ARCH=50,),);

DYNATRAM=CALC_CHAMP(reuse=DYNATRAM,GROUP_MA=('MAILF','MAILI'),RESULTAT=DYNATRAM,CONTRAINTE=('SIEF_ELGA','EFGE_ELNO'))


DYNATRAM=CALC_CHAMP(reuse =DYNATRAM,
                    RESULTAT=DYNATRAM,
                    FORCE=('REAC_NODA','FORC_NODA'),
                    MODELE=MODELE,
                    GROUP_MA=('MAILF','MAILI'),
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARAC,
                    EXCIT=(_F(CHARGE=CHARMX,),
                           _F(CHARGE=CL,),),);



# TEST SUR LES DEPLACEMENTS

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=5,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DRX',
                  GROUP_NO='B',
                  VALE=8.66477582461e-09,
                  REFERENCE='ANALYTIQUE',
                  PRECISION=0.002,),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=10,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DRX',
                  PRECISION=0.0015,
                  GROUP_NO='B',
                  REFERENCE='ANALYTIQUE',
                  VALE=1.73295916526e-08,),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=20,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DRX',
                  GROUP_NO='B',
                  REFERENCE='ANALYTIQUE',
                  VALE=3.46591033075e-08,),);

# TEST SUR LES REACTIONS NODALES

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=10,
                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DRX',
                  GROUP_NO='A',
                  VALE=0.0,
                  REFERENCE='ANALYTIQUE',
                  CRITERE='ABSOLU',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=15,
                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DRX',
                  GROUP_NO='A',
                  VALE=0.0,
                  REFERENCE='ANALYTIQUE',
                  CRITERE='ABSOLU',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=20,
                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DRX',
                  GROUP_NO='A',
                  VALE=0.0,
                  CRITERE='ABSOLU',
                  PRECISION=0.05,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=32,
                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DRX',
                  GROUP_NO='A',
                  VALE=-2.00000001424,
                  PRECISION=0.05,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAM,
                  NUME_ORDRE=32,
                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DRX',
                  GROUP_NO='B',
                  VALE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1E-5,
                  REFERENCE='ANALYTIQUE',),);

FIN();
