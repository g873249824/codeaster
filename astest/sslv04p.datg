OPTI DIME 2 ELEM QUA8 ;
OPTI SAUV FORM 'sslv04p.mgib' ;
*
O = .0 0. ;
A = .1 0. ;
B = .2 0. ;
C = 9.2387974262238E-02  3.8268297910690E-02 ;
D = 1.8477588891983E-01  7.6536655426025E-02 ;
E = 7.0710659027100E-02  7.0710659027100E-02 ;
F = 1.4142125844955E-01  1.4142125844955E-01 ;

* initial
* den1 = 0.01 ;
* den2 = 0.02 ;
* nel = 15 ;
den1 = 0.05 ;
den2 = 0.1 ;
nel = 8 ;
mnel = (-2)*nel ;

*
* maillage en quad8
*
*AB = DROIT mnel A B DINI den1 DFIN den2 ;
AB = DROIT nel A B ;
BD = CERCLE B O D nel;
*DC = DROIT mnel D C DINI den2 DFIN den1 ;
DC = DROIT nel D C ;
CA = CERCLE C O A nel;
S1 = DALLER AB BD DC CA ;
TRAC S1;
*
* maillage en tria6
*
OPTI DIME 2 ELEM TRI6 ;
* CD = DROIT mnel C D DINI den1 DFIN den2 ;
CD = DROIT nel C D ;
DF = CERCLE D O F nel;
* FE = DROIT mnel F E DINI den2 DFIN den1 ;
FE = DROIT nel F E  ;
EC = CERCLE E O C nel;
S2 = DALLER CD DF FE EC ;
TRAC S2;
S = S1 ET S2 ;
AE = EC ET CA ;
ELIM 0.0001 AE;
ELIM 0.0001 S;
NNOAE = NBNO AE;
LIST NNOAE;
NNO = NBNO S;
LIST NNO;
TRAC S;
*
* extrusion en 3D
*
OPTI DIME 3 ELEM CU20 ;
volu = s volu 1 tran (0. 0. 0.01);
oeil = 1. 1. 1. ;
trac oeil volu;
sb1 = s coul roug ;
