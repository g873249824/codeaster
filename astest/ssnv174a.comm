DEBUT(CODE=_F(NOM='SSNV174A',NIV_PUB_WEB='INTERNET'),);

PRE_GIBI()

M=LIRE_MAILLAGE( )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'S_INF')
           )
           
M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'S_SUP')
            )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'S_GCH')
            )
            
M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'S_DRT')
            )

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'S_AVT')
            )            

M=DEFI_GROUP( reuse=M,
              MAILLAGE=M,
              CREA_GROUP_NO=_F( GROUP_MA = 'S_ARR')
            )

MO=AFFE_MODELE( MAILLAGE=M,
                AFFE=_F( TOUT = 'OUI',
                         PHENOMENE = 'MECANIQUE',
                         MODELISATION = '3D')
              )

FONC1=DEFI_FONCTION( NOM_PARA='SECH',
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 100.0, 1.0,),
                   )

E=DEFI_FONCTION( NOM_PARA='TEMP',
                 PROL_DROITE='LINEAIRE',
                 PROL_GAUCHE='LINEAIRE',
                 VALE=(0.0, 31000., 1.0, 31000.,)
               )

NU=DEFI_FONCTION( NOM_PARA='TEMP',
                  PROL_DROITE='LINEAIRE',
                  PROL_GAUCHE='LINEAIRE',
                  VALE=(0.0,0.2,1.0,0.2,)
                ),

BETON=DEFI_MATERIAU( ELAS_FO =_F( E = E,
                                  NU = NU ,
                                  K_DESSIC = 10.0E-6,
                                  B_ENDOGE = 60.0E-6,
                                  FONC_DESORP = FONC1, ),
                     BETON_UMLV_FP =_F( K_RS   = 1.20E+05,
                                        ETA_RS = 2.21E+10,
                                        K_IS   = 6.22E+04,
                                        ETA_IS = 4.16E+10,
                                        K_RD   = 3.86E+04,
                                        ETA_RD = 6.19E+10,
                                        ETA_ID = 1.64e+12,
                                      ),
                  );

CM=AFFE_MATERIAU(  MAILLAGE=M,   
                   AFFE=_F( TOUT = 'OUI',
                            MATER = BETON,
                            TEMP_REF = 20.,
                            SECH_REF = 100.,) )

SECHI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=100.),);

SECHF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=50.),);

SECH1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
               _F(CHAM_GD=SECHI,
                   INST=0.0,),
                _F(CHAM_GD=SECHF,
                   INST=64800000.0,),   ),);

HYDRI=CREA_CHAMP(TYPE_CHAM='ELGA_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=0.0,),);

HYDRF=CREA_CHAMP(TYPE_CHAM='ELGA_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=1.0,),);

HYDR1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='HYDR_ELGA',
                AFFE=(
                _F(CHAM_GD=HYDRI,
                   INST=0.0,),
                _F(CHAM_GD=HYDRF,
                   INST=2419200.0,  ),),);

TEMP=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', MAILLAGE=M,
                         AFFE=_F( 
                               TOUT = 'OUI',
                               NOM_CMP = 'TEMP',
                               VALE = 20.)
                       )

FORCE=DEFI_FONCTION( NOM_PARA='INST',
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',
                     VALE=(0.0, 0.0, 1., -3.0, 64800000., -3.0,)
                   )

CH10=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'S_INF',
                                  DZ       = 0.
                               )
                    )
CH11=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'S_GCH',
                                  DX       = 0.
                                )
                   )
                   
CH12=AFFE_CHAR_MECA( MODELE=MO,
                     DDL_IMPO=_F( GROUP_NO = 'S_AVT',
                                 DY       = 0.
                               ),
                  ) 

CH22=AFFE_CHAR_MECA_F( MODELE=MO,
                      FORCE_NODALE=_F( GROUP_NO = 'S_SUP',
                                   FZ = FORCE),
                    )

CH3=AFFE_CHAR_MECA( MODELE        = MO,
                    TEMP_CALCULEE = TEMP,
                    SECH_CALCULEE = SECH1,
                    HYDR_CALCULEE = HYDR1,
                  )

L_INST=DEFI_LIST_REEL( DEBUT=-1.0,
                       INTERVALLE=(_F( JUSQU_A = 1.0, NOMBRE = 2),
                                   _F( JUSQU_A = 64800.0, NOMBRE = 20),
                                   _F( JUSQU_A = 648000.0, NOMBRE = 20),
                                   _F( JUSQU_A = 6480000.0, NOMBRE = 20),
                                   _F( JUSQU_A = 64800000.0, NOMBRE = 20),),
                      );

U=STAT_NON_LINE( MODELE=MO,
                 CHAM_MATER=CM,EXCIT=( _F(CHARGE = CH10),
                                       _F(CHARGE = CH11),
                                       _F(CHARGE = CH12),
                                       _F(CHARGE = CH22),
                                       _F(CHARGE = CH3),
                                      ),
                 COMP_INCR=_F(RELATION = 'BETON_UMLV_FP'),
                 INCREMENT=_F(LIST_INST = L_INST,
                              NUME_INST_FIN = 82),
                 NEWTON=_F(PREDICTION='ELASTIQUE',
                           MATRICE = 'TANGENTE',
                           REAC_ITER = 1),
                 CONVERGENCE=_F(RESI_GLOB_MAXI = 1.E-8,
                                ITER_GLOB_MAXI = 25,
                                ITER_INTE_MAXI = 1000)
              )

U=CALC_ELEM(reuse=U,   MODELE=MO,      RESULTAT=U,
             CHAM_MATER=CM,   OPTION='EPSI_ELNO_DEPL')

TEST_RESU(RESU=(
      _F( RESULTAT = U,
          NOM_CHAM = 'EPSI_ELNO_DEPL',
          MAILLE = 'M1',
          NOEUD = 'N6',
          INST = 0.,
          NOM_CMP = 'EPZZ',
          VALE = 0.0E+0,
          PRECISION = 0.005,
          REFERENCE = 'ANALYTIQUE'),
      _F( RESULTAT = U,
          NOM_CHAM = 'EPSI_ELNO_DEPL',
          MAILLE = 'M1',
          NOEUD = 'N6',
          INST = 64800.,
          NOM_CMP = 'EPZZ',
          VALE = -4.072E-04,
          PRECISION = 0.005,
          REFERENCE = 'ANALYTIQUE'),
      _F( RESULTAT = U,
          NOM_CHAM = 'EPSI_ELNO_DEPL',
          MAILLE = 'M1',
          NOEUD = 'N6',
          INST = 648000.,
          NOM_CMP = 'EPZZ',
          VALE =-5.157E-04,
          PRECISION = 0.005,
          REFERENCE = 'ANALYTIQUE'),
      _F( RESULTAT = U,
          NOM_CHAM = 'EPSI_ELNO_DEPL',
          MAILLE = 'M1',
          NOEUD = 'N6',
          INST = 6480000.,
          NOM_CMP = 'EPZZ',
          VALE = -8.134E-04,
          PRECISION = 0.005,
          REFERENCE = 'ANALYTIQUE'),
      _F( RESULTAT = U,
          NOM_CHAM = 'EPSI_ELNO_DEPL',
          MAILLE = 'M1',
          NOEUD = 'N6',
          INST = 64800000.,
          NOM_CMP = 'EPZZ',
          VALE = -1.3669E-03,
          PRECISION = 0.005,
          REFERENCE = 'ANALYTIQUE'),
           )),
FIN()
