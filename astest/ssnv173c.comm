DEBUT(CODE=_F(NOM='SSNV173C', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),
                                   );

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                        LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='VOLQUAD',
                                     GROUP_MA='VOL',),
                              );

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=_F(GROUP_MA='VOL',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='3D_XFEM',),
                     );

delta=0.02

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-X+Y+Z-5.*delta');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-100. ');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    DEFI_FISS=_F(
                                 FONC_LT=LT,
                                 FONC_LN=LN,
                                 ),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI=(2.5 ,0. ,2.5 ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(-0.5,0.,1.5 ),),
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         INFO=1,);

E=100.E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA='VOL',
                                MATER=ACIER,
                                TEMP_REF=0.0,),);

DEPZ=1.E-6

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(NOEUD=('N21','N22','N23','N24',),
                                DCX=0.0,
                                DCY=0.0,
                                DCZ=0.0,),
                             _F(NOEUD=('N13','N14','N15','N16',),
                                DCX=0.0,
                                DCY=0.0,
                                DCZ=DEPZ,),
                                ),
                  INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                                INFO=1,);

#   nombre de pas de chargement
nbpc = 1

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=100.0,
                                    NOMBRE=nbpc,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CH1,),
                          _F(CHARGE=CHXFEM,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                  CONVERGENCE=_F(ITER_GLOB_MAXI=8,
                                 RESI_GLOB_MAXI=1e-8,),
                   SOLVEUR=_F(METHODE='MUMPS',
                              TYPE_RESOL='SYMGEN',

                              RESI_RELA=3e-2,
                             ),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=2,);

DEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                             GROUP_NO='VOL',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP=('DCX','H1X','DCY','H1Y','DCZ','H1Z',),
                             OPERATION='EXTRACTION',),);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                             GROUP_NO='VOLQUAD',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2',),
                             OPERATION='EXTRACTION',),);

#IMPR_TABLE(TABLE=DEP,);
#IMPR_TABLE(TABLE=LAG,);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=0.E0,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MAX',
           VALE=0.E0,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEP,
           FILTRE=(_F(NOM_PARA='NOEUD',
                      VALE_K='N1',),
                   ),
           NOM_PARA='DCZ',
           VALE=5.E-7,
           PRECISION=2.E-5,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEP,
           FILTRE=(_F(NOM_PARA='NOEUD',
                      VALE_K='N1',),
                   ),
           NOM_PARA='H1Z',
           VALE=5.E-7,
           PRECISION=2.E-5,
           REFERENCE='ANALYTIQUE',);

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'),
                        INFO          = 2)
 
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));

FIN();
