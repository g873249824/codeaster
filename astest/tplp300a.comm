                           TEMP_IMPO=_F(  GROUP_NO = 'NO_TIMP',  TEMP = 100.),
                           ECHANGE=_F(  GROUP_MA = 'MA_CONV',  COEF_H = 750.,
                                                      TEMP_EXT = 0.)
                        )

#
# DEFINITION DE LA STRATEGIE DE CALCUL ---------------------------------
#

#
# RESOLUTION -----------------------------------------------------------
#


TEMPE=THER_LINEAIRE(   MODELE=MOTH,
                            CHAM_MATER=CHMAT,
                            EXCIT=_F(  CHARGE = CHTH)
                         )

#
