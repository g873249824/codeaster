DEBUT(CODE=_F( NOM = 'TTLL301B',NIV_PUB_WEB='INTERNET')
      )

#
# LECTURE DU MAILLAGE ----------------------------------------------
#

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ------------------------
#

TIMPO=DEFI_FONCTION(  
       NOM_RESU='TEMP',  NOM_PARA='INST',
       VALE=( 0.E0,     0.E0,
             40.E0,     0.E0,)
            )

SI = FORMULE(NOM_PARA='INST',VALE='100.*sin(pi*INST/40.)')

TFIN=40.E0

DPAS=0.5E0

LIS1=DEFI_LIST_REEL(    DEBUT=0.,
                          INTERVALLE=_F( JUSQU_A = TFIN, PAS = DPAS))

TSINUS=CALC_FONC_INTERP(   FONCTION=SI,  LIST_PARA=LIS1,
NOM_PARA='INST',
                                NOM_RESU='TSIN',
#                                INTERPOL='INT' )
                                INTERPOL='LIN' )

MATE=DEFI_MATERIAU( THER=_F(  LAMBDA = 35.0, RHO_CP = 3.1716E6))

# AFFECTATIONS: MATERIAU, MODELE, CHARGEMENT ----------------------


CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                 AFFE=_F(  TOUT = 'OUI', MATER = MATE) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
               AFFE=_F( TOUT = 'OUI', MODELISATION = '3D',
                                   PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(MODELE=MOTH,TEMP_IMPO=(
          _F( GROUP_NO = 'NOE_GAUC', TEMP = TIMPO),
          _F( GROUP_NO = 'NOE_DROI', TEMP = TSINUS))
         )

# DEFINITION DE LA STATEGIE DE CALCUL -----------------------------


LR8=DEFI_LIST_REEL(        DEBUT=0.,INTERVALLE=(
       _F(    JUSQU_A = 1.0,   NOMBRE = 5),
       _F(    JUSQU_A = 10.0,   NOMBRE = 18),
       _F(    JUSQU_A = 20.0,   NOMBRE = 20),
       _F(    JUSQU_A = 30.0,   NOMBRE = 20),
       _F(    JUSQU_A = 35.0,   NOMBRE = 10))
                  )

# RESOLUTION ------------------------------------------------------


TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           ETAT_INIT=_F( VALE = 0.E0),
           INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH)
             )

# ANALYSE DES RESULTATS -------------------------------------------

TEMP_32=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
            INST=32.0 )

TEST_RESU(CHAM_NO=(_F(NOEUD='N113',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.558353197,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N114',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.602052793,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N115',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.470240244,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N116',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.470240244,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N117',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.602052793,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N118',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.470240244,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N119',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.602052793,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   _F(NOEUD='N120',
                      NOM_CMP='TEMP',
                      PRECISION=0.02,
                      CHAM_GD=TEMP_32,
                      VALE_CALC=36.558353197,
                      VALE_REFE=36.600000000000001,
                      REFERENCE='NON_DEFINI',),
                   ),
          )

FIN()
