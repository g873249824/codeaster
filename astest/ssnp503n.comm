# CONTACT-FROTTANT EN GRANDS GLISSEMENTS 2D
# AVEC XFEM POUR DES FISSURES HORIZONTALES
# INTRODUCTION DE FONDS DE FISSURES

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

typma=1

if (typma==1):
  MAILLAG2=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
  Y=3

if (typma==2):
  MAILLAG1=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
  MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='SURF'));
  Y=2.5

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,Y),
                                       VECT_NORMALE = (0.0,1.0),
                                       PRECISION = 1E-1)))

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('SURF'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN')));

LN1=FORMULE(NOM_PARA=('X','Y'),VALE='Y-2.5');
LT1=FORMULE(NOM_PARA=('X','Y'),VALE='X-2.5');


LN2=FORMULE(NOM_PARA=('X','Y'),VALE='Y-6.5');
LT2=FORMULE(NOM_PARA=('X','Y'),VALE='X-3');

FISS1=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                     DEFI_FISS=_F(FONC_LT=LT1,FONC_LN=LN1),
                     INFO=1);

FISS2=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                     DEFI_FISS=_F(FONC_LT=LT2,FONC_LN=LN2),
                     INFO=1);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='STANDARD',
                         INFO=1);

CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      REAC_GEOM      = 'AUTOMATIQUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 10,
                      ITER_GEOM_MAXI = 10,
                      FROTTEMENT     = 'COULOMB',ELIM_ARETE     = 'ELIM',
                      ZONE = (_F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',
                                 TOLE_PROJ_EXT = 1.,
                                 FISS_MAIT     = FISS1,
                                 ALGO_LAGR     = 'VERSION1',
                              ),
                             _F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',
                                 TOLE_PROJ_EXT = 1.,
                                 FISS_MAIT     = FISS2,
                                 ALGO_LAGR     = 'VERSION1',
                              ),
                            )
                    );


E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER));

DEPX=2.E-1
DEPY=1.E-3
ZERO=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIGINF',
                                          DY=DEPY),
                             _F(GROUP_MA='LIGSUP',
                                          DY=-DEPY),
                             _F(GROUP_NO=('P1','P2','P3','P4'),
                                DX=0.0)),
                   INFO=1);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(NOEUD=('N21','N22','N23','N24'),
                                DX=DEPX,
                                ),
                             ),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                   LIAISON_DDL=(_F(NOEUD=('N20','N20'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,-2),
                                   COEF_IMPO=DEPX),
                                _F(NOEUD=('N25','N25'),
                                   DDL=('DX','H1X'),
                                   COEF_MULT=(1,2),
                                   COEF_IMPO=DEPX),
                                ),
                    INFO=1);

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    CONTACT = CTXFEM,
                    EXCIT=(
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2)),
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 GROUP_MA='SURF'),
                    INCREMENT=_F(LIST_INST=L_INST),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06
                                    )),
                    NEWTON=_F(REAC_ITER=1),
                    SOLVEUR=_F(METHODE='MUMPS',

                               NPREC=-1),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                    INFO=1);

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]

vale = [[-2.1285803197164E+05,-6.6645436452751E+04,-2.8138419026723E+04],
           [-4.2571785666887E+05,-1.3329045824943E+05,-5.6276612406346E+04],
           [-6.3857768137259E+05,-1.9993548004459E+05,-8.4414805785151E+04],
           [-8.5143750607631E+05,-2.6658050183975E+05,-1.1255299916396E+05]]

VALE_CALC=[
[ -2.1009495738549E+05,
 -66862.543578197,
 -27860.804028639,],
[ -4.1513714913183E+05,
 -1.3400647562898E+05,
 -55015.531055894,],
[ -6.1473074063196E+05,
 -2.0123665662603E+05,
 -81426.196959959,],
[ -8.0852276085475E+05,
 -2.683549418492E+05,
 -1.070674793633E+05,],
]

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO=('NFISSU'),
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1'),
                                         OPERATION='EXTRACTION'));
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              NOM_PARA=('NOEUD','INST','COOR_X','LAGS_C','LAGS_F1'),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X'),
                     ORDRE='CROISSANT'));

   for j in [0,1,2]: #Boucle sur les positions testees
      precision = 6E-2*inst[i]
      critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE_CALC=VALE_CALC[i-1][j], VALE_REFE=vale[i-1][j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU'));

FIN();
