DEBUT(CODE=_F( NOM = 'SSNA115A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'), PAR_LOT='NON')

tfin = 20
nbpas = 150
nbarch = 30
cmultpilo = 10.

PRE_GMSH()

MA_0  =  LIRE_MAILLAGE()

MA_0  =  DEFI_GROUP(
  reuse  = MA_0, 
  MAILLAGE = MA_0, 
  
  CREA_GROUP_MA = (
    _F(NOM = 'HAUT',      GROUP_MA = 'GM1'), 
    _F(NOM = 'BAS',       GROUP_MA = 'GM3'),
    _F(NOM = 'DROITE',    GROUP_MA = 'GM4'),     
    _F(NOM = 'J_G',       GROUP_MA = 'GM5'),
    _F(NOM = 'J_D',       GROUP_MA = 'GM2'),
    _F(NOM = 'J_H',       GROUP_MA = 'GM7'),
    _F(NOM = 'J_B',       GROUP_MA = 'GM6'),
    _F(NOM = 'MATRICE',   GROUP_MA = 'GM8'),
    ), 
     
   CREA_GROUP_NO = (
    _F(NOM = 'NO_JHD',   GROUP_MA = 'GM10'),
    _F(NOM = 'NO_JBD',   GROUP_MA = 'GM11'),
    _F(NOM = 'NO_JMD',   GROUP_MA = 'GM12'),
    _F(NOM = 'NO_JHG',   GROUP_MA = 'GM13'),
    _F(NOM = 'NO_JBG',   GROUP_MA = 'GM14'),
    _F(
       NOM = 'GN_J_D',   
       GROUP_MA = 'GM2',
       OPTION = 'NOEUD_ORDO',
       GROUP_NO_ORIG = 'NO_JBD',
       GROUP_NO_EXTR = 'NO_JHD',),    
    _F(
       NOM = 'GN_J_G',   
       GROUP_MA = 'GM5',
       OPTION = 'NOEUD_ORDO',
       GROUP_NO_ORIG = 'NO_JBG',
       GROUP_NO_EXTR = 'NO_JHG',),     
    )       
  )

            
MA = CREA_MAILLAGE(
  MAILLAGE = MA_0,
  CREA_FISS = _F(
    NOM = 'JOINT',
    GROUP_NO_2 = 'GN_J_D',
    GROUP_NO_1 = 'GN_J_G',
    PREF_MAILLE = 'MJ',
    )
  )

IMPR_RESU(
  FORMAT  = 'RESULTAT',
  RESU=_F(MAILLAGE=MA)
         ) 


BETON = DEFI_MATERIAU(
          ELAS      = _F (
                          E  = 1.5 , 
                          NU = 0.,
                          ),
          RUPT_FRAG = _F(
                          GC = 0.9,
                          SIGM_C = 1.1,
                         )          
     )
  
CHMAT = AFFE_MATERIAU(
          MAILLAGE = MA,
          AFFE = (
       _F(
          GROUP_MA = 'MATRICE',
          MATER    =  BETON),
       _F(
          GROUP_MA = 'JOINT',
          MATER    = BETON),
        ))

MO = AFFE_MODELE(
  MAILLAGE = MA,
  AFFE = (
    _F(
      GROUP_MA     = 'MATRICE', 
      PHENOMENE    = 'MECANIQUE', 
      MODELISATION = 'AXIS'
      ),
    _F(
      GROUP_MA     = 'JOINT',
      PHENOMENE    = 'MECANIQUE', 
      MODELISATION = 'AXIS_ELDI'
      ),      
    )
  )



FIXE=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(GROUP_MA='DROITE', DX=0., DY=0. ),
    _F(GROUP_MA='HAUT'  , DX=0.       ),
    _F(GROUP_MA='BAS'   , DX=0.       ),
    )
  )

TRACTION=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(GROUP_MA='J_G',  DX=0. ,DY=1. ),
    )
  )


INSTANTS = DEFI_LIST_REEL(
             DEBUT = -1.,
             INTERVALLE = (
                 _F(JUSQU_A =  0,       NOMBRE =  1),
                 _F(JUSQU_A =  0.001,   NOMBRE =  1),
                 _F(JUSQU_A =  tfin,    NOMBRE = nbpas),
             ))


INS_ARCH=DEFI_LIST_REEL(
             DEBUT = -1.,
             INTERVALLE = (
                 _F(JUSQU_A =  0,     NOMBRE =  1),
                 _F(JUSQU_A =  0.001, NOMBRE =  1),
                 _F(JUSQU_A =  tfin,  NOMBRE = nbarch),
             ))


     
FCT = DEFI_FONCTION(
  NOM_PARA = 'INST',
  VALE     = (-1,0, 0,0.00001)
  )
     

EVOL = STAT_NON_LINE(
         MODELE = MO,
         CHAM_MATER = CHMAT,
         EXCIT = (
      _F(CHARGE = FIXE),
      _F(CHARGE = TRACTION, FONC_MULT=FCT)
        ),
         COMP_INCR = (
      _F(GROUP_MA = 'MATRICE', RELATION = 'ELAS'),
      _F(GROUP_MA = 'JOINT'  , RELATION = 'CZM_EXP'),
       ),
         INCREMENT = _F(
                         LIST_INST = INSTANTS, 
                         INST_FIN  = 0.,
                         SUBD_PAS  = 10,
                         SUBD_PAS_MINI = 1.E-5
       ),
         RECH_LINEAIRE = _F (
                             ITER_LINE_MAXI = 2
         ),
       SOLVEUR = _F( RENUM = 'METIS'),
       NEWTON  = _F(
               MATRICE   = 'TANGENTE',
               REAC_ITER = 1,
           ),       
       CONVERGENCE = _F(
                          ITER_GLOB_MAXI = 20,               
          ),
       ARCHIVAGE =_F(LIST_INST = INS_ARCH)
    ) 


EVOL = STAT_NON_LINE(reuse  = EVOL,
  ETAT_INIT = _F(EVOL_NOLI = EVOL),
  MODELE = MO,
  CHAM_MATER = CHMAT,
  EXCIT = (
    _F(CHARGE = FIXE),
    _F(CHARGE = TRACTION, TYPE_CHARGE = 'FIXE_PILO')
    ),
  COMP_INCR = (
    _F(GROUP_MA = 'MATRICE', RELATION = 'ELAS'),
    _F(GROUP_MA = 'JOINT'  , RELATION = 'CZM_EXP'),
    ),
  INCREMENT = _F(
    LIST_INST = INSTANTS,
    INST_FIN  = tfin,
    SUBD_PAS  = 10,
    SUBD_PAS_MINI = 1.E-5
    ),
  PILOTAGE  = _F (
    GROUP_MA       = 'JOINT',
    TYPE           = 'PRED_ELAS',
    COEF_MULT      = cmultpilo,
    ETA_PILO_R_MIN = 0,
    ETA_PILO_MIN   = 0.00001,
    PROJ_BORNES    = 'NON'
    ),
  RECH_LINEAIRE = _F (
    ITER_LINE_MAXI = 0
    ),
  SOLVEUR = _F( RENUM = 'METIS'),
  NEWTON  = _F(
    MATRICE   = 'TANGENTE',
    REAC_ITER = 1,
    PAS_MINI_ELAS = 1.E-3,
    ),
  CONVERGENCE = _F(
    RESI_GLOB_MAXI = 1.E-4,
    ITER_GLOB_MAXI = 10,
    ),
  ARCHIVAGE =_F(LIST_INST = INS_ARCH)
  ) 


DEP_NO = POST_RELEVE_T(
   ACTION=_F(
     INTITULE='DEPLAC',
     GROUP_NO  = 'NO_JHG',
     RESULTAT=EVOL,
     NOM_CHAM='DEPL',
     TOUT_ORDRE='OUI',
     NOM_CMP='DY',
     OPERATION='EXTRACTION',
     ))

IMPR_RESU(
  FORMAT  = 'RESULTAT',
  RESU=_F(
          RESULTAT = EVOL,
          NOM_CHAM = 'VARI_ELGA', 
          MAILLE  = 'MJ38',
    ))  
      
IMPR_TABLE(TABLE=DEP_NO,);


TEST_RESU(RESU=(
                _F(    RESULTAT = EVOL,
                       REFERENCE = 'ANALYTIQUE',
                       POINT = 1,
                       MAILLE ='MJ38',
                       NOM_CHAM = 'VARI_ELGA',
                       NOM_CMP = 'V7',
                       INST = 6.00070E+00,
                       VALE = 7.69747E-01,
                     ),
                _F(    RESULTAT = EVOL,
                       REFERENCE = 'ANALYTIQUE',
                       POINT = 1,
                       MAILLE ='MJ38',
                       NOM_CHAM = 'VARI_ELGA',
                       NOM_CMP = 'V7',
                       INST = 1.20004E+01,
                       VALE = 4.34935E-01,
                     ),
                _F(    RESULTAT = EVOL,
                       REFERENCE = 'ANALYTIQUE',
                       POINT = 1,
                       MAILLE ='MJ38',
                       NOM_CHAM = 'VARI_ELGA',
                       NOM_CMP = 'V7',
                       INST = 1.93334E+01,
                       VALE = 1.28483E-01,
                     ),
                _F(    RESULTAT = EVOL,
                       REFERENCE = 'ANALYTIQUE',
                       NOEUD = 'N5',
                       NOM_CHAM = 'DEPL',
                       NOM_CMP = 'DY',
                       INST = 1.20004E+01,
                       VALE = 1.57674E+00,
                     ),


                ),)
     
FIN()
