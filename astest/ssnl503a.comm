#---------------------------------------------------------------------
#   TUYAU COUDE MINCE SOUMIS A UNE FLEXION DANS SON PLAN ET A UNE
#    PRESSION INTERNE AVEC EFFET DE FOND
#   MODELISATION AVEC DES ELEMENTS 'TUYAU' (SEG3; 3 MODES DE FOURIER)
#---------------------------------------------------------------------

DEBUT(CODE=_F(NOM='SSNL503A',
              NIV_PUB_WEB='INTERNET',),);

MA=LIRE_MAILLAGE(FORMAT='MED',);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOEUD='NO1',
                                NOM='A',),
                             _F(NOEUD='NO4',
                                NOM='D',),),);

#

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='TUYAU_3M',),);

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------

CAREL=AFFE_CARA_ELEM(MODELE=MO,
                     POUTRE=_F(GROUP_MA='G_MAIL',
                               SECTION='CERCLE',
                               CARA=('R','EP',),
                               VALE=(0.2035,0.0104,),),
                     ORIENTATION=_F(GROUP_NO='A',
                                    CARA='GENE_TUYAU',
                                    VALE=(0.,0.,1.,),),);

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------

SI=DEFI_FONCTION(NOM_PARA='EPSI',VALE=(0.001404,2.719E+08,
                       0.006134,3.459E+08,
                       0.014044,3.789E+08,
                       0.029764,4.036E+08,
                       0.050504,4.242E+08,
                       0.106404,5.276E+08,
                       ),PROL_DROITE='LINEAIRE',PROL_GAUCHE='LINEAIRE',);

#

MAT=DEFI_MATERIAU(ELAS=_F(E=1.9374E+11,
                          NU=0.2642,),
                  TRACTION=_F(SIGM=SI,),);

#

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------

F_PRESS=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                            10.,1.,
                            20.,1.,
                            ),PROL_DROITE='CONSTANT',PROL_GAUCHE='EXCLU',);

#

F_MOME=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                           10.,0.,
                           20.,1.,
                           ),PROL_DROITE='CONSTANT',PROL_GAUCHE='EXCLU',);

#

CHAR0=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='A',
                                  DX=0.,
                                  DY=0.,
                                  DZ=0.,
                                  DRX=0.,
                                  DRY=0.,
                                  DRZ=0.,
                                  UI2=0.,
                                  UI3=0.,
                                  UO2=0.,
                                  UO3=0.,
                                  VI2=0.,
                                  VI3=0.,
                                  VO2=0.,
                                  VO3=0.,
                                  WI2=0.,
                                  WI3=0.,
                                  WO2=0.,
                                  WO3=0.,
                                  WO=0.,
                                  WI1=0.,
                                  WO1=0.,),
                               _F(GROUP_NO='D',
                                  UI2=0.,
                                  UI3=0.,
                                  UO2=0.,
                                  UO3=0.,
                                  VI2=0.,
                                  VI3=0.,
                                  VO2=0.,
                                  VO3=0.,
                                  WI2=0.,
                                  WI3=0.,
                                  WO2=0.,
                                  WO3=0.,
                                  WO=0.,
                                  WI1=0.,
                                  WO1=0.,),),);

#

PRESS=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_NODALE=_F(GROUP_NO='D',
                                     FX=4.0414E5,),
                     FORCE_TUYAU=_F(TOUT='OUI',
                                    PRES=1.725E+06,),);

FPRESX=DEFI_FONCTION(NOM_PARA='INST',
                      VALE=( 0.,1.725E+06,
                            10.,1.725E+06, ),
                      PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',);

PRESFX=AFFE_CHAR_MECA_F(MODELE=MO,
                     FORCE_TUYAU=_F(TOUT='OUI',
                                    PRES=FPRESX),);
#

MOME=AFFE_CHAR_MECA(MODELE=MO,
                    FORCE_NODALE=_F(GROUP_NO='D',
                                    MZ=2.534E+05,),);

#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------

LIST_INS=DEFI_LIST_REEL(DEBUT=0.,
                        INTERVALLE=(_F(JUSQU_A=10.,
                                       NOMBRE=2,),
                                    _F(JUSQU_A=20.,
                                       NOMBRE=20,),),);

#
# ETAPE 1 : PRESSION IMPOSEE

RESUL=STAT_NON_LINE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CAREL,
                    EXCIT=(_F(CHARGE=CHAR0,),
                           _F(CHARGE=PRESS,
                              FONC_MULT=F_PRESS,),
                           _F(CHARGE=PRESFX,
                              FONC_MULT=F_PRESS,),),
                    COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',
                                 ITER_INTE_MAXI=30,),
                    INCREMENT=_F(LIST_INST=LIST_INS,
                                 NUME_INST_FIN=2,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=10,),);

#
# ETAPE 2 : ROTATION IMPOSEE (MOMENT)

RESUL=STAT_NON_LINE(reuse =RESUL,
                    MODELE=MO,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CAREL,
                    EXCIT=(_F(CHARGE=CHAR0,),
                           _F(CHARGE=PRESS,
                              FONC_MULT=F_PRESS,),
                           _F(CHARGE=PRESFX,
                              FONC_MULT=F_PRESS,),
                           _F(CHARGE=MOME,
                              TYPE_CHARGE='FIXE_PILO',),),
                    COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',
                                 ITER_INTE_MAXI=30,),
                    ETAT_INIT=_F(EVOL_NOLI=RESUL,),
                    INCREMENT=_F(LIST_INST=LIST_INS,
                                 NUME_INST_INIT=2,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='DDL_IMPO',
                                COEF_MULT=25.,
                                GROUP_NO='D',
                                NOM_CMP='DRZ',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=10,),);

TAB_DRZ=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                INTITULE='TB_DRZ',
                                RESULTAT=RESUL,
                                NOM_CHAM='DEPL',
                                TOUT_ORDRE='OUI',
                                GROUP_NO='D',
                                NOM_CMP='DRZ',),);

IMPR_TABLE(TABLE=TAB_DRZ,);

#
#---------------------------------------------------------------------
#                        VERIFICATION DES RESULTATS
#---------------------------------------------------------------------

TEST_RESU(RESU=(_F(NUME_ORDRE=17,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESUL,
                   VALE_CALC=1.160372174,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.20200000000000001,),
                _F(NUME_ORDRE=18,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESUL,
                   VALE_CALC=1.169884724,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.20200000000000001,),
                _F(NUME_ORDRE=19,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESUL,
                   VALE_CALC=1.178661562,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.20200000000000001,),
                _F(NUME_ORDRE=20,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESUL,
                   VALE_CALC=1.186876229,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.20200000000000001,),
                _F(NUME_ORDRE=21,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESUL,
                   VALE_CALC=1.194641946,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.20200000000000001,),
                _F(NUME_ORDRE=22,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESUL,
                   VALE_CALC=1.201973061,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.20200000000000001,),
                _F(NUME_ORDRE=22,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESUL,
                   VALE_CALC=1.2019730614499999,
                   TOLE_MACHINE=1.E-3,),
                ),
          )

FIN();

