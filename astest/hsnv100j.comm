# IDENTIQUE A HSNV100G MAIS AVEC TEST DE AFFE_VARC DANS SIMU_POIN_MAT 
## RESPONSABLE JMBHH01 J.M.PROIX

DEBUT(CODE=_F(NOM='HSNV100J', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='NON'),IMPR_MACRO='OUI')

#......................................................................
# CAS TEST THERMOPLASTICITE ELEMENTAIRE
#......................................................................
# DEFINITION DES CARACTERISTIQUES DU MATERIAU
#

#
# DONNEES DE MODELISATION
#

FCT1=DEFI_FONCTION(   NOM_PARA='EPSI',  PROL_DROITE='LINEAIRE',
                                          PROL_GAUCHE='LINEAIRE',
                       VALE=(
                             0.200E-2,      400.,
                             0.400E-2,      500.,)
                                                    )

#

FCT2=DEFI_FONCTION(   NOM_PARA='EPSI',  PROL_DROITE='LINEAIRE',
                                          PROL_GAUCHE='LINEAIRE',
                       VALE=(
                             0.100E-2,      200.,
                             0.300E-2,      300.,)
                                                    )

#

CTRACB=DEFI_NAPPE(   NOM_PARA='TEMP',  PROL_DROITE='LINEAIRE',
                                         PROL_GAUCHE='LINEAIRE',
                           PARA=(0.,50.,),
                           FONCTION=(FCT1,FCT2,)            )

#

MATER=DEFI_MATERIAU(
          THER=_F( RHO_CP = 0.0E-3,LAMBDA = 1.0E-3),
          ELAS=_F( E = 200.E+3, NU = .3,ALPHA = 10.0E-6),
           TRACTION=_F( SIGM = CTRACB)                                 )

#


TIMPVAR=DEFI_FONCTION(
                             NOM_PARA='INST',  NOM_RESU='TEMP',
                             VALE=(  0.E0,   0.E0,
                                  100.E0, 100.E0,)
                                             )

#
ZERO=DEFI_CONSTANTE(VALE=0.)

L_INST=DEFI_LIST_REEL(
               DEBUT=0.,INTERVALLE=(
           _F( JUSQU_A = 66.666E0,   NOMBRE = 1),
                      _F( JUSQU_A = 80.000E0,   NOMBRE = 2),
                      _F( JUSQU_A = 90.000E0,   NOMBRE = 2))
         )

RESU1=SIMU_POINT_MAT(INFO=2,
                   COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                   NEWTON=_F(MATRICE='TANGENTE',REAC_ITER=1,),
                   MATER      = MATER,
                   AFFE_VARC=(
                   _F(  NOM_VARC='TEMP',
                        VALE_FONC=TIMPVAR,
                        VALE_REF=0.),
                        ),
                   INCREMENT=_F(LIST_INST=L_INST),
                   EPSI_IMPOSE=_F(EPYY=ZERO,),
                        );

IMPR_TABLE(TABLE=RESU1)

inst1=6.66660E+01
inst2=7.33330E+01
inst3=8.00000E+01
inst4=8.50000E+01
inst5=9.00000E+01


TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst1,), NOM_PARA = 'EPXX',REFERENCE='ANALYTIQUE', VALE = 8.6666E-4)

TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst1,), NOM_PARA = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -133.333)

TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst3,), NOM_PARA = 'EPXX',REFERENCE='ANALYTIQUE', VALE = 1.1000E-3)

TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst3,), NOM_PARA = 'V1',REFERENCE='ANALYTIQUE', VALE = 3.0000E-4)

TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst3,), NOM_PARA = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -100.)

TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst5,), NOM_PARA = 'EPXX',REFERENCE='ANALYTIQUE', VALE = 1.2750E-3)

TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst5,), NOM_PARA = 'V1',REFERENCE='ANALYTIQUE', VALE = 5.2500E-4)

TEST_TABLE( TABLE = RESU1 ,   FILTRE=_F(NOM_PARA='INST',VALE=inst5,), NOM_PARA = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -75.000)


RESU2=SIMU_POINT_MAT(
                   COMP_INCR=_F(RELATION='VMIS_ISOT_TRAC',),
                   NEWTON=_F(MATRICE='TANGENTE',REAC_ITER=1,),
                   MATER      = MATER,
                   SUPPORT='ELEMENT',
                   AFFE_VARC=(
                   _F(  NOM_VARC='TEMP',
                        VALE_FONC=TIMPVAR,
                        VALE_REF=0.),
                        ),
                   INCREMENT=_F(LIST_INST=L_INST),
                   EPSI_IMPOSE=_F(EPYY=ZERO,),
                        );

IMPR_TABLE(TABLE=RESU2)



TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 1,), NOM_PARA = 'EPXX',REFERENCE='ANALYTIQUE', VALE = 8.6666E-4)

TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 1,), NOM_PARA = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -133.333)

TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 3,), NOM_PARA = 'EPXX',REFERENCE='ANALYTIQUE', VALE = 1.1000E-3)

TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 3,), NOM_PARA = 'V1',REFERENCE='ANALYTIQUE', VALE = 3.0000E-4)

TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 3,), NOM_PARA = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -100.)

TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 5,), NOM_PARA = 'EPXX',REFERENCE='ANALYTIQUE', VALE = 1.2750E-3)

TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 5,), NOM_PARA = 'V1',REFERENCE='ANALYTIQUE', VALE = 5.2500E-4)

TEST_TABLE( TABLE = RESU2 ,   FILTRE=_F(NOM_PARA='NUME_ORDRE',VALE_I = 5,), NOM_PARA = 'SIYY',REFERENCE='ANALYTIQUE', VALE = -75.000)



FIN()
