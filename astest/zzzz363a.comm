                   POINT=1,
                   NOM_CMP='SIYY',
                   PRECISION=1.E-3,
                   VALE_CALC=30022.1081035 ,
                   VALE_REFE=30022.1081035 ,
                   MAILLE='M1324',
                   RESULTAT=EVOL,
                   NOM_CHAM='SIEF_ELGA',
                   VALE_ABS='NON'),),);

TEST_RESU(RESU=(_F(CRITERE='RELATIF',
                   NUME_ORDRE=7,
                   REFERENCE='NON_DEFINI',
                   POINT=1,
                   NOM_CMP='SIXY',
                   PRECISION=1.E-3,
                   VALE_CALC=174.020347593 ,
                   VALE_REFE=174.020347593 ,
                   MAILLE='M1324',
                   RESULTAT=EVOL,
                   NOM_CHAM='SIEF_ELGA',
                   VALE_ABS='NON'),),);



# TEST DE NON REGRESSION SUR LE MAX DE SIEF
# SUR TOUTE LA STRUCTURE
TEST_RESU(RESU=(_F(NUME_ORDRE=7,
                   CRITERE='RELATIF',
                   RESULTAT=EVOL,
                   NOM_CHAM ='SIEF_ELGA',
                   VALE_CALC= 30371.6716155,
                   VALE_REFE= 30371.6716155,
                   REFERENCE='NON_DEFINI',
                   TYPE_TEST='MAX',),),)

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   CRITERE='RELATIF',
                   RESULTAT=EVOL,
                   NOM_CHAM ='SIEF_ELGA',
                   VALE_CALC= 12234.3431795,
                   VALE_REFE= 12234.3431795,
                   REFERENCE='NON_DEFINI',
                   TYPE_TEST='MAX',),),)
# TEST FORC_NODA
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='P_HAUT',
                  RESULTAT=EVOL,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_CALC= 11478.716,),)


TEST_RESU(RESU=_F(NUME_ORDRE=7,
                  GROUP_NO='P_HAUT',
                  RESULTAT=EVOL,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  VALE_CALC=38311.7301956 ,),)






IMPR_RESU(FORMAT='MED',
          RESU=(_F(RESULTAT=EVOL,),),);
