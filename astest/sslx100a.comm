DEBUT(  CODE=_F( NOM = 'SSLX100A',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),)

MO=AFFE_MODELE(  MAILLAGE=MA,AFFE=(
     _F(  MAILLE = 'HE1',    PHENOMENE = 'MECANIQUE',MODELISATION = '3D'),
          _F(  GROUP_MA = 'GRMA1',PHENOMENE = 'MECANIQUE',MODELISATION = 'DKT'),
          _F(  GROUP_MA = 'GRMA2',PHENOMENE = 'MECANIQUE',MODELISATION = 'POU_D_E'))
     )

MAT=DEFI_MATERIAU(  ELAS=_F( E = 2.E5, NU = 0.3, ALPHA = 0.))

CHMA=AFFE_MATERIAU(  MAILLAGE=MA, AFFE=_F( TOUT = 'OUI', MATER = MAT))

CARA=AFFE_CARA_ELEM(  MODELE=MO,
       COQUE=_F(  GROUP_MA = 'GRMA1',EPAIS = 1.),
       POUTRE=_F(  GROUP_MA = 'GRMA2',
                SECTION = 'RECTANGLE',CARA = ('HZ','HY',),VALE = (3.,1.,))
       )

CHME=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
       _F( GROUP_NO = 'GRNO1', DX = 0., DY = 0., DZ = 0.),
                _F( NOEUD = ('N10','N11','N26','N23',), DZ = 0.)),LIAISON_GROUP=(
        
        _F(   NOEUD_1 = ('N4','N16',),
           NOEUD_2 = ('N21','N25',),
           DDL_1 = 'DX',
           DDL_2 = 'DX',
           COEF_MULT_1 = 1.,
           COEF_MULT_2 = -1.,
           COEF_IMPO = 0.),
        _F(   NOEUD_1 = ('N4','N16',),
           NOEUD_2 = ('N21','N25',),
           DDL_1 = 'DY',
           DDL_2 = 'DY',
           COEF_MULT_1 = 1.,
           COEF_MULT_2 = -1.,
           COEF_IMPO = 0.),
        _F(   NOEUD_1 = ('N4','N16',),
           NOEUD_2 = ('N21','N25',),
           DDL_1 = 'DZ',
           DDL_2 = 'DZ',
           COEF_MULT_1 = 1.,
           COEF_MULT_2 = -1.,
           COEF_IMPO = 0.),
        _F(   NOEUD_1 = ('N5','N11','N17',),
           GROUP_NO_2 = 'GRNO3',
           DDL_1 = 'DX',
           DDL_2 = 'DRZ',
           COEF_MULT_1 = 1.,
           COEF_MULT_2 = 0.5,
           COEF_IMPO = 0.),
        _F(   NOEUD_1 = ('N3','N15','N10',),
           GROUP_NO_2 = 'GRNO3',
           DDL_1 = 'DX',
           DDL_2 = 'DRZ',
           COEF_MULT_1 = 1.,
           COEF_MULT_2 = -0.5,
           COEF_IMPO = 0.)),LIAISON_DDL=(
       
        _F( NOEUD = ('N22','N23',), DDL = ('DRZ','DRZ',),
         COEF_MULT = (1.,-1.,), COEF_IMPO = 0.),
        _F( NOEUD = ('N23','N24',), DDL = ('DRZ','DRZ',),
         COEF_MULT = (1.,-1.,), COEF_IMPO = 0.)),
       FORCE_NODALE=_F( NOEUD = 'N29',FY = -1.)
      )

DEP=MECA_STATIQUE(MODELE=MO, CHAM_MATER=CHMA, CARA_ELEM=CARA,
                  EXCIT=_F( CHARGE = CHME))

DEP1=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=DEP, NUME_ORDRE=1 )

TEST_RESU(CHAM_NO=( 
          _F( CHAM_GD = DEP1, NOEUD = 'N1',  NOM_CMP = 'DY', VALE = -0., PRECISION = 1.E-8,
                                                     CRITERE = 'ABSOLU'),
    _F( CHAM_GD = DEP1, NOEUD = 'N26', NOM_CMP = 'DY', VALE = -0.0267, PRECISION = 0.135),
    _F( CHAM_GD = DEP1, NOEUD = 'N4',  NOM_CMP = 'DY', VALE = -0.0267, PRECISION = 0.138),
    _F( CHAM_GD = DEP1, NOEUD = 'N22', NOM_CMP = 'DY', VALE = -0.0933, PRECISION = 0.084),
    _F( CHAM_GD = DEP1, NOEUD = 'N23', NOM_CMP = 'DY', VALE = -0.0933, PRECISION = 0.083),
    _F( CHAM_GD = DEP1, NOEUD = 'N29', NOM_CMP = 'DY', VALE = -0.18,   PRECISION = 0.064))
          )

FIN()
