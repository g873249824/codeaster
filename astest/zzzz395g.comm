                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=0.0696319525128,
                   NUME_ORDRE= 10,
                   NOM_CHAM='DEPL',
                   NOEUD='N27',
                   NOM_CMP='DX',
                   VALE_CALC=0.0697022675078,
                   PRECISION = 0.0015
                   ),
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=0.199062276741,
                   NUME_ORDRE= 10,
                   NOM_CHAM='DEPL',
                   NOEUD='N27',
                   NOM_CMP='DY',
                   VALE_CALC=0.198932466312,
                   ),
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=0.529606351907,
                   NUME_ORDRE= 10,
                   NOM_CHAM='DEPL',
                   NOEUD='N27',
                   NOM_CMP='DZ',
                   VALE_CALC=0.529809828586,
                   ),
                ),
          );

TEST_RESU(RESU=(
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=-2739.13961277,
                   NUME_ORDRE= 10,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N27',
                   NOM_CMP='SIXX',
                   VALE_CALC=-2734.67787213,
                   PRECISION = 0.002,
                   ),
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=-2737.51235419,
                   NUME_ORDRE= 10,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N27',
                   NOM_CMP='SIYY',
