                   ),
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=-2612.22311229,
                   NUME_ORDRE= 10,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N27',
                   NOM_CMP='SIZZ',
                   VALE_CALC=-2607.46423995,
                   PRECISION = 0.002,
                   ),
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=-1.01905904477,
                   NUME_ORDRE= 10,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N27',
                   NOM_CMP='SIXY',
                   VALE_CALC=-1.379329943,
                   PRECISION = 0.36,
                   ),
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=0.358496226016,
                   NUME_ORDRE= 10,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N27',
                   NOM_CMP='SIXZ',
                   VALE_CALC=0.0271149494991,
                   PRECISION = 0.93,
                   ),
                _F(RESULTAT=redu,
                   REFERENCE='AUTRE_ASTER',
                   VALE_REFE=1.80949985804,
                   NUME_ORDRE= 10,
                   NOM_CHAM='SIEF_NOEU',
                   NOEUD='N27',
                   NOM_CMP='SIYZ',
                   VALE_CALC=1.16957363179,
