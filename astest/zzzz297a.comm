TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MIN',NOM_PARA='DSIGXX',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MIN',NOM_PARA='DSIGYY',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MIN',NOM_PARA='DSIGZZ',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MIN',NOM_PARA='DSIGXY',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
