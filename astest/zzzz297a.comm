DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

mail=LIRE_MAILLAGE()

model=AFFE_MODELE(MAILLAGE=mail,
   AFFE=(
      _F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='3D',),
   ),
)

matcoeff=PROJ_CHAMP(
   PROJECTION='NON', METHODE='COLLOCATION',
   MAILLAGE_1=mail, MAILLAGE_2=mail,
   VIS_A_VIS=_F(GROUP_MA_2='MILCUB',GROUP_MA_1='LESCUBES',),
)

Uimpose = 1.0E-03
un8=1.0/8.0
Relation = {}
Relation[0] = (33,5,6,8,10,24,23,28,27)
Relation[1] = (34,6,7,10,9,27,29,23,26)
Relation[2] = (30,8,10,1,3,24,23,21,22)
Relation[3] = (31,3,2,10,9,22,25,23,26)
Relation[4] = (36,21,22,24,23,12,13,15,14)
Relation[5] = (37,22,25,23,26,13,16,14,17)
Relation[6] = (38,14,17,18,20,27,29,23,26)
Relation[7] = (39,19,18,15,14,24,23,28,27)

Liaison = {}
for nb in Relation.keys():
   Liaison[nb] = ['N%d' %i for i in Relation[nb] ]

DDLS = {}; DDLS['DX'] = ['DX']*9;  DDLS['DY'] = ['DY']*9; DDLS['DZ'] = ['DZ']*9
COEFFS = [-1.0] +  [un8]*8


CLPROJ=AFFE_CHAR_MECA(
   MODELE=model,
   DDL_IMPO=(
      _F(GROUP_MA='S0YZ', LIAISON='ENCASTRE',),
      _F(GROUP_MA='SXYZ', DX=Uimpose,DY=Uimpose,DZ=Uimpose),
   ),
   LIAISON_PROJ=_F(MATR_PROJECTION = matcoeff, DDL=('DX','DY','DZ'),),
   INFO=1,
)

CLMANO=AFFE_CHAR_MECA(
   MODELE=model,
   DDL_IMPO=(
      _F(GROUP_MA='S0YZ',   LIAISON='ENCASTRE',),
      _F(GROUP_MA='SXYZ', DX=Uimpose,DY=Uimpose,DZ=Uimpose),
   ),
   LIAISON_DDL=(
      _F(NOEUD = Liaison[0], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[0], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[0], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),

      _F(NOEUD = Liaison[1], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[1], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[1], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),

      _F(NOEUD = Liaison[2], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[2], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[2], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),

      _F(NOEUD = Liaison[3], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[3], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[3], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),

      _F(NOEUD = Liaison[4], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[4], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[4], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),

      _F(NOEUD = Liaison[5], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[5], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[5], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),

      _F(NOEUD = Liaison[6], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[6], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[6], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),

      _F(NOEUD = Liaison[7], DDL= DDLS['DX'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[7], DDL= DDLS['DY'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
      _F(NOEUD = Liaison[7], DDL= DDLS['DZ'], COEF_MULT = COEFFS, COEF_IMPO= 0.0 ),
   ),
   INFO=1,
)


MAT01=DEFI_MATERIAU(
   ELAS=_F(E=2.0E11,  NU=0.30, RHO=0.0,),
)

CHMAT=AFFE_MATERIAU(
   MAILLAGE=mail,
   AFFE=_F(TOUT='OUI', MATER=MAT01,),
)

LINST=DEFI_LIST_REEL(VALE=(0.0,1.0))

RESU1=STAT_NON_LINE(
   MODELE     = model,
   CHAM_MATER = CHMAT,
   EXCIT=_F(CHARGE=CLPROJ,),
   COMPORTEMENT=_F(RELATION='ELAS'),
   INCREMENT=_F(LIST_INST=LINST,)
)

RESU2=STAT_NON_LINE(
   MODELE     = model,
   CHAM_MATER = CHMAT,
   EXCIT=_F(CHARGE=CLMANO,),
   COMPORTEMENT=_F(RELATION='ELAS'),
   INCREMENT=_F(LIST_INST=LINST,)
)
#IMPR_RESU(RESU=_F(RESULTAT=RESU1,INST=1.0))
#IMPR_RESU(RESU=_F(RESULTAT=RESU2,INST=1.0))

DEPL1 = CREA_TABLE(RESU=_F(RESULTAT=RESU1, NOM_CHAM='DEPL',INST=1.0,TOUT='OUI',TOUT_CMP='OUI',),)
DEPL2 = CREA_TABLE(RESU=_F(RESULTAT=RESU2, NOM_CHAM='DEPL',INST=1.0,TOUT='OUI',TOUT_CMP='OUI',),)
TDEPL1=CALC_TABLE(TABLE=DEPL1,
   ACTION=(
      _F(OPERATION='EXTR',NOM_PARA=('NOEUD','DX','DY','DZ'),),
      _F(OPERATION='RENOMME',NOM_PARA=('DX','DX1'),),
      _F(OPERATION='RENOMME',NOM_PARA=('DY','DY1'),),
      _F(OPERATION='RENOMME',NOM_PARA=('DZ','DZ1'),),
   ),
)
TDEPL2=CALC_TABLE(TABLE=DEPL2,
   ACTION=(
      _F(OPERATION='EXTR',NOM_PARA=('NOEUD','DX','DY','DZ'),),
      _F(OPERATION='RENOMME',NOM_PARA=('DX','DX2'),),
      _F(OPERATION='RENOMME',NOM_PARA=('DY','DY2'),),
      _F(OPERATION='RENOMME',NOM_PARA=('DZ','DZ2'),),
   ),
)
DIFFDX=FORMULE(NOM_PARA=('DX1','DX2'),VALE='(DX1-DX2)/Uimpose')
DIFFDY=FORMULE(NOM_PARA=('DY1','DY2'),VALE='(DY1-DY2)/Uimpose')
DIFFDZ=FORMULE(NOM_PARA=('DZ1','DZ2'),VALE='(DZ1-DZ2)/Uimpose')
DDEPL=CALC_TABLE(
   TABLE=TDEPL1,
   ACTION=(
      _F(OPERATION='COMB',TABLE=TDEPL2,NOM_PARA='NOEUD'),
      _F(OPERATION='OPER',NOM_PARA='DDX',FORMULE=DIFFDX),
      _F(OPERATION='OPER',NOM_PARA='DDY',FORMULE=DIFFDY),
      _F(OPERATION='OPER',NOM_PARA='DDZ',FORMULE=DIFFDZ),
      _F(OPERATION='SUPPRIME',NOM_PARA=('DX1','DY1','DZ1','DX2','DY2','DZ2',),),
   ),
)
TEST_TABLE(TABLE=DDEPL, TYPE_TEST='MAX',NOM_PARA='DDX',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DDEPL, TYPE_TEST='MAX',NOM_PARA='DDY',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DDEPL, TYPE_TEST='MAX',NOM_PARA='DDZ',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DDEPL, TYPE_TEST='MIN',NOM_PARA='DDX',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DDEPL, TYPE_TEST='MIN',NOM_PARA='DDY',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DDEPL, TYPE_TEST='MIN',NOM_PARA='DDZ',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')


SIGM1 = CREA_TABLE(RESU=_F(RESULTAT=RESU1, NOM_CHAM='SIEF_ELGA',
INST=1.0,TOUT='OUI',TOUT_CMP='OUI',),)
SIGM2 = CREA_TABLE(RESU=_F(RESULTAT=RESU2, NOM_CHAM='SIEF_ELGA',
INST=1.0,TOUT='OUI',TOUT_CMP='OUI',),)
TSIGM1=CALC_TABLE(TABLE=SIGM1,
   ACTION=(
      _F(OPERATION='EXTR',NOM_PARA=('MAILLE','POINT','SIXX','SIYY','SIZZ','SIXY','SIXZ','SIYZ'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIXX','SIXX1'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIYY','SIYY1'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIZZ','SIZZ1'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIXY','SIXY1'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIXZ','SIXZ1'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIYZ','SIYZ1'),),
   ),
)
TSIGM2=CALC_TABLE(TABLE=SIGM2,
   ACTION=(
      _F(OPERATION='EXTR',NOM_PARA=('MAILLE','POINT','SIXX','SIYY','SIZZ','SIXY','SIXZ','SIYZ'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIXX','SIXX2'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIYY','SIYY2'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIZZ','SIZZ2'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIXY','SIXY2'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIXZ','SIXZ2'),),
      _F(OPERATION='RENOMME',NOM_PARA=('SIYZ','SIYZ2'),),
   ),
)
Sigmaref = 1.0E+06
DIFFSIXX=FORMULE(NOM_PARA=('SIXX1','SIXX2'),VALE='(SIXX1-SIXX2)/Sigmaref')
DIFFSIYY=FORMULE(NOM_PARA=('SIYY1','SIYY2'),VALE='(SIYY1-SIYY2)/Sigmaref')
DIFFSIZZ=FORMULE(NOM_PARA=('SIZZ1','SIZZ2'),VALE='(SIZZ1-SIZZ2)/Sigmaref')
DIFFSIXY=FORMULE(NOM_PARA=('SIXY1','SIXY2'),VALE='(SIXY1-SIXY2)/Sigmaref')
DIFFSIXZ=FORMULE(NOM_PARA=('SIXZ1','SIXZ2'),VALE='(SIXZ1-SIXZ2)/Sigmaref')
DIFFSIYZ=FORMULE(NOM_PARA=('SIYZ1','SIYZ2'),VALE='(SIYZ1-SIYZ2)/Sigmaref')
DSIGM=CALC_TABLE(
   TABLE=TSIGM1,
   ACTION=(
      _F(OPERATION='COMB',TABLE=TSIGM2,NOM_PARA=('MAILLE','POINT',),),
      _F(OPERATION='OPER',NOM_PARA='DSIGXX',FORMULE=DIFFSIXX),
      _F(OPERATION='OPER',NOM_PARA='DSIGYY',FORMULE=DIFFSIYY),
      _F(OPERATION='OPER',NOM_PARA='DSIGZZ',FORMULE=DIFFSIZZ),
      _F(OPERATION='OPER',NOM_PARA='DSIGXY',FORMULE=DIFFSIXY),
      _F(OPERATION='OPER',NOM_PARA='DSIGXZ',FORMULE=DIFFSIXZ),
      _F(OPERATION='OPER',NOM_PARA='DSIGYZ',FORMULE=DIFFSIYZ),
      _F(OPERATION='SUPPRIME',NOM_PARA=('SIXX1','SIYY1','SIZZ1','SIXY1','SIXZ1','SIYZ1',
                                        'SIXX2','SIYY2','SIZZ2','SIXY2','SIXZ2','SIYZ2',),),
   ),
)
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MAX',NOM_PARA='DSIGXX',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MAX',NOM_PARA='DSIGYY',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MAX',NOM_PARA='DSIGZZ',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MAX',NOM_PARA='DSIGXY',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MAX',NOM_PARA='DSIGXZ',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
    VALE_REFE=0.0,VALE_ABS='OUI',CRITERE='ABSOLU',REFERENCE='AUTRE_ASTER')
TEST_TABLE(TABLE=DSIGM, TYPE_TEST='MAX',NOM_PARA='DSIGYZ',VALE_CALC=0.0,ORDRE_GRANDEUR=1.0,
