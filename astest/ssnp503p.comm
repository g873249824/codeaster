# CONTACT-FROTTANT EN GRANDS GLISSEMENTS 3D
# AVEC XFEM POUR DES FISSURES HORIZONTALES
# INTRODUCTION DE FONDS DE FISSURES

DEBUT(CODE=_F(NOM='SSNP503P',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

typma=1

if (typma==1):
  MAILLAG2=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
  Y=3

if (typma==2):
  MAILLAG1=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
  MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='VOLU'));
  Y=2.5

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,Y,0.0),
                                       VECT_NORMALE = (0.0,1.0,0.0),
                                       PRECISION = 1E-1)))
MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU2',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,6,0.0),
                                       VECT_NORMALE = (0.0,1.0,0.0),
                                       PRECISION = 1E-1)))

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOLU'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D')));

LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y-2.5');
LT1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-2.5');


LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y-6.5');
LT2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-3');

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,
                     DEFI_FISS=_F(FONC_LT=LT1,FONC_LN=LN1),
                     INFO=1);

FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,
                     DEFI_FISS=_F(FONC_LT=LT2,FONC_LN=LN2),
                     INFO=1);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='P1P1',
                         INFO=1);

CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      REAC_GEOM      = 'AUTOMATIQUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 10,
                      ITER_GEOM_MAXI = 10,
                      FROTTEMENT     = 'COULOMB',
                      ZONE = (_F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',
                                 FISS_MAIT     = FISS1,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                             _F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',
                                 FISS_MAIT     = FISS2,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                            )
                    );

E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER));

DEPX=2.E-1
DEPY=1.E-3
DEPZ=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SINF',DY=DEPY,DZ=0.0),
                             _F(GROUP_MA='SSUP',DY=-DEPY,DZ=0.0),
                             _F(GROUP_MA=('L1','L2','L3','L4'),
                                DX=0.0),
                             ),
                   INFO=1);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SCOTE',
                               DX=DEPX,DZ=DEPZ)),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                   LIAISON_DDL=(_F(NOEUD=('N9','N9'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N29','N29'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,-1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N45','N45'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N65','N65'),
                                  DDL=('DX','H1X'),
                                  COEF_MULT=(1,-1),
                                  COEF_IMPO=DEPX),
                               _F(NOEUD=('N9','N9'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPZ),
                               _F(NOEUD=('N29','N29'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,-1),
                                  COEF_IMPO=DEPZ),
                               _F(NOEUD=('N45','N45'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,1),
                                  COEF_IMPO=DEPZ),
                               _F(NOEUD=('N65','N65'),
                                  DDL=('DZ','H1Z'),
                                  COEF_MULT=(1,-1),
                                  COEF_IMPO=DEPZ)),
                    INFO=1);

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    CONTACT=CTXFEM,
                    EXCIT=(
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2)),
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 GROUP_MA='VOLU'),
                    INCREMENT=_F(LIST_INST=L_INST),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06
                                    )),
                    NEWTON=_F(REAC_ITER=1),
                    SOLVEUR=_F(METHODE='MUMPS',

                               NPREC=-1),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                    INFO=1);

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]

vale = [[-1.4922085453317E+05,-4.4964660477316E+04,-1.1560081641114E+04],
        [-2.9844254580455E+05,-8.9929003174795E+04,-2.3119982111424E+04],
        [-4.4766423637664E+05,-1.3489334596908E+05,-3.4679882617927E+04],
        [-5.9687934486023E+05,-1.7986014193268E+05,-4.6241200926537E+04]]

VALE_CALC=[
 [-1.480791713127E+05,
 -45248.356817116,
 -10849.777679321,],
 [-2.936997727234E+05,
 -90974.69546961,
 -20223.578418532,],
 [-4.3661700071496E+05,
 -1.370454771602E+05,
 -28045.823935678,],
 [-5.7661053215892E+05,
 -1.8332448299193E+05,
 -34247.470373428,],
]

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                  GROUP_NO='NFISSU',
                                  RESULTAT=UTOT1,
                                  NOM_CHAM='DEPL',
                                  INST=inst[i],
                                  NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2'),
                                  OPERATION='EXTRACTION'));

   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              NOM_PARA=('NOEUD','INST','COOR_X','COOR_Z','LAGS_C','LAGS_F1','LAGS_F2'),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X'),
                     ORDRE='CROISSANT'));

   for j in [0,1,2]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-10
        critere = 'ABSOLU'
      else:
        precision = 3E-1*inst[i]
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE_CALC=VALE_CALC[i-1][j], VALE_REFE=vale[i-1][j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=(_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU'),
                         _F(NOM_PARA='COOR_Z',
                           CRIT_COMP='EQ',
                           VALE=0.0,
                           PRECISION=0.01,
                           CRITERE='ABSOLU')));

FIN();
