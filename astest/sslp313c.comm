DEBUT(CODE=_F(NOM='SSLP313C',NIV_PUB_WEB='INTERNET'),);

PRE_GIBI();

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),);

MA=MODI_MAILLAGE( reuse=MA,   MAILLAGE=MA,
                  MODI_MAILLE=_F( OPTION = 'NOEUD_QUART',
                                  GROUP_NO_FOND = 'P0'),
                  INFO=2
                      )

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='C_PLAN',),);

MAT=DEFI_MATERIAU(ELAS=_F(E=2.E11,
                          NU=0.3,
                          ALPHA=0.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

BL=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_MA='L34',
                               DY=0.0,),
                            _F(GROUP_NO='PS4',
                               DX=0.0,),),);

CH=AFFE_CHAR_MECA(MODELE=MO,
                  FORCE_CONTOUR=_F(GROUP_MA='L12',
                                   FY=1.E8,),);

AR=AFFE_CHAR_MECA(MODELE=MO,
                  ARLEQUIN=_F(GROUP_MA_1=('COUR','FISS'),
                              GROUP_MA_2='PLAQ',
                              GROUP_MA_COLL='COUR',
                              COLLAGE='GROUP_MA_1',
                              POIDS_1=0.99999,),);

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=AR,),
                          _F(CHARGE=BL,),
                          _F(CHARGE=CH,),),
                   COMP_ELAS=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=LINST,),);

DEPL=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RESU,
                NOM_CHAM='DEPL',
                NUME_ORDRE=1,);

FOND1=DEFI_FOND_FISS(MAILLAGE=MA,
                     FOND_FISS=_F(GROUP_NO='P0',),
                     NORMALE=(-0.6018,0.7986,0.0),);

THETA01=CALC_THETA(MODELE=MO,
                   DIRECTION=(0.7986,0.6018,0.0),
                   THETA_2D=_F(GROUP_NO='P0',
                               MODULE=1.0,
                               R_INF=1.875E-05,
                               R_SUP=7.5E-05,),);

G01=CALC_G_THETA_T(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   THETA=THETA01,
                   EXCIT=(_F(CHARGE=AR,),
                          _F(CHARGE=BL,),
                          _F(CHARGE=CH,),),
                   DEPL=DEPL,
                   SYME_CHAR='SANS',);

GK01=CALC_G_THETA_T(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    THETA=THETA01,
                    EXCIT=(_F(CHARGE=AR,),
                          _F(CHARGE=BL,),
                          _F(CHARGE=CH,),),
                    DEPL=DEPL,
                    SYME_CHAR='SANS',
                    OPTION='CALC_K_G',
                    FOND_FISS=FOND1,);

LEVH=INTE_MAIL_2D(MAILLAGE=MA,
                  DEFI_CHEMIN=_F(GROUP_MA='LPOP8',),
                  GROUP_NO_ORIG='P0',);

LEVB=INTE_MAIL_2D(MAILLAGE=MA,
                  DEFI_CHEMIN=_F(GROUP_MA='LPOP8B',),
                  GROUP_NO_ORIG='P0',);

DEPH=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL_DH',
                             CHEMIN=LEVH,
                             RESULTAT=RESU,
                             NOM_CHAM='DEPL',
                             NOM_CMP=('DX','DY'),
                             OPERATION='EXTRACTION',),);

DEPB=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL_DB',
                             CHEMIN=LEVB,
                             RESULTAT=RESU,
                             NOM_CHAM='DEPL',
                             NOM_CMP=('DX','DY'),
                             OPERATION='EXTRACTION',),);

TABL_K=POST_K1_K2_K3(MODELISATION='C_PLAN',
                     MATER=MAT,
                     TABL_DEPL_SUP=DEPH,
                     TABL_DEPL_INF=DEPB,
                     ABSC_CURV_MAXI=7.51E-05,
                     INST=1.0,
                     VECT_K1=(0.0,1.0,0.0),);

