# ARLEQUIN : SUBSTITUTION 2D/2D INCOMPATIBLE.
#            MAILLAGES DE DENSITES DIFFERENTES

DEBUT(CODE=_F(NOM='SSLP313C',
              NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI',),);

PRE_GIBI();

MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),);

MAIL=MODI_MAILLAGE( reuse=MAIL,   MAILLAGE=MAIL,
                    MODI_MAILLE=_F( OPTION = 'NOEUD_QUART',
                                    GROUP_NO_FOND = 'P0'),
                      )

MODARL=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN',),);

MATERI=DEFI_MATERIAU(ELAS=_F(E=2.E11,
                             NU=0.3,
                             ALPHA=0.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=MATERI,),);

BLOC=AFFE_CHAR_MECA(MODELE=MODARL,
                    DDL_IMPO=(_F(GROUP_MA='L34',
                                 DY=0.0,),
                              _F(GROUP_NO='PS4',
                                 DX=0.0,),),);

CHAREX=AFFE_CHAR_MECA(MODELE=MODARL,
                      FORCE_CONTOUR=_F(GROUP_MA='L12',
                                       FY=1.E8,),);

##################################################
# 1. Creation des relations de couplage Arlequin
#    entre les groupes de mailles
#    couronne et fissure (maillage fin)
#    et la plaque (maillage grossier)
#    Collage sur le modele fin
##################################################

ARLEQ=AFFE_CHAR_MECA(MODELE=MODARL,
                     ARLEQUIN=_F(GROUP_MA_1=('COUR','FISS'),
                                 GROUP_MA_2='PLAQ',
                                 GROUP_MA_COLL='COUR',
                                 COLLAGE='GROUP_MA_1',
                                 POIDS_1=0.99999,),);

####################
# 2. CALCUL STATIQUE
####################

RESU=MECA_STATIQUE(MODELE=MODARL,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=ARLEQ,),
                          _F(CHARGE=BLOC,),
                          _F(CHARGE=CHAREX,),),);

########################################################
# 3. POST-TRAITEMENT MECANIQUE DE LA RUPTURE
# Attention : on est sur un modele Arlequin !
#             Il faut donc restreindre le resultat RESU
#             au groupe de mailles du patch (RESU_RES).
########################################################

IMPR_RESU(FORMAT='MED', 
          UNITE=81,
          RESTREINT=_F(GROUP_MA=('COUR','FISS','LPOP8','LPOP8B',),
                       TOUT_GROUP_NO='OUI',),
          RESU=_F(RESULTAT=RESU,NOM_CHAM='DEPL'))

MA_RES=LIRE_MAILLAGE(UNITE=81,
                     FORMAT='MED',
                     VERI_MAIL=_F(VERIF='OUI',),)

MA_RES=MODI_MAILLAGE(reuse=MA_RES,
                     MAILLAGE=MA_RES,
                     MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                    GROUP_NO_FOND='P0',))

MO_RES=AFFE_MODELE(MAILLAGE=MA_RES,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='C_PLAN'))

MAT_RES=AFFE_MATERIAU(MAILLAGE=MA_RES, AFFE=_F(TOUT='OUI',MATER=MATERI))

RESU_RES=LIRE_RESU(TYPE_RESU='EVOL_ELAS', 
                   FORMAT='MED',
                   UNITE=81,
                   MODELE=MO_RES,
                   CHAM_MATER=MAT_RES,
                   TOUT_ORDRE='OUI',
                   FORMAT_MED=_F(NOM_CHAM_MED='&RESUR1_DEPL____________________',
                                 NOM_CHAM='DEPL'),)

FOND1=DEFI_FOND_FISS(MAILLAGE=MA_RES,
                     FOND_FISS=_F(GROUP_NO='P0',),
                     NORMALE=(-0.6018,0.7986,0.0),);

THETA01=CALC_THETA(MODELE=MO_RES,
                   DIRECTION=(0.7986,0.6018,0.0),
                   THETA_2D=_F(GROUP_NO='P0',
                               MODULE=1.0,
                               R_INF=1.875E-05,
                               R_SUP=7.5E-05,),);

###################################################
# 4. On peut post-traiter sur le modele restreint :
#    => calcul de G, K1, K2, G_IRWIN
###################################################

G01=CALC_G(        THETA=_F(THETA=THETA01),
                   RESULTAT=RESU_RES,
                   NUME_ORDRE=1,
                   SYME_CHAR='SANS',);

GK01=CALC_G(        THETA=_F(
                               DIRECTION=(0.7986,0.6018,0.0),
                               MODULE=1.0,
                               R_INF=1.875E-05,
                               R_SUP=7.5E-05,
                               FOND_FISS=FOND1),
                    RESULTAT=RESU_RES,
                    NUME_ORDRE=1,
                    SYME_CHAR='SANS',
                    OPTION='CALC_K_G');

LEVH=INTE_MAIL_2D(MAILLAGE=MA_RES,
                  DEFI_CHEMIN=_F(GROUP_MA='LPOP8',),
                  GROUP_NO_ORIG='P0',);

LEVB=INTE_MAIL_2D(MAILLAGE=MA_RES,
                  DEFI_CHEMIN=_F(GROUP_MA='LPOP8B',),
                  GROUP_NO_ORIG='P0',);

DEPH=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL_DH',
                             CHEMIN=LEVH,
                             RESULTAT=RESU_RES,
                             NOM_CHAM='DEPL',
                             NOM_CMP=('DX','DY'),
                             OPERATION='EXTRACTION',),);

DEPB=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL_DB',
                             CHEMIN=LEVB,
                             RESULTAT=RESU_RES,
                             NOM_CHAM='DEPL',
                             NOM_CMP=('DX','DY'),
                             OPERATION='EXTRACTION',),);

TABL_K=POST_K1_K2_K3(MODELISATION='C_PLAN',
                     MATER=MATERI,
                     TABL_DEPL_SUP=DEPH,
                     TABL_DEPL_INF=DEPB,
                     ABSC_CURV_MAXI=7.51E-05,
                     VECT_K1=(0.0,1.0,0.0),);

TEST_TABLE(TABLE=GK01,
           NOM_PARA='K1',
           VALE=3.574968087E6,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=GK01,
           NOM_PARA='K2',
           VALE=2.693931681E6,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=GK01,
           NOM_PARA='G',
           VALE=100.1883,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=1,),
           NOM_PARA='K1_MAX',
           VALE=3.574968087E6,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=1,),
           NOM_PARA='K1_MIN',
           VALE=3.574968087E6,
           PRECISION=1.5E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=1,),
           NOM_PARA='K2_MAX',
           VALE=2.693931681E6,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=1,),
           NOM_PARA='K2_MIN',
           VALE=2.693931681E6,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=1,),
           NOM_PARA='G_MAX',
           VALE=100.1883,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=1,),
           NOM_PARA='G_MIN',
           VALE=100.1883,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=2,),
           NOM_PARA='K1_MAX',
           VALE=3.574968087E6,
           PRECISION=1.5E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=2,),
           NOM_PARA='K1_MIN',
           VALE=3.574968087E6,
           PRECISION=3.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=2,),
           NOM_PARA='K2_MAX',
           VALE=2.693931681E6,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=2,),
           NOM_PARA='K2_MIN',
           VALE=2.693931681E6,
           PRECISION=1.5E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=2,),
           NOM_PARA='G_MAX',
           VALE=100.1883,
           PRECISION=1.E-2,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=TABL_K,
           FILTRE=_F(NOM_PARA='METHODE',
                     VALE_I=2,),
           NOM_PARA='G_MIN',
           VALE=100.1883,
           PRECISION=5.E-2,
           REFERENCE='AUTRE_ASTER',);

FIN();
