# DEFINITIONS DE CONSTANTES

F_INIT   = 1.0
F_FIN    = 3.848320E+02

N_POIN_0 = 1024*2
DF       = (F_FIN-F_INIT)/(N_POIN_0-1)

NB_TIR = 1
T_FIN  = NB_TIR/DF
T_PAS  = 1.E-5
T_INI  = 0.

N_POIN_1 = 1./(2.*DF*T_PAS)
N_POIN_1 = 2.**(int(log(N_POIN_1)/log(2))+1)

F_MAX_GEN = DF * N_POIN_1
T_PAS = 1./F_MAX_GEN

