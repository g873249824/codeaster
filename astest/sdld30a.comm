##############################


DEBUT(CODE=_F(NOM='SDLD30A',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);
M2 = 10.0;

M3 = 10.0;

K1 = 1000.0;

K2 = 1000.0;

K3 = 10000.0;


# PARAMETRES EXCITATIONS MULTI APPUIS SRO+DDS
# NO1
A1_F1 = 7.0;

A1_F2 = 5.0;

DDS1 = -0.04;


# NO4
A2_F1 = 12.0;

A2_F2 = 6.0;

DDS2 = 0.06;


MAILLAGE=LIRE_MAILLAGE();

MODELE=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=(_F(GROUP_MA='RESSORT',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='DIS_T',),
                         _F(GROUP_NO='MASSES',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='DIS_T',),),);

#

CARA_ELE=AFFE_CARA_ELEM(MODELE=MODELE,
                        DISCRET=(_F(REPERE='GLOBAL',
                                    CARA='K_T_D_L',
                                    MAILLE='ELN1',
                                    VALE=(K1,1.,K1,),),
                                 _F(REPERE='GLOBAL',
                                    CARA='K_T_D_L',
                                    MAILLE='ELN2',
                                    VALE=(K2,1.,K2,),),
                                 _F(REPERE='GLOBAL',
                                    CARA='K_T_D_L',
                                    MAILLE='ELN3',
                                    VALE=(K3,1.,K3,),),
                                 _F(CARA='M_T_D_N',
                                    NOEUD='NO2',
                                    VALE=M2,),
                                 _F(CARA='M_T_D_N',
                                    NOEUD='NO3',
                                    VALE=M3,),),);

MAT=DEFI_MATERIAU(ELAS=_F(E=1.6E+11,
                          NU=0.3,
                          RHO=1.3E+04,),);

#

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       AFFE=_F(TOUT='OUI',
                               MATER=MAT,),);

#

CON_LIM=AFFE_CHAR_MECA(MODELE=MODELE,
                       DDL_IMPO=(_F(GROUP_NO='ENCASTRE',
                                    DX=0.,
                                    DY=0.,
                                    DZ=0.,),
                                 _F(GROUP_NO='MASSES',
                                    DY=0.,
                                    DZ=0.,),),);

#

MACRO_MATR_ASSE(MODELE=MODELE,
                CHAM_MATER=CHAMPMAT,
                CARA_ELEM=CARA_ELE,
                CHARGE=CON_LIM,
                NUME_DDL=CO('NUMEDDL'),
                MATR_ASSE=(_F(MATRICE=CO('RIGIDITE'),
                              OPTION='RIGI_MECA',),
                           _F(MATRICE=CO('MASSE'),
                              OPTION='MASS_MECA',),),);

#
# CALCUL DES MODES AVEC BLOCAGE DES ANCRAGES:
#---------------------------------------------

MASSINER=POST_ELEM(MASS_INER=_F(TOUT='OUI',),
                   MODELE=MODELE,
                   CHAM_MATER=CHAMPMAT,
                   CARA_ELEM=CARA_ELE,
                   CHARGE=CON_LIM,);

MODES=MODE_ITER_SIMULT(MATR_A=RIGIDITE,
                       MATR_B=MASSE,
                       METHODE='SORENSEN',
                       CALC_FREQ=_F(NMAX_FREQ=3,),);

IMPR_RESU(RESU=(_F(RESULTAT=MODES,
                   TOUT_PARA = 'OUI',)))
#
MODES=NORM_MODE(reuse =MODES,
                MODE=MODES,
                NORME='MASS_GENE',);

#

MODES=CALC_ELEM(reuse =MODES,
                RESULTAT=MODES,
                OPTION=('SIEF_ELGA_DEPL','EFGE_ELNO_DEPL',),);

MODES=CALC_NO(reuse =MODES,
              RESULTAT=MODES,
              TOUT_ORDRE='OUI',
              OPTION=('REAC_NODA','FORC_NODA',),);

# TESTS DES FREQUENCES PROPRES
# REF : ANALYTIQUE
#-------------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=MODES,
                   NUME_MODE=1,
                   PARA='FREQ',
                   VALE=2.18815,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=MODES,
                   NUME_MODE=2,
                   PARA='FREQ',
                   VALE=5.30485,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

#
# CALCUL DES MODES STATIQUES AUX POINTS D'ANCRAGE:
#-------------------------------------------------

MODE_STA=MODE_STATIQUE(MATR_RIGI=RIGIDITE,
                       MATR_MASS=MASSE,
                       MODE_STAT=_F(TOUT='OUI',
                                    AVEC_CMP='DX',),);

MODE_STA=CALC_ELEM(reuse =MODE_STA,
                   RESULTAT=MODE_STA,
                   OPTION=('SIEF_ELGA_DEPL','EFGE_ELNO_DEPL',),);

MODE_STA=CALC_NO(reuse =MODE_STA,
                 RESULTAT=MODE_STA,
                 TOUT_ORDRE='OUI',
                 OPTION=('REAC_NODA','FORC_NODA',),);

#-------------------------------------------------
#-------------------------------------------------
# ANALYSE SPECTRALE :
# CALCUL DE LA REPONSE GLOBALE
# COMPOSANTES INERTIELLE (SRO ACCE) ET ENTRAINEMENT (DDS)
# CUMULEES LORS DU CALCUL DES REPONSES PAR APPUI
#-------------------------------------------------
#-------------------------------------------------
# ACC X NO1

ACCX1=DEFI_FONCTION(NOM_PARA='FREQ',VALE=(2.18815,A1_F1,
                          5.30485,A1_F2,
                          ),INTERPOL='LOG',PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',);

SPEX1=DEFI_NAPPE(NOM_PARA='AMOR',
                 PARA=(0.0,1.0,),
                 FONCTION=(ACCX1,ACCX1,),
                 INTERPOL=('LIN','LOG',),
                 PROL_DROITE='CONSTANT',
                 PROL_GAUCHE='CONSTANT',
                 INFO=2,);

# ACC X NO4

ACCX2=DEFI_FONCTION(NOM_PARA='FREQ',VALE=(2.18815,A2_F1,
                          5.30485,A2_F2,
                          ),INTERPOL='LOG',PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',);

SPEX2=DEFI_NAPPE(NOM_PARA='AMOR',
                 PARA=(0.0,1.0,),
                 FONCTION=(ACCX2,ACCX2,),
                 INTERPOL=('LIN','LOG',),
                 PROL_DROITE='CONSTANT',
                 PROL_GAUCHE='CONSTANT',
                 INFO=2,);

#------------------------------------------
# RGQBC
# REPONSE GLOBALE SUR BASE MODALE COMPLETE
# MODES 1 2
# GROUP_APPUI QUAD
#------------------------------------------

RGQBC=COMB_SISM_MODAL(MODE_MECA=MODES,
                      NUME_MODE=(1,2,),
                      AMOR_REDUIT=0.05,
                      MASS_INER=MASSINER,
                      CORR_FREQ='NON',
                      MULTI_APPUI ='DECORRELE',
                      EXCIT=(_F(NOEUD='NO1',
                                AXE=(1.,0.,0.,),
                                SPEC_OSCI=SPEX1,
                                ECHELLE=1.,),
                             _F(NOEUD='NO4',
                                AXE=(1.,0.,0.,),
                                SPEC_OSCI=SPEX2,
                                ECHELLE=1.,),),
                      COMB_MODE=_F(TYPE='SRSS',),
                      DEPL_MULT_APPUI=(_F(NOM_CAS='DDS NO1',
                                          NUME_CAS=1,
                                          MODE_STAT=MODE_STA,
                                          NOEUD='NO1',
                                          DX=DDS1,),
                                       _F(NOM_CAS='DDS NO4',
                                          NUME_CAS=2,
                                          MODE_STAT=MODE_STA,
                                          NOEUD='NO4',
                                          DX=DDS2,),),
                      OPTION=('DEPL','REAC_NODA',),
                      GROUP_APPUI=(_F(NOEUD='NO1',),),
                      INFO=2,
                      IMPRESSION=_F(TOUT='OUI',),);

IMPR_RESU(RESU=(_F(RESULTAT=RGQBC,
                   TOUT_PARA = 'OUI',)))


# TESTS DES DEPLACEMENTS
#------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=RGQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=0.04,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO2',
                   VALE=0.054382,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO3',
                   VALE=0.0575544,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO4',
                   VALE=0.06,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

# TESTS DES REACTIONS NODALES AUX APPUIS
#------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=RGQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=53.6769,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO4',
                   VALE=74.412,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

#------------------------------------------
# RGQBI
# REPONSE GLOBALE SUR BASE MODALE INCOMPLETE
# SANS CORRECTION STATIQUE
# MODES 1
# GROUP_APPUI QUAD
#------------------------------------------

RGQBI=COMB_SISM_MODAL(MODE_MECA=MODES,
                      NUME_MODE=1,
                      AMOR_REDUIT=0.05,
                      MASS_INER=MASSINER,
                      CORR_FREQ='NON',
                      MULTI_APPUI ='DECORRELE',
                      EXCIT=(_F(NOEUD='NO1',
                                AXE=(1.,0.,0.,),
                                SPEC_OSCI=SPEX1,
                                ECHELLE=1.,),
                             _F(NOEUD='NO4',
                                AXE=(1.,0.,0.,),
                                SPEC_OSCI=SPEX2,
                                ECHELLE=1.,),),
                      COMB_MODE=_F(TYPE='SRSS',),
                      DEPL_MULT_APPUI=(_F(NOM_CAS='DDS NO1',
                                          NUME_CAS=1,
                                          MODE_STAT=MODE_STA,
                                          NOEUD='NO1',
                                          DX=DDS1,),
                                       _F(NOM_CAS='DDS NO4',
                                          NUME_CAS=2,
                                          MODE_STAT=MODE_STA,
                                          NOEUD='NO4',
                                          DX=DDS2,),),
                      GROUP_APPUI=(_F(NOEUD='NO1',),
                                   _F(NOEUD='NO4',),),
                      OPTION=('DEPL','REAC_NODA',),
                      INFO=2,
                      IMPRESSION=_F(TOUT='OUI',),);

# TESTS DES DEPLACEMENTS
#------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=RGQBI,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=0.04,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBI,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO2',
                   VALE=0.0543794,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBI,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO3',
                   VALE=0.0573536,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBI,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO4',
                   VALE=0.06,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

# TESTS DES REACTIONS NODALES AUX APPUIS
#------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=RGQBI,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=53.6743,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBI,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO4',
                   VALE=56.8312,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

#---------------------------------------------------
# CALCUL PSEUDO MODES POUR LA CORRECTION STATIQUE
# ACCELEARATION UNITAIRE PAR APPUI
#---------------------------------------------------

MODEC=MODE_STATIQUE(MATR_RIGI=RIGIDITE,
                    MATR_MASS=MASSE,
                    PSEUDO_MODE=_F(NOEUD=('NO1','NO4',),
                                   AVEC_CMP='DX',),);

MODEC=CALC_ELEM(reuse =MODEC,
                RESULTAT=MODEC,
                OPTION=('SIEF_ELGA_DEPL','EFGE_ELNO_DEPL',),);

MODEC=CALC_NO(reuse =MODEC,
              RESULTAT=MODEC,
              TOUT_ORDRE='OUI',
              OPTION='REAC_NODA',);

#------------------------------------------
# RGQBIC
# REPONSE GLOBALE SUR BASE INCOMPLETE
# AVEC CORRECTION STATIQUE
# MODES 1
# GROUP_APPUI QUAD
#------------------------------------------

RGQBIC=COMB_SISM_MODAL(MODE_MECA=MODES,
                       NUME_MODE=1,
                       MODE_CORR=MODEC,
                       AMOR_REDUIT=0.05,
                       MASS_INER=MASSINER,
                       CORR_FREQ='NON',
                       MULTI_APPUI ='DECORRELE',
                       EXCIT=(_F(NOEUD='NO1',
                                 AXE=(1.,0.,0.,),
                                 SPEC_OSCI=SPEX1,
                                 ECHELLE=1.,),
                              _F(NOEUD='NO4',
                                 AXE=(1.,0.,0.,),
                                 SPEC_OSCI=SPEX2,
                                 ECHELLE=1.,),),
                       COMB_MODE=_F(TYPE='SRSS',),
                       DEPL_MULT_APPUI=(_F(NOM_CAS='DDS NO1',
                                           NUME_CAS=1,
                                           MODE_STAT=MODE_STA,
                                           NOEUD='NO1',
                                           DX=DDS1,),
                                        _F(NOM_CAS='DDS NO4',
                                           NUME_CAS=2,
                                           MODE_STAT=MODE_STA,
                                           NOEUD='NO4',
                                           DX=DDS2,),),
                       GROUP_APPUI=(_F(NOEUD='NO1',),
                                   _F(NOEUD='NO4',),),
                       OPTION=('DEPL','REAC_NODA',),
                       INFO=2,
                       IMPRESSION=_F(TOUT='OUI',),);

# TESTS DES DEPLACEMENTS
#------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=RGQBIC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=0.04,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBIC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO2',
                   VALE=0.054389658,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBIC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO3',
                   VALE=0.058152653,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBIC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO4',
                   VALE=0.06,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

# TESTS DES REACTIONS NODALES AUX APPUIS
#------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=RGQBIC,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=53.6846755,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RGQBIC,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO4',
                   VALE=111.6190600,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);


# TESTS D IMPRESSIONS
#----------------------

IMPR_RESU(RESU=_F(RESULTAT =RGQBIC));

TES_R = POST_RELEVE_T(ACTION=(_F(RESULTAT=RGQBIC,
                                 INTITULE='DEPLACEMENT_N1',
                                 NOM_CHAM=('DEPL',),
                                 NOEUD='NO1',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION'),
                              _F(RESULTAT=RGQBIC,
                                 INTITULE='DEPLACEMENT_N2',
                                 NOM_CHAM=('DEPL',),
                                 NOEUD='NO2',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION'),
                              _F(RESULTAT=RGQBIC,
                                 INTITULE='DEPLACEMENT_N3',
                                 NOM_CHAM=('DEPL',),
                                 NOEUD='NO3',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION'),
                              _F(RESULTAT=RGQBIC,
                                 INTITULE='DEPLACEMENT_N4',
                                 NOM_CHAM=('DEPL',),
                                 NOEUD='NO4',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION'),
                               _F(RESULTAT=RGQBIC,
                                 INTITULE='FORCE_N1',
                                 NOM_CHAM=('REAC_NODA',),
                                 NOEUD='NO1',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION'),
                              _F(RESULTAT=RGQBIC,
                                 INTITULE='FORCE_N2',
                                 NOM_CHAM=('REAC_NODA',),
                                 NOEUD='NO2',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION'),
                              _F(RESULTAT=RGQBIC,
                                 INTITULE='FORCE_N3',
                                 NOM_CHAM=('REAC_NODA',),
                                 NOEUD='NO3',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION'),
                              _F(RESULTAT=RGQBIC,
                                 INTITULE='FORCE_N4',
                                 NOM_CHAM=('REAC_NODA',),
                                 NOEUD='NO4',
                                 NOM_CMP = 'DX',
                                 OPERATION = 'EXTRACTION')));
                                 
IMPR_TABLE(TABLE=TES_R);

#-------------------------------------------------
#-------------------------------------------------
# ANALYSE SPECTRALE :
# PARTITION DES COMPOSANTES PRIMAIRE ET SECONDAIRE
# DE LA REPONSE
# COMPOSANTE PRIMAIRE INERTIELLE (SRO ACCE)
# COMPOSANTE SECONDAIRE ENTRAINEMENT (DDS)
#-------------------------------------------------
#-------------------------------------------------
#------------------------------------------
# PQQBC
# REPONSE PRIMAIRE SUR BASE MODALE COMPLETE
#     MODES 1 2
#     GROUP_APPUI QUAD
# REPONSE SECONDAIRE
#     COMB_DEPL_APPUI QUAD
#------------------------------------------

PQQBC=COMB_SISM_MODAL(MODE_MECA=MODES,
                      NUME_MODE=(1,2,),
                      AMOR_REDUIT=0.05,
                      MASS_INER=MASSINER,
                      CORR_FREQ='NON',
                      MULTI_APPUI ='DECORRELE',
                      EXCIT=(_F(NOEUD='NO1',
                                AXE=(1.,0.,0.,),
                                SPEC_OSCI=SPEX1,
                                ECHELLE=1.,),
                             _F(NOEUD='NO4',
                                AXE=(1.,0.,0.,),
                                SPEC_OSCI=SPEX2,
                                ECHELLE=1.,),),
                      COMB_MODE=_F(TYPE='SRSS',),
                      COMB_DEPL_APPUI=_F(LIST_CAS=(1,2,),
                                         TYPE_COMBI='QUAD',),
                      DEPL_MULT_APPUI=(_F(NOM_CAS='DDS NO1',
                                          NUME_CAS=1,
                                          MODE_STAT=MODE_STA,
                                          NOEUD='NO1',
                                          DX=DDS1,),
                                       _F(NOM_CAS='DDS NO4',
                                          NUME_CAS=2,
                                          MODE_STAT=MODE_STA,
                                          NOEUD='NO4',
                                          DX=DDS2,),),
                      GROUP_APPUI=(_F(NOEUD='NO1',),
                                   _F(NOEUD='NO4',),),
                      OPTION=('DEPL','REAC_NODA',),
                      INFO=2,
                      IMPRESSION=_F(TOUT='OUI',),);

# TEST COMPONSANTE PRIMAIRE
#------------------------------------------
# DEPLACEMENT
#----------------------

TEST_RESU(RESU=(_F(RESULTAT=PQQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=0.0,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=PQQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO2',
                   VALE=0.0412562,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=PQQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO3',
                   VALE=0.00660152,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=PQQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   NOEUD='NO4',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=0.0,
                   REFERENCE='ANALYTIQUE',),),);

# REACTIONS AUX APPUIS
#----------------------

TEST_RESU(RESU=(_F(RESULTAT=PQQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO1',
                   VALE=41.2562,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=PQQBC,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   NOEUD='NO4',
