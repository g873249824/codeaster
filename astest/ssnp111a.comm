                   INTERVALLE=_F(JUSQU_A=1.,
                                 NOMBRE=10,),);

CALCNL=STAT_NON_LINE(MODELE=MODEL,
                     CHAM_MATER=MATER,
                     EXCIT=(_F(CHARGE=CONDLIM,),
                            _F(CHARGE=TRAC_F,
                               FONC_MULT=TRAC_FO,),),
                     COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
