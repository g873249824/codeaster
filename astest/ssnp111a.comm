            MAILLE = 'M400',
            NOEUD = 'N2',
            NOM_CMP = 'V1',
            PRECISION = 0.007,
            VALE = 6.16E-02),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'VARI_ELNO_ELGA',
