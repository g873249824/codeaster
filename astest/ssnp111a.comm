            CRITERE = 'ABSOLU',
            PRECISION = 1.4,
            VALE = 0.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'EPSP_ELNO',
            NUME_ORDRE = 10,
            MAILLE = 'M20',
            NOEUD = 'N51',
            NOM_CMP = 'EPXX',
            CRITERE = 'ABSOLU',
            VALE = 0.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'VARI_ELNO_ELGA',
            NUME_ORDRE = 10,
            MAILLE = 'M20',
            NOEUD = 'N51',
            NOM_CMP = 'V1',
            CRITERE = 'ABSOLU',
            VALE = 0.),

          _F(  RESULTAT = CALCNL,
