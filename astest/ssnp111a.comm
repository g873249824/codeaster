DEBUT(CODE=_F(NOM='SSNP111A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

PRE_GIBI()

MAYA=LIRE_MAILLAGE( )

MAYA=DEFI_GROUP(  reuse=MAYA,   MAILLAGE=MAYA,
                             CREA_GROUP_NO=_F(  GROUP_MA = 'A4A1')
                         )

MAYA2=CREA_MAILLAGE(    MAILLAGE=MAYA,
                           MODI_MAILLE=_F(  GROUP_MA = 'S1',
                                         OPTION = 'QUAD8_9')
                       )

MODEL=AFFE_MODELE(    MAILLAGE=MAYA2,
                                VERIF='MAILLE',
                                 AFFE=_F(  TOUT = 'OUI',
                                        PHENOMENE = 'MECANIQUE',
                                        MODELISATION = 'C_PLAN')
                         )

ACIER=DEFI_MATERIAU(        ELAS=_F(  E = 200000.,
                                     NU = 0.,
                                     ALPHA = 1.E-6),
                          ECRO_LINE=_F(  D_SIGM_EPSI = 1930.,
                                      SY = 181.)
                      )

MATER=AFFE_MATERIAU(   MAILLAGE=MAYA2,
                                AFFE=_F(  TOUT = 'OUI',
                                       MATER = ACIER)
                         )

PRES_FO=DEFI_FONCTION(       NOM_PARA='Y',
                                   VALE=( 0.,  0.,
                                         30., 300.,)
                        )

TRAC_FO=DEFI_FONCTION(       NOM_PARA='INST',
                             PROL_DROITE='EXCLU',
                            PROL_GAUCHE='EXCLU',
                                   VALE=( 0.,      0.,
                                          1.,      1., )
                        )

TRAC_F=AFFE_CHAR_MECA_F(        MODELE=MODEL,
                                 PRES_REP=_F(  GROUP_MA = 'A2A3',
                                            PRES = PRES_FO)
                        )

CONDLIM=AFFE_CHAR_MECA(     MODELE=MODEL,DDL_IMPO=(
                               _F(  GROUP_NO = 'A4A1',
                                           DX = 0.),
                             _F(  GROUP_NO = ('A1','A4',),  DY = 0.))
                          )

CAT=DEFI_LIST_REEL(    DEBUT=0.0,
                          INTERVALLE=_F(  JUSQU_A = 1.,     NOMBRE = 10)
                      )

CALCNL=STAT_NON_LINE(
                            MODELE=MODEL,
                        CHAM_MATER=MATER,
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_LINE',
                DEFORMATION = 'PETIT'),
                         INCREMENT=_F(
                LIST_INST = CAT),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_INCR = 1,
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-4),EXCIT=(
                             _F(
                CHARGE = CONDLIM), _F(
                CHARGE = TRAC_F,
                FONC_MULT = TRAC_FO)),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3)
              )

CALCNL=CALC_ELEM( reuse=CALCNL,
                                  RESULTAT=CALCNL,
                                  OPTION=( 'SIEF_ELNO_ELGA',
                                           'VARI_ELNO_ELGA',
                                           'DCHA_ELGA_SIGM',
                                           'RADI_ELGA_SIGM',
                                           'DCHA_ELNO_SIGM',
                                           'RADI_ELNO_SIGM',
                                           'EPSP_ELGA',
                                           'EPSP_ELNO',      )
                     )

