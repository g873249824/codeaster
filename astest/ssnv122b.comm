DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(

                MAILLAGE=MA,
                   

                AFFE=_F(
                TOUT = 'OUI',
                PHENOMENE = 'MECANIQUE',
                MODELISATION = '3D')
              )

ACIER=DEFI_MATERIAU(

               ELAS=_F( E = 200000.,  NU = 0.3)
              )

CHP_MATE=AFFE_MATERIAU(

                MAILLAGE=MA,

                AFFE=_F(
                TOUT = 'OUI',
                MATER = ACIER)
              )

CHR_PLAN=AFFE_CHAR_MECA(

               MODELE=MO,

                DDL_IMPO=_F( NOEUD = ('NO1', 'NO2', 'NO3', 'NO4', 'NO5', 'NO6', 'NO7', 'NO8',), DZ = 0.)
              )

INSTANTS=DEFI_LIST_REEL(

               DEBUT=0.0,INTERVALLE=(

                _F( JUSQU_A = 1.,   NOMBRE = 5),
                _F( JUSQU_A = 2.,   NOMBRE = 1))
              )

INSTANT2=DEFI_LIST_REEL(

               DEBUT=0.0,INTERVALLE=(

                _F( JUSQU_A = 1.,   NOMBRE = 1),
                )
              )

# CALCUL DE LA ROTATION RIGIDE

ROT_Xa = FORMULE(NOM_PARA='INST',VALE='- 1000. * sin(INST*pi/2.)')

ROT_Ya = FORMULE(NOM_PARA='INST',VALE='- 1000. * (1. - cos(INST*pi/2.))')

ROT_X=CALC_FONC_INTERP(
                 FONCTION=ROT_Xa,
                LIST_PARA=INSTANTS,
              NOM_PARA='INST',
              PROL_GAUCHE='EXCLU',
               PROL_DROITE='EXCLU',
                 INTERPOL='NON'
             )

ROT_Y=CALC_FONC_INTERP(
                 FONCTION=ROT_Ya,
                LIST_PARA=INSTANTS,
              NOM_PARA='INST',
              PROL_GAUCHE='EXCLU',
               PROL_DROITE='EXCLU',
                 INTERPOL='NON'
             )

ZERO=DEFI_CONSTANTE(

              VALE=0.
             )

CHR_ROTA=AFFE_CHAR_MECA_F(

               MODELE=MO,DDL_IMPO=(

               _F( NOEUD = ('NO3', 'NO7',),  DX = ZERO,     DY = ZERO),
               _F( NOEUD = ('NO1', 'NO5',),  DX = ROT_X,    DY = ROT_Y))
             )

V=STAT_NON_LINE(
                MODELE=MO,
                CHAM_MATER=CHP_MATE,
                EXCIT=( _F( CHARGE = CHR_PLAN),
                        _F( CHARGE = CHR_ROTA)),
                COMPORTEMENT=_F( RELATION = 'ELAS',
                              DEFORMATION = 'GROT_GDEP'),
                INCREMENT=_F(  LIST_INST = INSTANTS,
                               INST_FIN = 1.),
                NEWTON=_F( MATRICE = 'TANGENTE',
                           REAC_ITER = 1),
                CONVERGENCE=_F(  RESI_GLOB_MAXI = 1.E-3,
                                 ITER_GLOB_MAXI = 50),
                RECH_LINEAIRE=_F( ITER_LINE_MAXI = 4)
              )

# CHARGEMENT DE PRESSION DANS LA CONFIGURATION DEFORMEE


F_CHAR=DEFI_FONCTION(

                  NOM_PARA='INST',
               PROL_GAUCHE='CONSTANT',
                PROL_DROITE='CONSTANT',
                      VALE=(1.,0.,  2.,1.,)
              )
F_CHAR2=DEFI_FONCTION(

                  NOM_PARA='INST',
               PROL_GAUCHE='CONSTANT',
                PROL_DROITE='CONSTANT',
                      VALE=(0.,0.,  1.,1.,)
              )

# FONCTION NON CONSTANTE POUR TESTER UNE PRESSION FONCTION DE L'ESPACE
pres = -26610.31
fpres=DEFI_FONCTION(NOM_PARA='Y',
                    VALE=(
                          0.                        , pres ,
                          1000.                     , 0.,
                          ),
                    PROL_GAUCHE='EXCLU',
                    PROL_DROITE='CONSTANT',
                    );

#CHR_PRES=AFFE_CHAR_MECA(

                 #MODELE=MO,
               #PRES_REP=_F( MAILLE = 'MA2',  PRES = pres)
              #)


CHR_PRES=AFFE_CHAR_MECA_F(

                 MODELE=MO,
               PRES_REP=_F( MAILLE = 'MA2',  PRES = fpres)
              )

LIAISON=AFFE_CHAR_MECA(

                 MODELE=MO,DDL_IMPO=(

                _F( NOEUD = ('NO3', 'NO7',),  DX = 0., DY = 0.),
                _F( NOEUD = ('NO1', 'NO5',),  DY = 0.))
              )
LIAISO2=AFFE_CHAR_MECA(

                 MODELE=MO,DDL_IMPO=(

                _F( NOEUD = ('NO3', 'NO7',),  DX = 0., DY = 0.),
                _F( NOEUD = ('NO1', 'NO5',),  DX = 0.))
              )

VREF=STAT_NON_LINE(
                    MODELE=MO,
                    CHAM_MATER=CHP_MATE,
                    EXCIT=(_F(CHARGE = CHR_PLAN),
                          _F(CHARGE = LIAISO2,),
                          _F(CHARGE = CHR_PRES,TYPE_CHARGE = 'SUIV',FONC_MULT = F_CHAR2)
                          ),
                    COMPORTEMENT=_F(
                                    RELATION = 'ELAS',
                                    DEFORMATION = 'GROT_GDEP'),
                    INCREMENT=_F(LIST_INST = INSTANT2,
                                ),
                    NEWTON=_F(MATRICE = 'TANGENTE',
                              REAC_ITER = 1),
                    CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-5,
                                   ITER_GLOB_MAXI = 20),
                    RECH_LINEAIRE=_F(ITER_LINE_MAXI = 5)
                   )


V=STAT_NON_LINE( reuse=V,
                            MODELE=MO,
                        CHAM_MATER=CHP_MATE,EXCIT=(
                             _F(
                CHARGE = CHR_PLAN), _F(
                CHARGE = LIAISON,
                TYPE_CHARGE = 'DIDI'), _F(
                CHARGE = CHR_PRES,
                TYPE_CHARGE = 'SUIV',
                FONC_MULT = F_CHAR)),
                         ETAT_INIT=_F(
                EVOL_NOLI = V),
                         COMPORTEMENT=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'GROT_GDEP'),
                         INCREMENT=_F(
                LIST_INST = INSTANTS,
                INST_FIN = 2),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-5,
                ITER_GLOB_MAXI = 20),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 5)
              )

IMPR_RESU(RESU=_F(RESULTAT=V, NOM_CHAM='SIEF_ELGA', INST=2.))
IMPR_RESU(RESU=_F(RESULTAT=VREF, NOM_CHAM='SIEF_ELGA', INST=1.))
# POST-TRAITEMENT

V=CALC_CHAMP(
             reuse=V,
                RESULTAT=V,
                  FORCE='REAC_NODA',
             )
VREF=CALC_CHAMP(
             reuse=VREF,
                RESULTAT=VREF,
                  FORCE='REAC_NODA',
             )


