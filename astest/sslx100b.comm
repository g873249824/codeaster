                      NOEUD='N26',
                      NOM_CMP='DY',
                      VALE=-0.0267,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=0.03,),
                   _F(CHAM_GD=DEP1,
                      NOEUD='N4',
                      NOM_CMP='DY',
                      VALE=-0.0267,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=0.03,),
                   _F(CHAM_GD=DEP1,
                      NOEUD='N22',
                      NOM_CMP='DY',
                      VALE=-0.0933,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=0.01,),
                   _F(CHAM_GD=DEP1,
                      NOEUD='N23',
                      NOM_CMP='DY',
                      VALE=-0.0933,
