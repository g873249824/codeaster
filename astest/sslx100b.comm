                   NOEUD='N27',
                   VALE=-10.0,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=0.01,),
                _F(RESULTAT=DEP,
                   NUME_ORDRE=1,
                   NOM_CHAM='EFGE_ELNO',
                   NOM_CMP='MFZ',
                   MAILLE='S2',
                   NOEUD='N29',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   REFERENCE='ANALYTIQUE',
                   PRECISION=0.01,),),);

DEP=CALC_NO(reuse =DEP,
            RESULTAT=DEP,
            OPTION='FORC_NODA',
            TOUT='OUI',);

TABLEAU1=POST_RELEVE_T(ACTION=_F(INTITULE='MOMENT',
                                 GROUP_NO='GRNO1',
                                 RESULTAT=DEP,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE=('DX','DY','DZ',),
                                 MOMENT=('DRX','DRY','DRZ',),
                                 POINT=(0.0,0.0,0.0,),
                                 REPERE='GLOBAL',
                                 OPERATION='MOYENNE',),);

TABLEAU2=POST_RELEVE_T(ACTION=_F(INTITULE='MOMENT',
                                 GROUP_NO='GRNO2',
                                 RESULTAT=DEP,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE=('DX','DY','DZ',),
                                 MOMENT=('DRX','DRY','DRZ',),
                                 POINT=(10.0,0.0,0.0,),
                                 REPERE='GLOBAL',
                                 OPERATION='MOYENNE',),);

TABLEAU3=POST_RELEVE_T(ACTION=_F(INTITULE='MOMENT',
                                 GROUP_NO='GRNO3',
                                 RESULTAT=DEP,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE=('DX','DY','DZ',),
