                                      COEF_MULT_1=1.0,
                                      DDL_2='DRZ',
                                      COEF_MULT_2=-0.5,
                                      COEF_IMPO=F0,),),
                    LIAISON_ELEM=(_F(OPTION='COQ_POU',
