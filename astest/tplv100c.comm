DEBUT(CODE=_F( NOM = 'TPLV100C',NIV_PUB_WEB='INTERNET'))

M=LIRE_MAILLAGE(   )

MO=AFFE_MODELE(        MAILLAGE=M,
                                AFFE=_F(  TOUT = 'OUI',
                                       PHENOMENE = 'THERMIQUE',
                                       MODELISATION = 'AXIS_FOURIER'))

MA=DEFI_MATERIAU(          THER=_F(  LAMBDA = 1.,
                                       RHO_CP = 0.))

CM=AFFE_MATERIAU(      MAILLAGE=M,
                                AFFE=_F(  TOUT = 'OUI',
                                       MATER = MA))

SOURCE=DEFI_CONSTANTE(    VALE=5. )

CH1=AFFE_CHAR_THER(        MODELE=MO,
                           
                        FLUX_REP=_F(  GROUP_MA = 'GRMA6', FLUN = 2.),
                           
                        ECHANGE=_F(  GROUP_MA = 'GRMA7', COEF_H = 2., TEMP_EXT = 2.),
                              TEMP_IMPO=_F(  NOEUD = 'N1', TEMP = 0.))

CH2=AFFE_CHAR_THER_F(        MODELE=MO,
                             
                        SOURCE=_F(  TOUT = 'OUI',  SOUR = SOURCE))

KE=CALC_MATR_ELEM(       MODELE=MO,
                              CHARGE=(CH1, CH2,),
                          CHAM_MATER=CM,
                          MODE_FOURIER=3,
                              OPTION='RIGI_THER'                      )

VE=CALC_VECT_ELEM(       CHARGE=(CH1, CH2,),
                              OPTION='CHAR_THER'                      )

NU=NUME_DDL(          MATR_RIGI=KE                               )

KAS=ASSE_MATRICE(      MATR_ELEM=KE,
                            NUME_DDL=NU                               )

VAS=ASSE_VECTEUR(      VECT_ELEM=VE,
                            NUME_DDL=NU                               )

KAS=FACT_LDLT(      reuse=KAS,   MATR_ASSE=KAS                              )

TEMP=RESO_LDLT(         MATR_FACT=KAS,
                             CHAM_NO=VAS                              )

FLUXNO=CALC_CHAM_ELEM(       MODELE=MO,
                           TEMP=TEMP,
                           MODE_FOURIER=3,
                               OPTION='FLUX_ELNO_TEMP',
                          CHAM_MATER=CM                               )

IMPR_RESU(      MODELE=MO,RESU=(
                           _F(  CHAM_GD = TEMP),
                                _F(  CHAM_GD = FLUXNO)))

TEST_RESU(CHAM_NO=(
   # TEMPERATURES
              
            _F(  CHAM_GD = TEMP,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
           NOEUD = 'N4',       NOM_CMP = 'TEMP',         VALE = 1.),
            _F(  CHAM_GD = TEMP,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
           NOEUD = 'N5',       NOM_CMP = 'TEMP',         VALE = 1.),
            _F(  CHAM_GD = TEMP,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
           NOEUD = 'N6',       NOM_CMP = 'TEMP',         VALE = 1.),
            _F(  CHAM_GD = TEMP,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
           NOEUD = 'N7',       NOM_CMP = 'TEMP',         VALE = 0.25),
            _F(  CHAM_GD = TEMP,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
           NOEUD = 'N12',      NOM_CMP = 'TEMP',         VALE = 0.25)),CHAM_ELEM=(
   # FLUX AUX NOEUDS
              
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N7',     NOM_CMP = 'FLUX',           VALE = -1.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'ABSOLU',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N7',     NOM_CMP = 'FLUY',           VALE = 0.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N7',     NOM_CMP = 'FLUZ',           VALE = 1.5),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M2',     NOEUD = 'N12',    NOM_CMP = 'FLUX',           VALE = -1.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'ABSOLU',  PRECISION = 1.E-6,
  MAILLE = 'M2',     NOEUD = 'N12',    NOM_CMP = 'FLUY',           VALE = 0.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M2',     NOEUD = 'N12',    NOM_CMP = 'FLUZ',           VALE = 1.5),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N4',     NOM_CMP = 'FLUX',           VALE = -2.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'ABSOLU',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N4',     NOM_CMP = 'FLUY',           VALE = 0.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N4',     NOM_CMP = 'FLUZ',           VALE = 3.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N5',     NOM_CMP = 'FLUX',           VALE = -2.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'ABSOLU',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N5',     NOM_CMP = 'FLUY',           VALE = 0.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M1',     NOEUD = 'N5',     NOM_CMP = 'FLUZ',           VALE = 3.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M2',     NOEUD = 'N6',     NOM_CMP = 'FLUX',           VALE = -2.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'ABSOLU',  PRECISION = 1.E-6,
  MAILLE = 'M2',     NOEUD = 'N6',     NOM_CMP = 'FLUY',           VALE = 0.),
            _F(  CHAM_GD = FLUXNO,   CRITERE = 'RELATIF',  PRECISION = 1.E-6,
  MAILLE = 'M2',     NOEUD = 'N6',     NOM_CMP = 'FLUZ',           VALE = 3.))

            )

FIN()
