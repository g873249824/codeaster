# TEST DE PERFORMANCE ANCIENNEMENT NOMME TTLL100E
# CE CAS TEST DE PERFORMANCE EST TRES INSPIRE DE TTLL02B

DEBUT(CODE=_F( NOM = 'YYYY105E',NIV_PUB_WEB='INTERNET'))

TIMPVAR=DEFI_CONSTANTE( 
       NOM_RESU='TEMP',    VALE=100.0E0 )

ACIER=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 1.0))

MAIL=LIRE_MAILLAGE( )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = 'PLAN',
                                      PHENOMENE = 'THERMIQUE'))

TEMP0=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R', 
MAILLAGE=MAIL,
                   AFFE=_F(   TOUT = 'OUI', NOM_CMP = 'TEMP', VALE = 0.0E0)
)

CHTH=AFFE_CHAR_THER_F(MODELE=MOTH,
          TEMP_IMPO=_F( GROUP_NO = 'GRNM14', TEMP = TIMPVAR)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,INTERVALLE=(
       _F(    JUSQU_A = 1.E-3, NOMBRE = 10),
       _F(    JUSQU_A = 1.E-2, NOMBRE = 10),
       _F(    JUSQU_A = 1.E-1, NOMBRE = 10),
       _F(    JUSQU_A = 1.E0,  NOMBRE = 10),
       _F(    JUSQU_A = 2.0,   NOMBRE = 10))
                       )

TEMPE=THER_LINEAIRE(
                MODELE=MOTH,
            TEMP_INIT=_F( CHAM_NO = TEMP0),
            INCREMENT=_F( LIST_INST = LR8),
            CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH),
              SOLVEUR=_F( METHODE = 'LDLT',   RENUM = 'RCMK')
             )

TEMP_1=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP',
             INST=0.1 )

TEST_RESU(
   CHAM_NO=_F( CHAM_GD = TEMP_1, NOEUD = 'N1000',   NOM_CMP = 'TEMP', VALE = 59.04,
   PRECISION = 1.E-3)
       )

FIN()
