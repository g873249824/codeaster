#----------------------------------------------------------------------
#    CAS TEST VPCS PLAQUE CARREE EN TRACTION CISAILLEMENT PROPORTIONNEL
#                  ESSAI AVEC CHARGE EN RAMPE A 88 N (0.17#)
#----------------------------------------------------------------------
#        UNITES : NEWTON , MILLIMETRE , SECONDE (N,MM,S,->MPA)
#----------------------------------------------------------------------
#
#               Y       A                         H = 1 MM (EPAISSEUR)
#               I<----------->                    A = 1 MM
#                                                         2
#                                                 S = 1 MM
#                 NO4         +  NO3
#               O-------------OB
#               +             I                F(T)
#               I             I                 A
#               I             I            88 N I____
#               I             I                 I   /I
#               I             I                 I  / I
#               I             I                 I /  I
#               I  NO1        I  NO2            I/   I
#             ++O-------------OB     - X        O---------> T
#               +             B                       1S
#
#                         SEUIL ELASTIQUE : F = 64.8    MPA (=RO(1-M))
#                          EN TRACTION      U = 3.24E-4 MM
#
#
#----------------------------------------------------------------------
#
DEBUT(CODE=_F(NOM='SSNV102B', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

CARRE=LIRE_MAILLAGE(    )

CPLAN=AFFE_MODELE(            MAILLAGE=CARRE,
                                    AFFE=_F(   PHENOMENE = 'MECANIQUE',
                                            MODELISATION = 'C_PLAN',
                                            TOUT = 'OUI'))

ACIER=DEFI_MATERIAU(            ELAS=_F(   E = 200000.,
                                            NU = .3,
                                            ALPHA = 0.),

                                  TAHERI=_F(   R_0 = 72.,
                                            ALPHA = .3,
                                            M = .1,
                                            A = 312.,
                                            B = 30.,
                                            C1 = -.012,
                                            C_INF = .065,
                                            S = 450.))

MAT=AFFE_MATERIAU(            MAILLAGE=CARRE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER,
                                           ))

LINST=DEFI_LIST_REEL(              DEBUT=0.,INTERVALLE=(
                              _F(  JUSQU_A = .35, NOMBRE = 1),
                                         _F(  JUSQU_A = 1.,  NOMBRE = 20)))

TR_CS=AFFE_CHAR_MECA(             MODELE=CPLAN,DDL_IMPO=(
                                _F(  NOEUD = 'NO3',  DX = 0.),
                                _F(  NOEUD = 'NO2',  DX = 0., DY = 0.)),FORCE_NODALE=(
                            _F(  NOEUD = 'NO3',         FY = 1.),
                            _F(  NOEUD = 'NO1',  FX = -2., FY = -1.),
                            _F(  NOEUD = 'NO4',         FY = -1.)))

COEF=DEFI_FONCTION(
                               NOM_PARA='INST',
                             PROL_DROITE='LINEAIRE',
                            PROL_GAUCHE='LINEAIRE',
                                   VALE=(   0.,  0.,
                                            1.,  44.,   ))

SOLNL=STAT_NON_LINE(
                TITRE='TEST TRAC_CIS MODELE TAHERI EN C_PLAN',
                            MODELE=CPLAN,
                        CHAM_MATER=MAT,
                             EXCIT=_F(
                CHARGE = TR_CS,
                FONC_MULT = COEF),
                         COMP_INCR=_F(
                RELATION = 'VISC_TAHERI',
                DEFORMATION = 'PETIT',
                RESI_INTE_RELA = 1.E-6,
                ITER_INTE_MAXI = 20,
                TOUT = 'OUI',
                ITER_CPLAN_MAXI=5),
                         INCREMENT=_F(
                LIST_INST = LINST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_INCR = 1,
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 20)
              )

SOLNL=CALC_CHAMP(reuse=SOLNL,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=SOLNL)


SOLNL=CALC_CHAMP(reuse=SOLNL,RESULTAT=SOLNL,DEFORMATION=('EPSI_ELNO'))


SOLNL=CALC_CHAMP(reuse=SOLNL,DEFORMATION=('EPSP_ELGA'),RESULTAT=SOLNL)


SOLNL=CALC_CHAMP(reuse=SOLNL,DEFORMATION=('EPSP_ELNO'),RESULTAT=SOLNL)


TEST_RESU(RESU=(_F(INST=1.0,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO4',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.017607115,
                   VALE_REFE=0.01721,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.025000000000000001,
                   MAILLE='MA1',),
                _F(INST=1.0,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='NO4',
                   NOM_CMP='EPXY',
                   VALE_CALC=0.026306177,
                   VALE_REFE=0.025729999999999999,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.025000000000000001,
                   MAILLE='MA1',),
                _F(INST=1.0,
                   POINT=4,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.017167115,
                   VALE_REFE=0.01678,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.025000000000000001,
                   MAILLE='MA1',),
                _F(INST=1.0,
                   POINT=4,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPXY',
                   VALE_CALC=0.025734177,
                   VALE_REFE=0.025149999999999999,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.025000000000000001,
                   MAILLE='MA1',),
                _F(INST=1.0,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELNO',
                   NOEUD='NO4',
                   NOM_CMP='EPXX',
                   VALE_CALC=0.017167115,
                   VALE_REFE=0.01678,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.025000000000000001,
                   MAILLE='MA1',),
                _F(INST=1.0,
                   RESULTAT=SOLNL,
                   NOM_CHAM='EPSP_ELNO',
                   NOEUD='NO4',
                   NOM_CMP='EPXY',
                   VALE_CALC=0.025734177,
                   VALE_REFE=0.025149999999999999,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.025000000000000001,
                   MAILLE='MA1',),
                _F(INST=1.0,
                   RESULTAT=SOLNL,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='NO4',
                   NOM_CMP='V2',
                   VALE_CALC=176.0,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='MA1',),
                ),
          )

FIN()
