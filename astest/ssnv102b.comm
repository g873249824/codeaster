#----------------------------------------------------------------------
#    CAS TEST VPCS PLAQUE CARREE EN TRACTION CISAILLEMENT PROPORTIONNEL
#                  ESSAI AVEC CHARGE EN RAMPE A 88 N (0.17#)
#----------------------------------------------------------------------
#        UNITES : NEWTON , MILLIMETRE , SECONDE (N,MM,S,->MPA)
#----------------------------------------------------------------------
#
#               Y       A                         H = 1 MM (EPAISSEUR)
#               I<----------->                    A = 1 MM
#                                                         2
#                                                 S = 1 MM
#                 NO4         +  NO3
#               O-------------OB
#               +             I                F(T)
#               I             I                 A
#               I             I            88 N I____
#               I             I                 I   /I
#               I             I                 I  / I
#               I             I                 I /  I
#               I  NO1        I  NO2            I/   I
#             ++O-------------OB     - X        O---------> T
#               +             B                       1S
#
#                         SEUIL ELASTIQUE : F = 64.8    MPA (=RO(1-M))
#                          EN TRACTION      U = 3.24E-4 MM
#
#
#----------------------------------------------------------------------
#
DEBUT(CODE=_F(NOM='SSNV102B', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

CARRE=LIRE_MAILLAGE(    )

CPLAN=AFFE_MODELE(            MAILLAGE=CARRE,
                                    AFFE=_F(   PHENOMENE = 'MECANIQUE',
                                            MODELISATION = 'C_PLAN',
                                            TOUT = 'OUI'))

ACIER=DEFI_MATERIAU(            ELAS=_F(   E = 200000.,
                                            NU = .3,
                                            ALPHA = 0.),

                                  TAHERI=_F(   R_0 = 72.,
                                            ALPHA = .3,
                                            M = .1,
                                            A = 312.,
                                            B = 30.,
                                            C1 = -.012,
                                            C_INF = .065,
                                            S = 450.))

MAT=AFFE_MATERIAU(            MAILLAGE=CARRE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER,
                                           ))

LINST=DEFI_LIST_REEL(              DEBUT=0.,INTERVALLE=(
                              _F(  JUSQU_A = .35, NOMBRE = 1),
                                         _F(  JUSQU_A = 1.,  NOMBRE = 20)))

TR_CS=AFFE_CHAR_MECA(             MODELE=CPLAN,DDL_IMPO=(
                                _F(  NOEUD = 'NO3',  DX = 0.),
                                _F(  NOEUD = 'NO2',  DX = 0., DY = 0.)),FORCE_NODALE=(
                            _F(  NOEUD = 'NO3',         FY = 1.),
                            _F(  NOEUD = 'NO1',  FX = -2., FY = -1.),
                            _F(  NOEUD = 'NO4',         FY = -1.)))

COEF=DEFI_FONCTION(
                               NOM_PARA='INST',
                             PROL_DROITE='LINEAIRE',
                            PROL_GAUCHE='LINEAIRE',
                                   VALE=(   0.,  0.,
                                            1.,  44.,   ))

SOLNL=STAT_NON_LINE(
                TITRE='TEST TRAC_CIS MODELE TAHERI EN C_PLAN',
                            MODELE=CPLAN,
                        CHAM_MATER=MAT,
                             EXCIT=_F(
                CHARGE = TR_CS,
                FONC_MULT = COEF),
                         COMP_INCR=_F(
                RELATION = 'VISC_TAHERI',
                DEFORMATION = 'PETIT',
                RESI_INTE_RELA = 1.E-6,
                ITER_INTE_MAXI = 20,
                TOUT = 'OUI',
                ALGO_C_PLAN = 'DEBORST',
                ITER_MAXI_DEBORST=5),
                         INCREMENT=_F(
                LIST_INST = LINST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_INCR = 1,
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 20)
              )

SOLNL=CALC_ELEM( reuse=SOLNL,
                            OPTION=(
                  'SIGM_ELNO',  'VARI_ELNO',),
                          RESULTAT=SOLNL,
              )

SOLNL=CALC_ELEM(  reuse=SOLNL,
                                 RESULTAT=SOLNL,
                                   OPTION='EPSI_ELNO'  )

SOLNL=CALC_ELEM( reuse=SOLNL,
                                    OPTION='EPSP_ELGA',
                                  RESULTAT=SOLNL )

SOLNL=CALC_ELEM( reuse=SOLNL,
                                    OPTION='EPSP_ELNO',
                                  RESULTAT=SOLNL )

TEST_RESU(RESU=(

  _F(  RESULTAT = SOLNL,  NOM_CHAM = 'EPSI_ELNO',  INST = 1.,
         PRECISION = 0.025,
         MAILLE = 'MA1',  NOEUD = 'NO4',  NOM_CMP = 'EPXX',  VALE = 1.721E-2),

  _F(  RESULTAT = SOLNL,  NOM_CHAM = 'EPSI_ELNO',  INST = 1.,
         PRECISION = 0.025,
         MAILLE = 'MA1',  NOEUD = 'NO4',  NOM_CMP = 'EPXY',  VALE = 2.573E-2),

  _F(  RESULTAT = SOLNL,  NOM_CHAM = 'EPSP_ELGA',       INST = 1.,
         PRECISION = 0.025,
         MAILLE = 'MA1',  POINT = 4,    NOM_CMP = 'EPXX',  VALE = 1.678E-2),

  _F(  RESULTAT = SOLNL,  NOM_CHAM = 'EPSP_ELGA',       INST = 1.,
         PRECISION = 0.025,
         MAILLE = 'MA1',  POINT = 4,    NOM_CMP = 'EPXY',  VALE = 2.515E-2),

  _F(  RESULTAT = SOLNL,  NOM_CHAM = 'EPSP_ELNO',       INST = 1.,
         PRECISION = 0.025,
         MAILLE = 'MA1',  NOEUD = 'NO4',  NOM_CMP = 'EPXX',  VALE = 1.678E-2),

  _F(  RESULTAT = SOLNL,  NOM_CHAM = 'EPSP_ELNO',       INST = 1.,
         PRECISION = 0.025,
         MAILLE = 'MA1',  NOEUD = 'NO4',  NOM_CMP = 'EPXY',  VALE = 2.515E-2),

#  RESU:( RESULTAT:SOLNL  NOM_CHAM:'VARI_ELNO'  INST: 1.
#         MAILLE:MA1  NOEUD:NO4 NOM_CMP:'V2' VALE:3.354E-2 )

#  RESU:( RESULTAT:SOLNL  NOM_CHAM:'VARI_ELNO'  INST: 1.
#         MAILLE:MA1  NOEUD:NO4  NOM_CMP:'V9'  VALE:1.677E-2 )

#  RESU:( RESULTAT:SOLNL  NOM_CHAM:'VARI_ELNO'  INST: 1.
#         MAILLE:MA1  NOEUD:NO4 NOM_CMP:'V12' VALE:3.558E-2 )

  _F(  RESULTAT = SOLNL,  NOM_CHAM = 'VARI_ELNO',   INST = 1.,
         MAILLE = 'MA1',   NOEUD = 'NO4', NOM_CMP = 'V2', VALE = 1.760E+2))
        )

FIN()
