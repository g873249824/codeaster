DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

mail=LIRE_MAILLAGE(FORMAT='MED',);

modele=AFFE_MODELE(MAILLAGE=mail,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);

CC=AFFE_CHAR_MECA(MODELE=modele,
                  DDL_IMPO=(_F(GROUP_MA='ENCAST',
                               DX=0.0,
                               DY=0.0,
                               DZ=0.0,),),);

CSEL=AFFE_CHAR_MECA(MODELE=modele,
                  FORCE_FACE=_F(GROUP_MA='EFFORT',
                                FX=-20000000.0,),);

acier=DEFI_MATERIAU(ELAS=_F(E=210000000000.0,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=70000000000.0,
                                 SY=4000000.0,),);

ch_mat=AFFE_MATERIAU(MAILLAGE=mail,
                     AFFE=_F(TOUT='OUI',
                             MATER=acier,),);

fonc=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.0,),);
#list=DEFI_LIST_REEL(DEBUT=0.0,
#                    INTERVALLE=_F(JUSQU_A=1.0,
#                                  NOMBRE=10,),);

list=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=_F(JUSQU_A=1,
                                  NOMBRE=10,),);

resuel=STAT_NON_LINE(MODELE=modele,
                   CHAM_MATER=ch_mat,
                   EXCIT=(_F(CHARGE=CC,),
                          _F(CHARGE=CSEL,
                             FONC_MULT=fonc,),),
                   COMPORTEMENT=_F(RELATION='ELAS',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=list,INST_FIN=0.5,),
                   NEWTON=_F(REAC_INCR=1,
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                  ITER_GLOB_MAXI=10,),
                   CRIT_STAB=_F()
                  );

resuel=STAT_NON_LINE(reuse=resuel,
                   MODELE=modele,
                   CHAM_MATER=ch_mat,
                   EXCIT=(_F(CHARGE=CC,),
                          _F(CHARGE=CSEL,
                             FONC_MULT=fonc,),),
                   ETAT_INIT=_F(EVOL_NOLI=resuel),
                   COMPORTEMENT=_F(RELATION='ELAS',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=list,INST_INIT=0.5,),
                   NEWTON=_F(REAC_INCR=1,
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                  ITER_GLOB_MAXI=10,),
                   CRIT_STAB=_F()
                  );

TEST_RESU(RESU=_F(PARA='CHAR_CRIT',
                  INST=1.0,
                  RESULTAT=resuel,
                  VALE_CALC=-19.065715124965,
                  ),
          )

# ELASTOPLASTIQUE
CS=AFFE_CHAR_MECA(MODELE=modele,
                  FORCE_FACE=_F(GROUP_MA='EFFORT',
                                FX=-10000000.0,),);
resu=STAT_NON_LINE(MODELE=modele,
                   CHAM_MATER=ch_mat,
                   EXCIT=(_F(CHARGE=CC,),
                          _F(CHARGE=CS,
                             FONC_MULT=fonc,),),
                   COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=list,INST_FIN=0.5,),
                   NEWTON=_F(REAC_INCR=1,
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                  ITER_GLOB_MAXI=10,),
                   CRIT_STAB=_F()
                  );

resu=STAT_NON_LINE(reuse=resu,
                   MODELE=modele,
                   CHAM_MATER=ch_mat,
                   EXCIT=(_F(CHARGE=CC,),
                          _F(CHARGE=CS,
                             FONC_MULT=fonc,),),
                   ETAT_INIT=_F(EVOL_NOLI=resu),
                   COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=list,INST_INIT=0.5,),
                   NEWTON=_F(REAC_INCR=1,
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                  ITER_GLOB_MAXI=10,),
                   CRIT_STAB=_F()
                  );
TEST_RESU(RESU=(_F(PARA='CHAR_CRIT',
                   INST=0.4,
                   RESULTAT=resu,
                   VALE_CALC=4.9916878019192,
                   ),
                _F(PARA='CHAR_CRIT',
                   INST=0.5,
                   RESULTAT=resu,
                   VALE_CALC=-1.3186108522121,
                   ),
                ),
          )

TEST_RESU(RESU=(_F(INST=0.4,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=resu,
                   NOM_CHAM='MODE_FLAMB',
                   NOEUD='N1',
                   NOM_CMP='DX',
                   VALE_CALC=0.0,
                   VALE_REFE=0.0,
                   CRITERE='RELATIF',
                   PRECISION=1.E-4,),
                _F(INST=1.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=resu,
                   NOM_CHAM='MODE_FLAMB',
                   NOEUD='N1',
                   NOM_CMP='DX',
                   VALE_CALC=0.0,
                   VALE_REFE=0.0,
                   CRITERE='RELATIF',
                   PRECISION=1.E-4,),
                ),
          )

FIN();
