DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),IMPR_MACRO='OUI',ERREUR=_F(ERREUR_F='EXCEPTION',),
      DEBUG=_F(HIST_ETAPE='OUI'));

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED')

#

MODELE=AFFE_MODELE(
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = ('COQUE',),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'Q4GG'),
                     _F(  GROUP_MA = 'CABLE2',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE')
                            ) )

CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       COQUE=(_F(  GROUP_MA = 'COQUE',
                               EPAIS = 1.,
                               VECTEUR = (0.,0.,1.),
                               ),
                               ),
                       BARRE=_F(  GROUP_MA = 'CABLE2',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 1.5E-2, ))
                               )


BETON = DEFI_MATERIAU(ELAS=_F(
                    E = 3E10,
                    NU = 0.,
                    RHO = 2500,
                   ),
                    BPEL_BETON=_F()
                  );


ACIER = DEFI_MATERIAU(ELAS=_F(
                    E = 2E11,
                    NU = 0.,
                    RHO = 500,
                   ),
                   BPEL_ACIER=_F(
                              FROT_COURB = 0.,
                              FROT_LINE = 0.)
                  );

CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAILLAGE,AFFE=(
                       _F(  GROUP_MA = ('COQUE',),
                              MATER = BETON),
                       _F(  GROUP_MA = 'CABLE2',
                              MATER = ACIER)
                              )
                              )


CL=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                      DDL_IMPO=(
                           _F(  GROUP_NO=('COTE1','COTEBLOC'),
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,
                               DRX = 0.,
                               DRY = 0.,
                               DRZ = 0.),
                            _F(  GROUP_NO=('COTE2',),
                               DRX = 0.,
                               DRY = 0.,
                               DRZ = 0.),
                               ),
                        INFO=1 )

CABLEPR=DEFI_CABLE_BP(MODELE=MODELE,
                      CHAM_MATER=CHMATER,
                      CARA_ELEM=CARAELEM,
                      GROUP_MA_BETON='COQUE',
                      DEFI_CABLE=_F(
                            GROUP_MA = 'CABLE2',
                            GROUP_NO_ANCRAGE = ( 'ANCR1',  'ANCR2', ),),
                            TYPE_ANCRAGE = ( 'ACTIF',   'ACTIF',  ),
                            TENSION_INIT=0.,
                            RECUL_ANCRAGE=0. )

#

GLIS=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                      RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'NON',
                               RELA_CINE = 'OUI',
                               TYPE_EPX = 'GLIS'),
                      INFO=1 )


TRAC=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                DDL_IMPO=_F(DY = -3., GROUP_NO = 'COTE2',),
                        INFO=1 )





FONC2=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,    0.0,
                           2e-3,   1.0,
                           1,  1.0,
                           ),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     );

TAB1 = CO('TAB1')

U1 = CALC_EUROPLEXUS(
   MODELE=MODELE,
   CHAM_MATER=CHMATER,
   CARA_ELEM=CARAELEM,
   COMPORTEMENT =(_F(
                  RELATION = 'ELAS',
                  GROUP_MA = ('CABLE2','COQUE'),
                 ),
              ),
   EXCIT=( _F(CHARGE=TRAC,FONC_MULT=FONC2,),
          _F(CHARGE=CL,),
           _F(CHARGE=GLIS,),
          ),
   LANCEMENT ='OUI',
   CALCUL = _F(TYPE_DISCRETISATION  ='UTIL',
               INST_INIT = 0,
               INST_FIN  = 1e-2,
               NMAX      = 2000000,
               PASFIX    = 1e-6,
               ),
   #OBSERVATION = _F(PAS_NBRE=1,
                    #TOUT_GROUP_NO = 'OUI',
                    #TOUT_GROUP_MA = 'OUI',
                    #NOM_CHAM = ('DEPL','VITE','ACCE'),
                    #),
   ARCHIVAGE   = _F(PAS_NBRE=200,),
   PAS_NBRE_COURBE=100,
   TABLE_COURBE    = TAB1,
   COURBE = (
           _F(
           NOM_CHAM = 'DEPL',
           NOM_CMP  = 'DX',
           GROUP_NO = 'NY10',
           NOM_COURBE = 'DX_NY10',
           ),
           _F(
           NOM_CHAM = 'DEPL',
           NOM_CMP  = 'DY',
           GROUP_NO = 'NY10',
           NOM_COURBE = 'DY_NY10',
           ),
   ),
   );

#IMPR_TABLE(TABLE=TAB1)
#IMPR_RESU(RESU=_F(RESULTAT=U1,NOM_CHAM=('SIEF_ELGA'), GROUP_MA='MCAY10'))


          
TEST_RESU(RESU=(_F(INST = 0.002,
                  RESULTAT=U1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DX',
                  VALE_CALC=1.58933179659,
                  VALE_REFE=1.58933E+00,
                  REFERENCE = 'SOURCE_EXTERNE',
                  PRECISION = 2E-6,
                  ),
                _F(INST = 0.002,
                  RESULTAT=U1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DY',
                  VALE_CALC=-0.12524285319,
                  VALE_REFE=-1.25243E-01,
                  REFERENCE = 'SOURCE_EXTERNE',
                  PRECISION = 2E-6,
                  ),
                )
          )

TEST_RESU(RESU=(_F(INST = 0.006,
                  RESULTAT=U1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DX',
                  VALE_CALC=2.17662703359,
                  VALE_REFE=2.17663E+00,
                  REFERENCE = 'SOURCE_EXTERNE',
                  PRECISION = 2E-6,
                  ),
                _F(INST = 0.006,
                  RESULTAT=U1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DY',
                  VALE_CALC=-0.277226520902,
                  VALE_REFE=-2.77227E-01,
                  REFERENCE = 'SOURCE_EXTERNE',
                  PRECISION = 2E-6,
                  ),
                )
          )

TEST_RESU(RESU=(_F(INST = 0.01,
                  RESULTAT=U1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DX',
                  VALE_CALC=2.52102121849,
                  VALE_REFE=2.52102E+00,
                  REFERENCE = 'SOURCE_EXTERNE',
                  PRECISION = 2E-6,
                  ),
                _F(INST = 0.01,
                  RESULTAT=U1,
                  NOM_CHAM='DEPL',
                  GROUP_NO='NY10',
                  NOM_CMP='DY',
                  VALE_CALC=-0.394617617956,
                  VALE_REFE=-3.94618E-01,
                  REFERENCE = 'SOURCE_EXTERNE',
                  PRECISION = 2E-6,
                  ),
                )
          )

FIN()
