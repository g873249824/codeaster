                       FORCE_NODALE=_F(NOEUD='C',
                                       FX=100.0,),);

RIGIELEM=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=RIGMAS,
                        CHARGE=CONDLIM,);

MASSELEM=CALC_MATR_ELEM(OPTION='MASS_MECA',
                        MODELE=MODELE,
                        CHAM_MATER=CHAMPMAT,
                        CARA_ELEM=RIGMAS,
                        CHARGE=CONDLIM,);

