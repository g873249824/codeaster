DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),
    DEBUG=_F(SDVERI='OUI')
)

MAIL_F=LIRE_MAILLAGE(FORMAT="ASTER",VERI_MAIL=_F(VERIF='OUI'),);


MASEC=LIRE_MAILLAGE(UNITE=21,
                    FORMAT='MED',);

GF=DEFI_GEOM_FIBRE(SECTION=_F(GROUP_FIBRE='TRAN',
                              TOUT_SECT='OUI',
                              MAILLAGE_SECT=MASEC,
                              COOR_AXE_POUTRE=(0.,0.,),),);

MOD_F=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='POU_D_TGM',),
                  MAILLAGE=MAIL_F,);

CAR_ELE=AFFE_CARA_ELEM(MODELE=MOD_F,INFO = 2,
                       POUTRE=_F(GROUP_MA='TOUT_ELT',
                                 SECTION='RECTANGLE',
                                 VALE=(0.15,0.025),
                                 CARA=('HZ','HY'),
                                 #SECTION='GENERALE',
                                 #VALE=(3.74999999999999986E-003,7.02438354492187556E-006,
                                 #1.95121765136718780E-007,
                                 #1.20000E+00,1.20000E+00,6.99224E-07,2.46127E-02),
                                 #CARA=('A','IY','IZ','AY','AZ','JX','RT'),
                                 ),
                       GEOM_FIBRE=GF,
                       MULTIFIBRE=_F(GROUP_MA='TOUT_ELT',
                                     GROUP_FIBRE='TRAN',),);

MAT_F=DEFI_MATERIAU(ECRO_LINE=_F(D_SIGM_EPSI=0.0,
                                 SY=2.35000000E8,),
                    ELAS=_F(NU=0.3,
                            E=2.E11,),);

COMPPMF=DEFI_COMPOR(GEOM_FIBRE= GF,MATER_SECT= MAT_F,
                  MULTIFIBRE=(_F(GROUP_FIBRE='TRAN',MATER= MAT_F, RELATION = 'VMIS_ISOT_LINE'),));


CHAM_MAT=AFFE_MATERIAU(MODELE=MOD_F,
                       AFFE=_F(MATER=MAT_F,
                               TOUT='OUI',),
                       AFFE_COMPOR=_F(GROUP_MA= 'TOUT_ELT',  COMPOR = COMPPMF),
                       MAILLAGE=MAIL_F,);

CHAR0=AFFE_CHAR_MECA(MODELE=MOD_F,
                     DDL_IMPO=(_F(DZ=0.0,
                                  DX=0.0,
                                  DY=0.0,
                                  GROUP_NO='NO1',
                                  DRZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,),
                               _F(GROUP_NO='NO2',
                                  DRZ=0.0,),),
                     LIAISON_DDL=_F(DDL=('DX','DY'),
                                    COEF_MULT=(1.0,-4.915157031),
                                    COEF_IMPO=0.0,
                                    GROUP_NO=('NO2','NO2'),),);
PRES_LIM=AFFE_CHAR_MECA(FORCE_POUTRE=_F(TOUT='OUI',
                                        FY=-7808.5780959,
                                        FX=-3.838038753E4,),
                        MODELE=MOD_F,);
LIS_INST=DEFI_LIST_REEL(INTERVALLE=(_F(JUSQU_A=5.0,
                                       NOMBRE=10,),
                                    _F(JUSQU_A=8.5,
                                       NOMBRE=20,),
                                    _F(JUSQU_A=25.0,
                                       NOMBRE=30,),),
                        DEBUT=0.0,);
RESULTAT=STAT_NON_LINE(CHAM_MATER=CHAM_MAT,
                       MODELE=MOD_F,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=30,
                                      RESI_GLOB_RELA=1.E-06,),
                       COMPORTEMENT=_F(RELATION='MULTIFIBRE',
                                    ITER_INTE_MAXI=30,),
                       INCREMENT=_F(LIST_INST=LIS_INST,INST_FIN=22.8),
                       PILOTAGE=_F(TYPE='DDL_IMPO',
                                   GROUP_NO='NO2',
                                   NOM_CMP='DX',
                                   COEF_MULT=-154.4086,),
                       CARA_ELEM=CAR_ELE,
                       NEWTON=_F(REAC_ITER=1,
                                 MATRICE='TANGENTE',),
                       EXCIT=(_F(CHARGE=CHAR0,),
                              _F(CHARGE=PRES_LIM,
                                 TYPE_CHARGE='FIXE_PILO',),),);
TABLE_DX=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='TAB_DX',
                                 GROUP_NO='NO2',
                                 NOM_CMP='DX',
                                 RESULTAT=RESULTAT,
                                 NOM_CHAM='DEPL',
                                 TOUT_ORDRE='OUI',),);
IMPR_TABLE(TABLE=TABLE_DX,);

TABLE_DY=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='TAB_DY',
                                 GROUP_NO='NO2',
                                 NOM_CMP='DY',
                                 RESULTAT=RESULTAT,
                                 NOM_CHAM='DEPL',
                                 TOUT_ORDRE='OUI',),);
IMPR_TABLE(TABLE=TABLE_DY,);

DEPX=RECU_FONCTION(RESULTAT=RESULTAT,
                   GROUP_NO='NO2',
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   TOUT_ORDRE='OUI',);
DEPY=RECU_FONCTION(RESULTAT=RESULTAT,
                   GROUP_NO='NO2',
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   TOUT_ORDRE='OUI',);
ETA=RECU_FONCTION(RESULTAT=RESULTAT,
                  NOM_PARA_RESU='ETA_PILOTAGE',
                  TOUT_ORDRE='OUI',);
IMPR_FONCTION(COURBE=_F(FONCTION=DEPX,),
            UNITE=25, UNITE_DIGR=26,
            FORMAT='AGRAF',
            TITRE='Depx en fonction du temps',);
IMPR_FONCTION(COURBE=_F(FONCTION=ETA,),
            UNITE=27, UNITE_DIGR=28,
            FORMAT='AGRAF',
            TITRE='Eta en fonction du temps',);
IMPR_FONCTION(COURBE=_F(FONCTION=DEPY,),
            UNITE=29, UNITE_DIGR=30,
            FORMAT='AGRAF',
            TITRE='Depy en fonction du temps',);
DEPL=CREA_CHAMP(NOM_CHAM='DEPL',
                TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                NUME_ORDRE=1,
                RESULTAT=RESULTAT,);

TEST_RESU(RESU=(_F(NUME_ORDRE=10,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESULTAT,
                   VALE_CALC=1.084212755427,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.14000000000000001,),
                _F(NUME_ORDRE=12,
                   PARA='ETA_PILOTAGE',
                   RESULTAT=RESULTAT,
                   VALE_CALC=1.089092178,
                   VALE_REFE=1.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.14000000000000001,),
                ),
          )

FIN();
