# CAS TEST  SSNV505D
# MODELE
# MAILLAGE : ELEMENT 3D HEXA A 8 NOEUDS
# UNITES   : NEWTON , MILLIMETRE , SECONDE (N,MM,S)
#
#


DEBUT(CODE=_F( NOM = 'SSNV505D',NIV_PUB_WEB='INTERNET'));

# ---------------------  MAILLAGE ----------------
MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'),);

# -------------------- MODELE ------------------
MODE=AFFE_MODELE(MAILLAGE=MAILL1,
                 VERIF='MAILLE',
                 AFFE=_F(GROUP_MA='STRUCT',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='3D',),);

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                   MAILLAGE=MAILL1,
                   ORIE_PEAU_3D=_F(GROUP_MA=('SMAI','SESC'),),
                   );

# ------------- DEFINITON DU MATERIAU -----------
MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,
                              NU=0.3333,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=_F(GROUP_MA='STRUCT',
                         MATER=MPOUTRE,),);

# ---------- CONDITIONS AUX LIMITES --------------
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='SA',
                                     DX=0.0,DY=0.0,DZ=0.0,),
                                  _F(GROUP_NO='SB',
                                     DX=0.0,DY=0.0,DZ=0.0,),
                                  _F(GROUP_NO='STRUCT',
                                     DZ=0.0,),
                                  ),);

CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=(_F(GROUP_NO=('LCC1'),
                                DY=-790.,),
                             ),);

CH2  = DEFI_CONTACT(MODELE         = MODE,
                    FORMULATION    = 'CONTINUE',
                    ALGO_RESO_CONT = 'POINT_FIXE',
                    ALGO_RESO_GEOM = 'POINT_FIXE',
                    REAC_GEOM      = 'CONTROLE',
                    NB_ITER_GEOM   = 3,
                    ZONE =(
                           _F(
                              GROUP_MA_MAIT='SMAI',
                              GROUP_MA_ESCL='SESC',
                             ),
                          ),
                   );

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=50,),),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=25,),),);

U=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  = CH2,
                COMP_ELAS=_F(RELATION='ELAS',
                             DEFORMATION='GROT_GDEP',
                             GROUP_MA='STRUCT',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=1.,
                             ),
                NEWTON=_F(REAC_ITER=1,
                          ),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
                ARCHIVAGE=_F(LIST_INST=L_ARCH),
                INFO=1,);

U=CALC_CHAMP(reuse =U,
          RESULTAT=U,
          FORCE='FORC_NODA',


          );

TAB_REAC=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('SA','SB'),
                                 RESULTAT=U,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);
TAB_DEPL=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO='A1',
                                 RESULTAT=U,
                                 NOM_CHAM='DEPL',
                                 NOM_CMP='DY',
                                 OPERATION='EXTRACTION',),);

TEST_TABLE(
           VALE_CALC=436.995,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.20000000000000001,),
           )

TEST_TABLE(
           VALE_CALC=1668.3399999999999,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.40000000000000002,),
           )

TEST_TABLE(REFERENCE='NON_DEFINI',
           PRECISION=1.5E-3,
           VALE_CALC= 3.26452976E+03,
           VALE_REFE=3264.54,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.59999999999999998,),
           )

TEST_TABLE(REFERENCE='NON_DEFINI',
           PRECISION=2.E-3,
           VALE_CALC= 2.74197587E+03,
           VALE_REFE=2737.1599999999999,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.80000000000000004,),
           )

TEST_TABLE(
           VALE_CALC=3095.2399999999998,
           TOLE_MACHINE=1.E-3,
           NOM_PARA='DY',
           TABLE=TAB_REAC,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

FIN();
