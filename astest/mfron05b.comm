import os

os.system("mfront --obuild hay01a.mfront --interface=aster")
os.system("cp src/libAsterBehaviour.so Hayhurst.so")
os.system("mfront --obuild hayi01a.mfront --interface=aster")
os.system("cp src/libAsterBehaviour.so Hayhurstimplicit.so")

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));


#temperature de travail en degres celsius
Tdegc=600;

#ATTENTION : f en h-1 et non en %/h
feps=5.2e7;
Qeps=3e5;
aK = -3.3e-2;
bK = 38.5;
fA = 9e4;
QA = 2e5;
f0 = 3.5e-2;
Q0 = -4.85e4;

########################################
######### DEFINITION DU CHARGEMENT #####
########################################

#niveau de contrainte nominal
sigmap = 160;


#  temps final
tref=2000;

#######################################################

#quelques calculs intermediaires
ep0val=feps*exp(-Qeps/(8.314*(Tdegc+273.)));
Kcoef=aK*(Tdegc+273.)+bK;
A0 = fA*exp(-QA/(8.314*(Tdegc+273.)));
sig0val = f0*exp(-Q0/(8.314*(Tdegc+273.)));
h1=30000.
h2=-280.
H1star=0.33
H2star=1.0

#alphaD=0.18439
alphaD=0.5
delta1=1.0
delta2=0.



MATFRONT=DEFI_MATERIAU(
                      ELAS=_F(E=145000., NU=0.3,ALPHA=1e-5,),
                      MFRONT=_F( LISTE_COEF = (145000. , 0.3, Kcoef,ep0val,sig0val,h1,h2,H1star,H2star,A0,alphaD,delta1,delta2,0.,),),
                      )
                               

char=DEFI_FONCTION(NOM_PARA='INST',VALE=(0,0,
                         100.,sigmap,
                         100000000,sigmap,
                         ),);


lalist=DEFI_LIST_REEL(DEBUT=0,
                      INTERVALLE=(_F(JUSQU_A=tref,
                                     NOMBRE=2000,),),);

listtps = DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST=lalist,),
                         ECHEC=_F(SUBD_NIVEAU=10,
                                  SUBD_METHODE='MANUEL',
                                  SUBD_PAS=10,
                                  EVENEMENT='ERREUR',
                                  SUBD_PAS_MINI=0.0,
                                  ACTION='DECOUPE'),
                                  )

listarch=DEFI_LIST_REEL(DEBUT=0,
                      INTERVALLE=(_F(JUSQU_A=tref,NOMBRE=2000,),),);

###########
# explicite
###########

resuexpl=SIMU_POINT_MAT(MATER=MATFRONT,INFO=1,
                     SIGM_IMPOSE=_F(SIYY=char,),
                     COMPORTEMENT=_F(RELATION='MFRONT',
                                     NOM_ROUTINE='asterhayhurst',
                                     LIBRAIRIE='Hayhurst.so',
                                  #RESI_INTE_RELA=1.E-8,
                                  ),
                     INCREMENT=_F(LIST_INST=listtps,),
                     NEWTON=_F(MATRICE='ELASTIQUE'),
                     CONVERGENCE=_F(ITER_GLOB_MAXI=50),
                     ARCHIVAGE=_F(LIST_INST=listarch),
                     );

IMPR_TABLE(TABLE=resuexpl, UNITE=37)

epYYexpl=RECU_FONCTION(TABLE=resuexpl,
                   PARA_X='INST',
                   PARA_Y='EPYY',
                   INTERPOL='LIN');

endoexpl=RECU_FONCTION(TABLE=resuexpl,
                   PARA_X='INST',
                   PARA_Y='V16',
                   INTERPOL='LIN');


#fluage secondaire comparaison ssnv225a
# valeurs eloignees car test en HPP, pour tester SIMU_POINT_MAT SUPPORT=POINT+MFRONT

# voici les resultats en GDEF
# OK   AUTRE_ASTER      XXXX             0.0210           0.0202           3.9%
# OK   AUTRE_ASTER      XXXX             0.0323           0.0309           4.4%
# OK   AUTRE_ASTER      XXXX             6.65E-06         6.57E-06         1.3%

tsec=2000
TEST_FONCTION(VALEUR=_F(VALE_CALC=0.01797769,
                        VALE_REFE=0.020968,
                        REFERENCE='AUTRE_ASTER',
                        PRECISION=0.15,
                        VALE_PARA=2000,
                        NOM_PARA='INST',
                        FONCTION=epYYexpl,
                        ),
              )

TEST_FONCTION(VALEUR=_F(VALE_CALC=0.02861034 ,
                        VALE_REFE=0.03230,
                        REFERENCE='AUTRE_ASTER',
                        VALE_PARA=2000,
                        PRECISION=0.12,
                        NOM_PARA='INST',
                        FONCTION=endoexpl,
                        ),
              )


depsexpl = CALC_FONCTION( DERIVE=_F( FONCTION= epYYexpl ),)


tvit = 1520.

TEST_FONCTION(VALEUR=_F(VALE_CALC=5.268210E-06,
                        VALE_REFE=6.653918E-06,
                        REFERENCE='AUTRE_ASTER',
                        PRECISION=0.21,
                        VALE_PARA=1520.0,
                        NOM_PARA='INST',
                        FONCTION=depsexpl,
                        ),
              )



###########
# implicite
###########

resuimpl=SIMU_POINT_MAT(MATER=MATFRONT,INFO=1,
                     SIGM_IMPOSE=_F(SIYY=char,),
                     COMPORTEMENT=_F(RELATION='MFRONT',
                                     NOM_ROUTINE='asterimplicithayhurst',
                                     LIBRAIRIE='Hayhurstimplicit.so',
                                  #RESI_INTE_RELA=1.E-8,
                                  ),
                     INCREMENT=_F(LIST_INST=listtps,),
                     NEWTON=_F(REAC_ITER=1),
                     ARCHIVAGE=_F(LIST_INST=listarch),
                     );

IMPR_TABLE(TABLE=resuimpl, UNITE=38)

epYYimpl=RECU_FONCTION(TABLE=resuimpl,
                   PARA_X='INST',
                   PARA_Y='EPYY',
                   INTERPOL='LIN');

endoimpl=RECU_FONCTION(TABLE=resuimpl,
                   PARA_X='INST',
                   PARA_Y='V7',
                   INTERPOL='LIN');


#fluage secondaire comparaison ssnv225a
# valeurs eloignees car test en HPP, pour tester SIMU_POINT_MAT SUPPORT=POINT+MFRONT

# voici les resultats en GDEF
# OK   AUTRE_ASTER      XXXX             0.0210           0.0202           3.9%
# OK   AUTRE_ASTER      XXXX             0.0323           0.0309           4.4%
# OK   AUTRE_ASTER      XXXX             6.65E-06         6.57E-06         1.3%

tsec=2000
TEST_FONCTION(VALEUR=_F(VALE_CALC=0.01797669,
                        VALE_REFE=0.020968,
                        REFERENCE='AUTRE_ASTER',
                        PRECISION=0.15,
                        VALE_PARA=2000,
                        NOM_PARA='INST',
                        FONCTION=epYYimpl,
                        ),
              )

TEST_FONCTION(VALEUR=_F(VALE_CALC=0.02861066 ,
                        VALE_REFE=0.03230,
                        REFERENCE='AUTRE_ASTER',
                        VALE_PARA=2000,
                        PRECISION=0.12,
                        NOM_PARA='INST',
                        FONCTION=endoimpl,
                        ),
              )


depsimpl = CALC_FONCTION( DERIVE=_F( FONCTION= epYYimpl ),)


tvit = 1520.

TEST_FONCTION(VALEUR=_F(VALE_CALC=5.267050E-06,
                        VALE_REFE=6.653918E-06,
                        REFERENCE='AUTRE_ASTER',
                        PRECISION=0.21,
                        VALE_PARA=1520.0,
                        NOM_PARA='INST',
                        FONCTION=depsimpl,
                        ),
              )

#pilote='INTERACTIF'
pilote=''

IMPR_FONCTION(
            FORMAT='XMGRACE',
            COURBE=(
            _F(FONCTION=epYYexpl),
            _F(FONCTION=epYYimpl),
            ),
            UNITE=41,
            PILOTE=pilote,
            TITRE='Espilon = f(INST)');

IMPR_FONCTION(
            FORMAT='XMGRACE',
            COURBE=(
            _F(FONCTION=endoexpl,),
            _F(FONCTION=endoimpl,),
            ),
            PILOTE=pilote,
            UNITE=42,
            TITRE='endo = f(INST)');

IMPR_FONCTION(
            FORMAT='XMGRACE',
            COURBE=(
            _F(FONCTION=depsexpl,),
            _F(FONCTION=depsimpl,),
            ),
            UNITE=43,
            PILOTE=pilote,
            TITRE='depsdt = f(INST)');

FIN();
