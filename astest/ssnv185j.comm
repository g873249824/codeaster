DEBUT(CODE=_F(NOM='SSNV185J',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),);


#----------------------------------------------
#           MAILLAGE  hexa_5_30_50
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(GROUP_MA='SURFSUP',),);

#----------------------------------------------
#                   MODELE
#----------------------------------------------

MODELE = [None]*5

MODELE[0]=AFFE_MODELE(MAILLAGE=MAILLAG2,
                      AFFE=(_F(GROUP_MA=('VOL',),
                               PHENOMENE='MECANIQUE',
                               MODELISATION='3D_XFEM',),
                            _F(GROUP_MA=('SURFINF','SURFSUP',),
                               PHENOMENE='MECANIQUE',
                               MODELISATION='3D',),),);  
                              
MAILLAG2=MODI_MAILLAGE(reuse =MAILLAG2,
                       MAILLAGE=MAILLAG2,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFSUP','SURFINF',),),
                       MODELE=MODELE[0],);                          

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=205000.0E6
nu=0.
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELE[0],
                       AFFE=_F(TOUT = 'OUI',
                                MATER=ACIER,
                                TEMP_REF=0.0,),
                             );

#----------------------------------------------
#                   FISSURE
#----------------------------------------------

# PARAMETRES DE LA FISSURE
a=4.9
b=10.
LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-15.');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='b-a-Y');

FISS = [None]*5

FISS[0]=DEFI_FISS_XFEM(MODELE=MODELE[0],  
                       DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                       GROUP_MA_ENRI='VOL',
                       ORIE_FOND=_F(PFON_INI= (0. ,5. ,15. ),
                                    VECT_ORIE=(0. ,0. ,1. ),
                                    PT_ORIGIN=(0. ,20. ,15. ),),
                       CONTACT=_F(ALGO_LAGR='NON',),);

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

PRES=-1.E6 
CH1=AFFE_CHAR_MECA(MODELE=MODELE[0],
                   DDL_IMPO=(_F(NOEUD='N3751',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(NOEUD='N9276',DZ=0.0,),
                             _F(NOEUD='N3760',DX=0.0,DZ=0.0,),),);

CH2=AFFE_CHAR_MECA(MODELE=MODELE[0],
                   PRES_REP=_F(GROUP_MA=('SURFSUP','SURFINF',),PRES=PRES,),);

          
#----------------------------------------------
#         PROPAGATION SUR 3 PAS DE TEMPS
#----------------------------------------------

CHXFEM = [None]*5
RESU = [None]*5
SIF = [None]*5

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=3.0,NOMBRE=3,),);
RI = 1.
RS = 3.

for i in range(1,4) :

   MODELE[i]=MODI_MODELE_XFEM(MODELE_IN=MODELE[0],FISSURE=FISS[i-1],INFO=1,);
     
   CHXFEM[i]=AFFE_CHAR_MECA(MODELE=MODELE[i],LIAISON_XFEM='OUI',INFO=1,);

   RESU[i]=STAT_NON_LINE(MODELE=MODELE[i],
                         CHAM_MATER=CHAMPMA1,              
                         EXCIT=(_F(CHARGE=CHXFEM[i],),                                           
                                _F(CHARGE=CH1,),
                                _F(CHARGE=CH2,),),  
                         COMP_ELAS=_F(RELATION='ELAS',
                                      GROUP_MA='VOL',),
                         INCREMENT=_F(LIST_INST=L_INS1,
                                      INST_FIN=1.0,),
                         SOLVEUR=_F(METHODE='MUMPS',
                                    PCENT_PIVOT=250,),
                         NEWTON=_F(REAC_ITER=1,),
                         ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                         INFO=1,);

   SIF[i]=CALC_G_LOCAL_T(MODELE=MODELE[i],
                         CHAM_MATER=CHAMPMA1,
                         RESULTAT=RESU[i],
                         SYME_CHAR='SANS',
                         COMP_ELAS=_F(RELATION='ELAS',),
                         OPTION='CALC_K_G',
                         FISSURE=FISS[i-1],
                         LISSAGE_THETA='LAGRANGE',
                         LISSAGE_G='LAGRANGE',
                         R_INF=RI,
                         R_SUP=RS,
                         INFO=1,);

   IMPR_TABLE(TABLE=SIF[i],);

   if ( i != 3 ) :
      FISS[i]=PROPA_XFEM(MODELE=MODELE[i],
                         TABLE=SIF[i],
                         LOI_PROPA=_F(LOI='PARIS',
                                      C=1.,
                                      M=1.),
                         GROUP_MA_ENRI='VOL',
                         INFO=1,);

#----------------------------------------------
#         POST TRAITEMENT
#----------------------------------------------
#       SOLUTIONS ANALYTIQUES DE K1 :
# POUR LE CHARGEMENT 1 : VOIR CHABOCHE P.53
b=10
afiss=[4.9 , 5.1 , 5.3 ]
coef1 = [None]*5
fab = [None]*5
K1REF = [None]*5

for i in range(1,4) :
   coef1[i]=(pi*afiss[i-1])/(2.*b)
   fab[i]=sqrt(tan(coef1[i])/coef1[i])*(0.752+0.37*(1.-sin(coef1[i]))**3+2.02*afiss[i-1]/b)/cos(coef1[i])
   K1REF[i]=-PRES*sqrt(pi*afiss[i-1])*fab[i]

   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MAX',               
              VALE=K1REF[i],
              CRITERE='RELATIF',
              PRECISION=0.02,
              REFERENCE='ANALYTIQUE',);

   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MIN',               
              VALE=K1REF[i],
              CRITERE='RELATIF',
              PRECISION=0.05,
              REFERENCE='ANALYTIQUE',);


#----------------------------------------------
#         EDITION DE FICHIERS GMSH
#----------------------------------------------
DEPL = [None]*4
MAXFEM = [None]*4

DEFI_FICHIER(UNITE=30,);
IMPR_RESU(FORMAT='GMSH',
          UNITE=30,
          RESU=_F(MAILLAGE=MAILLAG2,),);

for i in range(1,4) :
   MAXFEM[i]=POST_MAIL_XFEM(MODELE=MODELE[i],);

   DEPL[i]=POST_CHAM_XFEM(MAILLAGE=MAXFEM[i],
                          MODELE=MODELE[i],
                          RESULTAT=RESU[i],
                          NOM_CHAM='DEPL',);

   DEFI_FICHIER(UNITE=30+i,);
   IMPR_RESU(FORMAT='GMSH',
             UNITE=30+i,
             RESU=_F(RESULTAT=DEPL[i],),);


FIN();
