# PRESSION DE CONSOLIDATION P0 = 50KPA


DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'))

GammaRef=2.e-4
n=0.78
P0 = - 5.E4
N=0.4
PREF=-1.E6

# modules mecaniques [kPa]
K=516.2E6*(P0/PREF)**N
G=238.2E6*(P0/PREF)**N
YOUNG = 9.*K*G /(3.*K+G)
POISSON = (3.*K-2.*G) /(6.*K+2.*G)

MATE=DEFI_MATERIAU(ELAS=_F(E=YOUNG, 
                              NU=POISSON,),
                   Iwan=_F(YoungModulus = YOUNG ,
                               PoissonRatio = POISSON ,
                               GammaRef = GammaRef,
                               n = n,),)

TEMPS1=DEFI_LIST_REEL(DEBUT=0,
                      INTERVALLE=(_F(JUSQU_A=40, NOMBRE=160),),);


################################################################
#     ESSAI DE CISAILLEMENT CYCLIQUE AVEC MICRO-DECHARGES      #
################################################################

COEF1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0 ,0,
                          10,-1.E4,
                          15,-6.E3,
                          20,-6.3E3,
                          25,1.E4,
                          30,-5.E3,
                          35,-4.7E3,
                          40,-1.E4,),
                    PROL_DROITE='CONSTANT',);

COEF2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0 ,0,
                          10,-1.E4,
                          15,-6.E3,
                          25,1.E4,
                          30,-5.E3,
                          40,-1.E4,),
                    PROL_DROITE='CONSTANT',);



DEFLIST1 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = TEMPS1),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS      = 2,
                                 SUBD_NIVEAU   = 10),)

#Valeur de P0 non prise en compte dans le mod√®le
P0=-50e3

U1=SIMU_POINT_MAT(COMPORTEMENT=_F(RELATION='Iwan',
                               RESI_INTE_RELA=1E-8,
                               ITER_INTE_MAXI=200,
                               ITER_INTE_PAS=12,
                                ),
                  MATER=MATE,
                  SUPPORT='ELEMENT',
                  INCREMENT=_F(LIST_INST=DEFLIST1,
                               NUME_INST_INIT=0,
                               INST_FIN=40,),
                  NEWTON=_F(MATRICE='TANGENTE',
                            REAC_ITER=1,),
                  CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                 ITER_GLOB_MAXI = 20),
                  ARCHIVAGE=_F(LIST_INST=TEMPS1,),
                  SIGM_IMPOSE=_F(SIXY=COEF1,),
                  SIGM_INIT=_F(SIXX=P0,
                               SIYY=P0,
                               SIZZ=P0,),
                  EPSI_INIT=_F(EPXX=P0/(3*K),
                               EPYY=P0/(3*K),
                               EPZZ=P0/(3*K),
                               EPXY=0,
                               EPXZ=0,
                               EPYZ=0,),);

IMPR_TABLE(TABLE=U1);

U2=SIMU_POINT_MAT(COMPORTEMENT=_F(RELATION='Iwan',
                               RESI_INTE_RELA=1E-8,
                               ITER_INTE_MAXI=200,
                               ITER_INTE_PAS=12,
                                ),
                  MATER=MATE,
                  SUPPORT='ELEMENT',
                  INCREMENT=_F(LIST_INST=DEFLIST1,
                               NUME_INST_INIT=0,
                               INST_FIN=40,),
                  NEWTON=_F(MATRICE='TANGENTE',
                            REAC_ITER=1,),
                  CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                 ITER_GLOB_MAXI = 20),
                  ARCHIVAGE=_F(LIST_INST=TEMPS1,),
                  SIGM_IMPOSE=_F(SIXY=COEF2,),
                  SIGM_INIT=_F(SIXX=P0,
                               SIYY=P0,
                               SIZZ=P0,),
                  EPSI_INIT=_F(EPXX=P0/(3*K),
                               EPYY=P0/(3*K),
                               EPZZ=P0/(3*K),
                               EPXY=0,
                               EPXZ=0,
                               EPYZ=0,),);

IMPR_TABLE(TABLE=U2);


