MASRES=LIRE_MAILLAGE( )

MASRES=DEFI_GROUP( reuse=MASRES,   MAILLAGE=MASRES,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

#

MODELE=AFFE_MODELE(  MAILLAGE=MASRES,AFFE=(
                       _F(  TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'POU_D_T'),
              _F(  NOEUD = ('B','C',),  PHENOMENE = 'MECANIQUE',
        MODELISATION = 'DIS_T'))       )

#
MATER=DEFI_MATERIAU(  ELAS=_F( E = 28000.00, NU = 0.0,  RHO = 0.0,
                         AMOR_HYST = 0.))

MATHYST=DEFI_MATERIAU(  ELAS=_F( E = 28000.00, NU = 0.0,  RHO = 0.0,
                         AMOR_HYST = 0.1))

#
CHAMPMAT=AFFE_MATERIAU(  MAILLAGE=MASRES,AFFE=(
                                _F( MAILLE = 'K2', MATER = MATER),
                                _F( GROUP_MA = 'AMORT', MATER = MATHYST)))

#
RIGMAS=AFFE_CARA_ELEM(  MODELE=MODELE,
                 POUTRE=_F(
       GROUP_MA = 'TOUT',
  SECTION = 'RECTANGLE',
                          CARA = ('H',), VALE = (1.,)),DISCRET=(

                _F(  NOEUD = 'B',
                          CARA = 'M_T_D_N',
                          VALE = ( 10.,)),
                _F(  NOEUD = 'B',
                          CARA = 'K_T_D_N',
                          VALE = ( 0.,0.,0.,)),
                _F(  NOEUD = 'C',
                          CARA = 'M_T_D_N',
                          VALE = ( 5.,)),
                _F(  NOEUD = 'C',
                          CARA = 'K_T_D_N',
                          VALE = ( 0.,0.,0.,))
                 )
                        )

#
CONDLIM=AFFE_CHAR_MECA(  MODELE=MODELE,DDL_IMPO=(
                         _F(  TOUT = 'OUI', DY = 0., DZ = 0.,
                                    DRX = 0., DRY = 0., DRZ = 0.),
                                  _F(  NOEUD = 'A',  DX = 0.))
                        )

FORCENO=AFFE_CHAR_MECA(  MODELE=MODELE,
                         FORCE_NODALE=_F(  NOEUD = 'C', FX = 100.)  )

RIGIELEM=CALC_MATR_ELEM(  MODELE=MODELE,       OPTION='RIGI_MECA',
                            CARA_ELEM=RIGMAS,  CHARGE=CONDLIM,
                            CHAM_MATER=CHAMPMAT                )

MASSELEM=CALC_MATR_ELEM(  MODELE=MODELE,       OPTION='MASS_MECA',
                            CARA_ELEM=RIGMAS,  CHARGE=CONDLIM,
                            CHAM_MATER=CHAMPMAT                )

RIGCMPLX=CALC_MATR_ELEM(  MODELE=MODELE,       OPTION='RIGI_MECA_HYST',
                            RIGI_MECA=RIGIELEM,    CARA_ELEM=RIGMAS,
                            CHAM_MATER=CHAMPMAT,
                            CHARGE=CONDLIM              )

SCNDMEMB=CALC_VECT_ELEM(  OPTION='CHAR_MECA',
                            CARA_ELEM=RIGMAS,  CHARGE=FORCENO     )

INCONNUS=NUME_DDL(  MATR_RIGI=RIGCMPLX,    METHODE='LDLT')

MATRRIGI=ASSE_MATRICE(  MATR_ELEM=RIGCMPLX,  NUME_DDL=INCONNUS )

MATRMASS=ASSE_MATRICE(  MATR_ELEM=MASSELEM,  NUME_DDL=INCONNUS )

VECTASS=ASSE_VECTEUR(  VECT_ELEM=SCNDMEMB,  NUME_DDL=INCONNUS )

