TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.0E-09,
           VALE_CALC=0.0E+00,
           VALE_REFE=0.0,
           NOM_PARA='VALEUR',
           TABLE=TAB1,
           FILTRE=_F(NOM_PARA='TEST',
                     VALE_K='DX',),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=1.0E-09,
           VALE_CALC=1.0,
           VALE_REFE=1.0,
           NOM_PARA='VALEUR',
           TABLE=TAB1,
           FILTRE=_F(NOM_PARA='TEST',
                     VALE_K='DY',),
           )

