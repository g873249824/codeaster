# TEST DE PERFORMANCE ANCIENNEMENT NOMME SSLV101I
# MODELISATION 3D  POINTS   F'=NO2958   B5'=NO2974
#======================================================================

DEBUT( CODE=_F(  NOM = 'YYYY108I',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE(  
                       )

MO=AFFE_MODELE(      MAILLAGE=MA, 
                           AFFE=_F(  TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                                              MODELISATION = '3D')
                       )

MAT=DEFI_MATERIAU(    ELAS=_F(  E = 202702.7,  NU = 0.3),
                          THER=_F(  LAMBDA = 2.7,  RHO_CP = 0.3)
                       )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MA, 
                          AFFE=_F(  TOUT = 'OUI',  MATER = MAT)
                       )

CHA=AFFE_CHAR_MECA(   MODELE=MO,DDL_IMPO=( 
                 _F(  GROUP_NO = 'GRNO7',      # X =-30.MM , Y=20.25MM
                                    DX = 0.0,  DY = 0.01),
                          _F(  GROUP_NO = 'GRNO1',  DZ = 0.0),        # Z =0.
                          _F(  GROUP_NO = 'GRNO8',  DY = 0.0))        # Y =0.
                       )

MEL=CALC_MATR_ELEM(   MODELE=MO,   CHARGE=CHA,   CHAM_MATER=CHMAT,
                                      OPTION='RIGI_MECA'
                       )

#
NU=NUME_DDL(         MATR_RIGI=MEL,    RENUM='MD'
                       )

MATASS=ASSE_MATRICE(    MATR_ELEM=MEL,  NUME_DDL=NU
                       )

MATASS=FACTORISER(    reuse=MATASS,  MATR_ASSE=MATASS
                       )

VEL=CALC_VECT_ELEM(   CHARGE=CHA,   OPTION='CHAR_MECA'
                       )

VECASS=ASSE_VECTEUR(   VECT_ELEM=VEL,   NUME_DDL=NU
                       )

VECASS=RESOUDRE(    reuse=VECASS,  MATR=MATASS,     CHAM_NO=VECASS
                       )

TEST_RESU(CHAM_NO=( 
_F( CHAM_GD = VECASS, NOEUD = 'NO2958', NOM_CMP = 'DY', VALE = 0.0100000, 
                                                     PRECISION = 1.E-4),
_F( CHAM_GD = VECASS, NOEUD = 'NO2958', NOM_CMP = 'DZ', VALE = 1.0296E-4, 
                                                     PRECISION = 1.E-4),
_F( CHAM_GD = VECASS, NOEUD = 'NO2974', NOM_CMP = 'DX', VALE = 4.3006E-3, 
                                                     PRECISION = 1.E-4),
_F( CHAM_GD = VECASS, NOEUD = 'NO2974', NOM_CMP = 'DY', VALE = 9.2890E-3, 
                                                     PRECISION = 1.E-4),
_F( CHAM_GD = VECASS, NOEUD = 'NO2974', NOM_CMP = 'DZ', VALE = -2.9173E-5, 
                                                     PRECISION = 1.E-4))
                       )

FIN( )
