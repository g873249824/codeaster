#    NOUVEAU CHARGEMENT MECANIQUE : DEPLACEMENT UNIAXIALE UNITAIRE
#***********************************************************************


SIG_INI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELGA_SIEF_R',
NOM_CHAM='SIEF_ELGA', RESULTAT=U1,
                        INST=4.
                      )

VAR_INI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELGA_VARI_R',
NOM_CHAM='VARI_ELGA', RESULTAT=U1,
                        INST=4.
                      )

DEPL_2=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  MAILLE = 'M4',
                                          DY = 1.)
                           )

COEF2=DEFI_FONCTION(      NOM_PARA='INST',
                          PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                          VALE=( 4.0,  0.0,
                                 5.0,  -0.2, )
                   )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************


U2=STAT_NON_LINE( 
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F( 
                CHARGE = CHAR_U2,
                FONC_MULT = COEF1), _F( 
                CHARGE = DEPL_1), _F( 
                CHARGE = DEPL_2,
                FONC_MULT = COEF2)),
                         COMP_INCR=_F( 
                RELATION = 'LAIGLE'),
                         ETAT_INIT=_F( 
                SIGM = SIG_INI,
                VARI = VAR_INI),
                         INCREMENT=_F( 
                LIST_INST = TEMPS2,
                                  ),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1,
                ),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-6,
                RESI_INTE_RELA = 1.E-6,
                ITER_INTE_MAXI = 100,
                ITER_INTE_PAS  = -10,
                ITER_GLOB_MAXI = 100)
              )

U2=CALC_ELEM( reuse      = U2,
              OPTION     = ('SIEF_ELNO_ELGA','VARI_ELNO_ELGA',),
              RESULTAT   = U2,
              )

U2=CALC_NO(  reuse      = U2,
             RESULTAT   = U2,
             TOUT_ORDRE = 'OUI',
