#***********************************************************************

MAILLAGE=LIRE_MAILLAGE( )

MODELE=AFFE_MODELE(     MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = 'AXIS')
                           )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS1=DEFI_LIST_REEL(   DEBUT=0.,
                           INTERVALLE=_F(  JUSQU_A = 4.0,   NOMBRE = 1)
                         )

#***********************************************************************
#    MATERIAU : SOL
#***********************************************************************

SOL=DEFI_MATERIAU(    ELAS=_F(  E  = 1500.0,
                                NU = 0.27  ,),
                                LAIGLE =_F(  GAMMA_ULT =    0.132   ,
                                             GAMMA_E   =    0.005   ,
                                             M_ULT     =    2.0     ,
                                             M_E       =    7.0     ,
                                             A_E       =    0.65    ,
                                             M_PIC     =   15.0     ,
                                             A_PIC     =    0.5     ,
                                             ETA       =    0.45    ,
                                             SIGMA_C   =    4.5     ,
                                             GAMMA     =    0.23    ,
                                             KSI       =    0.25    ,
                                             GAMMA_CJS =    0.7     ,
                                             SIGMA_P1  =    9.09    ,
                                             PA        =    1.0E-2  )
                           )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',  MATER = SOL)
                          )

#***********************************************************************
#    CHARGEMENT MECANIQUE : FORCE DE COMPRESSION + DDL IMPOSES
#***********************************************************************

CHAR_U1=AFFE_CHAR_MECA(    MODELE=MODELE,
                              PRES_REP=_F(   MAILLE = 'M4',
                                          PRES = 1.)
                       )

CHAR_U2=AFFE_CHAR_MECA(    MODELE=MODELE,
                             PRES_REP=_F(   MAILLE = 'M3',
                                         PRES = 1.)
                        )

DEPL_1=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  MAILLE = 'M2',
                                          DY = 0.)
                           )

COEF1=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                         VALE=( 0.0,  0.0,
                                4.0,  1.0,)
                       )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************



U1=STAT_NON_LINE( 
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F( 
                CHARGE = CHAR_U1,
                FONC_MULT = COEF1), _F( 
                CHARGE = CHAR_U2,
                FONC_MULT = COEF1), _F( 
                CHARGE = DEPL_1)),
                         COMP_INCR=_F( 
                RELATION = 'LAIGLE'),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE'),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-4,
                RESI_INTE_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50,
                ITER_INTE_MAXI = 100),
                         INCREMENT=_F( 
                LIST_INST = TEMPS1)
              )

U1=CALC_ELEM( reuse=U1,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=U1,
              )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS2=DEFI_LIST_REEL(   DEBUT=4.,
                            INTERVALLE=(
                              _F(  JUSQU_A = 5.000,   NOMBRE = 1000,  ),
