                              DDL_IMPO=_F(  GROUP_NO = 'BORDAB',  DY = 0.),
                             FACE_IMPO=_F(  GROUP_MA = 'FACEEF',  DNOR = 0.),
                              PRES_REP=_F(  GROUP_MA = 'FACEAE',  PRES = 60.))

ME=CALC_MATR_ELEM(         MODELE=MO,
                                CHARGE=CH,
                            CHAM_MATER=CM,
                               OPTION='RIGI_MECA'   )

VE=CALC_VECT_ELEM(         CHARGE=CH,
                            CHAM_MATER=CM,
                               OPTION='CHAR_MECA'   )

NU=NUME_DDL(            MATR_RIGI=ME           )

K=ASSE_MATRICE(        MATR_ELEM=ME,
                              NUME_DDL=NU           )

F=ASSE_VECTEUR(        VECT_ELEM=VE,
                              NUME_DDL=NU           )

K=FACT_LDLT(      reuse=K,     MATR_ASSE=K            )

DEPLA=RESO_LDLT(           MATR_FACT=K,
                               CHAM_NO=F            )

SIGMA=CALC_CHAM_ELEM(         MODELE=MO,
                           DEPL=DEPLA,
                                OPTION='SIGM_ELNO_DEPL',
                            CHAM_MATER=CM           )

