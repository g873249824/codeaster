SOLNL1=CALC_CHAMP(reuse=SOLNL1,RESULTAT=SOLNL1,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO','EPSP_ELGA','EPSP_ELNO'))


