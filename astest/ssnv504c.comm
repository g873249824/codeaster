# CAS_TEST : SSNV504C
# MODELE
# MAILLAGE : MODELISATION : AXIS
#             SOLIDE : TRIA3
#             CONTACT : SEG2
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
#
#


DEBUT(CODE=_F(NOM='SSNV504C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
#......................................................................
#  CALCUL VALIDATION DU PASSAGE DES SINGULARITES GEOMETRIQUES
#......................................................................
#
#


MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_MA=_F(NOM='TOUT',
                                   UNION=('LOPIN','FILIERE'),),
                  CREA_GROUP_NO=(_F(GROUP_MA='LJA',
                                    NOM='LIM1',),
                                 _F(GROUP_MA='LIJ',
                                    NOM='LIM2',),
                                 _F(GROUP_MA='LHI',
                                    NOM='LIM3',),
                                 _F(GROUP_MA='LMN',
                                    NOM='LMN',),
                                 _F(GROUP_MA='LNK',
                                    NOM='LNK',),),);
#_____________________________________________________________________
#
#                    DEFINITION DU MODELE
#_____________________________________________________________________
#
#


MO=AFFE_MODELE(MAILLAGE=MAILL1,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);
#
# REORIENTATION DU MAILLAGE
#
#


MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_2D=_F(GROUP_MA=('SMAI1','SESC1'),),
                     );

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_2D=_F(GROUP_MA=('SMAI2','SESC2'),),
                     );
#_____________________________________________________________________
#
#                    DEFINITION DES MATERIAUX
#_____________________________________________________________________
#
#


MLOPIN=DEFI_MATERIAU(ELAS=_F(E=5000.0,
                             NU=0.45000000000000001,),);

MFILIERE=DEFI_MATERIAU(ELAS=_F(E=2.E5,
                               NU=0.29999999999999999,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=(_F(GROUP_MA='LOPIN',
                          MATER=MLOPIN,),
                       _F(GROUP_MA='FILIERE',
                          MATER=MFILIERE,),),);
#_____________________________________________________________________
#
#                    AFFECTATION DES CONDITIONS AUX LIMITES
#                       ET DU DEPLACEMENT IMPOSE
#_____________________________________________________________________
#
#


CH1=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_NO='LIM1',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='LIM2',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='LIM3',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='LNK',
                                DX=0.0,),),);

CH0=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=_F(GROUP_NO='LMN',
                               DY=1.0,),);

CONTACT= DEFI_CONTACT(MODELE       = MO,
                      FORMULATION  = 'DISCRETE',
                      ITER_GEOM_MAXI = 10,
                   ZONE=(
                        _F(
                            GLISSIERE     = 'OUI',
                            ALARME_JEU    = 0.1,
                            GROUP_MA_MAIT ='SMAI1',
                            GROUP_MA_ESCL ='SESC1',
                            SANS_GROUP_NO ='LIM1',
                         ),

                       ),
                  )

#_____________________________________________________________________
#
#                    DEFINITIONS DES FONCTIONS TEMPORELLES
#_____________________________________________________________________
#
#


DPLY=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.0),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=5.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=20.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=50.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=70.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=140.0,
                                     PAS=5.0,),
#                                  _F(JUSQU_A=155.0,
#                                     PAS=5.0,),
                                    ),);

U=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                MODELE=MO,
                CHAM_MATER=CM,
                INFO=1,
                EXCIT=(_F(CHARGE=CH0,
                          FONC_MULT=DPLY,),
                       _F(CHARGE=CH1,),
                       ),
                CONTACT  = CONTACT,
                COMPORTEMENT=_F(RELATION='ELAS',
                             TOUT='OUI',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=9.9999999999999995E-07,),);

DEFI_FICHIER(ACTION='ASSOCIER',UNITE=37)

IMPR_RESU(MODELE=MO,
          FORMAT='GMSH',UNITE=37,
          RESU=_F(RESULTAT=U,
                  MAILLAGE=MAILL1,
                  NOM_CHAM='DEPL',
                  TOUT_ORDRE='OUI',
                  ),);


DEFI_FICHIER(ACTION='LIBERER',UNITE=37)

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='K',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=5.0,
                   VALE_REFE=5.0,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=28,
                   GROUP_NO='K',
                   RESULTAT=U,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=144.2258670767,
                   ),
                ),
          )

FIN();
#
