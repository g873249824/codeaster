DEBUT(CODE=_F(NOM='SSNP504E',
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))

PRE_GMSH();

MAILLAG2=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=MODI_MAILLAGE(reuse =MAILLAG2,
                       MAILLAGE=MAILLAG2,
                       ORIE_PEAU_2D=_F(GROUP_MA=('GM4','GM5','GM6','GM7',),),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(GROUP_MA =('GM2','GM4',),),),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'LIGAU',
                                       OPTION = 'PLAN',
                                       POINT = (0,0),
                                       VECT_NORMALE = (1,0),
                                       PRECISION=0.05,),
                                    _F(NOM = 'LDROI',
                                       OPTION = 'PLAN',
                                       POINT = (4,0),
                                       VECT_NORMALE = (1,0),
                                       PRECISION=0.05,),
                                    _F(NOM = 'LIBAS',
                                       OPTION = 'PLAN',
                                       POINT = (0,0),
                                       VECT_NORMALE = (0,1),
                                       PRECISION=0.05,),
                                    _F(NOM = 'LHAUT',
                                       OPTION = 'PLAN',
                                       POINT = (0,9),
                                       VECT_NORMALE = (0,1),
                                       PRECISION=0.05,),),
                    );

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'P1',
                                       INTERSEC=('LIGAU','LIBAS',),),
                                    _F(NOM = 'P2',
                                       INTERSEC=('LDROI','LIBAS',),),
                                    _F(NOM = 'P11',
                                       INTERSEC=('LDROI','LHAUT',),),
                                    _F(NOM = 'P12',
                                       INTERSEC=('LIGAU','LHAUT',),),
                                    _F(NOM = 'LIGAU2',
                                       INTERSEC=('LIGAU','GM2',),),
                                    ),);

MODELE=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),),);

CONT = DEFI_CONTACT(MODELE         = MODELE,
                    FORMULATION    = 'CONTINUE',
                    FROTTEMENT     = 'COULOMB',
                    ALGO_RESO_CONT = 'NEWTON',
                    ALGO_RESO_GEOM = 'NEWTON',
                    ALGO_RESO_FROT = 'NEWTON',
                    ADAPT_COEF     = 'OUI',
                    ZONE =(
                           _F(
                             GROUP_MA_MAIT = 'GM5',
                             GROUP_MA_ESCL = 'GM4',
                             INTEGRATION   = 'AUTO',
                             SEUIL_INIT    = 0.0,

                             COULOMB       = 0.5,
                             TOLE_PROJ_EXT = 0.0,
                             CONTACT_INIT  = 'INTERPENETRE',
                             ),
                           _F(
                             GROUP_MA_MAIT = 'GM6',
                             GROUP_MA_ESCL = 'GM7',
                             INTEGRATION   = 'AUTO',
                             SEUIL_INIT    = 0.0,
                             COULOMB       = 0.5,
                             TOLE_PROJ_EXT = 0.0,
                             CONTACT_INIT  = 'INTERPENETRE',
                             ),
                          ),
                   );


E=100.0E6
nu=0.3

ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELE,
                       AFFE=_F(TOUT='OUI',
                       MATER=ACIER,
                      ),);

#DEPX=2.016
DEPX=2.0
DEPY=1.E-2
#DEPX=0.0
#DEPY=-1.0

CH1=AFFE_CHAR_MECA(MODELE=MODELE,
                   DDL_IMPO=(
                            _F(GROUP_NO='LIBAS',DY=DEPY,),
                            _F(GROUP_NO='LHAUT',DY=-DEPY,),
                            _F(GROUP_NO=('P1','P2','P11','P12',),
                               DX=0.0,),),
                    INFO=1,);

CH2=AFFE_CHAR_MECA(MODELE=MODELE,
                   DDL_IMPO=(_F(GROUP_NO='LIGAU2',
                                DX=DEPX,),),
                   INFO=1,);

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.,0.,1.,1.),
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='CONSTANT',);

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4,),);

UTOT1=STAT_NON_LINE(MODELE=MODELE,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2),
                           ),
                    CONTACT  = CONT,
                    NEWTON   =_F(REAC_ITER=1,) ,
                    COMPORTEMENT=_F(RELATION='ELAS',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06)),
                    SOLVEUR=_F(METHODE='MUMPS',
                               ),
                    INFO=1,);

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]
vale = [[0.0,-6.0789654242659E+04,-5.9710310763853E+04,-5.1197045305081E+04,0.0],
        [0.0,-3.9911326085987E+05,-1.2010507924936E+05,-1.0176447185762E+05,0.0],
        [0.0,0.0,-1.9289293574495E+05,-1.5195304167722E+05,0.0],
        [0.0,0.0,-9.4470170833590E+05,-2.0680206864516E+05,0.0]]

VALE_CALC=[
 [0.0,
 -60789.675043732,
 -59710.288276373,
 -51197.050552153,
 0.0,],
 [0.0,
 -3.9908494259421E+05,
 -1.2015319687127E+05,
 -1.0180267137167E+05,
 0.0,],
 [0.0,
 0.0,
 -1.9304089247819E+05,
 -1.5206162865366E+05,
 0.0,],
 [0.0,
 0.0,
 -9.4573880322547E+05,
 -2.0701956147639E+05,
 0.0,],
]

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO=('GM4'),
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1',),
                                         OPERATION='EXTRACTION',),);
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              NOM_PARA=('NOEUD','INST','COOR_X','LAGS_C','LAGS_F1',),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X',),
                     ORDRE='CROISSANT',),);

   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-12
        critere = 'ABSOLU'
      else:
        precision = 5.E-2
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE_CALC=VALE_CALC[i-1][j], VALE_REFE=vale[i-1][j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU',),);

