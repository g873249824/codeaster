# CAS_TEST__:SSNV128M
# TEST DU GRECO CALCUL 3D
# MODELE
# MAILLAGE : ELEMENT 2D QUADRANGLE A 4 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
#

DEBUT( CODE=_F(  NOM = 'SSNV128M',NIV_PUB_WEB='INTERNET') )

#......................................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 2D QUAD4
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#......................................................................

#PRE_GIBI()
#
MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'), )
#
MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'D_PLAN',)
              )
#
MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 MODELE=MO,
                 ORIE_PEAU_2D=(_F(GROUP_MA = 'LPRESV',),
                               _F(GROUP_MA = 'LPRESH',))
                )
#
MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11,
                             NU = 0.2,)
                    )
#
MATBAT=DEFI_MATERIAU(ELAS=_F(E = 1.E16,
                             NU = 0.0,)
                  )
#
CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA = 'SPLAQ',
                             MATER = MATPLA,),
                          _F(GROUP_MA = 'SBATI',
                             MATER = MATBAT,))
                    )
#
CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA = 'LENCA',  DX = 0.0, DY = 0.0,),
                              _F(GROUP_NO = 'PPS',    DY = 0.0,),
                              _F(GROUP_NO = 'PBS',    DX = 0.0, DY = 0.0,),
                              _F(GROUP_MA = 'LBLOCX', DX = 0.0,)),
#                              _F(GROUP_NO = 'PBS', DX = 0.0,)),
#                              _F(GROUP_MA = 'LBLBAX', DX = 0.0,)),
                    PRES_REP=(_F(GROUP_MA = 'LPRESV', PRES = 5.E07,),
                              _F(GROUP_MA = 'LPRESH', PRES = 15.E07,))
                   )
#
CHA4=AFFE_CHAR_MECA(MODELE=MO,
                    CONTACT=_F(GROUP_MA_ESCL     = 'LBATIR',
                               GROUP_MA_MAIT     = 'LCONTAR',
                               APPARIEMENT    = 'MAIT_ESCL',
                               RECHERCHE      = 'NOEUD_VOISIN',
                               NORMALE        = 'MAIT',
                               METHODE        = 'CONTINUE',
                               INTEGRATION    = 'NOEUD',
                               MODL_AXIS      = 'NON',
                               CONTACT_INIT   = 'OUI',
                               SEUIL_INIT     = 1.0E-1,
                               COEF_REGU_FROT = 1.0,
                               COEF_REGU_CONT = 1.0,
                               ITER_GEOM_MAXI = 1,
                               ITER_FROT_MAXI = 10,
                               ITER_CONT_MAXI = 4,
                               FROTTEMENT     = 'COULOMB',
                               COULOMB        = 1.0,
                               PROJECTION     = 'LINEAIRE',)
                    )
#
#
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0,)
                    )
#
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 5,)
                      )
#
#-----------------------------------------------------------
#
U4M=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE = CHA1,
                          FONC_MULT = RAMPE,),
                       _F(CHARGE = CHA4,)),
                COMP_ELAS=_F(RELATION = 'ELAS',),
                NEWTON=_F(REAC_ITER=1,),
                INCREMENT=_F(LIST_INST = L_INST,),
                CONVERGENCE=_F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 200,
                               RESI_GLOB_RELA = 1.0E-5,),
                INFO=2,
               )
#
U4M=CALC_ELEM(reuse=U4M,
            OPTION=('SIEF_ELNO_ELGA', 'VARI_ELNO_ELGA',),
            RESULTAT=U4M,
            MODELE=MO,
            CHAM_MATER=CHMAT
           )
#
DEPLA4M=CREA_CHAMP(OPERATION='EXTR',
                 TYPE_CHAM='NOEU_DEPL_R',
                 NOM_CHAM='DEPL',
                 RESULTAT=U4M,
                 INST=1.0
                )
#
TEST_RESU(RESU=(_F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'VALE_CONT',
                   NOEUD      = 'N365',
                   NOM_CMP    = 'JEU',
                   VALE       = 0.,
                   CRITERE    = 'ABSOLU',
                   PRECISION  = 1.E-12,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.3',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'VALE_CONT',
                   NOEUD      = 'N365',
                   NOM_CMP    = 'CONT',
                   VALE       = 1.,
                   PRECISION  = 1.E-5,
                   CRITERE    = 'RELATIF',
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.3',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'VALE_CONT',
                   NOEUD      = 'N365',
                   NOM_CMP    = 'RN',
                   VALE       = 1.04864E+05,
                   PRECISION  = 1.E-5,
                   CRITERE    = 'RELATIF',
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.3',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPA',
                   NOM_CMP    = 'DX',
                   VALE       = 2.86E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPB',
                   NOM_CMP    = 'DX',
                   VALE       = 2.72E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPC',
                   NOM_CMP    = 'DX',
                   VALE       = 2.28E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPD',
                   NOM_CMP    = 'DX',
                   VALE       = 1.98E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPE',
                   NOM_CMP    = 'DX',
                   VALE       = 1.50E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                ))
       )
#
#-----------------------------------------------------------
#
U4L=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE = CHA1,
                          FONC_MULT = RAMPE,),
                       _F(CHARGE = CHA4,)),
                COMP_ELAS=_F(RELATION = 'ELAS',),
                SOLVEUR    = _F(METHODE = 'LDLT',
                                RENUM = 'SANS',),
                NEWTON=_F(REAC_ITER=1,),
                INCREMENT=_F(LIST_INST = L_INST,),
                CONVERGENCE=_F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 200,
                               RESI_GLOB_RELA = 1.0E-5,),
                INFO=2,
               )
#
U4L=CALC_ELEM(reuse=U4L,
            OPTION=('SIEF_ELNO_ELGA', 'VARI_ELNO_ELGA',),
            RESULTAT=U4L,
            MODELE=MO,
            CHAM_MATER=CHMAT
           )
#
DEPLA4L=CREA_CHAMP(OPERATION='EXTR',
                 TYPE_CHAM='NOEU_DEPL_R',
                 NOM_CHAM='DEPL',
                 RESULTAT=U4L,
                 INST=1.0
                )
#
TEST_RESU(RESU=(_F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPA',
                   NOM_CMP    = 'DX',
                   VALE       = 2.86E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPB',
                   NOM_CMP    = 'DX',
                   VALE       = 2.72E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPC',
                   NOM_CMP    = 'DX',
                   VALE       = 2.28E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPD',
                   NOM_CMP    = 'DX',
                   VALE       = 1.98E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U4L,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPE',
                   NOM_CMP    = 'DX',
                   VALE       = 1.50E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                ))
       )
#
