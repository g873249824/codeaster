# MAILLAGE : ELEMENT 2D QUADRANGLE A 4 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
#

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#......................................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 2D QUAD4
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#......................................................................
#
MA=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'));
#
MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'D_PLAN'));
#
MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=(_F(GROUP_MA = 'LPRESV'),
                               _F(GROUP_MA = 'LPRESH')));
#
MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(GROUP_MA = 'LCONTA'));
#
MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11, NU = 0.2));
#
MATBAT=DEFI_MATERIAU(ELAS=_F(E = 1.E16, NU = 0.0));
#
CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA = 'SPLAQ',
                             MATER = MATPLA),
                          _F(GROUP_MA = 'SBATI',
                             MATER = MATBAT)));
#
CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA = 'LENCA',  DX = 0.0, DY = 0.0),
                              _F(GROUP_NO = 'PPS',    DY = 0.0),
                              _F(GROUP_NO = 'PBS',    DX = 0.0, DY = 0.0),
                              _F(GROUP_MA = 'LBLOCX', DX = 0.0)),
                    PRES_REP=(_F(GROUP_MA = 'LPRESV', PRES = 5.E07),
                              _F(GROUP_MA = 'LPRESH', PRES = 15.E07)));

CHA2 = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    FROTTEMENT     = 'COULOMB',
                    ALGO_RESO_CONT = 'NEWTON',
                    ALGO_RESO_GEOM = 'NEWTON',
                    ALGO_RESO_FROT = 'NEWTON',
                    ADAPT_COEF     = 'OUI',
                    ZONE =(
                           _F(
                             GROUP_MA_ESCL = 'LCONTA',
                             GROUP_MA_MAIT = 'LBATI',
                             SANS_GROUP_NO = 'PPS',
                             COULOMB       = 1.0,
                             ALGO_CONT     = 'STANDARD',
                             COEF_CONT     = 1.,
                             ALGO_FROT     = 'STANDARD',
                             COEF_FROT     = 1.,
                             CONTACT_INIT  = 'INTERPENETRE',
                             SEUIL_INIT    = 1.E-1,
                             ),
                          ),
                   );

#
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0));
#
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 1));
#
#-----------------------------------------------------------
#
U4M=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE = CHA1, FONC_MULT = RAMPE),
                       ),
                CONTACT  = CHA2,
                COMPORTEMENT=_F(RELATION = 'ELAS'),
                NEWTON=_F(REAC_ITER=1),
                INCREMENT=_F(LIST_INST = L_INST),
                CONVERGENCE=_F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 20,
                               RESI_GLOB_RELA = 1.0E-5),
                INFO=1);

TEST_RESU(RESU=(_F(INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='VALE_CONT',
                   NOEUD='N32',
                   NOM_CMP='JEU',
                   VALE_CALC= 3.231174267785E-27,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1.E-12,
                   ),
                _F(INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='VALE_CONT',
                   NOEUD='N32',
                   NOM_CMP='CONT',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,
                   CRITERE='RELATIF',
                   PRECISION=1.0000000000000001E-05,
                   ),
                _F(INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='VALE_CONT',
                   NOEUD='N32',
                   NOM_CMP='RN',
                   VALE_CALC= 1.04863224E+05,
                   VALE_REFE=1.04864E5,
                   CRITERE='RELATIF',
                   PRECISION=1.0000000000000001E-05,
                   ),
                _F(GROUP_NO='PPA',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.84594335E-05,
                   VALE_REFE=2.8600000000000001E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPB',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.70792315E-05,
                   VALE_REFE=2.72E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPC',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.27402508E-05,
                   VALE_REFE=2.2799999999999999E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPD',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.97270629E-05,
                   VALE_REFE=1.98E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPE',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.53641562E-05,
                   VALE_REFE=1.5E-05,
                   PRECISION=0.050000000000000003,
                   ),
                ),
          )

#
#-----------------------------------------------------------
#

U4L=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CHMAT,
                EXCIT=(_F(CHARGE = CHA1, FONC_MULT = RAMPE),
                       ),
                CONTACT  = CHA2,
                COMPORTEMENT=_F(RELATION = 'ELAS'),
                SOLVEUR=_F(METHODE = 'LDLT', RENUM = 'SANS'),
                NEWTON=_F(REAC_ITER=1),
                INCREMENT=_F(LIST_INST = L_INST),
                CONVERGENCE=_F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 20,
                               RESI_GLOB_RELA = 1.0E-5),
                INFO=1);

TEST_RESU(RESU=(_F(GROUP_NO='PPA',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U4L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.84594335E-05,
                   VALE_REFE=2.8600000000000001E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPB',
                   INST=1.0,
