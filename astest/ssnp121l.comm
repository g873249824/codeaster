DEBUT(CODE=_F(NOM='SSNP121L', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'),
      MESURE_TEMPS=_F(MOYENNE='NON'))

MA=LIRE_MAILLAGE(FORMAT='MED');

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(GROUP_MA='AB'),
                             _F(GROUP_MA='CD'),
                             _F(GROUP_MA='EF'),
                             _F(GROUP_MA='FG'),
                             _F(GROUP_MA='GH'),
                             _F(GROUP_MA='HE')));

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN')));

MAT2=DEFI_MATERIAU(ELAS=_F(E=2000000.0,
                           NU=0.0));

MAT1=DEFI_MATERIAU(ELAS=_F(E=2000000.0,
                           NU=0.0));

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='PLAQUE1',
                             MATER=MAT2),
                          _F(GROUP_MA='PLAQUE2',
                             MATER=MAT1)));

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='CD',
                                 DX=0.0,
                                 DY=-0.1),
                              _F(GROUP_NO='GH',
                                 DX=0.0,
                                 DY=0.0)));

CHA2   = DEFI_CONTACT(MODELE      = MO,
                      FORMULATION = 'DISCRETE',
                      RESI_ABSO   = 1.E-8,
                      PRE_COND    = 'DIRICHLET',
                      ZONE=_F(
                               ALGO_CONT     = 'GCP',
                               GROUP_MA_MAIT = 'AB',
                               GROUP_MA_ESCL = 'EF',
                             ),
                      );


L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1));

FONC=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,  0.0,
                         1.0,  1.0));

RESU=STAT_NON_LINE(SOLVEUR=_F(METHODE='MUMPS', SYME='OUI',MATR_DISTRIBUEE='OUI'),
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=CHA1,
                             FONC_MULT=FONC)),
                   CONTACT  = CHA2,
                   COMPORTEMENT=_F(RELATION='ELAS'),
                   INCREMENT=_F(LIST_INST=L_INST));


# STANLEY(RESU,MA,MO,CHMAT,None)


RESU=CALC_CHAMP(reuse =RESU,
