                  SYME='OUI',
                  FOND_FISS=_F(GROUP_MA='MLFF',
                               DTAN_ORIG=(0.0,1.0,0.0),
                               GROUP_NO_ORIG='LFFO',
                               GROUP_NO_EXTR='LFFE',
                               ),
                  LEVRE_SUP=_F(GROUP_MA='LEVREINF',),
                  );

