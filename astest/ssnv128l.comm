# CAS_TEST__:SSNV128L
# TEST DU GRECO CALCUL 3D
# MODELE
# MAILLAGE : ELEMENT 3D HEXAEDRE A 8 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
#

DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET'),
IGNORE_ALARM=('MODELE1_63'),DEBUG=_F(SDVERI='OUI'))

# <MODELE1_63> : DANS UN MODELE, IL EXISTE DES ELEMENTS DE TYPE "BORD" QUI N'ONT PAS DE VOISIN AVEC RIGIDITE
#  LE MODELE PROVOQUANT CES ALARMES CONTIENT UNE SURFACE RIGIDE POUR LE CONTACT

#......................................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 3D HEXA8
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#  PLAQUE SUR UN PLAN INCLINE A 45 DEGRES (ROTATION AUTOUR DE Y)
#......................................................................
#
MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED',)
#
MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = '3D',)
              )
#
MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=(_F(GROUP_MA = 'SPRESV',),
                               _F(GROUP_MA = 'SPRESH',))
                )
#
MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11,
                             NU = 0.2,)
                    )
#
CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT = 'OUI',
                             MATER = MATPLA,)
                    )
#
CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    VERI_NORM='NON',
                    LIAISON_GROUP=(_F(GROUP_MA_1 = 'SBLOCX',
                                     GROUP_MA_2 = 'SBLOCX',
                                     DDL_1      = 'DX',
                                     DDL_2      = 'DZ',
                                     COEF_MULT_1= 1.,
                                     COEF_MULT_2= 1.,
                                     COEF_IMPO  = 0.,),
                                   _F(GROUP_MA_1 = 'VPLAQ',
                                     GROUP_MA_2= 'VPLAQ',
                                     DDL_1      = 'DX',
                                     DDL_2      = 'DZ',
                                     COEF_MULT_1= 1.,
                                     COEF_MULT_2= -1.,
                                     COEF_IMPO  = 0.,)),
                    DDL_IMPO=(_F(GROUP_MA = 'SBATI',  DX = 0.0, DY = 0.0, DZ=0.0,),
                              _F(GROUP_MA = 'LBLOCY', DY = 0.0,)),
                     PRES_REP=(_F(GROUP_MA = 'SPRESV', PRES = 5.E07,),
                              _F(GROUP_MA = 'SPRESH', PRES = 15.E07,))
                   )
#
CHA2=DEFI_CONTACT(MODELE         = MO,
                  FORMULATION    = 'DISCRETE',
                  FROTTEMENT     = 'COULOMB',
                  ZONE=_F(
                          NORMALE        = 'MAIT',
                          VECT_MAIT      = 'VECT_Y',
                          MAIT_VECT_Y    = (1.,0.,-1.,),
                          GROUP_MA_MAIT  = 'SBATI',
                          GROUP_MA_ESCL  = 'SCONTA',
                          ALGO_CONT      = 'LAGRANGIEN',
                          ALGO_FROT      = 'LAGRANGIEN',
                          COEF_MATR_FROT = 0.8,
                          COULOMB        = 1.0,
                        )
                  )


#
#
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0,)
                    )
#
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 1,)
                      )
#
#-----------------------------------------------------------
#
U2M=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                  MODELE    = MO,
                CHAM_MATER = CHMAT,
                EXCIT      =(_F(CHARGE = CHA1,
                                FONC_MULT = RAMPE,),
                             ),
                CONTACT    = CHA2,
                COMPORTEMENT  = _F(RELATION = 'ELAS',),
                NEWTON     = _F(REAC_ITER = 1,),
                INCREMENT  = _F(LIST_INST = L_INST,),
                CONVERGENCE= _F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 200,
                               RESI_GLOB_RELA = 1.0E-5,),
#                INFO=2,
               )
#
TEST_RESU(RESU=(_F(GROUP_NO='PPA',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.01238619E-05,
                   VALE_REFE=2.02E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPB',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.91479116E-05,
                   VALE_REFE=1.9199999999999999E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPC',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.60797887E-05,
                   VALE_REFE=1.6099999999999998E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPD',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.39491425E-05,
                   VALE_REFE=1.4E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPE',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2M,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.08641013E-05,
                   VALE_REFE=1.06E-05,
                   PRECISION=0.050000000000000003,
                   ),
                ),
          )

#-----------------------------------------------------------
#
U2L=STAT_NON_LINE(MODELE    = MO,
                CHAM_MATER = CHMAT,
                EXCIT      =(_F(CHARGE = CHA1,
                                FONC_MULT = RAMPE,),
                             ),
                CONTACT    = CHA2,
                COMPORTEMENT  = _F(RELATION = 'ELAS',),
                SOLVEUR    = _F(SYME='OUI',
                                METHODE = 'LDLT',),
                NEWTON     = _F(REAC_ITER = 1,),
                INCREMENT  = _F(LIST_INST = L_INST,),
                CONVERGENCE= _F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 200,
                               RESI_GLOB_RELA = 1.0E-5,),
#                INFO=2,
               )
#
TEST_RESU(RESU=(_F(GROUP_NO='PPA',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.01238619E-05,
                   VALE_REFE=2.02E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPB',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.91479116E-05,
                   VALE_REFE=1.9199999999999999E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPC',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.60797887E-05,
                   VALE_REFE=1.6099999999999998E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPD',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.39491425E-05,
                   VALE_REFE=1.4E-05,
                   PRECISION=0.050000000000000003,
                   ),
                _F(GROUP_NO='PPE',
                   INST=1.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U2L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 1.08641013E-05,
                   VALE_REFE=1.06E-05,
                   PRECISION=0.050000000000000003,
                   ),
                ),
          )

#
#
FIN()
#
