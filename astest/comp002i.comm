                                 # memoire
#                                 ETA=0.5,
                                 MU=10.,
                                 Q_M=Qinf,
                                 Q_0=Qzer,

                                 # ecrouissage cinematique
                                 C1=C1inf,
                                 C2=C2inf,
                                 G1_0=Gam1,
                                 G2_0=Gam2,
#                                 A_I=1.,

                                 # viscosite
                                 K_0=Kv ,
                                 N=N,

                                 # ecoulement terme en exp
                                 A_K=1.,
#                                 ALP=0.,

                                 # terme Burlet
#                                 D2=1.,
#                                 D1=1.,

                                 # restauration
#                                 M_R=1.,
#                                 G_R=0.,
#                                 M_1=1.,
#                                 M_2=1.,
#                                 G_X1=0,
