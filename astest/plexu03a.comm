DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),IMPR_MACRO='OUI',ERREUR=_F(ERREUR_F='EXCEPTION',),);

MPa = 1000000.0;


# ------------------- Lecture du maillage

MAILLAGE=LIRE_MAILLAGE(    UNITE=20 )

#

MODELE=AFFE_MODELE(
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = ('GROUPE_____VOILE',),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'Q4GG'),
                     _F(  GROUP_MA = 'GROUPE__CABLE001',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE')
                            ) )
# DKTG permet d'utiliser les elements Q4GS d'EUROPLEXUS





#
EP=6.0E-2
CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       COQUE=(_F(  GROUP_MA = 'GROUPE_____VOILE',
                               EPAIS = EP,
                               ),
                               ),
                       BARRE=_F(  GROUP_MA = 'GROUPE__CABLE001',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 1.5E-4, ))
                               )



# ------------------- Definition des materiaux

BETON = DEFI_MATERIAU(ELAS=_F(
                    E = 30000*MPa,
                    NU = 0.3,
                    RHO = 2500,
                   ),
                    BPEL_BETON=_F()
                  );


ACIER = DEFI_MATERIAU(ELAS=_F(
                    E = 200000*MPa,
                    NU = 0.3,
                    RHO = 7500,
                   ),
                   BPEL_ACIER=_F(  F_PRG = 1.77E+9,
                              FROT_COURB = 0.,
                              FROT_LINE = 0.)
                  );

CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAILLAGE,AFFE=(
                       _F(  GROUP_MA = ('GROUPE_____VOILE',),
                              MATER = BETON),
                       _F(  GROUP_MA = 'GROUPE__CABLE001',
                              MATER = ACIER)
                              )
                              )



CABLEPR=DEFI_CABLE_BP(            MODELE=MODELE,
                                  CHAM_MATER=CHMATER,
                                   CARA_ELEM=CARAELEM,
                              GROUP_MA_BETON='GROUPE_____VOILE',
                                  DEFI_CABLE=_F(
                               GROUP_MA = 'GROUPE__CABLE001',
                               NOEUD_ANCRAGE = ( 'NC001001',  'NC001005', ),),
                               TYPE_ANCRAGE = ( 'ACTIF',   'ACTIF',  ),
                                TENSION_INIT=2.0E+5,
                               RECUL_ANCRAGE=0. )

#

CHARMECA=AFFE_CHAR_MECA(
                      MODELE=MODELE,DDL_IMPO=(
                    _F(  GROUP_NO='ENCAST',
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,
                               DRX = 0.,
                               DRY = 0.,
                               DRZ = 0.),
                               ),
                RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'NON',
                               RELA_CINE = 'OUI'),
                PRES_REP=_F(PRES = -1.E7,GROUP_MA = 'GROUPE_____VOILE',),
                        INFO=1 )
#
# -----------------------------------------------------------
# DEFINITION DE LA FORCE D'IMPACT LINEAIRE
# -----------------------------------------------------------

FONCCB=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,    0,
                           1e-5,  2e-4,
                           2e-4,  0.01,
                           5e-4,  0.1,
                           1e-3,  0.25,
                           2e-3,  0.7,
                           3e-3,  0.0,
                           ),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     );


U = CALC_EUROPLEXUS(
   MODELE=MODELE,
   CHAM_MATER=CHMATER,
   COMPORTEMENT =(_F(
                  RELATION = 'ELAS',
                  GROUP_MA = ('GROUPE__CABLE001','GROUPE_____VOILE'),
                 ),
              ),
   CARA_ELEM=CARAELEM,
   EXCIT=(_F(CHARGE=CHARMECA,FONC_MULT=FONCCB,),),
   LANCEMENT ='OUI',
   CALCUL = _F(TYPE_DISCRETISATION  ='UTIL',
               INST_INIT = 0,
               INST_FIN  = 2e-3,
               NMAX      = 100,
               PASFIX    = 2e-5,
               ),
   OBSERVATION = _F(PAS_NBRE=1,
                    TOUT_GROUP_NO = 'OUI',
                    TOUT_GROUP_MA = 'OUI',
                    NOM_CHAM = ('DEPL','VITE','ACCE'),
                    ),
   ARCHIVAGE   = _F(PAS_NBRE=1,),
   );


IMPR_RESU(RESU=_F(RESULTAT=U,NUME_ORDRE=(20,50,100),NOM_CHAM=('DEPL','VITE','ACCE','SIEF_ELGA')))



temps=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=(_F(JUSQU_A=2e-3,NOMBRE=100,),
                                    ),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = temps,),)



U_ASTER=DYNA_NON_LINE(MODELE=MODELE,
                      CHAM_MATER=CHMATER,
                      CARA_ELEM=CARAELEM,
                      MASS_DIAG='OUI',
                      SCHEMA_TEMPS=_F(SCHEMA='DIFF_CENT',FORMULATION='ACCELERATION'),
                      EXCIT=_F(CHARGE=CHARMECA,FONC_MULT=FONCCB),
                      COMPORTEMENT=(_F(RELATION='ELAS',GROUP_MA='GROUPE_____VOILE',DEFORMATION='GROT_GDEP',),
                                 _F(RELATION='ELAS',GROUP_MA='GROUPE__CABLE001',DEFORMATION='PETIT_REAC',),
                                ),
                      INCREMENT   =_F(LIST_INST=DEFLIST,),
                      )

IMPR_RESU(RESU=_F(RESULTAT=U_ASTER,NUME_ORDRE=(20,50,100),
          NOM_CHAM=('DEPL','SIEF_ELGA')))


TEST_RESU(RESU=_F(INST=4.0000000000000002E-4,
                  RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOEUD='NC001003',
                  NOM_CMP='DZ',
                  VALE_CALC=2.81012743199E-05,
                  TOLE_MACHINE=1.E-3,),
          )

TEST_RESU(RESU=_F(INST=4.0000000000000002E-4,
                  RESULTAT=U_ASTER,
                  NOM_CHAM='DEPL',
                  NOEUD='NC001003',
                  NOM_CMP='DZ',
                  VALE_CALC=2.8108463009E-05,
                  TOLE_MACHINE=1.E-3,),
          )

FIN()
