# CHARGEMENT EN TRACTION
# MAILLAGE : TRIA6
# COMPORTEMENT ELASTIQUE

DEBUT(CODE=_F(
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

#####################################
###Definition de la fissure en dur###
#####################################

#Equivalent quatre noeuds sur le maillage initial
Lfiss = 3.5;
Dmax = Lfiss / 2.;


# Vect_KI
#  c'est le vecteur pour avoir l'erreur sur KI

vx = 0.0;

vy = 1.0;

vz = 0.0;

#Fond de fissure

X0 = 3.5;

Y0 = 8.0;

#####################################
###Definition de la fissure en dur###
#####################################

young = 200000.0;
nu = 0.3;

#Fonctions necessaires a la definition du chargement pour le probleme dual
const = ((young * sqrt((2 * pi))) / 8.);


FPLUSX = FORMULE(VALE='const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vx',
                 NOM_PARA=('X','Y',),);

FPLUSY = FORMULE(VALE='const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vy',
                 NOM_PARA=('X','Y',),);

FPLUSZ = FORMULE(VALE='(const/(1+nu))*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vz',
                 NOM_PARA=('X','Y',),);

FMOINX = FORMULE(VALE='-const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vx',
                 NOM_PARA=('X','Y',),);

FMOINY = FORMULE(VALE='-const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vy',
                 NOM_PARA=('X','Y',),);

FMOINZ = FORMULE(VALE='-(const/(1+nu))*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vz',
                 NOM_PARA=('X','Y',),);

MAI=LIRE_MAILLAGE(FORMAT='MED',);

MAIB=CREA_MAILLAGE(MAILLAGE=MAI,
                   LINE_QUAD=_F(TOUT='OUI',
                                PREF_NOEUD='QN',
                                PREF_NUME=1,),);

MAIB=MODI_MAILLAGE(reuse =MAIB,
                   MAILLAGE=MAIB,
                   MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                  GROUP_MA_FOND='FONDFISS',),);

MAIB=DEFI_GROUP(reuse =MAIB,
                MAILLAGE=MAIB,
                CREA_GROUP_NO=_F(GROUP_MA='FONDFISS',
                                 NOM='NOEUFOND',),);

MATE=DEFI_MATERIAU(ELAS=_F(E=young,
                           NU=nu,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIB,
                    AFFE=_F(TOUT='OUI',
                            MATER=MATE,),);

MODL=AFFE_MODELE(MAILLAGE=MAIB,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='C_PLAN',),);

BLOC=AFFE_CHAR_MECA(MODELE=MODL,
                    DDL_IMPO=(_F(GROUP_MA='FONDFISS',
                                 DY=0.0,),
                              _F(GROUP_MA='BLOCAGE',
                                 DX=0.0,
                                 DY=0.0,),),);

# -----------------------------
# definition du probleme primal
# -----------------------------

CHAR=AFFE_CHAR_MECA(MODELE=MODL,
                    FORCE_CONTOUR=(_F(GROUP_MA='HAUT',
                                      FY=1.0,),
                                   _F(GROUP_MA='BAS',
                                      FY=-1.0,),),);

RESU=MECA_STATIQUE(MODELE=MODL,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=BLOC,),
                          _F(CHARGE=CHAR,),),);

MAIB=DEFI_GROUP(reuse =MAIB,
                MAILLAGE=MAIB,
                CREA_GROUP_MA=(_F(NOM='CURVMAX',
                                  OPTION='SPHERE',
                                  GROUP_NO_CENTRE='NOEUFOND',
                                  RAYON=Dmax,),
                               _F(NOM='LSUP',
                                  INTERSEC=('LEVSUP','CURVMAX',),),
                               _F(NOM='LINF',
                                  INTERSEC=('LEVINF','CURVMAX',),),),);

FISS=DEFI_FOND_FISS(MAILLAGE=MAIB,
                    FOND_FISS=_F(GROUP_NO='NOEUFOND',),
                    LEVRE_SUP=_F(GROUP_MA='LSUP',),
                    LEVRE_INF=_F(GROUP_MA='LINF',),);


POST_K=POST_K1_K2_K3(FOND_FISS=FISS,
                     RESULTAT=RESU,);

IMPR_TABLE(TABLE=POST_K,
           UNITE=8,);


RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION=('ERME_ELEM',
                         'ERZ1_ELEM',
                         'ERZ2_ELEM',),);

TABI=RECU_TABLE(CO=RESU,
                NOM_TABLE='ESTI_GLOB',);

IMPR_TABLE(TABLE=TABI);

# ---------------------------
# definition du probleme dual
# ---------------------------

CHARDUAL=AFFE_CHAR_MECA_F(MODELE=MODL,
                          FORCE_CONTOUR=(_F(GROUP_MA='LSUP',
                                            FX=FPLUSX,
                                            FY=FPLUSY,
                                            FZ=FPLUSZ,),
                                         _F(GROUP_MA='LINF',
                                            FX=FMOINX,
                                            FY=FMOINY,
                                            FZ=FMOINZ,),),);

RESUDUAL=MECA_STATIQUE(MODELE=MODL,
                       CHAM_MATER=CHMAT,
                       EXCIT=(_F(CHARGE=BLOC,),
                              _F(CHARGE=CHARDUAL,),),);

RESUDUAL=CALC_ERREUR(reuse =RESUDUAL,
                     RESULTAT=RESUDUAL,
                     OPTION=('ERME_ELEM',
                             'ERZ1_ELEM',
                             'ERZ2_ELEM',),);

TABII=RECU_TABLE(CO=RESUDUAL,
                 NOM_TABLE='ESTI_GLOB',);

IMPR_TABLE(TABLE=TABII);

# ---------------------------
# estimation de l'erreur en quantite d'interet
# ---------------------------

RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION='QIRE_ELEM',
                 RESU_DUAL=RESUDUAL,);

RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION='QIZ1_ELEM',
                 RESU_DUAL=RESUDUAL,);

RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION='QIZ2_ELEM',
                 RESU_DUAL=RESUDUAL,);

# ---------------------------
# tests sur les resultats
# ---------------------------

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=5.0000000000000001E-3,
           VALE_CALC=9.381110169,
           VALE_REFE=9.3699999999999992,
           NOM_PARA='K1',
           TABLE=POST_K,)

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERRE',
                  RESULTAT=RESU,
                  VALE_CALC=2.6654055228458001E-3,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ1',
                  RESULTAT=RESU,
                  VALE_CALC=3.4380781072905E-03,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ2',
                  RESULTAT=RESU,
                  VALE_CALC=7.4687359696583E-03,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERRE',
                  RESULTAT=RESUDUAL,
                  VALE_CALC=52.145715958030998,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ1',
                  RESULTAT=RESUDUAL,
                  VALE_CALC=66.87134739717,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ2',
                  RESULTAT=RESUDUAL,
                  VALE_CALC=142.28245781516,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_QIRE',
                  RESULTAT=RESU,
                  VALE_CALC=93.257156697599996,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_QIZ1',
                  RESULTAT=RESU,
                  VALE_CALC=3.4380781072905E-03,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_QIZ2',
                  RESULTAT=RESU,
                  VALE_CALC=7.4687359696583E-03,
                  CRITERE='RELATIF',
                  ),
          )

FIN();
