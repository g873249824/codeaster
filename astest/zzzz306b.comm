       _F(  CHAM_GD = CH2, MAILLE ='M2', POINT=6,  NOM_CMP='SIXX', VALE= 5.44591221681310E+01  , REFERENCE='AUTRE_ASTER', PRECISION = 1.E-9),
       _F(  CHAM_GD = CH2, MAILLE ='M2', POINT=7,  NOM_CMP='SIXX', VALE= 5.13991657414260E+01  , REFERENCE='AUTRE_ASTER', PRECISION = 1.E-9),
