)

# ------------------------------------------------------------------------------
DEFI_FICHIER(UNITE=54, FICHIER='./REPE_OUT/unite.54')
IMPR_FONCTION(
   FORMAT='XMGRACE',
   UNITE=54,
   COURBE=(
      _F(
         FONCTION=CTRACB,
      ),
   ),
)
DEFI_FICHIER(UNITE=54, ACTION='LIBERER')
TEST_FICHIER(FICHIER="./REPE_OUT/unite.54",
             NB_VALE_I=386,
