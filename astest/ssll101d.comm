DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#
PRE_IDEAS()

#
MAILLAGE=LIRE_MAILLAGE( )

#
MAILLAGE=DEFI_GROUP( reuse=MAILLAGE,  MAILLAGE=MAILLAGE,CREA_GROUP_NO=(
                          _F( NOM = 'ND2',  NOEUD = 'N42'),
                          _F( NOM = 'ND3',  NOEUD = 'N31'),
                          _F( NOM = 'ND4',  NOEUD = 'N36'),
                          _F( NOM = 'ND5',  NOEUD = 'N22'),
                          _F( NOM = 'ND6',  NOEUD = 'N28'),
                          _F( NOM = 'ND7',  NOEUD = 'N26'),
                          _F( NOM = 'ND8',  NOEUD = 'N24'),
                          _F( NOM = 'ND9',  NOEUD = 'N11'),
                          _F( NOM = 'ND10', NOEUD = 'N16'),
                          _F( NOM = 'ND11', NOEUD = 'N2'),
                          _F( NOM = 'ND12', NOEUD = 'N8'),
                          _F( NOM = 'ND13', NOEUD = 'N6'),
                          _F( NOM = 'ND14', NOEUD = 'N4'))
                        )

#
MODELE=AFFE_MODELE(   MAILLAGE=MAILLAGE,
                            AFFE=_F( TOUT = 'OUI',
                                  PHENOMENE = 'MECANIQUE',
                                  MODELISATION = 'TUYAU_6M')
                      )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CARELEM=AFFE_CARA_ELEM(        MODELE=MODELE,
                                POUTRE=_F( GROUP_MA = 'TOUT_ELT',
                                        SECTION = 'CERCLE',
                                        CARA = ('R', 'EP',),
                                        VALE = (0.0925, 0.00612,)),
                            ORIENTATION=_F( GROUP_NO = 'C',
                                         CARA = 'GENE_TUYAU',
                                         VALE = (1., 0., 0.,))
                          )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MATER=DEFI_MATERIAU(   ELAS=_F( E = 1.658E+11,
                               NU = 0.3,
                               RHO = 13404.10,
                               ALPHA = 0.1288E-04)
                       )

#
AFF_MAT=AFFE_MATERIAU(   MAILLAGE=MAILLAGE,
                               AFFE=_F( TOUT = 'OUI', MATER = MATER)
                         )

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------
#
COND_LIM=AFFE_CHAR_MECA(     MODELE=MODELE,
                             DDL_IMPO=_F( GROUP_NO = ('H', 'C',),
                                    DX = 0., DY = 0., DZ = 0.,
                                    DRX = 0., DRY = 0., DRZ = 0.,
                                    WI1 = 0., WO1 = 0., WO = 0.,
                                    UI2 = 0., VI2 = 0., WI2 = 0.,
                                    UO2 = 0., VO2 = 0., WO2 = 0.,
                                    UI3 = 0., VI3 = 0., WI3 = 0.,
                                    UO3 = 0., VO3 = 0., WO3 = 0.,
                                    UI4 = 0., VI4 = 0., WI4 = 0.,
                                    UO4 = 0., VO4 = 0., WO4 = 0.,
                                    UI5 = 0., VI5 = 0., WI5 = 0.,
                                    UO5 = 0., VO5 = 0., WO5 = 0.,
                                    UI6 = 0., VI6 = 0., WI6 = 0.,
                                    UO6 = 0., VO6 = 0., WO6 = 0.)
                           )

#
POI_PROP=AFFE_CHAR_MECA(      MODELE=MODELE,
                             PESANTEUR=_F(GRAVITE=9.81,
                                          DIRECTION=(0.,0.,-1.,),),
                           )

#
F_NODALE=AFFE_CHAR_MECA(   MODELE=MODELE,FORCE_NODALE=(
                       _F( GROUP_NO = 'ND2',
                                     FX = 0., FY = 0., FZ = -624.897),
                                    _F( GROUP_NO = 'ND3',
                                     FX = 0., FY = 0., FZ = -788.724),
                                    _F( GROUP_NO = ('ND4','ND10',),
                                     FX = 0., FY = 0., FZ = -327.654),
                                    _F( GROUP_NO = ('ND5','ND9',),
                                     FX = 0., FY = 0., FZ = -214.839),
                                    _F( GROUP_NO = ('ND6','ND7','ND8',),
                                     FX = 0., FY = 0., FZ = -102.5145),
                                    _F( GROUP_NO = 'ND11',
                                     FX = 0., FY = 0., FZ = -222.687),
                                    _F( GROUP_NO = ('ND12','ND13',),
                                     FX = 0., FY = 0., FZ = -117.720),
                                    _F( GROUP_NO = 'ND14',
                                     FX = 0., FY = 0., FZ = -176.580))
                           )

#
TEMP=CREA_CHAMP(     MAILLAGE=MAILLAGE,
                     TYPE_CHAM='NOEU_TEMP_R',
                     OPERATION='AFFE',AFFE=(
                           _F( TOUT = 'OUI',
                                 NOM_CMP = 'TEMP',
                                 VALE = 472.22),),
                     )

#
TEMPER=CREA_RESU(
               OPERATION='AFFE', TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                 AFFE=_F(
               INST = 0.0,
               CHAM_GD = TEMP)
             )

#
AFF_MAT1=AFFE_MATERIAU(   MAILLAGE=MAILLAGE,
                          AFFE=_F( TOUT = 'OUI', MATER = MATER),
                          AFFE_VARC=_F(TOUT='OUI',EVOL=TEMPER,
                          NOM_VARC='TEMP',NOM_CHAM='TEMP',VALE_REF=0.0),);
#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------
#
RESU=MACRO_ELAS_MULT(       MODELE=MODELE,
                          CHAM_MATER=AFF_MAT,
                           CARA_ELEM=CARELEM,
                          CHAR_MECA_GLOBAL=COND_LIM,CAS_CHARGE=(
                          _F( NOM_CAS = 'CHARGE NUMERO 1',
                                      CHAR_MECA = POI_PROP),
                          _F( NOM_CAS = 'CHARGE NUMERO 2',
                                      CHAR_MECA = F_NODALE),)
                        )

RESU1=MECA_STATIQUE(       MODELE=MODELE,
                          CHAM_MATER=AFF_MAT1,
                           CARA_ELEM=CARELEM,
                           EXCIT=_F( CHARGE = COND_LIM),)

CALC_CHAMP(reuse=RESU1, RESULTAT=RESU1, DEFORMATION=('EPME_ELGA', 'EPVC_ELGA',))


#
#---------------------------------------------------------------------
#                        VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
