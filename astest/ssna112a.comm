                       _F(JUSQU_A=60.0, NOMBRE=118,),
                       ),);

#---------------------------------
#  Analyses non lineaire
#---------------------------------


U1=STAT_NON_LINE(MODELE=MODELO,
                 CHAM_MATER=CHAMAT,
                 EXCIT=(_F(CHARGE=BLOQUAGE,),
                        _F(CHARGE=TRACT,
                           FONC_MULT=DEPLAC,),),
                 COMPORTEMENT=(_F(RELATION='ELAS',),
                            _F(RELATION = 'JOINT_BA',
                               GROUP_MA ='LIAISON',),
                               ),
                 INCREMENT=_F(LIST_INST=L_INST1,
                              INST_FIN=60.0,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(ITER_GLOB_MAXI=100,
                                RESI_GLOB_RELA=1.E-07,),);

U1=CALC_CHAMP(reuse=U1,CONTRAINTE=('SIGM_ELNO'),DEFORMATION=('EPSI_ELNO','EPSI_ELGA'),RESULTAT=U1)


U1=CALC_CHAMP(reuse =U1,

