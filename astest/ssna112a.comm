DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MALLA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),FORMAT='MED',
                    INFO = 2,);

MALLA=DEFI_GROUP(reuse =MALLA,
                 MAILLAGE=MALLA,
                 CREA_GROUP_MA=_F(NOM='TOUT',
                                  TOUT='OUI',),);

MALLA=DEFI_GROUP(reuse =MALLA,
                 MAILLAGE=MALLA,
                 CREA_GROUP_NO= (_F(GROUP_MA='ENCAST1'),
                                 _F(GROUP_MA='EJE1'),
                 ),);


MALLA = MODI_MAILLAGE (reuse = MALLA,
                       MAILLAGE = MALLA,
                       ORIE_FISSURE = _F(GROUP_MA = 'LIAISON'),
                       INFO = 2,);

#---------------------------------
#  Definition de modele
#---------------------------------

MODELO=AFFE_MODELE(MAILLAGE=MALLA,
                   
                   AFFE=(_F(GROUP_MA =('CABOT',
                                       'ACIER',
                                       'BETON',
                                     ),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'AXIS'),
                         _F(GROUP_MA = 'LIAISON',
                            PHENOMENE='MECANIQUE',
                            MODELISATION = 'AXIS_JOINT',),
                           ),);

#---------------------------------
#  Definition des materiaux
#---------------------------------

ACIERMAT=DEFI_MATERIAU(ELAS=_F( E  = 2.1E+5,
                                NU = 0.3),);


BETONMAT=DEFI_MATERIAU(ELAS     =_F(NU=0.17,
                                    E=1.55E+4),);


BOND=DEFI_MATERIAU(ELAS=_F(E=1.55E+4,
                           NU=0.17,),
                   JOINT_BA=_F(
                                 HPEN   =  0.64,
                                 GTT    =  6.65E+3,
                                 GAMD0  =  5.E-4,
                                 AD1    =  1.0,
                                 BD1    =  0.5,
                                 GAMD2  =  0.96,
                                 AD2    =  4.E-5,
                                 BD2    =  1.0,
                                 VIFROT =  10.0,
