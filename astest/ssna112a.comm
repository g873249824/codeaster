                                 FC     =  1.0,
                                 EPSTR0 =  0.9,
                                 ADN    =  1.E-9,
                                 BDN    =  1.5,
                                    ),);


CHAMAT=AFFE_MATERIAU(MAILLAGE=MALLA,
                     MODELE=MODELO,
                     AFFE=(_F(GROUP_MA = 'BETON',
                              MATER=BETONMAT,),
                           _F(GROUP_MA = 'ACIER',
                              MATER=ACIERMAT,),
                           _F(GROUP_MA = 'LIAISON',
                              MATER=BOND,),
                           ),);
#---------------------------------
#  Conditions limits
#---------------------------------

BLOQUAGE=AFFE_CHAR_MECA(MODELE=MODELO,
                        DDL_IMPO=(_F(GROUP_NO=('ENCAST1','PB3',),
                                     DX=0.0,
                                     DY=0.0,),
                                  _F(GROUP_NO='EJE1',
                                     DX=0.0,),
                                     ),);


TRACT=AFFE_CHAR_MECA(MODELE=MODELO,
                        DDL_IMPO=(_F(GROUP_NO=('PA2','PA3',),
                                     DY= 1.00
                                     ),),);

#---------------------------------
#  Functions de chargement
#---------------------------------

DEPLAC=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,0.0,
                           1.0,0.1,),
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',);


