                                   VALE=(0.,0.,-1.,0.,1.,0.,),),);
# ************* CHAMP DE VITESSE INITIALE ***************

CHAMP1=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                  OPERATION='AFFE',
                  MAILLAGE=MAILL,
                  AFFE=_F(
                  GROUP_NO='BARRE',
                  NOM_CMP=('DX','DY','DZ',),
                  VALE=(0.0,0.0,-100.,),),);
