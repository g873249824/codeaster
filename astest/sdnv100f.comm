# CALCUL EN EXPLICITE

DEBUT(CODE=_F(NOM='SDNV100F',
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU
#++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
#  CALCUL TEST ELEMENTS DISCRETS DE CONTACT EN DYNAMIQUE
#++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

MAILO=LIRE_MAILLAGE(FORMAT='MED',);

MAILL=CREA_MAILLAGE(MAILLAGE=MAILO,
                    CREA_POI1=_F(NOM_GROUP_MA='BASE',
                                 GROUP_MA='SBAS',),);

MAILL=DEFI_GROUP(reuse =MAILL,
                 MAILLAGE=MAILL,
                 CREA_GROUP_NO=_F(GROUP_MA='BARRE',),);

MODE=AFFE_MODELE(MAILLAGE=MAILL,
                 VERIF='MAILLE',
                 AFFE=(_F(GROUP_MA='BARRE',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='3D',),
                       _F(GROUP_MA='BASE',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='DIS_T',),),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.E11,
                            NU=0.,
                            RHO=8000.,),);

MATCHO=DEFI_MATERIAU(DIS_CONTACT=_F(RIGI_NOR=1.E10,
                                    JEU=0.,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL,
                 AFFE=(_F(GROUP_MA='BARRE',
                          MATER=ACIER,),
                       _F(GROUP_MA='BASE',
                          MATER=MATCHO,),),);

CH=AFFE_CHAR_MECA(MODELE=MODE,
                  DDL_IMPO=_F(GROUP_MA='BARRE',
                              DX=0.,
                              DY=0.,),);

CARA=AFFE_CARA_ELEM(MODELE=MODE,
                    DISCRET=(_F(GROUP_MA='BASE',
                               REPERE='LOCAL',
                               CARA='K_T_D_N',
                               VALE=(0.,0.,0.,),),
                             _F(GROUP_MA='BASE',
                               REPERE='LOCAL',
                               CARA='M_T_D_N',
                               VALE=(0.,),),),
                    ORIENTATION=_F(GROUP_MA='BASE',
                                   CARA='VECT_X_Y',
                                   VALE=(0.,0.,-1.,0.,1.,0.,),),);
# ************* CHAMP DE VITESSE INITIALE ***************

CHAMP1=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                  OPERATION='AFFE',
                  MAILLAGE=MAILL,
                  AFFE=_F(
                  GROUP_NO='BARRE',
                  NOM_CMP=('DX','DY','DZ',),
                  VALE=(0.0,0.0,-100.,),),);
# ************* CHAMP DE DEPLACEMENT INITIAL ************

CHAMP2=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                  OPERATION='AFFE',
                  MAILLAGE=MAILL,
                  AFFE=_F(
                  GROUP_NO='BARRE',
                  NOM_CMP=('DX','DY','DZ',),
                  VALE=(0.0,0.0,0.002,),),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,1.0,0.0001,1.0,0.00010001,0.0,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);
#
DT = 1e-07;

DT2 = 2e-06;

# CONDITION DE COURANT = (.005 m)/(5000 m/s) = 1e-6 s
# UNIQUEMENT SUR LES ELEMENTS ISOPARAMETRIQUES
# ATTENTION A LA REDUCTION DU PAS DE TEMPS DU A LA PENALISATION DE CHOC

DTC=.1*1.e-6

L_COURA=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A=0.00014,
                                    PAS=DTC,),);

L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A=0.00014,
                                    PAS=DT,),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A=0.00014,
                                    PAS=DT2,),);
#

L_RESU=DEFI_LIST_REEL(DEBUT=DT,
                      INTERVALLE=_F(JUSQU_A=0.000138,
                                    PAS=DT,),);
#

L_CONT=DEFI_LIST_REEL(DEBUT=2.E-5,
                      INTERVALLE=_F(JUSQU_A=10.E-5,
                                    PAS=DT,),);

U=DYNA_NON_LINE(MODELE=MODE,
                  CHAM_MATER=CM,
                  CARA_ELEM=CARA,
                  MASS_DIAG='NON',
                  EXCIT=_F(CHARGE=CH,
                           FONC_MULT=RAMPE,),
                  COMP_INCR=_F(RELATION='DIS_CHOC',
                               GROUP_MA='BASE',),
                  COMP_ELAS=_F(RELATION='ELAS',
                               GROUP_MA='BARRE',),
                  ETAT_INIT=_F(DEPL=CHAMP2,
                               VITE=CHAMP1,),
                  INCREMENT=_F(LIST_INST=L_INST,
                               INST_INIT=0.,),
                  SCHEMA_TEMPS=_F(SCHEMA='DIFF_CENT',
                                  FORMULATION='ACCELERATION',),)




VITECONT=RECU_FONCTION(RESULTAT=U,
                       NOM_CHAM='VITE',
                       NOM_CMP='DZ',
                       NOEUD='N5',
                       TITRE='VITE',);

VC=CALC_FONCTION(COMB=_F(FONCTION=VITECONT,
                         COEF=12500.,),);

MVC=CALC_FONCTION(INTEGRE=_F(FONCTION=VC,),);

DE100=RECU_FONCTION(RESULTAT=U,
                    NOM_CHAM='DEPL',
                    NOM_CMP='DZ',
                    NOEUD='N100',
                    TITRE='DEPL',);

VI100=RECU_FONCTION(RESULTAT=U,
                    NOM_CHAM='VITE',
                    NOM_CMP='DZ',
                    NOEUD='N100',
                    TITRE='VITE',);

DE5=RECU_FONCTION(RESULTAT=U,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  NOEUD='N5',
                  TITRE='DEPL',);

VI5=RECU_FONCTION(RESULTAT=U,
                  NOM_CHAM='VITE',
                  NOM_CMP='DZ',
                  NOEUD='N5',
                  TITRE='VITE',);

tpost=POST_ELEM(ENER_CIN=_F(TOUT='OUI',),
                RESULTAT=U,);


TEST_RESU(RESU=(_F(RESULTAT=U,
                   INST=1.e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=0.001,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(0.001,1e-8),
                   REFERENCE='ANALYTIQUE'),
                _F(RESULTAT=U,
                   INST=2.4e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N100',
                   VALE=-.00038,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(.06,1e-8),
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=U,
                   INST=8.e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=-.0008,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(.03,1e-8),
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=U,
                   INST=12.e-5,
                   NOM_CHAM='VITE',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=100.0,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(0.05,1e-8),
                   REFERENCE='ANALYTIQUE'),
                _F(RESULTAT=U,
                   INST=12.e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=0.002,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(.05,1e-8),
                   REFERENCE='ANALYTIQUE',)))
#
# Test de l energie cinetique

TEST_TABLE(TABLE=tpost,
           FILTRE=(_F(NOM_PARA='LIEU',
                      VALE_K='MAILL',),
                   _F(NOM_PARA='INST',
                      CRITERE='ABSOLU',
                      PRECISION=1.e-8,
                      VALE=1.e-6,),),
           NOM_PARA='TOTALE',
           VALE=800.0,
           PRECISION=0.0001,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=tpost,
           FILTRE=(_F(NOM_PARA='LIEU',
                      VALE_K='MAILL',),
                   _F(NOM_PARA='INST',
                      CRITERE='ABSOLU',
                      PRECISION=1.e-8,
                      VALE=1.2e-4,),),
           NOM_PARA='TOTALE',
           VALE=800.,
           PRECISION=0.01,
           REFERENCE='ANALYTIQUE', );



# TEST DE LA MASSE DIAGONALE

UDIAG=DYNA_NON_LINE(MODELE=MODE,
                      CHAM_MATER=CM,
                      CARA_ELEM=CARA,
                      MASS_DIAG='OUI',
                      EXCIT=_F(CHARGE=CH,
                               FONC_MULT=RAMPE,),
                      COMP_INCR=_F(RELATION='DIS_CHOC',
                                   GROUP_MA='BASE',),
                      COMP_ELAS=_F(RELATION='ELAS',
                                   GROUP_MA='BARRE',),
                      ETAT_INIT=_F(DEPL=CHAMP2,
                                   VITE=CHAMP1,),
                      INCREMENT=_F(LIST_INST=L_COURA,
                                   INST_INIT=0.,),
                      SCHEMA_TEMPS=_F(SCHEMA='DIFF_CENT',
                                       FORMULATION='ACCELERATION',),)




VITEDIAG=RECU_FONCTION(RESULTAT=UDIAG,
                       NOM_CHAM='VITE',
                       NOM_CMP='DZ',
                       NOEUD='N5',
                       TITRE='VITE',);

VCDIAG=CALC_FONCTION(COMB=_F(FONCTION=VITEDIAG,
                             COEF=12500.,),);

MVCDIAG=CALC_FONCTION(INTEGRE=_F(FONCTION=VCDIAG,),);

DE100DIA=RECU_FONCTION(RESULTAT=UDIAG,
                       NOM_CHAM='DEPL',
                       NOM_CMP='DZ',
                       NOEUD='N100',
                       TITRE='DEPL',);

VI100DIA=RECU_FONCTION(RESULTAT=UDIAG,
                       NOM_CHAM='VITE',
                       NOM_CMP='DZ',
                       NOEUD='N100',
                       TITRE='VITE',);

DE5DIA=RECU_FONCTION(RESULTAT=UDIAG,
                     NOM_CHAM='DEPL',
                     NOM_CMP='DZ',
                     NOEUD='N5',
                     TITRE='DEPL',);

VI5DIA=RECU_FONCTION(RESULTAT=UDIAG,
                     NOM_CHAM='VITE',
                     NOM_CMP='DZ',
                     NOEUD='N5',
                     TITRE='VITE',);

tpostDIA=POST_ELEM(ENER_CIN=_F(TOUT='OUI',),
                   RESULTAT=UDIAG,);

TEST_RESU(RESU=(_F(RESULTAT=UDIAG,
                   INST=1.e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=0.001,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(0.001,1e-8),
                   REFERENCE='ANALYTIQUE'),
                _F(RESULTAT=UDIAG,
                   INST=2.4e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N100',
                   VALE=-.00038,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(.06,1e-8),
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=UDIAG,
                   INST=8.e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=-.0008,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(.03,1e-8),
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=UDIAG,
                   INST=12.e-5,
                   NOM_CHAM='VITE',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=100.0,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(0.3,1e-8),
                   REFERENCE='ANALYTIQUE'),
                _F(RESULTAT=UDIAG,
                   INST=12.e-5,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   NOEUD='N5',
                   VALE=0.002,
                   CRITERE=('RELATIF','ABSOLU'),
                   PRECISION=(.08,1e-8),
                   REFERENCE='ANALYTIQUE',)))


# Test de l energie cinetique

TEST_TABLE(TABLE=tpostDIA,
           FILTRE=(_F(NOM_PARA='LIEU',
                      VALE_K='MAILL',),
                   _F(NOM_PARA='INST',
                      VALE=1.e-6,
                      CRITERE='ABSOLU',
                      PRECISION=1.e-8),),
           NOM_PARA='TOTALE',
           VALE=800.0,
           PRECISION=0.0001,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=tpostDIA,
           FILTRE=(_F(NOM_PARA='LIEU',
                      VALE_K='MAILL',),
                   _F(NOM_PARA='INST',
                      VALE=1.2e-4,
                      CRITERE='ABSOLU',
                      PRECISION=1.e-8),),
           NOM_PARA='TOTALE',
           VALE=800.,
