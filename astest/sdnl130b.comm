DEPCY = RECU_FONCTION(RESULTAT=RERE, NOM_CHAM='DEPL',      NOM_CMP='DY',   GROUP_NO='C',)
# Reaction d'appui en A
REACA = RECU_FONCTION(RESULTAT=RERE, NOM_CHAM='FORC_NODA', NOM_CMP='DY',   GROUP_NO='A',)

