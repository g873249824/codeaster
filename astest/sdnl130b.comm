DEBUT(CODE=_F(NOM = 'SDNL130B',NIV_PUB_WEB='INTERNET'))
# LECTURE DU SEISME
INCLUDE(UNITE=92)
# LECTURE MAILLAGE LINEIQUE
MAPOU = LIRE_MAILLAGE(FORMAT='MED',UNITE = 17,)
# LECTURE MAILLAGE SECTION1
MASEC = LIRE_MAILLAGE(FORMAT='MED',UNITE = 18,)
#
MAPOU = DEFI_GROUP (reuse=MAPOU, MAILLAGE =MAPOU, CREA_GROUP_NO =_F(NOM='TOUS_NO',GROUP_MA='POUTRE',),)

MOPOU=AFFE_MODELE(
   MAILLAGE=MAPOU,
   AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='POU_D_EM',),)

# LES MATERIAUX
EB = 3.7272E10
BETON = DEFI_MATER_GC(
   MAZARS=_F(
      UNITE_LONGUEUR = 'M',
      FCJ=40.963E+06, EIJ=EB,
      EPSI_C=1.75754E-03, AT=1.0,NU=0.2,
   ),
   RHO=2400.0, AMOR_ALPHA=8.E-5,AMOR_BETA=20.,
)

ACIER=DEFI_MATER_GC(
   ACIER=_F(
      E = 2.1E+11,
      D_SIGM_EPSI=3.28E+9, SY=4.E+8,
   ),
   RHO=7800., ALPHA=1.E-5, AMOR_ALPHA=8.E-5,AMOR_BETA=20.,
)

#
GF=DEFI_GEOM_FIBRE(
   SECTION= _F(GROUP_FIBRE = 'BETON', MAILLAGE_SECT = MASEC , TOUT_SECT = 'OUI', COOR_AXE_POUTRE = (0.,0.,)),
   FIBRE=   _F(CARA = 'SURFACE', GROUP_FIBRE = 'ACIER', COOR_AXE_POUTRE = (0., 0.,),
               VALE =(   0.056, -0.218, 0.804E-3,
                        -0.056, -0.218, 0.804E-3,
                         0.056,  0.218, 0.804E-3,
                        -0.056,  0.218, 0.804E-3, ),),
)
#
# ORIENTATIONS ET RIGIDITE DE TORSION
POUCA=AFFE_CARA_ELEM(
   MODELE=MOPOU,    INFO=1,
   GEOM_FIBRE=GF,
   POUTRE=  _F(GROUP_MA=('POUTRE'), SECTION='RECTANGLE', CARA = ('HY','HZ'), VALE = (.2 , .5 ),  ),
   ORIENTATION=_F(GROUP_MA=('POUTRE'), CARA='ANGL_VRIL', VALE=-90.0,),
   MULTIFIBRE=_F(GROUP_MA='POUTRE',GROUP_FIBRE=('BETON','ACIER'),
                  PREC_AIRE=3.3E-02,PREC_INERTIE=2.817E-01,),
)

COMP1=DEFI_COMPOR(
   GEOM_FIBRE=GF,MATER_SECT=BETON,
   MULTIFIBRE=(
      _F(GROUP_FIBRE='BETON',MATER=BETON,RELATION='MAZARS_GC'),
      _F(GROUP_FIBRE='ACIER',MATER=ACIER,RELATION='VMIS_CINE_GC'),)
)

# LES CHAMPS DE MATERIAUX
CHMAT=AFFE_MATERIAU(
   MAILLAGE=MAPOU,
   AFFE=_F(GROUP_MA='POUTRE', MATER=(BETON,ACIER)),
   AFFE_COMPOR=_F(GROUP_MA='POUTRE',COMPOR=COMP1) )
#
BLOQUAGE=AFFE_CHAR_MECA(
   MODELE=MOPOU,
   DDL_IMPO=(
      _F(GROUP_NO='A', DX=0.0, DY=0.0, DZ=0.0, DRX=0.0, DRY=0.0,),
      _F(GROUP_NO='B', DY=0.0,),
      _F(GROUP_NO='TOUS_NO', DZ=0.0, DRX=0.0, DRY=0.0,), ),
)

MASS_ELE=CALC_MATR_ELEM(MODELE=MOPOU, OPTION='MASS_MECA', CARA_ELEM=POUCA, CHAM_MATER=CHMAT, CHARGE=BLOQUAGE,)
RIGI_ELE=CALC_MATR_ELEM(MODELE=MOPOU, OPTION='RIGI_MECA', CARA_ELEM=POUCA, CHAM_MATER=CHMAT, CHARGE=BLOQUAGE,)
NUMEDDL=NUME_DDL(MATR_RIGI=RIGI_ELE, METHODE='LDLT', RENUM='SANS',)
MASSE=ASSE_MATRICE(MATR_ELEM=MASS_ELE, NUME_DDL=NUMEDDL,)
#
# ON DEFINIT L'ACCELEROGRAMME DU SEISME
ACCELERO = CALC_FONCTION( COMB= _F(FONCTION= ACS2C11, COEF= 45.))
# ET SA DIRECTION SOUS FORME DE CHAM_NO_DEPL_R
DEPSEISM=CALC_CHAR_SEISME (MATR_MASS=MASSE, MONO_APPUI='OUI', DIRECTION=(0., -1., 0.),)

# ON TRANSFORME LE CHAM_NO_DEPL_R ET CHAR_MECA
CHASEISM=AFFE_CHAR_MECA (MODELE=MOPOU, VECT_ASSE=DEPSEISM,)

# LES INSTANTS DE CALCUL
LINSTD= DEFI_LIST_REEL(
   DEBUT=0.0,
   INTERVALLE=(
      _F(JUSQU_A =  2.5 , NOMBRE =  250),
      _F(JUSQU_A =  3.0 , NOMBRE =  100),
      #_F(JUSQU_A = 15.0 , NOMBRE = 2400),
   ),
)

LINST = DEFI_LIST_INST(
   DEFI_LIST=_F(
      METHODE   = 'MANUEL',
      LIST_INST =  LINSTD, ),
   ECHEC=_F(
      EVENEMENT       = 'ERREUR',
      ACTION          = 'DECOUPE',
      SUBD_METHODE    = 'MANUEL',
      SUBD_PAS        = 4,
      SUBD_PAS_MINI   = 1.0E-10,
      SUBD_NIVEAU     = 5,
   ),
)

# CALCUL DYNAMIQUE NON LINEAIRE
RERE = DYNA_NON_LINE(
   MODELE=MOPOU,
   CHAM_MATER=CHMAT,
   CARA_ELEM=POUCA,
   SCHEMA_TEMPS=_F(SCHEMA='NEWMARK', FORMULATION='DEPLACEMENT',),
   NEWTON=_F(MATRICE='TANGENTE',REAC_ITER=1,),
   EXCIT=(
      _F(CHARGE=BLOQUAGE,),
      _F(CHARGE=CHASEISM, FONC_MULT=ACCELERO,), ),
   COMP_INCR=_F(RELATION='MULTIFIBRE', TOUT='OUI'),
   INCREMENT=_F(LIST_INST=LINST,),
   CONVERGENCE=_F(RESI_GLOB_RELA=1.E-5, ITER_GLOB_MAXI=15, ARRET='OUI',),
   ARCHIVAGE=_F( LIST_INST=LINSTD, ),
)
#
RERE=CALC_CHAMP(reuse =RERE, RESULTAT=RERE, FORCE='FORC_NODA', TOUT_ORDRE='OUI', TOUT='OUI',)
# QUELQUES TEST DE RESULTATS
TEST_RESU(RESU=(_F(GROUP_NO='A',
                   INST=1.76,
                   RESULTAT=RERE,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=-3.8515766763984E4,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                _F(GROUP_NO='C',
                   INST=1.76,
                   RESULTAT=RERE,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=1.4170139074950001E-3,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                _F(GROUP_NO='A',
                   INST=2.0499999999999998,
                   RESULTAT=RERE,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=2.8814150604268001E4,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                _F(GROUP_NO='C',
                   INST=2.0499999999999998,
                   RESULTAT=RERE,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-9.3311630121311999E-4,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                _F(GROUP_NO='A',
                   INST=2.6800000000000002,
                   RESULTAT=RERE,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=-7.5182532664141996E4,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                _F(GROUP_NO='C',
                   INST=2.6800000000000002,
                   RESULTAT=RERE,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=4.8174375448133003E-3,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                _F(GROUP_NO='A',
                   INST=2.8700000000000001,
                   RESULTAT=RERE,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=4.4564360755135996E4,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                _F(GROUP_NO='C',
                   INST=2.8700000000000001,
                   RESULTAT=RERE,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.8912620053437E-3,

                   CRITERE=('RELATIF', 'ABSOLU'),
                   ),
                ),
          )

