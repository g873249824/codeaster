#CAS_TEST__: TPLL100B
# CAS TEST TPLL100B

DEBUT(CODE=_F(NOM='TPLL100B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PHI0=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=0.0           )

PHI1=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=-1200.0       )

PHI2=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=1200.0       )

PHIP=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=400.   )

PHIN=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=-400.   )

T_IM=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=100.0         )

MATERI=DEFI_MATERIAU( THER_ORTH=_F(  LAMBDA_L = 1.,
          LAMBDA_T = 0.5, LAMBDA_N = 1., RHO_CP = 2.0))

MAIL=LIRE_MAILLAGE( )

MAIL=DEFI_GROUP(reuse=MAIL,MAILLAGE=MAIL, CREA_GROUP_NO=_F( TOUT_GROUP_MA = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = MATERI) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
   AFFE=_F( GROUP_MA = ('VOLU','FLUX','FPOS','FNEG',), MODELISATION = 'PLAN',
                                      PHENOMENE = 'THERMIQUE'))

ESSAI=AFFE_CARA_ELEM(MODELE=MOTH,

                MASSIF=_F(
                MAILLE = ('M1','M2','M3','M4',),
                ANGL_REP = (81.869897,  0.,  0., )))

CHTH=AFFE_CHAR_THER_F(  MODELE=MOTH,
          FLUX_REP=_F( GROUP_MA = 'FLUX',  FLUN = PHI1)
         )

CHTH2=AFFE_CHAR_THER_F(  MODELE=MOTH,

                   FLUX_REP=_F( GROUP_MA = 'FPOS',  FLUN = PHIN)

         )

CHTH3=AFFE_CHAR_THER_F(  MODELE=MOTH,

                   FLUX_REP=_F( GROUP_MA = 'FNEG',  FLUN = PHIP)

         )

CHTH4=AFFE_CHAR_THER_F(  MODELE=MOTH,

                   TEMP_IMPO=_F( NOEUD = ('N4','N5','N6',),  TEMP = T_IM)

         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,
       INTERVALLE=_F(    JUSQU_A = 10.0,   NOMBRE = 2)
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
          INCREMENT=_F( LIST_INST = LR8),
           PARM_THETA=1.,
             CHAM_MATER=CHMAT,EXCIT=( _F( CHARGE = CHTH),
          _F( CHARGE = CHTH2), _F( CHARGE = CHTH3),
          _F( CHARGE = CHTH4)),
           CARA_ELEM=ESSAI
             )

TEMPE=CALC_ELEM(reuse=TEMPE,
                OPTION=( 'FLUX_ELNO_TEMP','FLUX_ELGA_TEMP',  ),
                RESULTAT=TEMPE )


TEST_RESU(RESU=(
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   NOEUD = 'N5',     NOM_CMP = 'TEMP', VALE = 100.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   NOEUD = 'N2',     NOM_CMP = 'TEMP', VALE = 20.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   NOEUD = 'N8',     NOM_CMP = 'TEMP', VALE = 60.0),

   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELNO_TEMP',
   MAILLE = 'M1', POINT = 3, NOM_CMP = 'FLUX', VALE = 720.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELNO_TEMP',
   MAILLE = 'M2', POINT = 2, NOM_CMP = 'FLUY', VALE = 1040.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELGA_TEMP',
   MAILLE = 'M1', POINT = 3, NOM_CMP = 'FLUX', VALE = 720.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELGA_TEMP',
   MAILLE = 'M2', POINT = 2, NOM_CMP = 'FLUY', VALE = 1040.0))
          )

FIN()
