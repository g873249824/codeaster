# TEST DE PERFORMANCE ANCIENNEMENT NOMME SDLV100A
# RESOLUTION MULT_FRONT RENUM MDA


DEBUT(    CODE=_F( NOM = 'YYYY102B',NIV_PUB_WEB='INTERNET')  )

PRE_IDEAS()

M=LIRE_MAILLAGE()

MO=AFFE_MODELE(    MAILLAGE=M,    VERIF='MAILLE',
       AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = '3D') )

#
MAT=DEFI_MATERIAU(
        ELAS=_F(  E = 210000.,  NU = 0.3,  RHO = 7.8E-6) )

CM=AFFE_MATERIAU(    MAILLAGE=M,
       AFFE=_F(  TOUT = 'OUI', MATER = MAT)  )

#
CH=AFFE_CHAR_MECA(    MODELE=MO,
       DDL_IMPO=_F(  GROUP_NO = 'BLOCAGE',  DX = 0.,  DY = 0.,  DZ = 0.))

#
MER=CALC_MATR_ELEM(  OPTION='RIGI_MECA',
      MODELE=MO,  CHARGE=CH,  CHAM_MATER=CM )

MEM=CALC_MATR_ELEM(  OPTION='MASS_MECA',
      MODELE=MO,  CHARGE=CH,  CHAM_MATER=CM )

#
NU=NUME_DDL(    MATR_RIGI=MER, )

A=ASSE_MATRICE(MATR_ELEM=MER,NUME_DDL=NU)

B=ASSE_MATRICE(MATR_ELEM=MEM,NUME_DDL=NU)

#
MODE=MODE_ITER_SIMULT(    MATR_A=A,     MATR_B=B,
         CALC_FREQ=_F(  OPTION = 'PLUS_PETITE',   NMAX_FREQ = 10) )

TEST_RESU(RESU=(
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 1,  VALE = 0.31401786E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 2,  VALE = 0.77987974E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 3,  VALE = 2.5495422E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 4,  VALE = 2.7474513E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 5,  VALE = 3.7961008E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 6,  VALE = 6.6220969E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 7,  VALE = 10.122032E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 8,  VALE = 12.562072E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ',
           NUME_ORDRE = 9,  VALE = 18.980812E+00),
     _F( RESULTAT = MODE,  PARA = 'FREQ', PRECISION = 1.E-2,
           NUME_ORDRE = 10, VALE = 21.922584E+00))
             )

