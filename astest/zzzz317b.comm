#"JENNINGS_HOUSNER"
#----------------------------------------------------------------
#
ACCE4=GENE_ACCE_SEISME( INFO  =2, INIT_ALEA=10000,
              PAS_INST       = DT,PESANTEUR =9.81,DUREE_PHASE_FORTE =TSM,
              FREQ_FILTRE=0.05, FREQ_CORNER = 0.25,
              DSP        = _F(AMOR_REDUIT  =amor,  FREQ_FOND =frr, FREQ_PENTE=fp),
              MODULATION      = _F(TYPE=   "JENNINGS_HOUSNER", INTE_ARIAS =arias), )

FONCT4=RECU_FONCTION(    TABLE=ACCE4,
                              FILTRE=_F(  NOM_PARA = 'NUME_ORDRE',
                                       VALE_I = 1),
                              NOM_PARA_TABL='FONCTION',)

N4_intea=INFO_FONCTION(  NOCI_SEISME=_F(
                             FONCTION = FONCT4,
                             PESANTEUR = 9.81,OPTION = ('INTE_ARIAS') ))


N4_duree=INFO_FONCTION(  NOCI_SEISME=_F(
                             FONCTION = FONCT4,
                             PESANTEUR = 9.81, OPTION = ('DUREE_PHAS_FORT' ) ))


RMS4=INFO_FONCTION( ECART_TYPE=_F(  FONCTION = FONCT4))
#
TEST_TABLE(
           VALE_CALC = 0.0692962334718  ,
           NOM_PARA='INTE_ARIAS',
           TABLE=N4_intea,
           )

TEST_TABLE(
           VALE_CALC=5.02,
           NOM_PARA='DUREE_PHAS_FORT',
           TABLE=N4_duree,
           )

TEST_TABLE(
           VALE_CALC=0.16980052702,
           NOM_PARA='ECART_TYPE',
           TABLE=RMS4,
           )

