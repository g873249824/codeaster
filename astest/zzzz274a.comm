DEBUT(CODE=_F(
              NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='OUI',),
              DEBUG=_F(SDVERI='OUI'),);

ALPHA = 0.0
BETA  = 0.0
GAMMA = 0.0
COSA = cos(ALPHA*pi/180 )
SINA = sin(ALPHA*pi/180 )
COSB = cos(BETA *pi/180 )
SINB = sin(BETA *pi/180 )
COSG = cos(GAMMA*pi/180 )
SING = sin(GAMMA*pi/180 )

MA=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(GROUP_MA=('P1'),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='2D_DIS_T',),);

CARNS_G=AFFE_CARA_ELEM(
   MODELE=MO,INFO=2,
   DISCRET_2D=(
      _F(CARA='K_T_N',
         GROUP_MA='P1',
         REPERE='GLOBAL',
         SYME='NON',
         VALE=(
         COSA*COSB*(COSA*COSB+(SING*SINB*COSA-COSG*SINA))+(SING*SINB*COSA-COSG*SINA)*(SING*SINB*COSA-COSG*SINA),
         COSB*SINA*(COSA*COSB+(SING*SINB*COSA-COSG*SINA))+(COSG*COSA+SING*SINB*SINA)*(SING*SINB*COSA-COSG*SINA),
         COSA*COSB*(COSB*SINA+(COSG*COSA+SING*SINB*SINA))+(SING*SINB*COSA-COSG*SINA)*(COSG*COSA+SING*SINB*SINA),
         COSB*SINA*(COSB*SINA+(COSG*COSA+SING*SINB*SINA))+(COSG*COSA+SING*SINB*SINA)*(COSG*COSA+SING*SINB*SINA),
         ),),
   ),
)

CARNS_L=AFFE_CARA_ELEM(
   MODELE=MO,INFO=2,
   DISCRET_2D=(
      _F(CARA='K_T_N', GROUP_MA='P1', REPERE='LOCAL', SYME='NON', VALE=(1,0,1,1,),),
   ),
   ORIENTATION=_F(GROUP_MA='P1',
                  CARA='ANGL_NAUT',
                  VALE=(ALPHA,BETA,GAMMA,),),
)

MAT=DEFI_MATERIAU( ELAS=_F(E=1, NU=.3, RHO=1,),);

AFMA=AFFE_MATERIAU(
   MAILLAGE=MA,
   AFFE=_F(TOUT='OUI', MATER=MAT,),)


DEPL=AFFE_CHAR_MECA(
   MODELE=MO,
   DDL_IMPO=(
      _F( GROUP_NO='P1', DX=2.0 , DY=1.0),
   ),
)

FORC=AFFE_CHAR_MECA(
   MODELE=MO,
   FORCE_NODALE=(
      _F( GROUP_NO='P1', FY=1.0 ),
   ),
)

RDEPNS_G=MECA_STATIQUE(
   MODELE=MO,
   CHAM_MATER=AFMA,
   CARA_ELEM=CARNS_G,
   EXCIT=_F(CHARGE=DEPL,),
)
RDEPNS_G=CALC_CHAMP(reuse =RDEPNS_G,
                 RESULTAT=RDEPNS_G,
                 FORCE='REAC_NODA');

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RDEPNS_G,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   VALE_CALC=3.000000000,
                   VALE_REFE=3.0,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RDEPNS_G,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,),
                ),
          )

RFORNS_G=MECA_STATIQUE(
   MODELE=MO,
   OPTION='SANS',
   CHAM_MATER=AFMA,
   CARA_ELEM=CARNS_G,
   EXCIT=_F(CHARGE=FORC,),
)

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RFORNS_G,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=-1.000000000,
                   VALE_REFE=-1.0,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RFORNS_G,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,),
                ),
          )

RDEPNS_L=MECA_STATIQUE(
   MODELE=MO,
   CHAM_MATER=AFMA,
   CARA_ELEM=CARNS_L,
   EXCIT=_F(CHARGE=DEPL,),
)
RDEPNS_L=CALC_CHAMP(reuse =RDEPNS_L,
                 RESULTAT=RDEPNS_L,
                 FORCE='REAC_NODA');

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RDEPNS_L,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   VALE_CALC=3.000000000,
                   VALE_REFE=3.0,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RDEPNS_L,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DY',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,),
                ),
          )

RFORNS_L=MECA_STATIQUE(
   MODELE=MO,
   OPTION='SANS',
   CHAM_MATER=AFMA,
   CARA_ELEM=CARNS_L,
   EXCIT=_F(CHARGE=FORC,),
)

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RFORNS_L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=-1.000000000,
                   VALE_REFE=-1.0,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='P1',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RFORNS_L,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,),
                ),
          )

FIN();
