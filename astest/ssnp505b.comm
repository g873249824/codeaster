                              _F(INTEGRATION   = 'NOEUD',
                                 FISS_MAIT     = FISS4,),
                              _F(INTEGRATION   = 'NOEUD',
                                 FISS_MAIT     = FISS5,
                                 COEF_CONT     = 100000.0,),
                              _F(INTEGRATION   = 'NOEUD',
                                 FISS_MAIT     = FISS6,),
                              _F(INTEGRATION   = 'NOEUD',
                                 FISS_MAIT     = FISS7,),
                              _F(INTEGRATION   = 'NOEUD',
                                 FISS_MAIT     = FISS8,),
                             )
                      );


