DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),ERREUR=_F(ERREUR_F='EXCEPTION',),
DEBUG=_F(SDVERI='NON',HIST_ETAPE='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED')

MA=MODI_MAILLAGE(reuse =MA,
                   MAILLAGE=MA,
                   ORIE_NORM_COQUE=_F(GROUP_MA='TALLE',
                                      VECT_NORM=(0.,0.,1.,),
                                      GROUP_NO='T0',),)

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO= (_F(GROUP_MA=('A3A1'),
                                NOM='TSYMY'),
                              _F(GROUP_MA=('A1A2'),
                                NOM='TSYMX'),
                              _F(GROUP_MA=('A2A4'),
                                NOM='TCONTY'),
                              ),
                              );

MO=AFFE_MODELE(INFO=2,
               MAILLAGE=MA,AFFE=(
                     _F(  GROUP_MA = ('TALLE',),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'Q4GG'),
                     _F(  GROUP_MA = 'A5A6',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE')
                      ),)

EP=0.12
CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MO,
                       COQUE=_F(  GROUP_MA = ('TALLE',),
                               EPAIS = EP,
                               VECTEUR=(1,0,0),
                               COQUE_NCOU = 1,
                               MODI_METRIQUE = 'NON',
                               ),
                       BARRE=_F(  GROUP_MA = 'A5A6',
                               SECTION = 'GENERALE',
                               CARA =  'A',
                               VALE = 1.E-4 )
                               )

BETON = DEFI_MATERIAU(ELAS=_F(
                    E = 35700E6,
                    NU = 0.22,
                    RHO = 2500,
                   ),
                    BETON_ECRO_LINE = _F(
                                         D_SIGM_EPSI = 0.0,
                                         SYT   =  4.4E6,
                                         SYC    = -52.5E6,
                                         ),
                    BPEL_BETON=_F()
                  );

MAT_ARMA = DEFI_MATERIAU(ELAS=_F(
                    E = 210000E6,
                    NU = 0.,
                    RHO = 7500,
                   ),
                   ECRO_LINE = _F(
                                   D_SIGM_EPSI = 0.0,
                                   SY   =  500.E6,
                                   ),
                  );

ACIER = DEFI_MATERIAU(ELAS=_F(
                    E = 210000E6,
                    NU = 0.,
                    RHO = 7500,
                   ),
                   BPEL_ACIER=_F(),
                  );

FONC1 = DEFI_FONCTION(NOM_PARA='X',
             VALE = (
                 -1.E13,   0.,
                 -1.E12,   1.2E3,
                  1.E12,   1.2E3,
                  1.E13,   0.,),)

FONC2 = DEFI_FONCTION(NOM_PARA='X',
             VALE = (
                 -1.E13,   0.,
                 -1.E12,   -1.2E3,
                  1.E12,   -1.2E3,
                  1.E13,   0.,),)

MAT_GLRC = DEFI_GLRC(RELATION = 'GLRC_DAMAGE',
                 BETON = _F(
                            MATER = BETON,
                            EPAIS = EP,
                            GAMMA = 0.0,
                            QP1 = 0.5,
                            QP2 = 0.5,
                            BT1 = 1600.E6,
                            BT2 = 1600.E6,

                            C1M1 = 1.E6,   C1M2 =1.E6,   C1M3 =1.E6,
                            C2M1 = 1.E6,   C2M2 =1.E6,   C2M3 =1.E6,
                            C1N1 = 7.3E6,  C1N2 =7.3E6,  C1N3 =7.3E6,
                            C2N1 = 7.3E6,  C2N2 =7.3E6,  C2N3 =7.3E6,

                            MP1X_FO = FONC1,      MP1Y_FO = FONC1,
                            MP2X_FO = FONC2,      MP2Y_FO = FONC2,
                            ),
                  NAPPE = (_F(
                             MATER = MAT_ARMA,
                             OMX = 7.854E-4,
                             OMY = 7.854E-4,
                             RX = 0.633,
                             RY = 0.633,
                             ),
                          _F(
                             MATER = MAT_ARMA,
                             OMX = 7.854E-4,
                             OMY = 7.854E-4,
                             RX = -0.633,
                             RY = -0.633,)
                             ),
                 INFO = 2,
                 );

CHMATER=AFFE_MATERIAU(INFO=2,
                   MAILLAGE=MA,
                   AFFE=(_F(  GROUP_MA = 'TALLE',
                              MATER = MAT_GLRC),
                         _F(  GROUP_MA = 'A5A6',
                              MATER = ACIER),
                        )
                      )

CABLEPR=DEFI_CABLE_BP(MODELE=MO,
                      CHAM_MATER=CHMATER,
                      CARA_ELEM=CARAELEM,
                      GROUP_MA_BETON='TALLE',
                      DEFI_CABLE=_F(
                               GROUP_MA = 'A5A6',
                               GROUP_NO_ANCRAGE = ( 'A5',  'A6', ),),
                               TYPE_ANCRAGE = ( 'ACTIF',   'ACTIF',  ),
                               TENSION_INIT=2.0E+5,
                               RECUL_ANCRAGE=0. )

LIAISON=AFFE_CHAR_MECA(
                      MODELE=MO,
                      RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'NON',
                               RELA_CINE = 'OUI'),
                      INFO=1 )

BLOQ=AFFE_CHAR_MECA(
                      MODELE=MO,DDL_IMPO=(
                    _F(  GROUP_NO='TSYMY',
                               DX = 0.,),
                    _F(  GROUP_NO='TSYMX',
                               DY = 0.,),
                    _F(  GROUP_NO='TCONTY',
                               DZ = 0.,
                               DRX= 0.,
                               DRY= 0.,
                               DRZ= 0.,),
                               ),)

PRESS=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_COQUE=_F(PRES = 0.15E6,GROUP_MA = 'TALLE',),)

FONC=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,      0.0,
                           0.004,    1.0,),)

TEMPS=DEFI_LIST_REEL(DEBUT=-1.0,
                        INTERVALLE=(_F(JUSQU_A=0.0,NOMBRE=1,),
                                    ),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = TEMPS,),)

U_PREC=CALC_PRECONT(MODELE=MO,
                    CHAM_MATER=CHMATER,
                    CARA_ELEM=CARAELEM,
                    EXCIT=(_F(CHARGE=BLOQ,),),
                    CABLE_BP=CABLEPR,
                    COMPORTEMENT=_F(RELATION='ELAS'),
                    INCREMENT=_F(LIST_INST=TEMPS,),
                    )

U = CALC_EUROPLEXUS(
   ETAT_INIT=_F(RESULTAT=U_PREC,CONTRAINTE='OUI',EQUILIBRE='NON'),
   COMPORTEMENT =(_F(
                  RELATION = 'ELAS',
                  GROUP_MA = 'A5A6',
                 ),
               _F(
                  RELATION = 'GLRC_DAMAGE',
                  GROUP_MA = 'TALLE',
                 ),
              ),
   EXCIT=(_F(CHARGE = BLOQ),
          _F(CHARGE = LIAISON),
          _F(CHARGE=PRESS,FONC_MULT=FONC,),
         ),
   LANCEMENT ='OUI',
   CALCUL = _F(TYPE_DISCRETISATION  ='UTIL',
               INST_INIT = 0.,
               INST_FIN  = 2.5E-3,
               NMAX      = 20000000,
               PASFIX    = 1e-5,
               ),
   OBSERVATION = _F(PAS_NBRE=1,
                    GROUP_NO = 'T0',
                    NOM_CHAM = 'DEPL',
                    ),
   ARCHIVAGE   = _F(PAS_NBRE=1,),
   );

TPS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=(_F(JUSQU_A=2.5E-3,PAS=1.E-5,),
                                    ),);

DEFTPS =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = TPS,),)

U2=DYNA_NON_LINE(MODELE=MO,
                    ETAT_INIT=_F(EVOL_NOLI=U_PREC),
                    CHAM_MATER=CHMATER,
                    CARA_ELEM=CARAELEM,
                    COMPORTEMENT=(_F(RELATION='GLRC_DAMAGE',
                                 DEFORMATION='GROT_GDEP',
                                 GROUP_MA='TALLE',),
                              _F(RELATION='ELAS',
                                 DEFORMATION='PETIT_REAC',
                                 GROUP_MA='A5A6',),),
                    INCREMENT=_F(LIST_INST=DEFTPS,PRECISION=1E-10),
                    EXCIT=(_F(CHARGE=BLOQ,),
                           _F(CHARGE=LIAISON,),
                           _F(CHARGE=PRESS,
                              FONC_MULT=FONC,),),
                    SCHEMA_TEMPS=_F(SCHEMA='DIFF_CENT',
                                    FORMULATION='ACCELERATION',),
                    MASS_DIAG='OUI',);

# LA REFERENCE EST LE CALCUL DYNA_NON_LINE DE CODE_ASTER AVEC TEST DE NON REGRESSION.

TEST_RESU(RESU=(_F(RESULTAT   = U,
                   INST       = 2.5E-3,
                   NOM_CHAM   = 'DEPL',
                   NOM_CMP    = 'DZ',
                   GROUP_NO   = 'T0',
                   REFERENCE  = 'AUTRE_ASTER',
                   VALE_CALC=-0.000431991484151,
                   VALE_REFE  = -4.3214499919696E-04,
                   PRECISION=5.E-3, ),
                ),
          )

FIN()
