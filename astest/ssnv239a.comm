mail=LIRE_MAILLAGE(FORMAT='MED',);
mod=AFFE_MODELE(MAILLAGE=mail,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='3D',),);

rc1=38.3;
youn1=32000.;
epc1=2.e-3;
nu1=0.2;
mu11=(youn1/2.)/(1.+nu1);
k11=(youn1/3.)/(1.-(2.*nu1));
rt1=3.;
ept1=1.1*(rt1/youn1);

t1=1.e-2;
t2=10.e-2;

E=DEFI_CONSTANTE(VALE=youn1,);

Nu=DEFI_CONSTANTE(VALE=nu1,);

ALPHA=DEFI_CONSTANTE(VALE=0,);

RHOF=DEFI_CONSTANTE(VALE = 2300.,)
mat=DEFI_MATERIAU(ELAS_FO =_F(     E     = E,
                                   NU    = Nu,
                                   TEMP_DEF_ALPHA=12.,
                                   ALPHA   =ALPHA,
                                   K_DESSIC=0.,
                                   B_ENDOGE=0.,RHO=RHOF),
                        PORO_BETON=_F(HYDS = 0.2,
                                   F_C    = rc1,
                                   F_T    = rt1,
                                   EPS_COMP   = epc1,
                                   EPS_TRAC   = ept1,
                                   EKVP  = 2.e-3,
                                   CBIO = 0.3,
                                   MODU_EAU   = 0.,
                                   SFLD  = 12.,
                                   MODU_GEL    = 0.0,
                                   VOL_GEL   = 0.01,
                                   PORO  = 0.12,
                                   TKVP  = 0.1,
                                   NRJA  = 36500.,
                                   MSHR  = 0.,
                                   KD    = k11,
                                   MU    = mu11,
                                   DT80  = 0.3,
                                   TREF  = 12.,
                                   GFTL  = 10.e-4,
                                   GFCL  = 10.e-2,
                                   WREF  = 5e-6,
                                   TPHI  = 1.0,
                                   ANG_CRIT  = 1.,
                                   SREF  = 5.,
                                   VREF  = 1.0,
                                   VMAX  = 1.0,
                                   KWB   = 6.11,
                                   COVS  = 0.0,
                                   STMP=1.,
                                   KTMP=5.3),);

HYDRI=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                 OPERATION='AFFE',
                 MAILLAGE=mail,
                 AFFE=_F(TOUT='OUI',
                         NOM_CMP='HYDR',
                         VALE=1.0,),);

HYDRF=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
