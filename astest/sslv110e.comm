# TEST SSLV110E
# CALCUL DE G ET G LOCAL PAR LA METHODE 'THETA_LEGENDRE'
# PRESSION SUR LES LEVRES DE LA FISSURE
#
#

DEBUT(  CODE=_F( NOM = 'SSLV110E',NIV_PUB_WEB='INTERNET'))

#
# --------------------------------------------------------------------

MA=LIRE_MAILLAGE(  )

MO=AFFE_MODELE(  MAILLAGE=MA, 
                    AFFE=_F(  TOUT = 'OUI',
                           PHENOMENE = 'MECANIQUE',
                           MODELISATION = '3D', ) )

MA=MODI_MAILLAGE( reuse=MA,   MAILLAGE=MA,
                            MODELE=MO,
                        ORIE_PEAU_3D=_F(  GROUP_MA = 'GRPRES',  )
                      )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 210000.,  
                            NU = 0.3,    ))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI', 
                              MATER = MAT, ))

# ------------------------------------------------------------------
#    --- STRUCTURE EN APPUI SIMPLE AVEC PRESSION SUR LES LEVRES

CH=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
                     _F(  GROUP_NO = 'MACSYM',    DZ = 0.,  ), 
                              _F(  GROUP_NO = 'ELISYM',    DX = 0.,  ), 
                              _F(  GROUP_NO = 'GRNINF',    DY = 0.,  )), 
                     PRES_REP=_F(  GROUP_MA = 'GRPRES',  PRES = 1.,  ) )

PRESF = FORMULE(REEL="""(REEL:X,REEL:Y,REEL:Z) =
1.""")
FORCF = FORMULE(REEL="""(REEL:X,REEL:Y,REEL:Z) =
1.""")

CHFONC=AFFE_CHAR_MECA_F(  MODELE=MO, 
                            PRES_REP=_F(  GROUP_MA = 'GRPRES', 
                                       PRES = PRESF,  )    )

CHFACER=AFFE_CHAR_MECA(  MODELE=MO,
                          FORCE_FACE=_F(  GROUP_MA = 'GRPRES',  FZ = 1.,))

CHFACEF=AFFE_CHAR_MECA_F(  MODELE=MO, 
                             FORCE_FACE=_F(  GROUP_MA = 'GRPRES', 
                                          FZ = FORCF,   )      )

CHAMDEPL=MECA_STATIQUE(   MODELE=MO,
                             CHAM_MATER=CHMAT,
                              EXCIT=_F( CHARGE = CH,  ))

DEP=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=CHAMDEPL,
                         NUME_ORDRE=1 )

#
#    --- DEFINITION DU MODULE DE THETA ET DES RAYONS DEFINISSANT LA
#        COURONNE PAR FONCTIONS
#
MODUF=DEFI_FONCTION(  NOM_PARA='X',  VALE=( 0., 1.,     26.69, 1., ))

RINF1=DEFI_FONCTION(  NOM_PARA='X',  VALE=( 0., 0.,     26.69, 0., ))

RSUP1=DEFI_FONCTION(  NOM_PARA='X',  VALE=( 0., 0.1875, 26.69, 0.7812,))

RINF2=DEFI_FONCTION(  NOM_PARA='X',  VALE=( 0., 0.1875, 26.69, 0.7812,))

RSUP2=DEFI_FONCTION(  NOM_PARA='X',  VALE=( 0., 0.375,  26.69, 1.5625,))

RINF3=DEFI_FONCTION(  NOM_PARA='X',  VALE=( 0., 0.375,  26.69, 1.5625,))

RSUP3=DEFI_FONCTION(  NOM_PARA='X',  VALE=( 0., 1.6465, 26.69, 3.125, ))

FISS1=DEFI_FOND_FISS(    MAILLAGE=MA,
                   FOND_FISS=_F(  GROUP_NO = 'FONFIS',  ),
                   LEVRE_SUP=_F(  GROUP_MA = 'GRPRES',  ),
                   DTAN_ORIG=( 0.,  1.,  0.,),
                   DTAN_EXTR=( 1.,  0.,  0.,)
                    )

#----------------------------------------------------------------------
#                           COURONNE 1
#----------------------------------------------------------------------

THETA1=CALC_THETA(   OPTION='COURONNE',
                       MODELE=MO,
                      FOND_FISS=FISS1,
                        THETA_3D=_F(  TOUT = 'OUI', 
                                   MODULE_FO = MODUF, 
                                   R_INF_FO = RINF1,  
                                   R_SUP_FO = RSUP1,  )
                     )

G1=CALC_G_THETA_T(  MODELE=MO,
                      DEPL=DEP,
                     THETA=THETA1,
                    CHARGE=CHFONC,
                      COMP_ELAS=_F( RELATION = 'ELAS',     
                                 DEFORMATION = 'PETIT',  ),
                    CHAM_MATER=CHMAT )

GLOC1=CALC_G_LOCAL_T(  MODELE=MO,
                       RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                       CHAM_MATER=CHMAT,
                        FOND_FISS=FISS1,
                        DEGRE=7,
                       R_SUP_FO=RSUP1,
                       R_INF_FO=RINF1,
                        LISSAGE_THETA='LEGENDRE',
                            LISSAGE_G='LEGENDRE',
                         COMP_ELAS=_F( RELATION = 'ELAS',     
                                    DEFORMATION = 'PETIT',  ),
                       CHARGE=CHFONC,
                        INFO=1
                      )

#----------------------------------------------------------------------
#                           COURONNE 2
#----------------------------------------------------------------------

THETA2=CALC_THETA(   OPTION='COURONNE',
                       MODELE=MO,
                      FOND_FISS=FISS1,
                        THETA_3D=_F(  TOUT = 'OUI', 
                                   MODULE_FO = MODUF, 
                                   R_INF_FO = RINF2,  
                                   R_SUP_FO = RSUP2,  )
                     )

G2=CALC_G_THETA_T(  MODELE=MO,
                      DEPL=DEP,
                     THETA=THETA2,
                    CHARGE=CHFACER,
                      COMP_ELAS=_F( RELATION = 'ELAS',     
                                 DEFORMATION = 'PETIT',  ),
                    CHAM_MATER=CHMAT )

GLOC2=CALC_G_LOCAL_T(  MODELE=MO,
                       RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                       CHAM_MATER=CHMAT,
                        FOND_FISS=FISS1,
                        DEGRE=7,
                       R_SUP_FO=RSUP2,
                       R_INF_FO=RINF2,
                        LISSAGE_THETA='LEGENDRE',
                         LISSAGE_G='LEGENDRE',
                         COMP_ELAS=_F( RELATION = 'ELAS',     
                                    DEFORMATION = 'PETIT',  ),
                       CHARGE=CHFACER )

#----------------------------------------------------------------------
#                           COURONNE 3
#----------------------------------------------------------------------

THETA3=CALC_THETA(   OPTION='COURONNE',
                       MODELE=MO,
                      FOND_FISS=FISS1,
                        THETA_3D=_F(  TOUT = 'OUI', 
                                   MODULE_FO = MODUF, 
                                   R_INF_FO = RINF3,  
                                   R_SUP_FO = RSUP3,  )
                     )

G3=CALC_G_THETA_T(  MODELE=MO,
                      DEPL=DEP,
                     THETA=THETA3,
                    CHARGE=CHFACEF,
                      COMP_ELAS=_F( RELATION = 'ELAS',     
                                 DEFORMATION = 'PETIT',  ),
                    CHAM_MATER=CHMAT )

GLOC3=CALC_G_LOCAL_T(  MODELE=MO,
                       RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                       CHAM_MATER=CHMAT,
                        FOND_FISS=FISS1,
                        DEGRE=7,
                       R_SUP_FO=RSUP3,
                       R_INF_FO=RINF3,
                         COMP_ELAS=_F( RELATION = 'ELAS',     
                                    DEFORMATION = 'PETIT',  ),
                       CHARGE=CHFACEF )

#
#  CALCUL AVEC LA METHODE 'THETA_LAGRANGE'
#

GLOC4=CALC_G_LOCAL_T(  MODELE=MO,
                       RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                       CHAM_MATER=CHMAT,
                        FOND_FISS=FISS1,
                        DEGRE=7,
                        LISSAGE_THETA='LAGRANGE',
                         LISSAGE_G='LEGENDRE',
                       R_SUP_FO=RSUP2,
                       R_INF_FO=RINF2,
                         COMP_ELAS=_F( RELATION = 'ELAS',     
                                    DEFORMATION = 'PETIT',  ),
                       CHARGE=CH,
                        INFO=1
                      )

#
#  CALCUL AVEC LA METHODE 'LAGRANGE_LAGRANGE'
#

GLOC5=CALC_G_LOCAL_T(  MODELE=MO,
                       RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                       CHAM_MATER=CHMAT,
                        FOND_FISS=FISS1,
                        LISSAGE_THETA='LAGRANGE',
                         LISSAGE_G='LAGRANGE',
                       R_SUP_FO=RSUP2,
                       R_INF_FO=RINF2,
                         COMP_ELAS=_F( RELATION = 'ELAS',     
                                    DEFORMATION = 'PETIT',  ),
                       CHARGE=CH )

#
#  CALCUL AVEC LA METHODE 'NOEUD_NOEUD'
#

GLOC6=CALC_G_LOCAL_T(  MODELE=MO,
                       RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                       CHAM_MATER=CHMAT,
                        FOND_FISS=FISS1,
                        LISSAGE_THETA='LAGRANGE',
                         LISSAGE_G='LAGRANGE_NO_NO',
                       R_SUP_FO=RSUP2,
                       R_INF_FO=RINF2,
                         COMP_ELAS=_F( RELATION = 'ELAS',     
                                    DEFORMATION = 'PETIT',  ),
                       CHARGE=CH )

#----------------------------------------------------------------------
#              TEST DE G
#----------------------------------------------------------------------

TEST_TABLE(       TABLE=G1,
                       NOM_PARA='G',
                           VALE=7.0503219199279E-04,
                      PRECISION=5.E-6                )

TEST_TABLE(       TABLE=G2,
                       NOM_PARA='G',
                           VALE=7.2452560863439E-04,
                      PRECISION=5.E-6                )

TEST_TABLE(       TABLE=G3,
                       NOM_PARA='G',
                           VALE=6.9745852629086E-04,
                      PRECISION=5.E-6                )

#----------------------------------------------------------------------
#              TEST DE G(A)
#----------------------------------------------------------------------


TEST_TABLE(      TABLE=GLOC1,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=3.582E-5,
                 PRECISION=5.E-2      )

TEST_TABLE(      TABLE=GLOC2,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=3.582E-5,
                 PRECISION=5.E-2      )

TEST_TABLE(      TABLE=GLOC3,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=3.3598088955088E-05,
                 PRECISION=5.E-7      )

TEST_TABLE(      TABLE=GLOC4,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=3.582E-5,
                 PRECISION=7.E-2      )

TEST_TABLE(      TABLE=GLOC5,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=3.582E-5,
                 PRECISION=5.E-2      )

TEST_TABLE(      TABLE=GLOC6,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=3.582E-5,
                 PRECISION=5.E-2      )

#----------------------------------------------------------------------
#              TEST DE G(C)
#----------------------------------------------------------------------


TEST_TABLE(      TABLE=GLOC1,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1179',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=2.605E-5,
                 PRECISION=5.E-2      )

TEST_TABLE(      TABLE=GLOC2,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1179',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=2.605E-5,
                 PRECISION=5.E-2      )

TEST_TABLE(      TABLE=GLOC3,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1179',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=2.605E-5,
                 PRECISION=5.E-2      )

TEST_TABLE(      TABLE=GLOC4,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1179',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=2.605E-5,
                 PRECISION=5.E-2      )

TEST_TABLE(      TABLE=GLOC5,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1179',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=2.605E-5,
                 PRECISION=7.E-2      )

TEST_TABLE(      TABLE=GLOC6,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1179',      ),
                 NOM_PARA='G_LOCAL',
                      VALE=2.605E-5,
                 PRECISION=5.E-2      )

FIN()
