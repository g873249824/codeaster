MA=LIRE_MAILLAGE( )

LINST=DEFI_LIST_REEL(   DEBUT=0.,INTERVALLE=(
                             _F( JUSQU_A = 0.95,   NOMBRE = 1,   ),
                             _F( JUSQU_A = 1.00,   NOMBRE = 40,   ),
                             _F( JUSQU_A = 1.26,    NOMBRE = 10,   ),
                             _F( JUSQU_A = 1.98,    NOMBRE = 10,   ),
                             _F( JUSQU_A = 2.00,   NOMBRE = 10,    ))
                         )

COEF=DEFI_FONCTION(      NOM_PARA='INST',
                            PROL_DROITE='LINEAIRE',
                           PROL_GAUCHE='LINEAIRE',
                          VALE=( 0.0,     0.0,
                                 1.0,     1.0,
                                 2.0,     1.0,))

UN=DEFI_CONSTANTE( VALE=1.0)

ZERO=DEFI_CONSTANTE( VALE=0.0)

YOUNG=2.E5

S_F = FORMULE(NOM_PARA='TEMP',VALE='200.- 1.7*TEMP')

C_F = FORMULE(NOM_PARA='TEMP',VALE='(100.+1.7*TEMP)*(50.+2.*TEMP)')

D_F = FORMULE(NOM_PARA='TEMP',VALE='50.+2.*TEMP')

TEMPE1=DEFI_LIST_REEL(  DEBUT=-10.,
              INTERVALLE=_F(  JUSQU_A = 120.,    NOMBRE = 130,  )
                         )

C_T=CALC_FONC_INTERP(    FONCTION=C_F,
                             LIST_PARA=TEMPE1,
                            NOM_PARA = 'TEMP', )

D_T=CALC_FONC_INTERP(    FONCTION=D_F,
                             LIST_PARA=TEMPE1,
                          NOM_PARA = 'TEMP', )

R0_T=CALC_FONC_INTERP(FONCTION=S_F,LIST_PARA=TEMPE1,NOM_PARA ='TEMP')

ACIER=DEFI_MATERIAU(  THER=_F( LAMBDA = 1.,   ),
                            ELAS=_F( E = YOUNG,  NU = .3,
                                  ALPHA = 0.00001,   ),
                CIN1_CHAB_FO=_F(
                                 R_0 = R0_T,
                                 R_I = R0_T,
                                 B = ZERO,
                                 C_I = C_T,
                                 K = UN,
                                 W = ZERO,
                                 G_0 = D_T,
                                 A_I = UN,
                                    )
                          )

C2_F = FORMULE(NOM_PARA='TEMP',VALE='C_F(TEMP)*0.5')

C2_T=CALC_FONC_INTERP(    FONCTION=C2_F,
                             LIST_PARA=TEMPE1,
                          NOM_PARA = 'TEMP', )

ACIER2=DEFI_MATERIAU(  THER=_F( LAMBDA = 1.,   ),
