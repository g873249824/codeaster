              NOEUD = 'N521',     NOM_CMP = 'DZ',            VALE = -1.721655E-6),
     # C
            _F(  RESULTAT = RESU, NUME_ORDRE=1, NOM_CHAM='DEPL',
              CRITERE = 'ABSOLU',    PRECISION = 1.E-8,
              NOEUD = 'N989',     NOM_CMP = 'DX',            VALE = 0.),
            _F(  RESULTAT = RESU, NUME_ORDRE=1, NOM_CHAM='DEPL',
