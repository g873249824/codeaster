              NOEUD = 'N527',     NOM_CMP = 'DY',            VALE = 0.),
            _F(  RESULTAT = RESU, NUME_ORDRE=1, NOM_CHAM='DEPL',
              CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              NOEUD = 'N527',     NOM_CMP = 'DZ',            VALE = -1.291241E-6),
