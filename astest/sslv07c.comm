#CAS_TEST__: SSLV07C
# SSLV07/C                 COMMANDES                             
# ETIREMENT D'UN PARALLELIPIPEDE SOUS SON PROPRE POIDS
# FORCE_INTERNE + FORCE_FACE + CALCUL DES CONTRAINTES
# DECOUPAGE: Z=12  X=8     Y=8                      REF: SFM.VPCS SSLV07
# MODELISATION 3D: 1053 NOEUDS -- 768 MAILLES HEXA8
# A:N533  B:N521  E:N527  C:N989  D:N1001
#=======================================================================


DEBUT(CODE=_F(  NOM = 'SSLV07C',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE(  )

MO=AFFE_MODELE(  MAILLAGE=MA,
     AFFE=_F(  TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = '3D') )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.0E+11,  NU = 0.3,  RHO = 7800.,
                                 ALPHA = 0.)  )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

POIDS=DEFI_CONSTANTE(    VALE=-76518. )

FZ=DEFI_CONSTANTE(    VALE=229553. )

# ------------------------------------------------------------------
#    --- STRUCTURE SUSPENDUE //Z EN A + PAS DE ROTATION

CH1=AFFE_CHAR_MECA(    MODELE=MO,DDL_IMPO=(
       _F(  GROUP_NO = 'A',   DX = 0.,   DY = 0.,   DZ = 0.),
                _F(  GROUP_NO = 'D',             DY = 0.),
                _F(  GROUP_NO = 'ABSANSA',      DX = 0.,   DY = 0.)) )

CH2=AFFE_CHAR_MECA_F(  MODELE=MO,
       FORCE_INTERNE=_F( TOUT = 'OUI', FZ = POIDS),
       FORCE_FACE=_F(  GROUP_MA = 'FACESUP',       FZ = FZ) )

# ------------------------------------------------------------------

MELR=CALC_MATR_ELEM(  MODELE=MO,            CHARGE=(CH1, CH2,),
                        CHAM_MATER=CHMAT,    OPTION='RIGI_MECA' )

VEL=CALC_VECT_ELEM(  CHARGE=(CH1, CH2,),     CHAM_MATER=CHMAT,
                        OPTION='CHAR_MECA'                     )

NUM=NUME_DDL(  MATR_RIGI=MELR )

#
MATASSR=ASSE_MATRICE(  MATR_ELEM=MELR,    NUME_DDL=NUM  )

VECASS=ASSE_VECTEUR(  VECT_ELEM=VEL,     NUME_DDL=NUM  )

#
MATASSR=FACT_LDLT(reuse=MATASSR,  MATR_ASSE=MATASSR )

VECASS=RESO_LDLT(reuse=VECASS,  MATR_FACT=MATASSR,   CHAM_NO=VECASS )

SIG=CALC_CHAM_ELEM(  MODELE=MO,            DEPL=VECASS,
                       CHAM_MATER=CHMAT,
                       OPTION='SIGM_ELNO_DEPL'  )

#---------------------------------------------------------------------


TEST_RESU(CHAM_NO=( 
     # B
            _F(  CHAM_GD = VECASS,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N521',     NOM_CMP = 'DX',            VALE = 0.),
            _F(  CHAM_GD = VECASS,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N521',     NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = VECASS,   CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              NOEUD = 'N521',     NOM_CMP = 'DZ',            VALE = -1.721655E-6),
     # C
            _F(  CHAM_GD = VECASS,   CRITERE = 'ABSOLU',    PRECISION = 1.E-8,
              NOEUD = 'N989',     NOM_CMP = 'DX',            VALE = 0.),
            _F(  CHAM_GD = VECASS,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N989',     NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = VECASS,   CRITERE = 'RELATIF',   PRECISION = 2.E-3,
              NOEUD = 'N989',     NOM_CMP = 'DZ',            VALE = -1.707308E-6),
     # D
            _F(  CHAM_GD = VECASS,   CRITERE = 'RELATIF',   PRECISION = 3.E-2,
              NOEUD = 'N1001',    NOM_CMP = 'DX',            VALE = -1.721655E-7),
            _F(  CHAM_GD = VECASS,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N1001',    NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = VECASS,   CRITERE = 'RELATIF',   PRECISION = 2.E-1,
              NOEUD = 'N1001',    NOM_CMP = 'DZ',            VALE = 1.434713E-8),
     # E
            _F(  CHAM_GD = VECASS,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N527',     NOM_CMP = 'DX',            VALE = 0.),
            _F(  CHAM_GD = VECASS,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N527',     NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = VECASS,   CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              NOEUD = 'N527',     NOM_CMP = 'DZ',            VALE = -1.291241E-6)),
     # CONTRAINTES
            
            CHAM_ELEM=_F(  CHAM_GD = SIG,      CRITERE = 'RELATIF',   PRECISION = 1.1E-1,
              MAILLE = 'HEX336',
        NOEUD = 'N533',     NOM_CMP = 'SIZZ',          VALE = 2.29554E5) )

FIN()
