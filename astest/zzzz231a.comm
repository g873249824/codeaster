DEBUT(CODE=_F(NOM='ZZZZ231A',
              NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='OUI',),
IGNORE_ALARM=('CONTACT_93'),DEBUG=_F(SDVERI='OUI'))

# Test de la methode RESOLUTION='NON'. Interpenetration donc alarme CONTACT_93 ignoree

MA=LIRE_MAILLAGE(FORMAT='MED',);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=_F(GROUP_MA=('ContVis','ContTrou',),),);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.0E+11,
                            NU=0.3,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

ENCAST=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=_F(GROUP_MA='Base',
                                  LIAISON='ENCASTRE',),);

ENCAST2=AFFE_CHAR_MECA(MODELE=MO,
                       DDL_IMPO=_F(GROUP_MA='Dimpo',
                                   LIAISON='ENCASTRE',),);

Dimpo=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=_F(GROUP_MA='ContVis',
                                 DZ=-1,),);


CONT = DEFI_CONTACT(MODELE      = MO,
                   FORMULATION  = 'DISCRETE',
                   REAC_GEOM    = 'AUTOMATIQUE',
                   ZONE=(
                        _F(
                            GROUP_MA_MAIT='ContVis',
                            GROUP_MA_ESCL='ContTrou',
                         ),

                       ),
                  )


CONT2 = DEFI_CONTACT(MODELE      = MO,
                   FORMULATION  = 'DISCRETE',
                   ZONE=(
                        _F(
                            RESOLUTION   = 'NON',
                            GROUP_MA_MAIT='ContVis',
                            GROUP_MA_ESCL='ContTrou',
                         ),

                       ),
                  )


CONT3 = DEFI_CONTACT(MODELE      = MO,
                   FORMULATION  = 'CONTINUE',
                   ALGO_RESO_CONT = 'NEWTON',
                   ALGO_RESO_GEOM = 'NEWTON',
                   ZONE=(
                        _F(
                            RESOLUTION   = 'NON',
                            GROUP_MA_MAIT='ContVis',
                            GROUP_MA_ESCL='ContTrou',
                         ),

                       ),
                  )

RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0,0,
                          1,1,
                          2,2,
                          ),PROL_DROITE='LINEAIRE',);

TPS=DEFI_LIST_REEL(DEBUT=0,
                   INTERVALLE=(_F(JUSQU_A=1,
                                  NOMBRE=1,),
                               _F(JUSQU_A=3,
                                  NOMBRE=10,),),);

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=ENCAST,),
                          _F(CHARGE=ENCAST2,),
                          _F(CHARGE=Dimpo,
                             FONC_MULT=RAMPE,),
                          ),
                   CONTACT  = CONT,
                   COMP_INCR=_F(RELATION='ELAS',
                                DEFORMATION='PETIT',),
                   INCREMENT=_F(LIST_INST=TPS,),
                   NEWTON=_F(REAC_INCR=1,
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   SOLVEUR=_F(SYME='OUI',
                              METHODE='MUMPS',),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1.0E-6,
                                  ITER_GLOB_MAXI=10,),);

TAB_RESU=POST_RELEVE_T(ACTION=_F(OPERATION='MOYENNE',
                                 INTITULE='DEPLACEMENT CYLINDRE',
                                 RESULTAT=RESU,
                                 NOM_CHAM='DEPL',
                                 GROUP_NO='ContTrou',
                                 NOM_CMP='DZ',),);

IMPR_TABLE(TABLE=TAB_RESU,);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=-2.000000000,
           VALE_REFE=-2.0,
           NOM_PARA='DZ',
           TABLE=TAB_RESU,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                      VALE_K='MINIMUM',),
                   _F(NOM_PARA='INST',
                      VALE=3.0,),
                   ),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=-2.000000000,
           VALE_REFE=-2.0,
           NOM_PARA='DZ',
           TABLE=TAB_RESU,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                      VALE_K='MAXIMUM',),
                   _F(NOM_PARA='INST',
                      VALE=3.0,),
                   ),
           )

IMPR_RESU(FORMAT='MED',
          RESU=_F(MAILLAGE=MA,
                  RESULTAT=RESU,
                  TOUT_CHAM='OUI',),);

RESU2=STAT_NON_LINE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    EXCIT=(_F(CHARGE=ENCAST,),
                           _F(CHARGE=ENCAST2,),
                           _F(CHARGE=Dimpo,
                              FONC_MULT=RAMPE,),
                           ),
                    CONTACT  = CONT2,
                    COMP_INCR=_F(RELATION='ELAS',
                                 DEFORMATION='PETIT',),
                    INCREMENT=_F(LIST_INST=TPS,),
                    NEWTON=_F(REAC_INCR=1,
                              MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    SOLVEUR=_F(SYME='OUI',
                               METHODE='MUMPS',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.0E-6,
                                   ITER_GLOB_MAXI=10,),);


TAB_VC=POST_RELEVE_T(ACTION=_F(OPERATION='MOYENNE',
                               INTITULE='JEU METHODE VERIF DISCRET',
                               RESULTAT=RESU2,
                               NOM_CHAM='VALE_CONT',
                               GROUP_NO='ContTrou',
                               NOM_CMP=('CONT','JEU',),),);

IMPR_TABLE(TABLE=TAB_VC);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=-2.000000000,
           VALE_REFE=-2.0,
           NOM_PARA='JEU',
           TABLE=TAB_VC,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                      VALE_K='MINIMUM',),
                   _F(NOM_PARA='INST',
                      VALE=3.0,),
                   ),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=-2.000000000,
           VALE_REFE=-2.0,
           NOM_PARA='JEU',
           TABLE=TAB_VC,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                      VALE_K='MAXIMUM',),
                   _F(NOM_PARA='INST',
                      VALE=3.0,),
                   ),
           )

RESU3=STAT_NON_LINE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    EXCIT=(_F(CHARGE=ENCAST,),
                           _F(CHARGE=ENCAST2,),
                           _F(CHARGE=Dimpo,
                              FONC_MULT=RAMPE,),
                           ),
                    CONTACT  = CONT3,
                    COMP_INCR=_F(RELATION='ELAS',
                                 DEFORMATION='PETIT',),
                    INCREMENT=_F(LIST_INST=TPS,),
                    NEWTON=_F(REAC_INCR=1,
                              MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    SOLVEUR=_F(SYME='OUI',
                               METHODE='MUMPS',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.0E-6,
                                   ITER_GLOB_MAXI=10,),);


TAB_VC3=POST_RELEVE_T(ACTION=_F(OPERATION='MOYENNE',
                               INTITULE='JEU METHODE VERIF CONTINU',
                               RESULTAT=RESU3,
                               NOM_CHAM='VALE_CONT',
                               GROUP_NO='ContTrou',
                               NOM_CMP=('CONT','JEU',),),);

IMPR_TABLE(TABLE=TAB_VC3);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=-2.000000000,
           VALE_REFE=-2.0,
           NOM_PARA='JEU',
           TABLE=TAB_VC3,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                      VALE_K='MINIMUM',),
                   _F(NOM_PARA='INST',
                      VALE=3.0,),
                   ),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=-2.000000000,
           VALE_REFE=-2.0,
           NOM_PARA='JEU',
           TABLE=TAB_VC3,
           FILTRE=(_F(NOM_PARA='QUANTITE',
                      VALE_K='MAXIMUM',),
                   _F(NOM_PARA='INST',
                      VALE=3.0,),
                   ),
           )

FIN();
