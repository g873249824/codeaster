DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'),)

def zzzz216_ops(self,**kwargs):
    from Utilitai import sup_gmsh
    self.set_icmd(1)
    P0 = sup_gmsh.Point(0., 0., 0.)
    P1 = sup_gmsh.Point(1., 0., 0.)
    L01 = sup_gmsh.Line(P0, P1)
    L01.Transfinite(1)
    mesh = sup_gmsh.Mesh()
    mesh.Physical('MON_CHEMIN', L01)

    MA1 = mesh.LIRE_GMSH(UNITE_GMSH=19, UNITE_MAILLAGE=20, MODI_QUAD='NON', CREA_GROUP_NO='OUI')

    MA2 = mesh.LIRE_GMSH(UNITE_GMSH=19, UNITE_MAILLAGE=20, MODI_QUAD='OUI', CREA_GROUP_NO='NON')

    MA3 = mesh.LIRE_GMSH(UNITE_GMSH=19, UNITE_MAILLAGE=20, MODI_QUAD='OUI', CREA_GROUP_NO='OUI')

    MA4 = mesh.LIRE_GMSH(UNITE_GMSH=19, UNITE_MAILLAGE=20, MODI_QUAD='NON', CREA_GROUP_NO='NON')

    TEST_RESU(MAILLAGE=(
        _F(MAILLAGE=MA1,
           CARA='NB_MAILLE',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA1,
           CARA='NB_NOEUD',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA1,
           CARA='NB_GROUP_MA',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA1,
           CARA='NB_GROUP_NO',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA1,
           CARA='NB_MA_GROUP_MA',
           NOM_GROUP_MA=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA1,
           CARA='NB_NO_GROUP_NO',
           NOM_GROUP_NO=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
))
    
    TEST_RESU(MAILLAGE=(
        _F(MAILLAGE=MA2,
           CARA='NB_MAILLE',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA2,
           CARA='NB_NOEUD',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=3,
           VALE_CALC_I=3,
          ),
        _F(MAILLAGE=MA2,
           CARA='NB_GROUP_MA',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA2,
           CARA='NB_GROUP_NO',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=0,
           VALE_CALC_I=0,
          ),
        _F(MAILLAGE=MA2,
           CARA='NB_MA_GROUP_MA',
           NOM_GROUP_MA=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA2,
           CARA='NB_NO_GROUP_NO',
           NOM_GROUP_NO=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=0,
           VALE_CALC_I=0,
          ),
    ))
    
    TEST_RESU(MAILLAGE=(
        _F(MAILLAGE=MA3,
           CARA='NB_MAILLE',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA3,
           CARA='NB_NOEUD',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=3,
           VALE_CALC_I=3,
          ),
        _F(MAILLAGE=MA3,
           CARA='NB_GROUP_MA',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA3,
           CARA='NB_GROUP_NO',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA3,
           CARA='NB_MA_GROUP_MA',
           NOM_GROUP_MA=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA3,
           CARA='NB_NO_GROUP_NO',
           NOM_GROUP_NO=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=3,
           VALE_CALC_I=3,
          ),
    ))
    
    TEST_RESU(MAILLAGE=(
        _F(MAILLAGE=MA4,
           CARA='NB_MAILLE',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA4,
           CARA='NB_NOEUD',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA4,
           CARA='NB_GROUP_MA',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=2,
           VALE_CALC_I=2,
          ),
        _F(MAILLAGE=MA4,
           CARA='NB_GROUP_NO',
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=0,
           VALE_CALC_I=0,
          ),
        _F(MAILLAGE=MA4,
           CARA='NB_MA_GROUP_MA',
           NOM_GROUP_MA=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=1,
           VALE_CALC_I=1,
          ),
        _F(MAILLAGE=MA4,
           CARA='NB_NO_GROUP_NO',
           NOM_GROUP_NO=('MON_CHEMIN'),
           REFERENCE='ANALYTIQUE',
           CRITERE='ABSOLU',
           VALE_REFE_I=0,
           VALE_CALC_I=0,
          ),
    ))

ZZZZ216 = MACRO (nom='ZZZZ216', op=zzzz216_ops,
                 ERREUR = SIMP(statut='f',typ='TXM', max='**',),
                 INFO = SIMP(statut='f',typ='I', defaut=1, into=(1, 2),),)

ZZZZ216()

FIN();
