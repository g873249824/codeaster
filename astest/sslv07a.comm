CHMAT2=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(TOUT='OUI',
                             MATER=MAT2,),);

MASTOT=POST_ELEM(MASS_INER=_F(TOUT='OUI',),
                 MODELE=MO,
                 CHAM_MATER=CHMAT,);

MASVOL=POST_ELEM(MASS_INER=_F(GROUP_MA='VOLUM',),
                 MODELE=MO,
                 CHAM_MATER=CHMAT,);
