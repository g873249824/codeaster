#CAS_TEST__: SSLV07A
# SSLV07/A                 COMMANDES                             7/03/91
# ETIREMENT D'UN PARALLELEPIPEDE SOUS SON PROPRE POIDS
# PRESSION REPARTIE  + CALCUL DES CONTRAINTES
# DECOUPAGE: Z=3   X=2    Y=2                      REF: SFM.VPCS SSLV07
# MODELISATION 3D:  111 NOEUDS -- 12 MAILLES HEXA20
# POINTS:   A:N59  B:N53  E:N56  C:N12  D:N18
#======================================================================


DEBUT(CODE=_F(  NOM = 'SSLV07A',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE(  )

DEFI_GROUP(reuse=MA,       MAILLAGE=MA,
            CREA_GROUP_MA=_F( NOM = 'VOLUM',OPTION='APPUI_LACHE',
                           GROUP_NO='A'))

MO=AFFE_MODELE(  MAILLAGE=MA,
       AFFE=_F(  TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = '3D') )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.0E+11,  NU = 0.3,  RHO = 7800.,
                            ALPHA = 0.)  )

MAT2=DEFI_MATERIAU(      ELAS=_F(  E = 2.0E+11,  NU = 0.3,  RHO = 7800.,
                            ALPHA = 0.),
           ECRO_LINE=_F( D_SIGM_EPSI = 200., SY = 1000000.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

CHMAT2=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT2) )

MASTOT = POST_ELEM(MODELE = MO, CHAM_MATER = CHMAT,
                   MASS_INER =_F(TOUT='OUI'))

MASVOL = POST_ELEM(MODELE = MO, CHAM_MATER = CHMAT,
                   MASS_INER =_F(GROUP_MA ='VOLUM'))

# LA VALEUR DE LA MASSE EN VOLUM EST UN TIERS DE LA MASSE TOTALE
TEST_TABLE(TABLE=MASVOL, NOM_PARA='MASSE', VALE =7.80000E+03, REFERENCE= 'AUTRE_ASTER')


# ------------------------------------------------------------------
#    --- STRUCTURE SUSPENDUE //Z EN A + PAS DE ROTATION

BLOQU=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
             _F(  GROUP_NO = 'A',   DX = 0.,   DY = 0.,   DZ = 0.),
                      _F(  GROUP_NO = 'D',             DY = 0.),
                      _F(  GROUP_NO = 'ABSANSA',      DX = 0.,   DY = 0.))
                      )

CH=AFFE_CHAR_MECA(  MODELE=MO,
                     PESANTEUR=(  9.81,  0.,  0.,   -1., ),
               FORCE_FACE=_F(  GROUP_MA = 'FACESUP',   FZ = 229553.)
                   )

# ------------------------------------------------------------------

MELR=CALC_MATR_ELEM(  MODELE=MO,            CHARGE=( BLOQU, CH, ),
                        CHAM_MATER=CHMAT,    OPTION='RIGI_MECA' )

VEL=CALC_VECT_ELEM(  CHARGE=( BLOQU, CH, ),  CHAM_MATER=CHMAT,
                        OPTION='CHAR_MECA'                     )

NUM=NUME_DDL(  MATR_RIGI=MELR )

#
MATASSR=ASSE_MATRICE(  MATR_ELEM=MELR,    NUME_DDL=NUM  )

VECASS=ASSE_VECTEUR(  VECT_ELEM=VEL,     NUME_DDL=NUM  )

MATFACT=FACT_LDLT(  MATR_ASSE=MATASSR  )

DEPLA=RESO_LDLT(  MATR_FACT=MATFACT,   CHAM_NO=VECASS )

SIG=CALC_CHAM_ELEM(  MODELE=MO,            DEPL=DEPLA,
                        CHAM_MATER=CHMAT,
                        OPTION='SIGM_ELNO_DEPL'  )

EPO=CALC_CHAM_ELEM(  MODELE=MO,            DEPL=DEPLA,
                        CHAM_MATER=CHMAT,
                        OPTION='EPOT_ELEM_DEPL'  )

TEP=POST_ELEM(  MODELE=MO,    CHAM_MATER=CHMAT,
                   CHAM_GD=DEPLA,
                   ENER_POT=_F(  TOUT = 'OUI')  )

TE1=POST_ELEM(  MODELE=MO,    CHAM_MATER=CHMAT,
                   CHAM_GD=EPO,
                   ENER_POT=_F(  TOUT = 'OUI')  )

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
              INTERVALLE=_F( JUSQU_A = 1.0,      NOMBRE = 10)
           )

RAMPE=DEFI_FONCTION(   NOM_PARA='INST',
           PROL_GAUCHE='LINEAIRE',  PROL_DROITE='LINEAIRE',
           VALE=(0.0,0.0,1.0,1.0,))

# STAT_NON_LINE 'VMIS'

#---------------------


DEPLD2=STAT_NON_LINE( 
                            MODELE=MO, 
                        CHAM_MATER=CHMAT2,EXCIT=(
                             _F( 
                CHARGE = BLOQU), 
                             _F( 
                CHARGE = CH,
                FONC_MULT = RAMPE)), 
                         COMP_INCR=_F( 
                RELATION = 'VMIS_ISOT_LINE'),
                         INCREMENT=_F( 
                LIST_INST = L_INST),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 2),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-6,
                ITER_INTE_MAXI = 30,
                RESI_INTE_RELA = 1.E-8)
              )

ENER_ELA=POST_ELEM(     MODELE=MO,
                          RESULTAT=DEPLD2,
                          CHAM_MATER=CHMAT2,
                          NUME_ORDRE=10,
                           ENER_ELAS=_F( TOUT = 'OUI')
                       )

ENER_TOT=POST_ELEM(     MODELE=MO,
                          RESULTAT=DEPLD2,
                          CHAM_MATER=CHMAT2,
                          NUME_ORDRE=10,
                           ENER_TOTALE=_F( TOUT = 'OUI')
                       )

IMPR_TABLE(   TABLE=TEP)

IMPR_TABLE(   TABLE=ENER_ELA)

IMPR_TABLE(   TABLE=ENER_TOT )

TEST_TABLE(      TABLE=TEP,       NOM_PARA='TOTALE',
                TYPE_TEST='SOMM_ABS',
                REFERENCE='NON_REGRESSION',
                VALE=1.31738E-01,
                 PRECISION=1.E-5 )

TEST_TABLE(      TABLE=ENER_ELA,       NOM_PARA='TOTALE',
                TYPE_TEST='SOMM_ABS',
                REFERENCE='AUTRE_ASTER',
                VALE=1.31738E-01,
                 PRECISION=1.E-5 )

TEST_TABLE(      TABLE=ENER_TOT,       NOM_PARA='TOTALE',
                TYPE_TEST='SOMM_ABS',
                REFERENCE='AUTRE_ASTER',
                VALE=1.31738E-01,
                 PRECISION=1.E-5 )

#---------------------------------------------------------------------


FORCE=PROD_MATR_CHAM(    MATR_ASSE=MATASSR,
                                CHAM_NO=DEPLA,
                                  TITRE=' PROD_MATR_CHAM '  )

STATIQUE=MACRO_ELAS_MULT(            MODELE=MO,
                                     CHAM_MATER=CHMAT,
                               CHAR_MECA_GLOBAL=BLOQU,
                                       NUME_DDL=NUM,
                   CAS_CHARGE=_F(  NOM_CAS = 'VERIF VECT_ASSE',
                                VECT_ASSE = FORCE)
                             )

TEST_RESU(RESU=( 
     # B
           _F(  RESULTAT = STATIQUE,           NOM_CHAM = 'DEPL',
             NOM_CAS = 'VERIF VECT_ASSE',
             CRITERE = 'ABSOLU', 
             NOEUD = 'N53',      NOM_CMP = 'DX',            VALE = 0.),
           _F(  RESULTAT = STATIQUE,           NOM_CHAM = 'DEPL',
             NOM_CAS = 'VERIF VECT_ASSE',
             NOEUD = 'N53',      NOM_CMP = 'DZ',            VALE = -1.721655E-6),
     # C
           _F(  RESULTAT = STATIQUE,           NOM_CHAM = 'DEPL',
             NOM_CAS = 'VERIF VECT_ASSE',
             NOEUD = 'N12',      NOM_CMP = 'DZ',            VALE = -1.707308E-6),
     # D
           _F(  RESULTAT = STATIQUE,           NOM_CHAM = 'DEPL',
             NOM_CAS = 'VERIF VECT_ASSE',
             NOEUD = 'N18',      NOM_CMP = 'DX',            VALE = -1.721655E-7),
           _F(  RESULTAT = STATIQUE,           NOM_CHAM = 'DEPL',
             NOM_CAS = 'VERIF VECT_ASSE',   PRECISION = 0.05,
             NOEUD = 'N18',      NOM_CMP = 'DZ',            VALE = 1.434713E-8),
     # E
           _F(  RESULTAT = STATIQUE,           NOM_CHAM = 'DEPL',
             NOM_CAS = 'VERIF VECT_ASSE',   
             NOEUD = 'N56',      NOM_CMP = 'DZ',            VALE = -1.291241E-6))
            )

#---------------------------------------------------------------------


TEST_RESU(CHAM_NO=( 
     # B
            _F(  CHAM_GD = DEPLA,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N53',      NOM_CMP = 'DX',            VALE = 0.),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N53',      NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              NOEUD = 'N53',      NOM_CMP = 'DZ',            VALE = -1.721655E-6),
     # C
            _F(  CHAM_GD = DEPLA,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N12',      NOM_CMP = 'DX',            VALE = 0.),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N12',      NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              NOEUD = 'N12',      NOM_CMP = 'DZ',            VALE = -1.707308E-6),
     # D
            _F(  CHAM_GD = DEPLA,   CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              NOEUD = 'N18',      NOM_CMP = 'DX',            VALE = -1.721655E-7),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N18',      NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'RELATIF',   PRECISION = 3.E-3,
              NOEUD = 'N18',      NOM_CMP = 'DZ',            VALE = 1.434713E-8),
     # E
            _F(  CHAM_GD = DEPLA,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N56',      NOM_CMP = 'DX',            VALE = 0.),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'ABSOLU',    PRECISION = 1.E-10,
              NOEUD = 'N56',      NOM_CMP = 'DY',            VALE = 0.),
            _F(  CHAM_GD = DEPLA,   CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              NOEUD = 'N56',      NOM_CMP = 'DZ',            VALE = -1.291241E-6)),CHAM_ELEM=(
     # CONTRAINTES EN A ET E
            
            _F(  CHAM_GD = SIG,      CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              MAILLE = 'HEX3',
              NOEUD = 'N59',      NOM_CMP = 'SIZZ',          VALE = 2.29554E5),
            
            _F(  CHAM_GD = SIG,      CRITERE = 'RELATIF',   PRECISION = 1.E-3,
              MAILLE = 'HEX2',
        NOEUD = 'N56',      NOM_CMP = 'SIZZ',          VALE = 1.14777E5)) )

FIN()
