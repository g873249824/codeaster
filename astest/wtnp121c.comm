#    LINEAIRE SOUMIS A UN CHOC DE PRESSION EN 3D
#    MODELISATION VF DECENTRE ARETE (VFDA)


DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#=============================================================
#=============================================================
METHODE='VFDA'
#=============================================================
#=============================================================

MODELISA = 'D_PLAN_HH2SUDA'

#=============================================================
#                     donnees
#=============================================================

PARM_ALPHA=1.
MOLVP = 1.E-10
UN_SUR_K=0.
EMMAG=1.E-10


#*********************************************************
# CI/CL
#*********************************************************
#
PGREF = 10.

PVP0 = 0.0001
P0=10000.
PG0 = 0.
PCINI = PG0+PGREF - P0
PRE1GAUCHE=0

#*********************************************************
# PAS DE TEMPS
#*********************************************************
#
TFINAL=100.
NBDECOUP=100



#*********************************************************
# CRITERES DE CONVERGENCE
#*********************************************************
#
RESI_GLOB_MAXI = 1.E-17
ITER_GLOB_MAXI = 0

#
#
#*************************************************************
# Liste des instants de calculs
#*************************************************************
#

INST1=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=TFINAL,
                                    NOMBRE=NBDECOUP)),
                     );
LI =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =INST1 ),
                                ECHEC=_F(EVENEMENT = 'ERREUR',
                                         ACTION    = 'ARRET'))

#*********************************************************
# Definition des caracteristiques fluides
#**********************************************************

UN=DEFI_CONSTANTE(VALE=1.0);
ZERO=DEFI_CONSTANTE(VALE=0.0);
PERMINT=DEFI_CONSTANTE(VALE=1.E-13);
HENRY=DEFI_CONSTANTE(VALE=1.0);

#*********************
# Lecture du maillage
#*********************


MAILINI=LIRE_MAILLAGE(FORMAT='MED');


MAIL=CREA_MAILLAGE( MAILLAGE=MAILINI,
          MODI_MAILLE=(
           _F( TOUT = 'OUI',OPTION = 'TRIA6_7',PREF_NOEUD='NT'),
           ))
MAIL=DEFI_GROUP(reuse =MAIL,
         MAILLAGE=MAIL,
         CREA_GROUP_NO=(
         _F(GROUP_MA='MHAUT',NOM='NSHAUT',CRIT_NOEUD='MILIEU'),
         _F(GROUP_MA='MBAS',NOM='NSBAS',CRIT_NOEUD='MILIEU'),
         _F(GROUP_MA='MDROIT'),
         _F(GROUP_MA='MGAUCHE'),
         _F(GROUP_MA='TOUT',NOM='NS',CRIT_NOEUD='MILIEU'),
         _F(GROUP_MA='TOUT',NOM='NK',CRIT_NOEUD='CENTRE')
                   ));


#*********************************************************
# Modele de calcul
#*********************************************************

MODELT=AFFE_MODELE(MAILLAGE=MAIL,
                   VERI_JACOBIEN='NON',
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION=MODELISA),
                  );


#*********************************************************
# DEFI_MATERIAU
#*********************************************************
#

MATERIAU=DEFI_MATERIAU(

   THM_LIQU=_F(
         RHO = 1.,
         UN_SUR_K = UN_SUR_K,
         ALPHA = 1.E-4,
         VISC = UN,
         D_VISC_TEMP = ZERO),

   COMP_THM = 'LIQU_AD_GAZ_VAPE',

   THM_AIR_DISS=_F(
                   CP=0.,
                   COEF_HENRY=HENRY,
                   ),

   THM_VAPE_GAZ    =_F(
                   MASS_MOL   = MOLVP,
                   CP         = 1.,
                   VISC       = UN,
                   D_VISC_TEMP= ZERO,
                       ),

   THM_GAZ=_F(
          MASS_MOL    = 1.,
          VISC        = UN,
          D_VISC_TEMP = ZERO,
             ),

   THM_DIFFU=_F(
            R_GAZ      = 8.315,
            EMMAG      = EMMAG,
            RHO        = 1.,
            BIOT_L     = 1.,
            BIOT_T     = 1.,
            BIOT_N     = 1.,
            SATU_PRES  = UN,
            D_SATU_PRES= ZERO,
            PESA_X     = 0.0,
            PESA_Y     = 0.,
            PESA_Z     = 0.,
            PERMIN_L   = PERMINT,
            PERMIN_T   = ZERO,
            PERM_LIQU  = UN,
            D_PERM_LIQU_SATU=ZERO,
            PERM_GAZ   = UN,
            D_PERM_SATU_GAZ=ZERO,
            D_PERM_PRES_GAZ=ZERO,
            FICKV_T    = ZERO,
            FICKA_T    = ZERO,
            LAMB_TL    = ZERO,
            LAMB_TT    = ZERO,
              ),

   THM_INIT=_F(
         TEMP = 293.,
         PRE1 = 0.,
         PRE2 = PGREF,
         PORO = 0.5,
         PRES_VAPE=PVP0,
              ),
   )
#****************************************************************
# Affectation des materiaux
#****************************************************************
#

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='TOUT',
                             MATER=MATERIAU)));

#*************************************************************
# Affectation de l etat initial
#*************************************************************
#
PINIT=CREA_CHAMP(MAILLAGE=MAIL,
                    OPERATION='AFFE',
                    TYPE_CHAM='NOEU_DEPL_R',
                    AFFE=(
                    _F( GROUP_MA = 'TOUT'  ,
                    NOM_CMP  = ('PRE1','PRE2') ,
                    VALE = (PCINI,0.), ),
                ))
#*************************************************************
# Affectation des CL
#*************************************************************


CHIMP=AFFE_CHAR_CINE(MODELE=MODELT,
       MECA_IMPO=(_F(TOUT='OUI',PRE2=0.),
                 _F(GROUP_MA = 'MGAUCHE',PRE1=PRE1GAUCHE),
                 ));


#
#
#*************************************************************
#     STAT NON LINE
#*************************************************************
#

cal1=STAT_NON_LINE(MODELE=MODELT,
                 CHAM_MATER=CHMAT,

                 EXCIT=( _F(CHARGE=CHIMP)),

                 COMPORTEMENT=_F(RELATION='KIT_HH',
                              RELATION_KIT=('LIQU_AD_GAZ_VAPE','HYDR_UTIL'),
                 ),

                INCREMENT=_F(
                   LIST_INST = LI,
                     INST_FIN=TFINAL,
                ),

                 ETAT_INIT  = _F ( DEPL = PINIT  ,
                                  ),

                 SOLVEUR=_F(METHODE='MUMPS'),

                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1),

                 CONVERGENCE=_F( RESI_GLOB_MAXI = RESI_GLOB_MAXI,
                                 ITER_GLOB_MAXI = ITER_GLOB_MAXI),

                 ARCHIVAGE=_F(LIST_INST     = INST1 ),
                 );


#
#*************************************************************
# Impressions
#*************************************************************
#
L_INST=DEFI_LIST_REEL(VALE=(0.,1.,10.,100.));


#============== t=100======================
# NOEUD DU BAS
TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='ANALYTIQUE',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='N360',
                NOM_CMP='PRE1',
                VALE_CALC=-1.36143539E+03,
                VALE_REFE=-1330.51641,
                CRITERE='RELATIF',
                PRECISION=0.029999999999999999,),
        )

# NOEUD MILIEU
TEST_RESU(RESU=_F(NUME_ORDRE=100,
                REFERENCE='ANALYTIQUE',
                RESULTAT=cal1,
                NOM_CHAM='DEPL',
                NOEUD='N505',
                NOM_CMP='PRE1',
                VALE_CALC=-1.36023000E+03,
                VALE_REFE=-1330.51641,
                CRITERE='RELATIF',
                PRECISION=0.029999999999999999,),
        )


FIN();
