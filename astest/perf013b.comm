# ======================================================================
#       INTERET : CALCULER 10 MODES EN POSITION 11, 12, ..., 20
#---------------------------------------------------------------------------
# VARIANTE DU CAS TEST PERF003C MAIS AVEC LE SOLVEUR MUMPS.
# VARIANTE PARALLELE: 4 PROCESS MPI.
# ICI ON PRIVILEGIE LE TEMPS SUR LA MEMOIRE: D'OU LE CHOIX RENUM=QAMD +
# OUT_OF_CORE='NON (DEFAUT) + LIBERE_MEMOIRE='NON' (DEFAUT).
# ICI L'OPTION MATR_DISTRIBUEE N'A PAS LIEU D'ETRE. 
#---------------------------------------------------------------------------

DEBUT(CODE=_F(NOM='PERF013B',NIV_PUB_WEB='INTERNET'),
                MESURE_TEMPS=_F(NIVE_DETAIL=2))

MA=LIRE_MAILLAGE(FORMAT='MED')
MODEL=AFFE_MODELE(MAILLAGE=MA,PARTITION=_F(PARALLELISME='CENTRALISE'),
         AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='DKT') )
CAREL=AFFE_CARA_ELEM( MODELE=MODEL, COQUE=_F(GROUP_MA='ABCD', EPAIS=0.02) )
MAT1=DEFI_MATERIAU(ELAS=_F(E= 5.E11, NU=0.3, RHO=9800. ))
CHMAT=AFFE_MATERIAU(MAILLAGE=MA, AFFE=_F(TOUT='OUI',MATER=MAT1))
BLOQ=AFFE_CHAR_MECA(MODELE=MODEL,
        DDL_IMPO=(
          _F(GROUP_MA=('AB'), DX=0., DY=0., DZ=0., DRX=0., DRY=0., DRZ=0.),
          _F(GROUP_MA=('DA'), DX=0., DY=0., DZ=0., DRX=0., DRY=0., DRZ=0.),
        ))


RIGIELEM=CALC_MATR_ELEM(CHARGE=BLOQ,
                       OPTION='RIGI_MECA',
                       CARA_ELEM=CAREL,
                       MODELE=MODEL,
                       CHAM_MATER=CHMAT,
                       );
MASSELEM=CALC_MATR_ELEM(CHARGE=BLOQ,
                       OPTION='MASS_MECA',
                       CARA_ELEM=CAREL,
                       MODELE=MODEL,
                       CHAM_MATER=CHMAT,
                       );
NUME=NUME_DDL(MATR_RIGI=RIGIELEM,);
RIGI=ASSE_MATRICE(NUME_DDL=NUME,MATR_ELEM=RIGIELEM,);
MASS=ASSE_MATRICE(NUME_DDL=NUME,MATR_ELEM=MASSELEM,);
MODES=MODE_ITER_SIMULT(INFO=1,
                      CALC_FREQ=_F(
                                   OPTION='BANDE',
                                   FREQ=(840.0,1600.0,),
                                   ),
                      VERI_MODE=_F(),
                      TYPE_RESU='DYNAMIQUE',
                      SOLVEUR=_F(METHODE='MUMPS',RENUM='QAMD',),
                      METHODE='SORENSEN',
                      MATR_B=MASS,
                      MATR_A=RIGI,
                      );
TEST_RESU(RESU=
       _F( RESULTAT = MODES, PARA = 'FREQ', CRITERE = 'RELATIF',
           PRECISION = 3.E-3, NUME_ORDRE = 3, VALE = 993.5, REFERENCE = 'NON_REGRESSION'),)

FIN()
