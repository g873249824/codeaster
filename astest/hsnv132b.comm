DEBUT(CODE=_F(NOM='HSNV132B', NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'));

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MACR_ADAP_MAIL(ADAPTATION='RAFF_DERA_ZONE',
                  ZONE=_F(TYPE='DISQUE',
                        X_CENTRE = 0.5,
                        Y_CENTRE = 0.5,
                        RAYON = 0.1, ),
                  MAILLAGE_N = MAILLAGE,
                  MAILLAGE_NP1 = CO('MAILLAG1'),
                  QUALITE='OUI',
                  TAILLE='OUI',
                  )
#
###IMPR_RESU(FORMAT='MED',RESU=_F(MAILLAGE=MAILLAG1,),);

#--------------------------------------------------------------------
#               DEBUT DU THERMIQUE                                  #
#--------------------------------------------------------------------

MOTH=AFFE_MODELE(MAILLAGE=MAILLAG1,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='THERMIQUE',
                         MODELISATION='PLAN',),);

MATH=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,
                           RHO_CP=0.0,),);

CMTH=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                   AFFE=_F(TOUT='OUI',
                           MATER=MATH,),);

CHTH=AFFE_CHAR_THER(MODELE=MOTH,
                    TEMP_IMPO=(_F(GROUP_MA='LIG4',TEMP=20.0,),
                               _F(GROUP_MA='LIG23',TEMP=220.0,),),);


THLI=THER_LINEAIRE(MODELE=MOTH,
                   CHAM_MATER=CMTH,
                   EXCIT=_F(CHARGE=CHTH,),);

TEMP=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='EXTR',
                RESULTAT=THLI,
                NOM_CHAM='TEMP',
                INST=0.0,);

#IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(CHAM_GD = TEMP,))   

#--------------------------------------------------------------------
#               FIN DU THERMIQUE                                    #
#--------------------------------------------------------------------



MODELE=AFFE_MODELE(MAILLAGE=MAILLAG1,
                   AFFE=(_F(GROUP_MA='SURF',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='D_PLAN',),),);

LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y-0.5');
LT=FORMULE(NOM_PARA=('X','Y'),VALE='-X+0.5');


FISS=DEFI_FISS_XFEM(MODELE=MODELE,  
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    INFO=1,
                    );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELE,FISSURE=FISS,INFO=2,);


ACIER=DEFI_MATERIAU(ELAS=_F(E=205000000000.0,
                            NU=0.3,
                            RHO=7800.0,
                            ALPHA=1.282E-05,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA='SURF',MATER=ACIER,),
                       AFFE_VARC=_F( GROUP_MA='SURF',
                                     NOM_VARC='TEMP',
                                     CHAM_GD=TEMP,
                                     VALE_REF=20.0),
                       );


#*********************************************************************
#                          CONDITIONS AUX LIMITES                    *
#*********************************************************************

CHARBLO=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=_F(GROUP_MA='LIGINF',
                                   DX=0.0,
                                   DY=0.0,),
                       INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.0,
                                    NOMBRE=3,),);

UTOT=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=_F(CHARGE=CHARBLO,),
                   INST=1,)

#---------------POST--------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT,
                        INFO          = 2)



#IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(RESULTAT=RES_XFEM,NOM_CHAM='DEPL'))   

TABDEP= POST_RELEVE_T ( ACTION =_F(INTITULE  = 'DEPLACEMENT',
                        GROUP_NO  = 'P5',
                        RESULTAT   = UTOT,
                        NOM_CHAM   = 'DEPL',
                        NOM_CMP   = ( 'DX','DY'),
                        OPERATION = 'EXTRACTION',),);
                        
IMPR_TABLE(TABLE=TABDEP)


# REFERENCE : MODELISATION A (ELEMENTS CLASSIQUES)
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.02,
           VALE_CALC=-8.75308651E-04,
           VALE_REFE=-8.7404262899510004E-4,
           NOM_PARA='DX',
           TABLE=TABDEP,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=1.E-2,
           VALE_CALC= 3.82725446E-03,
           VALE_REFE=3.8260958514619E-3,
           NOM_PARA='DY',
           TABLE=TABDEP,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

# CALCUL AVEC POST_K1_K2_K3
PK=POST_K1_K2_K3(MODELISATION='D_PLAN',
                 MATER=ACIER,
                 FISSURE = FISS,
                 ABSC_CURV_MAXI = 0.05,
                 RESULTAT = UTOT,
                )

IMPR_TABLE(TABLE = PK)

# REFERENCE : MODELISATION A (ELEMENTS CLASSIQUES - CALCUL AVEC POST_K1_K2_K3)
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.040000000000000001,
           VALE_CALC= 8.78622505E+06,
           VALE_REFE=8.4543655482163001E6,
           NOM_PARA='K1',
           TABLE=PK,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

# CALCUL AVEC CALC_G
SIF=CALC_G(RESULTAT=UTOT,
           OPTION='CALC_K_G',
           THETA=_F(FISSURE=FISS,
                    R_INF=0.01,
                    R_SUP=0.05,),
           )

IMPR_TABLE(TABLE=SIF,);

TABG=CALC_G(RESULTAT=UTOT,
           THETA=_F(FISSURE=FISS,
                    R_INF=0.01,
                    R_SUP=0.05,),
           )

IMPR_TABLE(TABLE=TABG,);

# REFERENCE : MODELISATION A (ELEMENTS CLASSIQUES - CALCUL AVEC CALC_G)
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.02,
           VALE_CALC= 9.07949519E+06,
           VALE_REFE=9.0328413451143E6,
           NOM_PARA='K1',
           TABLE=SIF,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.02,
           VALE_CALC=490.663445803,
           VALE_REFE=492.81784641923002,
           NOM_PARA='G',
           TABLE=TABG,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

FIN();
