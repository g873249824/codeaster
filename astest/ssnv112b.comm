                   GROUP_NO='A',
                   VALE=19.9326,
                   CRITERE='RELATIF',
                   PRECISION=0.02,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='SIEF_NOEU_ELGA',
                   NOM_CMP='SIXY',
                   GROUP_NO='A',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=3.0,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='SIEF_NOEU_ELGA',
                   NOM_CMP='SIXX',
                   GROUP_NO='F',
                   VALE=20.003,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='SIEF_NOEU_ELGA',
