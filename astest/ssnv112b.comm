                   GROUP_NO='F',
                   VALE=20.003,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='SIEF_NOEU_ELGA',
                   NOM_CMP='SIZZ',
                   GROUP_NO='F',
                   VALE=20.003,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='SIEF_NOEU_ELGA',
                   NOM_CMP='SIXY',
                   GROUP_NO='F',
                   VALE=-20.003,
                   CRITERE='RELATIF',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',),),);

    # CONTRAINTES EQUIVALENTES
TEST_RESU(RESU=(_F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='EQUI_NOEU_SIGM',
                   NOM_CMP='VMIS',
                   MAILLE='M1',
                   GROUP_NO='A',
                   VALE=138.5226,
                   CRITERE='RELATIF',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='EQUI_NOEU_SIGM',
                   NOM_CMP='TRESCA',
