                   INST=1.0,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='A',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=1e-05,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   GROUP_NO='A',
                   VALE=6e-05,
                   CRITERE='RELATIF',
                   PRECISION=0.002,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='F',
                   VALE=2.1217941e-05,
                   CRITERE='RELATIF',
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
