                   GROUP_NO='A',
                   VALE=138.5226,
                   CRITERE='RELATIF',
                   PRECISION=0.01,
                   REFERENCE='ANALYTIQUE',),),);

#--------------------------------------------------------------------------------
#  TEST SUR L'ESTIMATEUR D'ERREUR EN RESIDU

TEST_RESU(RESU=_F(RESULTAT=EVOL,
                  INST=1.0,
                  NOM_CHAM='ERME_ELEM',
                  NOM_CMP='NUEST',
                  MAILLE='M537',
                  POINT=1,
                  VALE=2.1937947611240,
                  CRITERE='RELATIF',
                  PRECISION=6.E-7,
                  REFERENCE='NON_REGRESSION',
                  VERSION='9.05.04',),);

TEST_RESU(RESU=_F(RESULTAT=EVOL,
                  INST=1.0,
                  NOM_CHAM='ERME_ELNO',
                  NOM_CMP='ERREST',
                  MAILLE='M537',
                  GROUP_NO='A',
                  VALE=6.6736644029739E-04,
                  CRITERE='RELATIF',
                  PRECISION=6.E-7,
                  REFERENCE='NON_REGRESSION',
                  VERSION='9.05.04',),);

FIN();

