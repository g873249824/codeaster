                   GROUP_NO='F',
                   VALE=2.1217941e-05,
                   CRITERE='RELATIF',
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

    # CONTRAINTES
TEST_RESU(RESU=(_F(RESULTAT=EVOL,
                   INST=1.0,
                   NOM_CHAM='SIEF_NOEU_ELGA',
                   NOM_CMP='SIYY',
                   GROUP_NO='A',
                   VALE=99.9566,
                   CRITERE='RELATIF',
                   PRECISION=0.02,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=EVOL,
