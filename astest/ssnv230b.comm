DEBUT(CODE=_F(NOM='SSNV230B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'))



# DEFINITION DU MATERIAU : ACIER 304
YOUNG1 = FORMULE(NOM_PARA='TEMP', VALE='210000.0 - 30.0*TEMP', );
NU1    = FORMULE(NOM_PARA='TEMP', VALE='0.30 + 5.0E-05*TEMP', );
ALPHA1 = FORMULE(NOM_PARA='TEMP', VALE='1.0E-06*(15.0+2.0E-03*TEMP)',);
R021   = FORMULE(NOM_PARA=('TEMP','IRRA',),
    VALE='(270.0-0.65*TEMP+0.001*TEMP**2)*(4.0-3.0*exp(-IRRA/3.0))', );

RM1 = FORMULE(NOM_PARA=('TEMP','IRRA',),
    VALE='''R021(TEMP,IRRA)+( (600.0-1.5*TEMP+0.010*TEMP**2)
            -(270.00-0.65*TEMP+0.001*TEMP**2) )*0.50*
            (0.005-0.0020*(1.0-exp(-IRRA/4.0))+exp(-IRRA/1.8))''', );

EU1 = FORMULE(NOM_PARA=('TEMP','IRRA',),
    VALE='''log(1.0+((50.0-0.15*TEMP+0.0007*TEMP**2)*exp(-IRRA))*1.0E-02)''', );


# Liste de temperature pour la definition des nappes
LTEM=DEFI_LIST_REEL(
   DEBUT=0.0,
   INTERVALLE=_F(JUSQU_A=500.0, PAS=2.0,),
)
# Liste d'irradiation pour la definition des nappes
LIRR = DEFI_LIST_REEL(DEBUT=0.0,
   INTERVALLE=(
      _F(JUSQU_A =   5.0, PAS =   0.10,),
      _F(JUSQU_A =  10.0, PAS =   0.25,),
   ),
)

# POUR VALIDER LES DEUX METHODES ON INTERPOLE SEULEMENT UNE PARTIE
# DES PARAMETRES MATERIAUX

YOUNG=CALC_FONC_INTERP(FONCTION=YOUNG1, LIST_PARA=LTEM,
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
);
NU   =CALC_FONC_INTERP(FONCTION=NU1,    LIST_PARA=LTEM,
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
);
ALPHA=CALC_FONC_INTERP(FONCTION=ALPHA1, LIST_PARA=LTEM,
   PROL_GAUCHE='CONSTANT',
   PROL_DROITE='CONSTANT',
);

UN    =DEFI_CONSTANTE(VALE = 1.0)
ZERO  =DEFI_CONSTANTE(VALE = 0.0)

# [][][][][][][][] Maillage et Modele [][][][][][][][]
mail = LIRE_MAILLAGE();

mode = AFFE_MODELE( MAILLAGE=mail,
