DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
#......................................................................
#
# MODELISATION 2D_AXIS:
#......................................................................
#
#        INTERNE       HAUT            EXTERNE
#               D -----+----- C
# !               !    J    !
#                 !         !
# !               !         !
#                 !         !        10 MM
# !               !         !
#                 !         !
# !               !    F    !
#     19.5 MM   P1 -----+-----  P2
# !-------------->   1.0 MM
#                 <--------->
#======================================================================
#
#
#
#
#
#

MA=LIRE_MAILLAGE();

DEFI_GROUP(reuse =MA, MAILLAGE=MA,
              CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

MATHER=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,
                             RHO_CP=1000.0,),);

CHMATHE=AFFE_MATERIAU(MAILLAGE=MA,
                      AFFE=_F(TOUT='OUI',
                              MATER=MATHER,),);
#  ECHANGE EN PAROI INTERNE CHOC THERMIQUE FROID
#
#
#


HPAROI=DEFI_CONSTANTE(VALE=100.0,);

T_EXT=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,100.0,0.01,0.0,10.0,0.0,100.0,0.0,),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);

LISTTH=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.E-2,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=0.10000000000000001,
                                     NOMBRE=9,),
                                  _F(JUSQU_A=1.0,
                                     NOMBRE=9,),
                                  _F(JUSQU_A=2.0,
                                     NOMBRE=5,),
                                  _F(JUSQU_A=10.0,
                                     NOMBRE=8,),
                                  _F(JUSQU_A=100.0,
                                     NOMBRE=5,),),);
# ------------------------------------------------------------------
# PREMIER CALCUL SANS DIAG
# ON NE VERIFE PAS LE PRINCIPE DU MAXIMUM
#
#
#


MOTHER=AFFE_MODELE(MAILLAGE=MA,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='THERMIQUE',
                           MODELISATION='AXIS',),);

CHAR_TH=AFFE_CHAR_THER_F(MODELE=MOTHER,
                         ECHANGE=_F(GROUP_MA='ECHANGE',
                                    COEF_H=HPAROI,
                                    TEMP_EXT=T_EXT,),);

TEMPE=THER_LINEAIRE(MODELE=MOTHER,
                    CHAM_MATER=CHMATHE,
                    EXCIT=_F(CHARGE=CHAR_TH,),
                    INCREMENT=_F(LIST_INST=LISTTH,),
                    ETAT_INIT=_F(VALE=100.0,),);

IMPR_RESU(RESU=_F(RESULTAT=TEMPE,
                  INST=(0.0,0.10000000000000001,4.0,10.0,100.0,),
                  VALE_MAX='OUI',
                  VALE_MIN='OUI',),);
#
# DEUXIEME CALCUL AVEC AXIS_DIAG
# ON VERIFE LE PRINCIPE DU MAXIMUM
#
#
#


MOTHER2=AFFE_MODELE(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='THERMIQUE',
                            MODELISATION='AXIS_DIAG',),);

CHAR_TH2=AFFE_CHAR_THER_F(MODELE=MOTHER2,
                          ECHANGE=_F(GROUP_MA='ECHANGE',
                                     COEF_H=HPAROI,
                                     TEMP_EXT=T_EXT,),);

TEMPE2=THER_LINEAIRE(MODELE=MOTHER2,
                     CHAM_MATER=CHMATHE,
                     EXCIT=_F(CHARGE=CHAR_TH2,),
                     INCREMENT=_F(LIST_INST=LISTTH,),
                     ETAT_INIT=_F(VALE=100.0,),);

IMPR_RESU(RESU=_F(RESULTAT=TEMPE2,
                  INST=(0.0,0.10000000000000001,4.0,10.0,100.0,),
                  VALE_MAX='OUI',
                  VALE_MIN='OUI',),);
# ------------------------------------------------------------------
# TROISIEME CALCUL AVEC DIAG MAIS THETA = 0
# DIVERGENCE DU CALCUL
#
#


TEMPE3=THER_LINEAIRE(MODELE=MOTHER2,
                     CHAM_MATER=CHMATHE,
                     EXCIT=_F(CHARGE=CHAR_TH2,),
                     INCREMENT=_F(LIST_INST=LISTTH,),
                     ETAT_INIT=_F(VALE=100.0,),
                     PARM_THETA=0.0,);

IMPR_RESU(RESU=_F(RESULTAT=TEMPE3,
                  INST=(0.0,0.10000000000000001,4.0,10.0,100.0,),
                  VALE_MAX='OUI',
                  VALE_MIN='OUI',),);

TP1R3=RECU_FONCTION(RESULTAT=TEMPE3,
                    NOM_CHAM='TEMP',
                    NOM_CMP='TEMP',
                    GROUP_NO='P1',
                    TITRE='T=f(t) POINT P1 BAS GAUCHE THETA=0',);

TN8R1=RECU_FONCTION(RESULTAT=TEMPE,
                    NOM_CHAM='TEMP',
                    NOM_CMP='TEMP',
                    GROUP_NO='P2',
                    TITRE='T=f(t) POINT P2 MILIEU BAS AXIS NON DIAG',);

TN8R2=RECU_FONCTION(RESULTAT=TEMPE2,
                    NOM_CHAM='TEMP',
                    NOM_CMP='TEMP',
                    GROUP_NO='P2',
                    TITRE='T=f(t) POINT P2 MILIEU BAS AXIS_DIAG',);

IMPR_FONCTION(UNITE=29,
              FORMAT='XMGRACE',
              COURBE=(_F(FONCTION=TN8R1,),
                      _F(FONCTION=TN8R2,),
                      _F(FONCTION=TP1R3,),),);

IMPR_FONCTION(UNITE=30,
              FORMAT='XMGRACE',
              PILOTE='POSTSCRIPT',
              COURBE=(_F(FONCTION=TN8R1,),
                      _F(FONCTION=TN8R2,),
                      _F(FONCTION=TP1R3,),),);

IMPR_RESU(MODELE=MOTHER,FORMAT='CASTEM',
          RESU=_F(MAILLAGE=MA,
                  RESULTAT=TEMPE,
                  INST=(0.1, 4.0, 10.0,),),);


T1LBAS=POST_RELEVE_T(ACTION=_F(INTITULE='TEMP',
                               GROUP_NO='BAS',
                               RESULTAT=TEMPE,
                               NOM_CHAM='TEMP',
                               INST=4.0,
                               TOUT_CMP='OUI',
                               OPERATION='EXTRACTION',),
                     TITRE='TEMPERATURES EN BAS A 4 S NON DIAG',);

T2LBAS=POST_RELEVE_T(ACTION=_F(INTITULE='TEMP',
                               GROUP_NO='BAS',
                               RESULTAT=TEMPE2,
                               NOM_CHAM='TEMP',
                               INST=4.0,
                               TOUT_CMP='OUI',
                               OPERATION='EXTRACTION',),
                     TITRE='TEMPERATURES EN BAS A 4 S DIAG',);

IMPR_TABLE(FORMAT='AGRAF',
           TABLE=T1LBAS,
           NOM_PARA=('ABSC_CURV','TEMP',),)
IMPR_TABLE(FORMAT='AGRAF',
           TABLE=T2LBAS,
           NOM_PARA=('ABSC_CURV','TEMP',),)
#
# PREMIER CALCUL MECANIQUE : LE HAUT EST LIBRE
# MAIS LES CONTRAINTES NE SONT PAS NULLES
# CAR LE CHAMP DE DILATATION EST NON COMPATIBLE
#
#


MAT=DEFI_MATERIAU(ELAS=_F(E=2.E5,
                          NU=0.29999999999999999,
                          RHO=7.9999999999999996E-06,
                          ALPHA=1.0000000000000001E-05,),);


CHMTM=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),
                    AFFE_VARC=(
                    _F( TOUT='OUI',EVOL=TEMPE2,
                                  NOM_VARC='TEMP',
                                  NOM_CHAM='TEMP',VALE_REF=0.0),
                               ))

CHDIL=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=_F(GROUP_MA='BAS',
                                 DY=0.0,),);

INSTMECA=DEFI_LIST_REEL(VALE=(0.0,11.0,),);


DEPLDIL=MECA_STATIQUE(MODELE=MO,
                       CHAM_MATER=CHMTM,
                       EXCIT=_F(CHARGE=CHDIL,),
                       LIST_INST=INSTMECA,
                       );

CALC_CHAMP( reuse= DEPLDIL ,RESULTAT= DEPLDIL ,

                       LIST_INST=INSTMECA,
                        DEFORMATION=('EPSI_ELGA'),);

IMPR_RESU(RESU=_F(RESULTAT=DEPLDIL,
                  VALE_MAX='OUI',
                  VALE_MIN='OUI',),);
#
# DEUXIEME CALCUL MECANIQUE : LE HAUT EST BLOQUE AXIALEMENT
# EN PLUS DES CONTRAINTES DE NON COMPATIBILITE S'AJOUTENT
# LES CONTRAINTES DE BRIDAGE, QUI VALENT SIYY = -200 MPA A T=0
#
#


BRIDAGE=AFFE_CHAR_MECA(MODELE=MO,
                       DDL_IMPO=(_F(GROUP_MA='BAS',
                                    DY=0.0,),
                                 _F(GROUP_MA='HAUT',
                                    DY=0.0,),),);

DEPLBRID=MECA_STATIQUE(MODELE=MO,
                       CHAM_MATER=CHMTM,
                       EXCIT=_F(CHARGE=BRIDAGE,),
                       LIST_INST=INSTMECA,);

IMPR_RESU(RESU=_F(RESULTAT=DEPLBRID,
                  VALE_MAX='OUI',
                  VALE_MIN='OUI',),);
# TROISIEME CALCUL MECANIQUE : LE HAUT EST BLOQUE AXIALEMENT
# MAIS AVEC STAT_NON_LINE EN INCREMENTAL
# LES CONTRAINTES DE BRIDAGE, ON NE RETROUVE COMME MECA_STATIQUE
# CAR L'INSTANT T=0 (INSTANT INITAL NON CALCULE) ON SUPPOSE
# QU'IL N'Y A PAS DE CONTRAINTE.
# AU PREMIER INSTANT CALCULE, T=11, ON NE CALCULE QUE LES CONTRAINTES
# DUES A L'INCREMENT DE DILATATION ENTRE T=0 ET T=11
#
# ON OBTIENT DONC UNE ALARME


SNLBRID=STAT_NON_LINE(MODELE=MO,INFO=1,
                      CHAM_MATER=CHMTM,
                      EXCIT=_F(CHARGE=BRIDAGE,),
                      COMPORTEMENT=_F(RELATION='ELAS',),
                      INCREMENT=_F(LIST_INST=INSTMECA,),);

IMPR_RESU(RESU=_F(RESULTAT=SNLBRID,
                  VALE_MAX='OUI',
                  VALE_MIN='OUI',),);
# QUATRIEME CALCUL MECANIQUE : LE HAUT EST BLOQUE AXIALEMENT
# TOUJOURS AVEC STAT_NON_LINE EN INCREMENTAL
# POUR PRENDRE EN COMPTE LA DILATATION A T=0, ON DEFINIT TOUTE
# L'HISTOIRE, C'EST A DIRE UN CHAMP T=TREF=0, POUR UN INSTANT INITIAL T=-1
# ALORS L'INSTANT T=0 EST CALCULE, ET ON PREND EN COMPTE L'INCREMENT
# DE DILATATION ENTRE T=-1 ET T=0
# ON RETROUVE ALORS COMME MECA_STATIQUE, A T=0 ET A T=11
#
#


CHTEMP0=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                   OPERATION='AFFE',
                   MODELE=MOTHER2,
                   AFFE=_F(TOUT='OUI',
                           NOM_CMP='TEMP',
                           VALE=0.0,),);

CREA_RESU(reuse =TEMPE2,
                 OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD=CHTEMP0,
                         INST=-1.0,),);


BRIDAG2=AFFE_CHAR_MECA(MODELE=MO,
                       DDL_IMPO=(_F(GROUP_MA='BAS',
                                    DY=0.0,),
                                 _F(GROUP_MA='HAUT',
                                    DY=0.0,),),);

INSTMEC2=DEFI_LIST_REEL(VALE=(-1.0,0.0,11.0,),);

SNLBRID2=STAT_NON_LINE(MODELE=MO,
                       CHAM_MATER=CHMTM,
                       EXCIT=_F(CHARGE=BRIDAG2,),
                       COMPORTEMENT=_F(RELATION='ELAS',),
                       INCREMENT=_F(LIST_INST=INSTMEC2,),);

IMPR_RESU(RESU=_F(RESULTAT=SNLBRID2,
                  VALE_MAX='OUI',
                  VALE_MIN='OUI',),);

TEST_RESU(RESU=_F(INST=4.0,
                  TYPE_TEST='MAX',
                  RESULTAT=TEMPE2,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=99.999283381384,
                  ),
          )

FIN();
