               VALE=0.,),
GEOM_FIBRE=GF,
MULTIFIBRE=_F(GROUP_MA='POUTRE', PREC_AIRE=1.0E-05, PREC_INERTIE=2.6E-01, GROUP_FIBRE='SECF'),
);


BLOCAGE=AFFE_CHAR_MECA(MODELE=MOPOU,
                       DDL_IMPO=(_F(GROUP_NO='A', DX=0.0, DY=0.0, DZ=0.0, DRX=0.0, DRY=0.0, DRZ=0.0,),
                                 _F(GROUP_NO='B', DY=0.0, DZ=0.0, DRX=0.0, DRY=0.0, DRZ=0.0,),
                                  ),);


DEPLAC=AFFE_CHAR_MECA(MODELE=MOPOU,
                      DDL_IMPO=_F(GROUP_NO='B', DX=1.E-4,),);


FOFO=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.4,2.0,0.5,3.0,10.,4.0,-40.0,5.0,-20.0,6.0,-50.0,7.0,0.0,),
                   PROL_DROITE='EXCLU',
                   PROL_GAUCHE='EXCLU',);


LINST=DEFI_LIST_REEL(DEBUT=0.0,
INTERVALLE=(_F(JUSQU_A=1.0,  NOMBRE=50,),
            _F(JUSQU_A=2.0,  NOMBRE=50,),
            _F(JUSQU_A=3.0,  NOMBRE=50,),
            _F(JUSQU_A=4.0,  NOMBRE=50,),
            _F(JUSQU_A=5.0,  NOMBRE=50,),
            _F(JUSQU_A=6.0,  NOMBRE=50,),
            _F(JUSQU_A=7.0,  NOMBRE=50,),
),
            );


U1=STAT_NON_LINE(MODELE=MOPOU,
                 CHAM_MATER=CHMAT,
                 CARA_ELEM=POUCA,
                 EXCIT=(_F(CHARGE=BLOCAGE,),
                        _F(CHARGE=DEPLAC,
                           FONC_MULT=FOFO,),),
