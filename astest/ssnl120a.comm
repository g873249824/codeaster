DEBUT(CODE=_F( NOM = 'SSNL120A',NIV_PUB_WEB='INTERNET'))

# lecture maillage lineique
PRE_GIBI( UNITE_GIBI=17, UNITE_MAILLAGE=20,);
POUT=LIRE_MAILLAGE(UNITE=20,);

# lecture maillage section1
PRE_GIBI( UNITE_GIBI=18, UNITE_MAILLAGE=21,);
MASEC=LIRE_MAILLAGE(UNITE=21,);

MOPOU=AFFE_MODELE(MAILLAGE=POUT,
                  AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='POU_D_EM',),);


BETON=DEFI_MATERIAU(ELAS=_F(E=3.7272000000E10, NU=0.0,  RHO=2400.0,),
     LABORD_1D=_F(Y01 = 310. , Y02 = 0.070E+5, A1  = 9.E-3 ,  A2  = .52E-5 ,
                         B1  = 1.2 ,  B2  = 2. , BETA1  = .1E+7 ,  BETA2  =  -.4E+8, SIGF = 3.5E+6))

CHMAT=AFFE_MATERIAU(MAILLAGE=POUT,
                    AFFE=(_F(GROUP_MA='POUTRE', MATER=BETON,),),);



POUCA=AFFE_CARA_ELEM(MODELE=MOPOU,            INFO=1,
           POUTRE=_F(GROUP_MA=('POUTRE'),
                     SECTION='RECTANGLE',
                     CARA=('H'),
                     VALE=(1.),
                     PREC_INERTIE=0.334),
           ORIENTATION=_F(GROUP_MA=('POUTRE'),
                          CARA='ANGL_VRIL',
                          VALE=0.,),
           AFFE_SECT = _F( GROUP_MA = 'POUTRE',   NOM='ARMATURE',
                           MAILLAGE_SECT = MASEC, TOUT_SECT = 'OUI',
                           COOR_AXE_POUTRE = (.5, .5,),),
                  );


BLOCAGE=AFFE_CHAR_MECA(MODELE=MOPOU,
                       DDL_IMPO=(_F(GROUP_NO='A', DX=0.0, DY=0.0, DZ=0.0, DRX=0.0, DRY=0.0, DRZ=0.0,),
                                 ),);


DEPLAC=AFFE_CHAR_MECA(MODELE=MOPOU,
                      DDL_IMPO=_F(GROUP_NO='B', DX=1.E-4,),);


FOFO=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.4,2.0,0.5,3.0,10.,4.0,-40.0,5.0,-20.0,6.0,-50.0,7.0,0.0,),
                   PROL_DROITE='EXCLU',
                   PROL_GAUCHE='EXCLU',);


LINST=DEFI_LIST_REEL(DEBUT=0.0,
INTERVALLE=(_F(JUSQU_A=1.0,  NOMBRE=50,),
            _F(JUSQU_A=2.0,  NOMBRE=50,),
            _F(JUSQU_A=3.0,  NOMBRE=50,),
            _F(JUSQU_A=4.0,  NOMBRE=50,),
            _F(JUSQU_A=5.0,  NOMBRE=50,),
            _F(JUSQU_A=6.0,  NOMBRE=50,),
            _F(JUSQU_A=7.0,  NOMBRE=50,),
),
            );


U1=STAT_NON_LINE(MODELE=MOPOU,
                 CHAM_MATER=CHMAT,
                 CARA_ELEM=POUCA,
                 EXCIT=(_F(CHARGE=BLOCAGE,),
                        _F(CHARGE=DEPLAC,
                           FONC_MULT=FOFO,),),
                 COMP_INCR=(_F(RELATION='LABORD_1D',
                               TOUT='OUI',),),
                 INCREMENT=_F(LIST_INST=LINST,
              #                NUME_INST_FIN=178,
                              ),
              #   NEWTON=_F(MATRICE='ELASTIQUE'),
                 NEWTON=_F(MATRICE='TANGENTE',REAC_ITER=1),
                 );


U1=CALC_NO(OPTION='REAC_NODA',
           reuse =U1, RESULTAT=U1,    
           
           );

U1=CALC_ELEM(OPTION='DEGE_ELNO_DEPL',
             reuse =U1,RESULTAT=U1,   
             );

