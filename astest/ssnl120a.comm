# lecture maillage lineique
POUT=LIRE_MAILLAGE(UNITE=17,FORMAT='MED',);

# lecture maillage section1
MASEC=LIRE_MAILLAGE(UNITE=18,FORMAT='MED',);

MOPOU=AFFE_MODELE(MAILLAGE=POUT,
                  AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='POU_D_EM',),);

GF=DEFI_GEOM_FIBRE(
           SECTION = _F ( GROUP_FIBRE='SECF',
                            MAILLAGE_SECT = MASEC , TOUT_SECT = 'OUI',
                            COOR_AXE_POUTRE = (0.5, 0.5,)),
            )

BETON=DEFI_MATERIAU(ELAS=_F(E=3.7272000000E10, NU=0.0,  RHO=2400.0,),
     LABORD_1D=_F(Y01 = 310. , Y02 = 0.070E+5, A1  = 9.E-3 ,  A2  = .52E-5 ,
                         B1  = 1.2 ,  B2  = 2. , BETA1  = .1E+7 ,  BETA2  =  -.4E+8, SIGF = 3.5E+6))

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,MATER_SECT=BETON,
                  MULTIFIBRE=(_F(GROUP_FIBRE='SECF',MATER=BETON, RELATION='LABORD_1D'),))

CHMAT=AFFE_MATERIAU(MAILLAGE=POUT,
                    AFFE=(_F(GROUP_MA='POUTRE', MATER=BETON),),
                    AFFE_COMPOR=_F(TOUT='OUI',COMPOR=COMPF));



POUCA=AFFE_CARA_ELEM(MODELE=MOPOU,            INFO=1,
POUTRE=_F(GROUP_MA=('POUTRE'),
         SECTION='RECTANGLE',
         CARA=('H'),
         VALE=(1.)),
ORIENTATION=_F(GROUP_MA=('POUTRE'),
