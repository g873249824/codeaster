_F( RESULTAT=U1,NUME_ORDRE=178,NOM_CHAM='SIEF_ELGA',MAILLE='M1',POINT=1,SOUS_POINT=1,
NOM_CMP='SIXX',VALE=-40.9496E6,REFERENCE='NON_REGRESSION',PRECISION=1.E-5),

_F( RESULTAT=U1,NUME_ORDRE=178,NOM_CHAM='DEGE_ELNO_DEPL',MAILLE='M1',NOEUD='N1',
NOM_CMP='EPXX',VALE=-1.8E-3,REFERENCE='NON_REGRESSION',PRECISION=1.E-5),

))


