                         AFFE=_F(  TOUT = 'OUI',
                                PHENOMENE = 'THERMIQUE',
                                MODELISATION = 'AXIS_FOURIER') )

MA=DEFI_MATERIAU(  THER=_F(  LAMBDA = 1.,   RHO_CP = 0.) )

CM=AFFE_MATERIAU(  MAILLAGE=M,
                           AFFE=_F(  TOUT = 'OUI',   MATER = MA) )

CH=AFFE_CHAR_THER(     MODELE=MO,
                         FLUX_REP=_F(  GROUP_MA = 'GRMA6',  FLUN = 2.),
                          ECHANGE=_F(  GROUP_MA = 'GRMA7',
                                    COEF_H = 2.,   TEMP_EXT = 2.),
