                                  _F(  NOEUD = 'N12',  TEMP = 0.),
                                  _F(  NOEUD = 'N16',  TEMP = 0.)) )

KE=CALC_MATR_ELEM(  MODELE=MO,        OPTION='RIGI_THER',
                        CHARGE=CH,        CHAM_MATER=CM,
                        MODE_FOURIER=1                        )

VE=CALC_VECT_ELEM(  OPTION='CHAR_THER',
                        CHARGE=CH,     )

NU=NUME_DDL(  MATR_RIGI=KE   )

KAS=ASSE_MATRICE(  MATR_ELEM=KE,    NUME_DDL=NU   )

VAS=ASSE_VECTEUR(  VECT_ELEM=VE,    NUME_DDL=NU   )

KAS=FACT_LDLT(reuse=KAS,  MATR_ASSE=KAS   )

TEMP=RESO_LDLT(  MATR_FACT=KAS,    CHAM_NO=VAS   )

FLUXNO=CALC_CHAM_ELEM(  MODELE=MO,        OPTION='FLUX_ELNO_TEMP',
