#############################################################################################
# VALIDATION TEST FOR MACR_ECREVISSE : OPTIONS FOR THE FRICTION COEFFICIENT
#
#   HEAT EXCHANGE OPTION : 2 (ICHAL)
#   FLUID TYPE : 6 (AIR ALONE)
#############################################################################################

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),)


# Tested options for friction coefficient
#lst_frot = (0,1,2,3,4,11,14,-1,-2,-3,-4)
lst_frot = (0,1,2,3,4,11,12,13,14,-1,-2,-3,-4)
Nessai = len(lst_frot)
Npos = 9
Nneg = Nessai - Npos


# Autres options Ecrevisse
ichal  = 2
ivenac = 0

# Total flow computed by Ecrevisse alone
VQRECRE = (1.885115E-01, 4.579203E-02, 4.579203E-02, 4.579203E-02, 4.579203E-02, 3.162920E-02, 3.162915E-02, 3.162915E-02, 3.162920E-02, 3.194519E-02, 3.194519E-02,
3.194519E-02, 3.194519E-02)
# Temperature computed by Ecrevisse alone, abscissa s=0.475
VTRECRE = (2.000000E+01, 1.906403E+01, 1.906403E+01, 1.906403E+01, 1.906403E+01, 1.907983E+01, 1.907991E+01, 1.907991E+01, 1.907983E+01, 1.906709E+01, 1.906709E+01,
1.906709E+01, 1.906709E+01)

# Non regression values for total flow
VQCALC = (0.1885133,0.04579185,0.04579185, 0.04579185,0.04579185,0.03162911,3.16291E-02,3.16291E-02,0.03162911,0.03194517,0.03194517,0.03194517, 0.03194517)
# Non regression values for temperature
VTCALC = (20.,19.06399,19.06399,19.06399,19.06399,19.07994, 19.07994,19.07994,19.07994,19.06711,19.06711,19.06711,19.06711)

# Boundary conditions for pressure
prext  = 1.0E5
printe = 1.E6
prinin = 1.E6
PrAPart = 6.E5

# Boundary conditions for temperature (C)
tempint       = 180.
temp_ext      = 20.
temp_ref      = 20.

# Test duration (in seconds)
duree_inj    = 5000.

# Geometry parametres
profondeur = 1.    #m

# Material properties making coupling negligible
lambd   = 0.            #W/M/K
cp      = 88000000000.  #J/kg/K
rho     = 2500.         #kg/m3
rho_cp  = rho*cp        #J/m3/K
alpha   = 1.0E-20       #1/K
coeff_H = 0.            #W/m2/K


# ------------------------------------- #
# MESH
# ------------------------------------- #
MAIL = LIRE_MAILLAGE(FORMAT='MED',)

MAIL = DEFI_GROUP(reuse         = MAIL,
                  MAILLAGE      = MAIL,
                  CREA_GROUP_MA = (
                      _F(NOM='GHAUT',GROUP_MA = ('GM21'),),
                      _F(NOM='GBAS',GROUP_MA = ('GM22'),),
                      _F(NOM='BETON',UNION = ('GM21','GM22'),),
                      _F(NOM='INTRADOS',UNION = ('GM14','GM19'),),
                      _F(NOM='EXTRADOS',UNION = ('GM15','GM18'),),
                      _F(NOM='HAUT',GROUP_MA = ('GM16'),),
                      _F(NOM='BAS',GROUP_MA = ('GM20'),),
                      _F(NOM='ENCASTRE',UNION = ('BAS','HAUT'),),
                      _F(NOM='BFISH',GROUP_MA = ('GM13'),),
                      _F(NOM='BFISB',UNION = ('GM17'),),
                                  ),);

MAIL = DEFI_GROUP(
              reuse    = MAIL,
              MAILLAGE = MAIL,
              CREA_GROUP_NO = (
                    _F(GROUP_MA = ('BETON','ENCASTRE','INTRADOS',
                                              'EXTRADOS','BFISH','BFISB',),),
                    _F(OPTION    ='SEGM_DROI_ORDO',
                        NOM      ='BFISBO',
                        GROUP_NO ='BFISB',
                        GROUP_NO_ORIG = 'GM28',
                        GROUP_NO_EXTR = 'GM30',
                        PRECISION     = 1.E-6,
                        CRITERE = 'ABSOLU',),
                    _F(OPTION    = 'SEGM_DROI_ORDO',
                        NOM      ='BFISHO',
                        GROUP_NO ='BFISH',
                        GROUP_NO_ORIG = 'GM27',
                        GROUP_NO_EXTR = 'GM29',
                        PRECISION     = 1.E-6,
                        CRITERE = 'ABSOLU',),),)

MAIL = MODI_MAILLAGE(
              reuse        = MAIL,
              MAILLAGE     = MAIL,
              ORIE_PEAU_2D = _F(GROUP_MA = ('ENCASTRE','INTRADOS',
                                            'EXTRADOS','BFISH','BFISB',),),)

# ------------------------------------- #
# DEFINITION OF MODELS AND MATERIALS
# ------------------------------------- #
MODMECA = AFFE_MODELE(
                  MAILLAGE  = MAIL,
                  AFFE      = (_F(GROUP_MA = ('BETON','ENCASTRE','INTRADOS',
                                              'EXTRADOS','BFISH','BFISB',),
                  PHENOMENE = 'MECANIQUE',
                  MODELISATION = 'D_PLAN',),),)

MODTHER = AFFE_MODELE(
                  MAILLAGE  = MAIL,
                  AFFE      = _F(GROUP_MA=('BETON','ENCASTRE','INTRADOS',
                                           'EXTRADOS','BFISH','BFISB',),
                  PHENOMENE = 'THERMIQUE',
                  MODELISATION = 'PLAN_DIAG',),)

BETON = DEFI_MATERIAU(ELAS = _F(E      = 1.E25,
                                NU     = 0.2,
                                ALPHA  = alpha,),
                      THER = _F(LAMBDA = lambd,
                                RHO_CP = rho_cp,),)

CHMAT=AFFE_MATERIAU(MAILLAGE  = MAIL,
                    AFFE      = (_F(GROUP_MA = ('BETON','ENCASTRE','INTRADOS',
                                                'EXTRADOS','BFISH','BFISB',),
                                    MATER    = BETON,),),)


# ------------------------------------- #
# BOUNDARY CONDITIONS
# ------------------------------------- #
# Mechanical B.C. :
#
CONDMECA = AFFE_CHAR_MECA( MODELE      = MODMECA,
                           FACE_IMPO   = _F(GROUP_MA = ('ENCASTRE'), DNOR = 0.0,),
                           DDL_IMPO    = _F(GROUP_NO = ('GM24','GM25'), DX = 0), )

CONT  = DEFI_CONTACT(MODELE      = MODMECA,
                     FORMULATION = 'DISCRETE',
                     REAC_GEOM   = 'SANS',
                     ZONE        = _F(
                            GROUP_MA_MAIT = 'BFISH',
                            GROUP_MA_ESCL = 'BFISB', ),)

# Temperature and pressure time functions
#
PR_EXT = DEFI_CONSTANTE(VALE = prext,)
PR_INT = DEFI_FONCTION( NOM_PARA    = 'INST',
                        PROL_GAUCHE = 'CONSTANT',
                        PROL_DROITE = 'CONSTANT',
                        VALE = ( 0,              prinin,
                                 duree_inj,      printe, ), )

TEMP_EXT = DEFI_CONSTANTE(VALE = temp_ext,)
TEMP_INT = DEFI_FONCTION(NOM_PARA    = 'INST',
                       PROL_GAUCHE = 'CONSTANT',
                       PROL_DROITE = 'CONSTANT',
                       VALE = ( 0.,        tempint,
                                duree_inj, tempint, ),
                       )

# Thermal B.C. :
#
H_EXT    = DEFI_CONSTANTE(VALE = coeff_H,)
H_INT    = DEFI_CONSTANTE(VALE = coeff_H,)
CONDTHER = AFFE_CHAR_THER_F(
                    MODELE  = MODTHER,
                    ECHANGE = (_F(GROUP_MA = 'INTRADOS',
                                  COEF_H   = H_INT,
                                  TEMP_EXT = TEMP_INT,),
                               _F(GROUP_MA = 'EXTRADOS',
                                  COEF_H   = H_EXT,
                                  TEMP_EXT = TEMP_EXT,),),)


# --------------------------------------- #
# COMPUTATIONS WITH IFLOW = 6
#   (AIR ALONE)
#   USING DIFFERENT FRICTION COEFFICIENTS
# --------------------------------------- #
L_INST = DEFI_LIST_REEL(
                DEBUT      = 0.0,
                INTERVALLE = (
                        _F(JUSQU_A = duree_inj, NOMBRE = 2,),),)

RMECA = [None]*Nessai
iflow = 6 # Type of ingoing fluid

# Friction coefficient computed by rugosity
#  --> Ecrevisse : parameter ifrot>0
#
for icalc in xrange(Npos):
    ifrot = lst_frot[icalc]
    kgtest = 0.5
    if ifrot == 0 :
        kgtest = 0.3
    titre = 'ICHAL=2, IFLOW=6, IFROT='+str(ifrot)
    RMECA[icalc] = MACR_ECREVISSE(
                    TABLE  = CO('TABLECR'),
                    DEBIT  = CO('DEBECR'),
                    TEMPER = CO('TEMPECR'),
                    #
                    CHAM_MATER  = CHMAT,
                    MODELE_MECA = MODMECA,
                    MODELE_THER = MODTHER,
                    TEMP_INIT   = temp_ref,
                    EXCIT_MECA  = _F(CHARGE   = CONDMECA),
                    EXCIT_THER  = _F(CHARGE   = CONDTHER),
                    COMPORTEMENT= _F(RELATION ='ELAS',),
                    CONTACT     = CONT,
                    #
                    FISSURE = _F(
                          PREFIXE_FICHIER  = 'FISSURE1',
                          SECTION          = 'RECTANGLE',
                          GROUP_MA         = ('BFISH','BFISB',),
                          RUGOSITE         = 0.5E-06,
                          ZETA             = 0.0,
                          GROUP_NO_ORIG    = ('GM27','GM28',),
                          GROUP_NO_EXTR    = ('GM29','GM30',),
                          LISTE_VAL_BL     = (profondeur,profondeur),
                          OUVERT_REMANENTE = 10.0E-06,
                          TORTUOSITE       = 1.0, ),
                    #
                    ECOULEMENT = _F(
                          FLUIDE_ENTREE  = iflow,
                          PRES_ENTREE_FO = PR_INT,
                          PRES_SORTIE_FO = PR_EXT,
                          TEMP_ENTREE_FO = TEMP_INT,),
                    #
                    LIST_INST = L_INST,
                    #
                    MODELE_ECRE = _F(
                          IVENAC         = ivenac,
                          ECOULEMENT     = 'SATURATION',
                          FROTTEMENT     = ifrot,
                          TRANSFERT_CHAL = ichal,),
                    #
                    CONV_CRITERE = _F(
                          TEMP_REF = 0.5,
                          PRES_REF = 0.01*prext,
                          CRITERE  = 'EXPLICITE',),
                    #
                    CONVERGENCE_ECREVISSE = _F(
                          CRIT_CONV_DEBI  = 1.E-5,
                          KGTEST         = kgtest,
                          ITER_GLOB_MAXI = 400,),
                    #
                    VERSION     = '3.2.2',
                    COURBES     = 'AUCUNE',
                    ENTETE      = titre,
                    IMPRESSION  = 'NON',
                    INFO        = 1,
                    )

    # Test on the total flow
    TEST_TABLE(REFERENCE = 'SOURCE_EXTERNE',
               PRECISION = 1.E-5,
               VALE_CALC = VQCALC[icalc],
               VALE_REFE = VQRECRE[icalc],
               NOM_PARA  = 'DEBTOT',
               TABLE     = DEBECR,
               FILTRE    = (_F(NOM_PARA = 'INST',
                               VALE     = duree_inj,),),)
    # Test on temperature (s=0.475)
    TEST_TABLE(REFERENCE = 'SOURCE_EXTERNE',
               PRECISION = 1.E-5,
               VALE_CALC = VTCALC[icalc],
               VALE_REFE = VTRECRE[icalc],
               NOM_PARA  = 'TEMP',
               TABLE     = TABLECR,
               FILTRE    = (_F(NOM_PARA = 'COTES',
                               VALE     = 0.475,),
                            _F(NOM_PARA = 'INST',
                               VALE     = duree_inj,),),)

    DETRUIRE(CONCEPT = _F(NOM = DEBECR))
    DETRUIRE(CONCEPT = _F(NOM = TEMPECR))
    DETRUIRE(CONCEPT = _F(NOM = TABLECR))


# User defined friction coefficient
#  --> Ecrevisse : parameter -5<ifrot<0 (from Npos+1 to N)
#
for icalc in xrange(Npos,Npos+Nneg):

    kgtest = 0.5
    ifrot  = lst_frot[icalc]
    titre = 'ICHAL=2, IFLOW=6, IFROT='+str(ifrot)
    RMECA[icalc] = MACR_ECREVISSE(
                    TABLE  = CO('TABLECR'),
                    DEBIT  = CO('DEBECR'),
                    TEMPER = CO('TEMPECR'),
                    #
                    CHAM_MATER  = CHMAT,
                    MODELE_MECA = MODMECA,
                    MODELE_THER = MODTHER,
                    TEMP_INIT   = temp_ref,
                    EXCIT_MECA  = _F(CHARGE   = CONDMECA),
                    EXCIT_THER  = _F(CHARGE   = CONDTHER),
                    COMPORTEMENT= _F(RELATION ='ELAS',),
                    CONTACT     = CONT,
                    #
                    FISSURE = _F(
                          PREFIXE_FICHIER  = 'FISSURE1',
                          SECTION          = 'RECTANGLE',
                          GROUP_MA         = ('BFISH','BFISB',),
                          RUGOSITE         = 0.5E-06,
                          ZETA             = 0.0,
                          GROUP_NO_ORIG    = ('GM27','GM28',),
                          GROUP_NO_EXTR    = ('GM29','GM30',),
                          LISTE_VAL_BL     = (profondeur,profondeur),
                          OUVERT_REMANENTE = 10.0E-06,
                          TORTUOSITE       = 1.0, ),
                    #
                    ECOULEMENT = _F(
                          FLUIDE_ENTREE  = iflow,
                          PRES_ENTREE_FO = PR_INT,
                          PRES_SORTIE_FO = PR_EXT,
                          TEMP_ENTREE_FO = TEMP_INT,),
                    #
                    LIST_INST = L_INST,
                    #
                    MODELE_ECRE = _F(
                          IVENAC         = 0,
                          ECOULEMENT     = 'SATURATION',
                          REYNOLDS_LIM   = 1000.,
                          FROTTEMENT_LIM = 0.044,
                          FROTTEMENT     = ifrot,
                          TRANSFERT_CHAL = 2,),
                    #
                    CONV_CRITERE = _F(
                          TEMP_REF = 0.5,
                          PRES_REF = 0.01*prext,
                          CRITERE  = 'EXPLICITE',),
                    #
                    CONVERGENCE_ECREVISSE = _F(
                          KGTEST         = kgtest,
                          ITER_GLOB_MAXI = 400,),
                    #
                    VERSION     ='3.2.2',
                    COURBES     = 'AUCUNE',
                    ENTETE      = titre,
                    IMPRESSION  = 'NON',
                    INFO        = 1,
                    )

    # Test on the total flow
    TEST_TABLE(REFERENCE = 'SOURCE_EXTERNE',
               VALE_CALC = VQCALC[icalc],
               VALE_REFE = VQRECRE[icalc],
               NOM_PARA  = 'DEBTOT',
               TABLE     = DEBECR,
               FILTRE    = (_F(NOM_PARA = 'INST',
                               VALE     = duree_inj,),),)

    # Test on temperature (s=0.475)
    TEST_TABLE(REFERENCE = 'SOURCE_EXTERNE',
               VALE_CALC = VTCALC[icalc],
               VALE_REFE = VTRECRE[icalc],
               NOM_PARA  = 'TEMP',
               TABLE     = TABLECR,
               FILTRE    = (_F(NOM_PARA = 'COTES',
                              VALE     = 0.475,),
                            _F(NOM_PARA = 'INST',
                               VALE     = duree_inj,), ),)

    DETRUIRE(CONCEPT = _F(NOM = DEBECR))
    DETRUIRE(CONCEPT = _F(NOM = TEMPECR))
    DETRUIRE(CONCEPT = _F(NOM = TABLECR))

FIN();
