DEBUT(CODE=_F(NOM = 'VFIN001A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),)

MA=LIRE_MAILLAGE()

MO=AFFE_MODELE( MAILLAGE=MA, VERI_JACOBIEN='OUI',
   AFFE=_F( TOUT = 'OUI', PHENOMENE = 'MECANIQUE', MODELISATION = 'VF1'))
IMPR_CO(CONCEPT=_F(NOM=MO))

ACIER=DEFI_MATERIAU( ELAS=_F( E = 1.2, NU = 0.3, RHO=12.,) )

CHM=AFFE_MATERIAU( MAILLAGE=MA, AFFE=_F( TOUT = 'OUI', MATER = ACIER) )


# premier membre :
#-----------------
MK=CALC_MATR_ELEM(MODELE=MO,  CHAM_MATER=CHM, OPTION='RIGI_MECA', )
IMPR_CO(CONCEPT=_F(NOM=MK))

NU=NUME_DDL(  MATR_RIGI=MK, METHODE='LDLT', RENUM='SANS',INFO=2,)
IMPR_CO(CONCEPT=_F(NOM=NU))

MATK=ASSE_MATRICE(  MATR_ELEM=MK, NUME_DDL=NU,INFO=2)
IMPR_CO(CONCEPT=_F(NOM=MATK))

TEST_RESU(OBJET=_F(VALE_CALC=5.86586E5,
                   TOLE_MACHINE=1.E-3,
                   NOM='MATK               .VALM',),
          )

# second membre :
#-----------------
CH1=AFFE_CHAR_MECA(MODELE=MO, PESANTEUR=_F(GRAVITE=9.81,
                                           DIRECTION=(0.,  -1.,  0., ), ),)
VECEL=CALC_VECT_ELEM( OPTION='CHAR_MECA', CHAM_MATER=CHM, CHARGE=CH1)
IMPR_CO(CONCEPT=_F(NOM=VECEL))
VECAS=ASSE_VECTEUR( VECT_ELEM=VECEL, NUME_DDL=NU )
IMPR_CO(CONCEPT=_F(NOM=VECAS))


FIN()
