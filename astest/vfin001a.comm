MA=LIRE_MAILLAGE()

MO=AFFE_MODELE( MAILLAGE=MA, AFFE=_F( TOUT = 'OUI', PHENOMENE = 'MECANIQUE', MODELISATION = 'VF1'))
IMPR_CO(CONCEPT=_F(NOM='MO'))

ACIER=DEFI_MATERIAU( ELAS=_F( E = 1.2, NU = 0.3) )

CHM=AFFE_MATERIAU( MAILLAGE=MA, AFFE=_F( TOUT = 'OUI', MATER = ACIER) )


MK=CALC_MATR_ELEM(MODELE=MO,  CHAM_MATER=CHM, OPTION='RIGI_MECA', )
IMPR_CO(CONCEPT=_F(NOM='MK'))

NU=NUME_DDL(  MATR_RIGI=MK, METHODE='LDLT', RENUM='SANS',INFO=2,)
IMPR_CO(CONCEPT=_F(NOM='NU'))

MATK=ASSE_MATRICE(  MATR_ELEM=MK, NUME_DDL=NU,INFO=2)
IMPR_CO(CONCEPT=_F(NOM='MATK'))

TEST_RESU(OBJET=(
  _F( NOM='MATK               .VALM',  S_R=5.86586000E+05,  REFERENCE='NON_REGRESSION' ),
))

FIN()
