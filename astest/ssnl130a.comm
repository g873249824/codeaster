   MODELE=model,
   FORCE_COQUE=_F(GROUP_MA='DALLE', PRES=PRESS,),
   DDL_IMPO = _F(GROUP_MA='PTRESS',DX=ZERO,DY=ZERO,DZ=DEPLA),
)

