DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

acier=DEFI_MATERIAU(ELAS=_F(E=210000000000.0,
                            NU=0.0,),);


# 1 - VALIDATION DE ORIE_PEAU_2D/GROUP_MA_SURF
# --------------------------------------------
#
#        ^y
#        .
#      A .           C            E
#        |-----------|------------|
#        |           |            |
#        |           |            |
#        |    MA1    |     MA2    |
#        |           |            |
#        |           |            |
#      z .-----------|------------|...> x
#        B           D            F
#
# Le maillage est constitue des :
#  - mailles "solides" MA1 et MA2
#  - mailles de bords AB, CD et EF.
# On bloque AB et on impose une pression SIXX= p=1.E8 (TRACTION) sur le cote CD
#
# L'etat initial est tel que la maille CD est mal orientee, il faut donc la
# reoriente avec MODI_MAILLAGE et ORIE_PEAU_2D/GROUP_MA_SURF
#
mail2D = LIRE_MAILLAGE (INFO=2) ;

mail2D=MODI_MAILLAGE(reuse=mail2D,
                 MAILLAGE=mail2D,
                 ORIE_PEAU_2D=_F(GROUP_MA=('GXXXXXX_CD'),
                                 GROUP_MA_SURF=('GXXXXXX_MA1'),),)

chmat2D=AFFE_MATERIAU(MAILLAGE=mail2D,
                    AFFE=_F(TOUT='OUI',
                            MATER=acier,),);

mo2D=AFFE_MODELE(MAILLAGE=mail2D,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='C_PLAN',),);


clim2D=AFFE_CHAR_MECA(MODELE=mo2D,
                    DDL_IMPO=_F(GROUP_NO=('GXXXXXX_AB',),
                                DX=0.,DY=0.,),);

# On ajoute VERI_NORM='NON' pour annuler la verification de la normale

pres2D=AFFE_CHAR_MECA(MODELE=mo2D,VERI_NORM='NON',
                      PRES_REP=_F(GROUP_MA=('GXXXXXX_CD',),
                      PRES=-1.E8),);

resu2D=MECA_STATIQUE(
                   MODELE=mo2D,
                   CHAM_MATER=chmat2D,
                   EXCIT=(_F(CHARGE=pres2D,),
                        _F(CHARGE=clim2D,),),);

resu2D=CALC_CHAMP(CONTRAINTE=('SIGM_ELNO'),RESULTAT=resu2D,reuse=resu2D)


# On verifie que :
#  - le deplacement est positif et egal = 0.00142857 aux noeuds C et D.
#  - la contrainte SIXX dans la maille MA1 est positive et egale a 1.E8.

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=resu2D,
                  NOM_CHAM='DEPL',
                  NOEUD='C',
                  NOM_CMP='DX',
                  VALE_CALC= 1.42857143E-03,
                  VALE_REFE=1.4285700000000001E-3,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=resu2D,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='C',
                  NOM_CMP='SIXX',
                  VALE_CALC= 1.00000000E+08,
                  VALE_REFE=1.E8,
                  MAILLE='MA1',),
          )

IMPR_RESU(RESU=_F(RESULTAT=resu2D))



# 2 - VALIDATION DE ORIE_PEAU_3D/GROUP_MA_VOLU
# --------------------------------------------
#
#            A1          C1            E1
#             --------------------------
#        z   /.          /.           /|
#        ^  / .         / .          / |
#        . /  .        /  .         /  |
#      B1./   .     D1/   .      F1/   |
#        |-----------|------------|    |
#        |   A.      |   C.       |   E|
#        |   ........|............|.../
#        |  .  MA1   |  .  MA2    |  /
#        | .         | .          | /
#        |.          |.           |/
#        |-----------|------------|.....> x
#        B           D            F

## Le maillage est constitue des :
#  - mailles "solides" MA1 et MA2
#  - mailles de bords M1 (A,B,B1,A1), M2 (C,D,D1,C1) et M3 (E,F,F1,E1).
# On la face (A,B,B1,A1) et on impose une pression SIXX= p=1.E8 (TRACTION)
# la face (C,D,D1,C1)
#
# L'etat initial est tel que la maille M2 est mal orientee il faut donc la
# reoriente avec MODI_MAILLAGE et ORIE_PEAU_3D/GROUP_MA_VOLU
#

mail3D = LIRE_MAILLAGE (UNITE=21,INFO=2) ;

mail3D=MODI_MAILLAGE(reuse=mail3D,
                 MAILLAGE=mail3D,
                 ORIE_PEAU_3D=_F(GROUP_MA=('GXXXXXX_M2'),
                                 GROUP_MA_VOLU=('GXXXXXX_MA1'),),)

chmat3D=AFFE_MATERIAU(MAILLAGE=mail3D,
                    AFFE=_F(TOUT='OUI',
                            MATER=acier,),);

mo3D=AFFE_MODELE(MAILLAGE=mail3D,
                AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='3D',),);

clim3D=AFFE_CHAR_MECA(MODELE=mo3D,
                    DDL_IMPO=_F(GROUP_MA=('GXXXXXX_M1',),
                                DX=0.,DY=0.,DZ=0.),);

# On ajoute VERI_NORM='NON' pour annuler la verification de la normale

pres3D=AFFE_CHAR_MECA(MODELE=mo3D,VERI_NORM='NON',
                      PRES_REP=_F(GROUP_MA=('GXXXXXX_M2',),
                      PRES=-1.E8),);

resu3D=MECA_STATIQUE(
                   MODELE=mo3D,
                   CHAM_MATER=chmat3D,
                   EXCIT=(_F(CHARGE=pres3D,),
                        _F(CHARGE=clim3D,),),);

resu3D=CALC_CHAMP(CONTRAINTE=('SIGM_ELNO'),RESULTAT=resu3D,reuse=resu3D)


# On verifie que :
#  - le deplacement est positif et egal = 0.00142857 aux noeuds C,D
#  - la contrainte SIXX, dans la maille MA1, est positive et egale a 1.E8

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=resu3D,
                  NOM_CHAM='DEPL',
                  NOEUD='C',
                  NOM_CMP='DX',
                  VALE_CALC= 1.42857143E-03,
                  VALE_REFE=1.4285700000000001E-3,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=resu3D,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='C',
                  NOM_CMP='SIXX',
                  VALE_CALC= 1.00000000E+08,
                  VALE_REFE=1.E8,
                  MAILLE='MA1',),
          )

IMPR_RESU(RESU=_F(RESULTAT=resu3D))


FIN();
