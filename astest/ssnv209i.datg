opti dime 2 elem tri3;

*----------------------------------------
*donnees
*----------------------------------------
eps = 0.001;

* dimensions des cotés

r   = 0.04;
ly  = r;
lyd = -1*r/4;
lx  = r;

* nombre d'éléments
ny  = 10;
nyd = 3;
nx  = 16;

*---------------------------------------------
*construction des points
*---------------------------------------------

phs = 0. ly ;
pps = 0. 0. ;
pbs = 0. 0. ;
pcs = 0. lyd ;

pha = lx ly ;
ppa = lx 0. ;
pba = lx 0. ;
pca = lx lyd ;

PPC = ppa moins (0.005 0.);
PPD = ppa moins (0.0075 0.);

*---------------------------------------------
*construction des lignes
*---------------------------------------------

LENCAV = droi PCS PCA nx;
LBATI  = droi PBS PBA nx;

LCONTA = droi PPA PPS nx;
LBLOC  = droi PPS PHS ny;
LPRESV = droi PHS PHA nx;
LPRESH = droi PHA PPA ny;

*---------------------------------------------
*construction des surfaces
*---------------------------------------------

SBATI = LBATI regl nyd LENCAV;

SPLAQ = elim(
(dall LCONTA LBLOC LPRESV LPRESH) et PPC et PPD) eps;

maille = SBATI et SPLAQ;

trac maille;
opti sauv format
'/work/irlin831_1/siavelim/CAS/ssnv209/ssnv209i.mgib';
sauv form maille ;
fin;
