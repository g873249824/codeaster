DEBUT(CODE=_F(NOM='FORMA10B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'))
#  SDVERI='NON' pour des raisons de cout CPU
Pilote = 'PNG';


ACIER=DEFI_MATERIAU(ELAS=_F(E=195000.,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=1930.,
                                 SY=181.,),);

Sig=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                        1.,151.2,
                        2.,257.2,
                        3.,259.3,
                        4.,0.,
                        ),);

Tau=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                        1.,93.1,
                        2.,33.1,
                        3.,0.,
                        4.,0.,
                        ),);

IMPR_FONCTION(FORMAT='XMGRACE',
              PILOTE=Pilote,
              UNITE=30,
              COURBE=_F(FONC_X=Sig,
                        FONC_Y=Tau,),
              LEGENDE_X='Sig',
              LEGENDE_Y='Tau',);


L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=0.81,NOMBRE=1,),
                                  _F(JUSQU_A=1.00,NOMBRE=1,),
                                  _F(JUSQU_A=2.00,NOMBRE=1,),
                                  _F(JUSQU_A=3.00,NOMBRE=1,),),);

# gestion manuelle avec event-driven
DEFLIST = DEFI_LIST_INST(DEFI_LIST=_F(METHODE   ='MANUEL',
                                      LIST_INST = L_INST, ),
                         ECHEC=_F(EVENEMENT='DELTA_GRANDEUR',
                                  VALE_REF=0.2e-2,
                                  NOM_CHAM='VARI_ELGA',
                                  NOM_CMP ='V1',
                                  SUBD_NIVEAU=10),
                         )


U=SIMU_POINT_MAT(COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',),
                 MATER=ACIER,
                 INCREMENT=_F(LIST_INST=DEFLIST),
                 NEWTON=_F(PREDICTION='ELASTIQUE',
                           MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=1.E-8),
                 SIGM_IMPOSE=_F(SIXX=Sig,
                                SIXY=Tau,),INFO=1);

IMPR_TABLE(TABLE=U,);

# A

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=0.014829714,
           VALE_REFE=0.014829999999999999,
           NOM_PARA='EPXX',
           TABLE=U,
           FILTRE=_F(PRECISION=1.0000000000000001E-05,
                     NOM_PARA='INST',
                     VALE=1.0,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           VALE_CALC=0.013601401,
           VALE_REFE=0.0136014,
           NOM_PARA='EPXY',
           TABLE=U,
           FILTRE=_F(PRECISION=1.0000000000000001E-05,
                     NOM_PARA='INST',
                     VALE=1.0,),
           )

# B

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=5.0000000000000001E-3,
           VALE_CALC=0.035401234,
           VALE_REFE=0.035264999999999998,
           NOM_PARA='EPXX',
           TABLE=U,
           FILTRE=_F(PRECISION=1.0000000000000001E-05,
                     NOM_PARA='INST',
                     VALE=2.0,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.014999999999999999,
           VALE_CALC=0.020245631,
           VALE_REFE=0.020471,
           NOM_PARA='EPXY',
           TABLE=U,
           FILTRE=_F(PRECISION=1.0000000000000001E-05,
                     NOM_PARA='INST',
                     VALE=2.0,),
           )

# C

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=5.0000000000000001E-3,
           VALE_CALC=0.035412003,
           VALE_REFE=0.035277700000000002,
           NOM_PARA='EPXX',
           TABLE=U,
           FILTRE=_F(PRECISION=1.0000000000000001E-05,
                     NOM_PARA='INST',
                     VALE=3.0,),
           )

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           PRECISION=0.014999999999999999,
           VALE_CALC=0.020024964,
           VALE_REFE=0.020246199999999999,
           NOM_PARA='EPXY',
           TABLE=U,
           FILTRE=_F(PRECISION=1.0000000000000001E-05,
                     NOM_PARA='INST',
                     VALE=3.0,),
           )

# pour trace de courbe et comparaison avec modelisation A

EPXX=RECU_FONCTION(TABLE=U,
                   PARA_X='INST',
                   PARA_Y='EPXX',);

IMPR_FONCTION(FORMAT='TABLEAU',
              COURBE=_F(FONCTION=EPXX,
                        LEGENDE='EPXX',
                        MARQUEUR=1,),
              UNITE=31,);

EPXY=RECU_FONCTION(TABLE=U,
                   PARA_X='INST',
                   PARA_Y='EPXY',);

IMPR_FONCTION(FORMAT='TABLEAU',
              COURBE=_F(FONCTION=EPXY,
                        LEGENDE='EPXY',
                        MARQUEUR=1,),
              UNITE=32,);

FIN();
