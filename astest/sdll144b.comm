                           _F(VECTEUR=CO("bal_Xass"),
                              OPTION='CHAR_MECA',
                              CHARGE=bal_FX),
                           ),);




tours= [0.0 , 5000.0]
NTRIMdeb = tours[0]
OMdeb = 2 * pi * NTRIMdeb / 60.0
NTRIMfin = tours[1]
OMfin = 2 * pi * NTRIMfin / 60.0
PHIdeb = 0
tdeb = 0.0
tfin = 12.0
pdtf = 5e-05
alpha = 3.273e-01

A = ((exp(alpha*tfin)*OMfin)-(exp(alpha*tdeb)*OMdeb))/(exp(alpha*tfin)-exp(alpha*tdeb))
B = (OMfin-OMdeb)/(exp(-alpha*tfin)-exp(-alpha*tdeb))
C = PHIdeb - A*tdeb - ((B/alpha)*exp(-alpha*tdeb))

PHI=FORMULE(VALE='A*INST - ((B/alpha)*exp(-alpha*INST)) + C',
            NOM_PARA='INST',);

OM=FORMULE(VALE='A+B*exp(-alpha*INST)',
           NOM_PARA='INST',);

ACC=FORMULE(VALE='-alpha*B*exp(-alpha*INST)',
            NOM_PARA='INST',);


forx = FORMULE(VALE='(OM(INST) ** 2) * cos(PHI(INST)) + ACC(INST) * sin(PHI(INST))',
                NOM_PARA='INST',);

fory = FORMULE(VALE='(OM(INST) ** 2) * sin(PHI(INST)) - ACC(INST) * cos(PHI(INST))',
                NOM_PARA='INST',);

PROJ_BASE(BASE=MODES,
                STOCKAGE='PLEIN',
                MATR_ASSE_GENE=(_F(MATRICE=CO('MAGE'),
                                   MATR_ASSE=MASSE2,),
