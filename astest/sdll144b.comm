                                     PAS=pdtf,
                                     COEF_MULT_PAS=1.1,
                                     COEF_DIVI_PAS=1.3,
                                     PAS_MAXI=(pdtf*100.),
                                     PAS_MINI=(pdtf/100.),
                                     VERI_PAS='NON',
                                     ),
                        EXCIT=(_F(VECT_ASSE_GENE=BXGE,
