DEBUT(CODE=_F(NOM='SSNV173E',
              NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'));

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),   
                    );
                       
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                   AFFE=_F(GROUP_MA=('VOL',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                     );        

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-12.5 ');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN),
                    );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         );

E=205000.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL',),
                                MATER=ACIER,
                                ),
                             ),);
ZERO=0.0
DEPX=1.E-6
DEPY=2.E-6
DEPZ=3.E-6

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(NOEUD=('N1','N8','N30','N19',),
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(NOEUD=('N16','N33','N29','N44',),
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=DEPX,),
                                 ),
                       INFO=1,);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(NOEUD=('N1','N8','N30','N19',),
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(NOEUD=('N16','N33','N29','N44',),
                                    DX=DEPX,
                                    DY=DEPY,
                                    DZ=DEPZ,),
                                 ),
                       INFO=1,);
          
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CH1,), 
                          _F(CHARGE=CHXFEM,),),                  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
#                   NEWTON=_F(MATRICE='ELASTIQUE',),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=8,
                                  RESI_GLOB_MAXI=1e-07,
#                                  RESI_GLOB_RELA=1e-06,
                                  ),
#                   SOLVEUR=_F(METHODE='LDLT',
#                              RENUM='RCMK',
#                              STOP_SINGULIER='OUI',),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=2,);

IMPR_CO(CONCEPT=_F(NOM=FISS))

UTOT2=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CH2,), 
                          _F(CHARGE=CHXFEM,),),                  
                   COMP_ELAS=_F(RELATION='ELAS',
