# MODELISATION : DST
# 
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='NON',),
              DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(FORMAT='MED',);


MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(OPTION='PLAN',
                                  NOM='Plan_XOY',
                                  POINT=(0.,0.,0.,),
                                  VECT_NORMALE=(0.,0.,1.,),
                                  PRECISION=0.001,),
                               _F(OPTION='PLAN',
                                  NOM='Plan_YOZ',
                                  POINT=(0.,0.,0.,),
                                  VECT_NORMALE=(1.,0.,0.,),
                                  PRECISION=0.001,)))
MOEL=AFFE_MODELE(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DST',),);

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------

CAEL=AFFE_CARA_ELEM(MODELE=MOEL,
                    COQUE=_F(GROUP_MA=('Tore_Q4','Tore_T3'),
                             EPAIS=0.02,
                             COQUE_NCOU=1,
                             VECTEUR=(1.,0.,0.)))
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------

MATERIAU=DEFI_MATERIAU(ELAS=_F(E=2.1E+11,
                               NU=0.3,
                               RHO=7800,),);

#

CHAM_MAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATERIAU,),);

mass=POST_ELEM(MASS_INER=_F(TOUT='OUI',),
               MODELE=MOEL,
               CHAM_MATER=CHAM_MAT,
               CARA_ELEM=CAEL,);

IMPR_TABLE(TABLE=mass,);

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------

CON_LI=AFFE_CHAR_MECA(MODELE=MOEL,
                      DDL_IMPO=(_F(GROUP_NO='Plan_XOY',
                                   DZ=0.,),
                                _F(GROUP_NO='Plan_YOZ',
                                   DX=0.,),
                                _F(GROUP_MA=('Sect_A','Sect_B'),
                                   DY=0.,
                                   DRX=0.,
                                   DRZ=0.)))

#

CHARGE=AFFE_CHAR_MECA(MODELE=MOEL,
                      PRES_REP=(_F(GROUP_MA=('Tore_Q4','Tore_T3'),
                                      PRES=10000.,),),);

#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------

RES=MECA_STATIQUE(MODELE=MOEL,
                  CHAM_MATER=CHAM_MAT,
                  CARA_ELEM=CAEL,
                  EXCIT=(_F(CHARGE=CON_LI,),
                         _F(CHARGE=CHARGE,),),);


IMPR_RESU(RESU=_F(RESULTAT=RES,NOM_CHAM='DEPL',GROUP_NO='A_TRIA'))
IMPR_RESU(RESU=_F(RESULTAT=RES,NOM_CHAM='DEPL',GROUP_NO='A_QUAD'))
IMPR_RESU(RESU=_F(RESULTAT=RES,NOM_CHAM='DEPL',GROUP_NO='B_TRIA'))
IMPR_RESU(RESU=_F(RESULTAT=RES,NOM_CHAM='DEPL',GROUP_NO='B_QUAD'))

TEST_RESU(RESU=(
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RES,
                   NOM_CHAM='DEPL',
                   GROUP_NO='A_QUAD',
                   NOM_CMP='DX',
                   VALE_CALC=1.1602358980267E-07,
                   VALE_REFE=1.19e-7,
                   CRITERE='RELATIF',
                   PRECISION=3.E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RES,
                   NOM_CHAM='DEPL',
                   GROUP_NO='A_TRIA',
                   NOM_CMP='DX',
                   VALE_CALC=-1.1705106996213E-07,
                   VALE_REFE=-1.19e-7,
                   CRITERE='RELATIF',
                   PRECISION=2.E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RES,
                   NOM_CHAM='DEPL',
                   GROUP_NO='B_QUAD',
                   NOM_CMP='DX',
                   VALE_CALC=1.774570634308E-06,
                   VALE_REFE=1.79e-6,
                   CRITERE='RELATIF',
                   PRECISION=1.5E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RES,
                   NOM_CHAM='DEPL',
                   GROUP_NO='B_TRIA',
                   NOM_CMP='DX',
                   VALE_CALC=-1.7732064351646E-06,
                   VALE_REFE=-1.79e-6,
                   CRITERE='RELATIF',
                   PRECISION=1.5E-2,),
))

RES=CALC_CHAMP(   RESULTAT=RES,reuse=RES,
                  CONTRAINTE=('SIGM_ELNO'),
                  GROUP_MA=('AM_QUAD','AM_TRIA','BM_QUAD','BM_TRIA'))


SIG_M_EL=POST_CHAMP(RESULTAT=RES,
                EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                   NUME_COUCHE=1,
                                   NIVE_COUCHE='MOY',),);

SIG_M_EL=CALC_CHAMP( RESULTAT=SIG_M_EL,reuse= SIG_M_EL,
                    CONTRAINTE=('SIGM_NOEU'))

SIG_M_NO=CALC_CHAMP(RESULTAT=SIG_M_EL,
                CONTRAINTE='SIGM_NOEU',);

IMPR_RESU(RESU=_F(RESULTAT=SIG_M_NO,NOM_CHAM='SIGM_NOEU',GROUP_NO=('A_QUAD','A_TRIA')))
IMPR_RESU(RESU=_F(RESULTAT=SIG_M_NO,NOM_CHAM='SIGM_NOEU',GROUP_NO=('B_QUAD','B_TRIA')))



# Test de SIXX pour r=1 Point A

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='A_QUAD',
                   NOM_CMP='SIXX',
                   VALE_CALC=7.1744992534394E+05,
                   VALE_REFE=7.5e5,
                   CRITERE='RELATIF',
                   PRECISION=5.E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='A_TRIA',
                   NOM_CMP='SIXX',
                   VALE_CALC=7.1765347670987E+05,
                   VALE_REFE=7.5e5,
                   CRITERE='RELATIF',
                   PRECISION=5.E-2,)))

# Test de SIXX pour r=3 Point B

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='B_QUAD',
                   NOM_CMP='SIXX',
                   VALE_CALC=4.0660294888918E+05,
                   VALE_REFE=4.17e5,
                   CRITERE='RELATIF',
                   PRECISION=3.E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='B_TRIA',
                   NOM_CMP='SIXX',
                   VALE_CALC=4.0660330265533E+05,
                   VALE_REFE=4.17e5,
                   CRITERE='RELATIF',
                   PRECISION=3.E-2,)))

# Test de SIYY
TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='A_QUAD',
                   NOM_CMP='SIYY',
                   VALE_CALC=2.7770875198994E+05,
                   VALE_REFE=2.50e5,
                   CRITERE='RELATIF',
                   PRECISION=12.E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='A_TRIA',
                   NOM_CMP='SIYY',
                   VALE_CALC=2.7821843033481E+05,
                   VALE_REFE=2.5e5,
                   CRITERE='RELATIF',
                   PRECISION=12.E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='B_QUAD',
                   NOM_CMP='SIYY',
                   VALE_CALC=2.5899836868467E+05,
                   VALE_REFE=2.50e5,
                   CRITERE='RELATIF',
                   PRECISION=4.E-2,),
                _F(NUME_ORDRE=1,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=SIG_M_EL,
                   NOM_CHAM='SIGM_NOEU',
                   GROUP_NO='B_TRIA',
                   NOM_CMP='SIYY',
                   VALE_CALC=2.588511817791E+05,
                   VALE_REFE=2.5e5,
                   CRITERE='RELATIF',
                   PRECISION=4.E-2,),))

FIN();
