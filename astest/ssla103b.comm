# CALCUL AVEC MECA_STATIQUE

U2=MECA_STATIQUE(MODELE=MO,
                 CHAM_MATER=CM,
                 EXCIT=_F(CHARGE=CH1,),
                 INST=3600.0,);

U2=CALC_CHAMP(reuse=U2,RESULTAT=U2,CONTRAINTE=('SIGM_ELNO'),DEFORMATION=('EPSI_ELNO'))

U2=CALC_CHAMP(reuse =U2,
