                   VALE_REFE=110.52,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.041000000000000002,),
                _F(PARA='FREQ',
                   NUME_MODE=6,
                   RESULTAT=MODES,
                   VALE_CALC=106.066661395,
                   VALE_REFE=110.52,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.041000000000000002,),
                _F(PARA='FREQ',
                   NUME_MODE=7,
                   RESULTAT=MODES,
                   VALE_CALC=160.109646306,
                   VALE_REFE=169.08000000000001,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.055,),
                _F(PARA='FREQ',
                   NUME_MODE=8,
                   RESULTAT=MODES,
                   VALE_CALC=193.600964057,
                   VALE_REFE=193.93000000000001,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.02,),
                _F(PARA='FREQ',
                   NUME_MODE=11,
                   RESULTAT=MODES,
                   VALE_CALC=206.209316568,
                   VALE_REFE=206.63999999999999,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                _F(PARA='FREQ',
                   NUME_MODE=12,
                   RESULTAT=MODES,
                   VALE_CALC=206.211619713,
