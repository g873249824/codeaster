#---------------------------------------------------------------------
#  - CALCUL EN ANALYSE MODALE DYNAMIQUE D'UNE PLAQUE CARREE EPAISSE 
#    SIMPLEMENT SUPPORTEE 
#  - MODELISATION AVEC DES ELEMENTS COQUE_3D (TRIA7) 
#---------------------------------------------------------------------
#
DEBUT( CODE=_F( NOM = 'SDLS502E',NIV_PUB_WEB='INTERNET'),);

PLAQ_M=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED', );

PLAQ_MA=CREA_MAILLAGE(MAILLAGE=PLAQ_M,
                      MODI_MAILLE=_F(OPTION='TRIA6_7',
                                     TOUT='OUI',),);
#
MODEL=AFFE_MODELE(   MAILLAGE=PLAQ_MA,
                           AFFE=_F( TOUT = 'OUI',
                                 PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'COQUE_3D',),);
PLAQ_MA=DEFI_GROUP(reuse =PLAQ_MA,
                MAILLAGE=PLAQ_MA,
                CREA_GROUP_NO=_F(GROUP_MA='BORDS',NOM='BORDS',),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES 
#---------------------------------------------------------------------
#
CARA_ELE=AFFE_CARA_ELEM(   MODELE=MODEL,
                              COQUE=_F( GROUP_MA = 'TOUT_ELT',
                                     EPAIS = 1.,
                                     ANGL_REP = (15.5,0.,),),
                           );

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX 
#---------------------------------------------------------------------
#
ACIER=DEFI_MATERIAU(   ELAS=_F( E = 2.E+11,
                               NU = 0.3,
                               RHO = 8000.),);

#
MAT=AFFE_MATERIAU(   MAILLAGE=PLAQ_MA,
                           AFFE=_F( TOUT = 'OUI', MATER = ACIER)
                                                   );

#
#---------------------------------------------------------------------
#                     CHARGEMENTS 
#---------------------------------------------------------------------
#
CHAR0=AFFE_CHAR_MECA(     MODELE=MODEL,
                          DDL_IMPO=_F( GROUP_NO = 'BORDS', DZ = 0.)
                        );

#
#---------------------------------------------------------------------
#                     RESOLUTION 
#---------------------------------------------------------------------
#
M_EL_RIG=CALC_MATR_ELEM(       MODELE=MODEL,
                             CHAM_MATER=MAT,
                              CARA_ELEM=CARA_ELE,
                                 CHARGE=CHAR0,
                                 OPTION='RIGI_MECA'
                           );

#
M_EL_MAS=CALC_MATR_ELEM(       MODELE=MODEL,
                             CHAM_MATER=MAT,
                              CARA_ELEM=CARA_ELE,
                                 CHARGE=CHAR0,
                                 OPTION='MASS_MECA'
                           );

#
NU=NUME_DDL(   MATR_RIGI=M_EL_RIG)

#
M_AS_RIG=ASSE_MATRICE(   MATR_ELEM=M_EL_RIG,   NUME_DDL=NU)

#
M_AS_MAS=ASSE_MATRICE(   MATR_ELEM=M_EL_MAS,   NUME_DDL=NU)

#
MODES=MODE_ITER_SIMULT(      MATR_RIGI=M_AS_RIG,
                               MATR_MASS=M_AS_MAS,
                              METHODE='SORENSEN',
                            CALC_FREQ=_F( OPTION = 'BANDE',
                                       FREQ = (10., 350.,))   
                          );


#
#---------------------------------------------------------------------
#                     VERIFICATION DES RESULTATS 
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(_F(PARA='FREQ',
                   NUME_MODE=4,
                   RESULTAT=MODES,
                   VALE_CALC=43.866729613,
                   VALE_REFE=44.762,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.021000000000000001,),
                _F(PARA='FREQ',
                   NUME_MODE=5,
                   RESULTAT=MODES,
