DEBUT(CODE=_F(NOM='SSNV185B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'))

# MODELISATION B : X-FEM FISSURE COINCIDENTE - TRACTION (FACE SUP)

#----------------------------------------------
#                MAILLAGE : hexa_5_30_50.mgib
#----------------------------------------------

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='SURFSUP',),);
                    
                    
#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

MAILLAG1=MODI_MAILLAGE(reuse =MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFSUP','SURFINF',),),
                       );

# PARAMETRES DE LA FISSURE : 0 < a < 6.5 -> influt sur le choix Rsup !
#                            b = 10.
a=5.
b=10.
#LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-15.');
#LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='b-a-Y');
             
FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    INFO=1,
#                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    DEFI_FISS=_F(
                                 FORM_FISS = 'DEMI_PLAN',
                                 PFON      =(0. , 5. ,15.),
                                 NORMALE   =(0. , 0. , 1.),
                                 DTAN      =(0. ,-1. , 0.)),);
                   
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,FISSURE=FISS,INFO=1,);


#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=205000.0E6
nu=0.
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT = 'OUI',
                                MATER=ACIER,
                                ),
                             );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

PRES=-1.E6
CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(NOEUD='N3751',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(NOEUD='N9276',DZ=0.0,),
                             _F(NOEUD='N3760',DX=0.0,DZ=0.0,),),);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   PRES_REP=_F(GROUP_MA=('SURFSUP','SURFINF',),PRES=PRES,),);

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

UTOT2=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,
                   EXCIT=(
                          _F(CHARGE=CH1),
                          _F(CHARGE=CH2),
                          ),
                   INFO=1,
                   ); 

#------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS PAR LA METHODE ENERGETIQUE AVEC X-FEM
#------------------------------------------------------------------------

# TEST AVEC PLUSIEURS COURONNES
if a > 4  :
  RI=[2. , 0.666 , 1. , 1. , 1. , 2.1]
  RS=[4. , 1.666 , 2. , 3. , 4. , 3.9]
else :
  RI=[0.666 , 1.]
  RS=[1.666 , 2.]


nbc=len(RI)
SIF = [None]*nbc
GLO = [None]*nbc

for i in range(0,nbc) :

   SIF[i]=CALC_G(RESULTAT=UTOT2,
                 OPTION='CALC_K_G',
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[i],
                          R_SUP=RS[i],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),);

   IMPR_TABLE(TABLE=SIF[i],);
   GLO[i]=CALC_G(RESULTAT=UTOT2,
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[i],
                          R_SUP=RS[i],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),);

   IMPR_TABLE(TABLE=GLO[i],);

# SOLUTION ANALYTIQUE (chaboche p.53)
coef=(pi*a)/(2.*b)
fab=sqrt(tan(coef)/coef)*(0.752+0.37*(1.-sin(coef))**3+2.02*a/b)/cos(coef)
K1REF=-PRES*sqrt(pi*a)*fab
GREF = (1-nu**2)/E*K1REF**2

if a==2 :
   prec = 0.003
else :
   prec=0.01

for i in range(0,nbc) :
   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1',
              TYPE_TEST='MAX',
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=prec,
              REFERENCE='ANALYTIQUE',);

   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1',
              TYPE_TEST='MIN',
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=prec,
              REFERENCE='ANALYTIQUE',);
              
   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='G_IRWIN',
              TYPE_TEST='MAX',
              VALE=GREF,
              CRITERE='RELATIF',
              PRECISION=2.5*prec,
              REFERENCE='ANALYTIQUE',);

   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='G_IRWIN',
              TYPE_TEST='MIN',
              VALE=GREF,
              CRITERE='RELATIF',
              PRECISION=2.5*prec,
              REFERENCE='ANALYTIQUE',);

GLeg=CALC_G(RESULTAT=UTOT2,
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[0],
                          R_SUP=RS[0],),
                 LISSAGE=_F(DEGRE =1,),);

IMPR_TABLE(TABLE=GLeg,);   
 
TEST_TABLE(TABLE=GLeg,
           NOM_PARA='G',
           TYPE_TEST='MAX',
           VALE=GREF,
           CRITERE='RELATIF',
           PRECISION=0.02,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=GLeg,
           NOM_PARA='G',
           TYPE_TEST='MIN',
           VALE=GREF,
           CRITERE='RELATIF',
           PRECISION=0.02,
           REFERENCE='ANALYTIQUE',);
           
GLagr=CALC_G(RESULTAT=UTOT2,
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[0],
                          R_SUP=RS[0],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),);

IMPR_TABLE(TABLE=GLagr,);         

TEST_TABLE(TABLE=GLagr,
           NOM_PARA='G',
           TYPE_TEST='MAX',
           VALE=GREF,
           CRITERE='RELATIF',
           PRECISION=0.02,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=GLagr,
           NOM_PARA='G',
           TYPE_TEST='MIN',
           VALE=GREF,
           CRITERE='RELATIF',
           PRECISION=0.02,
           REFERENCE='ANALYTIQUE',);
#------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS AVEC POST_K1_K2_K3
#------------------------------------------------------------------------

PK=POST_K1_K2_K3(MODELISATION='3D',
                     MATER=ACIER,
                     FISSURE = FISS,
                     RESULTAT  = UTOT2,
                     ABSC_CURV_MAXI = 1.5,
                     NB_NOEUD_COUPE = 6,
                    )

IMPR_TABLE(TABLE = PK)

TEST_TABLE( TABLE=PK,
            NOM_PARA='K1',
            FILTRE=(_F( NOM_PARA = 'PT_FOND',  
                        VALE_I = 1,  ),
                    _F( NOM_PARA = 'INST',  
                        VALE = 0.,  ),),
            VALE=K1REF,
            PRECISION=0.05,
            REFERENCE='ANALYTIQUE' )


#------------------------------------------------------------------------
# POST-TRAITEMENT : MAILLAGE FISSURE
#------------------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

TABDEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                               GROUP_NO='VOL',
                               RESULTAT=UTOT2,
                               NOM_CHAM='DEPL',
                               NUME_ORDRE=1,
                               NOM_CMP='DX',
                               OPERATION='EXTRACTION',),);

# TEST DE NON REGRESSION PAR RAPPORT A LA VERSION 8.2.14

DXREF=4.7430e+03
DYREF=4.74300e+04
DZREF=1.42289996944e+05
PREC=1.e-10

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_X',
           TYPE_TEST='SOMM_ABS',
           VALE=DXREF,
           CRITERE='RELATIF',
           PRECISION=PREC,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '8.2.14'),

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_Y',
           TYPE_TEST='SOMM_ABS',
           VALE=DYREF,
           CRITERE='RELATIF',
           PRECISION=PREC,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '8.2.14'),

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_Z',
           TYPE_TEST='SOMM_ABS',
           VALE=DZREF,
           CRITERE='RELATIF',
           PRECISION=PREC,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '8.2.14'),


#------------------------------------------------------------------------
# POST-TRAITEMENT : DELACEMENTS POUR LE MAILLAGE FISSURE
#------------------------------------------------------------------------

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT2,
                        
                        INFO          = 2)

DEFI_FICHIER(UNITE=82);

IMPR_RESU(FORMAT='GMSH',UNITE=82,RESU=_F(RESULTAT=RES_XFEM));


TEST_RESU(RESU=(_F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DX',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 5.9178358450956E-04,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '9.0.21'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DY',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 1.6944743324555,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.17'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DZ',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 1.2530880070065,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.17'),
            ),)

FIN();
