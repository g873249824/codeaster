DEBUT( CODE=_F( NOM = 'SSLP311B',NIV_PUB_WEB='INTERNET'))

#
PRE_GIBI()

#
MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),  )

MO=AFFE_MODELE(  MAILLAGE=MA, 
      AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = 'C_PLAN'))

#
MAT1=DEFI_MATERIAU(ELAS=_F(  E = 20.E11, NU = 0.3, RHO = 0., ALPHA = 0.))

MAT2=DEFI_MATERIAU(ELAS=_F(  E = 2.E11,  NU = 0.3, RHO = 0., ALPHA = 0.))

#
CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,AFFE=(
                           _F(  GROUP_MA = 'VTOT1',  MATER = MAT1),
                           _F(  GROUP_MA = 'LP3P4',  MATER = MAT1),
                           _F(  GROUP_MA = 'LP2P3',  MATER = MAT1),
                           _F(  GROUP_MA = 'VTOT2',  MATER = MAT2),
                           _F(  GROUP_MA = 'LP7P8',  MATER = MAT2),
                           _F(  GROUP_MA = 'LP6P7',  MATER = MAT2))
                       )

#
#
MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA, 
                     CREA_GROUP_NO=_F( 
                         GROUP_MA = ( 'LP3P4',  'LP2P3',  
                                      'LP6P7',  'LP7P8',  ))
                 )

#
#
CH=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=( 
            _F(  GROUP_NO = 'LP6P7',   DY = 0.),
                     _F(  GROUP_NO = 'LP7P8',   DY = 0.),
                     _F(  GROUP_NO = 'P6',      DX = 0.)),FORCE_CONTOUR=(
            
                     _F(  GROUP_MA = 'LP3P4',  FY = 100.E6), 
                     _F(  GROUP_MA = 'LP2P3',  FY = 100.E6))

                          )

#
CHAMDEPL=MECA_STATIQUE(   MODELE=MO,   CHAM_MATER=CHMAT, 
                              EXCIT=_F( CHARGE = CH)
                          )

#
DEP=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=CHAMDEPL,
            NUME_ORDRE=1 )

#
# POINT DROIT
#
FOND1=DEFI_FOND_FISS(    MAILLAGE=MA,
                            FOND_FISS=_F( GROUP_NO = ('P0',)),
                            NORMALE=(-0.5, 0.866025404, 0.,)
                      )

#
THETA01A=CALC_THETA(    MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 0.,
                                     R_SUP = 0.001875E-2),
                          DIRECTION=(0.866025404, 0.5, 0.,)
                      )

#
G01A=CALC_G_THETA_T(    MODELE=MO,
                            DEPL=DEP,
                           THETA=THETA01A,
                          EXCIT=_F(CHARGE=CH,),
                          CHAM_MATER=CHMAT,
                          SYME_CHAR='SANS'
                      )

#

TEST_TABLE(       TABLE=G01A,
                       NOM_PARA='G',
                           VALE=8.484177E1,
                      PRECISION=0.05   )

#
THETA01B=CALC_THETA(    MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 0.001875E-2,
                                     R_SUP = 0.003750E-2),
                          DIRECTION=(0.866025404, 0.5, 0.,)
                      )

#
