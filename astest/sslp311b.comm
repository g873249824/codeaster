                            DEPL=DEP,
                           THETA=THETA02B,
                          EXCIT=_F(CHARGE=CH,),
                          CHAM_MATER=CHMAT,
                          SYME_CHAR='SANS'
                     )

#

TEST_TABLE(       TABLE=G02B,
                       NOM_PARA='G',
                           VALE=8.001707E1,
                      PRECISION=0.005   )

