                       NOM_PARA='G',
                           VALE=8.484177E1,
                      PRECISION=0.005   )

#
#
# POINT GAUCHE
#
FOND2=DEFI_FOND_FISS(    MAILLAGE=MA,
                            FOND_FISS=_F( GROUP_NO = ('P0G',)),
                            NORMALE=(0.5, -0.866025404, 0.,)
                       )

#
THETA02A=CALC_THETA(    MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0G',),
                                     MODULE = 1.,
                                     R_INF = 0.,
                                     R_SUP = 0.001875E-2),
                          DIRECTION=(-0.866025404, -0.5, 0.,)
                      )

#
