                            DEPL=DEP,
                           THETA=THETA02A,
                          EXCIT=_F(CHARGE=CH,),
                          CHAM_MATER=CHMAT,
                          SYME_CHAR='SANS'
                      )

#

TEST_TABLE(       TABLE=G02A,
                       NOM_PARA='G',
                           VALE=8.001707E1,
                      PRECISION=0.05    )

#
#
THETA02B=CALC_THETA(    MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0G',),
                                     MODULE = 1.,
                                     R_INF = 0.001875E-2,
                                     R_SUP = 0.003750E-2),
                          DIRECTION=(-0.866025404, -0.5, 0.,)
                      )

