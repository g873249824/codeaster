                          THETA_2D=_F(  GROUP_NO = ('P0G',),
                                     MODULE = 1.,
                                     R_INF = 0.003750E-2,
                                     R_SUP = 0.005625E-2),
                          DIRECTION=(-0.866025404, -0.5, 0.,)
                      )

#
G02C=CALC_G_THETA_T(    MODELE=MO,
                            DEPL=DEP,
                           THETA=THETA02C,
                          EXCIT=_F(CHARGE=CH,),
                          CHAM_MATER=CHMAT,
                          SYME_CHAR='SANS'
                      )

#

TEST_TABLE(       TABLE=G02C,
                       NOM_PARA='G',
                           VALE=8.001707E1,
                      PRECISION=0.005   )

#
THETA02D=CALC_THETA(    MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0G',),
                                     MODULE = 1.,
                                     R_INF = 0.005625E-2,
                                     R_SUP = 0.007500E-2),
                          DIRECTION=(-0.866025404, -0.5, 0.,)
                      )

#
G02D=CALC_G_THETA_T(    MODELE=MO,
                            DEPL=DEP,
                           THETA=THETA02D,
                          EXCIT=_F(CHARGE=CH,),
                          CHAM_MATER=CHMAT,
                          SYME_CHAR='SANS'
                      )

#

TEST_TABLE(       TABLE=G02D,
                       NOM_PARA='G',
                           VALE=8.001707E1,
                      PRECISION=0.005   )

FIN()
