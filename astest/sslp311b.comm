                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 0.003750E-2,
                                     R_SUP = 0.005625E-2),
                          DIRECTION=(0.866025404, 0.5, 0.,)
