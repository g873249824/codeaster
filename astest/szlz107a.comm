           NOM_PARA=('AMPCIS','PHYDRM'),
           VALE=''' (AMPCIS + 0.22*(max(PHYDRM,0)) ) - 352 '''
           )

WHOL_F =   FORMULE(
           NOM_PARA=('NBRUP'),
           VALE=''' 4098.3*(NBRUP**(-0.2693)) '''
