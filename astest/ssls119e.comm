DEBUT(CODE=_F(NOM='SSLS119E',NIV_PUB_WEB='INTERNET'),);

PRE_GIBI();

MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(OPTION='NOEUD_ORDO',
                                 GROUP_MA='K3',
                                 NOM='K3',
                                 GROUP_NO_EXTR='CC',
                                 GROUP_NO_ORIG='C',),);

MO=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),
               MAILLAGE=MAIL,);

CAR_ELE=AFFE_CARA_ELEM(MODELE=MO,
                        MASSIF=_F(GROUP_MA='TOUT_ELT',
                                  ANGL_REP=(0.,0.),),);

MAT=DEFI_MATERIAU(ELAS=_F(NU=0.35,
                          E=22752510.,
                        RHO = 8000.),);

CHAM_MAT=AFFE_MATERIAU(AFFE=_F(MATER=MAT,
                               TOUT='OUI',),
                       MAILLAGE=MAIL,);

CHA=AFFE_CHAR_MECA(FACE_IMPO=_F(GROUP_MA='ENCASTR',
                               DZ=0.0,
                               DX=0.0,
                               DY=0.0,),
                   MODELE=MO,
                   FORCE_FACE=_F(GROUP_MA='LIBRE',
                                  FZ= 172.4307,),);

RESU=MECA_STATIQUE(CHAM_MATER=CHAM_MAT,
                   MODELE=MO,
                   CARA_ELEM=CAR_ELE,
                   EXCIT=_F(CHARGE=CHA,),);

RESU=CALC_ELEM(reuse =RESU,
               NIVE_COUCHE='MOY',
               RESULTAT=RESU,
               NUME_COUCHE=1,
               OPTION='SIGM_ELNO_DEPL',
               TOUT_ORDRE='OUI',);

DEP_Z=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                              INTITULE='TAB_NORM',
                              NOM_CMP='DZ',
                              RESULTAT=RESU,
                              NOM_CHAM='DEPL',
                              GROUP_NO='K3',),);

SIGMA=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                              INTITULE='TAB_SIGM ',
                              TOUT_CMP='OUI',
                              GROUP_NO='K3',
                              RESULTAT=RESU,
                              NOM_CHAM='SIGM_ELNO_DEPL',),);

IMPR_RESU(RESU=_F(MAILLAGE=MAIL,
                  FORMAT='CASTEM',
                  RESULTAT=RESU,
                  NOM_CHAM=('DEPL','SIGM_ELNO_DEPL'),),
          MODELE=MO,);

IMPR_RESU(RESU=_F(FORMAT='RESULTAT',
                  RESULTAT=RESU,
                  FICHIER='RESULTAT',
                  NOM_CHAM='DEPL',
                  GROUP_NO='K3',
                  NOM_CMP='DZ',),);

TEST_RESU(RESU=(
     _F( RESULTAT = RESU,         NOM_CHAM = 'DEPL',
           NUME_ORDRE = 1,             GROUP_NO = 'C',
           NOM_CMP = 'DZ',          VALE = 1.33518E-01,
           REFERENCE='NON_REGRESSION',
           PRECISION = 0.00001,          CRITERE = 'RELATIF'),
     _F( RESULTAT = RESU,         NOM_CHAM = 'DEPL',
           NUME_ORDRE = 1,             GROUP_NO = 'CC',
           NOM_CMP = 'DZ',          VALE = 1.34928E-01,
           REFERENCE='NON_REGRESSION',
           PRECISION = 0.00001,          CRITERE = 'RELATIF'),),)

FIN();
