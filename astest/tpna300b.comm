#
# DEFINITION DU MATERIAU ------------------------
#

CONDUC=DEFI_FONCTION(   NOM_PARA='TEMP',  NOM_RESU='LAMBDA',
                                VALE=(    0.0,      21.461,
                                        100.0,      44.861,),
                          PROL_DROITE='LINEAIRE',
