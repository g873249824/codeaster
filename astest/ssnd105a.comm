DEBUT(CODE=_F(NOM='SSND105A',NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

CUBE=LIRE_MAILLAGE();

TROISD=AFFE_MODELE(MAILLAGE=CUBE,
                   AFFE=_F(MAILLE='MA1',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);

R_0=35.
Q_0=140.
Q_M=460.
MU=19.
b=12.
ETA=0.5

ACIER=DEFI_MATERIAU(ELAS=_F(E=145000.,
                            NU=0.3,
                            ALPHA=0.,),

                    CIN2_CHAB=_F(R_0=R_0,
                                 B=b,
                                 R_I=R_0+Q_0,
                                 C1_I=0.,
                                 C2_I=0.,
                                 G1_0=0.,
                                 G2_0=0.,
                                 ),

                    MEMO_ECRO=_F(
                                   MU = MU,
                                   Q_M = Q_M,
                                   Q_0 = Q_0,
                                   ETA = ETA,
                                 ),
                               );

MAT=AFFE_MATERIAU(MAILLAGE=CUBE,
                  AFFE=_F(MAILLE='MA1',
                          MATER=ACIER,),);

T1=8.
T2=16.
T3=24.

TFIN=T3

LINST=DEFI_LIST_REEL(DEBUT=0.,
                     INTERVALLE=(_F(JUSQU_A=4.,
                                    NOMBRE=60,),
                                 _F(JUSQU_A=T1,
                                    NOMBRE=120,),
                                 _F(JUSQU_A=T2,
                                    NOMBRE=2,),
                                 _F(JUSQU_A=T3,
                                    NOMBRE=120,),
                                    ),);

LARCH=DEFI_LIST_REEL(DEBUT=0.,
                     INTERVALLE=(_F(JUSQU_A=4.,
                                    NOMBRE=6,),
                                 _F(JUSQU_A=T1,
                                    NOMBRE=12,),
                                 _F(JUSQU_A=T2,
                                    NOMBRE=2,),
                                 _F(JUSQU_A=T3,
                                    NOMBRE=12,),
                                    ),);

COND_LIM=AFFE_CHAR_MECA(MODELE=TROISD,
                        DDL_IMPO=(_F(NOEUD='N6',
                                     DY=0.,
                                     DZ=0.,),
                                  _F(NOEUD='N2',
                                     DY=0.,),
                                  _F(GROUP_NO='FACE1YZ',
                                     DX=0.,),),);

TRAC=AFFE_CHAR_MECA(MODELE=TROISD,
                    FORCE_NODALE=_F(GROUP_NO='FACEYZ',
                                    FX=-.25,),);


COEF_TR=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                            T1 ,120.,
                            T2 ,0.,
                            T3 ,100.,
                            ),
                            PROL_DROITE='LINEAIRE');

VISCMEMO=STAT_NON_LINE(MODELE=TROISD,
                     CHAM_MATER=MAT,
                     EXCIT=(_F(CHARGE=COND_LIM,),
                            _F(CHARGE=TRAC,
                               FONC_MULT=COEF_TR,),
                               ),
                     COMP_INCR=_F(RELATION='VMIS_CIN2_MEMO',
                                  ),
                     INCREMENT=_F(LIST_INST=LINST,INST_FIN=TFIN,
                     ),
                     NEWTON=_F(PREDICTION='ELASTIQUE',
                     REAC_ITER=1,
                     ),
                     RECH_LINEAIRE=_F(ITER_LINE_MAXI=5),
                     CONVERGENCE=_F(RESI_GLOB_RELA=1e-06,
                                    ITER_GLOB_MAXI=40,),
                     ARCHIVAGE=_F(LIST_INST=LINST,),
                     SUIVI_DDL=(_F(NOM_CMP    ='V1',
                                 NOM_CHAM   ='VARI_ELGA',
                                 TOUT       = 'OUI',
                                 EVAL_ELGA  ='MAX',
                                 EVAL_CHAM ='MAX',),
                              _F(NOM_CMP    ='V2',
                                 NOM_CHAM   ='VARI_ELGA',
                                 TOUT       = 'OUI',
                                 EVAL_ELGA  ='MAX',
                                 EVAL_CHAM ='MAX',),
                              _F(NOM_CMP    ='V16',
                                 NOM_CHAM   ='VARI_ELGA',
                                 TOUT       = 'OUI',
                                 EVAL_ELGA  ='MAX',
                                 EVAL_CHAM ='MAX',),
                              _F(NOM_CMP    ='V23',
                                 NOM_CHAM   ='VARI_ELGA',
                                 TOUT       = 'OUI',
                                 EVAL_ELGA  ='MAX',
                                 EVAL_CHAM ='MAX',),
                                 ),
                   );

VISCMEMO=CALC_CHAMP(reuse=VISCMEMO,RESULTAT=VISCMEMO,DEFORMATION=('EPSP_ELGA'))



FQ=RECU_FONCTION(RESULTAT=VISCMEMO,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V16',
                   MAILLE='MA1',POINT=1);

FP=RECU_FONCTION(RESULTAT=VISCMEMO,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   MAILLE='MA1',POINT=1);

FEPXX=RECU_FONCTION(RESULTAT=VISCMEMO,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V23',
                   MAILLE='MA1',POINT=1);

FKSIXX=RECU_FONCTION(RESULTAT=VISCMEMO,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V17',
                   MAILLE='MA1',POINT=1);


IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=84,
             # PILOTE='INTERACTIF',
              COURBE=(
              _F(FONCTION=FQ,LEGENDE='Q'),
              _F(FONCTION=FP,LEGENDE='P'),
              _F(FONCTION=FEPXX,LEGENDE='EPXX'),
              _F(FONCTION=FKSIXX,LEGENDE='KSIXX'),
              ),
              TITRE='VISC_CIN2_MEMO, traction-compression',
              LEGENDE_X='temps',
              LEGENDE_Y='variables internes',);

#IMPR_RESU(RESU=_F(RESULTAT=VISCMEMO,INST=(T1,T2,T3),),);

p=3.70925E-02

TEST_RESU(RESU=_F(INST=8.0,
                  POINT=1,
                  RESULTAT=VISCMEMO,
                  NOM_CHAM='VARI_ELGA',
                  NOM_CMP='V1',
                  VALE_CALC=0.0370925,
                  TOLE_MACHINE=1.E-3,
                  MAILLE='MA1',),
          )

# p=FP(T1)
Rp=b*(Q_0-Q_M)/(b-2.*MU*ETA)*exp(-2*MU*ETA*p)
Rp=Rp+(2*MU*ETA*Q_M-b*Q_0)/(b-2.*MU*ETA)*exp(-b*p)
Rp=Q_M+Rp
Sig=R_0+Rp
#
TEST_RESU(RESU=_F(INST=8.0,
                  REFERENCE='ANALYTIQUE',
                  POINT=1,
                  RESULTAT=VISCMEMO,
                  NOM_CHAM='SIEF_ELGA',
                  NOM_CMP='SIXX',
                  VALE_CALC=119.999978319,
                  VALE_REFE=119.87362040218926,
                  PRECISION=2.E-3,
                  MAILLE='MA1',),
          )

FIN();
