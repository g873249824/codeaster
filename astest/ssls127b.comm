CARA_EL1=AFFE_CARA_ELEM(MODELE=MOD,
                        COQUE=_F(GROUP_MA='MESH',
                                 EPAIS=EP,
                                 ANGL_REP=(33.0,12.0,),),);

RESU1=STAT_NON_LINE(MODELE=MOD,
