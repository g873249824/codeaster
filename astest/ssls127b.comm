                 RESULTAT=RESU1,
                 CONTRAINTE=('EFGE_ELNO','EFGE_ELGA',),
                 DEFORMATION=('DEGE_ELNO','DEGE_ELGA',),
                 VARI_INTERNE='VARI_ELNO',);

