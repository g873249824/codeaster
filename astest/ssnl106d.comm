# AVEC ECRO-LINE OU UN MOMENT EN PLASTICITE PARFAITE
# MODELISATION TUYAU.

DEBUT(CODE=_F(NOM='SSNL106D',NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

TITRE='SSNL106 TUYAU NCOU=3 NSEC=16';

COMMENT='R=0.1 EP=0.001 ';
EP = 0.001;

REXT = 0.1;

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_MA=_F(NOM='TOUT',
                               TOUT='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='TUYAU_3M',),);

CAREL=AFFE_CARA_ELEM(MODELE=MO,
                     POUTRE=_F(GROUP_MA='TOUT',
                               SECTION='CERCLE',
                               CARA=('R','EP',),
                               VALE=(REXT,EP,),),
                     ORIENTATION=_F(GROUP_NO='A',
                                    CARA='GENE_TUYAU',
                                    VALE=(0.0,0.0,1.0,),),);

MATC2=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=2000000000.0,
                                 SY=150000000.0,),);

CHMAT1=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(TOUT='OUI',
                             MATER=MATC2,),);

CHART=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='A',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),
                               _F(GROUP_NO='B',
                                  DX=0.00075,),),);

FONCT=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,10.0,10.0,),);

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=1.0,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=2.0,
                                    NOMBRE=10,),
                                 _F(JUSQU_A=3.0,
                                    NOMBRE=10,),),);

REST=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT1,
                   CARA_ELEM=CAREL,
                   EXCIT=_F(CHARGE=CHART,
                            FONC_MULT=FONCT,),
                   COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',
                                RESI_INTE_RELA=1e-06,
                                ITER_INTE_MAXI=50,),
                   INCREMENT=_F(LIST_INST=LINST,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=3,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=0.0001,
                                  ITER_GLOB_MAXI=20,),);

REST=CALC_CHAMP(reuse=REST,
                RESULTAT=REST,
                CONTRAINTE=('EFGE_ELNO','SIGM_ELGA','SIGM_ELNO'),
                VARI_INTERNE=('VARI_ELNO'),
                DEFORMATION=('EPSI_ELGA','EPSI_ELNO','DEGE_ELNO'),
                CRITERES=('SIEQ_ELGA','EPEQ_ELGA'),);


REST=CALC_CHAMP(reuse =REST,
             RESULTAT=REST,
             FORCE='REAC_NODA',);

REST1=POST_CHAMP(RESULTAT=REST,
                 TOUT_ORDRE='OUI',
                 EXTR_TUYAU=_F(NOM_CHAM=('SIGM_ELNO','EPSI_ELNO'),
                               NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',
                               ANGLE=90,),);
REST2=POST_CHAMP(RESULTAT=REST,
                 TOUT_ORDRE='OUI',
                 EXTR_TUYAU=_F(NOM_CHAM='VARI_ELNO',
                               NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',
                               ANGLE=360,),);

REST3=POST_CHAMP(RESULTAT=REST,
                 TOUT_ORDRE='OUI',
                 EXTR_TUYAU=_F(NOM_CHAM=('SIGM_ELGA','EPSI_ELGA','SIEQ_ELGA','EPEQ_ELGA'),
                               NUME_COUCHE=1,
                               NIVE_COUCHE='INF',
                               ANGLE=0,),);

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   RESULTAT=REST1,
                   NOM_CHAM='EPSI_ELNO',
                   NOEUD='N1',
                   NOM_CMP='EPYY',
                   VALE_CALC=-2.25E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=1,
                   POINT=1,
                   SOUS_POINT=1,
                   RESULTAT=REST3,
                   NOM_CHAM='EPEQ_ELGA',
                   NOM_CMP='INVA_2',
                   VALE_CALC=5.8949100000000004E-4,
                   MAILLE='M1',),
                _F(NUME_ORDRE=1,
                   POINT=1,
                   SOUS_POINT=1,
                   RESULTAT=REST3,
                   NOM_CHAM='SIEQ_ELGA',
                   NOM_CMP='VMIS',
                   VALE_CALC=1.5E8,
                   MAILLE='M1',),
                _F(NUME_ORDRE=11,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=REST1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXX',
                   VALE_CALC=1.51500004E+08,
                   VALE_REFE=1.5E8,
                   PRECISION=0.012,
                   MAILLE='M2',),
                _F(NUME_ORDRE=11,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=REST2,
                   NOM_CHAM='VARI_ELNO',
                   NOM_CMP='V1',
                   VALE_CALC= 7.42501769E-04,
                   VALE_REFE=7.425E-4,
                   PRECISION=1.E-3,
                   MAILLE='M2',),
                _F(NUME_ORDRE=11,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=REST2,
                   NOM_CHAM='VARI_ELNO',
                   NOEUD='N5',
                   NOM_CMP='V1',
                   VALE_CALC=7.42499069E-04,
                   VALE_REFE=7.425E-4,
                   PRECISION=1.E-3,
                   MAILLE='M2',),
                _F(NUME_ORDRE=11,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=REST2,
                   NOM_CHAM='VARI_ELNO',
                   NOM_CMP='V2',
                   VALE_CALC=1.0,
                   VALE_REFE=1.0,
                   PRECISION=1.E-3,
                   MAILLE='M2',),
                _F(NUME_ORDRE=11,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=REST,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   VALE_CALC=9.47143082E+04,
                   VALE_REFE=9.47152E4,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=21,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=REST,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   VALE_CALC=9.56520736E+04,
                   VALE_REFE=9.5653E4,
                   PRECISION=1.E-3,),
                _F(NUME_ORDRE=21,
                   GROUP_NO='B',
                   RESULTAT=REST,
                   NOM_CHAM='DEGE_ELNO',
                   NOM_CMP='EPXX',
                   VALE_CALC=2.25000170E-03,
                   MAILLE='M2',),
                ),
          )

TAB1=POST_RELEVE_T(ACTION=_F(INTITULE='POST',
                             GROUP_NO='B',
                             RESULTAT=REST,
                             NOM_CHAM='DEGE_ELNO',
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TAB1)

MATC1=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=0.0,
                                 SY=150000000.0,),);

CHMAT2=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(TOUT='OUI',
                             MATER=MATC1,),);

CHARM=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='A',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),
                               _F(GROUP_NO=('A','B',),
                                  UO3=0.0,
                                  VO3=0.0,
                                  WO3=0.0,),
                               _F(GROUP_NO='B',
                                  DRZ=0.0075,),),);

FONCM=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,100.0,100.0,),);

LINSR=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=1.0,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=5.0,
                                    NOMBRE=20,),),);

RESM=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT2,
                   CARA_ELEM=CAREL,
                   EXCIT=_F(CHARGE=CHARM,
                            FONC_MULT=FONCM,),
                   COMPORTEMENT=_F(RELATION='VMIS_ISOT_LINE',
                                RESI_INTE_RELA=1e-06,
                                ITER_INTE_MAXI=100,),
                   INCREMENT=_F(LIST_INST=LINSR,),
                   NEWTON=_F(REAC_ITER=5,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=0.0001,
                                  ITER_GLOB_MAXI=1000),);

RESM=CALC_CHAMP(reuse =RESM,
             RESULTAT=RESM,
             FORCE='REAC_NODA',);

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESM,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DRZ',
                   VALE_CALC=4.64205985E+03,
                   VALE_REFE=4642.17,),
                _F(NUME_ORDRE=21,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESM,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DRZ',
                   VALE_CALC=5.93648157E+03,
                   VALE_REFE=5910.6,
                   PRECISION=5.0E-3,),
                ),
          )

RESM=CALC_CHAMP(reuse=RESM,
                RESULTAT=RESM,
                CONTRAINTE=('SIGM_ELNO'),
                DEFORMATION=('DEGE_ELNO'),);

RESM1=POST_CHAMP(RESULTAT=RESM,
                 TOUT_ORDRE='OUI',
                 EXTR_TUYAU=_F(NOM_CHAM='SIGM_ELNO',
                               NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',
                               ANGLE=90,),);

RESM2=POST_CHAMP(RESULTAT=RESM,
                 TOUT_ORDRE='OUI',
                 EXTR_TUYAU=_F(NOM_CHAM='SIGM_ELNO',
                               NUME_COUCHE=1,
                               NIVE_COUCHE='INF',
                               ANGLE=90,),);
#
# POUR VALIDER L'IMPRESSION DES VARIABLES INTERNES:
IMPR_RESU(RESU=_F(RESULTAT=REST,
                  NOM_CHAM='VARI_ELNO',
                  INST=1.1,
                  MAILLE='M1',),);

# SOLUTION ANALYTIQUE EVIDENTE : LA COURBURE EST CONSTANTE, CAR LA ROTATION
# VARIE LINEAIREMENT (LE MOMENT EST CONSTANT)

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='B',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESM,
                  NOM_CHAM='DEGE_ELNO',
                  NOM_CMP='KZ',
                  VALE_CALC=-7.5E-3,
                  VALE_REFE=-7.5E-3,
                  PRECISION=1.E-3,
                  MAILLE='M2',),
          )

TAB2=POST_RELEVE_T(ACTION=_F(INTITULE='POST',
                             GROUP_NO='B',
                             RESULTAT=RESM,
                             NOM_CHAM='DEGE_ELNO',
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TAB2)


FIN();
