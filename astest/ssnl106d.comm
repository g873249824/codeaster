# AVEC ECRO-LINE OU UN MOMENT EN PLASTICITE PARFAITE
# MODELISATION TUYAU.

DEBUT(CODE=_F(NOM='SSNL106D',
              NIV_PUB_WEB='INTERNET',),);

TITRE='SSNL106 TUYAU NCOU=3 NSEC=16';

COMMENT='R=0.1 EP=0.001 ';
EP = 0.001;

REXT = 0.1;


PRE_GIBI();

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_MA=_F(NOM='TOUT',
                               TOUT='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='TUYAU_3M',),);

CAREL=AFFE_CARA_ELEM(MODELE=MO,
                     POUTRE=_F(GROUP_MA='TOUT',
                               SECTION='CERCLE',
                               CARA=('R','EP',),
                               VALE=(REXT,EP,),),
                     ORIENTATION=_F(GROUP_NO='A',
                                    CARA='GENE_TUYAU',
                                    VALE=(0.0,0.0,1.0,),),);

MATC2=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=2000000000.0,
                                 SY=150000000.0,),);

CHMAT1=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(TOUT='OUI',
                             MATER=MATC2,),);

CHART=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='A',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),
                               _F(GROUP_NO='B',
                                  DX=0.00075,),),);

FONCT=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,10.0,10.0,),);
#

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=1.0,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=2.0,
                                    NOMBRE=10,),
                                 _F(JUSQU_A=3.0,
                                    NOMBRE=10,),),);

REST=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT1,
                   CARA_ELEM=CAREL,
                   EXCIT=_F(CHARGE=CHART,
                            FONC_MULT=FONCT,),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                                RESI_INTE_RELA=1e-06,
                                ITER_INTE_MAXI=50,),
                   INCREMENT=_F(LIST_INST=LINST,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=3,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=0.0001,
                                  ITER_GLOB_MAXI=20,),);

REST=CALC_ELEM(reuse =REST,
               RESULTAT=REST,
               OPTION=('SIEF_ELNO_ELGA','VARI_ELNO_ELGA','DEGE_ELNO_DEPL',),);

REST=CALC_NO(reuse =REST,
             RESULTAT=REST,
             OPTION='REAC_NODA',);

REST=CALC_ELEM(reuse =REST,
               RESULTAT=REST,
               TOUT_ORDRE='OUI',
               OPTION='SIGM_ELNO_TUYO',
               REPE_COQUE =_F( NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',
                               ANGLE=90,),);

REST=CALC_ELEM(reuse =REST,
               RESULTAT=REST,
               TOUT_ORDRE='OUI',
               OPTION=('EPSI_ELNO_TUYO','EPEQ_ELNO_TUYO','SIEQ_ELNO_TUYO',),
               REPE_COQUE =_F( NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',
                               ANGLE=90,),);

REST=CALC_ELEM(reuse =REST,
               RESULTAT=REST,
               TOUT_ORDRE='OUI',
               OPTION='VARI_ELNO_TUYO',
               REPE_COQUE =_F( NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',
                               ANGLE=360,),);

TEST_RESU(RESU=(
                _F(RESULTAT=REST,
                   NUME_ORDRE=1,
                   NOM_CHAM='EPSI_ELNO_TUYO',
                   NOM_CMP='EPYY',
                   MAILLE='M1',
                   NOEUD='N1',
                   VALE=-2.25000E-04,
                   PRECISION=1.0E-04,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=1,
                   NOM_CHAM='EPEQ_ELNO_TUYO',
                   NOM_CMP='INVA_2',
                   MAILLE='M1',
                   NOEUD='N1',
                   VALE=5.89491E-04,
                   PRECISION=1.0E-04,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIEQ_ELNO_TUYO',
                   NOM_CMP='VMIS',
                   MAILLE='M1',
                   NOEUD='N1',
                   VALE=0.0,
                   PRECISION=1.0E-03,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=11,
                   NOM_CHAM='SIGM_ELNO_TUYO',
                   NOM_CMP='SIXX',
                   MAILLE='M2',
                   GROUP_NO='B',
                   VALE=150000000.0,
                   PRECISION=0.012,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=11,
                   NOM_CHAM='VARI_ELNO_TUYO',
                   NOM_CMP='V1',
                   MAILLE='M2',
                   GROUP_NO='B',
                   VALE=0.0007425,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=11,
                   NOM_CHAM='VARI_ELNO_TUYO',
                   NOM_CMP='V1',
                   MAILLE='M2',
                   NOEUD='N5',
                   VALE=0.0007425,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=11,
                   NOM_CHAM='VARI_ELNO_TUYO',
                   NOM_CMP='V2',
                   MAILLE='M2',
                   GROUP_NO='B',
                   VALE=1.0,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=11,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   GROUP_NO='B',
                   VALE=94715.2,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=21,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   GROUP_NO='B',
                   VALE=95653.0,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=REST,
                   NUME_ORDRE=21,
                   NOM_CHAM='DEGE_ELNO_DEPL',
                   NOM_CMP='EPXX',
                   MAILLE='M2',
                   GROUP_NO='B',
                   VALE=0.00225,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

TAB1=POST_RELEVE_T(ACTION=_F(INTITULE='POST',
                             GROUP_NO='B',
                             RESULTAT=REST,
                             NOM_CHAM='DEGE_ELNO_DEPL',
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TAB1)







MATC1=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=0.0,
                                 SY=150000000.0,),);

CHMAT2=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(TOUT='OUI',
                             MATER=MATC1,),);

CHARM=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='A',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),
                               _F(GROUP_NO=('A','B',),
                                  UO3=0.0,
                                  VO3=0.0,
                                  WO3=0.0,),
                               _F(GROUP_NO='B',
                                  DRZ=0.0075,),),);

FONCM=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,100.0,100.0,),);

LINSR=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=1.0,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=5.0,
                                    NOMBRE=20,),),);

RESM=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT2,
                   CARA_ELEM=CAREL,
                   EXCIT=_F(CHARGE=CHARM,
                            FONC_MULT=FONCM,),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                                RESI_INTE_RELA=1e-06,
                                ITER_INTE_MAXI=100,),
                   INCREMENT=_F(LIST_INST=LINSR,),
                   NEWTON=_F(REAC_ITER=5,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=0.0001,
                                  ITER_GLOB_MAXI=1000),);

RESM=CALC_NO(reuse =RESM,
             RESULTAT=RESM,
             OPTION='REAC_NODA',);

TEST_RESU(RESU=(_F(RESULTAT=RESM,
                   NUME_ORDRE=1,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DRZ',
                   GROUP_NO='B',
                   VALE=4642.17,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RESM,
                   NUME_ORDRE=21,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DRZ',
                   GROUP_NO='B',
                   VALE=5910.6,
                   PRECISION=0.005,
                   REFERENCE='ANALYTIQUE',),),);

RESM=CALC_ELEM(reuse =RESM,
               RESULTAT=RESM,
               TOUT_ORDRE='OUI',
               OPTION=('SIGM_ELNO_TUYO','DEGE_ELNO_DEPL',),
               REPE_COQUE =_F( NUME_COUCHE=3,
                               NIVE_COUCHE='SUP',
                               ANGLE=90,),);

RESM2=CALC_ELEM(RESULTAT=RESM,
                TOUT_ORDRE='OUI',
                OPTION='SIGM_ELNO_TUYO',
                REPE_COQUE =_F( NUME_COUCHE=1,
                                NIVE_COUCHE='INF',
                                ANGLE=90,),);

#
# POUR VALIDER L'IMPRESSION DES VARIABLES INTERNES:

IMPR_RESU(RESU=_F(RESULTAT=REST,
                  NOM_CHAM='VARI_ELNO_ELGA',
                  INST=1.1,
                  MAILLE='M1',),);
                  
# SOLUTION ANALYTIQUE EVIDENTE : LA COURBURE EST CONSTANTE, CAR LA ROTATION
# VARIE LINEAIREMENT (LE MOMENT EST CONSTANT)

TEST_RESU(RESU=(_F(RESULTAT=RESM,REFERENCE='ANALYTIQUE',
                   NUME_ORDRE=1,
                   NOM_CHAM='DEGE_ELNO_DEPL',
                   NOM_CMP='KZ',
                   MAILLE='M2',
                   GROUP_NO='B',
                   VALE=-0.0075,
                   PRECISION=0.001,),),);

TAB2=POST_RELEVE_T(ACTION=_F(INTITULE='POST',
                             GROUP_NO='B',
                             RESULTAT=RESM,
                             NOM_CHAM='DEGE_ELNO_DEPL',
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TAB2)


FIN();
