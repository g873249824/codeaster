                   SCHEMA_TEMPS=_F(SCHEMA='HHT',ALPHA=-0.3, MODI_EQUI='OUI',
                                   FORMULATION='DEPLACEMENT',),);



###On extrait les champs de déplacements à partir des résultats de evol* et on stocke l'extraction dans une nouvelle structure de données
dep1=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                NOM_CHAM = 'DEPL',
                RESULTAT=evol1,
                INST=lfin2,
                OPERATION='EXTR',
                 );

dep2=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                NOM_CHAM = 'DEPL',
                RESULTAT=evol2,
                INST=lfin2,
                OPERATION='EXTR',
                 );

dep3=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                NOM_CHAM = 'DEPL',
                RESULTAT=evol3,
                INST=lfin2,
                OPERATION='EXTR',
                 );

dep4=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                NOM_CHAM = 'DEPL',
                RESULTAT=evol4,
                INST=lfin2,
                OPERATION='EXTR',
                 );

dep5=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                NOM_CHAM = 'DEPL',
                RESULTAT=evol5,
                INST=lfin2,
                OPERATION='EXTR',
                 );

### On calcule la différence des champs (depla-depb) puis on teste
dep12= CREA_CHAMP( OPERATION= 'ASSE',
                                     MODELE = modele , 
                                     TYPE_CHAM ='NOEU_DEPL_R',
                                     ASSE = (_F( CHAM_GD = dep1 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = 1.),
                                     _F( CHAM_GD = dep2 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = -1.),))

dep13= CREA_CHAMP( OPERATION= 'ASSE',
                                     MODELE = modele , 
                                     TYPE_CHAM ='NOEU_DEPL_R',
                                     ASSE = (_F( CHAM_GD = dep1 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = 1.),
                                     _F( CHAM_GD = dep3 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = -1.),) )

dep14= CREA_CHAMP( OPERATION= 'ASSE',
                                     MODELE = modele , 
                                     TYPE_CHAM ='NOEU_DEPL_R',
                                     ASSE = (_F( CHAM_GD = dep1 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = 1.),
                                     _F( CHAM_GD = dep4 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = -1.),))

dep15= CREA_CHAMP( OPERATION= 'ASSE',
                                     MODELE = modele , 
                                     TYPE_CHAM ='NOEU_DEPL_R',
                                     ASSE = (_F( CHAM_GD = dep1 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = 1.),
                                     _F( CHAM_GD = dep5 , TOUT ='OUI',
                                     CUMUL='OUI', COEF_R = -1.),))

