   RECO_SUPER_MAILLE=(

    _F( SUPER_MAILLE = ('S_1','S_2',), GROUP_NO = ('GRNM13','GRNM11',),
          OPTION = 'GEOMETRIQUE', PRECISION = 1.0E-2, CRITERE = 'ABSOLU'),
    _F( SUPER_MAILLE = ('S_2','S_3',), GROUP_NO = ('GRNM13','GRNM11',),
          OPTION = 'GEOMETRIQUE', PRECISION = 1.0E-2, CRITERE = 'RELATIF'),
