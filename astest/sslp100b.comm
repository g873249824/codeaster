      _F( MACR_ELEM = S_1, SUPER_MAILLE = 'S_1',  TRAN = (1.0E-6,1.0E-5,)),
      _F( MACR_ELEM = S_1, SUPER_MAILLE = 'S_2', ANGL_NAUT = (45.0,), CENTRE = (1.E-6,0.,)),
