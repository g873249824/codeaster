DEBUT(CODE=_F( NOM = 'TPLL01F',NIV_PUB_WEB='INTERNET'))

PHI0=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=0.0           )

PHI1=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=-1200.0       )

COEH=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=30.0          )

T_EX=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=140.0         )

T_IM=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=100.0         )

MATERI=DEFI_MATERIAU( THER=_F(  LAMBDA = 0.75, RHO_CP = 2.0))

PRE_GIBI ( ) 

MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'), )

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL, 
                 CREA_GROUP_NO=_F( TOUT_GROUP_MA = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = MATERI) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = '3D',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(  MODELE=MOTH,
                   FLUX_REP =_F( GROUP_MA = 'G2',    FLUN = PHI1),
                   ECHANGE  =_F( GROUP_MA = 'F1SUP', COEF_H = COEH, 
                                                     TEMP_EXT = T_EX),
                   TEMP_IMPO=_F( GROUP_NO = 'F1INF', TEMP = T_IM)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,
       INTERVALLE=_F(    JUSQU_A = 10.0,   NOMBRE = 2)
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           TEMP_INIT=_F( STATIONNAIRE = 'OUI'),
          INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH)
             )

T2=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
                NOM_CHAM='TEMP', RESULTAT=TEMPE,
                   NUME_ORDRE=2)

FLUXN=CALC_CHAM_ELEM(    MODELE=MOTH,  TEMP=T2,
            CHAM_MATER=CHMAT,   OPTION='FLUX_ELNO_TEMP')

FLUXG=CALC_CHAM_ELEM(    MODELE=MOTH,  TEMP=T2,
            CHAM_MATER=CHMAT,   OPTION='FLUX_ELGA_TEMP')

TEST_RESU(CHAM_NO=(
   _F( CHAM_GD = T2, GROUP_NO = 'A',     REFERENCE = 'ANALYTIQUE',
       NOM_CMP = 'TEMP', VALE = 100.0),
   _F( CHAM_GD = T2, GROUP_NO = 'B',     REFERENCE = 'ANALYTIQUE',
       NOM_CMP = 'TEMP', VALE = 20.0),
   _F( CHAM_GD = T2, GROUP_NO = 'G',     REFERENCE = 'ANALYTIQUE',
       NOM_CMP = 'TEMP', VALE = 60.0)),

CHAM_ELEM=(   
   _F( CHAM_GD = FLUXN, MAILLE = 'M1',   REFERENCE = 'ANALYTIQUE',
       POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( CHAM_GD = FLUXN, MAILLE = 'M2',   REFERENCE = 'ANALYTIQUE',
       POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0),
   _F( CHAM_GD = FLUXG, MAILLE = 'M1',   REFERENCE = 'ANALYTIQUE',
       POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( CHAM_GD = FLUXG, MAILLE = 'M2',   REFERENCE = 'ANALYTIQUE',
       POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0))
          )

FIN()
