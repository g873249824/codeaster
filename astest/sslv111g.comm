                   BORNE_INF=10.0,
                   SOUS_TITRE=
                   ('ESTIMATEUR DERREUR EN RESIDU :LISTE DES MAILLES OU LERREUR  ',
                                'RELATIVE EST SUPERIEURE A 10 POUR CENT',),),),);

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERZ1_ELEM',
                   NOM_CMP='ERREST',
                   MAILLE='M2145',
                   POINT=1,
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=0.001,),
                _F(RESULTAT=RESU,
                   NUME_ORDRE=1,
