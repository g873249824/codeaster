                   NOM_CMP='NUEST',
                   MAILLE='M2145',
                   POINT=1,
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=17.6,),
                _F(RESULTAT=RESU,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELEM',
                   NOM_CMP='ERREST',
                   MAILLE='M2145',
                   POINT=1,
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=0.006,),
                _F(RESULTAT=RESU,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELEM',
                   NOM_CMP='NUEST',
                   MAILLE='M2145',
                   POINT=1,
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=13.5,),
                _F(RESULTAT=RESU,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='ERREST',
                   MAILLE='M2145',
                   POINT=1,
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=0.006,),
                _F(RESULTAT=RESU,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='NUEST',
                   MAILLE='M2145',
                   POINT=1,
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=13.5,),),);

FIN();

