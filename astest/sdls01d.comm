DEBUT(CODE=_F(NOM='SDLS01D',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED', )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(  MAILLAGE=MA,
                  AFFE=_F(  TOUT = 'OUI',       PHENOMENE = 'MECANIQUE',
                         MODELISATION = 'DKT') )

CARELEM=AFFE_CARA_ELEM(   MODELE=MO,
                            COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.01) )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.1E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

#--------------------------------------------------------------------
CHCAS1=AFFE_CHAR_MECA(  MODELE=MO,
           DDL_IMPO=_F(  GROUP_NO = 'GRNM11',  DX = 0.,  DY = 0., DZ = 0.,
                                        DRX = 0., DRY = 0., DRZ = 0.) )

SSS=MACR_ELEM_STAT(
             DEFINITION=_F(  MODELE = MO, CHAM_MATER = CHMAT,
                        CARA_ELEM = CARELEM,   CHAR_MACR_ELEM = CHCAS1),
              EXTERIEUR=_F(  NOEUD = ('N1', 'N7', 'N9', 'N17', 'N23', 'N25', 'N39', 'N41', 'N47', 'N49',
                        'N65', 'N71', 'N73', 'N78', 'N80', 'N99', 'N101', 'N107', 'N109', 'N121',
                        'N127', 'N129', 'N139', 'N141', 'N145',   )),
              RIGI_MECA=_F(),
              MASS_MECA=_F()
        )

MAG=DEFI_MAILLAGE(
         DEFI_SUPER_MAILLE=_F( MACR_ELEM = SSS),
         RECO_GLOBAL=_F( TOUT = 'OUI'),
         DEFI_NOEUD=_F( TOUT = 'OUI', INDEX = (1,0,1,8,))
        )

MOG=AFFE_MODELE(
          MAILLAGE=MAG,
        AFFE_SOUS_STRUC=_F( TOUT = 'OUI')
        )

#-------------------------------------------------------------------
MELR1=CALC_MATR_ELEM(  MODELE=MOG,   OPTION='RIGI_MECA' )

MELM1=CALC_MATR_ELEM(  MODELE=MOG,   OPTION='MASS_MECA' )

NUM1=NUME_DDL(  MATR_RIGI=MELR1,    METHODE='LDLT'  )

MATASSR1=ASSE_MATRICE(MATR_ELEM=MELR1,NUME_DDL=NUM1)

MATASSM1=ASSE_MATRICE(MATR_ELEM=MELM1,NUME_DDL=NUM1)

#====================================================================
#                CALCUL MODAL
MODE1=MODE_ITER_SIMULT(  MATR_RIGI=MATASSR1,      MATR_MASS=MATASSM1,
              CALC_FREQ=_F(   FREQ = ( 8.,   140., ),
                           OPTION = 'BANDE')  )


# --------------------------------------------------------------------


TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   PARA='FREQ',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MODE1,
                   VALE_CALC=8.672645637,
                   VALE_REFE=8.7265999999999995,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=2,
                   PARA='FREQ',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MODE1,
                   VALE_CALC=21.302513258,
                   VALE_REFE=21.304200000000002,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=3,
                   PARA='FREQ',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MODE1,
                   VALE_CALC=53.269968364,
                   VALE_REFE=53.554200000000002,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=4,
                   PARA='FREQ',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MODE1,
                   VALE_CALC=68.272160075,
                   VALE_REFE=68.298400000000001,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=5,
                   PARA='FREQ',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MODE1,
                   VALE_CALC=77.961335316,
                   VALE_REFE=77.744799999999998,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=6,
                   PARA='FREQ',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=MODE1,
                   VALE_CALC=138.327303374,
                   VALE_REFE=136.0471,
                   CRITERE='RELATIF',
                   PRECISION=0.017000000000000001,),
                ),
          )

