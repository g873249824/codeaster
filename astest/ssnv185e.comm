DEBUT(CODE=_F(NOM='SSNV185E',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#----------------------------------------------
#             MAILLAGE : hexa_3_11_31.mgib
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAGE=CREA_MAILLAGE(MAILLAGE=MAILLAG1,);
IMPR_RESU(FORMAT='IDEAS',UNITE=30,RESU=_F(MAILLAGE=MAILLAGE,),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(GROUP_MA='SURFSUP',),
                    );


MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                 MAILLAGE=MAILLAG2,
                 CREA_GROUP_NO=(_F(NOM='NXFEM',NOEUD=('N501','N502','N503','N504','N505','N506','N1069',
                                                      'N534','N535','N536','N537','N538','N539','N1072',

                                                      'N512','N513','N514','N515','N516','N517','N1070',
                                                      'N545','N546','N547','N548','N549','N550','N1073',

                                                      'N523','N524','N525','N526','N527','N528','N1071',
                                                      'N556','N557','N558','N559','N560','N561','N1074',

                                                      'N1286','N1287','N1288','N1289','N1290','N1291','N1292',
                                                      'N1297','N1298','N1299','N1300','N1301','N1302','N1303',),),
                                _F(NOM='NCLA',DIFFE=('VOL','NXFEM',),),
                                ),
                 );


#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_XFEM',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

MAILLAG2=MODI_MAILLAGE(reuse =MAILLAG2,
                       MAILLAGE=MAILLAG2,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFSUP','SURFINF',),),
                       );

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-15.');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-Y+5.');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI= (0. ,5. ,15. ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(0. ,20. ,15. ),),
                    );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,FISSURE=FISS,INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,LIAISON_XFEM='OUI',INFO=1,);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=100.0E6
nu=0.
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT = 'OUI',
                                MATER=ACIER,
                                TEMP_REF=0.0,),
                             );

#--------------------------------------------------------
#                   CHARGEMENT : CHAMP AUXILIAIRE MODE I
#--------------------------------------------------------

R = FORMULE(NOM_PARA=('X','Y','Z'),VALE='sqrt((Y-5.)**2+(Z-15.)**2)');
T = FORMULE(NOM_PARA=('X','Y','Z'),VALE='atan2(Z-15.,-Y+5.)');

coef=(1+nu)/(E*sqrt(2.*pi))

OUAUX1 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='-1.*coef*sqrt(R(X,Y,Z))*cos(T(X,Y,Z)/2.)*(3.-4.*nu-cos(T(X,Y,Z)))');
UAUX2 = FORMULE(NOM_PARA=('X','Y','Z'),VALE=' coef*sqrt(R(X,Y,Z))*sin(T(X,Y,Z)/2.)*(3.-4.*nu-cos(T(X,Y,Z)))');
UAUX3 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='0.');

CHF=AFFE_CHAR_MECA_F(MODELE=MODELEK,DDL_IMPO=_F(GROUP_NO='NCLA',DX=UAUX3,DY=OUAUX1,DZ=UAUX2,),);

#  CES VALEURS SONT CALCULEES POUR CHAQUE NOEUD HEAVISIDE A PARTIR DE LA FORMULE GENERALE
CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                     DDL_IMPO=(_F(NOEUD=('N501','N502','N534','N535','N512','N513','N545','N546','N523','N524','N556','N557','N1286','N1287','N1297','N1298',),
                                  DCX=0., E1X=0.,              E2X=0.,              E3X=0.,       E4X=0.,
                                  DCY=0., E1Y=0.,              E2Y=coef*(4.*nu-2.), E3Y=-1.*coef, E4Y=0.,
                                  DCZ=0., E1Z=coef*(4.-4.*nu), E2Z=0.,              E3Z=0.,       E4Z=-1.*coef,),
                               _F(NOEUD=('N536','N547','N558','N1299',),DCX=0.,DCY= 2.80089E-15,DCZ= 1.01787E-11,H1X=0.,H1Y=-3.20989E-09,H1Z=1.86346E-08,),
                               _F(NOEUD=('N503','N514','N525','N1288',),DCX=0.,DCY=-2.80089E-15,DCZ=-1.01787E-11,H1X=0.,H1Y= 3.20989E-09,H1Z=1.86346E-08,),

                               _F(NOEUD=('N537','N548','N559','N1300',),DCX=0.,DCY= 2.28933E-15,DCZ= 1.84271E-12,H1X=0.,H1Y=-2.53273E-09,H1Z=2.40571E-08,),
                               _F(NOEUD=('N504','N515','N526','N1289',),DCX=0.,DCY=-2.28933E-15,DCZ=-1.84271E-12,H1X=0.,H1Y= 2.53273E-09,H1Z=2.40571E-08,),

                               _F(NOEUD=('N538','N549','N560','N1301',),DCX=0.,DCY= 1.96578E-15,DCZ= 5.80554E-13,H1X=0.,H1Y=-2.15205E-09,H1Z=2.84648E-08,),
                               _F(NOEUD=('N505','N516','N527','N1290',),DCX=0.,DCY=-1.96578E-15,DCZ=-5.80554E-13,H1X=0.,H1Y= 2.15205E-09,H1Z=2.84648E-08,),

                               _F(NOEUD=('N539','N550','N561','N1302',),DCX=0.,DCY= 1.74526E-15,DCZ= 2.43185E-13,H1X=0.,H1Y=-1.90218E-09,H1Z=3.2276E-08,),
                               _F(NOEUD=('N506','N517','N528','N1291',),DCX=0.,DCY=-1.74526E-15,DCZ=-2.43185E-13,H1X=0.,H1Y= 1.90218E-09,H1Z=3.2276E-08,),

                               _F(NOEUD=('N1072','N1073','N1074','N1303',),DCX=0.,DCY= 1.58404E-15,DCZ= 1.2106E-13,H1X=0.,H1Y=-1.72255E-09,H1Z=3.56825E-08,),
                               _F(NOEUD=('N1069','N1070','N1071','N1292',),DCX=0.,DCY=-1.58404E-15,DCZ=-1.2106E-13,H1X=0.,H1Y= 1.72255E-09,H1Z=3.56825E-08,),
                                 ),
                     );


#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=3.0,NOMBRE=3,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,
                   EXCIT=(_F(CHARGE=CHXFEM,),
                          _F(CHARGE=CHF,),
                          _F(CHARGE=CH,),
                          ),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INS1,
                                INST_FIN=1.0,),
                   SOLVEUR=_F(METHODE='MUMPS',
                              ),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);

#------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS PAR LA METHODE ENERGETIQUE AVEC X-FEM
#------------------------------------------------------------------------

# TEST AVEC PLUSIEURS COURONNES
RI=[2. , 0.666 , 1. , 1. , 1. , 2.1]
RS=[4. , 1.666 , 2. , 3. , 4. , 3.9]

nbc=len(RI)
SIF = [None]*nbc

for i in range(0,nbc) :

   SIF[i]=CALC_G(RESULTAT=UTOT1,
                 SYME_CHAR='SANS',
                 COMP_ELAS=_F(RELATION='ELAS',),
                 OPTION='CALC_K_G',
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[i],
                          R_SUP=RS[i],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),
                 INFO=2,);

   IMPR_TABLE(TABLE=SIF[i],);

# SOLUTION ANALYTIQUE
K1REF=1.

for i in range(0,nbc) :
   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MAX',
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=0.03,
              REFERENCE='ANALYTIQUE',);

   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MIN',
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=0.03,
              REFERENCE='ANALYTIQUE',);

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'),
                        INFO          = 2)

FIN();
