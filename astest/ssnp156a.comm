DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))

MESH=LIRE_MAILLAGE(FORMAT='MED',);

MESH=MODI_MAILLAGE(reuse =MESH,
                   MAILLAGE=MESH,
                   ORIE_PEAU_2D=(_F(GROUP_MA='Master',),
                                 _F(GROUP_MA='Slave',),),);

MAT_pi=DEFI_MATERIAU(ELAS=_F(E=210000.0,
                             NU=0.3,),);

MAT_sh=DEFI_MATERIAU(ELAS=_F(E=70000.0,
                             NU=0.3,),);

FONC=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                         1.0,1.0,
                         ),PROL_DROITE='EXCLU',PROL_GAUCHE='EXCLU',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

LINST=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                  LIST_INST=L_INST,),
                     ECHEC=_F(SUBD_PAS=4,
                              SUBD_NIVEAU=3,),);

MO=AFFE_MODELE(MAILLAGE=MESH,
               AFFE=_F(GROUP_MA=('Sheet','Pin',),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN',),);

MA=AFFE_MATERIAU(MAILLAGE=MESH,
                 AFFE=(_F(GROUP_MA='Sheet',
                          MATER=MAT_sh,),
                       _F(GROUP_MA='Pin',
                          MATER=MAT_pi,),),);

CL=AFFE_CHAR_CINE(MODELE=MO,
                  MECA_IMPO=(_F(GROUP_MA='Encast',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_MA='Axe',
                                DY=0.0,),),);

CH=AFFE_CHAR_MECA(MODELE=MO,
                  FORCE_NODALE=_F(GROUP_NO='F',
                                  FX=10000.0,),);

CON=DEFI_CONTACT(MODELE=MO,
                FORMULATION    = 'CONTINUE',
                FROTTEMENT     = 'COULOMB',
                    ALGO_RESO_CONT = 'NEWTON',
                    ALGO_RESO_GEOM = 'NEWTON',
                    ALGO_RESO_FROT = 'NEWTON',
                    #ADAPT_COEF     = 'OUI',
                ZONE=_F(GROUP_MA_MAIT='Master',
                        GROUP_MA_ESCL='Slave',
                        CONTACT_INIT='INTERPENETRE',
                        COULOMB=0.1,
                        SANS_GROUP_NO_FR=('Pi','Pf',),
                        #INTEGRATION='GAUSS',
                        ),);

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=MA,
                   EXCIT=(_F(CHARGE=CL,),
                          _F(CHARGE=CH,
                             FONC_MULT=FONC,),),
                   CONTACT=CON,
                   COMPORTEMENT=_F(RELATION='ELAS',
                                DEFORMATION='GROT_GDEP',),
                   INCREMENT=_F(LIST_INST=LINST,),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=50,
                                  ),
                   SOLVEUR=_F(),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=1,),);

FIN()
