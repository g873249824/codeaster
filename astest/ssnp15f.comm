MA=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MA,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=195000.,
