                EXCIT=(_F(CHARGE=LIAISON,),
                       _F(CHARGE=TRACTION,
                          FONC_MULT=SIGMA_F,),
                       _F(CHARGE=CISAIL,
                          FONC_MULT=TAU_F,),),
