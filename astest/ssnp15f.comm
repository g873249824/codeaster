                                    DX=0.,
                                    DY=0.,),
                                 _F(NOEUD='NO8',
                                    DX=0.,
                                    DY=0.,
                                    DZ=0.,),
                                 _F(NOEUD='NO2',
                                    DX=0.,),
                                 _F(NOEUD='NO6',
