                0.0,    0.0,
                1.0,   93.1,
                2.0,   33.1,
                3.0,    0.0,
                )
              )

TRACTION=AFFE_CHAR_MECA(

                      MODELE=MO,

                FORCE_FACE=_F(
                MAILLE = 'GAUCHE',
                FX = -1.)
              )

CISAIL=AFFE_CHAR_MECA(

                      MODELE=MO,FORCE_FACE=(

                _F( MAILLE = 'GAUCHE',          FY = -1.),
                _F( MAILLE = 'DROITE',          FY = 1.),
                _F( MAILLE = 'HAUT',    FX = 1.),
                _F( MAILLE = 'BAS',     FX = -1.))
              )

LIAISON=AFFE_CHAR_MECA(

               MODELE=MO,DDL_IMPO=(

                _F( NOEUD = 'NO4',     DX = 0.,  DY = 0.),
                _F( NOEUD = 'NO8',     DX = 0.,  DY = 0.,  DZ = 0.),
                _F( NOEUD = 'NO2',     DX = 0.),
                _F( NOEUD = 'NO6',     DX = 0.))
              )

