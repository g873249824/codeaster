DEBUT(CODE=_F( NOM = 'SSNV199F',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),PAR_LOT='OUI')


tfin = 4
nbpas = 10*tfin
nbarch = nbpas/2
c_mult_pilo=1

young = 100
nu=0.0
tenac = 0.9
sigc  = 3

dc=2*tenac/sigc

ri_gli=10
pen_lag=100

deply = 1.0
deplx = 0.0
deplz = 0.0


PRE_GMSH()

MA0  =  LIRE_MAILLAGE()

MA=CREA_MAILLAGE(MAILLAGE  = MA0, LINE_QUAD = _F(TOUT='OUI'))

MA  =  DEFI_GROUP(
  reuse  = MA,
  MAILLAGE = MA,

  CREA_GROUP_MA = (

# VOLUME DCB GAUCHE
    _F(NOM = 'DCB_1',   GROUP_MA = 'GM1015'),
# VOLUME DCB DROITE
    _F(NOM = 'DCB_2',   GROUP_MA = 'GM1016'),
# VOLUME DCB JOINT
    _F(NOM = 'DCB_J',   GROUP_MA = 'GM1020'),

# SURFACES
    _F(NOM = 'DCB_G',   GROUP_MA = 'GM1012'),
    _F(NOM = 'DCB_D',   GROUP_MA = 'GM1013'),
    _F(NOM = 'DCB2_B',   GROUP_MA = 'GM1014'),
    _F(NOM = 'JOINT_B', GROUP_MA = 'GM1019'),
    _F(NOM = 'JOINT_H', GROUP_MA = 'GM1014'),

# LIGNE BAS DE LA FACE DCB GAUCHE BAS
    _F(NOM = 'DCB_GB'  ,   GROUP_MA = 'GM1018'),
   ),

   CREA_GROUP_NO = (
    _F(NOM = 'JOINT_B', GROUP_MA = 'GM1019'),
    _F(NOM = 'JOINT_H', GROUP_MA = 'GM1014'),
    _F(NOM = 'NO7',     GROUP_MA = 'GM954'),
    _F(NOM = 'DCB_GB',  GROUP_MA = 'GM1018'),

  )
)

MA=MODI_MAILLAGE(reuse =MA,
                   MAILLAGE=MA,
                   ORIE_FISSURE=_F(GROUP_MA='DCB_J'),
                   INFO=1
                   )


MO=AFFE_MODELE(

                MAILLAGE=MA,
                AFFE= (
                _F(
                        GROUP_MA = ('DCB_1','DCB_2','DCB_G','DCB_GB'),
                        PHENOMENE = 'MECANIQUE',
                        MODELISATION = '3D'
                     ),
                _F(
                        GROUP_MA = 'DCB_J',
                        PHENOMENE = 'MECANIQUE',
                        MODELISATION = '3D_INTERFACE'
                       ),

                 )
              )


# AFFECTATION DES ANGLES NAUTIQUES : POSITION DE LA FISSURE DANS LE REPERE GLOBAL

ORIEN_FI=AFFE_CARA_ELEM(MODELE=MO,
                     MASSIF=(
                             _F(GROUP_MA='DCB_J',ANGL_REP=(-90, 0 ,-90)),
                                 ));


MAT=DEFI_MATERIAU(

     ELAS=_F(
              E  =  young,
              NU =  nu),
     RUPT_FRAG = _F(
                     GC        = tenac,
                     SIGM_C    = sigc,
                     PENA_LAGR = pen_lag,
                     RIGI_GLIS = ri_gli,
                    )
            )


CM=AFFE_MATERIAU(

               MAILLAGE=MA,
               AFFE=_F(
                       GROUP_MA = ('DCB_1','DCB_2','DCB_J'),
                       MATER = MAT),
              )


SYMETRIE = AFFE_CHAR_CINE(
  MODELE = MO,
  MECA_IMPO = (
               _F(GROUP_NO='JOINT_B',DY=0)
             ))


TRACTION = AFFE_CHAR_MECA(
 MODELE = MO,

  FACE_IMPO= (
      _F(GROUP_MA  = 'DCB_GB' , DY = deply ,
                                DX = deplx ,
                                DZ = deplz ),
         ))

L_INST=DEFI_LIST_REEL(

                     DEBUT   = -1,
     INTERVALLE=(
      _F( JUSQU_A = 0,        NOMBRE = 1 ),
      _F( JUSQU_A = tfin  ,   NOMBRE = nbpas ),

      ))

INS_ARCH=DEFI_LIST_REEL(

                     DEBUT   = -1,
     INTERVALLE=(
      _F( JUSQU_A = 0,        NOMBRE = 1 ),
      _F( JUSQU_A = tfin  ,   NOMBRE = nbarch),

      ))


FCT = DEFI_FONCTION(
  NOM_PARA = 'INST',
  VALE     = (-1,0 ,  0,0.0001,tfin,tfin  ),
                )


# LES SYSTEMES LINEAIRES SONT DIFFICILES A RESOUDRE. ILS SEMBLENT
# NUMERIQUEMENT SINGULIERS. DEVERROUILLER LA DETECTION DE SINGULARITE
# NE SUFFIT PAS. ON PARAMETRE ARTIFICIELLEMENT NPREC A UNE GRANDE VALEUR
# POUR FORCER MUMPS A RESOUDRE LES SYSTEMES SINGULIERS (A NE PAS FAIRE DANS
# UNE ETUDE STANDARD).
# LE PROCESSUS DE NEWTON CORRIGE LES EVENTUELLES DERIVES.
U=STAT_NON_LINE(
  MODELE     = MO,
  CHAM_MATER = CM,
  CARA_ELEM  = ORIEN_FI,
  EXCIT      = (
                 _F(CHARGE = SYMETRIE),
                 _F(CHARGE = TRACTION , FONC_MULT = FCT),
               ),
  COMP_INCR  = (
                _F(RELATION = 'ELAS'      ,  GROUP_MA = ('DCB_1','DCB_2')),
                _F(RELATION = 'CZM_OUV_MIX', GROUP_MA = 'DCB_J', ),
                ),
  INCREMENT   = _F(LIST_INST = L_INST, INST_FIN = 0.),
  CONVERGENCE = _F(
                   RESI_REFE_RELA = 1.E-6,
                   SIGM_REFE      = sigc,
                   DEPL_REFE      = dc,
                   ITER_GLOB_MAXI = 12,
                  ),
  SOLVEUR    = _F(METHODE = 'MUMPS', PCENT_PIVOT = 10, NPREC=1000),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  )


U=STAT_NON_LINE(reuse=U,
  ETAT_INIT  = _F(EVOL_NOLI = U),
  MODELE     = MO,
  CHAM_MATER = CM,
  CARA_ELEM  = ORIEN_FI,
  EXCIT      = (
                 _F(CHARGE = SYMETRIE),
                 _F(CHARGE = TRACTION ,
                    TYPE_CHARGE = 'FIXE_PILO'),
               ),
  COMP_INCR  = (
                _F(RELATION = 'ELAS'      ,  GROUP_MA = ('DCB_1','DCB_2')),
                _F(RELATION = 'CZM_OUV_MIX', GROUP_MA = 'DCB_J', ),
                ),
  INCREMENT  = _F(LIST_INST = L_INST,
                  INST_FIN = tfin,
                 ),
  NEWTON     = _F(MATRICE    = 'TANGENTE'  , REAC_ITER=1 ,
                   ),

  PILOTAGE   = _F(
                  SELECTION = 'RESIDU',
                  TYPE = 'PRED_ELAS',
                  GROUP_MA = 'DCB_J',
                  COEF_MULT = c_mult_pilo,
                  ETA_PILO_R_MIN = 0.),

  CONVERGENCE = _F(
                   RESI_REFE_RELA = 1.E-6,
                   SIGM_REFE      = sigc,
                   DEPL_REFE      = dc,
                   ITER_GLOB_MAXI = 12,
                  ),
  SOLVEUR      = _F(METHODE = 'MUMPS', PCENT_PIVOT = 10, NPREC=1000),
  ARCHIVAGE    = _F(LIST_INST  = INS_ARCH)
  )


U = CALC_NO(reuse = U,


  RESULTAT = U,
  OPTION = 'FORC_NODA',
  GROUP_MA='DCB_1'
  )


# DEPLACEMENT PILOTE
UPILO = POST_RELEVE_T(
 ACTION = _F(
   INTITULE     = 'U_PILOTE',
   OPERATION    = 'EXTRACTION',
   GROUP_NO     = 'NO7',
   NOM_CHAM     = 'DEPL',
   NOM_CMP      = 'DY',
   RESULTAT     =  U,
   TOUT_ORDRE   = 'OUI',
    ))

# FORCE RESULTANTE DANS LA DIRECTION Y:
FRES = POST_RELEVE_T(
 ACTION = _F(
   INTITULE     = 'F_RESULTANTE',
   OPERATION    = 'EXTRACTION',
   GROUP_NO     = 'DCB_GB',
   NOM_CHAM     = 'FORC_NODA',
   RESULTANTE   = 'DY',
   RESULTAT     =  U,
   TOUT_ORDRE   = 'OUI',
    ))


TEST_RESU (
  RESU = (
    _F(
      REFERENCE = 'ANALYTIQUE',
      RESULTAT  = U,
      NOM_CHAM  = 'DEPL',
      NOM_CMP   = 'DY',
      VALE      = 4.87012,
      GROUP_NO  = 'NO7',
      INST      = 2.0
      ),
    )
  )

TEST_TABLE(TABLE=FRES,
           FILTRE=(_F(NOM_PARA='INST',VALE=2.0)),
           NOM_PARA='DY',
           VALE=6.87460E+00,
           REFERENCE='ANALYTIQUE')


TEST_RESU (
  RESU = (
    _F(
      REFERENCE = 'ANALYTIQUE',
      RESULTAT  = U,
      NOM_CHAM  = 'DEPL',
      NOM_CMP   = 'DY',
      VALE      = 6.75984E+00,
      GROUP_NO  = 'NO7',
      INST      = 3.0
      ),
    )
  )

TEST_TABLE(TABLE=FRES,
           FILTRE=(_F(NOM_PARA='INST',VALE=3.0)),
           NOM_PARA  = 'DY',
           VALE      = 5.83764679,
           REFERENCE = 'ANALYTIQUE')


TEST_RESU (
  RESU = (
    _F(
      REFERENCE = 'ANALYTIQUE',
      RESULTAT  = U,
      NOM_CHAM  = 'DEPL',
      NOM_CMP   = 'DY',
      VALE      = 8.91468E+00,
      GROUP_NO  = 'NO7',
      INST      = 4.0
      ),
    )
  )

TEST_TABLE(TABLE=FRES,
           FILTRE=(_F(NOM_PARA='INST',VALE=4.0)),
           NOM_PARA  = 'DY',
           VALE      = 5.05991E+00,
           REFERENCE = 'ANALYTIQUE')


IMPR_TABLE(TABLE = UPILO)
IMPR_TABLE(TABLE = FRES)

