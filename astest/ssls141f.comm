                     VALE_REFE=u_l,
                     NOM_CMP='DZ',
                     REFERENCE='ANALYTIQUE',),);
        
TEST_RESU(RESU=_F(RESULTAT=RESU2A,NOM_CHAM='DEPL',NUME_ORDRE=1,
                     GROUP_NO='A2',PRECISION=0.009,
                     VALE_CALC=-3.9564413694642E-05,
                     VALE_REFE=u_l,
                     NOM_CMP='DZ',
                     REFERENCE='ANALYTIQUE',),);


