DEBUT(   CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#

MAILLAGE=LIRE_MAILLAGE(    UNITE=20 )

#

MODELE=AFFE_MODELE(
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = 'VOILE',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'DKT'),
                     _F(  GROUP_MA = 'CABLE001',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'BARRE')) )

#

CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       COQUE=_F(  GROUP_MA = 'VOILE', COQUE_NCOU=5,
                               EPAIS = 6.0E-1),
                       BARRE=_F(  GROUP_MA = 'CABLE001',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 1.5E-4, )) )

#

BETON=DEFI_MATERIAU(
                       ELAS=_F(  E = 3.0E+10,
                              NU = 0.),
                 BPEL_BETON=_F() )

#

ACIER=DEFI_MATERIAU(
                       ELAS=_F(  E = 2.1E+11,
                              NU = 0.),
                 BPEL_ACIER=_F(  F_PRG = 1.77E+9,
                              FROT_COURB = 0.,
                              FROT_LINE = 0.) )

#

CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAILLAGE,AFFE=(
                       _F(  GROUP_MA = 'VOILE',
                              MATER = BETON),
                       _F(  GROUP_MA = 'CABLE001',
                              MATER = ACIER)) )

#

CABLEPR=DEFI_CABLE_BP(            MODELE=MODELE,
                                  CHAM_MATER=CHMATER,
                                   CARA_ELEM=CARAELEM,
                              GROUP_MA_BETON='VOILE',
                                  DEFI_CABLE=_F(
                               GROUP_MA = 'CABLE001',
                               NOEUD_ANCRAGE = ( 'NC001001',  'NC001005', ),),
                               TYPE_ANCRAGE = ( 'ACTIF',   'ACTIF',  ),
                                TENSION_INIT=2.0E+5,
                               RECUL_ANCRAGE=0. )

#

CHARMECA=AFFE_CHAR_MECA(
                      MODELE=MODELE,DDL_IMPO=(
                    _F(  NOEUD = 'NB001001',
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,
                               DRX = 0.,
                               DRY = 0.,
                               DRZ = 0.),
                    _F(  NOEUD = 'NB002001',
                               DX = 0.,
                               DZ = 0.)),
                RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'OUI',
                               RELA_CINE = 'OUI'),
                        INFO=2 )

#

LINST=DEFI_LIST_REEL(    VALE=( 0.0,  1.0, ) )

#

EVOLNOLI=STAT_NON_LINE(
                MODELE=MODELE,
                CHAM_MATER=CHMATER,
                CARA_ELEM=CARAELEM,
                EXCIT=      _F( CHARGE = CHARMECA),
                COMPORTEMENT=  _F( RELATION = 'ELAS',
                                DEFORMATION = 'PETIT',
                                TOUT = 'OUI'),
                INCREMENT=  _F( LIST_INST = LINST),
                SOLVEUR=    _F( METHODE = 'MULT_FRONT',),
                CONVERGENCE=_F( ITER_GLOB_MAXI = 10,
                                RESI_GLOB_MAXI = 1.E-5),
                INFO=2
              )

EVOLNOLI=CALC_CHAMP( reuse=EVOLNOLI,  RESULTAT=EVOLNOLI, CONTRAINTE=('EFGE_ELNO'))
EVOLNOLI=CALC_CHAMP( reuse=EVOLNOLI,  RESULTAT=EVOLNOLI, CONTRAINTE=('SIEF_ELNO'))



