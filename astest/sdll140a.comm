DEBUT(CODE=_F(NOM='SDLL140A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),)

MAIL=LIRE_MAILLAGE()

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=(_F(GROUP_MA='ROTOR',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='POU_D_T',),
                         _F(GROUP_MA=('PALIER_A','PALIER_E','DISQUE1','DISQUE2','DISQUE3'),
                           PHENOMENE='MECANIQUE',
                           MODELISATION='DIS_TR',),))

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.0E11,
                            NU=0.3,
                            RHO=7800.0,),);


CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='ROTOR',
                            MATER=ACIER,),))

CARELEM=AFFE_CARA_ELEM(MODELE=MODELE,
                       POUTRE=(_F(GROUP_MA='ROTOR',
                                 SECTION='CERCLE',
                                 CARA='R',
                                 VALE=(.05,),),),
                        DISCRET=(
                                 _F(GROUP_MA='PALIER_A',
                                    CARA='K_TR_D_N',
                                    VALE=(0.,.5E8,.7E8,0.,0.,0.)
                                  ),
                                 _F(GROUP_MA='PALIER_E',
                                    CARA='K_TR_D_N',
                                    VALE=(0.,.5E8,.7E8,0.,0.,0.)
                                  ),
                                 _F(GROUP_MA='PALIER_A',
                                    CARA='A_TR_D_N',
                                    VALE=(0.,.5E3,.7E3,0.,0.,0.)
                                  ),
                                 _F(GROUP_MA='PALIER_E',
                                    CARA='A_TR_D_N',
                                    VALE=(0.,.5E3,.7E3,0.,0.,0.)
                                  ),
                                 _F(GROUP_MA='DISQUE1',
                                    CARA='M_TR_D_N',
                                    VALE=(14.580130,0.1232021,0.06463858,0.06463858,0,0,0,0,0,0),
                                  ),
                                 _F(GROUP_MA='DISQUE2',
                                    CARA='M_TR_D_N',
                                    VALE=(45.945793,0.97634809,0.4977460,0.4977460,0,0,0,0,0,0),
                                  ),
                                 _F(GROUP_MA='DISQUE3',
                                    CARA='M_TR_D_N',
                                    VALE=(55.134951,1.1716177,0.06023493,0.06023493,0,0,0,0,0,0),
                                  ),
                        ))
                                 
                                 

BLOQUAGE=AFFE_CHAR_MECA(MODELE=MODELE,
                        DDL_IMPO=(_F(GROUP_NO=('PALIER_A',),
                                     DX=0,
                                     ),))
                                    

MACRO_MATR_ASSE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARELEM,
                CHARGE=BLOQUAGE,
                NUME_DDL=CO('NUMEDDL'),
                MATR_ASSE=(_F(MATRICE=CO('RIGIDITE'),
                              OPTION='RIGI_MECA',),
                           _F(MATRICE=CO('MASSE'),
                              OPTION='MASS_MECA',),
                           _F(MATRICE=CO('AMOR'),
                              OPTION='AMOR_MECA',),),);

GYELEM=CALC_MATR_ELEM(OPTION='MECA_GYRO',
                      MODELE=MODELE,
                      CHAM_MATER=CHMAT,
                      CARA_ELEM=CARELEM,);


GYASS=ASSE_MATRICE(MATR_ELEM=GYELEM,
                   NUME_DDL=NUMEDDL,);


                    
MODES=MODE_ITER_SIMULT(MATR_A=RIGIDITE,
                       MATR_B=MASSE,
                       CALC_FREQ=_F(OPTION='BANDE',
                                    FREQ=(3.,1000.)));

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  REFERENCE='NON_REGRESSION',
                  NUME_ORDRE=1,
                  PARA='FREQ',
                  VALE=60.87,),)


OM=25000.*pi/30.

GYOM=COMB_MATR_ASSE(COMB_R=(_F(MATR_ASSE=GYASS, COEF_R=OM,),
                            _F(MATR_ASSE=AMOR, COEF_R=1.,),))


# TEST DE LA MATRICE GYROSCOPIQUE AVEC UN TRANSITOIRE

TEMPLI=DEFI_LIST_REEL(DEBUT=0., INTERVALLE=_F( JUSQU_A = .02, NOMBRE = 2000))

CHARGE=AFFE_CHAR_MECA(  MODELE=MODELE,
                FORCE_NODALE=_F( GROUP_NO = 'PALIER_E', MX = 1.E9),)

VECTELEM=CALC_VECT_ELEM(   CHARGE=CHARGE,
                           CARA_ELEM=CARELEM,
                           CHAM_MATER=CHMAT,
                           OPTION='CHAR_MECA')
 
VECAS=ASSE_VECTEUR(   VECT_ELEM=VECTELEM,  NUME_DDL=NUMEDDL)

FONC=DEFI_FONCTION(   NOM_PARA='INST', VALE=(0.,0.,1.E-6,1.,1.,1.,))

TRANSI=DYNA_LINE_TRAN(  MATR_MASS=MASSE, MATR_RIGI=RIGIDITE,
                          NEWMARK=_F(),
                          INCREMENT=_F(  LIST_INST = TEMPLI),
                          MATR_AMOR=AMOR,
                          EXCIT=_F( VECT_ASSE = VECAS, FONC_MULT = FONC))

DRXA=RECU_FONCTION(RESULTAT=TRANSI,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DRX',
                   GROUP_NO='PALIER_A',);

IMPR_FONCTION(FORMAT='XMGRACE',
              UNITE=29,
              COURBE=_F(FONCTION=DRXA,
                        MARQUEUR=0,),);


TEST_RESU(RESU=_F(RESULTAT=TRANSI,
                  INST=.01,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DRX',
                  GROUP_NO='PALIER_A',
                  VALE=2.0685227e4,
                  PRECISION=(1e-06,1e-06,),
                  REFERENCE='NON_REGRESSION',),);


FIN()


