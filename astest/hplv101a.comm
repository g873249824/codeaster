DEBUT(CODE=_F(NOM='HPLV101A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

#


MAT=DEFI_MATERIAU( ELAS=_F(  E = 1.0,      NU = 0.3, ALPHA = 0.),
                          THER=_F(  LAMBDA = 1.0,   RHO_CP = 0.0) )

MAT2=DEFI_MATERIAU(      ELAS=_F( E = 1.0,  NU = 0.3),
           ECRO_LINE=_F( D_SIGM_EPSI = 0., SY = 10000.) )

MAILL=LIRE_MAILLAGE(   )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAILL,
                          AFFE=_F(  TOUT = 'OUI',   MATER = MAT) )

CHMAT2=AFFE_MATERIAU(  MAILLAGE=MAILL,
                          AFFE=_F(  TOUT = 'OUI',   MATER = MAT2) )

MODTH=AFFE_MODELE(  MAILLAGE=MAILL,
                        AFFE=_F( TOUT = 'OUI',   MODELISATION = 'PLAN',
                              PHENOMENE = 'THERMIQUE') )

MODME=AFFE_MODELE(  MAILLAGE=MAILL,
                        AFFE=_F( TOUT = 'OUI',   MODELISATION = 'C_PLAN',
                              PHENOMENE = 'MECANIQUE') )

#-----------------------------------------------------------------------

CHARTH=AFFE_CHAR_THER(  MODELE=MODTH,
             TEMP_IMPO=_F(  GROUP_NO = 'GRNM14',TEMP = 0.0),
             GRAD_TEMP_INIT=_F( TOUT = 'OUI',FLUX_X = -1.0))

# POUR TESTER AFFE_CHAR_THER_F/GRAD_TEMP_INIT :
#   (FL1+FL2+FL3+FL4)(INST,Y)=0.0

FL1=DEFI_FONCTION(NOM_PARA='INST',
       VALE=(-1., -1.,  1., 1.,))

FL2=DEFI_FONCTION(NOM_PARA='INST',
       VALE=(-1., +2.,  1., 0.,))

FL3=DEFI_FONCTION(NOM_PARA='Y',
       VALE=(-1000., +5.,  1000., 0.,))

FL4=DEFI_FONCTION(NOM_PARA='Y',
       VALE=(-1000., -6.,  1000., -1.,))

CHART1=AFFE_CHAR_THER_F(  MODELE=MODTH,
             GRAD_TEMP_INIT=_F( TOUT = 'OUI', FLUX_X = FL1))

CHART2=AFFE_CHAR_THER_F(  MODELE=MODTH,
             GRAD_TEMP_INIT=_F( TOUT = 'OUI', FLUX_X = FL2))

CHART3=AFFE_CHAR_THER_F(  MODELE=MODTH,
             GRAD_TEMP_INIT=_F( TOUT = 'OUI', FLUX_X = FL3))

CHART4=AFFE_CHAR_THER_F(  MODELE=MODTH,
             GRAD_TEMP_INIT=_F( TOUT = 'OUI', FLUX_X = FL4))

CHARMM=AFFE_CHAR_MECA(  MODELE=MODME,DDL_IMPO=(
              _F(  GROUP_NO = 'GRNM14',  DX = 0.0),
                       _F(  NOEUD = 'O',       DY = 0.0)),
                  EPSI_INIT=_F( TOUT = 'OUI',EPXX = -1.0))

TEMPGR=THER_LINEAIRE(
                MODELE=MODTH,   CHAM_MATER=CHMAT,EXCIT=(
                _F( CHARGE = CHARTH),
                _F( CHARGE = CHART1),
                _F( CHARGE = CHART2),
                _F( CHARGE = CHART3),
                _F( CHARGE = CHART4)) )


DEPLDM=MECA_STATIQUE(
                  MODELE=MODME,
                  CHAM_MATER=CHMAT,
                  EXCIT=_F( CHARGE = CHARMM),

                      )

CALC_ELEM( reuse= DEPLDM ,RESULTAT= DEPLDM ,

                  OPTION='SIGM_ELNO_DEPL'
                      )
#
# CALC_ELEM SERT A TESTER QUE SIEF_ELNO_ELGA PEUT ETRE FAIT APRES
# MECA_STATIQUE QUI CALCULE PAR DEFAUT SIEF_ELGA_DEPL
#
CALC_ELEM( reuse = DEPLDM,
                   OPTION =(
                         'SIEF_ELNO_ELGA'),
                          RESULTAT=DEPLDM,
                 )
# ------------------------------------------------------------------
L_INST=DEFI_LIST_REEL(  DEBUT=0.0,
              INTERVALLE=_F( JUSQU_A = 1.0,      NOMBRE = 10)
           )

RAMPE=DEFI_FONCTION(   NOM_PARA='INST',
           PROL_GAUCHE='LINEAIRE',  PROL_DROITE='LINEAIRE',
           VALE=(0.0,0.0,1.0,1.0,))

# STAT_NON_LINE 'VMIS'

#---------------------


DEPLD2=STAT_NON_LINE(
                            MODELE=MODME,
                        CHAM_MATER=CHMAT2,
                             EXCIT=_F(
                CHARGE = CHARMM,
                FONC_MULT = RAMPE),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_LINE',
                ITER_INTE_MAXI = 30,
                RESI_INTE_RELA = 1.E-8),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 2),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,)
              )

IMPR_CO(  CONCEPT=_F(NOM=DEPLD2))

# ------------------------------------------------------------------
T_DZETA=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
     NOM_CHAM='TEMP', RESULTAT=TEMPGR,   NUME_ORDRE=0   )

TT=POST_ELEM(   MODELE=MODTH,   CHAM_MATER=CHMAT,
                 CHAM_GD=T_DZETA,    INST=0.,  ENER_POT=_F(  TOUT = 'OUI') )

ENER_ELA=POST_ELEM(
                          RESULTAT=DEPLD2,
                          NUME_ORDRE=10,
                           ENER_ELAS=_F( TOUT = 'OUI')
                       )

ENER_TOT=POST_ELEM(
                          RESULTAT=DEPLD2,
                          NUME_ORDRE=10,
                           ENER_TOTALE=_F( TOUT = 'OUI')
                       )

DEM_KI11=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
          NOM_CHAM='DEPL', RESULTAT=DEPLDM,  NUME_ORDRE=1 )

PM=POST_ELEM(   MODELE=MODME,   CHAM_MATER=CHMAT,
                  TITRE=( ' CAS TEST &CODE','&RL COMMANDE &COMMANDE', ),
                CHAM_GD=DEM_KI11,             ENER_POT=_F(  TOUT = 'OUI') )

IMPR_TABLE(   TABLE=ENER_ELA)

IMPR_TABLE(   TABLE=ENER_TOT )

TEST_RESU(CHAM_NO=(

     # TEMPERATURES
            _F(  CHAM_GD = T_DZETA,  CRITERE = 'RELATIF',   PRECISION = 1.E-6,
              NOEUD = 'A',        NOM_CMP = 'TEMP',  VALE = -1.000000000),
     # DEPLACEMENTS
            _F(  CHAM_GD = DEM_KI11, CRITERE = 'RELATIF',   PRECISION = 1.E-6,
              NOEUD = 'A',        NOM_CMP = 'DX',    VALE = -1.000000000),
            _F(  CHAM_GD = DEM_KI11, CRITERE = 'RELATIF',   PRECISION = 1.E-6,
              NOEUD = 'N6',       NOM_CMP = 'DX',    VALE = -0.500000000))
            )

TEST_TABLE(      TABLE=TT,       NOM_PARA='TOTALE',
                TYPE_TEST='SOMM_ABS',
                REFERENCE='ANALYTIQUE',
                VALE=0.500,
                 PRECISION=1.E-6 )

TEST_TABLE(      TABLE=PM,       NOM_PARA='TOTALE',
                TYPE_TEST='SOMM_ABS',
                REFERENCE='ANALYTIQUE',
                VALE=0.5494505,
                 PRECISION=1.E-6 )

TEST_TABLE(      TABLE=ENER_ELA,        NOM_PARA='TOTALE',
                TYPE_TEST='SOMM_ABS',
                REFERENCE='ANALYTIQUE',
                VALE=0.5494505,
                 PRECISION=1.E-6 )

TEST_TABLE(      TABLE=ENER_TOT,       NOM_PARA='TOTALE',
                TYPE_TEST='SOMM_ABS',
                REFERENCE='ANALYTIQUE',
                VALE=0.5494505,
                 PRECISION=1.E-6 )

FIN( )
#
