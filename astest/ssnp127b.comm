DEBUT(CODE=_F(NOM='SSNP127B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='OUI'))
TOTO=LIRE_MAILLAGE(FORMAT='MED',INFO=2,);
TOTO=DEFI_GROUP(reuse =TOTO,
                MAILLAGE=TOTO,
                CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MAT1= DEFI_MATERIAU(NON_LOCAL=_F(LONG_CARA=3.0**0.5,),
              ELAS = _F(E = 32000.,  NU = 0.2),
              ENDO_ORTH_BETON = _F(ALPHA = 0.87,
                                   K0 = 3.e-4,
                                   K1 = 10.5,
                                   K2 = 6.e-4,
                                   ECROB=1.e-3,
                                   ECROD=0.06,
                                   ),);


CHMAT=AFFE_MATERIAU(AFFE=_F(MATER=MAT1,
                            TOUT='OUI',),
                    MAILLAGE=TOTO,);

MO=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                                              PHENOMENE='MECANIQUE',
                                              MODELISATION='D_PLAN_GRAD_EPSI',),
                               MAILLAGE=TOTO,
                               INFO=2,);

CL1=AFFE_CHAR_MECA(DDL_IMPO=_F(DY=0,
                               GROUP_NO='GMA7',),
                   MODELE=MO,);

CL2=AFFE_CHAR_MECA(DDL_IMPO=(_F(DX=0,
                               GROUP_NO='GMA8',),
                             ),
                   MODELE=MO,);

TRACTION=AFFE_CHAR_MECA(FORCE_CONTOUR=_F(FY=1.0,
                                    GROUP_MA='GMA9',),
                        MODELE=MO,);

INSTANTS=DEFI_LIST_REEL(INTERVALLE=(_F(JUSQU_A=0.0,
                                      NOMBRE=1,),
                                    _F(JUSQU_A=10,
                                      NOMBRE=100,),),
                        DEBUT=-1.,);

FONCMULT=DEFI_FONCTION(NOM_PARA='INST',
                       VALE=(-1.,0,0,0.005),);

EVOL1=STAT_NON_LINE(CHAM_MATER=CHMAT,
                    MODELE=MO,
                    CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                   ITER_GLOB_ELAS=25,
                                   RESI_GLOB_RELA=1.E-06,),
                    COMP_INCR=_F(RELATION='ENDO_ORTH_BETON',),
                    INCREMENT=_F(LIST_INST=INSTANTS,
                                 INST_FIN=0.,),
                    NEWTON=_F(PREDICTION='TANGENTE',
                              REAC_ITER=1,
                              PAS_MINI_ELAS=1.E-03,
                              MATRICE='TANGENTE',
                              REAC_INCR=1,),
                    EXCIT=(_F(CHARGE=TRACTION,
                              FONC_MULT=FONCMULT,),
                           _F(CHARGE=CL1,),
                           _F(CHARGE=CL2,),),);

EVOL1=STAT_NON_LINE(reuse=EVOL1,
                     MODELE=MO,
                       CHAM_MATER=CHMAT,
                    EXCIT=(_F(CHARGE=TRACTION,
                              TYPE_CHARGE='FIXE_PILO',),
                           _F(CHARGE=CL1,),
                           _F(CHARGE=CL2,),),
                    COMP_INCR=_F(RELATION='ENDO_ORTH_BETON',),
                    ETAT_INIT=_F(EVOL_NOLI=EVOL1,),
                    INCREMENT=_F(LIST_INST=INSTANTS,),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                   ITER_GLOB_ELAS=100,
                                   RESI_GLOB_RELA=1.E-06,),
                    NEWTON=_F(PREDICTION='TANGENTE',
                              REAC_ITER=1,
                              PAS_MINI_ELAS=1.E-04,
                              MATRICE='TANGENTE',
                              REAC_INCR=1,),
                       PILOTAGE=_F(TYPE='PRED_ELAS',
                                 SELECTION='RESIDU',
                                 GROUP_MA = ('GMA10',),
                                 ETA_PILO_R_MIN=0.,
                                 ETA_PILO_MIN=0.4,
                                 ETA_PILO_R_MAX=100.,),
                       );

TEST_RESU(RESU=(_F(NUME_ORDRE=101,
                   RESULTAT=EVOL1,
                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='DY',
                   VALE_CALC=2.06997E-3,
                   TOLE_MACHINE=1.E-3,),
                _F(NUME_ORDRE=101,
                   POINT=2,
                   RESULTAT=EVOL1,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=1.7028499999999999E-3,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='M31',),
                _F(NUME_ORDRE=101,
                   POINT=2,
                   RESULTAT=EVOL1,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V2',
                   VALE_CALC=0.89355399999999996,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='M31',),
                ),
          )

FIN();
