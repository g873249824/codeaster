DEBUT(CODE=_F(
              NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='OUI',),);

MBI=LIRE_MAILLAGE(FORMAT='ASTER');

MOD=AFFE_MODELE(MAILLAGE=MBI,
                AFFE=(
                      _F(GROUP_MA='POU',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_T',),
                      _F(GROUP_MA=('VOL','RACC'),
                         PHENOMENE='MECANIQUE',
                         MODELISATION='3D',),
                      ),);

MAT=DEFI_MATERIAU(ELAS=_F(E=2.E11,
                           NU=0.3,
                           RHO=7800.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MBI,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

CARA_POU=AFFE_CARA_ELEM(MODELE=MOD,
                        POUTRE=_F(GROUP_MA=('POU',),
                                  SECTION='RECTANGLE',
                                  CARA='H',
                                  VALE=(.02),),);

CL=AFFE_CHAR_MECA(MODELE=MOD,
                  DDL_IMPO=_F(GROUP_NO='Encas',DX=0,DY=0,DZ=0,),);

RACC=AFFE_CHAR_MECA(MODELE=MOD,
                    LIAISON_ELEM=_F(OPTION='3D_POU',
                                    GROUP_MA_1='RACC',
                                    NOEUD_2='N1332',),);

ASSEMBLAGE(MODELE=MOD,
           CHAM_MATER=CHMAT,
           CARA_ELEM=CARA_POU,
           CHARGE=(CL,RACC),
           NUME_DDL=CO('NUMEDDL'),
           MATR_ASSE=(_F(MATRICE=CO('RIGIDITE'),
                         OPTION='RIGI_MECA',),
                      _F(MATRICE=CO('MASSE'),
                         OPTION='MASS_MECA',),),
           );

FORCE=AFFE_CHAR_MECA(MODELE=MOD,
                     FORCE_NODALE=_F(GROUP_NO='ForNod',
                                     FZ=-500.0,),);

FONC = FORMULE(NOM_PARA='INST',VALE='sin(0.025*pi*INST)');

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=80.0,
                                   PAS=1,),);

TRANSI=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='PHYS',
                  MODELE=MOD,
                  CHAM_MATER=CHMAT,
                  CARA_ELEM=CARA_POU,
                  MATR_MASS=MASSE,
                  MATR_RIGI=RIGIDITE,
                  SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',), 
                  EXCIT=(_F(CHARGE=FORCE,
                            FONC_MULT=FONC,),
                         _F(CHARGE=CL,),
                         _F(CHARGE=RACC,),),
                  INCREMENT=_F(LIST_INST=LINST,),);

DZ_DIS=RECU_FONCTION(RESULTAT=TRANSI,
                     NOM_CHAM='DEPL',
                     NOM_CMP='DZ',
                     GROUP_NO='ForNod',
                     INTERPOL='LIN',);

TEST_FONCTION(VALEUR=_F(VALE_CALC=-0.002657124,
                        VALE_REFE=-0.002686535,
                        VALE_PARA=20,
                        REFERENCE='AUTRE_ASTER',
                        PRECISION=7.E-2,
                        FONCTION=DZ_DIS,),
              )

TEST_FONCTION(VALEUR=_F(VALE_CALC=0.002657034,
                        VALE_REFE=0.002686443,
                        REFERENCE='AUTRE_ASTER',
                        VALE_PARA=60,
                        PRECISION=7.E-2,
                        FONCTION=DZ_DIS,
                        ),
              )

FIN();
