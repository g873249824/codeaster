DEBUT(CODE=_F(NOM='SSNP144L',NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',);

MAILLAG2= COPIER(CONCEPT= MAILLAG1)

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA='SURF',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),
                           _F(GROUP_MA=('LIG1','LIG3',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURF',
                                      NOM='SURF',),
                                   _F(NOM='SURF_SUP',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N3',
                                      PRECISION=2.1,
                                      VECT_NORMALE=(0.0,1.0),),
                                   _F(NOM='SURF_INF',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N1',
                                      PRECISION=2.1,
                                      VECT_NORMALE=(0.0,1.0),),
                                   _F(NOM='NSUP',
                                      NOEUD=('N7','N10',),),
                                   _F(NOM='NINF',
                                      NOEUD=('N6','N11',),),
                                   _F(NOM='NSUP2',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N7',
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=0.00001,),
                                   _F(NOM='NINF2',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N6',
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=0.00001,),
                                   ),);

# FORME ANALYTIQUE POUR LA FISSURE

LN = FORMULE(VALE='Y-2.5',
             NOM_PARA=('X','Y',),);


FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),
                    INFO=1,);

#-------------------------------------------------
#                   CONTACT
#-------------------------------------------------

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='P1P1',);

CTXFEM = DEFI_CONTACT(
            MODELE         = MODELEK,
            FORMULATION    = 'XFEM',
            FROTTEMENT     = 'SANS',
            ITER_CONT_MAXI=4,
            ZONE=(_F(  INTEGRATION='GAUSS',
                       ORDRE_INT=3,
                       ALGO_CONT='CZM',
                       FISS_MAIT = FISS,
                       ALGO_LAGR='VERSION2',
                       RELATION='CZM_EXP_REG',),),);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
nu = 0.0;
E = 500000.0;

MATE=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,),
                    RUPT_FRAG=_F(GC=0.9E3,
                                 SIGM_C=1.1E6,
                                 PENA_ADHERENCE=1.0E-3),
                   );
CHAMPMA=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA='SURF',MATER=MATE,),
                            ),
                      );
#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=(
                                  _F(JUSQU_A=0.5,
                                    NOMBRE=1,),
                                  _F(JUSQU_A=1.0,
                                     NOMBRE=1,),
                                    ),);

#----------------MODE-I------------------------

DEPY1 = 1.0;

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIG1',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_MA='LIG3',
                                DX=0.0,
                                DY=DEPY1,),),
                      LIAISON_GROUP=(_F(GROUP_NO_1 = ('NINF'),
                                        GROUP_NO_2 = ('NSUP'),
                                        DDL_1      = 'H1Y',
                                        DDL_2      = 'H1Y',
                                        COEF_MULT_1=  1.,
                                        COEF_MULT_2=  -1.,
                                       COEF_IMPO= 0.,),),
                                       );

#--------------------------------------------------------
#                       CALCUL - MODE I
#--------------------------------------------------------


UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CH1,TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=0.5,),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_IMPO',
                                COEF_MULT=1.83335E6,
                                NOEUD='N6',
                                FISSURE=FISS,
                                DIRE_PILO='DY',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               NPREC=-1),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);


UTOT1=STAT_NON_LINE(reuse =UTOT1,
                    ETAT_INIT=_F(EVOL_NOLI=UTOT1,),
                    MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CH1,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=1.,),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_IMPO',
                                COEF_MULT=-9.16675E5,
                                NOEUD='N6',
                                FISSURE=FISS,
                                DIRE_PILO='DY',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               NPREC=-1,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK,
                       PREF_GROUP_CO='NFISSU',);

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='C_PLAN',),);

RESXFEM1=POST_CHAM_XFEM(RESULTAT=UTOT1,
                        MODELE_VISU=MOD_VISU,);
#--------------------------------------------------------
#                  VALIDATION DES RESULTATS
#--------------------------------------------------------
TOLE=1.0E-8
LINST=[0.5,1.]
LLAG=[3.66296853301E+05 ,-3.66296853301E+05 ]

LDEP=[2.72724793411E-07 ,-2.72724793411E-07 ]
nbc=2

TAB1 = [None]*nbc
TAB2 = [None]*nbc
TAB3 = [None]*nbc
TAB4 = [None]*nbc

VALE_CALC=[3.6629685330084E+05,
 3.6629685330084E+05,
 2.7272479341534E-07,
 2.7272479319329E-07,
 -3.6629685330084E+05,
 -3.6629685330084E+05,
 -2.7272479319329E-07,
 -2.7272479363738E-07,]

#--------------------------------------------------------
#                           MODE I
#--------------------------------------------------------

SAUT=FORMULE(NOM_PARA=('DYP','DYM'),
             VALE='DYP-DYM',);

for i in range(0,nbc) :

   TAB1[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NFISSU',),
                                   RESULTAT=RESXFEM1,
                                   NOM_CHAM='DEPL',
                                   INST=LINST[i],
                                   TOUT_CMP='OUI',
                                   OPERATION='EXTRACTION',),);
   TAB2[i]=CALC_TABLE(TABLE=TAB1[i],
                      ACTION=(_F(OPERATION='FILTRE',
                                NOM_PARA='NOEUD',
                                CRIT_COMP='REGEXP',
                                VALE_K='NM',),
                               _F(OPERATION='RENOMME',
                                NOM_PARA=('DY','DYM'),),
                               ),);

   TAB3[i]=CALC_TABLE(TABLE=TAB1[i],
                      ACTION=(_F(OPERATION='FILTRE',
                                NOM_PARA='NOEUD',
                                CRIT_COMP='REGEXP',
                                VALE_K='NP',),
                             _F(OPERATION='RENOMME',
                                NOM_PARA=('DY','DYP'),),
                              ),);

   TAB4[i]=CALC_TABLE(TABLE=TAB2[i],
                      ACTION=(_F(OPERATION='COMB',
                                TABLE=TAB3[i],
                                NOM_PARA=('INST','COOR_X'),),
                              _F(OPERATION='OPER',
                                FORMULE=SAUT,
                                NOM_PARA='SAUY',),
                              ),);


   TEST_TABLE(TABLE=TAB1[i],
              NOM_PARA='LAGS_C',
              TYPE_TEST='MAX',
              VALE_CALC=VALE_CALC[i*4], VALE_REFE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE');

   TEST_TABLE(TABLE=TAB1[i],
              NOM_PARA='LAGS_C',
              TYPE_TEST='MIN',
              VALE_CALC=VALE_CALC[i*4+1], VALE_REFE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE');

   TEST_TABLE(TABLE=TAB4[i],
              NOM_PARA='SAUY',
              TYPE_TEST='MAX',
              VALE_CALC=VALE_CALC[i*4+2], VALE_REFE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE');

   TEST_TABLE(TABLE=TAB4[i],
              NOM_PARA='SAUY',
              TYPE_TEST='MIN',
              VALE_CALC=VALE_CALC[i*4+3], VALE_REFE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE');

FIN();
