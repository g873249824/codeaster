#---------------------------------------------------------------------
#  - CALCUL EN ANALYSE MODALE DYNAMIQUE D'UNE PLAQUE CARREE EPAISSE 
#    SIMPLEMENT SUPPORTEE 
#  - MODELISATION AVEC DES ELEMENTS DST(TRIA3) 
#---------------------------------------------------------------------

DEBUT(CODE=_F(NOM='SDLS502A',
              NIV_PUB_WEB='INTERNET',),);
#

PRE_GIBI();

PLAQ_MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI',),);
#

MODEL=AFFE_MODELE(MAILLAGE=PLAQ_MA,
                  AFFE=_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='DST',),);

PLAQ_MA=DEFI_GROUP(reuse =PLAQ_MA,
                   MAILLAGE=PLAQ_MA,
                   CREA_GROUP_NO=_F(GROUP_MA='BORDS',
                                    NOM='BORDS',),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES 
#---------------------------------------------------------------------

CARA_ELE=AFFE_CARA_ELEM(MODELE=MODEL,
                        COQUE=_F(GROUP_MA='TOUT_ELT',
                                 EPAIS=1.0,
                                 ANGL_REP=(15.5,0.0,),),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX 
#---------------------------------------------------------------------

ACIER=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                            NU=0.3,
                            RHO=8000.0,),);
#

MAT=AFFE_MATERIAU(MAILLAGE=PLAQ_MA,
                  AFFE=_F(TOUT='OUI',
                          MATER=ACIER,),);
#
#---------------------------------------------------------------------
#                     CHARGEMENTS 
#---------------------------------------------------------------------

CHAR0=AFFE_CHAR_MECA(MODELE=MODEL,
                     DDL_IMPO=_F(GROUP_NO='BORDS',
                                 DZ=0.0,),);
#
#---------------------------------------------------------------------
#                     RESOLUTION 
#---------------------------------------------------------------------

M_EL_RIG=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                        MODELE=MODEL,
                        CHAM_MATER=MAT,
                        CARA_ELEM=CARA_ELE,
                        CHARGE=CHAR0,);
#

M_EL_MAS=CALC_MATR_ELEM(OPTION='MASS_MECA',
                        MODELE=MODEL,
                        CHAM_MATER=MAT,
                        CARA_ELEM=CARA_ELE,
                        CHARGE=CHAR0,);
#

NU=NUME_DDL(MATR_RIGI=M_EL_RIG,);
#

M_AS_RIG=ASSE_MATRICE(MATR_ELEM=M_EL_RIG,
                      NUME_DDL=NU,);
#

M_AS_MAS=ASSE_MATRICE(MATR_ELEM=M_EL_MAS,
                      NUME_DDL=NU,);
#

MODES=MODE_ITER_SIMULT(MATR_A=M_AS_RIG,
                       MATR_B=M_AS_MAS,
                       METHODE='SORENSEN',
                       CALC_FREQ=_F(OPTION='BANDE',
                                    FREQ=(10.0,350.0,),),);
#
#---------------------------------------------------------------------
#                     VERIFICATION DES RESULTATS 
#---------------------------------------------------------------------

TEST_RESU(RESU=(_F(RESULTAT=MODES,
                   NUME_MODE=4,
                   PARA='FREQ',
                   VALE=44.762,
                   CRITERE='RELATIF',
                   PRECISION=0.01,
                   REFERENCE='SOURCE_EXTERNE',),
                _F(RESULTAT=MODES,
                   NUME_MODE=5,
                   PARA='FREQ',
                   VALE=110.52,
                   CRITERE='RELATIF',
                   PRECISION=0.03,
                   REFERENCE='SOURCE_EXTERNE',),
                _F(RESULTAT=MODES,
                   NUME_MODE=6,
                   PARA='FREQ',
                   VALE=110.52,
                   CRITERE='RELATIF',
                   PRECISION=0.03,
                   REFERENCE='SOURCE_EXTERNE',),
                _F(RESULTAT=MODES,
                   NUME_MODE=7,
                   PARA='FREQ',
                   VALE=169.08,
                   CRITERE='RELATIF',
                   PRECISION=0.03,
                   REFERENCE='SOURCE_EXTERNE',),
                _F(RESULTAT=MODES,
                   NUME_MODE=8,
                   PARA='FREQ',
                   VALE=193.93,
                   CRITERE='RELATIF',
                   PRECISION=0.03,
                   REFERENCE='SOURCE_EXTERNE',),
                _F(RESULTAT=MODES,
                   NUME_MODE=11,
                   PARA='FREQ',
                   VALE=206.64,
                   CRITERE='RELATIF',
                   PRECISION=0.03,
                   REFERENCE='SOURCE_EXTERNE',),
                _F(RESULTAT=MODES,
                   NUME_MODE=12,
                   PARA='FREQ',
                   VALE=206.64,
                   CRITERE='RELATIF',
                   PRECISION=0.03,
                   REFERENCE='SOURCE_EXTERNE',),),);

FIN();
