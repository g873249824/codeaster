# - CALCUL QUASI-STATIQUE D'UN PATIN GLISSANT SUR UN PLAN RIGIDE
# - MODELISATION AVEC DES ELEMENTS D_PLAN (MAILLES QUA4)
# - MODELISATION DU CONTACT AVEC LA METHODE CONTACT (AFFE_CHAR_MECA)
# - METHODE CONTINUE
# - JEU DEFINI AVEC DIST_2

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAILL=LIRE_MAILLAGE(FORMAT='MED',);

MAILL=DEFI_GROUP(reuse =MAILL,
                 MAILLAGE=MAILL,
                 CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MODE=AFFE_MODELE(MAILLAGE=MAILL,
                 
                 AFFE=(_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='D_PLAN',),
                       _F(GROUP_MA='RES_LAT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T',),
                       _F(GROUP_MA='RES_HAUT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T',),),);

MAILL=MODI_MAILLAGE(reuse =MAILL,
                    MAILLAGE=MAILL,
                    ORIE_PEAU_2D=_F(GROUP_MA=('LMAI','LESC','FACESUP','FACELAT'),),
                    );

#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------

CARA=AFFE_CARA_ELEM(MODELE=MODE,
                    DISCRET_2D=(_F(GROUP_MA='RES_LAT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(2.0,0.0),),
                                _F(GROUP_MA='RES_HAUT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(0.005,0.0),),
                              ),);

#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------

MPATIN=DEFI_MATERIAU(ELAS=_F(E=2.10000E5,
                             NU=0.0,),);

MBATI=DEFI_MATERIAU(ELAS=_F(E=1.E5,
                            NU=0.0,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL,
                 MODELE=MODE,
                 AFFE=(_F(GROUP_MA='PATIN',
                          MATER=MPATIN,),
                       _F(GROUP_MA='BATI',
                          MATER=MBATI,),
                          ),);

#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------

# CONDITIONS AUX LIMITES
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='BATI',
                                     DX=0.0,DY=0.0,),
                                  _F(GROUP_NO=('R1','R2','X','Y'),
                                     DX=0.0,DY=0.0,),),);

# CHARGEMENT 1 : APPLICATION DE LA PRESSION NORMALE
PNOR=AFFE_CHAR_MECA(MODELE=MODE,
                    PRES_REP=_F(GROUP_MA='FACESUP',
                                PRES=300.0,),);

# CHARGEMENT 2 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*0.99)
PTAN_99=AFFE_CHAR_MECA(MODELE=MODE,
                       PRES_REP=_F(GROUP_MA='FACELAT',
                                   PRES=178.19999999999999,),);

# CHARGEMENT 3 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*1.01)
PTAN_101=AFFE_CHAR_MECA(MODELE=MODE,
                        PRES_REP=_F(GROUP_MA='FACELAT',
                                    PRES=181.80000000000001,),);

# DEFINITION DU CONTACT

F_DIST_E=DEFI_CONSTANTE(VALE=-2.)

CTAC = DEFI_CONTACT(MODELE         = MODE,
                    FORMULATION    = 'CONTINUE',
                    FROTTEMENT     = 'COULOMB',
                    ALGO_RESO_CONT = 'POINT_FIXE',
                    ALGO_RESO_GEOM = 'POINT_FIXE',
                    ALGO_RESO_FROT = 'NEWTON',
                    ZONE =(
                           _F(
                             GROUP_MA_ESCL = 'LESC',
                             GROUP_MA_MAIT = 'LMAI',
                             COULOMB       = 0.3,
                             COEF_FROT     = 1E-5,
                             CONTACT_INIT  = 'NON',
                             DIST_ESCL=F_DIST_E,
                             ),
                          ),
                   );

#---------------------------------------------------------------------
#                         RESOLUTION
#---------------------------------------------------------------------


RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,
                          1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=2,),);


# CHARGEMENT 1
U_NOR=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=COND_LIM,),),
                    CONTACT  = CTAC,
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=30,),
                    INFO=1,);

U_NOR=CALC_CHAMP(reuse =U_NOR,
              RESULTAT=U_NOR,
              FORCE='REAC_NODA',

              );

# CHARGEMENT 2
U_99=STAT_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CM,
                   CARA_ELEM=CARA,
                   EXCIT=(_F(CHARGE=PNOR,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=PTAN_99,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=COND_LIM,),),
                   CONTACT  = CTAC,
                   COMPORTEMENT=_F(RELATION='ELAS',
                                TOUT='OUI',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=1500,),);

U_99=CALC_CHAMP(reuse =U_99,
             RESULTAT=U_99,
             FORCE='REAC_NODA',

              );

# CHARGEMENT 3
U_101=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=PTAN_101,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=COND_LIM,),),
                    CONTACT  = CTAC,
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=1500,),
                    );

U_101=CALC_CHAMP(reuse =U_101,
              RESULTAT=U_101,
              FORCE='REAC_NODA',

              );

#---------------------------------------------------------------------
#                         VERIFICATION DES RESULTATS
#---------------------------------------------------------------------

# CHARGEMENT 1
TEST_RESU(RESU=(_F(GROUP_NO='A',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='A',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.732050808,
                   VALE_REFE=-1.732,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.732050808,
                   VALE_REFE=-1.732,
                   PRECISION=1.E-2,
                   ),
                ),
          )

TU_NOR=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                               GROUP_NO='LESC',
                               RESULTAT=U_NOR,
                               NOM_CHAM='REAC_NODA',
                               NOM_CMP=('DX','DY'),
                               REPERE='LOCAL',
                               OPERATION='EXTRACTION',
                               INST=1.,),);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 1.19999797E+04,
           VALE_REFE=1.2000E4,
           NOM_PARA='DY',
           TYPE_TEST='SOMM',
           TABLE=TU_NOR,)

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC=-2.20751360E-07,
           VALE_REFE=0.0,
           NOM_PARA='DX',
           TYPE_TEST='SOMM',
           TABLE=TU_NOR,)

# CHARGEMENT 2
TEST_RESU(RESU=(_F(GROUP_NO='A',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.021344408,
                   VALE_REFE=1.0,
                   PRECISION=0.023,
                   ),
                _F(GROUP_NO='A',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.719727608,
                   VALE_REFE=-1.732,
