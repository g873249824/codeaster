                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.000000000,
                   VALE_REFE=1.0,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.732050808,
                   VALE_REFE=-1.732,
                   PRECISION=1.E-2,
                   ),
                ),
          )

TU_99=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                              GROUP_NO='LESC',
                              RESULTAT=U_99,
                              NOM_CHAM='REAC_NODA',
                              NOM_CMP=('DX','DY'),
                              REPERE='LOCAL',
                              OPERATION='EXTRACTION',
                              INST=1.),);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 1.19999797E+04,
           VALE_REFE=1.2000E4,
           NOM_PARA='DY',
           TYPE_TEST='SOMM',
           TABLE=TU_99,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC=-3.56396168E+03,
           VALE_REFE=-3564.0,
           NOM_PARA='DX',
           TYPE_TEST='SOMM',
           TABLE=TU_99,)

# CHARGEMENT 3
TEST_RESU(RESU=(_F(GROUP_NO='A',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_101,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=8.809533448,
