                   PRECISION=1.E-2,
                   ),
                ),
          )

TU_101=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                               GROUP_NO='LESC',
                               RESULTAT=U_101,
                               NOM_CHAM='REAC_NODA',
                               NOM_CMP=('DX','DY'),
                               REPERE='LOCAL',
                               OPERATION='EXTRACTION',
                               INST=1.),);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 1.19999797E+04,
           VALE_REFE=1.2000E4,
           NOM_PARA='DY',
           TYPE_TEST='SOMM',
           TABLE=TU_101,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
