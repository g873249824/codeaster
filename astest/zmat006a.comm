#                    MODELE ELASTOPLASTIQUE DE CHABOCHE
#----------------------------------------------------------------------
#
#               Y       A                          H = 1 MM (EPAISSEUR)
#               I<----------->                     A = 1 MM (COTE)
#                                                          2
#                                                  S = 1 MM (AIRE)
#                 NO4         +  NO3
#               O-------------OB
#           H  /+             I                F(T)
#             / I             I                 A
#               I NO6         I           100 N I____
#               I-------------I  NO5            I   /I
#               I             I                 I  / I
#               I             I                 I /  I
#               I  NO1        I  NO2            I/   I
#             ++O-------------OB     - X        O---------> T
#               +             B                       1S
#
#
#----------------------------------------------------------------------

DEBUT(CODE=_F(NOM='ZMAT006A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

CARRE=LIRE_MAILLAGE();

CPLAN=AFFE_MODELE(MAILLAGE=CARRE,
                  AFFE=_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='C_PLAN',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=145200.0,
                            NU=0.3,
                            ALPHA=0.0,),
                    CIN2_CHAB=_F(R_0=87.0,
                                 R_I=151.0,
                                 B=2.3,
                                 C1_I=63767.0,
                                 C2_I=498336.0,
                                 G1_0=341.0,
                                 G2_0=17184.0,),);

MAT=AFFE_MATERIAU(MAILLAGE=CARRE,
                  AFFE=_F(MAILLE=('MA1','MA2',),
                          MATER=ACIER),);

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.4,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=1.435,
                                    NOMBRE=30,),),);

TR_CS=AFFE_CHAR_MECA(MODELE=CPLAN,
                     DDL_IMPO=(_F(NOEUD=('NO3','NO5',),
                                  DX=0.0,),
                               _F(NOEUD='NO2',
                                  DX=0.0,
                                  DY=0.0,),),
                     FORCE_NODALE=(_F(NOEUD='NO1',
                                      FX=-1.5,
                                      FY=-0.5,),
                                   _F(NOEUD='NO3',
                                      FY=0.5,),
                                   _F(NOEUD='NO4',
                                      FX=0.5,
                                      FY=-0.5,),
                                   _F(NOEUD='NO5',
                                      FY=1.0,),
                                   _F(NOEUD='NO6',
                                      FX=-1.0,
                                      FY=-1.0,),),);

COEF=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,
                         1.0,50.0,
                         ),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);

SOLNL=STAT_NON_LINE(MODELE=CPLAN,
                    CHAM_MATER=MAT,
                    EXCIT=_F(CHARGE=TR_CS,
                             FONC_MULT=COEF,),
                    COMP_INCR=_F(RELATION='VMIS_CIN2_CHAB',),
                    INCREMENT=_F(LIST_INST=LINST,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    TITRE='VMIS_CIN2_CHAB C_PLAN DEBORST',);

TEST_RESU(RESU=(_F(INST=1.4350000000000001,
                   RESULTAT=SOLNL,
                   NOM_CHAM='DEPL',
                   NOEUD='NO1',
                   NOM_CMP='DX',
                   VALE_CALC=-4.7770399999999998E-3,
                   TOLE_MACHINE=1.E-3,),
                _F(INST=1.4350000000000001,
                   RESULTAT=SOLNL,
                   NOM_CHAM='DEPL',
                   NOEUD='NO1',
                   NOM_CMP='DY',
                   VALE_CALC=-0.0139358,
                   TOLE_MACHINE=1.E-3,),
                _F(INST=1.4350000000000001,
                   POINT=1,
                   RESULTAT=SOLNL,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC=143.5,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='MA1',),
                _F(INST=1.4350000000000001,
                   POINT=1,
                   RESULTAT=SOLNL,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXY',
                   VALE_CALC=143.5,
                   TOLE_MACHINE=1.E-3,
                   MAILLE='MA2',),
                ),
          )

SOLNLZ=STAT_NON_LINE(MODELE=CPLAN,
                     CHAM_MATER=MAT,
                     EXCIT=_F(CHARGE=TR_CS,
                              FONC_MULT=COEF,),
                     COMP_INCR=_F(RELATION='ZMAT',
                                  NB_VARI=17,
                                  UNITE=33,),
                     INCREMENT=_F(LIST_INST=LINST,),
                     NEWTON=_F(MATRICE='TANGENTE',
                               REAC_ITER=1,),
                     TITRE='CHABOCHE C_PLAN ZMAT',);

TEST_RESU(RESU=(_F(INST=1.4350000000000001,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=SOLNLZ,
                   NOM_CHAM='DEPL',
                   NOEUD='NO1',
                   NOM_CMP='DX',
                   VALE_CALC=-4.77714042E-03,
                   VALE_REFE=-4.7770399999999998E-3,),
                _F(INST=1.4350000000000001,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=SOLNLZ,
                   NOM_CHAM='DEPL',
                   NOEUD='NO1',
                   NOM_CMP='DY',
                   VALE_CALC=-0.013936101,
                   VALE_REFE=-0.0139358,),
                _F(INST=1.4350000000000001,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=SOLNLZ,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC=143.500000014,
                   VALE_REFE=143.5,
                   MAILLE='MA1',),
                _F(INST=1.4350000000000001,
                   REFERENCE='AUTRE_ASTER',
                   POINT=1,
                   RESULTAT=SOLNLZ,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXY',
                   VALE_CALC=143.499987620,
                   VALE_REFE=143.5,
                   MAILLE='MA2',),
                ),
          )

FIN();
