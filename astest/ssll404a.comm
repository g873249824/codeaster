# ELEMENTS  : MEPOUDE


DEBUT(CODE=_F( NOM = 'SSLL404A',NIV_PUB_WEB='INTERNET'))

ACIER=DEFI_MATERIAU(         
                         ELAS=_F(  NU = 0.3,          
                           E = 7.E10,            ))

MAYA=LIRE_MAILLAGE( )

MATER=AFFE_MATERIAU(    MAILLAGE=MAYA,
                                 AFFE=_F(  TOUT = 'OUI',  
                                        MATER = ACIER,   ))

STRUCTUR=AFFE_MODELE(    MAILLAGE=MAYA,
                               AFFE=_F(  TOUT = 'OUI',          
                                      MODELISATION = 'POU_D_E',  
                                      PHENOMENE = 'MECANIQUE',     ))

CARA=AFFE_CARA_ELEM(     MODELE=STRUCTUR,
                               POUTRE=_F( 
                            GROUP_MA = 'TOUT',     
                            SECTION = 'RECTANGLE',  
                            CARA = ( 'HY',  'HZ', ),     
                            VALE = ( 0.015,  0.002, ),     )
                          )

# CONDITIONS AUX LIMITES

FIXA=AFFE_CHAR_MECA(      MODELE=STRUCTUR,DDL_IMPO=(
                              _F(  GROUP_NO = 'POINT_A',  
                                               DX = 0.,  
                                               DY = 0.,  
                                               DZ = 0.,  
                                               DRY = 0.,  ),
                                       _F(  GROUP_NO = 'POINT_B',  
                                               DY = 0.,  
                                               DZ = 0.,  
                                               DRX = 0.,  ))
                          )

CHARGE=AFFE_CHAR_MECA(      MODELE=STRUCTUR,FORCE_NODALE=(
                          _F(  GROUP_NO = 'POINT_A',  
                                               MZ = 1.,    ),
                                       _F(  GROUP_NO = 'POINT_B',  
                                               MZ = -1.,   ))
                          )

# CALCUL STATIQUE

STATIQ=MECA_STATIQUE(         MODELE=STRUCTUR,
                                 CARA_ELEM=CARA,
                                CHAM_MATER=MATER,EXCIT=(
                                     _F(  CHARGE = FIXA,     ),
                                           _F(  CHARGE = CHARGE,   ))
                            )

# RECUPERATION DU CHAMP DE DEPLACEMENT

DEPLA=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=STATIQ,
                              NUME_ORDRE=1
                               )

# CALCUL DES CONTRAINTES ELEMENTAIRES A PARTIR DES DEPLACEMENTS
                              
                              
SIGMA=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='ELGA_SIEF_R',
                    RESULTAT=STATIQ, NUME_ORDRE=1,
                    NOM_CHAM='SIEF_ELGA_DEPL' ),


# CALCUL DES MATRICES ELEMENTAIRES ET ASSEMBLAGE


K_ELEM=CALC_MATR_ELEM(        MODELE=STRUCTUR,
                                 CARA_ELEM=CARA,
                                CHAM_MATER=MATER,
                                    CHARGE=FIXA,
                                   OPTION='RIGI_MECA' )

KS_ELEM=CALC_MATR_ELEM(        MODELE=STRUCTUR,
                                 OPTION='RIGI_GEOM',
                               CARA_ELEM=CARA,
                               SIEF_ELGA=SIGMA )

NUMERO=NUME_DDL(    MATR_RIGI=K_ELEM )

K_ASSE=ASSE_MATRICE(    MATR_ELEM=K_ELEM,
                             NUME_DDL=NUMERO  )

KS_ASSE=ASSE_MATRICE(    MATR_ELEM=KS_ELEM,
                             NUME_DDL=NUMERO  )

# CALCUL DES MODES PAR SORENSEN

MODE1=MODE_ITER_SIMULT(       MATR_A=K_ASSE,
                                   MATR_B=KS_ASSE,
                                TYPE_RESU='MODE_FLAMB',
                               CALC_FREQ=_F(  OPTION = 'PLUS_PETITE',     
                                           NMAX_FREQ = 5,   ))

TEST_RESU(RESU=(
   _F(   RESULTAT = MODE1,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.5E-2,   
           NUME_MODE = 1,   
           VALE = -14.4147,        
           REFERENCE = 'ANALYTIQUE',    ),

   _F(   RESULTAT = MODE1,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.8E-2,   
           NUME_MODE = 2,   
           VALE = -8.63207,        
           REFERENCE = 'ANALYTIQUE',      ),

   _F(   RESULTAT = MODE1,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.9E-2,   
           NUME_MODE = 3,   
           VALE = -2.86074,        
           REFERENCE = 'ANALYTIQUE',      ),

   _F(   RESULTAT = MODE1,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 4.5E-2,   
           NUME_MODE = 4,   
           VALE = 8.78382,        
           REFERENCE = 'ANALYTIQUE',      ),

   _F(   RESULTAT = MODE1,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.8E-2,   
           NUME_MODE = 5,   
           VALE = 14.5551,        
           REFERENCE = 'ANALYTIQUE',    ))

         )

MODE2=MODE_ITER_INV(       MATR_A=K_ASSE,
                                MATR_B=KS_ASSE,
                             TYPE_RESU='MODE_FLAMB',
                            CALC_FREQ=_F(  OPTION = 'PROCHE',     
                                        CHAR_CRIT = ( -13.7, -8.35,
                                            -2.88, 8.35, 13.7,),    
                                       ),
                            CALC_MODE=_F(  OPTION = 'RAYLEIGH',     )
                         )

TEST_RESU(RESU=(
   _F(   RESULTAT = MODE2,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.4E-2,   
           NUME_ORDRE = 1,  
           VALE = -14.4147,        
           REFERENCE = 'ANALYTIQUE',      ),

   _F(   RESULTAT = MODE2,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.8E-2,   
           NUME_ORDRE = 2,  
           VALE = -8.63207,        
           REFERENCE = 'ANALYTIQUE',       ),

   _F(   RESULTAT = MODE2,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.9E-2,   
           NUME_ORDRE = 3,  
           VALE = -2.86074,        
           REFERENCE = 'ANALYTIQUE',    ),

   _F(   RESULTAT = MODE2,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 4.5E-2,   
           NUME_ORDRE = 4,  
           VALE = 8.78382,        
           REFERENCE = 'ANALYTIQUE',     ),

   _F(   RESULTAT = MODE2,    
           PARA = 'CHAR_CRIT',        
           CRITERE = 'RELATIF',     
           PRECISION = 3.8E-2,   
           NUME_ORDRE = 5,  
           VALE = 14.5551,        
           REFERENCE = 'ANALYTIQUE',       ))

         )

FIN()
