DEBUT(CODE=_F(NOM='HSNV132D', NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM=('CALCULEL2_63'));

n = 51
MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURFINF',),
                                   _F(GROUP_MA='VOL',),
                                   ),
                    INFO=1);

#--------------------------------------------------------------------
#               DEBUT DU THERMIQUE                                  #
#--------------------------------------------------------------------

MOTH=AFFE_MODELE(MAILLAGE=MAILLAG1,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='THERMIQUE',
                         MODELISATION='3D',),);

MATH=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,
                           RHO_CP=0.0,),);

CMTH=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                   AFFE=_F(TOUT='OUI',
                           MATER=MATH,),);

CHTH=AFFE_CHAR_THER(MODELE=MOTH,
                    TEMP_IMPO=(_F(GROUP_MA='SURFGAU',TEMP=20.0,),
                               _F(GROUP_MA='SURFDRO',TEMP=220.0,),),);

THLI=THER_LINEAIRE(MODELE=MOTH,
                   CHAM_MATER=CMTH,
                   EXCIT=_F(CHARGE=CHTH,),);

TEMP=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='EXTR',
                RESULTAT=THLI,
                NOM_CHAM='TEMP',
                INST=0.0,);

#IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(CHAM_GD = TEMP,))   

#--------------------------------------------------------------------
#               FIN DU THERMIQUE                                    #
#--------------------------------------------------------------------



MODELE=AFFE_MODELE(MAILLAGE=MAILLAG1,
                   AFFE=(_F(GROUP_MA='VOL',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),),);

LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y-0.5');
LT=FORMULE(NOM_PARA=('X','Y'),VALE='-X+0.5');


FISS=DEFI_FISS_XFEM(MODELE=MODELE,  
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    INFO=1,
                    );


MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELE,FISSURE=FISS,INFO=2,);


ACIER=DEFI_MATERIAU(ELAS=_F(E=205000000000.0,
                            NU=0.3,
                            RHO=7800.0,
                            ALPHA=1.282E-05,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA='VOL',MATER=ACIER,),
                       AFFE_VARC=_F( GROUP_MA='VOL',
                                     NOM_VARC='TEMP',
                                     CHAM_GD=TEMP,
                                     VALE_REF=20.0),
                       );


#*********************************************************************
#                          CONDITIONS AUX LIMITES                    *
#*********************************************************************

CHARBLO=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=_F(GROUP_NO='SURFINF',DX=0.0,DY=0.0,DZ=0.0),
                       INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.0,
                                    NOMBRE=3,),);

UTOT=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=_F(CHARGE=CHARBLO,),
                   INST=1,)

#---------------post--------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT,
                        INFO          = 2)


IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(RESULTAT=RES_XFEM,NOM_CHAM='DEPL'))   


SIF=CALC_G(RESULTAT=UTOT,
           OPTION='CALC_K_G',
           THETA=_F(FISSURE=FISS,
                    R_INF=0.1,
                    R_SUP=0.2,),
           LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                      LISSAGE_G='LAGRANGE',),
           INFO=2,);


IMPR_TABLE(TABLE=SIF,);

# ces valeurs de reference ont ete obtenues par un calcul classique 3D
# avec les memes conditions aux limites, et un maillage 2*100*100

# on ne peut pas les obtenir avec la modelisation a (2d), ni c (3d avec DZ=0)


TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.040000000000000001,
           VALE_CALC= 6.65779388E+06,
           VALE_REFE=6.894100E6,
           NOM_PARA='K1',
           TABLE=SIF,
           FILTRE=(_F(NOM_PARA='NUM_PT',
                      VALE_I=1,),
                   _F(NOM_PARA='NUME_ORDRE',
                      VALE_I=1,),
                   ),
           )

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='AUTRE_ASTER',
           PRECISION=0.02,
           VALE_CALC= 3.03433598E+06,
           VALE_REFE=3.076700E6,
           NOM_PARA='K2',
           TABLE=SIF,
           FILTRE=(_F(NOM_PARA='NUM_PT',
                      VALE_I=1,),
                   _F(NOM_PARA='NUME_ORDRE',
                      VALE_I=1,),
                   ),
           )

# on peut s'etonner de trouver 4% d'erreur sur K1 alors que le maillage x-fem est fin (51*51)
# en fait, si on regarde les contributions de la thermique pour K, et la contribution non-thermique pour K, on a les resultats suivants:


# K1
#                non-thermique  +    thermique           =    total
# reference :       4.640e8           -4.5709e8          =   6.89e6
# x-fem     :       4.629e8           -4.562e8           =   6.66e6
# ecart     :        0.24%              0.19%                3.43%


FIN();
