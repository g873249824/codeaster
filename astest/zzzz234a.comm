   OPERATION='ASSE', TYPE_CHAM='ELGA_NEUT_R',
   MODELE=MO_D_T,  PROL_ZERO='OUI',
   ASSE=_F( GROUP_MA='POUD',   CHAM_GD = NELG10, NOM_CMP = ('N','VY','VZ'  ), NOM_CMP_RESU = ('X1','X2','X3'),), )

NELG12=CREA_CHAMP(
   OPERATION='AFFE', TYPE_CHAM='ELGA_NEUT_R',
