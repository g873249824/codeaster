TEST_RESU(CHAM_ELEM=_F(REFERENCE='ANALYTIQUE',
                       POINT=1,
                       NOM_CMP='N',
                       MAILLE='M1',
                       CHAM_GD=SELG01,
                       VALE_CALC=2000.0,
                       VALE_REFE=2000.0,),
          )

