FELG01=CREA_CHAMP(
   OPERATION='AFFE', TYPE_CHAM='ELGA_NEUT_F',
   MODELE=MO_D_T,  PROL_ZERO='OUI',   INFO=1,
   AFFE=_F( GROUP_MA='POUD', NOM_CMP = ('X1'), VALE_F = FFDEUX,),)

