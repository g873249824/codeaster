   ASSE=(_F( GROUP_MA='POUD', CHAM_GD = NELG11, CUMUL='OUI',COEF_R= 1.0),
         _F( GROUP_MA='POUD', CHAM_GD = NELG12, CUMUL='OUI',COEF_R= -1000.0),),)

VELG01=CREA_CHAMP(
