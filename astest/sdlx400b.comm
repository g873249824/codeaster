           PRECISION = 0.6E-2,
           NUME_ORDRE = 4,
           VALE = 286.5,       
           REFERENCE = 'ANALYTIQUE'),

   _F(   RESULTAT = MODE_A_2,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 0.8E-2,
           NUME_ORDRE = 6,
           VALE = 582.3,      
           REFERENCE = 'ANALYTIQUE'),

   _F(   RESULTAT = MODE_A_2,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 1.2E-2,
           NUME_ORDRE = 8,
           VALE = 990.7,       
           REFERENCE = 'ANALYTIQUE'),

   _F(   RESULTAT = MODE_A_2,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 3.7E-2,
           NUME_ORDRE = 10,
           VALE = 1509.6,      
           REFERENCE = 'ANALYTIQUE'))

        )

# CALCUL DES MODES PAR JACOBI  + BANDE

MODE_B_2=MODE_ITER_SIMULT(       MATR_RIGI=K_AS_2, 
                                   MATR_MASS=M_AS_2, 
                                  METHODE='JACOBI',
                                  PREC_BATHE=1.E-12,
                                  PREC_JACOBI=1.E-4,
                               CALC_FREQ=_F(  OPTION = 'BANDE',
                                           FREQ = ( 10.,  2000., )),
                                VERI_MODE=_F( SEUIL = 2.E-4) )


TEST_RESU(RESU=(
