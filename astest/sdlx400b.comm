           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 1.2E-2,
           NUME_MODE = 11,
           VALE = 988.2,     
           REFERENCE = 'ANALYTIQUE'),

   _F(   RESULTAT = MODE_B_1,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 3.7E-2,
           NUME_MODE = 13,
           VALE = 1507.9,     
           REFERENCE = 'ANALYTIQUE'))

        )

# APPUI ELASTIQUE

