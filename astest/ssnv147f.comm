DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',)

MA=DEFI_GROUP( reuse=MA,
               MAILLAGE=MA,
               CREA_GROUP_NO=_F(
                TOUT_GROUP_MA = 'OUI')
              )

MO=AFFE_MODELE(
                MAILLAGE=MA,
                AFFE=_F(
               GROUP_MA = ('BLOC', 'FIXE', 'TRAC',),
               PHENOMENE = 'MECANIQUE',
               MODELISATION = '3D')
             )
FC=DEFI_CONSTANTE(NOM_RESU='F_C',
                  VALE=38.3,);
FT1=DEFI_CONSTANTE(NOM_RESU='F_T',
                   VALE=4.0,);
FT2=DEFI_CONSTANTE(NOM_RESU='F_T',
                   VALE=3.0,);
BETA=DEFI_CONSTANTE(NOM_RESU='COEF_BIAX',
                    VALE=1.16,);
GC=DEFI_CONSTANTE(NOM_RESU='ENER_COMP_RUPT',
                  VALE=2.83E-2,);
GT=DEFI_CONSTANTE(NOM_RESU='ENER_TRAC_RUPT',
                  VALE=2.83E-4,);
CALCAIRE=DEFI_MATERIAU(BETON_DOUBLE_DP=_F(COEF_BIAX=BETA,
                                        F_T=FT1,
                                        ENER_TRAC_RUPT=GT,
                                        ECRO_TRAC_P_PIC='LINEAIRE',
                                        ENER_COMP_RUPT=GC,
                                        COEF_ELAS_COMP=33.3333,
                                        ECRO_COMP_P_PIC='LINEAIRE',
                                        F_C=FC,),
                     BPEL_BETON=_F(),
                     ELAS=_F(NU=0.22,
                             E=3.1000E4,),);
CAL_FAIB=DEFI_MATERIAU(BETON_DOUBLE_DP=_F(COEF_BIAX=BETA,
                                        F_T=FT2,
                                        ENER_TRAC_RUPT=GT,
                                        ECRO_TRAC_P_PIC='LINEAIRE',
                                        ENER_COMP_RUPT=GC,
                                        COEF_ELAS_COMP=33.3333,
                                        ECRO_COMP_P_PIC='LINEAIRE',
                                        F_C=FC,),
                     BPEL_BETON=_F(),
                     ELAS=_F(NU=0.22,
                             E=3.1000E4,),);

CHMAT=AFFE_MATERIAU(
                MAILLAGE=MA,AFFE=(
                    _F(
                GROUP_MA = 'BLOC',
                MATER = CALCAIRE), _F(
                MAILLE = 'M1',
                MATER = CAL_FAIB))
              )

L_INST=DEFI_LIST_REEL(
               DEBUT=0.0,INTERVALLE=(

                _F(  JUSQU_A = 1.,       NOMBRE = 1),
                _F(  JUSQU_A = 15.,     NOMBRE = 280))
              )

LIAISON=AFFE_CHAR_MECA(
               MODELE=MO,

                FACE_IMPO=_F( GROUP_MA = 'FIXE',  DX = 0., DY = 0., DZ = 0.)
              )

TRACTION=AFFE_CHAR_MECA(
                      MODELE=MO,
               FORCE_FACE=_F( GROUP_MA = 'TRAC', FZ = 1.)
              )

RAMPE=DEFI_FONCTION(
              NOM_PARA='INST',
                  VALE=(0., 0.,
                        1., 1.,)
             )

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = L_INST),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS      = 4,
                                 SUBD_PAS_MINI = 1e-6,),)

REPONSE=STAT_NON_LINE(
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, FONC_MULT = RAMPE)),
                  COMPORTEMENT=_F(
                RELATION = 'BETON_DOUBLE_DP'),

                INCREMENT=_F(
                LIST_INST = DEFLIST,
                INST_FIN = 1.),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50)
             )

REPONSE=STAT_NON_LINE( reuse=REPONSE,
                  ETAT_INIT=_F(
                EVOL_NOLI = REPONSE),
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, TYPE_CHARGE = 'FIXE_PILO')),
                  COMPORTEMENT=_F(
                RELATION = 'BETON_DOUBLE_DP'),

                INCREMENT=_F(
                LIST_INST = DEFLIST,
                INST_FIN = 3.,),
               PILOTAGE=_F(
                TYPE = 'PRED_ELAS',
                ETA_PILO_MIN = 0.50),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 10)
             )


TEST_RESU(RESU=(_F(NUME_ORDRE=41,
                   GROUP_NO='E',
                   RESULTAT=REPONSE,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=1.08163518E-4,),
                _F(NUME_ORDRE=41,
                   POINT=1,
                   RESULTAT=REPONSE,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIZZ',
                   VALE_CALC=2.94149327,
                   MAILLE='M1',),
                ),
          )

FIN()
