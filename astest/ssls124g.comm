                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB',),);


CHARG32=AFFE_CHAR_MECA(MODELE=MODMEC32,
                      DDL_IMPO=(_F(GROUP_MA=('SUENCAS'),
                                   DX=0.0,),
                                _F(GROUP_MA='LI15',
                                   DY=0.0,),
                                _F(GROUP_MA='LI21',
                                   DZ=0.0,),),
                      FORCE_ARETE=(_F(GROUP_MA='LI87',
                                      FZ=F/b,),),
                           );

RESU32=MECA_STATIQUE(MODELE=MODMEC32,
                    CHAM_MATER=CHMAT32,
                    EXCIT=_F(CHARGE=CHARG32,),
                    );

Iy=b*h*h*h/12
Uz=F*L*L*L/3/YG/Iy


TEST_RESU(RESU=(
            _F(  RESULTAT = RESU32, NOM_CHAM='DEPL',  REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = 1.861E-06),

            _F(   RESULTAT = RESU32, NOM_CHAM='DEPL', REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = 1.863E-06),
              ),)

TEST_RESU(RESU=(
            _F(  RESULTAT = RESU32, NOM_CHAM='DEPL', PRECISION = 7.E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = Uz),

            _F(   RESULTAT = RESU32, NOM_CHAM='DEPL',  PRECISION = 7.E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = Uz),
              ),)



# CAS 3 :

h=0.1


PRE_GIBI(UNITE_GIBI=23,UNITE_MAILLAGE=33);

MAIL33=LIRE_MAILLAGE(UNITE=33);

MAIL33=MODI_MAILLAGE(reuse=MAIL33,MAILLAGE=MAIL33,
ORIE_SHB=_F(GROUP_MA='GEOTOT'),)

MAIL33=DEFI_GROUP(reuse =MAIL33, MAILLAGE=MAIL33,
                CREA_GROUP_NO=(_F(GROUP_MA='SUENCAS',),
                               _F(GROUP_MA='SUDEPIM',),
                               _F(GROUP_MA='LI15',),
                               _F(GROUP_MA='LI21',),
                               _F(GROUP_MA='LI87',),),);

CHMAT33=AFFE_MATERIAU(MAILLAGE=MAIL33,
                  AFFE=_F(TOUT='OUI',MATER=MAT,),);

MODMEC33=AFFE_MODELE(MAILLAGE=MAIL33,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB',),);


CHARG33=AFFE_CHAR_MECA(MODELE=MODMEC33,
                      DDL_IMPO=(_F(GROUP_MA=('SUENCAS'),
                                   DX=0.0,),
                                _F(GROUP_MA='LI15',
                                   DY=0.0,),
                                _F(GROUP_MA='LI21',
                                   DZ=0.0,),),
                      FORCE_ARETE=(_F(GROUP_MA='LI87',
                                      FZ=F/b,),),
                           );

RESU33=MECA_STATIQUE(MODELE=MODMEC33,
                    CHAM_MATER=CHMAT33,
                    EXCIT=_F(CHARGE=CHARG33,),
                    SOLVEUR=_F(METHODE ='MULT_FRONT',NPREC=11 ),
                    );

Iy=b*h*h*h/12
Uz=F*L*L*L/3/YG/Iy

TEST_RESU(RESU=(
            _F(  RESULTAT = RESU33, NOM_CHAM='DEPL',  REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = 1.853E-03),

            _F(   RESULTAT = RESU33, NOM_CHAM='DEPL', REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = 1.854E-03),
              ),)

TEST_RESU(RESU=(
            _F(  RESULTAT = RESU33, NOM_CHAM='DEPL', PRECISION = 7.4E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = Uz),

            _F(   RESULTAT = RESU33, NOM_CHAM='DEPL',  PRECISION = 7.3E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = Uz),
              ),)


FIN()

