DEBUT(CODE=_F(NOM='SSLS124G', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

YG=2.E11
L=100.
b=10.
F=1.

MAT=DEFI_MATERIAU(ELAS=_F(E=YG,
                          NU=0.3,),);



# CAS 1 :

h=10.


PRE_GIBI(UNITE_GIBI=21,UNITE_MAILLAGE=31);

MAIL31=LIRE_MAILLAGE(UNITE=31);

MAIL31=MODI_MAILLAGE(reuse=MAIL31,MAILLAGE=MAIL31,
ORIE_SHB=_F(GROUP_MA='GEOTOT'),)

MAIL31=DEFI_GROUP(reuse =MAIL31, MAILLAGE=MAIL31,
                CREA_GROUP_NO=(_F(GROUP_MA='SUENCAS',),
                               _F(GROUP_MA='SUDEPIM',),
                               _F(GROUP_MA='LI15',),
                               _F(GROUP_MA='LI21',),
                               _F(GROUP_MA='LI87',),),);

CHMAT31=AFFE_MATERIAU(MAILLAGE=MAIL31,
                  AFFE=_F(TOUT='OUI',MATER=MAT,),);

MODMEC31=AFFE_MODELE(MAILLAGE=MAIL31,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB',),);


CHARG31=AFFE_CHAR_MECA(MODELE=MODMEC31,
                      DDL_IMPO=(_F(GROUP_MA=('SUENCAS'),
                                   DX=0.0,),
                                _F(GROUP_MA='LI15',
                                   DY=0.0,),
                                _F(GROUP_MA='LI21',
                                   DZ=0.0,),),
                      FORCE_ARETE=(_F(GROUP_MA='LI87',
                                      FZ=F/b,),),
                           );

RESU31=MECA_STATIQUE(MODELE=MODMEC31,
                    CHAM_MATER=CHMAT31,
                    EXCIT=_F(CHARGE=CHARG31,),
                    );

RESU31=CALC_ELEM(reuse=RESU31, RESULTAT=RESU31,
                    OPTION=('SIGM_ELNO',),
                    );



Iy=b*h*h*h/12
Uz=F*L*L*L/3/YG/Iy
# point de gauss maille m1 situe a x=5
moment1=(L-5)*F
# coordonnees des points de Gauss
z1=0.90618/2*h
z2=0.5385/2*h
z3=0
z4=-0.5385/2*h
z5=-0.90618/2*h

Sig1=moment1*z1/Iy
Sig2=moment1*z2/Iy
Sig3=moment1*z3/Iy
Sig4=moment1*z4/Iy
Sig5=moment1*z5/Iy


TEST_RESU(RESU=(
            _F(  RESULTAT = RESU31, NOM_CHAM='DEPL', REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = 1.958E-09),

            _F(   RESULTAT = RESU31, NOM_CHAM='DEPL', REFERENCE='NON_REGRESSION',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = 1.965E-09),
              ),)

TEST_RESU(RESU=(
            _F(  RESULTAT = RESU31, NOM_CHAM='DEPL', PRECISION = 2.1E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P7',      NOM_CMP = 'DZ',           VALE = Uz),

            _F(   RESULTAT = RESU31, NOM_CHAM='DEPL', PRECISION = 2.E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'P8',      NOM_CMP = 'DZ',           VALE = Uz),
              ),)



# CAS 2 :

h=1.0


PRE_GIBI(UNITE_GIBI=22,UNITE_MAILLAGE=32);

MAIL32=LIRE_MAILLAGE(UNITE=32);

MAIL32=MODI_MAILLAGE(reuse=MAIL32,MAILLAGE=MAIL32,
ORIE_SHB=_F(GROUP_MA='GEOTOT'),)

MAIL32=DEFI_GROUP(reuse =MAIL32, MAILLAGE=MAIL32,
                CREA_GROUP_NO=(_F(GROUP_MA='SUENCAS',),
                               _F(GROUP_MA='SUDEPIM',),
                               _F(GROUP_MA='LI15',),
                               _F(GROUP_MA='LI21',),
                               _F(GROUP_MA='LI87',),),);

CHMAT32=AFFE_MATERIAU(MAILLAGE=MAIL32,
                  AFFE=_F(TOUT='OUI',MATER=MAT,),);

