DEBUT(CODE=_F(NOM='SSNV186C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                        LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='VOLQUAD',
                                     GROUP_MA='VOL',),);

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_XFEM',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-17.5');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-100. ');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    DEFI_FISS=_F(
                                 FONC_LT=LT,
                                 FONC_LN=LN,
                                 ),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI=(2.5 ,0. ,2.5 ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(-0.5,0.,1.5 ),),
                    CONTACT=_F(INTEGRATION='FPG4',
                               COEF_REGU_CONT=100.,
                               ITER_CONT_MAXI=4,
                               CONTACT_INIT='OUI',
                               ALGO_LAGR='VERSION1',
                               COEF_ECHELLE=1.E11,
#                               FROTTEMENT='SANS',
                               FROTTEMENT='COULOMB',
                               COULOMB=1.0,
                               ITER_FROT_MAXI=6,
                               COEF_REGU_FROT=1000.,
#                               SEUIL_INIT=-1.E9,
                               ),
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         INFO=1,);

E=1.0E11
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('VOL','SURFINF','SURFSUP',),
                                MATER=ACIER,
                                TEMP_REF=0.0,),);

PRES=100.
FCTPRES=FORMULE(NOM_PARA=('X','Y','Z'),VALE='(PRES-(Y-10.)*(Y-10.)/2.)*E/1e6');

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SURFINF',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(GROUP_MA='SURFSUP',DX=0.0,DY=0.0,),),
                    INFO=1,);

CHF=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     PRES_REP=_F(GROUP_MA='SURFSUP',
                                 PRES=FCTPRES,),
                      );

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                                INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CH1,),
                          _F(CHARGE=CHF,),
                          _F(CHARGE=CHXFEM,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   SOLVEUR=_F(METHODE='MUMPS',
                              ),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                             GROUP_NO='VOLQUAD',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2',),
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=LAG,);


# CETTE VALEUR A ETE OBTENUE PAR LA MODELISATION A
LAREF=-9.52844E-05

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           FILTRE=(_F(NOM_PARA='COOR_X',
                      CRIT_COMP='EQ',
                      VALE=0.,),
                   _F(NOM_PARA='COOR_Y',
                      CRIT_COMP='EQ',
                      VALE=10.,),
                   _F(NOM_PARA='COOR_Z',
                     CRIT_COMP='EQ',
                     VALE=17.5,),
                  ),
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=0.002,
           REFERENCE='AUTRE_ASTER',);

# LA2REF EST LA SOLUTION ANALYTIQUE (PAS DE FROTTEMENT AU POINT MILIEU)
LA2REF=0.

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           FILTRE=(_F(NOM_PARA='COOR_X',
                      CRIT_COMP='EQ',
                      VALE=0.,),
                   _F(NOM_PARA='COOR_Y',
                      CRIT_COMP='EQ',
                      VALE=10.,),
                   _F(NOM_PARA='COOR_Z',
                     CRIT_COMP='EQ',
                     VALE=17.5,),
                  ),
           VALE=LA2REF,
           CRITERE='ABSOLU',
           PRECISION=5.E-3,
           REFERENCE='ANALYTIQUE',);

MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK)

DEPL=POST_CHAM_XFEM(MAILLAGE=MA_XFEM,
                    MODELE=MODELEK,
                    NOM_CHAM='DEPL',
                    RESULTAT=UTOT1)


IMPR_RESU(FORMAT='IDEAS',RESU=_F(MAILLAGE=MA_XFEM,RESULTAT=DEPL))

FIN();

