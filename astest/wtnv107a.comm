# SURFACE POREUSE SATUREE SOUMISE A UN CHOC THERMO_HYDRAULIQUE

DEBUT(  CODE=_F( NOM = 'WTNV107A',NIV_PUB_WEB='INTERNET'))

# MODELISATION A : CALCUL SUR 20 S, MODELISATION 3D_JOINT_CT



PRE_GIBI()

MAIL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),)

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(


                      _F( GROUP_MA = 'L1'),
                      _F( GROUP_MA = 'L2'),
                      _F( GROUP_MA = 'BAS'),
                      _F( GROUP_MA = 'HAUT'))
            #        CREA_GROUP_NO : (GROUP_MA : COT)

                   )

MODELE=AFFE_MODELE(   MAILLAGE=MAIL,
                        AFFE=_F(  GROUP_MA = 'MAIL',
                      PHENOMENE = 'MECANIQUE',
                      MODELISATION = '3D_JOINT_CT')


                     )

LI=DEFI_LIST_REEL(   DEBUT=1.,
                       INTERVALLE=_F(  JUSQU_A = 20., NOMBRE = 1)
                     )

MATER1=DEFI_MATERIAU(   PORO_JOINT=_F( 
                     C_0 = 3.3333333333333E-10,
                     ALPHA_M = 1.E-12,
                     BIOT_M = 3.E9,
                     LAMBDA_T = 3.E-8,
                     LAMBDA_H = 3.E-8,
                     SOURCE_INIT = 0.,
                     OMEGA_0 = 0.,
                     RHO_FLUI = 1.E3,
                     T_R = 1.,
                     ENTRO_FLUI = 10.)
                        )

CHMAT0=AFFE_MATERIAU(   MAILLAGE=MAIL,AFFE=(

                          _F( GROUP_MA = 'A1',
                        MATER = MATER1),
                          _F( GROUP_MA = 'A2',
                        MATER = MATER1))
                        )

CHARG0=AFFE_CHAR_MECA(    MODELE=MODELE,
                            PESANTEUR=( 10., 1., 1., 1.,)
                         )

CHAR0=AFFE_CHAR_MECA(   MODELE=MODELE,FLUX_THM_REP=(

               _F(  GROUP_MA = 'HAUT',
                              FLUN = 0.),

               _F(  GROUP_MA = 'BAS',
                              FLUN = 0.),
               _F(  GROUP_MA = 'L2',
                              FLUN = 0.)),


                DDL_IMPO=_F(  GROUP_NO = 'L1',
                           PRES = 10.,
                           TEMP = 10.)


  )

MESTAT=STAT_NON_LINE( 
                            MODELE=MODELE,
                           SOLVEUR=_F( 
                METHODE = 'LDLT'),
                        CHAM_MATER=CHMAT0,
                             EXCIT=_F( 
                CHARGE = CHAR0),
                         COMP_ELAS=_F( 
                RELATION = 'ELAS'),
                         INCREMENT=_F( 
                LIST_INST = LI,
                NUME_INST_FIN = 1),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F( 
                ITER_GLOB_MAXI = 5,
                ITER_INTE_MAXI = 5,
                RESI_GLOB_RELA = 1.E-6),
                        PARM_THETA=1.
              )

TEST_RESU(RESU=(
  _F( RESULTAT = MESTAT, NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
 NOEUD = 'N22',  NOM_CMP = 'TEMP', VALE = 9.74,       PRECISION = 1.0E-3),
  _F( RESULTAT = MESTAT, NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
 NOEUD = 'N16',  NOM_CMP = 'PRES', VALE = 9.76,       PRECISION = 1.0E-3),
  _F( RESULTAT = MESTAT, NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
 NOEUD = 'N10',  NOM_CMP = 'TEMP', VALE = 9.81,       PRECISION = 1.0E-3),
  _F( RESULTAT = MESTAT, NUME_ORDRE = 1, NOM_CHAM = 'DEPL',
 NOEUD = 'N4',  NOM_CMP = 'PRES', VALE = 9.89,       PRECISION = 1.0E-3))

 )

FIN()
