                     VALE_I=1,),
           )

TEST_TABLE(
           VALE_CALC=0.36024062057934,

           NOM_PARA='G',
           TABLE=G2,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

TEST_TABLE(
           VALE_CALC=0.36022087398746,

           NOM_PARA='G',
           TABLE=G3,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

TEST_TABLE(
           VALE_CALC=0.36021835942526,

           NOM_PARA='G',
           TABLE=G4,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
