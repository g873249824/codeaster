DEBUT(CODE=_F(NOM='SSNV195C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

MAILLE=LIRE_MAILLAGE(FORMAT='MED',INFO=1);

MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'H',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,50.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'B',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,0.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'M',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,25.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),   
                            )    
           );


MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'face_hau',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'H',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_mil',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'M',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_bas',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'B',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
#                              _F(GROUP_MA='VOL'),                            
                            )    
           );


MAILLAGE=CREA_MAILLAGE(MAILLAGE=MAILLE,COPIE=_F(),
                       );

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL'),   
                    );

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAGE,
                       LINE_QUAD=_F(GROUP_MA='VOL'),
);
                       
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                   AFFE=_F(GROUP_MA=('VOL'),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D_XFEM_CONT'),
                     );        

PENTE1=0.57735
PENTE2=0.57735
#B1=12.5-2.5*PENTE1
B1=12.-2.*PENTE1
B2=37.5-2.5*PENTE2

LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-PENTE1*Y-B1');

LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-PENTE2*Y-B2');

     

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN1));

                    
FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN2));
                    

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='P1P1A',                         
                        );
                        

CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      FROTTEMENT     = 'COULOMB',
                      ITER_CONT_MAXI = 2, 
                      ITER_FROT_MAXI = 6,                    
                      REAC_GEOM      = 'SANS',
                      ZONE=(
                            _F(
                               FISS_MAIT    = FISS1,
                               INTEGRATION  = 'GAUSS',
                               CONTACT_INIT = 'OUI',
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,   
                               COULOMB      = 1.0,
                               SEUIL_INIT   = -2.E11,   
                               ALGO_FROT    = 'STANDARD',
                               COEF_FROT    = 1000.), 
                            _F(
                               FISS_MAIT    = FISS2,
                               INTEGRATION  = 'GAUSS',
                               CONTACT_INIT = 'OUI',
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,   
                               COULOMB      = 1.0,
                               SEUIL_INIT   = -2.E11,   
                               ALGO_FROT    = 'STANDARD',
                               COEF_FROT    = 1000.,        
                             ),                              
                         ),
                   );

E=100.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL'),
                                MATER=ACIER,
                                ),
                             ));
ZERO=0.0
DEPX=1.E-3
DEPY=2.E-3
DEPZ=3.E-3

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(GROUP_NO='face_mil',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(GROUP_NO='face_hau',
                                    DX=ZERO,
                                    DY=ZERO,   
                                    DZ=DEPZ),
                                 _F(GROUP_NO='face_bas',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=DEPZ,
                                    ),
                                 ),
                       INFO=1);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CH1),                    
                          _F(CHARGE=CHXFEM)), 
                   CONTACT  = CTXFEM,                        
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL'),
                   INCREMENT=_F(LIST_INST=L_INST),
                   SOLVEUR=_F(METHODE='MUMPS',
                               ),
                   NEWTON=_F(REAC_ITER=1),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                   INFO=2);


# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAGE,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D')) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAGE,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)
 
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));


# -------------------------------------------------------------   
#            TEST_TABLE
# -------------------------------------------------------------   


DEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU'),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION'));

IMPR_TABLE(TABLE=DEP,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           IMPR_FONCTION='NON');

# POUR LES DEUX FISSURES

TEST_TABLE(TABLE=DEP,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');


# POUR LA PREMIERE FISSURE

LAREF=-9.E3
LA1REF=0.
LA2REF=0.57735

LAG = CALC_TABLE(TABLE = DEP,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'COOR_Z',
                               CRIT_COMP = 'LT',
                               VALE      = 25)));

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-6,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-6,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MAX',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MIN',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MAX',
           VALE=LA2REF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MAX',
           VALE=LA2REF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE'); 


# POUR LA DEUXIEME FISSURE

DEP2P = CALC_TABLE(TABLE = DEP,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^P]'),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'COOR_Z',
                               CRIT_COMP = 'GT',
                               VALE      = 25)));

TEST_TABLE(TABLE=DEP2P,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP2P,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           VALE=ZERO,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE');

DEP2M = CALC_TABLE(TABLE = DEP,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^M]'),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'COOR_Z',
                               CRIT_COMP = 'GT',
                               VALE      = 25)));

TEST_TABLE(TABLE=DEP2M,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           VALE=DEPZ,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE');

TEST_TABLE(TABLE=DEP2M,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           VALE=DEPZ,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE');

FIN();
