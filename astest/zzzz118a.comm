# PLAQUE RECTANGULAIRE MINCE ENCASTREE SOUMISE
# A UN GRADIENT THERMIQUE DANS L'EPAISSEUR
# ET MODELISEE EN DKT - DST - Q4G
# TEST DE NON REGRESSION INCLUDE_MATERIAU
#====================================================================

DEBUT(CODE=_F(NIV_PUB_WEB='INTRANET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(GROUP_MA='AB',),
                             _F(GROUP_MA='BC',),
                             _F(GROUP_MA='CD',),
                             _F(GROUP_MA='DA',),),);

MOD=AFFE_MODELE(MAILLAGE=MA,
                AFFE=(_F(GROUP_MA='STRIA1',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKT',),
                      _F(GROUP_MA='STRIA2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKT',),
                      _F(GROUP_MA='SQUA11',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKT',),
                      _F(GROUP_MA='SQUA12',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKT',),
                      _F(GROUP_MA='SQUA2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKT',),),);

CARA=AFFE_CARA_ELEM(MODELE=MOD,
                    COQUE=_F(GROUP_MA=('STRIA1','STRIA2','SQUA',),
                             EPAIS=0.01,),);

TEMP0=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=MA,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP=('TEMP_MIL','TEMP_INF','TEMP_SUP',),
                        VALE=(20.0,20.0,20.0,),),);
TEMP1=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=MA,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP=('TEMP_MIL','TEMP_INF','TEMP_SUP',),
                        VALE=(50.0,20.0,100.0,),),);

TEMP=CREA_RESU(  OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD=TEMP0,
                         INST=0.0,),);

TEMP=CREA_RESU(  reuse=TEMP,
                 OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD=TEMP1,
                         INST=1.0,),);

TEMPER1=CREA_RESU(  OPERATION='PREP_VRC2', TYPE_RESU='EVOL_THER',
    PREP_VRC2= _F(  MODELE=MOD, CARA_ELEM=CARA, EVOL_THER = TEMP, ),)


CHAR=AFFE_CHAR_MECA(MODELE=MOD,
                    DDL_IMPO=_F(GROUP_NO=('BC','CD','DA',),
                                DX=0.0,
                                DY=0.0,
                                DZ=0.0,
                                DRX=0.0,
                                DRY=0.0,
                                DRZ=0.0,),
                    PRES_REP=_F(GROUP_MA='STOT',
                                PRES=-100,),);

ZLIST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);
#====================================================================
# DEFINITION DU MATERIAU DANS LE FICHIER .COMM AVEC TEMP_DEF_ALPHA
# E EN FONCTION DE LA TEMPERATURE

YOUN=DEFI_FONCTION(NOM_PARA='TEMP',
                   VALE=
                   (0.0,205000000000.0,20.0,204000000000.0,50.0,203000000000.0,100.0,200000000000.0,150.0,
                          197000000000.0,200.0,193000000000.0,250.0,189000000000.0,300.0,185000000000.0,350.0,180000000000.0,
                          400.0,176000000000.0,450.0,171000000000.0,500.0,166000000000.0,550.0,160000000000.0,600.0,
                          155000000000.0,),
                   PROL_DROITE='EXCLU',
                   PROL_GAUCHE='EXCLU',);
#
# NU EN FONCTION DE LA TEMPERATURE

NU=DEFI_FONCTION(NOM_PARA='TEMP',
                 VALE=(0.0,0.3,),
                 PROL_DROITE='CONSTANT',
                 PROL_GAUCHE='CONSTANT',);
#
# ALPHA EN FONCTION DE LA TEMPERATURE

ALPH=DEFI_FONCTION(NOM_PARA='TEMP',
                   VALE=
                   (20.0,1.122e-05,50.0,1.145e-05,100.0,1.179e-05,150.0,1.214e-05,200.0,1.247e-05,250.0,
                          1.278e-05,300.0,1.308e-05,350.0,1.34e-05,400.0,1.372e-05,450.0,1.402e-05,),
                   PROL_DROITE='EXCLU',
                   PROL_GAUCHE='EXCLU',);

MAT1=DEFI_MATERIAU(ELAS_FO=_F(E=YOUN,
                              NU=NU,
                              TEMP_DEF_ALPHA=20.0,
                              ALPHA=ALPH,),);

CHMAT1=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(GROUP_MA=('STRIA1','STRIA2','SQUA',),
                             MATER=MAT1,
                             ),
                     AFFE_VARC=  _F(TOUT='OUI',EVOL=TEMPER1,NOM_VARC='TEMP',
                                     NOM_CHAM='TEMP',VALE_REF=20.0), )

MESTAT1=STAT_NON_LINE(MODELE=MOD,
                      CHAM_MATER=CHMAT1,
                      CARA_ELEM=CARA,
                      EXCIT=_F(CHARGE=CHAR,),
                      COMPORTEMENT=_F(RELATION='ELAS',),
                      INCREMENT=_F(LIST_INST=ZLIST,),);

MESTAT1=CALC_CHAMP(reuse =MESTAT1,
                  RESULTAT=MESTAT1,
                   CONTRAINTE=('EFGE_ELNO'),
                  );

#====================================================================
# DEFINITION DU MATERIAU DANS LE FICHIER .COMM SANS TEMP_DEF_ALPHA

MAT2=DEFI_MATERIAU(ELAS_FO=_F(E=YOUN,
                              NU=NU,
                              TEMP_DEF_ALPHA=20.0,
                              ALPHA=ALPH,),);

CHMAT2=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(GROUP_MA=('STRIA1','STRIA2','SQUA',),
                             MATER=MAT2,
                             ),
                     AFFE_VARC=  _F(TOUT='OUI',EVOL=TEMPER1,NOM_VARC='TEMP',
                                     NOM_CHAM='TEMP',VALE_REF=20.0), )

MESTAT2=STAT_NON_LINE(MODELE=MOD,
                      CHAM_MATER=CHMAT2,
                      CARA_ELEM=CARA,
                      EXCIT=_F(CHARGE=CHAR,),
                      COMPORTEMENT=_F(RELATION='ELAS',),
                      INCREMENT=_F(LIST_INST=ZLIST,),);

MESTAT2=CALC_CHAMP(reuse =MESTAT2,
                  RESULTAT=MESTAT2,
                   CONTRAINTE=('EFGE_ELNO'),
                  );
#====================================================================
# DEFINITION DU MATERIAU PAR INCLUDE_MATERIAU
MAT3=INCLUDE_MATERIAU(NOM_AFNOR='18MND5',
                      TYPE_MODELE='REF',
                      VARIANTE='A',
                      TYPE_VALE='NOMI',
                      INFO=2,);

CHMAT3=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(GROUP_MA=('STRIA1','STRIA2','SQUA',),
                             MATER=MAT3,
                             ),
                     AFFE_VARC=  _F(TOUT='OUI',EVOL=TEMPER1,NOM_VARC='TEMP',
                                     NOM_CHAM='TEMP',VALE_REF=20.0), )

MESTAT3=STAT_NON_LINE(MODELE=MOD,
                      CHAM_MATER=CHMAT3,
                      CARA_ELEM=CARA,
                      EXCIT=_F(CHARGE=CHAR,),
                      COMPORTEMENT=_F(RELATION='ELAS',),
                      INCREMENT=_F(LIST_INST=ZLIST,),);

MESTAT3=CALC_CHAMP(reuse =MESTAT3,
                  RESULTAT=MESTAT3,
                   CONTRAINTE=('EFGE_ELNO'),
                  );
#====================================================================

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   RESULTAT=MESTAT1,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N104',
                   NOM_CMP='NXX',
                   VALE_CALC=-3.5917320600000001E5,

                   CRITERE='RELATIF',
                   MAILLE='M98',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=MESTAT1,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N104',
                   NOM_CMP='NYY',
                   VALE_CALC=-1.5154346335E6,

                   CRITERE='RELATIF',
                   MAILLE='M98',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=MESTAT2,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N104',
                   NOM_CMP='NXX',
                   VALE_CALC=-3.5917320600000001E5,

                   CRITERE='RELATIF',
                   MAILLE='M98',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=MESTAT2,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N104',
                   NOM_CMP='NYY',
                   VALE_CALC=-1.5154346335E6,

                   CRITERE='RELATIF',
                   MAILLE='M98',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=MESTAT3,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N104',
                   NOM_CMP='NXX',
                   VALE_CALC=-3.5917320600000001E5,

                   CRITERE='RELATIF',
                   MAILLE='M98',
                   ),
                _F(NUME_ORDRE=1,
                   RESULTAT=MESTAT3,
                   NOM_CHAM='EFGE_ELNO',
                   NOEUD='N104',
                   NOM_CMP='NYY',
                   VALE_CALC=-1.5154346335E6,

                   CRITERE='RELATIF',
                   MAILLE='M98',
                   ),
                ),
          )

#----------------------------------------------------------------

FIN();
#
