#CAS_TEST__: SDLL04A
# SDLL04/A                 COMMANDES                             5/03/91
# POUTRE ELANCEE SUR 2 APPUIS COUPLEE A UN SYSTEME DISCRET MASSE-RESSORT
# 3 CALCULS: LAMBDA= 0 ; 0.001 ; 0.01               REF: SFM.VPCS SDLL04
# MODELISATION POU_D_T  DIS_T  --
# NOMS DES NOEUDS:   A=N1    B=N21    C=N22    D=N6
#=======================================================================


DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE( )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.0E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
          AFFE=_F(  GROUP_MA = 'MPOUTRE',     MATER = MAT)   )

#*********************************************************************
#
#   CALCUL 1         LAMBDA = 0.
#                    ==> SANS MASSE-RESSORT
#                      = CAS D'UME POUTRE POSEE-POSEE
#*********************************************************************

MODP=AFFE_MODELE(  MAILLAGE=MA,
         AFFE=_F(  PHENOMENE = 'MECANIQUE',
                GROUP_MA = 'MPOUTRE',    MODELISATION = 'POU_D_T') )

CHP=AFFE_CHAR_MECA(  MODELE=MODP,DDL_IMPO=(
        _F(  GROUP_NO = 'NPOUTRE',   DZ = 0., DRX = 0., DRY = 0.),
                 _F(  GROUP_NO = 'A',    DX = 0., DY = 0.),
                 _F(  GROUP_NO = 'B',            DY = 0.)) )

CARLEMP=AFFE_CARA_ELEM(   MODELE=MODP,
            POUTRE=_F(  GROUP_MA = 'MPOUTRE',      SECTION = 'GENERALE',
              CARA = ( 'A',  'IY',   'IZ',    'JX',  'AY',  'AZ', ),
              VALE = ( 0.01,   1.,   0.39E-5,   1.,   1.2,   1.2,  )) )

#--------------------------------------

MELRP=CALC_MATR_ELEM(  MODELE=MODP,
                         CHARGE=CHP,
                        OPTION='RIGI_MECA',
                         CHAM_MATER=CHMAT,
                          CARA_ELEM=CARLEMP   )

MELMP=CALC_MATR_ELEM(  MODELE=MODP,
                         CHARGE=CHP,
                        OPTION='MASS_MECA',
                         CHAM_MATER=CHMAT,
                          CARA_ELEM=CARLEMP   )

NUMP=NUME_DDL(  MATR_RIGI=MELRP  )

MATASSRP=ASSE_MATRICE(MATR_ELEM=MELRP,NUME_DDL=NUMP)

MATASSMP=ASSE_MATRICE(MATR_ELEM=MELMP,NUME_DDL=NUMP)

#--------- CALCUL MODAL

MDP=CALC_MODES(MATR_RIGI=MATASSRP,
               OPTION='PLUS_PETITE',
               CALC_FREQ=_F(NMAX_FREQ=2,
                            ),
               MATR_MASS=MATASSMP,
               SOLVEUR_MODAL=_F(METHODE='TRI_DIAG',
                                ),
               )


#*********************************************************************
#
#   CALCULS 2 ET 3    LAMBDA = 0.001 ET 0.01
#                     ==>  AVEC MASSE-RESSORT
#*********************************************************************


#--------- DEFINITIONS COMMUNES

MODM=AFFE_MODELE(  MAILLAGE=MA,AFFE=(
         _F(  PHENOMENE = 'MECANIQUE',
                GROUP_MA = 'MPOUTRE',  MODELISATION = 'POU_D_T'),
              _F(  MAILLE = ('CD','MC',),    MODELISATION = 'DIS_T',
              PHENOMENE = 'MECANIQUE')) )

CHM=AFFE_CHAR_MECA(  MODELE=MODM,DDL_IMPO=(
        _F(  GROUP_NO = 'NPOUTRE',   DZ = 0., DRX = 0., DRY = 0.),
                 _F(  GROUP_NO = 'A',         DX = 0., DY = 0.),
                 _F(  GROUP_NO = 'B',                 DY = 0.),
                 _F(  GROUP_NO = 'C',         DX = 0., DZ = 0.)) )

#---------------------------------------------------------------------

#                     CALCUL 2         LAMBDA = 0.001

#---------------------------------------------------------------------

CARLEM2=AFFE_CARA_ELEM(   MODELE=MODM,

            #------ POUTRE AB
            POUTRE=_F(  GROUP_MA = 'MPOUTRE',        SECTION = 'GENERALE',
                        CARA = (  'A', 'IY',   'IZ',    'JX',  'AY',  'AZ', ),
                        VALE = ( 0.01,   1.,   0.39E-5,   1.,   1.2,   1.2,  )),

            #------ POINT C   MASSE + RAIDEUR
            DISCRET=( _F(  MAILLE = 'MC',     CARA = 'M_T_D_N',
                                     #          M
                                     VALE =    0.78,            ),
                      _F(  MAILLE = 'MC',     CARA = 'K_T_D_N',
                                     #         K
                                     VALE =   ( 0.,0.,0.,),            ),
                      _F(  MAILLE = 'CD',     CARA = 'K_T_D_L',
                                     #       KX     KY       KZ
                                     VALE = ( 0.,   75.9791,  0., ),),),  )

#--------------------------------------

MELR2=CALC_MATR_ELEM(  MODELE=MODM,
                         CHARGE=CHM,
                        OPTION='RIGI_MECA',
                         CHAM_MATER=CHMAT,
                          CARA_ELEM=CARLEM2    )

MELM2=CALC_MATR_ELEM(  MODELE=MODM,
                         CHARGE=CHM,
                        OPTION='MASS_MECA',
                         CHAM_MATER=CHMAT,
                          CARA_ELEM=CARLEM2    )

NUM2=NUME_DDL(  MATR_RIGI=MELR2  )

MATASSR2=ASSE_MATRICE(MATR_ELEM=MELR2,NUME_DDL=NUM2)

MATASSM2=ASSE_MATRICE(MATR_ELEM=MELM2,NUME_DDL=NUM2)

#--------- CALCUL MODAL

MD2=CALC_MODES(MATR_RIGI=MATASSR2,
               OPTION='PROCHE',
               CALC_FREQ=_F(FREQ=(  1.5,1.6,6.5,),
                            ),
               MATR_MASS=MATASSM2,
               )


#---------------------------------------------------------------------

#                     CALCUL 3         LAMBDA = 0.01

#---------------------------------------------------------------------

CARLEM3=AFFE_CARA_ELEM(   MODELE=MODM,
            #------ POUTRE AB
            POUTRE=_F(  GROUP_MA = 'MPOUTRE',        SECTION = 'GENERALE',
                        CARA = (  'A', 'IY',   'IZ',    'JX',  'AY',  'AZ', ),
                        VALE = ( 0.01,   1.,   0.39E-5,   1.,   1.2,   1.2,  )),

            #------ POINT C   MASSE + RAIDEUR
            DISCRET=(_F(  MAILLE = 'MC',     CARA = 'M_T_D_N',
                                     #          M
                                     VALE =     7.80,           ),
                     _F(  MAILLE = 'MC',     CARA = 'K_T_D_N',
                                     #          K
                                     VALE =     (0.,0.,0.),           ),
                     _F(  MAILLE = 'CD',     CARA =  'K_T_D_L',
                                     #       KX     KY      KZ
                                     VALE = ( 0.,  759.791,  0., ),),),  )

#--------------------------------------

MELR3=CALC_MATR_ELEM(  MODELE=MODM,
                         CHARGE=CHM,
                        OPTION='RIGI_MECA',
                         CHAM_MATER=CHMAT,
                          CARA_ELEM=CARLEM3   )

MELM3=CALC_MATR_ELEM(  MODELE=MODM,
                         CHARGE=CHM,
                        OPTION='MASS_MECA',
                         CHAM_MATER=CHMAT,
                          CARA_ELEM=CARLEM3   )

MATASSR3=ASSE_MATRICE(MATR_ELEM=MELR3,NUME_DDL=NUM2)

MATASSM3=ASSE_MATRICE(MATR_ELEM=MELM3,NUME_DDL=NUM2)

#--------- CALCUL MODAL

MD3=CALC_MODES(MATR_RIGI=MATASSR3,
               OPTION='AJUSTE',
               CALC_FREQ=_F(FREQ=(  1.,7.,),
                            ),
               MATR_MASS=MATASSM3,
               )


#----------------------------------------------------------------------


TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   PARA='FREQ',
                   RESULTAT=MDP,
                   VALE_REFE=1.5707,
                   VALE_CALC=1.5706726502733,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=2,
                   PARA='FREQ',
                   RESULTAT=MDP,
                   VALE_REFE=6.2831,
                   VALE_CALC=6.2812483632983,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=1,
                   PARA='FREQ',
                   RESULTAT=MD2,
                   VALE_CALC=1.5460375862617,
                   VALE_REFE=1.546,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=2,
                   PARA='FREQ',
                   RESULTAT=MD2,
                   VALE_CALC=1.5957059479014,
                   VALE_REFE=1.5958,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=3,
                   PARA='FREQ',
                   RESULTAT=MD2,
                   VALE_CALC=6.2816673616446,
                   VALE_REFE=6.2336,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=1,
                   PARA='FREQ',
                   RESULTAT=MD3,
                   VALE_CALC=1.4936275739886,
                   VALE_REFE=1.4937,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=2,
                   PARA='FREQ',
                   RESULTAT=MD3,
                   VALE_CALC=1.6505829630799,
                   VALE_REFE=1.6506,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   ),
                _F(NUME_ORDRE=3,
                   PARA='FREQ',
                   RESULTAT=MD3,
                   VALE_CALC=6.2854384483149,
                   VALE_REFE=6.2874,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',
                   CRITERE='RELATIF',
                   ),
                ),
          )

FIN()
