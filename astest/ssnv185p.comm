DEBUT(CODE=_F(NOM='SSNV185P',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),DEBUG=_F(SDVERI='OUI'))

#----------------------------------------------
#                MAILLAGE : hexa_5_30_50.mgib
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='SURFSUP',),);
                    
                    
#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

MAILLAG1=MODI_MAILLAGE(reuse =MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFSUP','SURFINF',),),
                       );

# PARAMETRES DE LA FISSURE : 0 < a < 6.5 -> influt sur le choix Rsup !
#                            b = 10.
a=5.
b=10.
#LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-15.');
#LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='b-a-Y');
             
FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    INFO=1,
#                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    DEFI_FISS=_F(
                                 FORM_FISS = 'DEMI_PLAN',
                                 PFON      =(0. , 5. ,15.),
                                 NORMALE   =(0. , 0. , 1.),
                                 DTAN      =(0. ,-1. , 0.)),);
                   
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,FISSURE=FISS,INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,LIAISON_XFEM='OUI',INFO=1,);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=205000.0E6
nu=0.
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT = 'OUI',
                                MATER=ACIER,
                                ),
                             );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

CH1=AFFE_CHAR_MECA(
  MODELE=MODELEK,
  FACE_IMPO=(_F(GROUP_MA=('SURFSUP'),DX=0.0,DY=0.0,DZ=0.0))          
  );
                   
CH2 = AFFE_CHAR_MECA(
  MODELE = MODELEK, 
  FORCE_INTERNE = (_F(TOUT='OUI',FX=0,FY=0,FZ=-78000))
  );

CH3 = AFFE_CHAR_MECA(
  MODELE = MODELEK,
  PESANTEUR=_F(GRAVITE=10,
               DIRECTION=(0.,0.,-1.),)
  );



#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=3.0,NOMBRE=3,),);

# CALCUL AVEC CHARGEMENT FORCE_INTERNE
UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,
                   EXCIT=(_F(CHARGE=CHXFEM,),
                          _F(CHARGE=CH1,),
                          _F(CHARGE=CH2,),
                          ),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INS1,
                                INST_FIN=1.0,),
#                   SOLVEUR=_F(METHODE='MUMPS',),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);
                   
# CALCUL AVEC CHARGEMENT PESANTEUR                   
UTOT2=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,
                   EXCIT=(_F(CHARGE=CHXFEM,),
                          _F(CHARGE=CH1,),
                          _F(CHARGE=CH3,),
                          ),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INS1,
                                INST_FIN=1.0,),
#                   SOLVEUR=_F(METHODE='MUMPS',),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);

#------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS PAR LA METHODE ENERGETIQUE AVEC X-FEM
#------------------------------------------------------------------------

## TEST AVEC PLUSIEURS COURONNES
if a > 4  :
  RI=[2. , 0.666 , 1. , 1. , 1. , 2.1]
  RS=[4. , 1.666 , 2. , 3. , 4. , 3.9]
else :
  RI=[0.666 , 1.]
  RS=[1.666 , 2.]

nbc=len(RI)
SIF1 = [None]*nbc
SIF2 = [None]*nbc

for i in range(0,nbc) :

   SIF1[i]=CALC_G(RESULTAT=UTOT1,
                 SYME_CHAR='SANS',
                 COMP_ELAS=_F(RELATION='ELAS',),
                 OPTION='CALC_K_G',
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[i],
                          R_SUP=RS[i],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),
                 INFO=1,);
                 
   IMPR_TABLE(TABLE=SIF1[i],);
                 
   SIF2[i]=CALC_G(RESULTAT=UTOT2,
                 SYME_CHAR='SANS',
                 COMP_ELAS=_F(RELATION='ELAS',),
                 OPTION='CALC_K_G',
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[i],
                          R_SUP=RS[i],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),
                 INFO=1,);

   IMPR_TABLE(TABLE=SIF2[i],);


# TEST PAR RAPPORT A LA MODELISATION O AVEC FISSURE MAILLEE

for i in range(0,nbc) :
   TEST_TABLE(TABLE=SIF1[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MAX',
              VALE=1.23E+07,
              CRITERE='RELATIF',
              PRECISION=0.06,
              REFERENCE='AUTRE_ASTER',);

   TEST_TABLE(TABLE=SIF1[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MIN',
              VALE=1.23E+07,
              CRITERE='RELATIF',
              PRECISION=0.06,
              REFERENCE='AUTRE_ASTER',);

   TEST_TABLE(TABLE=SIF2[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MAX',
              VALE=1.23E+07,
              CRITERE='RELATIF',
              PRECISION=0.06,
              REFERENCE='AUTRE_ASTER',);

   TEST_TABLE(TABLE=SIF2[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MIN',
              VALE=1.23E+07,
              CRITERE='RELATIF',
              PRECISION=0.06,
              REFERENCE='AUTRE_ASTER',);

##------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS AVEC POST_K1_K2_K3
#------------------------------------------------------------------------

PK=POST_K1_K2_K3(MODELISATION='3D',
                     MAILLAGE=MAILLAG1,
                     MATER=ACIER,
                     FISSURE = FISS,
                     DTAN_ORIG = (0,-1,0),
                     DTAN_EXTR = (0,-1,0),
                     RESULTAT  = UTOT1,
                     ABSC_CURV_MAXI = 1.5,
                     NB_NOEUD_COUPE = 6,
                     VECT_K1        = ( 0., 0., 1., ),
                    )

IMPR_TABLE(TABLE = PK)

TEST_TABLE( TABLE=PK,
            NOM_PARA='K1_MAX',
            FILTRE=(_F( NOM_PARA = 'PT_FOND',  
                        VALE_I = 1,  ),
                    _F( NOM_PARA = 'INST',  
                        VALE = 1.,  ),
                    _F( NOM_PARA = 'METHODE',  
                        VALE_I = 3,  ),),
            VALE=1.3648E+07,
            REFERENCE='NON_REGRESSION' )


#------------------------------------------------------------------------
# POST-TRAITEMENT : MAILLAGE FISSURE
#------------------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

TABDEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                               GROUP_NO='VOL',
                               RESULTAT=UTOT1,
                               NOM_CHAM='DEPL',
                               NUME_ORDRE=1,
                               NOM_CMP='DX',
                               OPERATION='EXTRACTION',),);

# TESTS DE NON REGRESSION PAR RAPPORT A LA VERSION 9.3.15
DXREF=4.7430e+03
DYREF=4.74300e+04
DZREF=1.42289996944e+05
PREC1=1.e-10
PREC2=1.e-6

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_X',
           TYPE_TEST='SOMM_ABS',
           VALE=DXREF,
           CRITERE='RELATIF',
           PRECISION=PREC1,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '9.3.15'),

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_Y',
           TYPE_TEST='SOMM_ABS',
           VALE=DYREF,
           CRITERE='RELATIF',
           PRECISION=PREC1,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '9.3.15'),

TEST_TABLE(TABLE=TABDEP,
           NOM_PARA='COOR_Z',
           TYPE_TEST='SOMM_ABS',
           VALE=DZREF,
           CRITERE='RELATIF',
           PRECISION=PREC1,
           REFERENCE = 'NON_REGRESSION',
           VERSION = '9.3.15'),


#------------------------------------------------------------------------
# POST-TRAITEMENT : DELACEMENTS POUR LE MAILLAGE FISSURE
#------------------------------------------------------------------------

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)
                        
RES_XFEM=CALC_ELEM(reuse =RES_XFEM,
                   RESULTAT=RES_XFEM,
                   OPTION=('EQUI_ELGA_SIGM','EQUI_ELNO_SIGM'));


# TESTS DE NON REGRESSION PAR RAPPORT A LA VERSION 10.2.06
TEST_RESU(RESU=(_F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DX',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 3.8871722013806E-04,
                     REFERENCE = 'NON_REGRESSION',
                     PRECISION=PREC2,
                     VERSION = '10.2.17'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DY',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 2.3488575278018,
                     REFERENCE = 'NON_REGRESSION',
                     PRECISION=PREC2,
                     VERSION = '10.2.17'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DZ',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 2.3103081507325,
                     REFERENCE = 'NON_REGRESSION',
                     PRECISION=PREC2,
                     VERSION = '10.2.17'),
            ),)

#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM,NOM_CHAM='DEPL',TYPE_CHAM='VECT_3D',NOM_CMP=('DX','DY','DZ')));
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM,NOM_CHAM='EQUI_ELNO_SIGM',NOM_CMP='VMIS'));


FIN();
