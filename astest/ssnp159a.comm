DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(
               MAILLAGE=MA,
               VERIF='MAILLE',
               AFFE=_F(
               TOUT = 'OUI',
               PHENOMENE = 'MECANIQUE',
               MODELISATION = '3D')
             )


ACIER=DEFI_MATERIAU(
                   ELAS=_F(
                   E = 200000.,
                   NU = 0.3,),
                   ECRO_LINE=_F(D_SIGM_EPSI=2000,
                                SY=1000,)
             )

L_INST=DEFI_LIST_REEL(
              DEBUT=0.0,INTERVALLE=(
                                   _F(  JUSQU_A = 2.00, NOMBRE = 20),)
             )

F_CHAR=DEFI_FONCTION(
                NOM_PARA='INST',
                PROL_DROITE='CONSTANT',
                    VALE=(
                          0.,   0.,
                          2.,  290,
                          )
                  )


CHP_MATT=AFFE_MATERIAU(
               MAILLAGE=MA,
               AFFE=_F(
                       TOUT = 'OUI',
                       MATER = ACIER,),
)


CHR_LIAI=AFFE_CHAR_MECA(
              MODELE=MO,DDL_IMPO=(
               _F( NOEUD = 'NO2',  DX = 0.,  DY = 0.,  DZ = 0.),
               _F( NOEUD = 'NO6',  DX = 0.,  DY = 0.),
               _F( NOEUD = 'NO1',  DX = 0.,          DZ = 0.),
               _F( NOEUD = ('NO9', 'NO13', 'NO14', 'NO5', 'NO17',),  DX = 0.))
             )

CHR_TRAC=AFFE_CHAR_MECA(
                     MODELE=MO,
                     FACE_IMPO=_F( MAILLE = 'MA2',  DX = 1.)
             )
###################################################
#CALCUL ET TEST EN PETITES DEFORMATIONS
PT=STAT_NON_LINE( MODELE=MO,
                  CHAM_MATER=CHP_MATT,
                  EXCIT=(_F(CHARGE = CHR_LIAI),
                         _F(CHARGE = CHR_TRAC,
                            FONC_MULT = F_CHAR,
                            TYPE_CHARGE = 'DIDI')),
                  COMPORTEMENT=_F(RELATION = 'VMIS_ISOT_LINE',
                               DEFORMATION = 'PETIT'),
                  INCREMENT=_F(LIST_INST = L_INST),
                  NEWTON=_F(MATRICE = 'TANGENTE',
                            PREDICTION = 'EXTRAPOLE',
                            REAC_ITER = 1),
                  CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                 ITER_GLOB_MAXI = 50),
                  RECH_LINEAIRE=_F(ITER_LINE_MAXI = 3)
              )


ENERPT=POST_ELEM(ENER_ELAS=_F(TOUT='OUI'),
                 RESULTAT=PT,
                 INST=2.,
                 );

TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=PT,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='DX',
                   VALE_CALC=290.000000000,
                   VALE_REFE=290.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=PT,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC= 1.57000000E+03,
                   VALE_REFE=1570.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=PT,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=0.282150000,
                   VALE_REFE=0.28215000000000001,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.014999999999999999,
                   MAILLE='MA1',),
                ),
          )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 6.16225000E+09,
           VALE_REFE=6.160000000E9,
           NOM_PARA='TOTALE',
           TABLE=ENERPT,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,),
           )


PT = CALC_CHAMP(reuse=PT,RESULTAT=PT,ENERGIE=('ENEL_ELGA'),)


TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=PT,
                   NOM_CHAM='ENEL_ELGA',
                   MAILLE='MA1',POINT=1,
                   NOM_CMP='TOTALE',
                   VALE_CALC=6.16225,
                   VALE_REFE=6.16,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=1.E-2,),))

###################################################
#CALCUL ET TEST AVEC LES DEFORMATIONS LOGARITHMIQUE
LOG=STAT_NON_LINE( MODELE=MO,
                  CHAM_MATER=CHP_MATT,
                  EXCIT=(_F(CHARGE = CHR_LIAI),
                         _F(CHARGE = CHR_TRAC,
                            FONC_MULT = F_CHAR,
                            TYPE_CHARGE = 'DIDI')),
                  COMPORTEMENT=_F(RELATION = 'VMIS_ISOT_LINE',
                               DEFORMATION = 'GDEF_LOG'),
                  INCREMENT=_F(LIST_INST = L_INST),
                  NEWTON=_F(MATRICE = 'TANGENTE',
                            PREDICTION = 'EXTRAPOLE',
                            REAC_ITER = 1),
                  CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                 ITER_GLOB_MAXI = 50),
                  RECH_LINEAIRE=_F(ITER_LINE_MAXI = 3)
              )


ENERLO=POST_ELEM(ENER_ELAS=_F(TOUT='OUI'),
                 RESULTAT=LOG,
                 INST=2.,
                 );

TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=LOG,
                   NOM_CHAM='DEPL',
                   NOEUD='NO8',
                   NOM_CMP='DX',
                   VALE_CALC=290.000000000,
                   VALE_REFE=290.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=LOG,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC= 1.49479541E+03,
                   VALE_REFE=1495.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=LOG,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=0.247145796,
                   VALE_REFE=0.2475,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.014999999999999999,
                   MAILLE='MA1',),
                ),
          )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 5.61963456E+09,
           VALE_REFE=5.625000000E9,
           NOM_PARA='TOTALE',
           TABLE=ENERLO,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,),
           )

LOG = CALC_CHAMP(reuse=LOG,RESULTAT=LOG,ENERGIE=('ENEL_ELGA'),)


TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=LOG,
                   NOM_CHAM='ENEL_ELGA',
                   MAILLE='MA1',POINT=1,
                   NOM_CMP='TOTALE',
                   VALE_CALC=5.61963456,
                   VALE_REFE=5.625,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=1.E-2,),))

###################################################
#CALCUL ET TEST AVEC LES DEFORMATIONS DE SIMO_MIEHE
SM=STAT_NON_LINE( MODELE=MO,
                  CHAM_MATER=CHP_MATT,
                  EXCIT=(_F(CHARGE = CHR_LIAI),
                         _F(CHARGE = CHR_TRAC,
                            FONC_MULT = F_CHAR,
                            TYPE_CHARGE = 'DIDI')),
                  COMPORTEMENT=_F(RELATION = 'VMIS_ISOT_LINE',
                               DEFORMATION = 'SIMO_MIEHE'),
