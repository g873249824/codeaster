                   NOM_CMP='DX',
                   VALE_CALC=290.000000000,
                   VALE_REFE=290.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=SM,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC= 1.50055349E+03,
                   VALE_REFE=1495.0,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2,
                   MAILLE='MA1',),
                _F(INST=2.0,
                   POINT=1,
                   RESULTAT=SM,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=0.250006467,
                   VALE_REFE=0.2475,
                   REFERENCE='NON_DEFINI',
                   PRECISION=0.014999999999999999,
                   MAILLE='MA1',),
                ),
          )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC= 5.6386852423871E+09,
           VALE_REFE=5.63E9,
           NOM_PARA='TOTALE',
           TABLE=ENERSM,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,),
           )

SM = CALC_CHAMP(reuse=SM,RESULTAT=SM,ENERGIE=('ENEL_ELGA'),)


TEST_RESU(RESU=(_F(INST=2.0,
                   RESULTAT=SM,
                   NOM_CHAM='ENEL_ELGA',
                   MAILLE='MA1',POINT=1,
                   NOM_CMP='TOTALE',
                   VALE_CALC=5.6386852,
                   VALE_REFE=5.63,
                   REFERENCE='ANALYTIQUE',
                   PRECISION=1.E-2,),))

FIN()
