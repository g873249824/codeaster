DEBUT(CODE=_F(NOM='PERF003A',NIV_PUB_WEB='INTERNET'),MEMOIRE=_F(DYNAMIQUE=400,))

EXEC_LOGICIEL(MAILLAGE=_F(FORMAT='GIBI', UNITE_GEOM=14, UNITE=20, MAILLAGE=CO('MA'),),INFO=1,)

MODEL=AFFE_MODELE(MAILLAGE=MA,
         AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='DKT') )

CAREL=AFFE_CARA_ELEM( MODELE=MODEL, COQUE=_F(GROUP_MA='ABCD', EPAIS=0.02) )

MAT1=DEFI_MATERIAU(ELAS=_F(E= 5.E11, NU=0.3, RHO=9800. ))
CHMAT=AFFE_MATERIAU(MAILLAGE=MA, AFFE=_F(TOUT='OUI',MATER=MAT1))

BLOQ=AFFE_CHAR_MECA(MODELE=MODEL,
        DDL_IMPO=(
          _F(GROUP_MA=('AB'), DX=0., DY=0., DZ=0., DRX=0., DRY=0., DRZ=0.),
          _F(GROUP_MA=('DA'), DX=0., DY=0., DZ=0., DRX=0., DRY=0., DRZ=0.),
        ))

MACRO_MATR_ASSE( MODELE=MODEL, CHARGE=BLOQ, CARA_ELEM=CAREL,
                 CHAM_MATER=CHMAT, NUME_DDL=CO("NU"),
                 MATR_ASSE=(
                     _F(  MATRICE = CO("MATK"), OPTION = 'RIGI_MECA'),
                     _F(  MATRICE = CO("MATM"), OPTION = 'MASS_MECA'),
                ))


MODES = MACRO_MODE_MECA(
  MATR_A      = MATK, MATR_B= MATM,
  VERI_MODE=_F(STOP_ERREUR='NON',),
 #CALC_FREQ   =_F( FREQ = (   0.0,   900.0 )),   # pour les frequences 1, ..., 10
  CALC_FREQ   =_F( FREQ = ( 840.0,  1600.0 )),   # pour les frequences 11, ..., 20
)


TEST_RESU(RESU=
       _F( RESULTAT = MODES, PARA = 'FREQ', CRITERE = 'RELATIF',
           PRECISION = 3.E-3, NUME_ORDRE = 3, VALE = 993.5, REFERENCE = 'NON_REGRESSION'),)



FIN()
