DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM=('RUPTURE0_1',),)

# MODELIDATION E : X-FEM 2D
#
# test inspire de forma06a avec une seule fissure

#----------------------------------------------
#                MAILLAGE, MODELE INITIAL
#----------------------------------------------


MA=LIRE_MAILLAGE(FORMAT='MED')

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=_F(GROUP_MA=('M_haut','M_bas')))

MODEF=AFFE_MODELE(MAILLAGE=MA,
                  AFFE= _F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='D_PLAN'))

#----------------------------------------------
#               FISSURES ET ELEMENTS X-FEM
#----------------------------------------------

# longueurs de la fissure : 2a
a = 0.15

FISS=DEFI_FISS_XFEM(MAILLAGE=MA,
                     DEFI_FISS=_F(FORM_FISS  = 'SEGMENT',
                                  PFON_ORIG  = (-a,0.,0.),
                                  PFON_EXTR  = ( a,0.,0.),),
                     )

MODXFEM=MODI_MODELE_XFEM(MODELE_IN=MODEF,FISSURE=FISS)

#----------------------------------------------
#               MATERIAU
#----------------------------------------------

#---
# 1. materiau constant
#---

MAT=DEFI_MATERIAU(ELAS=_F(E=210E9,
                          NU=0.3))

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT))

#---
# 2. materiau fonction de TEMP
#---

# les modules elastiques sont en pratique des fonctions constantes,
# le but etant de reproduire strictement la solution obtenue avec
# un materiau constant

EFTEMP=DEFI_FONCTION(NOM_PARA='TEMP',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     VALE=(0., 210000e6,
                           1., 210000e6),);

NUFTEMP=DEFI_FONCTION(NOM_PARA='TEMP',
                      PROL_DROITE='CONSTANT',
                      PROL_GAUCHE='CONSTANT',
                      VALE=(0., 0.3,
                            1., 0.3),);

# alpha est la fonction nulle, afin de ne pas generer de deformations thermiques

ALPFTEMP=DEFI_FONCTION(NOM_PARA='TEMP',
                       PROL_DROITE='CONSTANT',
                       PROL_GAUCHE='CONSTANT',
                       VALE=(0., 0.,
                             1., 0.),);

MATTEMP=DEFI_MATERIAU(ELAS_FO=_F(E=EFTEMP,
                                 NU=NUFTEMP,
                                 ALPHA=ALPFTEMP,
                                 TEMP_DEF_ALPHA=20.0,))

CHTEMP=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                  MAILLAGE=MA,
                  OPERATION='AFFE',
                  AFFE=_F(TOUT='OUI',
                          NOM_CMP='TEMP',
                          VALE=100.0),)

CHMATEMP=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATTEMP),
                       AFFE_VARC=_F(TOUT='OUI',
                                    CHAM_GD=CHTEMP,
                                    NOM_VARC='TEMP',
                                    VALE_REF=20,))

#---
# 3. materiau fonction de NEUT1
#---

# les modules elastiques sont en pratique des fonctions constantes,
# le but etant de reproduire strictement la solution obtenue avec
# un materiau constant

EFNEUT=DEFI_FONCTION(NOM_PARA='NEUT1',
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     VALE=(0., 210000e6,
                           1., 210000e6),);

NUFNEUT=DEFI_FONCTION(NOM_PARA='NEUT1',
                      PROL_DROITE='CONSTANT',
                      PROL_GAUCHE='CONSTANT',
                      VALE=(0., 0.3,
                            1., 0.3),);

MATNEUT=DEFI_MATERIAU(ELAS_FO=_F(E=EFNEUT,
                                 NU=NUFNEUT))

CHNEUT=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_R',
                  MAILLAGE=MA,
                  OPERATION='AFFE',
                  AFFE=_F(TOUT='OUI',
                          NOM_CMP='X1',
                          VALE=100.0),)

CHMANEUT=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATNEUT),
                       AFFE_VARC=_F(TOUT='OUI',
                                    CHAM_GD=CHNEUT,
                                    NOM_VARC='NEUT1'))

#----------------------------------------------
#               CHARGEMENTS
#----------------------------------------------


# on encastre le point en bas a gauche et on bloque en Y le point en bas a droite
CHBLOC=AFFE_CHAR_MECA(MODELE=MODXFEM,
                      DDL_IMPO=(_F(GROUP_NO='N_A',DX=0.,DY=0.),
                                _F(GROUP_NO='N_B',DY=0.)))

# on applique un effort de traction reparti sur les faces haut et bas.
CHPRES=AFFE_CHAR_MECA(MODELE=MODXFEM,
                      PRES_REP=_F(GROUP_MA=('M_haut','M_bas',),PRES=-1.E6))

#----------------------------------------------
#               RESOLUTION
#----------------------------------------------

L_INST=DEFI_LIST_REEL(DEBUT=0,INTERVALLE=(_F(JUSQU_A=1,PAS=1,),),)

DEFLIST=DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST=L_INST,),)

#---
# 1. sans variables de commande et avec un materiau constant
#---

RESU=MECA_STATIQUE(MODELE=MODXFEM,
                   CHAM_MATER=CHMAT,
                   EXCIT=(
                          _F(CHARGE=CHBLOC),
                          _F(CHARGE=CHPRES),),
                   INST=1.,
                   )

#---
# 2. avec la variable de commande TEMP et le materiau fonction associe
#---

RESUTEMP=STAT_NON_LINE(MODELE=MODXFEM,
                       CHAM_MATER=CHMATEMP,
                       EXCIT=(_F(CHARGE=CHBLOC),
                              _F(CHARGE=CHPRES),),
                       COMPORTEMENT=_F(RELATION='ELAS',),
                       INCREMENT=_F(LIST_INST=DEFLIST,),)

#---
# 3. avec la variable de commande NEUT1 et le materiau fonction associe
#---

RESUNEUT=STAT_NON_LINE(MODELE=MODXFEM,
                       CHAM_MATER=CHMANEUT,
                       EXCIT=(_F(CHARGE=CHBLOC),
                              _F(CHARGE=CHPRES),),
                       COMPORTEMENT=_F(RELATION='ELAS',),
                       INCREMENT=_F(LIST_INST=DEFLIST,),)

#------------------------------------------------------------------------
# POST-TRAITEMENT
#------------------------------------------------------------------------

#---
# 1. sans variables de commande
#---

K=POST_K1_K2_K3(RESULTAT=RESU,
                FISSURE=FISS,
                NUME_FOND=2)

IMPR_TABLE(TABLE=K)

#---
# 2. avec la variable de commande TEMP
#---

KTEMP=POST_K1_K2_K3(RESULTAT=RESUTEMP,
                    FISSURE=FISS,
                    NUME_FOND=2,
                    INST=1.,)

IMPR_TABLE(TABLE=KTEMP)

#---
# 3. avec la variable de commande NEUT1
#---

KNEUT=POST_K1_K2_K3(RESULTAT=RESUNEUT,
                    FISSURE=FISS,
                    NUME_FOND=2,
                    INST=1.,)

IMPR_TABLE(TABLE=KNEUT)

#---
# 4. sans variables de commande et en ecrasant le materiau 
#    avec un materiau constant identique
#---

MAT2=DEFI_MATERIAU(ELAS=_F(E=210E9,
                           NU=0.3))

KMAT2=POST_K1_K2_K3(RESULTAT=RESU,
                    MATER=MAT2,
                    FISSURE=FISS,
                    NUME_FOND=2)

IMPR_TABLE(TABLE=KMAT2)

#----------------------------------------------
#                   TEST
#----------------------------------------------

# rq : pour une modelisation donnee, on doit avoir strictement les meme valeurs
#      de non regression sans variable de commande, avec TEMP et avec NEUT1.

#---
# 1. sans variables de commande
#---

TEST_TABLE(VALE_CALC=760045.177343,
           NOM_PARA='K1',
           TABLE=K,)

#---
# 2. avec la variable de commande TEMP
#---

TEST_TABLE(VALE_CALC=760045.177343,
           NOM_PARA='K1',
           TABLE=KTEMP,)

#---
# 3. avec la variable de commande NEUT1
#---

TEST_TABLE(VALE_CALC=760045.177343,
           NOM_PARA='K1',
           TABLE=KNEUT,)

#---
# 4. sans variables de commande et en ecrasant le materiau
#---

TEST_TABLE(VALE_CALC=760045.177343,
           NOM_PARA='K1',
           TABLE=KMAT2,)

FIN()
