#
# REPRISE DU CAS TEST SSLS139a AVEC PASSAGE DE LA MEMBRANE EN NON LINEAIRE
# COMPARAISON DES RESULTATS ENTRE MECA_STATIQUE ET STAT_NON_LINE
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'), PAR_LOT='OUI')
#
# -------------------------------------------------
# MISE EN DONNEES
# -------------------------------------------------
#

#  Parametres geometriques
ep = 0.001

# Comportement materiau
relation = 'ELAS_HYP_MEMB_SV'

# Comportement du beton et de l'acier
E_A    = 200.E9
NU_A   = 0.3
E_B    = 30.E9
NU_B   = 0.22

# Parametres
fact_A = E_A*ep/(1.-NU_A*NU_A)
multMax = 1.

# Comportement de la membrane equivalente
# obtenu par des calculs elementaires d'homogeneisation
M_LLLL = fact_A
M_LLTT = fact_A*NU_A
M_TTTT = fact_A
M_LTLT = fact_A*0.5*(1.-NU_A)

MA_B=DEFI_MATERIAU(ELAS =_F(E  = E_B,
                            NU = NU_B,),)

MA_A1=DEFI_MATERIAU(ELAS_MEMBRANE =_F(M_LLLL = M_LLLL,
                                     M_LLTT = M_LLTT,
                                     M_LLLT = 0.,
                                     M_TTTT = M_TTTT,
                                     M_TTLT = 0.,
                                     M_LTLT = M_LTLT,),)

MA_A2=DEFI_MATERIAU(ELAS =_F(E  = E_A,
                             NU = NU_A,),)

MAIL=LIRE_MAILLAGE(FORMAT    = 'MED',);

MODE=AFFE_MODELE(MAILLAGE = MAIL,
                 AFFE     = (_F(GROUP_MA    = ('BETON','BOUT'),
                               PHENOMENE    = 'MECANIQUE',
                               MODELISATION = '3D'),
                             _F(GROUP_MA    = ('MEMBRA'),
                               PHENOMENE    = 'MECANIQUE',
                               MODELISATION = 'MEMBRANE')))

MATE1=AFFE_MATERIAU(MAILLAGE = MAIL,
                   AFFE     = (_F(GROUP_MA = 'MEMBRA',
                                  MATER    = MA_A1,),
                               _F(GROUP_MA = 'BETON',
                                  MATER    = MA_B,)));

MATE2=AFFE_MATERIAU(MAILLAGE = MAIL,
                   AFFE     = (_F(GROUP_MA = 'MEMBRA',
                                  MATER    = MA_A2,),
                               _F(GROUP_MA = 'BETON',
                                  MATER    = MA_B,)));

CL_DEPL=AFFE_CHAR_CINE(MODELE   = MODE,
                       MECA_IMPO =(_F(GROUP_MA = 'ENCAST',
                                      DX = 0.,
                                      DY = 0.,
                                      DZ = 0.),
                                   _F(GROUP_MA = 'BORDS',
                                      DY = 0.)),);

# Couple de 400 N.m exerce en bout de plaque
F_FORCE = FORMULE(NOM_PARA = 'Z',
                  VALE     = '((Z-0.08)/0.08)*937500.')

COUPLE=AFFE_CHAR_MECA_F(MODELE   = MODE,
                        FORCE_FACE = _F(GROUP_MA = 'BOUT',
                                        FX       = F_FORCE))

ORI_MEMB=AFFE_CARA_ELEM(MODELE = MODE,
                        MEMBRANE =(_F(GROUP_MA     = 'MEMBRA',
                                      ANGL_REP     = (0., 0.,),
                                      EPAIS        = ep ,
                                      N_INIT       = 0.)));

#
# ----------------------------------------------
# CALCUL
# ----------------------------------------------
#
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,100.,100.,),
                    );
CST=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,multMax,100.,multMax,),
                    );

LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=multMax,
                                   NOMBRE =1,),);

RESU1 = MECA_STATIQUE(MODELE        = MODE,
                     CHAM_MATER    = MATE1,
                     CARA_ELEM     = ORI_MEMB,
                     EXCIT         =(_F(CHARGE = CL_DEPL),
                                     _F(CHARGE = COUPLE,
                                        FONC_MULT=CST,)),
                     SOLVEUR       = _F(METHODE = 'MUMPS'
                                       ),)

RESU2 = STAT_NON_LINE(CARA_ELEM=ORI_MEMB,
                      CHAM_MATER=MATE2,
                      METHODE='NEWTON',
                      MODELE=MODE,
                      EXCIT=(_F(CHARGE=CL_DEPL,
                                TYPE_CHARGE='FIXE_CSTE',),
                             _F(CHARGE=COUPLE,
                                TYPE_CHARGE='FIXE_CSTE',
                                FONC_MULT=RAMPE,),
                             ),
                      INCREMENT=_F(LIST_INST=LINST,
                                   PRECISION=1.E-06,),
                      CONVERGENCE=_F(ARRET='OUI',
                                     ITER_GLOB_ELAS=25,
                                     ITER_GLOB_MAXI=10,),
                      COMPORTEMENT=(_F(PARM_THETA=1.0,
                                       DEFORMATION='GROT_GDEP',
                                       GROUP_MA=('MEMBRA', ),
                                       RELATION=relation,
                                       ITER_CPLAN_MAXI=1,
                                       PARM_ALPHA=1.0,
                                       ITER_INTE_MAXI=20,
                                       RESI_INTE_RELA=1.E-06,
                                       ITER_INTE_PAS=0,
                                       RESI_CPLAN_RELA=1.E-06,),
                                    _F(GROUP_MA=('BETON', 'BOUT'),
                                       RELATION='ELAS',
                                       ITER_INTE_MAXI=20,
                                       RESI_INTE_RELA=1.E-06,
                                       ITER_INTE_PAS=0,
                                       RESI_CPLAN_RELA=1.E-06,
                                       PARM_THETA=1.0,
                                       ITER_CPLAN_MAXI=1,
                                       DEFORMATION='PETIT',
                                       PARM_ALPHA=1.0,),
                                    ),
                      SOLVEUR=_F(METHODE='MUMPS',
                                 RENUM='METIS',
                                 STOP_SINGULIER='OUI',
                                 ELIM_LAGR='NON',
                                 NPREC=8,),
                      ARCHIVAGE=_F(CRITERE='RELATIF',
                                   PRECISION=1.E-06,),
                      NEWTON=_F(REAC_INCR=1,
                                MATRICE='TANGENTE',
                                REAC_ITER_ELAS=0,
                                REAC_ITER=1,
                                MATR_RIGI_SYME='NON',),
                      MESURE=_F(TABLE='NON',),
                      )

#
# ----------------------------------------------
# POST-TRAITEMENT
# ----------------------------------------------
#

RESU1 = CALC_CHAMP(RESULTAT    = RESU1,
                   reuse       = RESU1,
                   GROUP_MA    = 'MEMBRA',
                   CONTRAINTE  = 'SIEF_NOEU')

RESU2 = CALC_CHAMP(RESULTAT    = RESU2,
                   reuse       = RESU2,
                   GROUP_MA    = 'MEMBRA',
                   CONTRAINTE  = 'SIEF_NOEU')

# ----------------------------------------------
# TESTS SUR LES RESULTATS
# ----------------------------------------------

# Deplacements et contraintes de la membrane 
# Comparaison entre non linéaire et linéaire (non régression sur non linéaire)
TEST_RESU(RESU=(_F(LEGENDE='Depl point 1',
                   NUME_ORDRE=1,
                   GROUP_NO='POINT_1',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.000103799658764,
                   VALE_REFE=-1.0379953227545338E-4,
                   PRECISION=3.E-3,),
                _F(LEGENDE='Depl point 2',
                   NUME_ORDRE=1,
                   GROUP_NO='POINT_2',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.000103624525785,
                   VALE_REFE=-1.03624755270052E-4,
                   PRECISION=3.E-3,),
                _F(LEGENDE='Eff gen point 2',
                   NUME_ORDRE=1,
                   GROUP_NO='POINT_2',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=RESU2,
                   NOM_CHAM='SIEF_NOEU',
                   NOM_CMP='NXX',
                   VALE_CALC=2308.56984549,
                   VALE_REFE=2317.098784116031,
                   PRECISION=5.E-3,),
               ),
         )

# Non régression sur linéaire
TEST_RESU(RESU=(_F(LEGENDE='Depl point 1',
                   NUME_ORDRE=1,
                   GROUP_NO='POINT_1',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-1.0379953227545338E-4,),
                _F(LEGENDE='Depl point 2',
                   NUME_ORDRE=1,
                   GROUP_NO='POINT_2',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-1.03624755270052E-4,),
                _F(LEGENDE='Eff gen point 2',
                   NUME_ORDRE=1,
                   GROUP_NO='POINT_2',
                   RESULTAT=RESU1,
                   NOM_CHAM='SIEF_NOEU',
                   NOM_CMP='NXX',
                   VALE_CALC=2317.098784116031,),
               ),
         )

FIN();
