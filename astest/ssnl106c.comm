DEBUT( CODE=_F(  NOM = 'SSNL106C',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE(  )

MO=AFFE_MODELE(        MAILLAGE=MA,
     AFFE=_F( TOUT = 'OUI', PHENOMENE = 'MECANIQUE',MODELISATION = 'POU_D_E')
                        )

