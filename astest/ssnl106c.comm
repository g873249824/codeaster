          # SECTION CERCLE :
           _F(  GROUP_MA = 'GC1',   SECTION = 'CERCLE',
          CARA = (   'R',   ), VALE = (  0.1,)),
          # SECTION RECTANGLE  :
           _F(  GROUP_MA = 'GR1',   SECTION = 'RECTANGLE',
          CARA = (   'HY', 'HZ',  ), VALE = (  0.2,  0.1, )))
)

