lx = 1.0;

ly = 1.0;


# a : profondeur de la fissure
# h : hauteur du plan de fissure
a = 0.5;

h = 0.5;


LN = FORMULE(VALE='Y-h',
             NOM_PARA=('X','Y',),);

LT = FORMULE(VALE='-X+a',
             NOM_PARA=('X','Y',),);

ZERO = FORMULE(VALE='0.',
               NOM_PARA=('X','Y',),);

EFFORT = FORMULE(VALE='-20.',
                 NOM_PARA=('X','Y',),);

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.0,
                                    NOMBRE=3,),);

MA=LIRE_MAILLAGE(FORMAT='MED',);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(GROUP_MA=('LIGSUP','LIGINF','LIGGAU','LIGDRO',),),);

MOD=AFFE_MODELE(MAILLAGE=MA,
                AFFE=_F(GROUP_MA=('SURF','LIGSUP','LIGINF','LIGGAU','LIGDRO',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN',),);

FISS=DEFI_FISS_XFEM(MODELE=MOD,
                    DEFI_FISS=_F(FONC_LT=LT,
                                 FONC_LN=LN,),);

MODK=MODI_MODELE_XFEM(MODELE_IN=MOD,
                      FISSURE=FISS,);


MAT=AFFE_MATERIAU(MAILLAGE=MA,
                  MODELE=MODK,
                  AFFE=_F(GROUP_MA='SURF',
                          MATER=ACIER,),);

CH2=AFFE_CHAR_MECA_F(MODELE=MODK,
                     DDL_IMPO=_F(GROUP_NO='LIGGAU',
                                 DX=ZERO,
                                 DY=ZERO,),
                     PRES_REP=_F(GROUP_MA='LIGDRO',
                                 PRES=EFFORT,),);

UTOT=MECA_STATIQUE(MODELE=MODK,
                   CHAM_MATER=MAT,
                   EXCIT=(
                          _F(CHARGE=CH2),
                          ),
                   );

UTOT=CALC_ERREUR(reuse =UTOT,
                 RESULTAT=UTOT,
                 NUME_ORDRE=1,
                 OPTION='ERME_ELEM',);

UTOT=CALC_CHAMP(reuse=UTOT,RESULTAT=UTOT,NUME_ORDRE=1,CONTRAINTE=('SIEF_ELNO'))


