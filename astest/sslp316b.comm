# TEST V
# CHARGEMENT EN DEPLACEMENT
# MAILLAGE : QUAD LINEAIRE
# COMPORTEMENT ELASTIQUE D_PLAN

DEBUT(CODE=_F(NOM='SSLP316B',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

e = 100000.0;
nu = 0.0;

ACIER=DEFI_MATERIAU(ELAS=_F(E=e,NU=nu,RHO=7800.0,),);

lx = 1.0;
ly = 1.0;

# a : profondeur de la fissure  
# h : hauteur du plan de fissure
a = 0.5;
h = 0.5;

LN = FORMULE(VALE='Y-h',NOM_PARA=('X','Y',),);

LT = FORMULE(VALE='-X+a',NOM_PARA=('X','Y',),);

ZERO = FORMULE(VALE='0.',NOM_PARA=('X','Y',),);

EFFORT = FORMULE(VALE='-20.',NOM_PARA=('X','Y',),);

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.0,
                                    NOMBRE=3,),);

PRE_GIBI();

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(GROUP_MA=('LIGSUP','LIGINF','LIGGAU','LIGDRO',),),);

MOD=AFFE_MODELE(MAILLAGE=MA,
                AFFE=_F(GROUP_MA=('SURF','LIGSUP','LIGINF','LIGGAU','LIGDRO',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN',),);

FISS=DEFI_FISS_XFEM(MODELE=MOD,
                    DEFI_FISS=_F(FONC_LT=LT,
                                 FONC_LN=LN,),
                    GROUP_MA_ENRI='SURF',);

MODK=MODI_MODELE_XFEM(MODELE_IN=MOD,
                      FISSURE=FISS,);

CHXFM=AFFE_CHAR_MECA(MODELE=MODK,
                     LIAISON_XFEM='OUI',);

MAT=AFFE_MATERIAU(MAILLAGE=MA,
                  MODELE=MODK,
                  AFFE=_F(GROUP_MA='SURF',
                          MATER=ACIER,),);

CH2=AFFE_CHAR_MECA_F(MODELE=MODK,
                     DDL_IMPO=_F(GROUP_NO='LIGGAU',
                                 DX=ZERO,
                                 DY=ZERO,),
                     PRES_REP=_F(GROUP_MA='LIGDRO',
                                 PRES=EFFORT,),);

UTOT=STAT_NON_LINE(MODELE=MODK,
                   CHAM_MATER=MAT,
                   EXCIT=(_F(CHARGE=CHXFM,),
                          _F(CHARGE=CH2,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='SURF',),
                   INCREMENT=_F(LIST_INST=L_INS1,
                                INST_FIN=1.0,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),);

UTOT=CALC_ELEM(reuse =UTOT,
               RESULTAT=UTOT,
               OPTION=('SIEF_ELNO_ELGA',),);

TEST_RESU(RESU=(_F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO_ELGA',
                   NOM_CMP='SIXX',
                   TYPE_TEST='MAX',
                   VALE=2.01e+1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO_ELGA',
                   NOM_CMP='SIXX',
                   TYPE_TEST='MIN',
                   VALE=1.97e+1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',),
                 _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO_ELGA',
                   NOM_CMP='SIXY',
                   TYPE_TEST='MAX',
                   VALE=2.58e-1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_ELNO_ELGA',
                   NOM_CMP='SIXY',
                   TYPE_TEST='MIN',
                   VALE=-1.76e-1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',),
                 _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_SENO_SEGA',
                   NOM_CMP='X4',
                   TYPE_TEST='MAX',
                   VALE=2.69e-1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=UTOT,
                   INST=1.0,
                   NOM_CHAM='SIEF_SENO_SEGA',
                   NOM_CMP='X4',
                   TYPE_TEST='MIN',
                   VALE=-1.21e-1,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='NON_REGRESSION',),),);

FIN();
