DEBUT(CODE=_F(NOM='SSLV113C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MESH=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MESH,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

MA1=DEFI_MATERIAU(ELAS=_F(E=2.0,
                          NU=0.3,
                          ALPHA=0.0,),);

MA2=DEFI_MATERIAU(ELAS=_F(E=1.0,
                          NU=0.3,
                          ALPHA=0.0,),);

CM=AFFE_MATERIAU(MAILLAGE=MESH,
                 AFFE=(_F(GROUP_MA='DOM1',
                          MATER=MA1,),
                       _F(GROUP_MA='DOM2',
                          MATER=MA2,),),);

#-----------------------------------------------------------------------
#
#  RESOLUTION DU PROBLEME PRIMAL
#    =>ASSEMBLAGE DE L(.)
#    =>ASSEMBLAGE DE A(.,.)
#    =>RESOLUTION
#    =>CALCUL DE L'ESTIMATEUR EN RESIDU POUR LE PRIMAL
#
#-----------------------------------------------------------------------

CHARPRIM=AFFE_CHAR_MECA(MODELE=MO,
                        FACE_IMPO=(_F(GROUP_MA='GRMA1',
                                      DY=0.0,),
                                   _F(GROUP_MA='GRMA2',
                                      DY=0.91333,),),
                        FORCE_CONTOUR=(_F(GROUP_MA='GRMA3',
                                          FX=1.0,),
                                       _F(GROUP_MA='GRMA4',
                                          FX=-2.0,),),);

RESUPRIM=MECA_STATIQUE(MODELE=MO,
                       CHAM_MATER=CM,
                       EXCIT=_F(CHARGE=CHARPRIM,),);

RESUPRIM=CALC_ELEM(reuse =RESUPRIM,
                   RESULTAT=RESUPRIM,
                   TOUT_ORDRE='OUI',
                   OPTION=('SIRE_ELNO_DEPL',),);

RESUPRIM=CALC_ELEM(reuse =RESUPRIM,
                   RESULTAT=RESUPRIM,
                   TOUT_ORDRE='OUI',
                   OPTION=('ERRE_ELEM_SIGM',),);

#-----------------------------------------------------------------------
#
#  RESOLUTION DU PROBLEME DUAL
#    =>ASSEMBLAGE DE Q(.)
#    =>ASSEMBLAGE DE A(.,.)
#    =>RESOLUTION
#    =>CALCUL DE L'ESTIMATEUR EN RESIDU POUR LE DUAL
#
#-----------------------------------------------------------------------

CHARDUAL=AFFE_CHAR_MECA(MODELE=MO,
                        FACE_IMPO=(_F(GROUP_MA='GRMA1',
                                      DY=0.0,),
                                   _F(GROUP_MA='GRMA2',
                                      DY=0.91333,),),
                        FORCE_INTERNE=_F(TOUT='OUI',
                                         FX=1.0,),);

RESUDUAL=MECA_STATIQUE(MODELE=MO,
                       CHAM_MATER=CM,
                       EXCIT=_F(CHARGE=CHARDUAL,),);

RESUDUAL=CALC_ELEM(reuse =RESUDUAL,
                   RESULTAT=RESUDUAL,
                   TOUT_ORDRE='OUI',
                   OPTION=('SIRE_ELNO_DEPL',),);

RESUDUAL=CALC_ELEM(reuse =RESUDUAL,
                   RESULTAT=RESUDUAL,
                   TOUT_ORDRE='OUI',
                   OPTION=('ERRE_ELEM_SIGM',),);

#-----------------------------------------------------------------------
#
#  CALCUL DE L'ESTIMATEUR D'ERREUR EN QUANTITE D'INTERET
#
#-----------------------------------------------------------------------

RESUPRIM=CALC_ELEM(reuse =RESUPRIM,
                 RESULTAT=RESUPRIM,
                 OPTION=('QIRE_ELEM_SIGM','QIRE_ELNO_ELEM',),
                 RESU_DUAL=RESUDUAL,);

#-----------------------------------------------------------------------
#
#  TESTS DE NON REGRESSION
#
#-----------------------------------------------------------------------



TEST_RESU(RESU=_F(RESULTAT=RESUPRIM,
                  NUME_ORDRE=1,
                  NOM_CHAM='ERRE_ELEM_SIGM',
                  NOM_CMP='ERREST',
                  MAILLE='M1',
                  POINT=1,
                  VALE=5.4510863727309E-02,
                  CRITERE='RELATIF',
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='8.2.15',),);

TEST_RESU(RESU=_F(RESULTAT=RESUPRIM,
                  NUME_ORDRE=1,
                  NOM_CHAM='QIRE_ELEM_SIGM',
                  NOM_CMP='ERREST',
                  MAILLE='M1',
                  POINT=1,
                  VALE=-1.8544612753107E-02,
                  CRITERE='RELATIF',
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='8.2.15',),);

TEST_RESU(RESU=_F(RESULTAT=RESUPRIM,
                  NUME_ORDRE=1,
                  NOM_CHAM='QIRE_ELNO_ELEM',
                  NOM_CMP='ERREST',
                  MAILLE='M4',
                  NOEUD='N1',
                  VALE=-4.2645738460480E-04,
                  CRITERE='RELATIF',
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='8.2.15',),);


TEST_RESU(RESU=_F(RESULTAT=RESUPRIM,
                  NUME_ORDRE=1,
                  PARA='ERREUR_ABSOLUE',
                  VALE=2.0868814360412E-02,
                  CRITERE='RELATIF',
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='8.2.15',),);

TEST_RESU(RESU=_F(RESULTAT=RESUPRIM,
                  NUME_ORDRE=1,
                  PARA='TERME_VOLU_ABSO',
                  VALE=2.1418875170219E-02,
                  CRITERE='RELATIF',
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='8.2.15',),);

TEST_RESU(RESU=_F(RESULTAT=RESUPRIM,
                  NUME_ORDRE=1,
                  PARA='TERME_NORM_ABSO',
                  VALE=0.0000000000000E+00,
                  CRITERE='ABSOLU',
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='9.0.29',),);

TEST_RESU(RESU=_F(RESULTAT=RESUPRIM,
                  NUME_ORDRE=1,
                  PARA='TERME_SAUT_ABSO',
                  VALE=1.5377392287121E-03,
                  CRITERE='RELATIF',
                  PRECISION=1e-05,
                  REFERENCE='NON_REGRESSION',
                  VERSION='9.0.29',),);

FIN();
