# TITRE SYSTEME 2 MASSES 3 RESSORTS
#  SYSTEME MASSE-RESSORT A 2 DEGRES DE LIBERTE
#  ANALYSE SPECTRALE MONO SUPPORTEE AVEC
#  CORRECTION STATIQUE
#
POURSUITE(CODE=_F(  NOM = 'SDLD301A'));

#-----------------------------------------------------------------------
#
# Calcul des modes statiques pour la correction statique
#
#-----------------------------------------------------------------------

MODE_CO=MODE_STATIQUE(MATR_MASS=MATRMASS,
                      PSEUDO_MODE=_F(AXE='X',),
                      MATR_RIGI=MATRRIGI,);

MODE_CO=CALC_ELEM(reuse =MODE_CO,
                  RESULTAT=MODE_CO,
                  OPTION=('SIEF_ELGA','EFGE_ELNO'),);

MODE_CO=CALC_NO(reuse =MODE_CO,
                RESULTAT=MODE_CO,
                OPTION='FORC_NODA')

MODE_CO=CALC_NO(reuse =MODE_CO,
                RESULTAT=MODE_CO,
                OPTION='REAC_NODA')

TEST_RESU(RESU=(_F(NOM_CHAM='DEPL',
                   RESULTAT=MODE_CO,
                   REFERENCE='ANALYTIQUE',
                   NOEUD_CMP=('ACCE','X'),
                   VALE=0.02533,
                   NOM_CMP='DX',
                   NOEUD='P2',),
                _F(NOM_CHAM='DEPL',
                   RESULTAT=MODE_CO,
                   REFERENCE='ANALYTIQUE',
                   NOEUD_CMP=('ACCE','X'),
                   VALE=0.02533,
                   NOM_CMP='DX',
                   NOEUD='P3',),),);

#-----------------------------------------------------------------------

COABS=COMB_SISM_MODAL(CORR_FREQ='NON',
                      MASS_INER=MAS,
                      COMB_MODE=_F(TYPE='ABS',),
                      MODE_MECA=MODES,
                      AMOR_REDUIT=0.0,
                      NUME_MODE=2,
                      MONO_APPUI='OUI',
                      OPTION=('DEPL','EFGE_ELNO','FORC_NODA','REAC_NODA'),
                      MODE_CORR=MODE_CO,
                      EXCIT=_F(AXE=(1.0,0.0,0.0),                               
                               SPEC_OSCI=SPEC1,
                               ECHELLE=1.0,),);

TEST_RESU(RESU=(_F(NOM_CHAM='DEPL',
                   RESULTAT=COABS,
                   REFERENCE='ANALYTIQUE',
                   NOEUD_CMP=('DIR','X'),
                   VALE=0.02302302705,
                   NOM_CMP='DX',
                   NOEUD='P2',),
                _F(NOM_CHAM='DEPL',
                   RESULTAT=COABS,
                   REFERENCE='ANALYTIQUE',
                   NOEUD_CMP=('DIR','X'),
                   VALE=0.02302302705,
                   NOM_CMP='DX',
                   NOEUD='P3',),),);

