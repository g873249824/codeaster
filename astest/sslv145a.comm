DEBUT(  CODE=_F(  NOM = 'SSLV145A',NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI') );

##################################
# 1. GEOMETRIE COMPORTANT UN COUDE
# => MAILLAGE :
#      => ELEMENTS PLAQUES DKT
#      => PRODUIT PAR SALOME
##################################

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

###########################################
# 2. DEFINITION DES ELEMENTS FINIS UTILISES
###########################################

MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA=('TUYAU1','TUYAU2','EFOND','ENCAST','SYMEL',),
                             PHENOMENE='MECANIQUE',
                             MODELISATION='DKT',),
                          _F(GROUP_MA=('COUDE','VOLU1','VOLU2','INTVOL','SYMEV',),
                             PHENOMENE='MECANIQUE',
                             MODELISATION='3D',),),);

#########################################
# 3. ORIENTATION DES NORMALES AUX PLAQUES
#    RENTRANTES DANS LE TUYAU
#########################################

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='TUYAU1',
                                      VECT_NORM=(1.0,0.0,0.0,),
                                      GROUP_NO='A1',),
                   );

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='TUYAU2',
                                      VECT_NORM=(0.0,1.0,0.0,),
                                      GROUP_NO='A2',),
                   );

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_3D=_F(GROUP_MA='INTVOL',),
                   );

###################################
# 4. DEFINITION DU MATERIAU UTILISE
###################################

ACIER=DEFI_MATERIAU(ELAS=_F(E=204000000000.0,
                            NU=0.3,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

#################################
# 5. CARACTERISTIQUES DES PLAQUES
#################################

CARA_COQ=AFFE_CARA_ELEM(MODELE=MODMECA,
                        COQUE=_F(GROUP_MA=('TUYAU1','TUYAU2',),
                                 EPAIS=0.02,
                                 ANGL_REP=(0.0,-90.0,),),);

##########################################
# 6. DEFINITION DES CONDITIONS AUX LIMITES
#    ENCASTREMENT SUR LA SECTION ENCAST
##########################################

BLOQUAGE=AFFE_CHAR_CINE(MODELE=MODMECA,
                        MECA_IMPO=(_F(GROUP_MA='ENCAST',
                                      DX=0.0,
                                      DY=0.0,
                                      DZ=0.0,
                                      DRX=0.0,
                                      DRY=0.0,
                                      DRZ=0.0,),
                                   _F(GROUP_MA='SYMEL',
                                      DZ=0.0,
                                      DRX=0.0,
                                      DRY=0.0,),
                                   _F(GROUP_MA='SYMEV',
                                      DZ=0.0,),),);

###############################################################
# 7. DEFINITION DES CHARGEMENTS
#    CHARGEMENT 1 : FY=100KN (FX,FZ,MX,MY,MZ NULS)
#    CHARGEMENT 2 : PRESSION INTERNE DE 15 MPA ET EFFET DE FOND
###############################################################

FYTOT = 100000.0;

EPTUB = 0.02;

REXT = 0.2;

RMOY = (REXT - (EPTUB / 2));

FYREP = (((FYTOT / 2.0) / pi) / RMOY);

RINT = (RMOY - (EPTUB / 2));

SINT = (pi * (RINT * RINT));

####################
# 2*pi*R*FY = 100 KN
####################

CHARG1=AFFE_CHAR_MECA(MODELE=MODMECA,
                      FORCE_ARETE=_F(GROUP_MA='EFOND',
                                     FY=FYREP,),);
#########################
# 2*pi*R*FX = PINT * SINT ;
#########################

PINT = 15000000.0;

PRES = ((PINT * RINT) / RMOY);

CHARG2=AFFE_CHAR_MECA(MODELE=MODMECA,
                      PRES_REP=_F(GROUP_MA='INTVOL',
                                  PRES=PRES,),
                      FORCE_COQUE=_F(GROUP_MA=('TUYAU1','TUYAU2',),
                                     PRES=PRES,),);

ARLE=AFFE_CHAR_MECA(MODELE=MODMECA,
                    ARLEQUIN=(_F(GROUP_MA_1='TUYAU1',
                                 GROUP_MA_2='VOLU1',
                                 CARA_ELEM=CARA_COQ,
                                 COLLAGE='GROUP_MA_2',
                                 POIDS_1=0.999,),
                              _F(GROUP_MA_1='TUYAU2',
                                 GROUP_MA_2='VOLU2',
                                 CARA_ELEM=CARA_COQ,
                                 COLLAGE='GROUP_MA_2',
                                 POIDS_1=0.999,),),);

RESU=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARA_COQ,
                    EXCIT=(_F(CHARGE=BLOQUAGE,),
                           _F(CHARGE=CHARG2,),
                           _F(CHARGE=ARLE,),),);

RESU=CALC_ELEM(reuse =RESU,
                MODELE=MODMECA,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARA_COQ,
                RESULTAT=RESU,
                OPTION='SIGM_ELNO_DEPL',);

RESU=CALC_NO(reuse =RESU,
              RESULTAT=RESU,
              OPTION='SIGM_NOEU_DEPL',
              CHAM_MATER=CHMAT,
              CARA_ELEM=CARA_COQ,);


DEPL1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                             GROUP_NO='A1',
                             RESULTAT=RESU,
                             NOM_CHAM='DEPL',
                             NOM_CMP=('DX','DY','DZ'),
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=DEPL1)


DEPL2=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                             GROUP_NO='A2',
                             RESULTAT=RESU,
                             NOM_CHAM='DEPL',
                             NOM_CMP=('DX','DY','DZ'),
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=DEPL2)

###############################
# 8. TESTS VALEURS DEPLACEMENTS
#    NON REGRESSION
###############################

TEST_TABLE(TABLE=DEPL1,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='DX',
           VALE = 0,
           CRITERE= 'ABSOLU',
           PRECISION=1E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=DEPL1,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='DY',
           VALE = 0,
           CRITERE= 'ABSOLU',
           PRECISION=1E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=DEPL1,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='DZ',
           VALE = 0,
           CRITERE= 'ABSOLU',
           PRECISION=1E-8,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=DEPL2,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='DX',
           VALE = -1.8054157613817E-01,
           CRITERE= 'RELATIF',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=DEPL2,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='DY',
           VALE = 4.7598471831257E-02,
           CRITERE= 'RELATIF',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=DEPL2,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='DZ',
           VALE = 0,
           CRITERE= 'ABSOLU',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

##############################
# 9. TESTS VALEURS CONTRAINTES
#    NON REGRESSION
##############################

SIGM1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                             GROUP_NO='A1',
                             RESULTAT=RESU,
                             NOM_CHAM='SIGM_NOEU_DEPL',
                             NOM_CMP=('SIXX','SIYY','SIZZ','SIXY'),
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=SIGM1)

TEST_TABLE(TABLE=SIGM1,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='SIXX',
           VALE = -4.7339031158235E+08,
           CRITERE= 'RELATIF',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=SIGM1,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='SIYY',
           VALE = -1.5779677052745E+09,
           CRITERE= 'RELATIF',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=SIGM1,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='SIXY',
           VALE = 2.2633300955843E+08,
           CRITERE= 'RELATIF',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

SIGM2=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                             GROUP_NO='A2',
                             RESULTAT=RESU,
                             NOM_CHAM='SIGM_NOEU_DEPL',
                             NOM_CMP=('SIXX','SIYY','SIZZ','SIXY'),
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=SIGM2)

TEST_TABLE(TABLE=SIGM2,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='SIXX',
           VALE = 1.2378305316156E+08,
           CRITERE= 'RELATIF',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=SIGM2,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='SIYY',
           VALE = -1.4069526952971E+07,
           CRITERE= 'RELATIF',
           PRECISION=1E-6,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

TEST_TABLE(TABLE=SIGM2,
           FILTRE = _F(NOM_PARA='ABSC_CURV',VALE=0),
           NOM_PARA='SIXY',
           VALE = -8.4280848779111E+06,
           CRITERE= 'RELATIF',
           PRECISION=1E-5,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.20',);

FIN();
