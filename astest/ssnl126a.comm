DEBUT(CODE=_F(NOM='SSNL126A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

mail=LIRE_MAILLAGE(FORMAT='MED',);

modele=AFFE_MODELE(MAILLAGE=mail,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);

acier=DEFI_MATERIAU(ELAS=_F(E=210000000000.0,
                            NU=0.,),
                    ECRO_LINE=_F(D_SIGM_EPSI=70000000000.0,
                                 SY=4000000.0,),);

ch_mat=AFFE_MATERIAU(MAILLAGE=mail,
                     AFFE=_F(TOUT='OUI',
                             MATER=acier,),);

CL=AFFE_CHAR_MECA(MODELE=modele,
                  DDL_IMPO=_F(GROUP_MA='ENCAST',
                              DX=0.0,
                              DY=0.0,
                              DZ=0.0,),);

CS=AFFE_CHAR_MECA(MODELE=modele,
                  FORCE_FACE=_F(GROUP_MA='EFFORT',
                                FX=-6500000.0,),);

fonc=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,10.0,10.0,),);

list=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=_F(JUSQU_A=1.,
                                  NOMBRE=10,),);

res=STAT_NON_LINE(MODELE=modele,
                  CHAM_MATER=ch_mat,
                  EXCIT=(_F(CHARGE=CS,
                            FONC_MULT=fonc,
                            TYPE_CHARGE='FIXE_CSTE',),
                         _F(CHARGE=CL,
                            TYPE_CHARGE='FIXE_CSTE',),),
                  COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                               DEFORMATION='PETIT',
                               TOUT='OUI',),
                  INCREMENT=_F(LIST_INST=list,),
                  NEWTON=_F(REAC_INCR=1,
                            PREDICTION='TANGENTE',
                            MATRICE='TANGENTE',
                            REAC_ITER=1,),
                  CONVERGENCE=_F(ITER_GLOB_MAXI=300,),
                  CRIT_STAB=_F(CHAR_CRIT=(-15,15),),
                  );




TEST_RESU(RESU=(_F(PARA='CHAR_CRIT',
                   INST=0.2,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=res,
                   VALE_CALC=-9.976366410,
                   VALE_REFE=-9.9645,
                   PRECISION=2.E-3,),
                _F(PARA='CHAR_CRIT',
                   INST=1.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=res,
                   VALE_CALC=-0.675266654,
                   VALE_REFE=-0.6643,
                   PRECISION=0.03,),
                ),
          )

FIN();
