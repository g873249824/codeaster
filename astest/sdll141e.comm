                 OPTION='BANDE',
                 CALC_FREQ=_F(FREQ=(3.,3000.,),
                              ),
                 MATR_MASS=MASSE,
                 )


TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   PARA='FREQ',
                   REFERENCE='SOURCE_EXTERNE',
