                                _F(CARA='A_TR_D_N',
                                   GROUP_MA='DISQUI',
                                   REPERE='LOCAL',
                                   VALE=(0.,0.,0.,0.,0.,0.,),),
                                _F(CARA='M_TR_D_N',
