# CAS TEST SSNV503A
# - MODELISATION AVEC DES ELEMENTS D_PLAN (MAILLES QUA4)
# - MODELISATION DU CONTACT AVEC LA METHODE CONTACT (AFFE_CHAR_MECA)
# - METHODE LAGRANGIENNE POUR LE CONTACT ET LE FROTTEMENT
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MAILL=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI',),);

MAILL=DEFI_GROUP(reuse =MAILL,
                 MAILLAGE=MAILL,
                 CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MODE=AFFE_MODELE(MAILLAGE=MAILL,

                 AFFE=(_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='D_PLAN',),
                       _F(GROUP_MA='RES_LAT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T',),
                       _F(GROUP_MA='RES_HAUT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T',),),);

MAILL=MODI_MAILLAGE(reuse =MAILL,
                    MAILLAGE=MAILL,
                    ORIE_PEAU_2D=_F(GROUP_MA=('LMAI','LESC','FACESUP','FACELAT'),),
                    );

#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------

CARA=AFFE_CARA_ELEM(MODELE=MODE,
                    DISCRET_2D=(_F(GROUP_MA='RES_LAT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(2.0,0.0),),
                                _F(GROUP_MA='RES_HAUT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(0.005,0.0),),
                              ),);

#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------

MPATIN=DEFI_MATERIAU(ELAS=_F(E=2.10000E5,
                             NU=0.0,),);

MBATI=DEFI_MATERIAU(ELAS=_F(E=1.E5,
                            NU=0.0,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL,
                 MODELE=MODE,
                 AFFE=(_F(GROUP_MA='PATIN',
                          MATER=MPATIN,),
                       _F(GROUP_MA='BATI',
                          MATER=MBATI,),
                       ),);

#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------

# CONDITIONS AUX LIMITES
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='BATI',
                                     DX=0.0,DY=0.0,),
                                  _F(GROUP_NO=('R1','R2','X','Y'),
                                     DX=0.0,DY=0.0,),),);

# CHARGEMENT 1 : APPLICATION DE LA PRESSION NORMALE
PNOR=AFFE_CHAR_MECA(MODELE=MODE,
                    PRES_REP=_F(GROUP_MA='FACESUP',
                                PRES=300.0,),);

# CHARGEMENT 2 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*0.99)
PTAN_99=AFFE_CHAR_MECA(MODELE=MODE,
                       PRES_REP=_F(GROUP_MA='FACELAT',
                                   PRES=178.19999999999999,),);

# CHARGEMENT 3 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*1.01)
PTAN_101=AFFE_CHAR_MECA(MODELE=MODE,
                        PRES_REP=_F(GROUP_MA='FACELAT',
                                    PRES=181.80000000000001,),);

# DEFINITION DU CONTACT
CTAC =DEFI_CONTACT(MODELE       = MODE,
                   FORMULATION  = 'DISCRETE',
                   FROTTEMENT   = 'COULOMB',
                   REAC_GEOM    = 'CONTROLE',
                   NB_ITER_GEOM = 2,
                   ZONE=(
                        _F(
                          ALGO_CONT  = 'LAGRANGIEN',
                          ALGO_FROT  = 'LAGRANGIEN',
                          COULOMB    = 0.3,
                          COEF_MATR_FROT=0.9,
                          GROUP_MA_MAIT='LMAI',
                          GROUP_MA_ESCL='LESC',
                         ),
                       ),
                  )

#---------------------------------------------------------------------
#                         RESOLUTION
#---------------------------------------------------------------------

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,
                          1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=2,),);

# CHARGEMENT 1
U_NOR=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=COND_LIM,),),
                    CONTACT  = CTAC,
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    SOLVEUR=_F(
                               METHODE='LDLT',),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=30,),
                    INFO=1,);

U_NOR=CALC_CHAMP(reuse =U_NOR, RESULTAT=U_NOR,
              FORCE='REAC_NODA',   );

U_NOR=CALC_CHAMP(reuse =U_NOR, RESULTAT=U_NOR,
                 VARI_INTERNE=('VARI_ELNO'),   );

# CHARGEMENT 2
U_99=STAT_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CM,
                   CARA_ELEM=CARA,
                   EXCIT=(_F(CHARGE=PNOR,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=PTAN_99,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=COND_LIM,),),
                   CONTACT  = CTAC,
                   COMPORTEMENT=_F(RELATION='ELAS',
                                TOUT='OUI',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   SOLVEUR=_F(
                              METHODE='LDLT',),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=1500,),);

U_99=CALC_CHAMP(reuse =U_99,
             RESULTAT=U_99,
             FORCE='REAC_NODA',

              );

# CHARGEMENT 3
U_101=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=PTAN_101,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=COND_LIM,),),
                    CONTACT  =CTAC,
                    COMPORTEMENT=_F(RELATION='ELAS',
                                 TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    SOLVEUR=_F(
                               METHODE='LDLT',),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=1500,),
                    );

U_101=CALC_CHAMP(reuse =U_101,
              RESULTAT=U_101,
              FORCE='REAC_NODA',

              );

#---------------------------------------------------------------------
#                         VERIFICATION DES RESULTATS
#---------------------------------------------------------------------

# CHARGEMENT 1
TEST_RESU(RESU=(_F(GROUP_NO='A',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=0.999999999028,
                   VALE_REFE=1.0,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='A',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.73205080647,
                   VALE_REFE=-1.732,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.0000000012,
                   VALE_REFE=1.0,
                   PRECISION=1.E-2,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=U_NOR,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.73205080719,
                   VALE_REFE=-1.732,
                   PRECISION=1.E-2,
                   ),
                ),
          )

TU_NOR=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                               GROUP_NO='LESC',
                               RESULTAT=U_NOR,
                               NOM_CHAM='REAC_NODA',
                               NOM_CMP=('DX','DY'),
                               REPERE='LOCAL',
                               OPERATION='EXTRACTION',
                               INST=1.,),);

TU_VARI=POST_RELEVE_T(ACTION=_F(INTITULE='VARI',
                                GROUP_NO='LESC',
                                RESULTAT=U_NOR,
                                NOM_CHAM='VARI_ELNO',
                                NOM_CMP='V1',
                                OPERATION='MOYENNE',
                                INST=1.),);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.05,
           VALE_CALC=11999.9795455,
           VALE_REFE=1.2000E4,
           NOM_PARA='DY',
           TYPE_TEST='SOMM',
           TABLE=TU_NOR,)

TEST_TABLE(CRITERE='ABSOLU', REFERENCE='ANALYTIQUE',  PRECISION=1.E-3,
           ORDRE_GRANDEUR=1.E4,
           VALE_CALC=0.0,
           VALE_REFE=0.0,
           NOM_PARA='DX',
           TYPE_TEST='SOMM',
           TABLE=TU_NOR,)

# CHARGEMENT 2
TEST_RESU(RESU=(_F(GROUP_NO='A',
                   INST=1.0,
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.02134440663,
                   ),
                _F(GROUP_NO='A',
                   INST=1.0,
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.7197276065,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=1.00000000042,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   RESULTAT=U_99,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=-1.73205080739,
                   ),
                ),
          )

TU_99=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                              GROUP_NO='LESC',
                              RESULTAT=U_99,
                              NOM_CHAM='REAC_NODA',
                              NOM_CMP=('DX','DY'),
                              REPERE='LOCAL',
                              OPERATION='EXTRACTION',
                              INST=1.),);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.05,
           VALE_CALC=11999.9796722,
           VALE_REFE=1.2000E4,
           NOM_PARA='DY',
           TYPE_TEST='SOMM',
           TABLE=TU_99,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.05,
           VALE_CALC=-3563.96166068,
           VALE_REFE=-3564.0,
           NOM_PARA='DX',
           TYPE_TEST='SOMM',
           TABLE=TU_99,)

# CHARGEMENT 3
TEST_RESU(RESU=(_F(GROUP_NO='A',
                   INST=1.0,
                   RESULTAT=U_101,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=8.80953344426,
                   ),
                _F(GROUP_NO='A',
                   INST=1.0,
                   RESULTAT=U_101,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=2.77678543112,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   RESULTAT=U_101,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC=8.78694382254,
                   ),
                _F(GROUP_NO='B',
                   INST=1.0,
                   RESULTAT=U_101,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=2.76374330463,
                   ),
                ),
          )

TU_101=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                               GROUP_NO='LESC',
                               RESULTAT=U_101,
                               NOM_CHAM='REAC_NODA',
                               NOM_CMP=('DX','DY'),
                               REPERE='LOCAL',
                               OPERATION='EXTRACTION',
                               INST=1.),);

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.05,
           VALE_CALC=11999.9796721,
           VALE_REFE=1.2000E4,
           NOM_PARA='DY',
           TYPE_TEST='SOMM',
           TABLE=TU_101,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.05,
           VALE_CALC=-3599.99386241,
           VALE_REFE=-3624.0,
           NOM_PARA='DX',
           TYPE_TEST='SOMM',
           TABLE=TU_101,)

FIN();
