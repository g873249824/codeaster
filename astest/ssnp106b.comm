            MODELE=MO,DDL_IMPO=(
           _F( NOEUD = 'NO3',  DX = 2.E-2),
           _F( NOEUD = 'NO2',  DX = 2.E-2),
           _F( NOEUD = 'NO3',  DY = -2.E-2),
