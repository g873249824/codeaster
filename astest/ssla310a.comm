                                _F(  GROUP_NO = 'LP8P9S', DX = 0.),
                                _F(  GROUP_NO = 'PB',     DY = 0.)),
                     FORCE_NODALE=( _F( GROUP_NO = 'P8',  FY =  1.591549E-3),
                                    _F( GROUP_NO = 'P8S', FY = -1.591549E-3))
                           )

