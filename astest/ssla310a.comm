                     DDL_IMPO=( _F(  GROUP_NO = 'LP8P9',  DX = 0.),
                                _F(  GROUP_NO = 'LP8P9S', DX = 0.),
                                _F(  GROUP_NO = 'PB',     DY = 0.)),
