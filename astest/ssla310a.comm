MAT2=DEFI_MATERIAU(ELAS=_F(  E = 1.E3, NU = 0.3))

MAT3=DEFI_MATERIAU(ELAS=_F(  E = 1.E3, NU = 0.2))

CHMAT1=AFFE_MATERIAU(  MAILLAGE=MA,AFFE=(
                            _F(  GROUP_MA = 'VMAT1', MATER = MAT2),
                            _F(  GROUP_MA = 'VMAT2', MATER = MAT2))
                          )

