             NOM_PARA='SIGM',
   VALE=(      138.,     1000000.,
               152.,      500000.,
               165.,      200000.,
               180.,      100000.,
               200.,       50000.,
               250.,       20000.,
               295.,       12000.,
               305.,       10000.,
               340.,        5000.,
               430.,        2000.,
               540.,        1000.,
               690.,         500.,
               930.,         200.,
              1210.,         100.,
              1590.,          50.,
              2210.,          20.,
              2900.,          10.,
