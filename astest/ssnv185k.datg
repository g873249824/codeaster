opti dime 3  elem cub8 ;

dens 10 ;

*----------------------------------------
*DONNEES
*----------------------------------------
* dimensions des cotés
LX = 1;
LY = 10;
LZ = 60;

* nombre d'éléments
NX = 5;
NY = 21;
NZ = 50; 

*---------------------------------------------
*construction des surfaces dans le plan inférieur
*---------------------------------------------

p1 = LX   0.  -30.;
p2 = LX  LY   -30.;
p3 = 0.  LY   -30.;
p4 = 0.  0.  -30.;

lig1 = droit p1 p2 NY;
lig2 = droit p2 p3 NX;
lig3 = droit p3 p4 NY;
lig4 = droit p4 p1 NX;

cont  = lig1 et lig2 et lig3 et lig4;

*SURFINF= surface cont 'PLANE';
SURFINF= DALL lig1 lig2 lig3 lig4 PLAN;
trac SURFINF;

p5 = 0.  0. LZ.;

*---------------------------------------------
*extrusion
*---------------------------------------------

vol = SURFINF volu  'TRAN' p5 NZ;
SURFSUP = FACE 2  vol ;
elim vol 1E-6;

trac cach vol;

MAILLE = vol et SURFINF et SURFSUP;
                 

opti sauv format 'ssnv185l.mgib' ;
sauv format maille ;
fin ;
