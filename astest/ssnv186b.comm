DEBUT(CODE=_F(NOM='SSNV186B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                        LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='VOLQUAD',
                                     GROUP_MA='VOL',),);

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_XFEM_CONT',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-17.5');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN),
                    GROUP_MA_ENRI='VOL',);

                    

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='OUI',
                         INFO=1,);                    
                    
CTXFEM = AFFE_CHAR_MECA( CONTACT=_F( FISS_MAIT=FISS,
                                      METHODE = 'XFEM',
                                     INTEGRATION='FPG4',
                               COEF_REGU_CONT=100.,
                               ITER_CONT_MAXI=4,
                               CONTACT_INIT='OUI',
                               ALGO_LAGR='NON',
                               COEF_ECHELLE=1.E11,
                               FROTTEMENT='COULOMB',
                               COULOMB=1.0,
                               ITER_FROT_MAXI=6,
                               COEF_REGU_FROT=1000.,
                               SEUIL_INIT=-1.E11,
                                    ),
                          MODELE=MODELEK,
                          ),                       



E=1.0E11
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('VOL','SURFINF','SURFSUP',),
                                MATER=ACIER,
                                ),);

PRES=100.
FCTPRES=FORMULE(NOM_PARA=('X','Y','Z'),VALE='(PRES-(Y-10.)*(Y-10.)/2.)*E/1e6');

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SURFINF',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(GROUP_MA='SURFSUP',DX=0.0,DY=0.0,),),
                    INFO=1,);

CHF=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     PRES_REP=_F(GROUP_MA='SURFSUP',
                                 PRES=FCTPRES,),
                      );

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CH1,),
                          _F(CHARGE=CHF,),
                          _F(CHARGE=CTXFEM,),                          
                          _F(CHARGE=CHXFEM,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   NEWTON=_F(REAC_ITER=1,),
                   SOLVEUR=_F(METHODE='MUMPS',
                              ),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                             GROUP_NO='VOLQUAD',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2',),
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=LAG,);


# CETTE VALEUR A ETE OBTENUE PAR LA MODELISATION A
LAREF=-9.52844E-05


# LA PRECISION EST ICI DE 0.97 CAR ON CHERCHE A METTRE EN EVIDENCE
# DES OSCILLATIONS (ANOMALIE)

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           FILTRE=(_F(NOM_PARA='COOR_X',
                      CRIT_COMP='EQ',
                      VALE=0.,),
                   _F(NOM_PARA='COOR_Y',
                      CRIT_COMP='EQ',
                      VALE=10.,),
                   _F(NOM_PARA='COOR_Z',
                     CRIT_COMP='EQ',
                     VALE=17.5,),
                  ),
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=0.97,
           REFERENCE='AUTRE_ASTER',);

# TEST DE NON-REGRESSION PAR RAPPORT A LA VERSION 8.2.16

LA8216=-1.8685302453462E-4


TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           FILTRE=(_F(NOM_PARA='COOR_X',
                      CRIT_COMP='EQ',
                      VALE=0.,),
                   _F(NOM_PARA='COOR_Y',
                      CRIT_COMP='EQ',
                      VALE=10.,),
                   _F(NOM_PARA='COOR_Z',
                     CRIT_COMP='EQ',
                     VALE=17.5,),
                  ),
           VALE=LA8216,
           CRITERE='RELATIF',
           PRECISION=1.E-10,
           REFERENCE='NON_REGRESSION',VERSION='8.2.16');

# TEST DE NON-REGRESSION PAR RAPPORT A LA VERSION 8.2.18
LA2REF=-3.9336395589177E-01

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           FILTRE=(_F(NOM_PARA='COOR_X',
                      CRIT_COMP='EQ',
                      VALE=0.,),
                   _F(NOM_PARA='COOR_Y',
                      CRIT_COMP='EQ',
                      VALE=10.,),
                   _F(NOM_PARA='COOR_Z',
                     CRIT_COMP='EQ',
                     VALE=17.5,),
                  ),
           VALE=LA2REF,
           CRITERE='RELATIF',
           PRECISION=1.E-10,
           REFERENCE='NON_REGRESSION',VERSION='8.2.18');

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'),
                        INFO          = 2)
 
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));




FIN();
