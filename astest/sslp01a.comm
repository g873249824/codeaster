DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE()

#
# DEFINITION DES GROUPES DE NOEUDS
MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,CREA_GROUP_NO=(
                       _F(  GROUP_MA = 'ENCAST'),
                       _F(  GROUP_MA = 'BORD_CH'))
                   )

#
# DEFINITION DU MODELE
MODELE=AFFE_MODELE(    MAILLAGE=MAIL,
                         AFFE=_F(  TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'C_PLAN')  )

#
# DEFINITION DU MATERIAU
ACIER=DEFI_MATERIAU(    ELAS=_F(  E = 3.E10,  NU = 0.25)
                      )

#
# ATTRIBUTION DU MATERIAU
CHMAT=AFFE_MATERIAU(    MAILLAGE=MAIL,
                          AFFE=_F(  TOUT = 'OUI',
                                 MATER = ACIER)  )

#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS
CLIM=AFFE_CHAR_MECA(    MODELE=MODELE,
                            DDL_IMPO=_F(  GROUP_NO = 'ENCAST',
                                       DX = 0.,   DY = 0.)
                        )

FONCT=DEFI_FONCTION(    NOM_PARA='Y',
                          VALE=( -0.006,   0.,
                                 -0.003,   3.750E6,
                                  0.,      5.000E6,
                                  0.003,   3.750E6,
                                  0.006,   0., )
                      )

CHARGT=AFFE_CHAR_MECA_F(    MODELE=MODELE,
                              PRES_REP=_F(  GROUP_MA = 'BORD_CH',
                                            CISA_2D = FONCT)
                        )

#
# RESOLUTION
DEF=MECA_STATIQUE(    MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                       EXCIT=( _F(  CHARGE = CHARGT),
                               _F(  CHARGE = CLIM))
                    )

#
# RECUPERATION DU CHAMP DE DEPLACEMENTS
U=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                 NOM_CHAM='DEPL', RESULTAT=DEF,
                 NUME_ORDRE=1
                  )

#
# CALCUL DES CONTRAINTES

DEF=CALC_CHAMP(reuse=DEF,
               RESULTAT= DEF ,
               CONTRAINTE='SIGM_ELNO',);

#
# TEST_RESU DU DEPLACEMENT
#
TEST_RESU(CHAM_NO=_F(NOEUD='N33',
                     NOM_CMP='DY',
                     PRECISION=0.023,
                     CHAM_GD=U,
                     VALE_CALC= 3.33369791E-04,
                     VALE_REFE=3.4099999999999999E-4,
                     REFERENCE='NON_DEFINI',),
          )

#
# TEST_RESU DE LA CONTRAINTE
#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  RESULTAT=DEF,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N1',
                  NOM_CMP='SIXX',
                  VALE_CALC= 8.13652308E+07,
                  VALE_REFE=8.E7,
                  REFERENCE='NON_DEFINI',
                  PRECISION=0.017999999999999999,
                  MAILLE='M1',),
          )

#----------------------------------------------------------
# TEST DE IMPR_RESU
#----------------------------------------------------------
#

IMPR_RESU( FORMAT = 'IDEAS', UNITE = 38,
           RESU=_F( RESULTAT = DEF,
                    TOUT_ORDRE = 'OUI', ))

#
#----------------------------------------------------------
# TEST DE LIRE_RESU
#----------------------------------------------------------


DEF_R=LIRE_RESU(           UNITE=38,
                          FORMAT='IDEAS',
                       TYPE_RESU='EVOL_ELAS',
                        NOM_CHAM='DEPL',
                      TOUT_ORDRE='OUI',
                          MODELE=MODELE,
                      CHAM_MATER=CHMAT,
                           EXCIT=(_F(CHARGE=CHARGT),),
                  )

#
# RECUPERATION DU CHAMP DE DEPLACEMENTS
U_R=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
                 NOM_CHAM='DEPL', RESULTAT=DEF_R,
                 NUME_ORDRE=1
                    )

#
# CALCUL DES CONTRAINTES

DEF_R=CALC_CHAMP(reuse=DEF_R,
                 RESULTAT= DEF_R ,
                 CONTRAINTE=('SIGM_ELNO',),);

#
# TEST_RESU DU DEPLACEMENT
#
TEST_RESU(CHAM_NO=_F(NOEUD='N33',
                     REFERENCE='AUTRE_ASTER',
                     NOM_CMP='DY',
                     PRECISION=0.023,
                     CHAM_GD=U_R,
                     VALE_CALC= 3.33370000E-04,
                     VALE_REFE=3.4099999999999999E-4,),
          )

#
# TEST_RESU DE LA CONTRAINTE
#
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=DEF_R,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='N1',
                  NOM_CMP='SIXX',
                  VALE_CALC= 8.13651021E+07,
                  VALE_REFE=8.E7,
                  PRECISION=0.017999999999999999,
                  MAILLE='M1',),
          )

FIN()
#
