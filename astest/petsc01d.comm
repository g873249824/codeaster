DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),);

MA=LIRE_MAILLAGE(FORMAT='MED',);

for matr_distribuee in ('OUI', 'NON') :
    for type_partition in (1,2,3,4) :
         if  type_partition == 1 :
             MO=AFFE_MODELE(MAILLAGE=MA,
                        AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='3D',),
                        PARTITION=_F(PARALLELISME='GROUP_ELEM+', NB_PART=3))
         elif  type_partition == 2 :
             MO=AFFE_MODELE(MAILLAGE=MA,
                        AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='3D',),
                        PARTITION=_F(PARALLELISME='GROUP_ELEM'))
         elif  type_partition == 3 :
             MO=AFFE_MODELE(MAILLAGE=MA,
                        AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='3D',),
                        PARTITION=_F(PARALLELISME='MAIL_CONTIGU'))
         elif  type_partition == 4 :
             MO=AFFE_MODELE(MAILLAGE=MA,
                        AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='3D',),
                        PARTITION=_F(PARALLELISME='MAIL_DISPERSE'))
         else :
             assert 0, type_partition


         MAT=DEFI_MATERIAU(ELAS=_F(E=202702.7,  NU=0.3,),);

         CHMAT=AFFE_MATERIAU(MAILLAGE=MA, AFFE=_F(TOUT='OUI', MATER=MAT,),);

         CHA1=AFFE_CHAR_MECA(MODELE=MO,
                             DDL_IMPO=(_F(GROUP_NO='GRNO1',
                                          DZ=0.0,),
                                       _F(GROUP_NO='GRNO8',
                                          DY=0.0,),),);

         CHA2=AFFE_CHAR_CINE(MODELE=MO,
                             MECA_IMPO=_F(GROUP_NO='GRNO7',
                                          DX=0.0,
                                          DY=0.01,),);
         MESTAT=MECA_STATIQUE(MODELE=MO,
                             INFO=2,
                             CHAM_MATER=CHMAT,
                             EXCIT=(_F(CHARGE=CHA1,),
                                    _F(CHARGE=CHA2,),
                                    ),
                             SOLVEUR=_F(METHODE='PETSC',  MATR_DISTRIBUEE=matr_distribuee,
                                        ALGORITHME='CR',
                                        PRE_COND='LDLT_SP',),
                             );

         # En présence d'un AFFE_CHAR_MECA,
         # le solveur CR donne des résultats corrects mais
         # variables d'une exécution à l'autre. On supprime
         # les tests qui posent probleme pour éviter les NOOK de non-régression
         TEST_RESU(RESU=(_F(RESULTAT=MESTAT,
                            NUME_ORDRE=1,
                            NOM_CHAM='DEPL',
                            NOM_CMP='DY',
                            NOEUD='NO2958',
                            VALE_CALC=0.0099999997769,
                            VALE_REFE=0.01,
                            TOLE_MACHINE=1.e-5,
                            PRECISION=0.00015,
                            REFERENCE='AUTRE_ASTER',),
                         _F(RESULTAT=MESTAT,
                            NUME_ORDRE=1,
                            NOM_CHAM='DEPL',
                            NOM_CMP='DX',
                            NOEUD='NO2974',
                            VALE_CALC=0.00430057989488,
                            VALE_REFE=0.0043006,
                            TOLE_MACHINE=1.e-5,
                            PRECISION=0.00015,
                            REFERENCE='AUTRE_ASTER',),
                         _F(RESULTAT=MESTAT,
                            NUME_ORDRE=1,
                            NOM_CHAM='DEPL',
                            NOM_CMP='DY',
                            NOEUD='NO2974',
                            VALE_CALC=0.00928897690092,
                            VALE_REFE=0.009289,
                            TOLE_MACHINE=1.e-5,
                            PRECISION=0.00015,
                            REFERENCE='AUTRE_ASTER',),
                        ),);
         DETRUIRE(CONCEPT=_F(NOM=(MO,MAT,CHMAT,CHA1,CHA2,MESTAT,)),INFO=1,)

FIN();
