                   VALE_CALC=0.00430057989488,
                   VALE_REFE=0.0043006,
                   TOLE_MACHINE=1.e-5,
                   PRECISION=0.00015,
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=VECAS,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   NOEUD='NO2974',
                   VALE_CALC=0.00928897690092,
                   VALE_REFE=0.009289,
                   TOLE_MACHINE=1.e-5,
                   PRECISION=0.00015,
                   REFERENCE='AUTRE_ASTER',),
