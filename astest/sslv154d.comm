from sslv154a import lissGchap, lissGbar

DEBUT(PAR_LOT='NON', CODE=_F(NIV_PUB_WEB='INTERNET',
              ),
      DEBUG=_F(SDVERI='OUI'))

# MODELISATION D : FEM, ZCRACKS, MAILLAGE QUADRATIQUE

# parametres utiles
#------------------

# angle d'inclinaison de la force imposee
alpha = pi / 4.0

# valeur nominale de la force imposee
sig = 1000000.

# rayon de la fissure
a = 2

#-----------------------------------------------------------------------------------------------------------------------------
#                                 CALCUL
#-----------------------------------------------------------------------------------------------------------------------------

MA=LIRE_MAILLAGE(FORMAT='MED')

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(OPTION='NOEUD_ORDO',
                               NOM='NFF3',
                               GROUP_MA='LFF1',
                               GROUP_NO_ORIG='NFF1',
                               GROUP_NO_EXTR='NFF2',),);

# Barsoum
MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                GROUP_NO_FOND='NFF3'))

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_3D=_F(GROUP_MA=('FACE_AV','FACE_SUP','FACE_INF','FACE_LAD','FACE_LAG',),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

E=2.E11
nu = 0.3
MAT=DEFI_MATERIAU(ELAS=_F(E=E,
                          NU=nu),)

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);

SYMETR=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=(_F(GROUP_NO='D1',
                                   DX=0,
                                   DZ=0,),
                                _F(GROUP_NO='D2',
                                   DZ=0,),),
                      FACE_IMPO=_F(GROUP_MA='FACE_AV',
                                   DY=0.,),);

PRESSION=AFFE_CHAR_MECA(MODELE=MO,
                        FORCE_FACE=(_F(GROUP_MA='FACE_SUP', FX= cos(alpha)*sin(alpha)*sig, FZ= sin(alpha)*sin(alpha)*sig ),
                                    _F(GROUP_MA='FACE_INF', FX=-cos(alpha)*sin(alpha)*sig, FZ=-sin(alpha)*sin(alpha)*sig ),
                                    _F(GROUP_MA='FACE_LAG', FX=-cos(alpha)*cos(alpha)*sig, FZ=-cos(alpha)*sin(alpha)*sig ),
                                    _F(GROUP_MA='FACE_LAD', FX= cos(alpha)*cos(alpha)*sig, FZ= cos(alpha)*sin(alpha)*sig ),
                                    )
                        )

RESU=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=PRESSION,),
                          _F(CHARGE=SYMETR,),),
                   #SOLVEUR=_F(METHODE='PETSC'),
                   )


#-----------------------------------------------------------------------------------------------------------------------------
#                                 POST-TRAITEMENT
#-----------------------------------------------------------------------------------------------------------------------------

FISS=DEFI_FOND_FISS(MAILLAGE=MA, SYME='NON',
                    FOND_FISS=_F(GROUP_MA='LFF1',
                               GROUP_NO_ORIG='NFF1',
                               GROUP_NO_EXTR='NFF2',
                    ),
                    LEVRE_SUP=_F(GROUP_MA='LEVSUP'),
                    LEVRE_INF=_F(GROUP_MA='LEVINF'),
                    )

RSUP = 0.22
RINF = 0.06

# Methode G-theta
CG=CALC_G(THETA=_F(FOND_FISS=FISS,
                   R_INF=RINF,
                   R_SUP=RSUP),
          LISSAGE=_F(LISSAGE_THETA='LEGENDRE',
                     LISSAGE_G='LEGENDRE',),
          RESULTAT=RESU,
          OPTION='CALC_K_G')

CG1=CALC_G(THETA=_F(FOND_FISS=FISS,
                   R_INF=RINF,
                   R_SUP=RSUP),
          LISSAGE=_F(LISSAGE_THETA='LEGENDRE',
                     LISSAGE_G='LEGENDRE',),
          RESULTAT=RESU,)

CG2=CALC_G(THETA=_F(FOND_FISS=FISS,
                   R_INF=RINF,
                   R_SUP=RSUP,
                   NB_POINT_FOND=21),
          LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                     LISSAGE_G='LAGRANGE',),
          RESULTAT=RESU,
          OPTION='CALC_K_G')

CG3=CALC_G(THETA=_F(FOND_FISS=FISS,
                   R_INF=RINF,
                   R_SUP=RSUP,
                   NB_POINT_FOND=21),
          LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                     LISSAGE_G='LAGRANGE',),
          RESULTAT=RESU,)
          
CG4=CALC_G(THETA=_F(FOND_FISS=FISS,
                   R_INF=RINF,
                   R_SUP=RSUP,
                   ),
          LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                     LISSAGE_G='LAGRANGE',),
          RESULTAT=RESU,
          OPTION='CALC_K_G')
                
# formule pour le calcul de l'angle en degre
ANGLE = FORMULE(VALE='ABSC_CURV/a * 180./pi',
                a=a,
                NOM_PARA='ABSC_CURV',)

# formules pour les valeurs de reference
K1REF = FORMULE(VALE=' 2/pi * sig * sin(alpha)**2 * sqrt(pi*a) ',
                sig=sig,
                alpha=alpha,
                a=a,
                NOM_PARA='ANGLE',)
K2REF = FORMULE(VALE=' 4./(pi*(2.-nu)) * sig*sin(alpha)*cos(alpha) * cos(ANGLE/180*pi) * sqrt(pi*a)',
                nu=nu,
                sig=sig,
                alpha=alpha,
                a=a,
                NOM_PARA='ANGLE',)
K3REF = FORMULE(VALE=' 4.*(1 - nu)/(pi*(2.-nu)) * sig*sin(alpha)*cos(alpha) * sin(ANGLE/180*pi) * sqrt(pi*a) ',
                nu=nu,
                sig=sig,
                alpha=alpha,
                a=a,
                NOM_PARA='ANGLE',)
GREF = FORMULE(VALE=' (1-nu**2)/E * ( K1REF(ANGLE)**2 + K2REF(ANGLE)**2 ) + (1+nu)/E * K3REF(ANGLE)**2 ',
               nu=nu,
               E=E,
               K1REF=K1REF,
               K2REF=K2REF,
               K3REF=K3REF,
               NOM_PARA='ANGLE',)

# ajout des colonnes 'angle' et valeurs de ref dans les tables
CG=CALC_TABLE(TABLE=CG,
              reuse=CG,
              ACTION=(_F(OPERATION='OPER',FORMULE=ANGLE,NOM_PARA='ANGLE'),
                      _F(OPERATION='OPER',FORMULE=K1REF,NOM_PARA='K1REF'),
                      _F(OPERATION='OPER',FORMULE=K2REF,NOM_PARA='K2REF'),
                      _F(OPERATION='OPER',FORMULE=K3REF,NOM_PARA='K3REF'),
                      _F(OPERATION='OPER',FORMULE=GREF ,NOM_PARA='GREF'),
                      )
              )

CG1=CALC_TABLE(TABLE=CG1,
              reuse=CG1,
              ACTION=(_F(OPERATION='OPER',FORMULE=ANGLE,NOM_PARA='ANGLE'),
                      _F(OPERATION='OPER',FORMULE=GREF ,NOM_PARA='GREF'),
                      )
              )

CG2=CALC_TABLE(TABLE=CG2,
              reuse=CG2,
              ACTION=(_F(OPERATION='OPER',FORMULE=ANGLE,NOM_PARA='ANGLE'),
                      _F(OPERATION='OPER',FORMULE=K1REF,NOM_PARA='K1REF'),
                      _F(OPERATION='OPER',FORMULE=K2REF,NOM_PARA='K2REF'),
                      _F(OPERATION='OPER',FORMULE=K3REF,NOM_PARA='K3REF'),
                      _F(OPERATION='OPER',FORMULE=GREF ,NOM_PARA='GREF'),
                      )
              )

CG3=CALC_TABLE(TABLE=CG3,
              reuse=CG3,
              ACTION=(_F(OPERATION='OPER',FORMULE=ANGLE,NOM_PARA='ANGLE'),
                      _F(OPERATION='OPER',FORMULE=GREF ,NOM_PARA='GREF'),
                      )
              )
              
CG4=CALC_TABLE(TABLE=CG4,
              reuse=CG4,
              ACTION=(_F(OPERATION='OPER',FORMULE=ANGLE,NOM_PARA='ANGLE'),
                      )
              )

CG4=CALC_TABLE(TABLE=CG4,
              reuse=CG4,
              ACTION=(_F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'G_CHAP',VALE_COLONNE =  lissGchap(CG4)),
                      _F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'G_BAR', VALE_COLONNE =  lissGbar(CG4)),
                      _F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'K1_CHAP',VALE_COLONNE = lissGchap(CG4, val='K1')),
                      _F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'K1_BAR', VALE_COLONNE = lissGbar(CG4,  val='K1')),
                      _F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'K2_CHAP',VALE_COLONNE = lissGchap(CG4, val='K2')),
                      _F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'K2_BAR', VALE_COLONNE = lissGbar(CG4,  val='K2')),
                      _F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'K3_CHAP',VALE_COLONNE = lissGchap(CG4, val='K3')),
                      _F(OPERATION = 'AJOUT_COLONNE',NOM_PARA = 'K3_BAR', VALE_COLONNE = lissGbar(CG4,  val='K3')),
                      )
              )

# impression de chaque table
IMPR_TABLE(TABLE=CG)
IMPR_TABLE(TABLE=CG1)
IMPR_TABLE(TABLE=CG2)
IMPR_TABLE(TABLE=CG3)
IMPR_TABLE(TABLE=CG4)




