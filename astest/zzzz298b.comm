# MODELISATION B : FISSURE X-FEM, RESOLUTION MECANIQUE

DEBUT(CODE=_F(NOM='ZZZZ298B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),IGNORE_ALARM=('POST0_24','RUPTURE0_46','RUPTURE0_6',),);

#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------

MAILLAGE=LIRE_MAILLAGE(UNITE=20, 
                       FORMAT='MED',);

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURFINF',),),);

#----------------------------------------------
#                   MODELE 
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAGE,
                     AFFE=_F(GROUP_MA=('VOL','SURFINF','SURFSUP',),
                             PHENOMENE='MECANIQUE',
                             MODELISATION='3D',),);

MAILLAGE=MODI_MAILLAGE(reuse =MAILLAGE,
                       MAILLAGE=MAILLAGE,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFINF','SURFSUP',),),);

#----------------------------------------------
#                   FISSURE 
#----------------------------------------------

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-0.5');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-Y-X/2.+1.');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    DEFI_FISS=_F(FONC_LT=LT,
                                 FONC_LN=LN,),);
                                 
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,);


#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=205000.E6
nu=0.

FONC1=DEFI_FONCTION(    NOM_PARA='TEMP',
                           VALE=(  -1000.,          0.3,
                                   +1000.,           0.3, ))
FONC2=DEFI_CONSTANTE(VALE=205000.E6)                                                             
                                                    
MAT=DEFI_MATERIAU(ELAS_FO=_F( E = FONC2, NU = FONC1, TEMP_DEF_ALPHA=20.),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,),);

CHAMPMA=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                      MODELE=MODELEK,
                      AFFE=(_F(GROUP_MA=('VOL','SURFINF','SURFSUP',),
                               MATER=ACIER,),),);

#----------------------------------------------
#                   CHARGEMENTS 
#----------------------------------------------

CHA1=AFFE_CHAR_MECA(MODELE=MODELEK,
                    DDL_IMPO=(_F(GROUP_NO='SURFINF',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,),),);
                                
CHA2=AFFE_CHAR_MECA(MODELE=MODELEK,
                    PRES_REP=_F(GROUP_MA='SURFSUP',
                                PRES=-1.E6 ,),)
                                    
#----------------------------------------------
#                   RESOLUTIONS
#----------------------------------------------

UTOT1x=MECA_STATIQUE(MODELE=MODELEK,
                     CHAM_MATER=CHAMPMA,
                     EXCIT=(
                            _F(CHARGE=CHA1),
                            _F(CHARGE=CHA2),),); 
                   
#----------------------------------------------
#                   POST-TRAITEMENT
#----------------------------------------------

TABK = POST_K1_K2_K3(MODELISATION='3D',
                     FISSURE=FISS,
                     MATER=MAT,
                     RESULTAT=UTOT1x,
                     ABSC_CURV_MAXI = 0.066,)

IMPR_TABLE(TABLE=TABK)

#----------------------------------------------
#                   TEST
#----------------------------------------------

k1_deb = 9.35585e5
k1_fin = 1.45592e6

TEST_TABLE(
           VALE_CALC=9.35585E5,
           NOM_PARA='K1',
           TABLE=TABK,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=24,),
           )

TEST_TABLE(
           VALE_CALC=9.35585E5,
           NOM_PARA='K1',
           TABLE=TABK,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=23,),
           )

TEST_TABLE(
           VALE_CALC=9.35585E5,
           NOM_PARA='K1',
           TABLE=TABK,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=22,),
           )

TEST_TABLE(
           VALE_CALC=1.4559178114059E6,
           NOM_PARA='K1',
           TABLE=TABK,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=2,),
           )

FIN();
