           )

TEST_TABLE(
           VALE_CALC=851382.586304,
           NOM_PARA='K1',
           TABLE=TABK,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=22,),
           )

TEST_TABLE(
           VALE_CALC=1324885.20838,
           NOM_PARA='K1',
           TABLE=TABK,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=2,),
           )

#---
# 2. avec la variable de commande TEMP
#---

TEST_TABLE(
           VALE_CALC=851382.586304,
           NOM_PARA='K1',
           TABLE=TABKTEMP,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=24,),
           )

TEST_TABLE(
           VALE_CALC=851382.586304,
           NOM_PARA='K1',
           TABLE=TABKTEMP,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=23,),
           )

TEST_TABLE(
           VALE_CALC=851382.586304,
           NOM_PARA='K1',
           TABLE=TABKTEMP,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=22,),
           )

TEST_TABLE(
           VALE_CALC=1324885.20838,
           NOM_PARA='K1',
           TABLE=TABKTEMP,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=2,),
           )

#---
# 3. avec la variable de commande NEUT1
#---

TEST_TABLE(
           VALE_CALC=851382.586304,
           NOM_PARA='K1',
           TABLE=TABKNEUT,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=24,),
           )

TEST_TABLE(
           VALE_CALC=851382.586304,
           NOM_PARA='K1',
           TABLE=TABKNEUT,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=23,),
           )

TEST_TABLE(
           VALE_CALC=851382.586304,
           NOM_PARA='K1',
           TABLE=TABKNEUT,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=22,),
           )

TEST_TABLE(
           VALE_CALC=1324885.20838,
           NOM_PARA='K1',
           TABLE=TABKNEUT,
           FILTRE=_F(NOM_PARA='NUM_PT',
                     VALE_I=2,),
           )

FIN();
