DEBUT(CODE=_F(NOM='SSLS123A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAIL=LIRE_MAILLAGE();

MAIL=MODI_MAILLAGE(reuse=MAIL,MAILLAGE=MAIL,
ORIE_SHB=_F(GROUP_MA='VOLTOT'),)

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='SUD1',),
                               _F(GROUP_MA='SUG1',),
                               _F(GROUP_MA='SUB1',),),);

MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB',),);

MAIL=MODI_MAILLAGE(MAILLAGE=MAIL,reuse=MAIL,
                   ORIE_PEAU_3D=_F(GROUP_MA='SEXT',),
                   );

MAT=DEFI_MATERIAU(ELAS=_F(E=6.825E7,
                          NU=0.3,),);

CHM=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          MATER=MAT,),);


CHARGT=AFFE_CHAR_MECA(MODELE=MODMECA,
                      DDL_IMPO=(_F(GROUP_MA=('SUD1'),
                                   DY=0.0,),
                                _F(GROUP_MA='SUG1',
                                   DX=0.0,),
                                _F(GROUP_MA='SUB1',
                                   DZ=0.0,),),
                      PRES_REP=_F(GROUP_MA='SEXT',
                                   PRES=1.,));

RESUT=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHM,
                    EXCIT=_F(CHARGE=CHARGT,),);

RESUT=CALC_CHAMP(reuse =RESUT,
                 CONTRAINTE=('SIEF_ELNO'),
                RESULTAT=RESUT,);


TEST_RESU(RESU=(
            _F(  RESULTAT = RESUT, NOM_CHAM='DEPL', PRECISION = 1.3E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'PAE',      NOM_CMP = 'DX',           VALE = -1.28279E-5),

            _F(  RESULTAT = RESUT, NOM_CHAM='DEPL', PRECISION = 1.3E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'PBE',      NOM_CMP = 'DY',           VALE = -1.28279E-5),

            _F(   RESULTAT = RESUT, NOM_CHAM='DEPL',  PRECISION = 1.7E-2, REFERENCE='ANALYTIQUE',NUME_ORDRE=1,
              GROUP_NO = 'PCE',      NOM_CMP = 'DZ',           VALE = -1.28279E-5),
              ),)
