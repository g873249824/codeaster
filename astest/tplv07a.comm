#
# LECTURE DU MAILLAGE --------------------------------------------------
#


MAIL=LIRE_MAILLAGE( )

MAIL=DEFI_GROUP(   reuse=MAIL,   MAILLAGE=MAIL,
         CREA_GROUP_MA=_F(  NOM = 'TOUT',   TOUT = 'OUI'))

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ----------------------------
#

# ----- MATERIAU


MATER=DEFI_MATERIAU(   THER_ORTH=_F(  LAMBDA_L = 1.00, LAMBDA_T = 0.75,
                                        LAMBDA_N = 0.50,   RHO_CP = 1.)
                         )

#
# AFFECTATIONS: MATERIAU, MODELE, CHARGEMENT ---------------------------
#


CHMAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',  MATER = MATER)
                        )

MOTH=AFFE_MODELE(   MAILLAGE=MAIL,
                        AFFE=_F(  TOUT = 'OUI',
                               PHENOMENE = 'THERMIQUE',
                               MODELISATION = '3D')
                      )

ORIEN=AFFE_CARA_ELEM(   MODELE=MOTH,
                            MASSIF=_F(  GROUP_MA = 'TOUT',
                                     ANGL_REP = (0.,0.,0.,))
                         )

FLU_Y=DEFI_FONCTION(  NOM_PARA='Y',
                           VALE=(-0.1E0,  60.E0,
                                  0.1E0, -60.E0, ),
                          INTERPOL='NON'
                          )

FLU_Z=DEFI_FONCTION(  NOM_PARA='Z',
                           VALE=(-0.1E0,  30.E0,
                                  0.1E0, -30.E0, ),
                               INTERPOL='LIN'
                          )

HCV=DEFI_CONSTANTE(   VALE=15.E0 )

TARY0=DEFI_FONCTION(  NOM_PARA='Y',
                           VALE=( 0.0E0,   30.E0,
                                  1.0E0,  -50.E0, ),
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='LINEAIRE'
                        )

TARY1=DEFI_FONCTION(  NOM_PARA='Y',
                           VALE=( 0.0E0,  -30.E0,
                                  1.0E0, -110.E0, ),
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='LINEAIRE'
                        )

TEX_ARR=DEFI_NAPPE(  NOM_PARA='Z',
                               PARA=( 0.0E0,   1.0E0, ),
                           FONCTION=( TARY0,    TARY1, ),
                             PROL_DROITE='LINEAIRE',
                            PROL_GAUCHE='LINEAIRE'
                          )

TAVY0=DEFI_FONCTION(  NOM_PARA='Y',
                           VALE=( 0.0E0,   15.E0,
                                  1.0E0,  -65.E0, ),
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='LINEAIRE'
                        )

TAVY1=DEFI_FONCTION(  NOM_PARA='Y',
                           VALE=( 0.0E0,  -45.E0,
                                  1.0E0, -125.E0, ),
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='LINEAIRE'
                        )

TEX_AVA=DEFI_NAPPE(  NOM_PARA='Z',
                               PARA=( 0.0E0,   1.0E0, ),
                           FONCTION=( TAVY0,    TAVY1, ),
                             PROL_DROITE='LINEAIRE',
                            PROL_GAUCHE='LINEAIRE'
                          )

CHTH=AFFE_CHAR_THER_F(   MODELE=MOTH,FLUX_REP=(
                            _F(  GROUP_MA = 'F_NPJI',   FLUN = FLU_Y),
                            _F(  GROUP_MA = 'F_RQKL',   FLUN = FLU_Y),
                            _F(  GROUP_MA = 'F_NPQR',   FLUN = FLU_Z),
                            _F(  GROUP_MA = 'F_IJKL',   FLUN = FLU_Z)),ECHANGE=(
                             _F(  GROUP_MA = 'F_NRLI', COEF_H = HCV,
                                                     TEMP_EXT = TEX_ARR),
                             _F(  GROUP_MA = 'F_PQKJ', COEF_H = HCV,
                                                     TEMP_EXT = TEX_AVA))
                        )

#
# DEFINITION DE LA STRATEGIE DE CALCUL ---------------------------------
#

#
# RESOLUTION -----------------------------------------------------------
#

TEMPE=THER_LINEAIRE(   MODELE=MOTH,
                           CHAM_MATER=CHMAT,
                            EXCIT=_F(  CHARGE = CHTH),
                            CARA_ELEM=ORIEN
                         )

#
# ANALYSE DES RESULTATS ------------------------------------------------
#


