                   NOEUD='N5',
                   NOM_CMP='SIYY',
                   VALE_CALC=-8.73681044E-09,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSP_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPXX',
                   VALE_CALC=-1.16665876E-19,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U,
                   NOM_CHAM='EPSP_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPYY',
                   VALE_CALC=-1.25934367E-18,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                ),
          )

# Resultat avec COMP_ELAS
U3=STAT_NON_LINE( MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=_F(CHARGE=CH1,),
                COMP_ELAS=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=1e-06,
                               ITER_GLOB_MAXI=25),);
U3=CALC_CHAMP(reuse=U3,RESULTAT=U3,CONTRAINTE=('SIGM_ELNO'),DEFORMATION=('EPSI_ELNO','EPSP_ELNO'))


U3=CALC_CHAMP(reuse =U3,
             RESULTAT = U3,
             DEFORMATION=('EPSI_NOEU','EPSP_NOEU'),CONTRAINTE='SIGM_NOEU')

TEST_RESU(RESU=(_F(INST=3600.0,
