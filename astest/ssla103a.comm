                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U3,
                   NOM_CHAM='EPSP_NOEU',
                   NOEUD='N5',
                   NOM_CMP='EPXX',
                   VALE_CALC=-1.87415891E-19,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999995E-07,),
                _F(INST=3600.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=U3,
