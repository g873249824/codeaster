# DETAILS : MODELISATION : DKTG (GLRC_DM)
#           MAILLAGE     : TRIA3
#           CHARGEMENT   : CYCLIQUE TRACTION - COMPRESSION
#           REMARQUE 1   :ON TESTE UNE VALEUR DE ALPHA_C DIFFERENTE DE 1

DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI',),);

EP = 0.1;
DEP0 = 0.002;

t1 = 1.0;

t2 = (3 * t1);

t3 = (4 * t1);

dt = 0.05;

MAIL=LIRE_MAILLAGE(FORMAT="ASTER",);

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='A1A2',),
                               _F(GROUP_MA='A4A3',),
                               _F(GROUP_MA='A2A4',),
                               _F(GROUP_MA='A3A1',),),);

MAIL=MODI_MAILLAGE(reuse =MAIL,
                   MAILLAGE=MAIL,
                   ORIE_NORM_COQUE=_F(GROUP_MA='DALLE',
                                      VECT_NORM=(0.,0.,1.,),
                                      GROUP_NO='A1',),);

MA=CREA_MAILLAGE(MAILLAGE=MAIL,
                 CREA_MAILLE=(_F(NOM='DALLE_G',
                                   GROUP_MA='DALLE',
                                   PREF_MAILLE='G',),),);

MO_G=AFFE_MODELE(MAILLAGE=MA,
                 AFFE=_F(GROUP_MA='DALLE_G',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='DKTG',),);

EGM = 35620000000.0;
EGF = 38700000000.0;

MAT_G=DEFI_MATERIAU(ELAS_GLRC=_F(E_M=EGM,
                                 NU_M=0.18,
                                 E_F=EGF,
                                 NU_F=0.17,),
                    GLRC_DM=_F(GAMMA_T=0.02,
                               GAMMA_C=0.02,
                               GAMMA_F=0.32,
                               ALPHA_C=100.,
                               NYT=3.7E5,
                               NYC=-3.7E6,
                               MYF=9.0E3,),);

MATER=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='DALLE_G',
                             MATER=MAT_G,),),);

CAEL_G=AFFE_CARA_ELEM(MODELE=MO_G,
                      COQUE=_F(GROUP_MA='DALLE_G',
                               EPAIS=EP,
                               ANGL_REP=(0.0,0.0,),
                               INER_ROTA='OUI',),);

COND_0G=AFFE_CHAR_MECA(MODELE=MO_G,
                       DDL_IMPO=(_F(GROUP_NO='A1',
                                    DX=0.0,
                                    DY=0.0,
                                    DZ=0.0,
                                    DRX=0.0,
                                    DRY=0.0,
                                    DRZ=0.0,),
                                 _F(GROUP_MA='A3A1',
                                    DX=0.0,
                                    DRY=0.0,),),);

COND_1G=AFFE_CHAR_MECA(MODELE=MO_G,
                       DDL_IMPO=_F(GROUP_MA='A2A4',
                                   DX=DEP0,),);

chf1=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,
                         t1,1.,
                         t2,-5.,
                         t3,0.0,),
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='CONSTANT',);

inst1=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=t3,
                                   PAS=dt,),);

lisnt1=DEFI_LIST_INST(METHODE='MANUEL',
                      DEFI_LIST=_F(LIST_INST=inst1,),
                      ECHEC=_F(ACTION        = 'DECOUPE',
                               SUBD_METHODE  = 'MANUEL',),);

#-----------------------------------------------
# CALCUL DES RESULTATS POUR LE CHARGEMENT chf1
#-----------------------------------------------

RESU_G1=STAT_NON_LINE(MODELE=MO_G,
                      CHAM_MATER=MATER,
                      CARA_ELEM=CAEL_G,
                      EXCIT=(_F(CHARGE=COND_0G,
                                FONC_MULT=chf1,),
                             _F(CHARGE=COND_1G,
                                FONC_MULT=chf1,),),
                      COMPORTEMENT=_F(RELATION='GLRC_DM',
                                   ITER_INTE_MAXI=30,),
                      INCREMENT=_F(LIST_INST=lisnt1,
                                   PRECISION=0.1,),
                      NEWTON=_F(REAC_ITER=1,),
                      CONVERGENCE=_F(ITER_GLOB_MAXI=25,
                                     RESI_GLOB_MAXI=1.E-4,),
                      ARCHIVAGE=_F(LIST_INST=inst1,),);

RESU_G1=CALC_CHAMP(reuse =RESU_G1,
                   RESULTAT=RESU_G1,
                   FORCE='FORC_NODA',
                   VARI_INTERNE=('VARI_ELNO','VARI_NOEU',),
                   ENERGIE=('DISS_ELGA','DISS_ELNO','DISS_NOEU'),);

IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT=RESU_G1,
                  NOM_CHAM='DISS_ELGA'),);

IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT=RESU_G1,
                  NOM_CHAM='DISS_ELNO'),);

TAB_DISS=POST_ELEM(RESULTAT=RESU_G1,
                   ENER_DISS=_F(TOUT='OUI'),);

IMPR_TABLE(TABLE=TAB_DISS,);

TFXA4_G1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='A4FX',
                                 RESULTAT=RESU_G1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A4',
                                 NOM_CMP='DX',),);

TFXA4_G1=CALC_TABLE(reuse =TFXA4_G1,
               TABLE=TFXA4_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TFXA2_G1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='A2FX',
                                 RESULTAT=RESU_G1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A2',
                                 NOM_CMP='DX',),);

TFXA2_G1=CALC_TABLE(reuse =TFXA2_G1,
               TABLE=TFXA2_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TFXA6_G1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='A6FX',
                                 RESULTAT=RESU_G1,
                                 NOM_CHAM='FORC_NODA',
                                 GROUP_NO='A6',
                                 NOM_CMP='DX',),);


TFXA6_G1=CALC_TABLE(reuse =TFXA6_G1,
               TABLE=TFXA6_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TD_G1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                              INTITULE='A4FX',
                              RESULTAT=RESU_G1,
                              NOM_CHAM='VARI_NOEU',
                              GROUP_NO='A4',
                              NOM_CMP=('V1','V2',),),);

TD_G1=CALC_TABLE(reuse =TD_G1,
               TABLE=TD_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TD_G1=CALC_TABLE(reuse =TD_G1,
                 TABLE=TD_G1,
                 ACTION=(_F(OPERATION='EXTR',
                            NOM_PARA=('INST','V1','V2',),),
                         _F(OPERATION='RENOMME',
                            NOM_PARA=('V1','D1',),),
                         _F(OPERATION='RENOMME',
                            NOM_PARA=('V2','D2',),),),);

TDISS_G1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='A5FX',
                                 RESULTAT=RESU_G1,
                                 NOM_CHAM='DISS_NOEU',
                                 GROUP_NO='A4',
                                 NOM_CMP=('ENDO',),),);

TDISS_G1=CALC_TABLE(reuse =TDISS_G1,
               TABLE=TDISS_G1,
               ACTION=(_F(
                          OPERATION = 'FILTRE',
                          NOM_PARA  = 'NUME_ORDRE',
                          CRIT_COMP = 'GT',
                          VALE    = 0,
                         ),),);

TDISS_G1=CALC_TABLE(reuse =TDISS_G1,
                    TABLE=TDISS_G1,
                    ACTION=_F(OPERATION='EXTR',
                              NOM_PARA=('INST','ENDO',),),);

TFXA2_G1=CALC_TABLE(reuse =TFXA2_G1,
                    TABLE=TFXA2_G1,
                    ACTION=(_F(OPERATION='EXTR',
                               NOM_PARA=('INST','DX',),),
                            _F(OPERATION='RENOMME',
                               NOM_PARA=('DX','FXA2',),),),);

TFXA4_G1=CALC_TABLE(reuse =TFXA4_G1,
                    TABLE=TFXA4_G1,
                    ACTION=(_F(OPERATION='EXTR',
                               NOM_PARA=('INST','DX',),),
                            _F(OPERATION='RENOMME',
                               NOM_PARA=('DX','FXA4',),),),);

TFXA6_G1=CALC_TABLE(reuse =TFXA6_G1,
                    TABLE=TFXA6_G1,
                    ACTION=(_F(OPERATION='EXTR',
                               NOM_PARA=('INST','DX',),),
                            _F(OPERATION='RENOMME',
                               NOM_PARA=('DX','FXA6',),),),);

SUM_FX = FORMULE(VALE='FXA2+FXA4+FXA6',
                 NOM_PARA=('FXA2','FXA4','FXA6',),);

D_REL_FX = FORMULE(VALE='(FXG-FX)/FX',
                   NOM_PARA=('FX','FXG',),);

D_REL_DY = FORMULE(VALE='(DYG-DY)/DY',
                   NOM_PARA=('DY','DYG',),);

TFX_G1=CALC_TABLE(TABLE=TFXA2_G1,
                  ACTION=(_F(OPERATION='FILTRE',
                             NOM_PARA='INST',
                             CRIT_COMP = 'GT',
                             VALE = 0.,),
                           _F(OPERATION='COMB',
                             TABLE=TFXA4_G1,
                             NOM_PARA='INST',),
                          _F(OPERATION='COMB',
                             TABLE=TFXA6_G1,
                             NOM_PARA='INST',),
                          _F(OPERATION='OPER',
                             FORMULE=SUM_FX,
                             NOM_PARA='FXG',),
                          _F(OPERATION='EXTR',
                             NOM_PARA=('INST','FXG',),),),);

IMPR_TABLE(TABLE=TFX_G1,);

DETRUIRE(CONCEPT=_F(NOM=(TFXA2_G1,TFXA4_G1,TFXA6_G1,),),);

TDY_G1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='A4DY',
                               RESULTAT=RESU_G1,
                               NOM_CHAM='DEPL',
                               GROUP_NO='A4',
                               NOM_CMP='DY',),);

TDY_G1=CALC_TABLE(reuse =TDY_G1,
                  TABLE=TDY_G1,
                  ACTION=(_F(OPERATION = 'FILTRE',
                             NOM_PARA  = 'NUME_ORDRE',
                             CRIT_COMP = 'GT',
                             VALE    = 0,),),);

TDY_G1=CALC_TABLE(reuse =TDY_G1,
                  TABLE=TDY_G1,
                  ACTION=(_F(OPERATION='EXTR',
                             NOM_PARA=('INST','DY',),),
                          _F(OPERATION='RENOMME',
                             NOM_PARA=('DY','DYG',),),),);

D_DY_1=CALC_TABLE(TABLE=TDY_G1,
                  ACTION=(_F(OPERATION='COMB',
                             TABLE=TDY_G1,
                             NOM_PARA='INST',),
                          _F(OPERATION='EXTR',
                             NOM_PARA=('INST','DYG',),),),);

DETRUIRE(CONCEPT=_F(NOM=(TDY_G1,),),);

IMPR_TABLE(TABLE=D_DY_1,);

IMPR_TABLE(TABLE=TD_G1,);

IMPR_TABLE(TABLE=TDISS_G1,);

#----- TRACTION - PHASE ELASTIQUE -----


TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           VALE_CALC=0.0E+00,
           VALE_REFE=0.0,
           NOM_PARA='ENDO',
           TABLE=TDISS_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.05,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=4.0463375509777E5,
           NOM_PARA='FXG',
           TABLE=TFX_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.25,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-4,
           VALE_CALC=-2.7909902623352E-05,
           VALE_REFE=0.0,
           NOM_PARA='DYG',
           TABLE=D_DY_1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.25,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=254.97562079354,
           NOM_PARA='ENDO',
           TABLE=TDISS_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.75,),
           )

#----- TRACTION - PHASE ENDOMMAGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=346.57215692717,
           NOM_PARA='ENDO',
           TABLE=TDISS_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=5.2046600330233E5,
           NOM_PARA='FXG',
           TABLE=TFX_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-4,
           VALE_CALC=-4.3616602919061E-05,
           VALE_REFE=0.0,
           NOM_PARA='DYG',
           TABLE=D_DY_1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,),
           )

#----- TRACTION - PHASE DECHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=-2.6878204520247E6,
           NOM_PARA='FXG',
           TABLE=TFX_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.5,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-3,
           VALE_CALC= 7.3794724856944E-04,
           VALE_REFE=0.0,
           NOM_PARA='DYG',
           TABLE=D_DY_1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.5,),
           )

#----- COMPRESSION - PHASE CHARGEMENT(TOUJOURS ELASTIQUE) -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=-4.1751434275306E6,
           NOM_PARA='FXG',
           TABLE=TFX_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.1,
           VALE_CALC= 6.1639788696219E-03,
           VALE_REFE=0.0,
           NOM_PARA='DYG',
           TABLE=D_DY_1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,),
           )

#----- COMPRESSION - PHASE DECHARGEMENT -----

TEST_TABLE(CRITERE='ABSOLU',
           VALE_CALC=-2.0875717137653E6,
           NOM_PARA='FXG',
           TABLE=TFX_G1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.5,),
           )

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-2,
           VALE_CALC= 3.081989434811E-03,
           VALE_REFE=0.0,
           NOM_PARA='DYG',
           TABLE=D_DY_1,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.5,),
           )

FIN()
