DEBUT( CODE=_F(  NOM = 'SSLS27B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

E=1.0E11

NU=0.25

H=5.E-2

MATER=DEFI_MATERIAU( ELAS=_F(E=E,
                             NU=NU,) )

MAILL=LIRE_MAILLAGE(FORMAT='MED',  )

MAILL=DEFI_GROUP( reuse=MAILL,   MAILLAGE=MAILL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAILL,
                         AFFE=_F(  TOUT = 'OUI', MATER = MATER) )

MOD=AFFE_MODELE(  MAILLAGE=MAILL,
                       AFFE=_F(  TOUT = 'OUI',     MODELISATION = 'DST',
                              PHENOMENE = 'MECANIQUE')  )

CARAC=AFFE_CARA_ELEM(  MODELE=MOD,
                          COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = H) )

CHARG=AFFE_CHAR_MECA( MODELE=MOD,
                          DDL_IMPO=_F(  GROUP_NO = ('AD',),
                                     DX = 0.,  DY = 0.,  DZ = 0.,
                                     DRX = 0.,  DRY = 0.,  DRZ = 0.) )

# ---------------------------------------------------------------------
# FORCES DE SENS OPPOSEES  PERPENDICULAIRES A LA PLAQUE

CHARG1=AFFE_CHAR_MECA( MODELE=MOD,FORCE_NODALE=(
              _F(  GROUP_NO = 'B',   FZ = -1.),
                           _F(  GROUP_NO = 'C',   FZ = 1.))  )

# ---------------------------------------------------------------------
# FORCES DE MEME SENS PERPENDICULAIRES A LA PLAQUE
#         S(FZ)=2

CHARG2=AFFE_CHAR_MECA( MODELE=MOD,FORCE_NODALE=(
              _F(  GROUP_NO = 'B',   FZ = 1.),
                           _F(  GROUP_NO = 'C',   FZ = 1.))  )

# ---------------------------------------------------------------------

RESU1 = MECA_STATIQUE(MODELE=MOD,
                      CARA_ELEM=CARAC,
                      CHAM_MATER=CHMAT,
                      EXCIT=(_F(CHARGE=CHARG,),
                             _F(CHARGE=CHARG1,),),
                      )

RESU1 = CALC_CHAMP(reuse=RESU1,
                RESULTAT=RESU1,
                FORCE='FORC_NODA',)

FORC1=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                  INTITULE='Reactions',
                                  RESULTAT=RESU1,
                                  NOM_CHAM='FORC_NODA',
                                  GROUP_NO=('AD',),
                                  POINT=(0,0,0,),
                                  RESULTANTE=('DX','DY','DZ',),
                                  MOMENT    =('DRX','DRY','DRZ',),),),)

IMPR_TABLE(TABLE=FORC1)
# ---------------------------------------------------------------------

RESU2 = MECA_STATIQUE(MODELE=MOD,
                      CARA_ELEM=CARAC,
                      CHAM_MATER=CHMAT,
                      EXCIT=(_F(CHARGE=CHARG,),
                             _F(CHARGE=CHARG2,),),
                      )

RESU2 = CALC_CHAMP(reuse=RESU2,
                RESULTAT=RESU2,
                FORCE='FORC_NODA',)

FORC2=POST_RELEVE_T(ACTION=(_F(OPERATION='EXTRACTION',
                                  INTITULE='Reactions',
                                  RESULTAT=RESU2,
                                  NOM_CHAM='FORC_NODA',
                                  GROUP_NO=('AD',),
                                  POINT=(0,0,0,),
                                  RESULTANTE=('DX','DY','DZ',),
                                  MOMENT    =('DRX','DRY','DRZ',),),),)

IMPR_TABLE(TABLE=FORC2)

# ---------------------------------------------------------------------

#

TEST_RESU(RESU=(_F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N13',
                   NOM_CMP='DZ',
                   VALE_CALC=-3.52480262E-06,
                   VALE_REFE=-3.5370000000000002E-06,
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                _F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N39',
                   NOM_CMP='DZ',
                   VALE_CALC=-6.138635845248E-17,
                   VALE_REFE=0.0,
                   TOLE_MACHINE=9.9999999999999998E-13,
                   CRITERE='ABSOLU',
                   PRECISION=9.9999999999999998E-13,),
                _F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU1,
                   NOM_CHAM='DEPL',
                   NOEUD='N65',
                   NOM_CMP='DZ',
                   VALE_CALC= 3.52480262E-06,
                   VALE_REFE=3.5370000000000002E-06,
                   CRITERE='RELATIF',
                   PRECISION=5.0000000000000001E-3,),
                _F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOEUD='N13',
                   NOM_CMP='DZ',
                   VALE_CALC= 1.09499326E-03,
                   VALE_REFE=1.1058999999999999E-3,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOEUD='N39',
                   NOM_CMP='DZ',
                   VALE_CALC= 1.09500938E-03,
                   VALE_REFE=1.1058999999999999E-3,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                _F(INST=0.0,
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU2,
                   NOM_CHAM='DEPL',
                   NOEUD='N65',
                   NOM_CMP='DZ',
                   VALE_CALC= 1.09499326E-03,
                   VALE_REFE=1.1058999999999999E-3,
                   CRITERE='RELATIF',
                   PRECISION=1.E-2,),
                ),
          )

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-08,
           VALE_CALC=-2.000000000,
           VALE_REFE=-2,
           NOM_PARA='RESULT_Z',
           TABLE=FORC2,)

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-08,
           VALE_CALC= 8.71480665E-12,
           VALE_REFE=0,
           NOM_PARA='MOMENT_X',
           TABLE=FORC2,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-08,
           VALE_CALC=23.999999996,
           VALE_REFE=24,
           NOM_PARA='MOMENT_Y',
           TABLE=FORC2,)

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-08,
           VALE_CALC=-9.39248679E-14,
           VALE_REFE=0,
           NOM_PARA='RESULT_Z',
           TABLE=FORC1,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-08,
           VALE_CALC=-1.000000000,
           VALE_REFE=-1,
           NOM_PARA='MOMENT_X',
           TABLE=FORC1,)

TEST_TABLE(CRITERE='ABSOLU',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.E-08,
           VALE_CALC=-9.18820575E-13,
           VALE_REFE=0,
           NOM_PARA='MOMENT_Y',
           TABLE=FORC1,)

FIN()
