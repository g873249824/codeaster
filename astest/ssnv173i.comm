           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEPP,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           VALE=0,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEPP,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           VALE=0,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE',);



TEST_TABLE(TABLE=DEPM,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=0.,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEPM,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=0.,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEPM,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=0.,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEPM,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=0.,
           CRITERE='ABSOLU',
           PRECISION=1.0E-16,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEPM,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           VALE=1.E-6,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEPM,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           VALE=1.E-6,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);

FIN();
