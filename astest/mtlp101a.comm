                   MAILLE='M19',),
                _F(INST=240.0,
                   RESULTAT=TEMPE,
                   NOM_CHAM='META_ELNO',
                   NOEUD='N39',
                   NOM_CMP='V1',
                   VALE_CALC=1.000000000,
                   VALE_REFE=0.99990000000000001,
                   REFERENCE='NON_DEFINI',
                   PRECISION=1.E-2, TOLE_MACHINE=(1.E-2, 1.E-4),
                   MAILLE='M13',),
                _F(INST=240.0,
                   RESULTAT=TEMPE,
                   NOM_CHAM='META_ELNO',
                   NOEUD='N70',
                   NOM_CMP='V3',
                   VALE_CALC=20.000000000,
                   VALE_REFE=20.0,
                   REFERENCE='NON_DEFINI',
