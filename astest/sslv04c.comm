DEBUT( CODE=_F( NOM = 'SSLV04C',NIV_PUB_WEB='INTERNET'))

PRE_GIBI()

M=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),   )

DEFI_GROUP(reuse=M,       MAILLAGE=M,
                  CREA_GROUP_NO=_F( NOM = 'DIRI', GROUP_MA = 'DIRI'))

MO=AFFE_MODELE(            MAILLAGE=M,
                                   VERIF='MAILLE',
                                  AFFE=_F(  TOUT = 'OUI',
                                         PHENOMENE = 'MECANIQUE',
                                         MODELISATION = '3D'))

MA=DEFI_MATERIAU(            ELAS=_F(  E = 200000.,
                                         NU = 0.3,
                                         ALPHA = 0.) )

CM=AFFE_MATERIAU(        MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MA))

CH=AFFE_CHAR_MECA(         MODELE=MO,DDL_IMPO=(
                              _F(  GROUP_NO = 'DIRI',  DY = 0.),
                              _F(  GROUP_NO = 'F',     DZ = 0.)),
                             FACE_IMPO=_F(  GROUP_MA = 'DNOR',  DNOR = 0.),
                              PRES_REP=_F(  GROUP_MA = 'PRES',  PRES = 60.))


RESU=MECA_STATIQUE(         MODELE=MO,    CHAM_MATER=CM,
                          EXCIT=_F(  CHARGE = CH),
                      )

CALC_ELEM( reuse= RESU ,RESULTAT= RESU ,              
                          
                      OPTION=('EPSI_ELNO_DEPL', 'SIGM_ELNO_DEPL',))

CALC_NO( reuse=RESU,                      RESULTAT=RESU,
                     OPTION=('EPSI_NOEU_DEPL', 'SIGM_NOEU_DEPL',))

