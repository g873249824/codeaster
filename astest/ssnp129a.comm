DEBUT(CODE=_F(NOM='SSNP129A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(MAILLE='M1',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='DKT',),
                     )

CAEL=AFFE_CARA_ELEM(MODELE=MO,
                    COQUE=_F(MAILLE='M1',
                             EPAIS=1.,),
                    )

ACIER=DEFI_MATERIAU(ELAS=_F(E=30000.0E6,
                            NU=0.2,),
                    BETON_REGLE_PR=_F(D_SIGM_EPSI=-10000.0E6,
                                       SYT=3.0E6,
                                       SYC=30.0E6,
                                       EPSC=1.E-3,
                                       N=2.),)

MATER=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(MAILLE='M1',
                            MATER=ACIER,),)

CHAR0=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(NOEUD=('N1','N2','N3','N4',),
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),
                               _F(NOEUD='N1',
                                  DX=0.0,
                                  DY=0.0,),
                               _F(NOEUD='N2',
                                  DX=0.0,),
                               _F(NOEUD='N4',
                                  DY=0.0,),
                                  ),)

CHAR=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(NOEUD=('N2','N3',),
                                 DY=-1.0,),
                              _F(NOEUD=('N3','N4',),
                                 DX=0.1,),),)

INST=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=(_F(JUSQU_A=1.0,
                                   NOMBRE=1,),
                                _F(JUSQU_A=10.0,
                                   NOMBRE=90,),),)

RAMPE1=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=( 0.0 , 0.0000,
                            1.0 , 0.0001,
                            10. , 0.0020,),)

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=MATER,
                   CARA_ELEM=CAEL,
                   EXCIT=(_F(CHARGE=CHAR0,),
                          _F(CHARGE=CHAR,
                             FONC_MULT=RAMPE1,),),
                   COMP_INCR=_F(RELATION='BETON_REGLE_PR',
                                MAILLE='M1',),
                   INCREMENT=_F(LIST_INST=INST,),
                   NEWTON=_F(REAC_ITER=1,),)

RESU=CALC_CHAMP(reuse =RESU,



               RESULTAT=RESU,
               TOUT_ORDRE='OUI',
               TOUT='OUI',
                CONTRAINTE=('EFGE_ELNO'),)

RESU=CALC_CHAMP(reuse =RESU,
             RESULTAT=RESU,
             FORCE=('FORC_NODA','REAC_NODA'),


             )

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST = 4.6,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DX',
                  NOEUD='N4',
                  VALE=1.29E6,
                  CRITERE='RELATIF',
                  PRECISION=1.E-6,
                  REFERENCE='ANALYTIQUE',),);
TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST = 4.6,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  NOEUD='N2',
                  VALE=-1.4706E7,
                  CRITERE='RELATIF',
                  PRECISION=1.E-6,
                  REFERENCE='ANALYTIQUE',),);
TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST = 10.,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DX',
                  NOEUD='N4',
                  VALE=1.E6,
                  CRITERE='RELATIF',
                  PRECISION=1.E-6,
                  REFERENCE='ANALYTIQUE',),);
TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST = 10.,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  NOEUD='N2',
                  VALE=-1.5E7,
                  CRITERE='RELATIF',
                  PRECISION=1.E-6,
                  REFERENCE='ANALYTIQUE',),);

FIN()
