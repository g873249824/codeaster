                   ),
           )

RELEVE_2=POST_RELEVE_T(ACTION=(
                   _F(  RESULTAT = COUPLA,
                            INTITULE = 'SIEF',
                            NOM_CHAM = 'SIGM_ELNO',
                            NOM_CMP = 'SIXX',
                            LIST_INST = LINST,
                            PRECISION = 1.E-8,
                            GROUP_NO = 'C1',
                            OPERATION = 'EXTRACTION'),
