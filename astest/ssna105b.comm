# CONTACT METHODE CONTINUE

DEBUT(CODE=_F(NOM='SSNA105B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

L_INST=DEFI_LIST_REEL(  DEBUT=-1.0,
                        INTERVALLE=(
                        _F( PAS = 0.1,         JUSQU_A = 0.),
                        _F( PAS = 1.E-6,       JUSQU_A = 1.E-6),
                        _F( PAS = 0.999E-3,    JUSQU_A = 0.001),
                        _F( PAS = 1.E-3,       JUSQU_A = 0.01),
                        _F( PAS = 2.E-3,       JUSQU_A = 0.03),
                        _F( PAS = 5.E-3,       JUSQU_A = 0.1),
                        _F( PAS = 1.E-2,       JUSQU_A = 0.9),
                                     )
                      )

MA = LIRE_MAILLAGE(FORMAT='MED',);

MA = DEFI_GROUP(reuse =MA,
                MAILLAGE=MA,
                CREA_GROUP_NO =(
                       _F(GROUP_MA = 'DABP',),
                       _F(GROUP_MA = 'DCDP',),
                       _F(GROUP_MA = 'DABG',),
                       _F(GROUP_MA = 'DCDG',),
                        )
                )

MO = AFFE_MODELE( MAILLAGE =MA,
                  AFFE=_F(
                       TOUT ='OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS',)
                 )

MATP = DEFI_MATERIAU(
            ELAS=_F( E = 1., NU = 0.3, ALPHA = 0.),)

MATG = DEFI_MATERIAU(
            ELAS=_F( E = 1., NU = 0.3, ALPHA = 0.),
            LEMAITRE=_F( N = 1.,UN_SUR_K = 1.,UN_SUR_M = 0.)
           )
CHMAT = AFFE_MATERIAU (MAILLAGE = MA,
                       AFFE =(_F(GROUP_MA = 'MAILP',
                                MATER = MATP,
                                ),
                              _F(GROUP_MA = 'MAILG',
                                MATER = MATG,
                                ), )
                       )

# PRESSION P1
# L INSTANT DE REFERENCE EST T= -1


R1 = 0.82
R2 = 0.98
R3 = 1.
R4 = 2.
P0 = 1.E-3
NU = 0.3
E = 1.
K = 1.


A =((R2**2)-(R1**2))/(2.*(R1**2)*(1.-NU))


B =E/(R2*(1.+NU))


C = (P0*(R3**3))/((R4**2)-(R3**2))


D = (1/E)*((1+NU)*((R4**2)/(R3**2))+(3./2.)*(1.-(2.*NU)))

G = (-1.)*((1.-(2.*NU))**2)/(2*E)


H =(3./2.)*K*(R4**2)/(R3**2)


K1 =((P0*(R2**2))/((R2**2)-(R1**2)))*(1-(2*NU)+((R1**2)/(R2**2)))



F_PREP =FORMULE(NOM_PARA='INST',VALE='''
          A*(B*(R3-R2+(C*(D +(G*exp(-E*K*INST))+(H*INST))))+K1) ''')


F_PREM =((R3/R2)-1)*((E*((R2**2)-(R1**2)))/(2*(R1**2)*(1-(NU**2))))

F1 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*1.E-6))+(H*1.E-6))))+K1)
F2 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*1.E-3))+(H*1.E-3))))+K1)
F3 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*1.E-2))+(H*1.E-2))))+K1)
F4 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*1.E-1))+(H*1.E-1))))+K1)
F5 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*2.E-1))+(H*2.E-1))))+K1)
F6 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*3.E-1))+(H*3.E-1))))+K1)
F7 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*4.E-1))+(H*4.E-1))))+K1)
F8 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*5.E-1))+(H*5.E-1))))+K1)
F9 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*6.E-1))+(H*6.E-1))))+K1)
F10 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*7.E-1))+(H*7.E-1))))+K1)
F11 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*8.E-1))+(H*8.E-1))))+K1)
F12 = A*(B*(R3-R2+(C*(D +(G*exp(-E*K*9.E-1))+(H*9.E-1))))+K1)

PRES_F = DEFI_FONCTION( NOM_PARA = 'INST',
                        PROL_DROITE = 'CONSTANT',
                        PROL_GAUCHE = 'EXCLU',
                        VALE = ( -1.,     F_PREM,
                                  0. ,    F_PREM,
                                  1.E-6,  F1,
                                  1.E-3,  F2,
                                  1.E-2,  F3,
                                  1.E-1,  F4,
                                  2.E-1,  F5,
                                  3.E-1,  F6,
                                  4.E-1,  F7,
                                  5.E-1,  F8,
                                  6.E-1,  F9,
                                  7.E-1,  F10,
                                  8.E-1,  F11,
                                  9.E-1, F12,))


BLOC = AFFE_CHAR_MECA (MODELE = MO,
             DDL_IMPO=_F(GROUP_NO =('DABP','DCDP','DABG','DCDG'),
                         DY=0.)
                      )
PRES = AFFE_CHAR_MECA_F (MODELE = MO,
                          PRES_REP=(_F (GROUP_MA = 'DDAP',
                                    PRES = PRES_F),
                       )
                          )

CONT = DEFI_CONTACT(MODELE         = MO,
                    FORMULATION    = 'CONTINUE',
                    ALGO_RESO_CONT = 'POINT_FIXE',
                    ALGO_RESO_GEOM = 'POINT_FIXE',
                    ZONE=_F(
                           GROUP_MA_MAIT = 'DDAG',
                           GROUP_MA_ESCL = 'DBCP',
                         ),
                   )

RESU = STAT_NON_LINE (MODELE = MO,
                      CHAM_MATER = CHMAT,
                      EXCIT =(
                         _F( CHARGE = BLOC),
                         _F( CHARGE = PRES),),
                      CONTACT   = CONT,
                      COMP_INCR =_F(RELATION ='LEMAITRE',PARM_THETA = 0.5,
                                    GROUP_MA = 'MAILG',
                                    ITER_INTE_MAXI = 1000),
                      COMP_ELAS =_F(RELATION ='ELAS',
                                     GROUP_MA = 'MAILP'),
                      INCREMENT =_F(LIST_INST =L_INST,),
                      SOLVEUR =_F( METHODE='LDLT',
                                   RENUM='SANS',),
                      NEWTON    =_F(
                                MATRICE ='TANGENTE',
                                REAC_ITER= 1),
                      CONVERGENCE=_F(
                                RESI_GLOB_RELA = 1.E-6,
                                ITER_GLOB_MAXI = 25),
                        )

RESU = CALC_CHAMP( reuse= RESU,
                  RESULTAT = RESU,
                   CONTRAINTE=('SIGM_ELNO'),)
RESU = CALC_CHAMP ( reuse= RESU,
                  RESULTAT = RESU,
                  CONTRAINTE='SIGM_NOEU',)

TEST_RESU(RESU=(_F(NUME_ORDRE=125,
                   GROUP_NO='BG',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   VALE_CALC= 2.11960764E-03,
                   VALE_REFE=2.14E-3,
                   PRECISION=0.012,),
                _F(NUME_ORDRE=125,
                   GROUP_NO='BG',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIXX',
                   VALE_CALC=-9.57114221E-06,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',),
                _F(NUME_ORDRE=125,
                   GROUP_NO='BG',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   VALE_CALC= 2.69662872E-04,
                   VALE_REFE=2.7912E-4,
                   PRECISION=0.035000000000000003,),
                _F(NUME_ORDRE=125,
                   GROUP_NO='BG',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIZZ',
                   VALE_CALC= 6.52992158E-04,
                   VALE_REFE=6.6660000000000005E-4,
                   PRECISION=0.025000000000000001,),
                ),
          )

FIN()
