DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# lecture maillage lineique
MAPOU=LIRE_MAILLAGE(UNITE=20,);

# lecture maillage section1
MASEC=LIRE_MAILLAGE(UNITE=21,);

MOPOU=AFFE_MODELE(MAILLAGE=MAPOU,
                  AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='POU_D_TGM',),);


BETON=DEFI_MATERIAU(ELAS=_F(E=3.7272000000E10, NU=0.0,  RHO=2400.0,),)

GF=DEFI_GEOM_FIBRE( INFO=2,
                    SECTION = _F(   GROUP_FIBRE='SBET',
                                    MAILLAGE_SECT = MASEC, TOUT_SECT = 'OUI',
                                    COOR_AXE_POUTRE = (0., 0.,),),
                                )


POUCA_1=AFFE_CARA_ELEM( MODELE=MOPOU,
                        INFO=1,
                        POUTRE=_F(  GROUP_MA=('POUTRE'),
                                    SECTION='RECTANGLE',
                                    CARA=('HY','HZ'),
                                    VALE=(0.2,0.1), ),
                        ORIENTATION=_F( GROUP_MA=('POUTRE'),
                                        CARA='ANGL_VRIL',
                                        VALE=90.0,
                                        ),
                        GEOM_FIBRE=GF,
                        MULTIFIBRE=(_F( GROUP_MA=('POUTRE'),
                        PREC_AIRE=1.0E-06, PREC_INERTIE=2.55E-01, GROUP_FIBRE=('SBET',)  ),  )
                    );

COMPPMF=DEFI_COMPOR(    GEOM_FIBRE=GF,
                        MATER_SECT=BETON,
                        MULTIFIBRE=_F(  GROUP_FIBRE='SBET',
                                        MATER=BETON,
                                    ),
                    );

CHMAT=AFFE_MATERIAU(MAILLAGE=MAPOU,
                    AFFE=_F(GROUP_MA='POUTRE', MATER=BETON,),
                    AFFE_COMPOR=_F(GROUP_MA='POUTRE',COMPOR=COMPPMF),
                          );

BLOCAGE=AFFE_CHAR_MECA( MODELE=MOPOU,
                        DDL_IMPO=(_F(GROUP_NO='ENC', DX=0.0, DY=0.0, DZ=0.0, DRX=0.0, DRY=0.0, DRZ=0.0,),
                                 ),);
CHARGE=AFFE_CHAR_MECA(  MODELE=MOPOU,
                        FORCE_NODALE=(_F(GROUP_NO='CHA',FX = 100.0,FY = 100.0,FZ = 100.)
                                 ),);

FOFO=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,3.5,3.5),
                   PROL_DROITE='EXCLU',
                   PROL_GAUCHE='EXCLU',);


LINST=DEFI_LIST_REEL(   DEBUT=0.0,
                        INTERVALLE=(_F(JUSQU_A=0.1,  NOMBRE=2,),
                                    _F(JUSQU_A=1.4,  NOMBRE=10,),
                                    _F(JUSQU_A=2.84 ,  NOMBRE=9,),
                                    _F(JUSQU_A=3. ,  NOMBRE=10,),
                                    ),
                    );


U_90=STAT_NON_LINE(MODELE=MOPOU,
                 CHAM_MATER=CHMAT,
                 CARA_ELEM=POUCA_1,
                 EXCIT=(_F(CHARGE=BLOCAGE,),
                        _F(CHARGE=CHARGE,
                           FONC_MULT=FOFO,),),
                 COMPORTEMENT=(_F(RELATION='MULTIFIBRE'),),
                 INCREMENT=_F(LIST_INST=LINST,),
                 NEWTON=_F(MATRICE='TANGENTE',REAC_ITER=1),
                 CONVERGENCE=_F(RESI_GLOB_RELA=3.E-5,
                                ITER_GLOB_MAXI=250,),
                 );

TB_90=CREA_TABLE(RESU=_F(RESULTAT=U_90,GROUP_MA='POUTRE',NOM_CHAM='SIEF_ELGA',TOUT_CMP='OUI',NUME_ORDRE = 1))
IMPR_TABLE(TABLE=TB_90)

