            CARA_ELEM=CAREL1,EXCIT=( _F( CHARGE = CH111),_F( CHARGE = CH221)) )

TEST_RESU(
   RESU=_F( RESULTAT = MEST21, NOM_CHAM = 'DEPL', NUME_ORDRE = 1,
   NOEUD = 'A',  NOM_CMP = 'DRZ', VALE = 0.227118,       PRECISION = 2.E-3) )

