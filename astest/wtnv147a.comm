DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
              
#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE  = MAILLAG1,
                       LINE_QUAD = _F(TOUT='OUI'),
                      );

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURF'),
                                   _F(GROUP_MA='LIG1'),
                                   _F(GROUP_MA='LIG3'),
                                  ),
                    INFO=2,
                   );

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MO=AFFE_MODELE(MAILLAGE=MAILLAG2,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_HM',),
              );

LS_N=FORMULE(NOM_PARA=('X','Y'),VALE='Y-2.5');

FISS=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LS_N,),
                    INFO=1,
                   );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO,
                         FISSURE=FISS,
                         INFO=1,
                         CONTACT='MORTAR',
                         DECOUPE_FACETTE='SOUS_ELEMENTS',
                        );

CTXFEM = DEFI_CONTACT(MODELE=MODELEK,
                      FORMULATION='XFEM',
                      ITER_CONT_MAXI=4,
                      ZONE=(_F(INTEGRATION='GAUSS',
                               ORDRE_INT=2,
                               ALGO_CONT='CZM',
                               FISS_MAIT = FISS,
                               ALGO_LAGR='VERSION2',
                               RELATION='CZM_LIN_MIX'
                              ),
                            ),
                      INFO=2,
                     );

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
UN = DEFI_CONSTANTE(VALE = 1.0,);

ZERO = DEFI_CONSTANTE(VALE = 0.0,);

KINT = DEFI_CONSTANTE(VALE = 1.01937E-9,);

SOL  = DEFI_MATERIAU(ELAS      = _F(E=5800E6,
                                    NU=0.0,
                                    RHO=2800.0,
                                    ALPHA       = 0.0,),
                     COMP_THM  =   'LIQU_SATU',
                     THM_INIT  = _F(PRE1        = 0.0,
                                    PORO        = 0.15,),
                     THM_DIFFU = _F(RHO         = 2500,
                                    BIOT_COEF   = 1.,
                                    PESA_X      = 0.0,
                                    PESA_Y      = 0.0,
                                    PESA_Z      = 0.0,
                                    PERM_IN     = KINT,),
                     THM_LIQU  = _F(RHO         = 1000,
                                    UN_SUR_K    = 5E-10,
                                    VISC        = UN,
                                    D_VISC_TEMP = ZERO,),
                     RUPT_FRAG = _F(GC=0.9E3,
                                    SIGM_C=1.1E6,
                                    PENA_LAGR=10,
                                    ),
                    );

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                               MATER=SOL,
                              ),
                      );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

def depn(x,y) :
   return 0.

DEPL_N = FORMULE(VALE='depn(X,Y)',
                  NOM_PARA=('X','Y',),);

CH=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                    DDL_IMPO=(
                              _F(TOUT='OUI',DX=DEPL_N,DY=DEPL_N,),
                             ),
                    INFO=1,
                   );

HYDR=AFFE_CHAR_MECA(MODELE=MODELEK,
                    DDL_IMPO=(
                              _F(GROUP_NO='LIG1',PRE1=0.0,),
                              _F(GROUP_NO='LIG3',PRE1=0.0,),
                             ),
                    INFO=1,
                   );

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                    DDL_IMPO=(
                              _F(NOEUD=('N6','N11'),LAGS_C=0.0,LAGS_F1=0.,PRE_FLU=10000000),
                             ),
                    INFO=1,
                   );

FLUH=AFFE_CHAR_MECA(MODELE=MODELEK,
                    FLUX_THM_REP=_F(TOUT='OUI',FLUN_HYDR1=0.,),
                   );

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

LI=DEFI_LIST_REEL(DEBUT=0.0,
                  INTERVALLE=(_F(JUSQU_A=1.E-4,
                                 NOMBRE=1,),
                              _F(JUSQU_A=1.E-3,
                                 NOMBRE=9,),
                              _F(JUSQU_A=1.E-2,
                                 NOMBRE=10,),
                              _F(JUSQU_A=0.1,
                                 NOMBRE=10,),
                              _F(JUSQU_A=1.0,
                                 NOMBRE=10,),
                              _F(JUSQU_A=10,
                                 NOMBRE=10,),
                                 ),);



UTOT1=STAT_NON_LINE(MODELE      = MODELEK,
                    CHAM_MATER  = CHAMPMAT,
                    EXCIT       = (_F(CHARGE = CH   ),
                                   _F(CHARGE = HYDR ),
                                   _F(CHARGE = CH1 ),
                                   _F(CHARGE = FLUH ),),
                    COMPORTEMENT   = _F(RELATION       = 'KIT_HM', PARM_THETA=0.56999999999999995,
                                        RELATION_KIT   = ('ELAS',
                                                          'LIQU_SATU',
                                                          'HYDR_UTIL',),
                                        RESI_INTE_RELA = 1.E-6,
                                        ITER_INTE_PAS  = -10,
                                        ITER_INTE_MAXI = 100,
                                       ),
                    NEWTON      = _F(MATRICE   = 'TANGENTE',
                                     REAC_ITER = 1, 
                                    ),
                    CONVERGENCE = _F(RESI_GLOB_RELA = 1.E-6,
                                     ITER_GLOB_MAXI = 100,    
                                    ),
                    INCREMENT   = _F(LIST_INST = LI,),
                    CONTACT =  CTXFEM,
                   )

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MODELE = MODELEK)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN_HM',),),
                    );  

RES_X1=POST_CHAM_XFEM(MODELE_VISU = MOD_VISU,
                      RESULTAT    = UTOT1,
                     );

IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_X1,));
#IMPR_RESU(RESU=_F(RESULTAT=RES_X1,));
#IMPR_RESU(RESU=_F(MAILLAGE=MA_XFEM));

# -------------------------------------------------------------   
#            TEST_TABLE
# -------------------------------------------------------------   

DEP1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                             GROUP_NO=('NFISSU'),
                             RESULTAT=RES_X1,
                             NOM_CHAM='DEPL',
                             INST=(10),
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',
                            ),
                  );

#IMPR_TABLE(TABLE=DEP1,);


TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.0E-2,
           VALE_CALC=4.08568261112,
           VALE_REFE=4.07748,
           NOM_PARA='LAG_FLI',
           TYPE_TEST='MIN',
           TABLE=DEP1,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.0E-2,
           VALE_CALC=4.08568385465,
           VALE_REFE=4.07748,
           NOM_PARA='LAG_FLI',
           TYPE_TEST='MAX',
           TABLE=DEP1,)

DEP2=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                             NOEUD=('NX2','NX4','NX8','NX10'),
                             RESULTAT=RES_X1,
                             NOM_CHAM='DEPL',
                             INST=(10),
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',
                            ),
                  );

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.0E-3,
           VALE_CALC=8003995.8385,
           VALE_REFE=8000000.0,
           NOM_PARA='PRE1',
           TYPE_TEST='MIN',
           TABLE=DEP2,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=1.0E-3,
           VALE_CALC=8003996.10266,
           VALE_REFE=8000000.0,
           NOM_PARA='PRE1',
           TYPE_TEST='MAX',
           TABLE=DEP2,)

FIN();
