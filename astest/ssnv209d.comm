#
#       CAS_TEST SSNV209D: 2D, ELEMENTS T3, METHODE XFEM

DEBUT( CODE=_F(  NOM = 'SSNV209D',NIV_PUB_WEB='INTERNET') )

#......................................................
#  CALCUL TEST ELEMENTS FINIS XFEM ET CONTACT 2D TRI3
#  ECRASEMENT D UN LOPIN FISSURE AVEC FROTTEMENT
#......................................................
#
# La fissure coincide avec les faces du maillage
#
# cas test de De Saxce and Feng : exemple no 1, p. 241 
# G. De Saxce, Z.Q. Feng, The bipotentiel method: a constitutive 
# approach to design the complete contact law with friction and 
# improved numerical algorithms, Mathl. Comput. Modelling, vol. 28,
# no 4-8, pp. 225-245, 1998

#----------------------------------------------
#                MAILLAGE 
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'));

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO= (_F(GROUP_MA='SURF',),
                                    _F(GROUP_MA='LIG1',
                                          NOM='NLIG1',),
                                    _F(NOM='BLIG1',
                                          DIFFE=('NLIG1','P1','P2'),),),
                    CREA_GROUP_MA= (_F(OPTION='BANDE',
                                          NOM='BAND',
                                          GROUP_NO_CENTRE='P4',
                                          VECT_NORMALE= (0.,1.),
                                          DIST=0.031,
                                          ), 
                                    _F(OPTION='BANDE',
                                          NOM='PLAQ',
                                          GROUP_NO_CENTRE='P4',
                                          VECT_NORMALE= (0.,1.),
                                          DIST=0.0399,
                                          ),
                                    _F(OPTION='BANDE',
                                          NOM='BAT',
                                          GROUP_NO_CENTRE='P1',
                                          VECT_NORMALE= (0.,1.),
                                          DIST=0.00999,
                                          ),                                                                                                      
                                    _F(NOM='NPHD',
                                       INTERSEC = ('BAND','LIG2'),),
                                    _F(NOM='NPHG',
                                       INTERSEC = ('BAND','LIG4'),),),
                     INFO=1);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA=('SURF',),),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(NOM='NFISSU',
                                       OPTION='PLAN',
                                       POINT=(0.,0.),
                                       VECT_NORMALE=(0.,1.),
                                       PRECISION=1E-7),
                                   ),
                    INFO=1);

MAILLAG2=MODI_MAILLAGE(reuse=MAILLAG2,
                 MAILLAGE=MAILLAG2,
                 ORIE_PEAU_2D=(_F(GROUP_MA = 'LIG2',),
                               _F(GROUP_MA = 'LIG3',),
                               _F(GROUP_MA = 'LIG4',),),);

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

  
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('LIG3',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),
                           _F(GROUP_MA=('LIG1',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),
                           _F(GROUP_MA=('SURF',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN_XFEM_CONT',),
                           _F(GROUP_MA=('LIG2',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),
                           _F(GROUP_MA=('LIG4',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN',),),
                              INFO=1);                              



# FORME ANALYTIQUE POUR LA FISSURE
 
LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y');
LT=FORMULE(NOM_PARA=('X','Y'),VALE='X-10.');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    GROUP_MA_ENRI=('SURF'),
                    INFO=1,
                    );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='OUI',                        
                         INFO=1,
                         );



#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=1.3E11
nu=0.2
rho=7800.
MATPLA=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,),);

MATBAT=DEFI_MATERIAU(ELAS=_F(E=1.3E11,NU=0.2,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('PLAQ'),
                                MATER=MATPLA,),
                             _F(GROUP_MA=('BAT'),
                                MATER=MATBAT,),),);

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------


CTXFEM = AFFE_CHAR_MECA( CONTACT=_F( FISS_MAIT      = FISS,
                                     METHODE        = 'XFEM',
                                     CONTACT_INIT   = 'OUI',
                                     FROTTEMENT     = 'COULOMB',
                                     COULOMB        = 1.,
                                     ITER_FROT_MAXI = 10,
                                     INTEGRATION    = 'NOEUD',
                                     ALGO_LAGR='VERSION2',   
                                     COEF_REGU_CONT = 100.,
                                     ITER_CONT_MAXI = 6.,
                                     COEF_ECHELLE=1, 
                                     COEF_REGU_FROT = 100.,
                                    ),
                          MODELE=MODELEK,
                          ),                     

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,);
                      
RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0,)
                   )


CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                  DDL_IMPO=(
#                            ENCASTREMENT DU BAS
                            _F(GROUP_NO='NLIG1',
                                    DX=0.,
                                    DY=0.,),),
                   
#                            FORCE IMPOSEE EN A
                  PRES_REP=(_F(GROUP_MA = 'LIG3', 
                                 PRES = 5.E07,),
                           ),);


def pr(y) :
   if y<0.0 :  return 0.E07
   if y>0.0 :  return  15.E07
   if y==0.0:  return  0.
FCTPRES = FORMULE(VALE='pr(Y)',
                  NOM_PARA=('X','Y'),);

CHAF=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     PRES_REP=_F(GROUP_MA = ('LIG2','LIG4',),
                              PRES=FCTPRES,),);

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=1.0,NOMBRE=2,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,              
                   EXCIT=(_F(CHARGE=CHXFEM,),
                          _F(CHARGE=CTXFEM,),                                          
                          _F(CHARGE=CH,
                             FONC_MULT=RAMPE),                                          
                          _F(CHARGE=CHAF,
                             FONC_MULT=RAMPE), 
                             ),  
                   COMP_ELAS=_F(RELATION='ELAS',
                                    GROUP_MA='SURF',),
                   INCREMENT=_F(
                                    LIST_INST=L_INS1,
                                    SUBD_METHODE='UNIFORME',
                                    SUBD_PAS_MINI=1.E-5,
                                    INST_FIN=1.0,),
                   CONVERGENCE=_F(ARRET='OUI',
                                    ITER_GLOB_MAXI=30,
                                    RESI_GLOB_RELA=1.E-6),
                   SOLVEUR=_F(METHODE='MUMPS',
                                    PCENT_PIVOT=250,
                                    RESI_RELA=-1.),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   );
                   
#----------------------------------------------
#                   POST-TRAITEMENT 
#----------------------------------------------
#


TABLAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                              GROUP_NO='NFISSU',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=2,
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',),);

TABDEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                              GROUP_NO='SURF',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=2,
                             TOUT_CMP='OUI',
                             OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TABDEP,
           FORMAT_R='E14.7',);

IMPR_TABLE(TABLE=TABLAG,
           FORMAT_R='E14.7',);

#-----------------------------------------------------------
#             POST-TRAITEMENT POUR LA VISUALISATION
#-----------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                          MODELE        = MODELEK,
                          )
   
MA_XFEM=DEFI_GROUP(reuse=MA_XFEM,
                     MAILLAGE=MA_XFEM,
                     CREA_GROUP_NO=(_F(OPTION='ENV_SPHERE',
                                       NOM='PPA',
                                       POINT=(0.,0.),
                                       RAYON=0.000001,
                                       PRECISION=0.00001,),
                                    _F(OPTION='ENV_SPHERE',
                                       NOM='PPB',
                                       POINT=(0.00125,0.),
                                       RAYON=0.000001,
                                       PRECISION=0.00001,),
                                    _F(OPTION='ENV_SPHERE',
                                       NOM='PPC',
                                       POINT=(0.005,0.),
                                       RAYON=0.000001,
                                       PRECISION=0.00001,),
                                    _F(OPTION='ENV_SPHERE',
                                       NOM='PPD',
                                       POINT=(0.0075,0.),
                                       RAYON=0.000001,
                                       PRECISION=0.00001,),
                                    _F(OPTION='ENV_SPHERE',
                                       NOM='PPE',
                                       POINT=(0.01125,0.),
                                       RAYON=0.000001,
                                       PRECISION=0.00001,),
                                    _F(NOM='NFISSU',
                                      OPTION='PLAN',
                                      POINT=(0.,0.),
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=1.E-6,)),
                     INFO=1);
   
MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                        AFFE=_F(TOUT='OUI',
                                 PHENOMENE='MECANIQUE',
                                 MODELISATION='D_PLAN',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                           MAILLAGE_FISS = MA_XFEM,
                           MODELE        = MODELEK,
                           MODELE_VISU   = MOD_VISU,
                           RESULTAT      = UTOT1,
                           NOM_CHAM      = ('DEPL','SIEF_ELGA'),
                           )


#----------------------------------------------
#                   POST RELEVE
#----------------------------------------------

DEP_NFI=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU',),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=2,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                      TABLE = DEP_NFI,
                      ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]',),),);

DEP_NFI = CALC_TABLE(reuse = DEP_NFI,
                      TABLE = DEP_NFI,
                      ACTION=_F(OPERATION='TRI',
                                 NOM_PARA='COOR_X',
                                 ORDRE='CROISSANT',),);

IMPR_TABLE(TABLE=DEP_NFI,);


DEP_PPA=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPA',),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=2,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);
DEP_PPA = CALC_TABLE(reuse = DEP_PPA,
                      TABLE = DEP_PPA,
                      ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]',),),);


DEP_PPB=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPB',),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=2,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);
DEP_PPB = CALC_TABLE(reuse = DEP_PPB,
                      TABLE = DEP_PPB,
                      ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]',),),);

DEP_PPC=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPC',),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=2,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);                                
DEP_PPC = CALC_TABLE(reuse = DEP_PPC,
                      TABLE = DEP_PPC,
                      ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]',),),);

DEP_PPD=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPD',),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=2,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);
DEP_PPD = CALC_TABLE(reuse = DEP_PPD,
                      TABLE = DEP_PPD,
                      ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]',),),);

DEP_PPE=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('PPE',),
                                RESULTAT=RES_XFEM,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=2,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);
DEP_PPE = CALC_TABLE(reuse = DEP_PPE,
                      TABLE = DEP_PPE,
                      ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^M]',),),);


#----------------------------------------------
#                   TEST TABLE
#----------------------------------------------

preci=1.e-2;

TEST_TABLE(TABLE=DEP_PPA,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=2.46973E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=DEP_PPA,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=2.46973E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);
           
TEST_TABLE(TABLE=DEP_PPB,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=2.33932E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=DEP_PPB,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=2.33932E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);            
           
TEST_TABLE(TABLE=DEP_PPC,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=1.96365E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=DEP_PPC,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=1.96365E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);                      

TEST_TABLE(TABLE=DEP_PPD,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=1.71444E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=DEP_PPD,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=1.71444E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);
                                
TEST_TABLE(TABLE=DEP_PPE,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=1.34938E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=DEP_PPE,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=1.34938E-05,
           CRITERE='RELATIF',
           PRECISION=preci,
           REFERENCE='AUTRE_ASTER',);

FIN();
