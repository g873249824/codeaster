# MATERIAU DEFINI POUR LE COMPORTEMENT  ECP_CFC. Unites : MPa, mm, N

MU=125000. # MPa
NU=0.3
E=MU*2*(1.+NU)

TAU_F  = 44.9
RHO_0  = 1.77E6    # en mm**-2
N      = 73.5
GAMMA0 = 4.E-11
BETA   = 2.54E-7     # 2.5 Angstrom
UNSURD = 0.
GC0    = 1.33E-6
K      = 37.14

# variable adimensionnee, voir R5.03.11

RHO_0=RHO_0*BETA*BETA

ALPHA=0.0

C11=244000.
C12=96000.
C44=74000.

y1111=C11
y1122=C12
y1212=C44

# Correspondance avec ELAS_ORTH : la matrice d'elasticite vaut
# pour une elasticite orthotrope cubique
#H11=H22=H33=E*(1-Nu**2)/(1-3*Nu**2-2*Nu**3)
#H44=H55=H66=y1212=G_LT
# donc NU_LT=1/(1+y1111/y1122)

Nu=1.0/(1.0+y1111/y1122)

E_L=y1111*(1.0-3.0*Nu**2-2.0*Nu**3)/(1.0-Nu**2)

a_copl=0.1236
a_colineaire=0.633
a_glissile=0.1388
a_Lomer=0.1236
a_Hirth=0.0709


ACIERECP=DEFI_MATERIAU(
                      ELAS_ORTH =_F(E_L  =E_L,
                                    E_T  =E_L,
                                    E_N  =E_L,
                                    NU_LT=Nu,
                                    NU_LN=Nu,
                                    NU_TN=Nu,
                                    G_LT =y1212,
                                    G_LN =y1212,
                                    G_TN =y1212,
                                 ),

                      MONO_DD_FAT=_F(TAU_F  = TAU_F,  # MPa
                                     GAMMA0 = GAMMA0,
                                     BETA=BETA,          # 2.5 Angstrom
                                     N      = N,
                                     UN_SUR_D   =   UNSURD,
                                     GC0 =  GC0,
                                     K   =   K,
                                     H1      = a_copl,
                                     H2      = a_colineaire,
                                     H3      = a_glissile,
                                     H4      = a_Lomer,
                                     H5      = a_Hirth,
                                   ),
                      );


# ECP_CFC
CMP_MONO=DEFI_COMPOR(MONOCRISTAL=_F(MATER=ACIERECP,
                                    ECOULEMENT='MONO_DD_FAT',
                                    ELAS='ELAS',
                                    FAMI_SYST_GLIS='OCTAEDRIQUE',
                                    ),);

NBGR=40

FVOL=1.0/NBGR

