# essai  eps-point=1.E-3 /s
tmax=150.

tfin=45.

EPSIMPO=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,tmax,0.15),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',
                   );

# LOI DE COMPORTEMENT ECP_CFC en Explicite
LISTARCH=DEFI_LIST_REEL(DEBUT=0.0,
                       INTERVALLE=(
                                   _F(JUSQU_A=tmax,
                                      NOMBRE=200,),
                                  ),
                      );

LINSTECP=DEFI_LIST_REEL(DEBUT=0.0,
                       INTERVALLE=(
                                   _F(JUSQU_A=tmax,
                                      NOMBRE=400,),
                                  ),
                      );

LIS2ECP=DEFI_LIST_INST(INFO=1,
                        DEFI_LIST=_F(LIST_INST=LINSTECP),
                        ECHEC=_F(SUBD_NIVEAU=20,
                                 SUBD_PAS=4),
                        );



angles=[0.,0.,0.]


# densites de dislocation initiales : variables internes

nbvari=7+NBGR*(6+3*12+6)+1
avari = zeros(nbvari)
for p in range(int(NBGR)):
   for i in range(12):
      avari[7+48*p+(6+i*3)]=RHO_0

lvari=list(avari)


POLYECP=SIMU_POINT_MAT(INFO=1,
                      MATER=ACIERECP,
                      MASSIF=_F(ANGL_EULER=angles),
                      COMPORTEMENT=(_F(RELATION='POLYCRISTAL',
                                    COMPOR=CMP_POLY,
                                    ALGO_INTE='RUNGE_KUTTA',
                                    RESI_INTE_RELA=1.E-4,
                                    TOUT='OUI',),),
                      INCREMENT=(_F(LIST_INST=LIS2ECP,INST_FIN=tfin),),
                      NEWTON=(_F(MATRICE='ELASTIQUE'
                                 ),),
                      CONVERGENCE=(_F(ITER_GLOB_MAXI=100,RESI_GLOB_RELA=1.E-4),),
                      VARI_INIT=_F(VALE=lvari,
                                  ),
                      EPSI_IMPOSE=(
                                   _F(EPZZ=EPSIMPO),
                                  ),
                      NB_VARI_TABLE=6,
                      ARCHIVAGE=_F(LIST_INST=LISTARCH),
                       );


sigeps=RECU_FONCTION(TABLE=POLYECP,
                   PARA_X='EPZZ',
                   PARA_Y='SIZZ',
                   INTERPOL='LIN');


sigtps=RECU_FONCTION(TABLE=POLYECP,
                   PARA_X='INST',
                   PARA_Y='SIZZ',
                   INTERPOL='LIN');

#Valeur experimentale
TEST_FONCTION(VALEUR=_F(VALE_CALC=407.7115377185,
                        VALE_REFE=387.8,
                        VALE_PARA=45.0,
                        REFERENCE='SOURCE_EXTERNE',
                        PRECISION=0.06,
                        NOM_PARA='INST',
                        FONCTION=sigtps,),
              )

ecpb4 = LIRE_FONCTION(UNITE=21,
                      NOM_PARA='EPSI',
                      PROL_DROITE='CONSTANT',)

IMPR_FONCTION(
            FORMAT='XMGRACE',
            COURBE=(
                    _F( FONCTION=sigeps,
                        COULEUR= 0,
                        LEGENDE= 'Aster',),
                    _F( FONCTION=ecpb4,
                        COULEUR= 0,
                        LEGENDE= 'ECP',),
                   ),
            UNITE=29,
            TITRE='SIG = f(EPS) pour '+str(NBGR)+' phases');

