DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),ERREUR=_F(ERREUR_F='EXCEPTION',),);

MPa = 1000000.0;


# ------------------- Lecture du maillage

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED', UNITE=20 )

DEFI_GROUP(reuse = MAILLAGE, MAILLAGE = MAILLAGE, 
           CREA_GROUP_NO=(_F(GROUP_MA='FACE_SUP',
                             NOM = 'FACE_SUP'
                           ),
                          _F(GROUP_MA='FACE_COTE',
                             NOM = 'FACE_COTE'
                           ),
                        )
        )


#

MODELE=AFFE_MODELE(INFO=1,
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = ('CUBE',),
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = '3D'),
                            ) )


# ------------------- Definition des materiaux

BETON = DEFI_MATERIAU(ELAS=_F(
                    E = 30000*MPa,
                    NU = 0.3,
                    RHO = 2500,
                   ),
                  );




CHMATER=AFFE_MATERIAU(
                   MAILLAGE=MAILLAGE,AFFE=(
                       _F(  GROUP_MA = ('CUBE',),
                              MATER = BETON),
                              )
                              )




BLOQ=AFFE_CHAR_MECA(
                      MODELE=MODELE,DDL_IMPO=(
                    _F(  GROUP_NO='ENCAST',
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,
                               ),
                               ),)
FORC=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                      FORCE_NODALE=(
                               _F(  GROUP_NO=('FACE_SUP',),
                                    FZ = -100000,
                               ),
                               _F(  GROUP_NO=('FACE_COTE',),
                                    FX = -50000,
                               ),
                               ),)

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,    0,
                           1.0,    1,
                           ),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     );

temps=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=(_F(JUSQU_A=1.0,NOMBRE=5,),
                                    ),);
DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = temps,),)


U_ASTER=STAT_NON_LINE(MODELE=MODELE,
                      CHAM_MATER=CHMATER,
                      EXCIT=(_F(CHARGE=FORC,FONC_MULT=RAMPE),
                             _F(CHARGE=BLOQ,),),
                      COMPORTEMENT=(_F(RELATION='ELAS',GROUP_MA='CUBE',DEFORMATION='GROT_GDEP',
                      ),
                                ),
                      CONVERGENCE=_F(
                                         RESI_GLOB_RELA=1E-10),
                      INCREMENT   =_F(LIST_INST=DEFLIST,),
                      )


PLATEAU=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,   1.0,
                           8e-3,  1.0,
                           ),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     );

U_EPX = CALC_EUROPLEXUS(
   ETAT_INIT=_F(RESULTAT=U_ASTER,CONTRAINTE='OUI',EQUILIBRE='NON'),
   COMPORTEMENT =(_F(
                  RELATION = 'ELAS',
                  GROUP_MA='CUBE',
                 ),
              ),
   EXCIT=(_F(CHARGE=FORC,FONC_MULT=PLATEAU),
          _F(CHARGE=BLOQ,),),
   LANCEMENT ='OUI',
   CALCUL = _F(TYPE_DISCRETISATION  ='UTIL',
               INST_INIT = 0,
               INST_FIN  = 0.004,
               NMAX      = 200,
               PASFIX    = 2e-5,
               ),
   ARCHIVAGE   = _F(PAS_NBRE=100,),
   );


#IMPR_RESU(RESU=_F(RESULTAT=U_ASTER,NOM_CHAM=('DEPL','SIEF_ELGA',),INST=1.0))
#IMPR_RESU(RESU=_F(RESULTAT=U_EPX,NOM_CHAM=('DEPL','SIEF_ELGA',),INST=0.004))

# REFERENCE ETAT INITIAL ASTER

TEST_RESU(RESU=(_F(INST=1.0,
                   RESULTAT=U_ASTER,
                   NOM_CHAM='DEPL',
                   GROUP_NO='GRN1',
                   NOM_CMP='DX',
                   VALE_CALC=-8.6090494774E-05,
                   ),
                _F(INST=1.0,
                   RESULTAT=U_ASTER,
                   NOM_CHAM='DEPL',
                   GROUP_NO='GRN1',
                   NOM_CMP='DY',
                   VALE_CALC=-1.86758056933E-05,
                   ),
                _F(INST=1.0,
                   RESULTAT=U_ASTER,
                   NOM_CHAM='DEPL',
                   GROUP_NO='GRN1',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.000110181067659 ,
                   ),
                ),
            )


            
TEST_RESU(RESU=(            
                _F(INST=1.0,
                   POINT=1,
                   RESULTAT=U_ASTER,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC=-1220186.68329,
                   MAILLE = 'M93',),
                _F(INST=1.0,
                   POINT=1,
                   RESULTAT=U_ASTER,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIYY',
                   VALE_CALC=-71707.7384702,
                   MAILLE = 'M93',),
                _F(INST=1.0,
                   POINT=1,
                   RESULTAT=U_ASTER,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIZZ',
                   VALE_CALC=-2036191.89547,
                   MAILLE = 'M93',),
                ),
          )

# RESULTATS APRES PASSAGE DANS EPX

TEST_RESU(RESU=(_F(INST=0.004,
                   RESULTAT=U_EPX,
                   NOM_CHAM='DEPL',
                   GROUP_NO='GRN1',
                   NOM_CMP='DX',
                   VALE_CALC=-8.6090494774E-05,
                   VALE_REFE=-8.6090494774E-05,
                   REFERENCE = 'ANALYTIQUE',
                   PRECISION = 1E-6,
                   ),
                _F(INST=0.004,
                   RESULTAT=U_EPX,
                   NOM_CHAM='DEPL',
                   GROUP_NO='GRN1',
                   NOM_CMP='DY',
                   VALE_CALC=-1.86758056933E-05,
                   VALE_REFE=-1.86758056933E-05,
                   REFERENCE = 'ANALYTIQUE',
                   PRECISION = 1E-6,
                   ),
                _F(INST=0.004,
                   RESULTAT=U_EPX,
                   NOM_CHAM='DEPL',
                   GROUP_NO='GRN1',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.000110181067659 ,
                   VALE_REFE=-0.000110181067659 ,
                   REFERENCE = 'ANALYTIQUE',
                   PRECISION = 1E-6,
                   ),
                ),
            )


            
TEST_RESU(RESU=(            
                _F(INST=0.004,
                   POINT=1,
                   RESULTAT=U_EPX,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   VALE_CALC=-1220186.68329,
                   VALE_REFE=-1220186.68329,
                   REFERENCE = 'ANALYTIQUE',
                   PRECISION = 1E-6,
                   MAILLE = 'M93',),
                _F(INST=0.004,
                   POINT=1,
                   RESULTAT=U_EPX,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIYY',
                   VALE_CALC=-71707.7384702,
                   VALE_REFE=-71707.7384702,
                   REFERENCE = 'ANALYTIQUE',
                   PRECISION = 1E-6,
                   MAILLE = 'M93',),
                _F(INST=0.004,
                   POINT=1,
                   RESULTAT=U_EPX,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIZZ',
                   VALE_CALC=-2036191.89547,
                   VALE_REFE=-2036191.89547,
                   REFERENCE = 'ANALYTIQUE',
                   PRECISION = 1E-6,
                   MAILLE = 'M93',),
                ),
          )



FIN()
