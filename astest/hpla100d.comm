LISTIME=DEFI_LIST_REEL(VALE=(0.0,1.0,),)

STATNL1=STAT_NON_LINE(MODELE=MO,
                      CHAM_MATER=CHMAT1,
                      CARA_ELEM=CARE,
                      COMP_INCR=_F(RELATION='ELAS'),
                      EXCIT=_F(CHARGE=CHDIL),
                      INCREMENT=_F(LIST_INST=LISTIME,),);

DEP1=CREA_CHAMP(OPERATION='EXTR',
                TYPE_CHAM='NOEU_DEPL_R',
                RESULTAT=STATNL1,
                NOM_CHAM='DEPL',
                INST=1.);

DEPLDIL=CREA_RESU(OPERATION='AFFE',
                  TYPE_RESU='EVOL_ELAS',
                  NOM_CHAM='DEPL',
                  AFFE=_F(INST=1.,
                          CHAM_GD=DEP1,
                          MODELE=MO,
                          CHAM_MATER=CHMAT1,
                          CARA_ELEM=CARE,),);

DEPLDIL=CALC_ELEM(reuse=DEPLDIL,
                  RESULTAT= DEPLDIL ,
                  OPTION=('SIEF_ELGA','EFGE_ELNO','SIGM_ELNO',),);

STATNL2=STAT_NON_LINE(MODELE=MO,
                      CHAM_MATER=CHMATH1,
                      CARA_ELEM=CARE,
                      COMP_INCR=_F(RELATION='ELAS'),
                      EXCIT=_F(CHARGE=CHDIL),
                      INCREMENT=_F(LIST_INST=LISTIME,),);

DEP2=CREA_CHAMP(OPERATION='EXTR',
                TYPE_CHAM='NOEU_DEPL_R',
                RESULTAT=STATNL2,
                NOM_CHAM='DEPL',
                INST=1.),

DEPLDILT=CREA_RESU(OPERATION='AFFE',
                   TYPE_RESU='EVOL_ELAS',  NOM_CHAM='DEPL',
                   AFFE=_F(INST=1.,
                           CHAM_GD=DEP2,
                           MODELE=MO,
                           CHAM_MATER=CHMATH1,
                           CARA_ELEM=CARE,),);

DEPLDILT=CALC_ELEM(reuse=DEPLDILT,
                   RESULTAT=DEPLDILT,
                   OPTION=('SIEF_ELGA','EFGE_ELNO','SIGM_ELNO',),);

# ------------------------------------------------------------------

DEPLDI2=MECA_STATIQUE(MODELE=MO,
                      CHAM_MATER=CHMAT2,
                      CARA_ELEM=CARE,
                      EXCIT=_F(CHARGE=CHDI2),)

DEPLDI2=CALC_ELEM(reuse=DEPLDI2,
                  RESULTAT=DEPLDI2 ,
                  OPTION=('EFGE_ELNO','SIGM_ELNO',),);

DEPLDI2T=MECA_STATIQUE(MODELE=MO,
                       CHAM_MATER=CHMATH2,
                       CARA_ELEM=CARE,
                       EXCIT=_F(CHARGE=CHDI2),)

DEPLDI2T=CALC_ELEM(reuse=DEPLDI2T,
                   RESULTAT=DEPLDI2T ,
                   OPTION=('EFGE_ELNO','SIGM_ELNO',),);

