                   PRECISION=0.029999999999999999,),
                _F(NUME_ORDRE=8,
                   GROUP_NO='D',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DRZ',
                   VALE_CALC= 5.88981326E+06,
                   VALE_REFE=5.889810E6,),
                _F(NUME_ORDRE=8,
                   GROUP_NO='D',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=0.022715198,
                   VALE_REFE=0.022083599999999998,
                   PRECISION=0.029999999999999999,),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='A',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DRZ',
                   VALE_CALC=-3.08661769E+06,
                   VALE_REFE=-3.086700E6,),
                _F(NUME_ORDRE=8,
                   GROUP_NO='A',
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U1,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DRZ',
                   VALE_CALC=-5.88964685E+06,
                   VALE_REFE=-5.889810E6,),
                ),
          )

# NON REGRESSION :


TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='D',
                   RESULTAT=U1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   VALE_CALC=0.011190311916921001,
                   TOLE_MACHINE=1.E-3,),
                _F(NUME_ORDRE=8,
                   GROUP_NO='D',
                   RESULTAT=U1,
