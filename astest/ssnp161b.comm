DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),);

MAIL=LIRE_MAILLAGE(FORMAT='MED',);


MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(GROUP_MA='haut',
                                 NOM='depno',),);

MA=DEFI_MATERIAU(ELAS=_F(E=34000000000.0,
                          NU=0.19,
                         RHO=2400.0,),
                 MAZARS=_F(AT=1.,
                           BT=9000.0,
                           AC=1.25,
                           BC=1965.0,
                           EPSD0=0.00011,
                          K=0.7),);

MODE=AFFE_MODELE(MAILLAGE=MAIL,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='C_PLAN',),);

MATE=AFFE_MATERIAU(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           MATER=MA,),);

BLO=AFFE_CHAR_MECA(MODELE=MODE,
                    DDL_IMPO=(
                        _F(GROUP_MA='gau',
                           DX=0.0,),
                        _F(GROUP_MA='bas',
                           DY=0.0,),),);

CHAR=AFFE_CHAR_MECA(MODELE=MODE,
                    PRES_REP=(
                        _F(GROUP_MA='droi',
                           PRES=-1560000.0,),
                        ), );
PRESS=AFFE_CHAR_MECA(MODELE=MODE,
                    PRES_REP=(
                        _F(GROUP_MA='haut',
                           PRES=30000000.0,),
                        ),
                    );

List=DEFI_LIST_REEL(DEBUT=0.0,
                    INTERVALLE=(_F(JUSQU_A=0.46,
                                  PAS=0.01,),
                                  ),);


Fonc=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                         1.0,2.0,
                         ),INTERPOL=('LIN','LIN',),);

STAT=STAT_NON_LINE(MODELE=MODE,
                     CHAM_MATER=MATE,
                     EXCIT=(_F(CHARGE=BLO,),
                            _F(CHARGE=CHAR,
                               FONC_MULT=Fonc,),
                            _F(CHARGE=PRESS,
                               FONC_MULT=Fonc,),),
                     COMPORTEMENT=_F(RELATION='MAZARS',),
                     INCREMENT=_F(LIST_INST=List,),
                     CONVERGENCE=_F(RESI_GLOB_RELA=9.E-5,
                                    ITER_GLOB_MAXI=100,
                                    ARRET='OUI',),
                     NEWTON=_F(MATRICE='TANGENTE',
                               REAC_ITER=1,),);




STAT=CALC_CHAMP  ( reuse  = STAT,
                RESULTAT=STAT,
                NUME_ORDRE = (6,46),
                CONTRAINTE='SIGM_NOEU',DEFORMATION='EPSI_NOEU',
                );


TEST_RESU(RESU=(_F(RESULTAT=STAT,
                   NUME_ORDRE=6,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   NOEUD='N1',
                   MAILLE='M1',
                   REFERENCE='SOURCE_EXTERNE',
                   CRITERE='RELATIF',
                   PRECISION=5.E-2,
                   VALE_CALC=-3.5997984448939E+06,
                   VALE_REFE=-3589800.,),
               _F( RESULTAT=STAT,
                   NUME_ORDRE=6,
                   NOM_CHAM='EPSI_NOEU',
                   NOM_CMP='EPYY',
                   NOEUD='N1',
                   MAILLE='M1',
                   REFERENCE='SOURCE_EXTERNE',
                   CRITERE='RELATIF',
                   PRECISION=5.E-2,
                   VALE_CALC=-1.0692366883424E-04,
                   VALE_REFE=-1.05E-04,),
                _F(RESULTAT=STAT,
                   NUME_ORDRE=46,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   NOEUD='N1',
                   MAILLE='M1',
                   REFERENCE='SOURCE_EXTERNE',
                   CRITERE='RELATIF',
                   PRECISION=5.E-2,
                   VALE_CALC=-2.7600630870447E+07,
                   VALE_REFE=-27899998.,),),)

FIN();
