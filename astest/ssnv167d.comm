                    DDL_IMPO=(_F(GROUP_MA='Blocage',
                                                  DX=0.,
                                                  DY=0.,
                                                  DZ=0.,),
                                       _F(GROUP_MA='Deplacement',
                                                  DX=0.,
                                                  DY=0.,
                                                  DZ=-0.5,),),);
#

