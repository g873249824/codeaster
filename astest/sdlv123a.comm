#
IMPR_RESU( FORMAT='CASTEM', UNITE = 37,
           MODELE=MODELE,
           RESU=(_F(MAILLAGE=MAIL,),
                 _F(RESULTAT=evol,),
                  ),);

TEST_TABLE(REFERENCE='ANALYTIQUE',
