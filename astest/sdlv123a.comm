                   DIRECTION=(1.,0.,0.,),
                   THETA_2D=_F (GROUP_NO = ('P2',),
                                MODULE = 1. ,
                                R_INF = 5.,
                                R_SUP = 18.)
                               )

G1=CALC_G( MODELE=MODELE,
                    RESULTAT=evol,
                    TOUT_ORDRE='OUI',
                    THETA=_F(THETA=THETA1),
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=_F(CHARGE=CH_P,),
                    SYME_CHAR='SYME'
                  )

G2=CALC_G( MODELE=MODELE,
                    RESULTAT=evol,
                    TOUT_ORDRE='OUI',
                    THETA=_F(THETA=THETA2),
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=_F(CHARGE=CH_P,),
                    SYME_CHAR='SYME'
                  )

G3=CALC_G( MODELE=MODELE,
