                    TOUT_ORDRE='OUI',
                    THETA=_F(THETA=THETA3),
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=_F(CHARGE=CH_P,),
                    SYME_CHAR='SYME'
                  )

DEPL5=CREA_CHAMP(OPERATION='EXTR',
                TYPE_CHAM='NOEU_DEPL_R',
                RESULTAT=evol,
                NUME_ORDRE=5,
                NOM_CHAM='DEPL',
                 )
VITE5=CREA_CHAMP(OPERATION='EXTR',
                TYPE_CHAM='NOEU_DEPL_R',
                RESULTAT=evol,
                NUME_ORDRE=5,
                NOM_CHAM='VITE',
                 )
ACCE5=CREA_CHAMP(OPERATION='EXTR',
                TYPE_CHAM='NOEU_DEPL_R',
                RESULTAT=evol,
                NUME_ORDRE=5,
                NOM_CHAM='ACCE',
                 )

G4=CALC_G( MODELE=MODELE,
                    THETA=_F(THETA=THETA1),
                    DEPL=DEPL5,
                    VITE=VITE5,
                    ACCE=ACCE5,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=_F(CHARGE=CH_P,),
                    SYME_CHAR='SYME'
                  )

IMPR_TABLE ( TABLE   = G1 );

IMPR_TABLE ( TABLE   = G2 );

IMPR_TABLE ( TABLE   = G3 );



