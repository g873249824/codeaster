DEBUT(CODE=_F( NOM = 'SSNV141A',NIV_PUB_WEB='INTERNET'))

# HEMISPHERE DOUBLEMENT PINCE
# MAILLAGE 1/4 DE L'HEMISPHERE
#
# MODELISATION MEC3QU9H: 100 MAILLES QUAD9
#
#

MATERI_1=DEFI_MATERIAU( ELAS=_F(  E = 6.825E+7,     NU = 0.3) )

#

MAILLA_1=LIRE_MAILLAGE(   )

MAILLA_1=DEFI_GROUP( reuse=MAILLA_1,   MAILLAGE=MAILLA_1,
                           CREA_GROUP_MA=_F(  NOM = 'TOUT',  TOUT = 'OUI') )

#

CH_MAT_1=AFFE_MATERIAU(  MAILLAGE=MAILLA_1,
                            AFFE=_F(  TOUT = 'OUI',   MATER = MATERI_1) )

#

MODELE_1=AFFE_MODELE(  MAILLAGE=MAILLA_1, VERIF='MAILLE',
                     AFFE=_F(  TOUT = 'OUI', MODELISATION = 'COQUE_3D',
                            PHENOMENE = 'MECANIQUE') )

#
#
#

CHARGE_1=AFFE_CHAR_MECA(   MODELE=MODELE_1,DDL_IMPO=(
                  _F( GROUP_NO = 'G_41',  DY = 0.,
                                              DRX = 0., DRZ = 0.),
                           _F( GROUP_NO = 'G_23',  DX = 0.,
                                              DRY = 0., DRZ = 0.),
                           _F( GROUP_NO = 'G_10',  DZ = 0.)),FORCE_NODALE=(
                  _F(  GROUP_NO = 'G_1',  FX = 1.E0),
                  _F(  GROUP_NO = 'G_2',  FY = -1.E0))
                           )

#

CARAC_1=AFFE_CARA_ELEM(    MODELE=MODELE_1,
                                COQUE=_F(  GROUP_MA = 'TOUT',
                                        EPAIS = 0.040,
                                        COEF_RIGI_DRZ = 1.E-3)
                           )

#
#

L_INST=DEFI_LIST_REEL(
                            DEBUT=0.0,
                       INTERVALLE=_F(  JUSQU_A = 100., NOMBRE = 10)

                                       )

#

D_FONC=DEFI_FONCTION(    NOM_PARA='INST',
                                VALE=(   0.,   0.,
                                       500., 500.,
                                     )
                         )

#
#

NONLIN=STAT_NON_LINE(
                            MODELE=MODELE_1,
                        CHAM_MATER=CH_MAT_1,
                         CARA_ELEM=CARAC_1,
                             EXCIT=_F(
                CHARGE = CHARGE_1,
                FONC_MULT = D_FONC),
                         COMP_ELAS=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'GREEN_GR'),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 50,
                RESI_GLOB_RELA = 1.E-6),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1)
              )

#
#
################## DEPLACEMENT X POSITIF


TEST_RESU(  RESU=_F(
                      RESULTAT = NONLIN,
                                 INST = 20.,
                      GROUP_NO = 'G_1',
                      NOM_CHAM = 'DEPL',
                      CRITERE = 'RELATIF',
                      PRECISION = 0.2E-2,
                      NOM_CMP = 'DX',
                      VALE = 1.4846)
            )


TEST_RESU(RESU=_F(NOM_CHAM='DEPL',
                  RESULTAT=NONLIN,
                  PRECISION=1.E-2,
                  CRITERE='RELATIF',
                  INST=50.0,
                  VALE=2.5785,
                  GROUP_NO='G_1',
                  NOM_CMP='DX',),);


TEST_RESU(RESU=_F(NOM_CHAM='DEPL',
                  RESULTAT=NONLIN,
                  PRECISION=1.E-2,
                  CRITERE='RELATIF',
                  INST=100.0,
                  VALE=3.3903,
                  GROUP_NO='G_1',
                  NOM_CMP='DX',),);

#
###########################   DEPLACEMENT Y NEGATIF


