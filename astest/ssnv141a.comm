                    RESULTAT = NONLIN,
                     INST = 20.,
                     NOM_CHAM = 'DEPL',
                     CRITERE = 'RELATIF',
                     PRECISION = 1.E-2,
                     GROUP_NO = 'G_2',
                     NOM_CMP = 'DY',
                     VALE = -1.7997)
            )

TEST_RESU(RESU=_F(NOM_CHAM='DEPL',
                  RESULTAT=NONLIN,
                  PRECISION=1.E-2,
                  CRITERE='RELATIF',
                  INST=50.0,
                  VALE=-3.7586,
                  GROUP_NO='G_2',
                  NOM_CMP='DY',),);

TEST_RESU(RESU=_F(NOM_CHAM='DEPL',
                  RESULTAT=NONLIN,
                  PRECISION=1.E-2,
                  CRITERE='RELATIF',
                  INST=100.0,
                  VALE=-5.8021,
                  GROUP_NO='G_2',
                  NOM_CMP='DY',),);

