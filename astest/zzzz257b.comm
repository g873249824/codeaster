# COMPORTEMENT ELASTIQUE

DEBUT(CODE=_F(NOM='ZZZZ257B',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

#####################################
###Definition de la fissure en dur###
#####################################

#Equivalent quatre noeuds sur le maillage initial
Lfiss = 3.5;
Dmax = Lfiss / 2.;

# Vect_KI
#  c'est le vecteur pour avoir l'erreur sur KI
#
#vx = 0.0;
#
#vy = 1.0;
#
#vz = 0.0;

# Vect_KII
#  c'est le vecteur pour avoir l'erreur sur KII

vx = 1.0;

vy = 0.0;

vz = 0.0;

#Fond de fissure

X0 = 3.5;

Y0 = 8.0;

#####################################
###Definition de la fissure en dur###
#####################################

young = 200000.0;
nu = 0.3;

#Fonctions necessaires a la definition du chargement pour le probleme dual
const = ((young * sqrt((2 * pi))) / 8.);


FPLUSX = FORMULE(VALE='const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vx',
                 NOM_PARA=('X','Y',),);

FPLUSY = FORMULE(VALE='const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vy',
                 NOM_PARA=('X','Y',),);

FPLUSZ = FORMULE(VALE='(const/(1+nu))*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vz',
                 NOM_PARA=('X','Y',),);

FMOINX = FORMULE(VALE='-const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vx',
                 NOM_PARA=('X','Y',),);

FMOINY = FORMULE(VALE='-const*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vy',
                 NOM_PARA=('X','Y',),);

FMOINZ = FORMULE(VALE='-(const/(1+nu))*2/(Dmax)**2*sqrt((X-X0)**2+(Y-Y0)**2)*vz',
                 NOM_PARA=('X','Y',),);

MAI=LIRE_MAILLAGE(FORMAT='MED',);

MAIB=CREA_MAILLAGE(MAILLAGE=MAI,
                   LINE_QUAD=_F(TOUT='OUI',
                                PREF_NOEUD='QN',
                                PREF_NUME=1,),);

MAIB=MODI_MAILLAGE(reuse =MAIB,
                   MAILLAGE=MAIB,
                   MODI_MAILLE=_F(OPTION='NOEUD_QUART',
                                  GROUP_MA_FOND='FONDFISS',),);

MAIB=DEFI_GROUP(reuse =MAIB,
                MAILLAGE=MAIB,
                CREA_GROUP_NO=_F(GROUP_MA='FONDFISS',
                                 NOM='NOEUFOND',),);

MATE=DEFI_MATERIAU(ELAS=_F(E=young,
                           NU=nu,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIB,
                    AFFE=_F(TOUT='OUI',
                            MATER=MATE,),);

MODL=AFFE_MODELE(MAILLAGE=MAIB,
                 AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='C_PLAN',),);

BLOC=AFFE_CHAR_MECA(MODELE=MODL,
                    DDL_IMPO=(_F(GROUP_MA='BAS',
                                 DX=0.0,
                                 DY=0.0,),),);

# -----------------------------
# definition du probleme primal
# -----------------------------

CHAR=AFFE_CHAR_MECA(MODELE=MODL,
                    FORCE_CONTOUR=(_F(GROUP_MA='HAUT',
                                      FX=1.0,),),);

RESU=MECA_STATIQUE(MODELE=MODL,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=BLOC,),
                          _F(CHARGE=CHAR,),),);

MAIB=DEFI_GROUP(reuse =MAIB,
                MAILLAGE=MAIB,
                CREA_GROUP_MA=(_F(NOM='CURVMAX',
                                  OPTION='SPHERE',
                                  GROUP_NO_CENTRE='NOEUFOND',
                                  RAYON=Dmax,),
                               _F(NOM='LSUP',
                                  INTERSEC=('LEVSUP','CURVMAX',),),
                               _F(NOM='LINF',
                                  INTERSEC=('LEVINF','CURVMAX',),),),);

FISS=DEFI_FOND_FISS(MAILLAGE=MAIB,
                    FOND_FISS=_F(GROUP_NO='NOEUFOND',),
                    LEVRE_SUP=_F(GROUP_MA='LSUP',),
                    LEVRE_INF=_F(GROUP_MA='LINF',),);

POST_K=POST_K1_K2_K3(MODELISATION='C_PLAN',
                     FOND_FISS=FISS,
                     MATER=MATE,
                     RESULTAT=RESU,);

IMPR_TABLE(TABLE=POST_K,
           UNITE=8,);


RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION=('ERME_ELEM',
                         'ERZ1_ELEM',
                         'ERZ2_ELEM',),);

# ---------------------------
# definition du probleme dual
# ---------------------------

CHARDUAL=AFFE_CHAR_MECA_F(MODELE=MODL,
                          FORCE_CONTOUR=(_F(GROUP_MA='LSUP',
                                            FX=FPLUSX,
                                            FY=FPLUSY,
                                            FZ=FPLUSZ,),
                                         _F(GROUP_MA='LINF',
                                            FX=FMOINX,
                                            FY=FMOINY,
                                            FZ=FMOINZ,),),);

RESUDUAL=MECA_STATIQUE(MODELE=MODL,
                       CHAM_MATER=CHMAT,
                       EXCIT=(_F(CHARGE=BLOC,),
                              _F(CHARGE=CHARDUAL,),),);

RESUDUAL=CALC_ERREUR(reuse =RESUDUAL,
                     RESULTAT=RESUDUAL,
                     OPTION=('ERME_ELEM',
                             'ERZ1_ELEM',
                             'ERZ2_ELEM',),);

# ---------------------------
# estimation de l'erreur en quantite d'interet
# ---------------------------

RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION='QIRE_ELEM',
                 RESU_DUAL=RESUDUAL,);

RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION='QIZ1_ELEM',
                 RESU_DUAL=RESUDUAL,);

RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 OPTION='QIZ2_ELEM',
                 RESU_DUAL=RESUDUAL,);

# ---------------------------
# tests sur les resultats
# ---------------------------

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.029999999999999999,
           VALE_CALC=4.646733265,
           VALE_REFE=4.5300000000000002,
           NOM_PARA='K2',
           TABLE=POST_K,)

TEST_TABLE(CRITERE='RELATIF',
           VALE_CALC=4.6467332648066,
           NOM_PARA='K2',
           TABLE=POST_K,
           )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERRE',
                  RESULTAT=RESU,
                  VALE_CALC=0.010037405543237,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ1',
                  RESULTAT=RESU,
                  VALE_CALC=0.012771522000038,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ2',
                  RESULTAT=RESU,
                  VALE_CALC=0.043550246358789,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERRE',
                  RESULTAT=RESUDUAL,
                  VALE_CALC=31.841311747473,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ1',
                  RESULTAT=RESUDUAL,
                  VALE_CALC=58.904641859877,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_ERZ2',
                  RESULTAT=RESUDUAL,
                  VALE_CALC=98.516683079872,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_QIRE',
                  RESULTAT=RESU,
                  VALE_CALC=47.392126029164999,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_QIZ1',
                  RESULTAT=RESU,
                  VALE_CALC=0.012771522000038,
                  CRITERE='RELATIF',
                  ),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  PARA='ERREUR_QIZ2',
                  RESULTAT=RESU,
                  VALE_CALC=0.043550246358789,
                  CRITERE='RELATIF',
                  ),
          )

FIN();
