                                 GROUP_NO='COTEG'),
                              _F(DZ=0.0,
                                 NOEUD='N1',
                                 DY=0.0),
                              _F(NOEUD='N5',
                                 DY=0.0),),
                    MODELE=MODELE);

DEPL1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=( 0.0,0.0,
                           1.0,1.00e-3,
                           2.0,0.0,
                           3.0,5.0e-3,
                           4.0,-4.0e-3,
                           5.0,0.0,
                           6.0,-10.0e-3,
                           7.0,0.0),
                    PROL_DROITE='EXCLU',
                    PROL_GAUCHE='EXCLU',);

CHAR1=AFFE_CHAR_MECA_F(MODELE=MODELE,
                       DDL_IMPO=_F(GROUP_NO='COTED',
                                   DX=DEPL1,),);

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.0,
                                     NOMBRE=50,),
                                  _F(JUSQU_A=2.0,
