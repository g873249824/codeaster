DEBUT(CODE=_F( NOM = 'SDLS300A',NIV_PUB_WEB='INTERNET'))

MAYA=LIRE_MAILLAGE(FORMAT='MED', )

#
# DEFINITION DU MODELE
#

STRUC=AFFE_MODELE(    MAILLAGE=MAYA,
                               AFFE=_F(  GROUP_MA = 'TOUT',
                                      MODELISATION = 'DKT',
                                      PHENOMENE = 'MECANIQUE'))

#
# DEFINITION DES PROPRIETES PHYSIQUES
#

ACIER=DEFI_MATERIAU(         
                         ELAS=_F(  RHO = 2244.,
                           NU = 0.166,
                           E = 2.76E10))

MATER=AFFE_MATERIAU(    MAILLAGE=MAYA,
                                 AFFE=_F(  GROUP_MA = 'TOUT',
                                        MATER = ACIER))

CARA=AFFE_CARA_ELEM(      MODELE=STRUC,
                                 COQUE=_F(  GROUP_MA = 'TOUT',
                                         EPAIS = 0.305)
                          )

#
# DEFINITION DES CONDITIONS AUX LIMITES
#

FIXA=AFFE_CHAR_MECA(      MODELE=STRUC,

                                       DDL_IMPO=_F(  GROUP_NO = 'FIXA',
                                               DX = 0.,
                                               DY = 0.,
                                               DZ = 0.,
                                               DRX = 0.,
                                               DRY = 0.,
                                               DRZ = 0.)
                          )

# CALCUL DES MATRICES ELEMENTAIRES DE RIGIDITE ET DE MASSE

K_ELEM=CALC_MATR_ELEM(        MODELE=STRUC,
                                 CARA_ELEM=CARA,
                                    CHARGE=FIXA,
                                CHAM_MATER=MATER,
                                   OPTION='RIGI_MECA' )

M_ELEM=CALC_MATR_ELEM(        MODELE=STRUC,
                                 CARA_ELEM=CARA,
                                    CHARGE=FIXA,
                                CHAM_MATER=MATER,
                                   OPTION='MASS_MECA' )

# NUMEROTATION

NUMERO=NUME_DDL(    MATR_RIGI=K_ELEM )

# ASSEMBLAGE DES MATRICES DE RIGIDITE ET DE MASSE


K_ASSE=ASSE_MATRICE(    MATR_ELEM=K_ELEM,
                             NUME_DDL=NUMERO  )

M_ASSE=ASSE_MATRICE(    MATR_ELEM=M_ELEM,
                             NUME_DDL=NUMERO   )

TABL_MAS=POST_ELEM(        MODELE=STRUC,
                          CHAM_MATER=MATER,
                           CARA_ELEM=CARA,
                          MASS_INER=_F(  TOUT = 'OUI')
                      )

MODE1=MACRO_MODE_MECA(    MATR_RIGI=K_ASSE,
                          MATR_MASS=M_ASSE,
                          CALC_FREQ=_F(  FREQ=(1.,2.,3.,4.,5.,6.,7.),
                                         COEF_DIM_ESPACE = 3,),
                          NORM_MODE=_F(  NORME = 'TRAN_ROTA',),
                          FILTRE_MODE=_F(  CRIT_EXTR = 'MASS_EFFE_UN'),
                          IMPRESSION=_F(  CUMUL = 'OUI')
                       )


TEST_RESU(RESU=(_F(PARA='FREQ',
                   NUME_MODE=47,
                   RESULTAT=MODE1,
                   VALE_CALC=2.800653445,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                _F(PARA='FREQ',
                   NUME_MODE=48,
                   RESULTAT=MODE1,
                   VALE_CALC=2.800653445,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                _F(PARA='FREQ',
                   NUME_MODE=158,
                   RESULTAT=MODE1,
                   VALE_CALC=5.9249066729999997,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                _F(PARA='FREQ',
                   NUME_MODE=159,
                   RESULTAT=MODE1,
                   VALE_CALC=5.9249066729999997,
                   TOLE_MACHINE=9.9999999999999995E-08,
                   CRITERE='RELATIF',
                   ),
                ),
          )

FIN()
