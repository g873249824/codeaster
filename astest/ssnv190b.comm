INCLUDE(DONNEE='ssnv190b_mater.comm')
materS=AFFE_MATERIAU(
   MAILLAGE=mail,
   AFFE      = _F(TOUT='OUI', MATER=ACIERS,),
   AFFE_VARC = (_F(TOUT='OUI', EVOL =Irrad,   NOM_VARC='IRRA'),
