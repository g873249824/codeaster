   MODELE=mode,
   DDL_IMPO=(
      _F(NOEUD='N1',DX=ZERO,DY=ZERO,DZ=ZERO),
      _F(NOEUD='N4',DX=ZERO,        DZ=ZERO),
      _F(NOEUD='N8',DX=ZERO,),
      _F(NOEUD='N5',DX=ZERO,DY=ZERO),

      _F(NOEUD='N2',DX=FDepla,DY=ZERO,DZ=ZERO),
      _F(NOEUD='N3',DX=FDepla,        DZ=ZERO),
      _F(NOEUD='N7',DX=FDepla,),
      _F(NOEUD='N6',DX=FDepla,DY=ZERO),
   )
)

