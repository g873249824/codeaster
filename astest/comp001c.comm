DEBUT(CODE=_F(NOM='COMP001C',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),);
# donnee materiau et fonction

#parametres elastiques
YOUNG = 200000.0;
POISSON = 0.3;

#parametres loi CINE_LINE
SY = 437.0;
pente = 2024.74690664;
# def des materiaux
ACIER = [None]*2
#unite en Pa
C_Pa = 1.e+6
YOUNG_Pa = YOUNG * C_Pa
pente_Pa = pente * C_Pa
SY_Pa = SY * C_Pa;

ACIER[0]=DEFI_MATERIAU(ELAS=_F(E=YOUNG_Pa,
                               NU=POISSON,
                               ALPHA=11.8e-6),
                    ECRO_LINE=_F(D_SIGM_EPSI=pente_Pa,
                                 SY=SY_Pa,),);
#unite en MPa
ACIER[1]=DEFI_MATERIAU(ELAS=_F(E=YOUNG,
                               NU=POISSON,
                               ALPHA=11.8e-6),
                    ECRO_LINE=_F(D_SIGM_EPSI=pente,
                                 SY=SY,),);


compor='VMIS_CINE_LINE'


tabresu=TEST_COMPOR(OPTION='MECA',

              COMPORTEMENT=_F(RELATION=compor,),
              NEWTON=_F(REAC_ITER=1),
              LIST_MATER=ACIER,
              LIST_NPAS=4*[1] + [1, 5, 25],
              VARI_TEST=('V7','VMIS','TRACE'),
              LIST_TOLE=4*[2.] + [1.E-1] + 2*[1.E-2] + [1.E-6] ,
              YOUNG=YOUNG,POISSON=POISSON,
              )

IMPR_TABLE(TABLE=tabresu)

FIN()
