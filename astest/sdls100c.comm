           NUME_ORDRE = 3,       VALE = 2.582),
   _F(   RESULTAT = MODE, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.6E-2,
           NUME_ORDRE = 4,       VALE = 3.306),
   _F(   RESULTAT = MODE, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.6E-2,
           NUME_ORDRE = 5,       VALE = 3.753),
   _F(   RESULTAT = MODE, REFERENCE = 'ANALYTIQUE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.6E-2,
           NUME_ORDRE = 6,       VALE = 6.555),
           #-----------------------------------------
           # TESTS / RESULTATS ASTER V1.02.12 -------
           #-----------------------------------------
   _F(   RESULTAT = MODE, REFERENCE = 'NON_REGRESSION', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 3.E-2,
           NUME_ORDRE = 7,       VALE = 7.3756),
   _F(   RESULTAT = MODE, REFERENCE = 'NON_REGRESSION', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 2.E-2,
           NUME_ORDRE = 8,       VALE = 7.7332),
   _F(   RESULTAT = MODE, REFERENCE = 'NON_REGRESSION', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 3.E-2,
           NUME_ORDRE = 9,       VALE = 8.5567),
   _F(   RESULTAT = MODE, REFERENCE = 'NON_REGRESSION', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 4.E-2,
           NUME_ORDRE = 10,      VALE = 11.1199),
   _F(   RESULTAT = MODE, REFERENCE = 'NON_REGRESSION', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 4.E-2,
           NUME_ORDRE = 11,      VALE = 11.6474),
   _F(   RESULTAT = MODE, REFERENCE = 'NON_REGRESSION', PARA = 'FREQ',
