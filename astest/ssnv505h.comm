CHSIG=AFFE_CHAR_MECA(MODELE=MODE,
                   CONTACT=_F(GROUP_MA_MAIT='POU1',
                              GROUP_MA_ESCL='POU2',                                  
                              METHODE='CONTINUE',
                              MODL_AXIS='NON',
                                 VECT_ORIE_POU=(0.0,0.0,-1.0,),
                                 DIST_MAIT=31.75,
