                INCREMENT=_F(LIST_INST=L_INST,
#                             INST_FIN=1.,
                             SUBD_PAS=4,
                             SUBD_PAS_MINI=1E-8,
                             ),
                NEWTON=_F(REAC_ITER=5,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
#                ARCHIVAGE=_F(ARCH_ETAT_INIT='OUI',
#                             LIST_INST=L_ARCH),
                INFO=1,);

UCON=CALC_NO(reuse =UCON,
          RESULTAT=UCON,
          CARA_ELEM=CHAMPCAR,
          OPTION='FORC_NODA',
          MODELE=MODE,
          CHAM_MATER=CM,
         );

# ---- TABLEAUX ----
                  
T_REA_CO=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=UCON,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);
                                 
T_DEP_CO=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
