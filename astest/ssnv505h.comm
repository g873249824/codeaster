#       ELEMENTS DE POUTRE, METHODES LAGRANGIENNE ET CONTINUE
# CAS TEST  SSNV505H
# MODELE 
# MAILLAGE : ELEMENT DE POUTRES SEG2 EN MODELISATION 3D
# UNITES   : NEWTON , MILLIMETRE , SECONDE (N,MM,S)
#
#

DEBUT(CODE=_F( NOM = 'SSNV505H',NIV_PUB_WEB='INTERNET'));

# ---------------------  MAILLAGE ----------------

PRE_GIBI();

#Acquisition du maillage des poutres encastrees

MAILL1=LIRE_MAILLAGE();

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_NO=(_F(GROUP_MA=('POU1',),
                                   NOM='GROUP1',),
                                 _F(GROUP_MA=('POU2',),
                                   NOM='GROUP2',),
                                 ));

#Acquisition du maillage de la section en fibres

PRE_GIBI(UNITE_GIBI=21,UNITE_MAILLAGE=31);

MA_SECT=LIRE_MAILLAGE(UNITE=31);

                       
# ---- MODELE : POUTRE DROITE D'EULER ----
                
MODE=AFFE_MODELE(MAILLAGE=MAILL1,
                 VERIF='MAILLE',
                 AFFE=(_F(GROUP_MA='POU1',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_TGM',),
                       _F(GROUP_MA='POU2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_TGM',),
                       ),);

# ------------- DEFINITON DU MATERIAU -----------

MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,NU=0.33333,RHO=7800.,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=_F(GROUP_MA=('POU1','POU2'),
                         MATER=MPOUTRE,),);
                         
CHAMPCAR=AFFE_CARA_ELEM(MODELE=MODE,INFO=2,
                      POUTRE=_F(GROUP_MA=('POU1','POU2'),
                                SECTION='GENERALE',
                                CARA=('A','IY','IZ','AY','AZ','JX','JG','IYR2','IZR2',),
                                VALE=(645.16,5.54975E3,2.167872E5,1.2,1.2,
                                      5.08,0.0,0.0,0.),),
                      AFFE_SECT=_F(GROUP_MA=('POU1','POU2'),
                                   GROUP_MA_SECT='SECTION',
                                   MAILLAGE_SECT=MA_SECT,
                                   COOR_AXE_POUTRE=(0.0,0.0,),),);

# ---------- CONDITIONS AUX LIMITES --------------

COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO=('A','B',),
                                     DX=0.0,DY=0.0,DZ=0.0,
                                     DRX=0.0,DRY=0.0,DRZ=0.0),
                                  _F(GROUP_NO=('GROUP1','GROUP2'),
                                     DZ=0.0,DRX=0.0,DRY=0.0,),
                                  ),);

CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=(_F(GROUP_NO=('C'),
                                DY=-790.,),
                             ),);

# ---- METHODE LAGRANGIENNE ----
CHLAG=AFFE_CHAR_MECA(MODELE=MODE,
                   CONTACT=_F(GROUP_MA_MAIT='POU1',
                              GROUP_MA_ESCL='POU2',                              
                              METHODE='LAGRANGIEN',
                                  DIST_MAIT=31.75,
                                  DIST_ESCL=31.75,
                                  FROTTEMENT='COULOMB',
                                  COEF_MATR_FROT=1.,
                                  VECT_Y=(0.,0.,1.,),
                                  REAC_GEOM='AUTOMATIQUE',
                                  COULOMB=0.,
                              
                              ),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

#L_INST=DEFI_LIST_REEL(DEBUT=0.0,
#                      INTERVALLE=(_F(JUSQU_A=1.,
#                                     NOMBRE=50,),),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                                     INTERVALLE=(_F(JUSQU_A=.1,
                                     NOMBRE=25,),),);
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=50,),),);

ULAG=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                       _F(CHARGE=CHLAG,),
                      ),
                COMP_INCR=_F(RELATION='ELAS',
                             DEFORMATION='REAC_GEOM',),
                INCREMENT=_F(LIST_INST=L_INST,
#                             INST_FIN=1.,
                             SUBD_PAS=4,
                             SUBD_PAS_MINI=1E-8,
                             ),
                NEWTON=_F(REAC_ITER=5,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
#                ARCHIVAGE=_F(ARCH_ETAT_INIT='OUI',
#                             LIST_INST=L_ARCH),
                INFO=1,);

ULAG=CALC_NO(reuse =ULAG,
          RESULTAT=ULAG,
          CARA_ELEM=CHAMPCAR,
          OPTION='FORC_NODA',
          MODELE=MODE,
          CHAM_MATER=CM,
         );

# ---- TABLEAUX ----
                  
T_REA_LA=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=ULAG,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);
                                 
T_DEP_LA=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO=('C','A1','B1'),
                                 RESULTAT=ULAG,
                                 NOM_CHAM='DEPL',
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=T_REA_LA,);
IMPR_TABLE(TABLE=T_DEP_LA,);

IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(RESULTAT=ULAG,NOM_CHAM='DEPL'))

TEST_TABLE(TABLE=T_REA_LA,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.2),
           VALE=3.11885E2,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.7',);

TEST_TABLE(TABLE=T_REA_LA,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4),
           VALE=1.20897E3,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.7',);

TEST_TABLE(TABLE=T_REA_LA,
