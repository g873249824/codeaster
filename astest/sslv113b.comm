# EN PRESSION INTERNE ET EXTERNE SUR 2 MATERIAUX
#
# MODELISATION 1 : ELEMENT MECANIQUE AXISYMETRIQUE QUAD8
# MAILLAGE       : GIBI > ASTER

DEBUT(CODE=_F(NOM='SSLV113B',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

M=LIRE_MAILLAGE(FORMAT='MED',);

MO=AFFE_MODELE(MAILLAGE=M,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

MA1=DEFI_MATERIAU(ELAS=_F(E=2.,
                          NU=0.3,
                          ALPHA=0.,),);

MA2=DEFI_MATERIAU(ELAS=_F(E=1.,
                          NU=0.3,
                          ALPHA=0.,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=(_F(GROUP_MA='DOM1',
                          MATER=MA1,),
                       _F(GROUP_MA='DOM2',
                          MATER=MA2,),),);

#
#  F1 = SIGRR SUR LE MATERIAU MA1

F1 = FORMULE(VALE="""
1.15385*(2*-0.98097+0.91333)+2*0.76923*(-0.98097
                -(-1.11741/R**2))
                """,
             NOM_PARA='R',);

#
#  F2 = SIGTT SUR LE MATERIAU MA1

F2 = FORMULE(VALE="""
1.15385*(2*-0.98097+0.91333)+2*0.76923*(-0.98097
                +(-1.11741/R**2))
                """,
             NOM_PARA='R',);

#
#  F3 = SIGRR SUR LE MATERIAU MA2

F3 = FORMULE(VALE="""
0.57692*(2*-1.34405+0.91333)+2*0.38461*(-1.34405
                -(-0.30048/R**2))
                """,
             NOM_PARA='R',);

#
#  F4 = SIGTT SUR LE MATERIAU MA2

