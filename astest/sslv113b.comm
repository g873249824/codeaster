                             _F(GROUP_MA='GRMA2',
                                DY=0.91333,),),
                  FORCE_CONTOUR=(_F(GROUP_MA='GRMA3',
                                    FX=1.,),
                                 _F(GROUP_MA='GRMA4',
                                    FX=-2.,),),);

RESU=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CM,
                   EXCIT=_F(CHARGE=CH,),);

RESU=CALC_ERREUR(reuse =RESU,
                 RESULTAT=RESU,
                 TOUT_ORDRE='OUI',
                 OPTION='ERME_ELEM',);

