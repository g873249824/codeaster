DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),
      DEBUG=_F(SDVERI='OUI'), IGNORE_ALARM=('MODELE1_63'))

#----------------------------------------------
#            MAILLAGE
#----------------------------------------------

MA=LIRE_MAILLAGE(INFO=1,FORMAT='MED',);

# longueur du cote de la plaque
L = 1.

# GROUPES SUR LESQUELS ON VA IMPOSER LES DIRICHLETS
MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOM = 'LIGNSUP',GROUP_MA='LIGSUP',),
                             _F(NOM = 'LIGNINF',GROUP_MA='LIGINF',),),)

#----------------------------------------------
#            MODELE ET FISSURE
#----------------------------------------------

MODTHIN=AFFE_MODELE(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='THERMIQUE',
                            MODELISATION='PLAN',),);

FISSTH=DEFI_FISS_XFEM(MAILLAGE=MA,
                      DEFI_FISS=_F(FORM_FISS='DEMI_DROITE',
                                   PFON=( 0.5*L, 0.5*L, 0.,),
                                   DTAN=(   -1.,    0., 0.,),),
                      INFO=1,);

MODTHX=MODI_MODELE_XFEM(MODELE_IN=MODTHIN,
                        FISSURE=FISSTH,
                        INFO=2,);

#----------------------------------------------
#            MATERIAU
#----------------------------------------------

MATE=DEFI_MATERIAU(THER=_F(LAMBDA=1.0,RHO_CP = 2.0))

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    MODELE=MODTHX,
                    AFFE=_F(TOUT='OUI',MATER=MATE,),)


#----------------------------------------------
#            CHARGEMENT THERMIQUE
#----------------------------------------------

TIMPSUP=DEFI_FONCTION(NOM_PARA='INST',
                      NOM_RESU='TEMP',  
        VALE=(0.,    20. ,
              1.,    40.,))

TIMPINF=DEFI_FONCTION(NOM_PARA='INST',
                      NOM_RESU='TEMP',  
        VALE=(0.,    10. ,
              1.,    20.,))

CHTH=AFFE_CHAR_THER_F(MODELE=MODTHX,
                      TEMP_IMPO=(
                               _F(GROUP_NO = 'LIGNSUP',TEMP = TIMPSUP),
                               _F(GROUP_NO = 'LIGNINF',TEMP = TIMPINF),
                               ))

CHTHEPR=AFFE_CHAR_THER(MODELE=MODTHX,
                       ECHANGE_PAROI=_F(FISSURE = FISSTH,
                                        COEF_H  =  2.,),)

#----------------------------------------------
#            CALCUL THERMIQUE LINEAIRE TRANSITOIRE
#----------------------------------------------

LR8=DEFI_LIST_REEL(DEBUT=0.,
                   INTERVALLE=(_F(JUSQU_A = 1., NOMBRE = 5)))

TEMPEX=THER_LINEAIRE(MODELE=MODTHX,
                     CHAM_MATER=CHMAT,
                     EXCIT=(
                            _F(CHARGE=CHTH,),
                            _F(CHARGE=CHTHEPR,),
                            ),
                     ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
                     INCREMENT=_F( LIST_INST = LR8),                    
                     )

#----------------------------------------------
#            POST-TRAITEMENTS
#----------------------------------------------

MA_VISU=POST_MAIL_XFEM(MODELE=MODTHX)

# GROUPES SUR LESQUELS ON VA TESTER LES VALEURS DE T
hsain = L/101.
hfiss = 0.5*hsain
MA_VISU=DEFI_GROUP(reuse =MA_VISU,
                   MAILLAGE=MA_VISU,
                   CREA_GROUP_MA=(
                                  _F(NOM='MASUPTMP',OPTION='SPHERE',
                                     POINT=(L,0.5*L+hfiss),RAYON=1.e-6*hfiss),
                                  _F(NOM='MAINFTMP',OPTION='SPHERE',
                                     POINT=(L,0.5*L-hfiss),RAYON=1.e-6*hfiss),
                                  ),
                   CREA_GROUP_NO=(
                                  _F(GROUP_MA='MASUPTMP',),
                                  _F(GROUP_MA='MAINFTMP',),
                                  _F(NOM='GNFOND',OPTION='ENV_SPHERE',
                                     POINT=(0.5*L,0.5*L,),RAYON=1.e-6*hfiss,
                                     PRECISION=1.e-6*hfiss),
                                  _F(NOM='GNOTMP',OPTION='ENV_SPHERE',
                                     POINT=(L,0.5*L,),RAYON=1.e-6*hfiss,
                                     PRECISION=1.e-6*hfiss),
                                  _F(NOM='NTESTSUP',INTERSEC=('GNOTMP','MASUPTMP')),
                                  _F(NOM='NTESTINF',INTERSEC=('GNOTMP','MAINFTMP')),
                                  ),)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_VISU,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='THERMIQUE',
                             MODELISATION='PLAN',),)

RES_VISU=POST_CHAM_XFEM(MODELE_VISU = MOD_VISU,
                        RESULTAT    = TEMPEX)

#----------------------------------------------
#            RELEVES
#----------------------------------------------

TEMPSUP=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='NTESTSUP',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

TEMPINF=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='NTESTINF',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

TEMPFON=POST_RELEVE_T(ACTION=_F(INTITULE='TEMPE',
                                GROUP_NO='GNFOND',
                                RESULTAT=RES_VISU,
                                NOM_CHAM='TEMP',
                                TOUT_CMP='OUI',
                                NUME_ORDRE = 5,
                                OPERATION='EXTRACTION',),)

#----------------------------------------------
#            TESTS
#----------------------------------------------

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=29.1588843137,
           VALE_REFE=29.156091860463,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPSUP,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=29.1588843137,
           VALE_REFE=29.156091860463,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPSUP,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=23.3961322475,
           VALE_REFE=23.393394671258,
           TYPE_TEST='MIN',
           NOM_PARA='TEMP',
           TABLE=TEMPINF,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=23.3961322475,
           VALE_REFE=23.393394671258,
           TYPE_TEST='MAX',
           NOM_PARA='TEMP',
           TABLE=TEMPINF,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=26.2775156497,
           VALE_REFE=26.25259365185,
           PRECISION=5.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MIN',
           TABLE=TEMPFON,)

TEST_TABLE(REFERENCE='AUTRE_ASTER',
           VALE_CALC=26.2775156497,
           VALE_REFE=26.25259365185,
           PRECISION=5.E-3,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TEMPFON,)

#----------------------------------------------
#            IMPRESSIONS
#----------------------------------------------

IMPR_TABLE(TABLE=TEMPSUP)
IMPR_TABLE(TABLE=TEMPINF)
IMPR_TABLE(TABLE=TEMPFON)

IMPR_RESU(FORMAT='MED',UNITE=83,RESU=_F(MAILLAGE=MA))
IMPR_RESU(FORMAT='MED',UNITE=81,RESU=_F(MAILLAGE=MA_VISU))
IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_VISU))

FIN();
