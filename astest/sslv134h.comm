# METHODE X-FEM (FISSURE NON MAILLEE)

DEBUT(   CODE=_F( NOM = 'SSLV134H',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA1=LIRE_MAILLAGE(INFO=1,FORMAT='MED',);

MA1=DEFI_GROUP(reuse =MA1,
                    MAILLAGE=MA1,
                    CREA_GROUP_NO=(_F(GROUP_MA='PEAUEXT',),
                                   _F(GROUP_MA='PEAUINT',), 
                                   _F(GROUP_MA='EXTUBE_S',),
                                   _F(GROUP_MA='EXTUBE_I',), 
                                   _F(GROUP_MA='BORD1',), 
                                   _F(GROUP_MA='BORD2',), 
                                ),
                                    );
  

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.E11,  NU = 0.3, RHO = 0.,
                            ALPHA = 0.))
                            
MODELEIN=AFFE_MODELE(MAILLAGE=MA1,
                     INFO = 1,
                   AFFE=(_F(GROUP_MA=('TUBE',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                         _F(GROUP_MA=('EXTUBE_S','EXTUBE_I',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                  ),);        

MA1=MODI_MAILLAGE(reuse =MA1,
                       MAILLAGE=MA1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('EXTUBE_S','EXTUBE_I',),),
                 );

# PARAMETRES DE LA FISSURE
x0 = 8.18
y0 = 0.
z0 = 2.05
ray = 2.

# avec l'orientation automatique du fond de fissure
# on tourne dans l'autre sans, il faudra changer les DTAN_ORIG....
FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(
                                 FORM_FISS      = 'ELLIPSE',
                                 DEMI_GRAND_AXE = ray,
                                 DEMI_PETIT_AXE = ray,
                                 CENTRE         = (x0, y0, z0),
                                 VECT_X         = (0. , 1. , 0. ),
                                 VECT_Y         = (1., 0. , 0. ),
                                 ),
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         INFO=1,
                         );

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MA1,
                       MODELE=MODELEK,
                       AFFE=(_F(TOUT = 'OUI',
                                MATER=MAT,),
                             ),);
CHAR1=AFFE_CHAR_MECA(INFO=1,
                      MODELE=MODELEK,
                     DDL_IMPO=(
                              _F(GROUP_NO='PEAUEXT',
                                       DX=0.0,),   
                              _F(GROUP_NO='EXTUBE_I',
                                       DZ=0.0,),
                              _F(GROUP_NO='BORD1',
                                       DY=0.0,),
                                        ), );   

CHAR2=AFFE_CHAR_MECA(INFO=1,
                      MODELE=MODELEK,
                      PRES_REP=(_F(GROUP_MA='EXTUBE_S',
                                    PRES=-1.E6,), 
                      ),   );
                      
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',);

UTOT1=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CHAR1),
                          _F(CHARGE=CHAR2),
                          _F(CHARGE=CHXFEM),
                          ),
                   SOLVEUR=_F(PCENT_PIVOT=10,
                            METHODE='MUMPS'),
                   INFO=1,
                   ); 


#----------------------------------------------------------------------
#                       POST-TRAITEMENT DE K1
#----------------------------------------------------------------------

# LISSAGE LEGENDRE
SIFTH1=CALC_G( RESULTAT=UTOT1,
               OPTION='CALC_K_G',
               NUME_ORDRE=1,
               THETA = _F(R_INF=0.2,
                          R_SUP=0.8,
                          FISSURE=FISS,
                          DTAN_EXTR=(0.,1.,0.),
                          DTAN_ORIG=(-1.,0.,0.),),);

IMPR_TABLE(TABLE=SIFTH1,);

# LISSAGE LEGENDRE + REPARTITION UNIFORME DE 24 POINTS
SIFTH2=CALC_G(    RESULTAT=UTOT1,
                  OPTION='CALC_K_G',
                  NUME_ORDRE=1,
                  THETA = _F(R_INF=0.2,
                             R_SUP=0.8,
                             FISSURE=FISS,
                             NB_POINT_FOND = 24,
                             DTAN_EXTR=(0.,1.,0.),
                             DTAN_ORIG=(-1.,0.,0.),),);
IMPR_TABLE(TABLE=SIFTH2,);

# LISSAGE LAGRANGE
SIFTH3=CALC_G(  RESULTAT=UTOT1,
                OPTION='CALC_K_G',NUME_ORDRE=1,
                LISSAGE = _F(LISSAGE_THETA='LAGRANGE',
                             LISSAGE_G='LAGRANGE',),
                THETA = _F(R_INF=0.2,
                           R_SUP=0.8,
                           FISSURE=FISS,
                           DTAN_EXTR=(0.,1.,0.),
                           DTAN_ORIG=(-1.,0.,0.),),);
IMPR_TABLE(TABLE=SIFTH3,);

# LISSAGE LAGRANGE + REPARTITION UNIFORME DE 24 POINTS
SIFTH4=CALC_G(  RESULTAT=UTOT1,
                OPTION='CALC_K_G',NUME_ORDRE=1,
                LISSAGE = _F(LISSAGE_THETA='LAGRANGE',
                             LISSAGE_G='LAGRANGE',),
                THETA = _F(R_INF=0.2,
                           R_SUP=0.8,
                           FISSURE=FISS,
                           NB_POINT_FOND = 24,
                           DTAN_EXTR=(0.,1.,0.),
                           DTAN_ORIG=(-1.,0.,0.),),);
IMPR_TABLE(TABLE=SIFTH4,);

# LISSAGE LAGRANGE + REPARTITION UNIFORME DE 10 POINTS
SIFTH5=CALC_G(  RESULTAT=UTOT1,
                OPTION='CALC_K_G',NUME_ORDRE=1,
                LISSAGE = _F(LISSAGE_THETA='LAGRANGE',
                             LISSAGE_G='LAGRANGE',),
                THETA = _F(R_INF=0.2,
                           R_SUP=0.8,
                           FISSURE=FISS,
                           NB_POINT_FOND = 10,
                           DTAN_EXTR=(0.,1.,0.),
                           DTAN_ORIG=(-1.,0.,0.),),);
IMPR_TABLE(TABLE=SIFTH5,);



SIFK1=POST_K1_K2_K3(  MODELISATION='3D',
                      FISSURE=FISS,
                      MATER=MAT,NUME_ORDRE=1,
                      RESULTAT=UTOT1,
                      ABSC_CURV_MAXI=0.5,
                      DTAN_EXTR = (0.,1.,0.),
                      DTAN_ORIG = (-1.,0.,0.),                      
                      )

IMPR_TABLE(TABLE=SIFK1,);


SIFK2=POST_K1_K2_K3(  MODELISATION='3D',
                      FISSURE=FISS,
                      NB_POINT_FOND = 10,
                      INFO=1,
                      MATER=MAT,NUME_ORDRE=1,
                      RESULTAT=UTOT1,
                      ABSC_CURV_MAXI=0.5,
                      DTAN_EXTR = (0.,1.,0.),
                      DTAN_ORIG = (-1.,0.,0.),
                      )

IMPR_TABLE(TABLE=SIFK2,);

                    
#----------------------------------------------------------------------
#              TEST DE K1 EN 1 POINT
#----------------------------------------------------------------------

# TEST DU LISSAGE LEGENDRE
TEST_TABLE(      TABLE=SIFTH1,
                   TYPE_TEST='MAX',
                   FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                 NOM_PARA='K1',
                      VALE=1.595E+6,
                 PRECISION=1.2E-1  ,
                      REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=SIFTH1,
                   TYPE_TEST='MIN',
                 NOM_PARA='K1',
                 FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                      VALE=1.595E+6,
                 PRECISION=3.E-2  ,
                      REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=SIFTH1,
                   TYPE_TEST='MIN',
                 NOM_PARA='K1',
                 FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                      VALE=1.56653508E+06,
                 PRECISION=1.E-3  ,
                      REFERENCE='NON_REGRESSION'      )

# TEST DU LISSAGE LEGENDRE + REPARTITION UNIFORME DE 24 POINTS
TEST_TABLE(      TABLE=SIFTH2,
                 TYPE_TEST='MAX',
                 FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                 NOM_PARA='K1',
                 VALE=1.595E+6,
                 PRECISION=0.18,
                 REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=SIFTH2,
                 TYPE_TEST='MIN',
                 FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                 NOM_PARA='K1',
                 VALE=1.595E+6,
                 PRECISION=0.04 ,
                 REFERENCE='ANALYTIQUE'      )

# TEST DU LISSAGE LAGRANGE : OSCILLATIONS
TEST_TABLE(      TABLE=SIFTH3,
                 TYPE_TEST='MAX',
                 FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                 NOM_PARA='K1',
                 VALE=2.719488E+06 ,
                 PRECISION=1.E-4  ,
                 REFERENCE='NON_REGRESSION'      )

# TEST DU LISSAGE LAGRANGE + REPARTITION UNIFORME DE 24 POINTS : OSCILLATIONS MOINDRES
TEST_TABLE(      TABLE=SIFTH4,
                 TYPE_TEST='MAX',
                 NOM_PARA='K1',
                 FILTRE = _F(NOM_PARA='NUME_ORDRE',VALE_I=1),
                 VALE=2.02995E+06,
                 PRECISION=1.E-4 ,
                 REFERENCE='NON_REGRESSION'      )

# TEST DU LISSAGE LAGRANGE + REPARTITION UNIFORME DE 10 POINTS : PLUS D'OSCILLATIONS
TEST_TABLE(      TABLE=SIFTH5,
                 FILTRE=(_F(NOM_PARA = 'NUM_PT', VALE_I = 5), 
                 _F(NOM_PARA='NUME_ORDRE',VALE_I=1),),
                 NOM_PARA='K1',
                 VALE=1.595E+6,
                 PRECISION=0.06 ,
                 REFERENCE='ANALYTIQUE'      )

# TEST DE POST_K1_K2_K3
TEST_TABLE(      TABLE=SIFK1,
                 FILTRE=(_F(NOM_PARA = 'PT_FOND',
                            VALE_I = 1),
                            ),
                 NOM_PARA='K1',
                 VALE=1.595E+6,
                 PRECISION=0.07,
                 REFERENCE='ANALYTIQUE'      )
                 
TEST_TABLE(      TABLE=SIFK1,
                 FILTRE=(_F(NOM_PARA = 'PT_FOND',
                            VALE_I = 23),
                            ),
                 NOM_PARA='K1',
                 VALE=1.595E+6,
                 PRECISION=10.E-2  ,
                 REFERENCE='ANALYTIQUE'      )


TEST_TABLE(      TABLE=SIFK2,
                 FILTRE=(_F(NOM_PARA = 'PT_FOND',
                            VALE_I = 5),
                            ),
                 NOM_PARA='K1',
                 VALE=1.595E+6,
                 PRECISION=0.03,
                 REFERENCE='ANALYTIQUE'      )
                 


FIN();

