# METHODE X-FEM (FISSURE NON MAILLEE)

DEBUT(   CODE=_F( NOM = 'SSLV134H',NIV_PUB_WEB='INTERNET'))

PRE_GIBI();
MA1=LIRE_MAILLAGE(INFO=1,);

MA1=DEFI_GROUP(reuse =MA1,
                    MAILLAGE=MA1,
                    CREA_GROUP_NO=(_F(GROUP_MA='PEAUEXT',),
                                   _F(GROUP_MA='PEAUINT',), 
                                   _F(GROUP_MA='EXTUBE_S',),
                                   _F(GROUP_MA='EXTUBE_I',), 
                                   _F(GROUP_MA='BORD1',), 
                                   _F(GROUP_MA='BORD2',), 
                                ),
                                    );
  

MAT=DEFI_MATERIAU( ELAS=_F(  E = 210000.,  NU = 0.3, RHO = 0.,
                            ALPHA = 0.))
                            
MODELEIN=AFFE_MODELE(MAILLAGE=MA1,
                     INFO = 1,
                   AFFE=(_F(GROUP_MA=('TUBE',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                         _F(GROUP_MA=('EXTUBE_S','EXTUBE_I',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                  ),);        

MA1=MODI_MAILLAGE(reuse =MA1,
                       MAILLAGE=MA1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('EXTUBE_S','EXTUBE_I',),),
                 );

# PARAMETRES DE LA FISSURE
x0 = 8.18
y0 = 0.
z0 = 2.05
ray = 2.
yi = y0 + ray 
FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(
                                 FORM_FISS      = 'ELLIPSE',
                                 DEMI_GRAND_AXE = ray,
                                 DEMI_PETIT_AXE = ray,
                                 CENTRE         = (x0, y0, z0),
                                 VECT_X         = (0. , 1. , 0. ),
                                 VECT_Y         = (1., 0. , 0. ),
                                 ),
                    GROUP_MA_ENRI='TUBE',
                   # GROUP_MA_ENRI='BLOC',
                    RAYON_ENRI=0.,
                    ORIE_FOND=_F(PFON_INI  = (x0, yi, z0),
                                 VECT_ORIE = (0. , 0. , 1. ),
                                 PT_ORIGIN = (x0, y0, z0),),
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         INFO=1,
                         );

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MA1,
                       MODELE=MODELEK,
                       AFFE=(_F(TOUT = 'OUI',
                                MATER=MAT,),
                             ),);
CHAR1=AFFE_CHAR_MECA(INFO=1,
                      MODELE=MODELEK,
                     DDL_IMPO=(
                              _F(GROUP_NO='PEAUEXT',
                                       DX=0.0,),   
                              _F(GROUP_NO='EXTUBE_I',
                                       DZ=0.0,),
                              _F(GROUP_NO='BORD1',
                                       DY=0.0,),
                                        ), );   

CHAR2=AFFE_CHAR_MECA(INFO=1,
                      MODELE=MODELEK,
                      PRES_REP=(_F(GROUP_MA='EXTUBE_S',
                                    PRES=-1.0,), 
                      ),   );
                      
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(INFO=1,
                  MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CHAR1,),
                          _F(CHARGE=CHAR2,),
                          _F(CHARGE=CHXFEM,),
                           ),              
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='TUBE',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                      SOLVEUR=_F(METHODE='MUMPS',),
                   );

#MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK)

#DEPL=POST_CHAM_XFEM(MAILLAGE=MA_XFEM,
#                    MODELE=MODELEK,
#                    NOM_CHAM='DEPL',
#                    RESULTAT=UTOT1)


SIFTH=CALC_G(  RESULTAT=UTOT1,
                  SYME_CHAR='SANS',
                  COMP_ELAS=_F(RELATION='ELAS',),
                  OPTION='CALC_K_G',
                  LISSAGE = _F(LISSAGE_THETA='LEGENDRE',
                           DEGRE = 5,
                           LISSAGE_G='LEGENDRE',),
                  THETA = _F(R_INF=0.2,
                        R_SUP=0.8,
                        FISSURE=FISS,),
                  INFO=1,);
IMPR_TABLE(TABLE=SIFTH,);

SIFK1=POST_K1_K2_K3(  MODELISATION='3D',
                        FISSURE=FISS,
                        INFO=1,
                        MAILLAGE=MA1,
                        MATER=MAT,
                        RESULTAT=UTOT1,
                        ABSC_CURV_MAXI=0.5,
                        VECT_K1=(0., 0., 1.,),
                        DTAN_ORIG = (0.,1.,0.),
                        DTAN_EXTR = (-1.,0.,0.),
                        
                      )
IMPR_TABLE(TABLE=SIFK1,);
                    
#----------------------------------------------------------------------
#              TEST DE K1 EN 1 POINT
#----------------------------------------------------------------------

TEST_TABLE(      TABLE=SIFTH,
                   TYPE_TEST='MAX',
                 NOM_PARA='K1_LOCAL',
                      VALE=1.595E+00,
                 PRECISION=3.E-1  ,
                      REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=SIFTH,
                   TYPE_TEST='MIN',
                 NOM_PARA='K1_LOCAL',
                      VALE=1.595E+00,
                 PRECISION=5.E-2  ,
                      REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=SIFTH,
                   TYPE_TEST='MIN',
                 NOM_PARA='K1_LOCAL',
                      VALE=1.53472E+00,
                 PRECISION=1.E-3  ,
                      REFERENCE='NON_REGRESSION'      )

TEST_TABLE(      TABLE=SIFK1,
                 FILTRE=(_F(  NOM_PARA = 'PT_FOND',
                            VALE_I = 1), 
                            _F(NOM_PARA = 'METHODE',
                            VALE_I = 3),),
                 NOM_PARA='K1_MAX',
                 VALE=1.595E+00,
                 PRECISION=10.E-2  ,
                 REFERENCE='ANALYTIQUE'      )
                 
TEST_TABLE(      TABLE=SIFK1,
                 FILTRE=(_F(  NOM_PARA = 'PT_FOND',
                            VALE_I = 24), 
                            _F(NOM_PARA = 'METHODE',
                            VALE_I = 3),),
                 NOM_PARA='K1_MAX',
                 VALE=1.595E+00,
                 PRECISION=10.E-2  ,
                 REFERENCE='ANALYTIQUE'      )

FIN();

