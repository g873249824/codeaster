DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='OUI'))
TOTO=LIRE_MAILLAGE(FORMAT='MED',INFO=2,);
TOTO=DEFI_GROUP(reuse =TOTO,
                MAILLAGE=TOTO,
                CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MAT1=DEFI_MATERIAU(NON_LOCAL=_F(LONG_CARA=3.0**0.5,),
                   BETON_ECRO_LINE=_F(D_SIGM_EPSI=-2000.0,
                                SYT=3.0,),
                   ELAS=_F(NU=0.1,
                           E=10000,),);

CHMAT=AFFE_MATERIAU(AFFE=_F(MATER=MAT1,
                            TOUT='OUI',),
                    MAILLAGE=TOTO,);

MO=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                                           PHENOMENE='MECANIQUE',
                                           MODELISATION='D_PLAN_GRAD_EPSI',),
                               MAILLAGE=TOTO,
                               INFO=2,);


CL1=AFFE_CHAR_MECA(DDL_IMPO=_F(DY=0,
                               GROUP_NO='GMA7',),
                   MODELE=MO,);

CL2=AFFE_CHAR_MECA(DDL_IMPO=(_F(DX=0,
                               GROUP_NO='GMA8',),
                             ),
                   MODELE=MO,);

TRACTION=AFFE_CHAR_MECA(FORCE_CONTOUR=_F(FY=1.0,
                                    GROUP_MA='GMA9',),
                        MODELE=MO,);

INSTANTS=DEFI_LIST_REEL(INTERVALLE=(_F(JUSQU_A=0.0,
                                      NOMBRE=1,),
                                    _F(JUSQU_A=10,
                                      NOMBRE=100,),),
                        DEBUT=-1.,);

FONCMULT=DEFI_FONCTION(NOM_PARA='INST',
                       VALE=(-1.,0,0,0.005),);

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =INSTANTS ),
                        ECHEC=(_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_NIVEAU = 10,
                                 SUBD_PAS=4,
                                 SUBD_PAS_MINI=1e-6,),
                              _F(EVENEMENT = 'DIVE_RESI'),)
                        )

EVOL1=STAT_NON_LINE(CHAM_MATER=CHMAT,
                    MODELE=MO,
                    CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                   ITER_GLOB_ELAS=25,
                                   RESI_GLOB_RELA=1.E-06,),
                    COMPORTEMENT=_F(RELATION='ENDO_ISOT_BETON',),
                    INCREMENT=_F(LIST_INST=DEFLIST,
                                 INST_FIN=0.,),
                    NEWTON=_F(PREDICTION='TANGENTE',
                              REAC_ITER=1,
                              PAS_MINI_ELAS=1.E-03,
                              MATRICE='TANGENTE',
                              REAC_INCR=1,),
                    EXCIT=(_F(CHARGE=TRACTION,
                              FONC_MULT=FONCMULT,),
                           _F(CHARGE=CL1,),
                           _F(CHARGE=CL2,),),);

EVOL1=STAT_NON_LINE(reuse=EVOL1,
                     MODELE=MO,
                       CHAM_MATER=CHMAT,
                    EXCIT=(_F(CHARGE=TRACTION,
                              TYPE_CHARGE='FIXE_PILO',),
                           _F(CHARGE=CL1,),
                           _F(CHARGE=CL2,),),
                    COMPORTEMENT=_F(RELATION='ENDO_ISOT_BETON',),
                    ETAT_INIT=_F(EVOL_NOLI=EVOL1,),
                    INCREMENT=_F(LIST_INST=DEFLIST,),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=10,
                                   ITER_GLOB_ELAS=100,
                                   RESI_GLOB_RELA=1.E-06,),
                    NEWTON=_F(PREDICTION='TANGENTE',
                              REAC_ITER=1,
                              PAS_MINI_ELAS=1.E-04,
                              MATRICE='TANGENTE',
                              REAC_INCR=1,),
                       PILOTAGE=_F(TYPE='PRED_ELAS',
                                 COEF_MULT=3.,
                                 SELECTION='RESIDU',
                                 GROUP_MA = ('GMA10',),
                                 ETA_PILO_R_MIN=0.,
                                 ETA_PILO_MIN=0.4,
                                 ETA_PILO_R_MAX=10),
                       );

TEST_RESU(RESU=(_F(NUME_ORDRE=33,
                   RESULTAT=EVOL1,
                   NOM_CHAM='DEPL',
                   NOEUD='N2',
                   NOM_CMP='DY',
                   VALE_CALC=7.02307415944546E-03,),
                _F(NUME_ORDRE=33,
                   POINT=2,
                   RESULTAT=EVOL1,
                   NOM_CHAM='VARI_ELGA',
                   NOM_CMP='V1',
                   VALE_CALC=0.956942781428,
                   MAILLE='M31',),
                ),
          )

FIN();
