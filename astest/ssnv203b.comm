DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
              
# CETTE MODELISATION TESTE L'APPLICATION D'UNE PRESSION (CONSTANTE ET AFFINE) 
# SUR LES LEVRES D'UNE FISSURE X-FEM EN 3D (EN FAIT, C'EST PLUTOT UNE INTERFACE)

#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------
# precision
n = 1000.0;

# dimensions de la structure
lx = 1.0;

ly = 1.0;

MAILLAG2=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='VOL'),
                                   _F(GROUP_MA='SURFSUP'),
                                   _F(GROUP_MA='SURFINF'),),
                    INFO=1,);

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MO=AFFE_MODELE(MAILLAGE=MAILLAG2,
               AFFE=_F(GROUP_MA=('VOL',),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-0.5');

FISS=DEFI_FISS_XFEM(MAILLAGE=MAILLAG2,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO,
                         FISSURE=FISS,
                         INFO=1,);


#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
E = 10000000000.0;

nu = 0.;

rho = 7800.0;


ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=rho,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MO,
                       AFFE=_F(GROUP_MA='VOL',
                               MATER=ACIER,
                               ),);
#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                  DDL_IMPO=(_F(GROUP_NO='SURFSUP',DX=0.,DY=0.,DZ=0.),
                            _F(GROUP_NO='SURFINF',DX=0.,DY=0.,DZ=0.),),
                  INFO=1,);


# pour tester option CHAR_MECA_PRES_R
CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   PRES_REP=_F(FISSURE=FISS,PRES=10000.)
                   )

# pour tester option CHAR_MECA_PRES_F
FCTPRES = FORMULE(VALE='Z*20000.',
                  NOM_PARA=('Z',),);

CH2=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     PRES_REP=_F(FISSURE=FISS,PRES=FCTPRES)
                   )

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=3.0,
                                   NOMBRE=3,),);

UTOT1=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(
                          _F(CHARGE=CH),
                          _F(CHARGE=CH1),
                          ),
                   INFO=1,
                   ); 

UTOT2=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(
                          _F(CHARGE=CH),
                          _F(CHARGE=CH2),
                          ),
                   INFO=1,
                   ); 


MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK)

#IMPR_RESU(FORMAT='RESULTAT',RESU=_F(MAILLAGE = MA_XFEM,))
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(MAILLAGE = MA_XFEM,))



MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),
                              );  

RES_X1=POST_CHAM_XFEM( MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        )

RES_X2=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT2,
                        )


IMPR_RESU(FORMAT='GMSH',UNITE=82,RESU=_F(RESULTAT=RES_X1,));

# -------------------------------------------------------------   
#            TEST_TABLE
# -------------------------------------------------------------   

DEP1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU',),
                                RESULTAT=RES_X1,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);

DEP2=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU',),
                                RESULTAT=RES_X2,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);


IMPR_TABLE(TABLE=DEP1,);
IMPR_TABLE(TABLE=DEP2,);


DEP1P = CALC_TABLE(TABLE = DEP1,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = '^N[M]',),),);


DEP1M = CALC_TABLE(TABLE = DEP1,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = '^N[P]',),),);


DEP2P = CALC_TABLE(TABLE = DEP2,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = '^N[M]',),),);


DEP2M = CALC_TABLE(TABLE = DEP2,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = '^N[P]',),),);

# POUR LE CHARGEMENT 1

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=-5E-07,
           VALE_REFE=-4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           TABLE=DEP1P,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=-5E-07,
           VALE_REFE=-4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           TABLE=DEP1P,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=5E-07,
           VALE_REFE=4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           TABLE=DEP1M,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=5E-07,
           VALE_REFE=4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           TABLE=DEP1M,)

# POUR LE CHARGEMENT 2

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=-5E-07,
           VALE_REFE=-4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           TABLE=DEP2P,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=-5E-07,
           VALE_REFE=-4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           TABLE=DEP2P,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=5E-07,
           VALE_REFE=4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           TABLE=DEP2M,)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999994E-12,
           VALE_CALC=5E-07,
           VALE_REFE=4.9999999999999998E-07,
           NOM_PARA='DZ',
           TYPE_TEST='MAX',
           TABLE=DEP2M,)

FIN();
