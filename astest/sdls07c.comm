        )

#......................................................................
# MODELISATION COQUE_AXIS:   41 NOEUDS -- 20 MAILLES SEG3 ; KAPPA=1.E6
#              LOVE_KIRCHHOFF
#......................................................................



MAILL=LIRE_MAILLAGE(   )

MAILL=DEFI_GROUP( reuse=MAILL,   MAILLAGE=MAILL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MATER=DEFI_MATERIAU( ELAS=_F(  E = 2.E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAILL,
                            AFFE=_F( TOUT = 'OUI',   MATER = MATER) )

MODEL=AFFE_MODELE(    MAILLAGE=MAILL,
                            AFFE=_F( TOUT = 'OUI',   MODELISATION = 'COQUE_AXIS',
                            PHENOMENE = 'MECANIQUE') )

CARELEM=AFFE_CARA_ELEM(  MODELE=MODEL,
                   COQUE=_F( 
       GROUP_MA = 'TOUT',
 EPAIS = 0.10,
                        A_CIS = 1.E2,  MODI_METRIQUE = 'NON') )

RIGELEM=CALC_MATR_ELEM(   MODELE=MODEL,
                                 CARA_ELEM=CARELEM,  CHAM_MATER=CHMAT,
                                 OPTION='RIGI_MECA' )

MASELEM=CALC_MATR_ELEM(   MODELE=MODEL,
                                 CARA_ELEM=CARELEM,  CHAM_MATER=CHMAT,
                                OPTION='MASS_MECA' )

NUM=NUME_DDL(   MATR_RIGI=RIGELEM,
                METHODE  ='MULT_FRONT',
                RENUM    ='MDA' ),

RIGASS=ASSE_MATRICE(   MATR_ELEM=RIGELEM,   NUME_DDL=NUM   )

MASASS=ASSE_MATRICE(   MATR_ELEM=MASELEM,   NUME_DDL=NUM   )

MODVIB=MODE_ITER_SIMULT(  MATR_A=RIGASS,
                          MATR_B=MASASS,
                          CALC_FREQ=_F( OPTION = 'BANDE',  FREQ = (220., 540.,),)
                       )

MODITI=MODE_ITER_INV(     MATR_A=RIGASS,  MATR_B=MASASS,
          CALC_FREQ=_F( NMAX_FREQ = 12, OPTION = 'AJUSTE', FREQ = (220., 540.,),
                     NMAX_ITER_AJUSTE = 20),
          CALC_MODE=_F( NMAX_ITER = 50)
            )

TEST_RESU(RESU=(
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 1, VALE = 237.25),
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
