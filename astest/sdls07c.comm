                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 3, VALE = 305.24),
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 4, VALE = 324.17),
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 5, VALE = 346.76),
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 6, VALE = 376.68),
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 7, VALE = 416.00),
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 8, VALE = 465.75),
                 _F(   RESULTAT = MODVIB,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 9, VALE = 526.20))
                                                     )

TEST_RESU(RESU=(
                 _F(   RESULTAT = MODITI,PARA = 'FREQ',PRECISION = 1.E-2,
                         NUME_ORDRE = 1, VALE = 237.25),
                 _F(   RESULTAT = MODITI,PARA = 'FREQ',PRECISION = 1.E-2,
