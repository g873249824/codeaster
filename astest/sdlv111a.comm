MRA=CALC_MATR_ELEM(
                MODELE=MO,
                OPTION='RIGI_MECA',
            CHAM_MATER=AMA,
                CHARGE=CHA,
             CARA_ELEM=ACA
                       )

MRB=CALC_MATR_ELEM(
                MODELE=MO,
                OPTION='RIGI_MECA',
            CHAM_MATER=AMB,
                CHARGE=CHB,
             CARA_ELEM=ACB
                       )

MMA=CALC_MATR_ELEM(
                MODELE=MO,
                OPTION='MASS_MECA',
            CHAM_MATER=AMA,
                CHARGE=CHA,
             CARA_ELEM=ACA
                       )

MMB=CALC_MATR_ELEM(
                MODELE=MO,
                OPTION='MASS_MECA',
            CHAM_MATER=AMB,
                CHARGE=CHB,
             CARA_ELEM=ACB
                       )

NUMA=NUME_DDL(
            MATR_RIGI=MRA
                 )

NUMB=NUME_DDL(
            MATR_RIGI=MRB,
                 RENUM='MD'
                 )

MASRA=ASSE_MATRICE(  MATR_ELEM=MRA,    NUME_DDL=NUMA  )

MASMA=ASSE_MATRICE(  MATR_ELEM=MMA,    NUME_DDL=NUMA  )

MASRB=ASSE_MATRICE(  MATR_ELEM=MRB,    NUME_DDL=NUMB  )

MASMB=ASSE_MATRICE(  MATR_ELEM=MMB,    NUME_DDL=NUMB  )

RESA=MODE_ITER_SIMULT(
                MATR_RIGI=MASRA,
                MATR_MASS=MASMA,
                METHODE='JACOBI',
                NMAX_ITER_BATHE = 35,
                OPTION='SANS',
                CALC_FREQ=_F(  OPTION = 'BANDE',
                               FREQ = ( 3.0,  9.3, ),
                               SEUIL_FREQ = 0.,),
                VERI_MODE=_F(SEUIL=4.E-6),
                     )


RESB=MODE_ITER_SIMULT(
                MATR_RIGI=MASRB,
                MATR_MASS=MASMB,
                OPTION='SANS',
                CALC_FREQ=_F(  OPTION = 'BANDE',
                               FREQ = ( 0.5,  3.8, ),
                               SEUIL_FREQ = 0.)
                      )


