MAIL=LIRE_MAILLAGE(FORMAT='MED',INFO=2)

MAIL=DEFI_GROUP(reuse=MAIL,MAILLAGE=MAIL,
                 CREA_GROUP_NO=(_F(OPTION='NOEUD_ORDO',
                                   NOM='AB_ordo',
                                   GROUP_MA='AB',
                                   GROUP_NO_ORIG='A')))

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI', PHENOMENE='MECANIQUE', MODELISATION='DKT',),
                  );

# DEFINITION DES FONCTIONS DE TEMPERATURE
# ---------------------------------------

T_SUP = FORMULE(NOM_PARA=('X','Y'),VALE=' 100-5*abs(X)-5*abs(Y)+0.5*abs(X*Y)')
T_MOY = FORMULE(NOM_PARA=('X','Y'),VALE='0.')
T_INF = FORMULE(NOM_PARA=('X','Y'),VALE='-100+5*abs(X)+5*abs(Y)-0.5*abs(X*Y)')


# CONSTRUCTION DES CHAMPS DE GEOMETRIE
# ------------------------------------

CH_GEOM=CREA_CHAMP(OPERATION='EXTR',
                   TYPE_CHAM='NOEU_GEOM_R',
                   NOM_CHAM='GEOMETRIE',
                   MAILLAGE=MAIL,
                  )

#------------------------------------------------

TEMP_S=CREA_CHAMP(OPERATION='AFFE',
                 TYPE_CHAM='NOEU_NEUT_F',
                 MAILLAGE=MAIL,
                 AFFE=_F( TOUT = 'OUI',
                          NOM_CMP = ('X1',),
                          VALE_F = (T_SUP,)
                         )
                )

TEMP_S0= CREA_CHAMP(OPERATION='EVAL',
                 TYPE_CHAM='NOEU_NEUT_R',
                 CHAM_F=TEMP_S,
                 CHAM_PARA=(CH_GEOM)
                 )

#------------------------------------------------

TEMP_M=CREA_CHAMP(OPERATION='AFFE',
                 TYPE_CHAM='NOEU_NEUT_F',
                 MAILLAGE=MAIL,
                 AFFE=_F( TOUT = 'OUI',
