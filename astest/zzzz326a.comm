DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI',),);

MA=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='DKT',),);

EP=AFFE_CARA_ELEM(MODELE=MO,
                  COQUE=(_F(GROUP_MA='GR1',
                            EPAIS=0.002,COQUE_NCOU=2,
                            ANGL_REP=(0.0,0.0,),),
                         _F(GROUP_MA='GR2',
                            EPAIS=0.003,COQUE_NCOU=3,
                            ANGL_REP=(0.0,0.0,),),),);

MAT1A=DEFI_MATERIAU(ELAS=_F(E=6800.0,
                            NU=0.35,),);

CHMUL1=AFFE_MATERIAU(MAILLAGE=MA,
                     AFFE=_F(TOUT='OUI',
                             MATER=MAT1A,),);

CHME=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(NOEUD='N1',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,
                                 DRX=0.0,
                                 DRY=0.0,
                                 DRZ=0.0,),
                              _F(NOEUD='N4',
                                 DX=0.0707099974155,
                                 DY=0.0707099974155,),),);

STA1=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHMUL1,
                   CARA_ELEM=EP,
                   EXCIT=_F(CHARGE=CHME,),);

IMPR_RESU(FORMAT='MED',
          RESU=_F(RESULTAT=STA1,
                  CARA_ELEM=EP,),);

IMPR_RESU(FORMAT='MED',
          UNITE=81,
          RESU=_F(RESULTAT=STA1,),);

# "DUMMY" TEST RESU
TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   REFERENCE='NON_DEFINI',
                   RESULTAT=STA1,
                   NOM_CHAM='DEPL',
                   NOEUD='N4',
                   NOM_CMP='DY',
                   VALE_CALC=0.0707099974155,
                   VALE_REFE=0.0707099974155,
                   PRECISION=5.0000000000000001E-3,),),);

FIN();
