           VALE_REFE=9.286E+06,
           TOLE_MACHINE=1.E-10,
           NOM_PARA='TENSION',
           TABLE=tab_eve0,
           FILTRE=(_F(NOM_PARA='NOM_CABLE',VALE_K='CAB2'), 
                   _F(NOM_PARA='ABSC_CURV', VALE=0.)))
# A LA LIMITE DU RECUL D'ANCRAGE 
# (ATTENTION ABSC_CURV PAS PARFAITEMENT COINCIDENTE)

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.5E-2,
           VALE_CALC=1.0079408711746E+07,
           VALE_REFE=10.079E+06,
           TOLE_MACHINE=1.E-10,
           NOM_PARA='TENSION',
           TABLE=tab_eve0,
           FILTRE=(_F(NOM_PARA='NOM_CABLE',VALE_K='CAB2'), 
                   _F(NOM_PARA='ABSC_CURV', VALE=9.200)))
# AU MILIEU DU CABLE                   
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.5E-2,
           VALE_CALC=6.2195728853276E+06,
           VALE_REFE=6.210E+06,
           TOLE_MACHINE=1.E-10,
           NOM_PARA='TENSION',
           TABLE=tab_eve0,
           FILTRE=(_F(NOM_PARA='NOM_CABLE',VALE_K='CAB2'), 
                   _F(NOM_PARA='ABSC_CURV', VALE=64.403)))


FIN();


