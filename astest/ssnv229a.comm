           VALE_CALC=1.0458482047476E+07,
           VALE_REFE=10.436E+06,
           TOLE_MACHINE=1.E-10,
           NOM_PARA='TENSION',
           TABLE=tab_odd0,
           FILTRE=(_F(NOM_PARA='NOM_CABLE',VALE_K='CAB1'), 
                   _F(NOM_PARA='ABSC_CURV', VALE=9.200)))
# AU MILIEU DU CABLE 
#(SANS RELAXATION ACIER)                  
TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.5E-2,
           VALE_CALC=6.2841275093704E+06,
           VALE_REFE=6.273E+06,
           TOLE_MACHINE=1.E-10,
           NOM_PARA='TENSION',
           TABLE=tab_odd0,
