                                   MODULE = 1.,
                                   R_INF = 0.9,
                                   R_SUP = 1.5),
                                     DIRECTION=(1., 0., 0.,)
                     )

G_0=CALC_G( THETA=_F(THETA=THETA0),
            RESULTAT=CHAMDEP,
            TOUT_ORDRE='OUI',SYME_CHAR='SYME',
            COMP_ELAS=_F(RELATION='ELAS_VMIS_LINE',),
            );

IMPR_TABLE(TABLE=G_0,);

