DEBUT(CODE=_F(NOM='SSLL110A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAILLAGE=LIRE_MAILLAGE();

ACIER=DEFI_MATERIAU(ELAS=_F(NU=0.3,
                            RHO=8000.0,
                            E=2.E11,),);

PLUME=DEFI_MATERIAU(ELAS=_F(NU=0.3,
                            RHO=0.0,
                            E=2.E11,),);

CH_MATER=AFFE_MATERIAU(AFFE=(_F(MATER=ACIER,
                                GROUP_MA='PESA',),
                             _F(MATER=PLUME,
                                GROUP_MA='VERT',),),
                       MAILLAGE=MAILLAGE,);

MODELE=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='BARRE',),
                   MAILLAGE=MAILLAGE,);

CARA_ELE=AFFE_CARA_ELEM(MODELE=MODELE,
                        BARRE=_F(SECTION='GENERALE',
                                 GROUP_MA=('PESA','VERT'),
                                 CARA='A',
                                 VALE=1.0,),);

CHARGE_1=AFFE_CHAR_MECA(DDL_IMPO=(_F(TOUT='OUI',
                                     DZ=0.0,
                                     DX=0.0,),
                                  _F(DX=0.0,
                                     DY=0.0,
                                     GROUP_NO='A',),
                                  _F(DX=0.0,
                                     DY=0.0,
                                     GROUP_NO='B',),),
                        MODELE=MODELE,
                        PESANTEUR=_F(GRAVITE=20.0,
                                     DIRECTION=(0.866,-0.5,0.0),),);

STATIQU1=MECA_STATIQUE(CHAM_MATER=CH_MATER,
                       MODELE=MODELE,
                       CARA_ELEM=CARA_ELE,
                       EXCIT=_F(CHARGE=CHARGE_1,),);


STATIQU1=CALC_ELEM(reuse =STATIQU1,
                   RESULTAT=STATIQU1,
                   OPTION=('EFGE_ELNO','EPSI_ELNO'),
                   );

TEST_RESU(RESU=_F(NOM_CHAM='EFGE_ELNO',
                  RESULTAT=STATIQU1,
                  MAILLE='M1',
                  REFERENCE='ANALYTIQUE',
                  VALE=4.E5,
                  NOM_CMP='N',
                  NUME_ORDRE=1,
                  NOEUD='N1',),);

TEST_RESU(RESU=_F(NOM_CHAM='EFGE_ELNO',
                  RESULTAT=STATIQU1,
                  MAILLE='M3',
                  REFERENCE='ANALYTIQUE',
                  VALE=4.E5,
                  NOM_CMP='N',
                  NUME_ORDRE=1,
                  NOEUD='N4',),);

TEST_RESU(RESU=_F(NOM_CHAM='EPSI_ELNO',
                  RESULTAT=STATIQU1,
                  MAILLE='M1',
                  REFERENCE='ANALYTIQUE',
                  VALE=2.E-6,
                  NOM_CMP='EPXX',
                  NUME_ORDRE=1,
                  NOEUD='N1',),);

TEST_RESU(RESU=_F(NOM_CHAM='EPSI_ELNO',
                  RESULTAT=STATIQU1,
                  MAILLE='M3',
                  REFERENCE='ANALYTIQUE',
                  VALE=2.E-6,
                  NOM_CMP='EPXX',
                  NUME_ORDRE=1,
                  NOEUD='N4',),);

TEST_RESU(RESU=_F(NOM_CHAM='DEPL',
                  RESULTAT=STATIQU1,
                  REFERENCE='ANALYTIQUE',
                  VALE=-2.E-5,
                  NOM_CMP='DY',
                  NUME_ORDRE=1,
                  NOEUD='N2',),);
TEST_RESU(RESU=_F(NOM_CHAM='DEPL',
                  RESULTAT=STATIQU1,
                  REFERENCE='ANALYTIQUE',
                  VALE=-2.E-5,
                  NOM_CMP='DY',
                  NUME_ORDRE=1,
                  NOEUD='N3',),);

# Validation de CREA_CHAMP / DISC : ELGA -> NOEU :
#---------------------------------------------------
XX1=CREA_CHAMP(OPERATION='EXTR', TYPE_CHAM='ELGA_SIEF_R',
                    NOM_CHAM='SIEF_ELGA', RESULTAT=STATIQU1, INST=0.)

XX2=CREA_CHAMP(OPERATION='DISC', TYPE_CHAM='NOEU_SIEF_R',  INFO=2,
                     MODELE=MODELE,   CHAM_GD=XX1)

TEST_RESU(CHAM_NO=(
  _F( CHAM_GD = XX2, NOEUD = 'N3',  NOM_CMP = 'N', VALE = 2.0000440014521E+05 , PRECISION = 1.E-8 ,REFERENCE='NON_REGRESSION',),
  _F( CHAM_GD = XX2, NOEUD = 'N4',  NOM_CMP = 'N', VALE = 4.0000880029041E+05 , PRECISION = 1.E-8 ,REFERENCE='NON_REGRESSION',),
))

FIN();

