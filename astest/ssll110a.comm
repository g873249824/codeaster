DEBUT(CODE=_F(NOM='SSLL110A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAILLAGE=LIRE_MAILLAGE();

ACIER=DEFI_MATERIAU(ELAS=_F(NU=0.3,
                            RHO=8000.0,
                            E=2.E11,),);

PLUME=DEFI_MATERIAU(ELAS=_F(NU=0.3,
                            RHO=0.0,
                            E=2.E11,),);

CH_MATER=AFFE_MATERIAU(AFFE=(_F(MATER=ACIER,
                                GROUP_MA='PESA',),
                             _F(MATER=PLUME,
                                GROUP_MA='VERT',),),
                       MAILLAGE=MAILLAGE,);

MODELE=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='BARRE',),
                   MAILLAGE=MAILLAGE,);

CARA_ELE=AFFE_CARA_ELEM(MODELE=MODELE,
                        BARRE=_F(SECTION='GENERALE',
                                 GROUP_MA=('PESA','VERT'),
                                 CARA='A',
                                 VALE=1.0,),);

CHARGE_1=AFFE_CHAR_MECA(DDL_IMPO=(_F(TOUT='OUI',
                                     DZ=0.0,
                                     DX=0.0,),
                                  _F(DX=0.0,
                                     DY=0.0,
                                     GROUP_NO='A',),
                                  _F(DX=0.0,
                                     DY=0.0,
                                     GROUP_NO='B',),),
                        MODELE=MODELE,
                        PESANTEUR=_F(GRAVITE=20.0,
                                     DIRECTION=(0.866,-0.5,0.0),),);

STATIQU1=MECA_STATIQUE(CHAM_MATER=CH_MATER,
                       MODELE=MODELE,
                       CARA_ELEM=CARA_ELE,
                       EXCIT=_F(CHARGE=CHARGE_1,),);


STATIQU1=CALC_CHAMP(reuse=STATIQU1,RESULTAT=STATIQU1,CONTRAINTE=('EFGE_ELNO'),DEFORMATION=('EPSI_ELNO'))


TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=STATIQU1,
                  NOM_CHAM='EFGE_ELNO',
                  NOEUD='N1',
                  NOM_CMP='N',
                  VALE_CALC= 4.00008800E+05,
                  VALE_REFE=4.E5,
                  MAILLE='M1',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=STATIQU1,
                  NOM_CHAM='EFGE_ELNO',
                  NOEUD='N4',
                  NOM_CMP='N',
                  VALE_CALC= 4.00008800E+05,
                  VALE_REFE=4.E5,
                  MAILLE='M3',),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=STATIQU1,
                  NOM_CHAM='EPSI_ELNO',
                  NOEUD='N1',
                  NOM_CMP='EPXX',
                  VALE_CALC= 2.00004400E-06,
