                CHAM_MATER=CHMATP,
                CARA_ELEM=CARA,
                CHARGE=SUPPORT,
                NUME_DDL=CO('NUMDDL'),
                MATR_ASSE=(_F(MATRICE=CO("MATRRIGI"),
                              OPTION='RIGI_MECA',),
                           _F(MATRICE=CO("MATRMASS"),
                              OPTION='MASS_MECA',),),);
                              
                              
#-----------------------------------------------------------------------
