               MATR_A=MATRRIGI,
               MATR_B=MATRMASS,
               CALC_FREQ=_F( 
               FREQ_MIN = 0.0,
               FREQ_MAX = 33.0,
               NB_BLOC_FREQ = 1),
               VERI_MODE=_F( STOP_ERREUR = 'NON'),
               NORM_MODE=_F( 
                     NORME = 'MASS_GENE',
                     ),
                 IMPRESSION=_F( TOUT_PARA = 'OUI')
                  )

