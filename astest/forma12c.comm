                  MASS_GENE=MASSEGEN,
                  RIGI_GENE=RIGIDGEN,
                  AMOR_REDUIT=0.02,
                  EXCIT=(
                  _F(
                     VECT_GENE=EFGENX,
                     FONC_MULT=ACCELH1,
                        ),
                  _F(
                     VECT_GENE=EFGENY,
                     FONC_MULT=ACCELH1,
                        ),      
