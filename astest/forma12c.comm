              _F( 
            INTITULE = 'DEPL_MAX_R',
            GROUP_NO='GNSTR01',                                         
            CHAM_GD=DPSCR,            
            TOUT_CMP='OUI',                            
            OPERATION ='EXTRACTION',                           
