                        VECTEUR=CO("EFGENX"),
                        VECT_ASSE=CHSEIX,
                        ),
                        _F(
                        VECTEUR=CO("EFGENY"),
                        VECT_ASSE=CHSEIY,
                        ),
                        _F(
                        VECTEUR=CO("EFGENZ"),
                        VECT_ASSE=CHSEIZ,
                        ),                        
