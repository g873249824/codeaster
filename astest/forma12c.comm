               BASE=MODES,
               MATR_ASSE_GENE=(
                        _F(
                        MATRICE=CO("MASSEGEN"),
                        MATR_ASSE=MATRMASS,
                        ),
                        _F(
                        MATRICE=CO("RIGIDGEN"),
                        MATR_ASSE=MATRRIGI,
                        ),        
               ),
               VECT_ASSE_GENE=(
