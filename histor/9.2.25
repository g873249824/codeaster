           IF (JEUF1.NE.' ') THEN
             CALL FOINTE ('F',JEUF1,3,NOMPAR,VALPAR,DIST1,IER)
           ENDIF
           IF (JEUF2.NE.' ') THEN
             CALL FOINTE ('F',JEUF2,3,NOMPAR,VALPAR,DIST2,IER)
           ENDIF
           DIST6 = DIST1+DIST2
           ZR(JJSUP+IZONE-1) = DIST6
   ENDIF
   
   Et, bien sur, corriger la syntaxe !
