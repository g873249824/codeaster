   AMOR_REDUIT=(2.0e-03,2.0e-03,2.0e-03,2.0e-03,2.0e-03,2.0e-03,2.0e-03,2.0e-
   03,),),
   TYPE_FLUI_STRU=TYPEFLUI,             IMPRESSION=_F(PARA_COUPLAGE='OUI',),);
   
