   THM, on supprime toute référence aux éléments linéaires de bords et de rigidité des
   modélisations THM suivantes :
   
   -> KIT_HM:   
      D_PLAN_HMS,   D_PLAN_HMD,  AXIS_HMS,    AXIS_HMD,   3D_HMS,   3D_HMD 
   -> KIT_THM:  
      D_PLAN_THMS,  D_PLAN_THMD, AXIS_THMS,   AXIS_THMD,  3D_THMS,  3D_THMD
   -> KIT_HHM:  
      D_PLAN_HHMS,  D_PLAN_HHMD, AXIS_HHMS,   AXIS_HHMD,  3D_HHMS,  3D_HHMD,  
      D_PLAN_HH2MD,AXIS_HH2MD,  3D_HH2MD
   -> KIT_THH:  
      D_PLAN_THHS,  D_PLAN_THHD, AXIS_THHS,   AXIS_THHD,  3D_THHS,  3D_THHD,  
      D_PLAN_THH2D,AXIS_THH2D,  3D_THH2D
   -> KIT_THV:  
      D_PLAN_THVD ,   AXIS_THVD,   3D_THVD
   -> KIT_THHM: 
      D_PLAN_THHMS, D_PLAN_THHMD, AXIS_THHMS, AXIS_THHMD, 3D_THHMS, 3D_THHMD, 
      D_PLAN_THH2MD,AXIS_THH2MD, 3D_THH2MD
   -> KIT_HH:   
      D_PLAN_HHS,   D_PLAN_HHD,   AXIS_HHS,   AXIS_HHD,   3D_HHS,   3D_HHD,   
      D_PLAN_HH2S,  D_PLAN_HH2D,  AXIS_HH2S, AXIS_HH2D, 3D_HH2S, 3D_HH2D,
   
   
