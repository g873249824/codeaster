   C --- MULTI-APPUIS
   C
         IF (LMUAP) THEN
           CALL NDYNKK(SDDYNA,'DEPENT',DEPENT)
           CALL NDYNKK(SDDYNA,'VITENT',VITENT)
           CALL NDYNKK(SDDYNA,'ACCENT',ACCENT)
