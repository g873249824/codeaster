           CALL NDYNKK(SDDYNA,'VITABS',VITABS)
           CALL NDYNKK(SDDYNA,'ACCABS',ACCABS)
           CALL VTCREB(DEPENT,NUMEDD,'V','R',NEQ)
           CALL VTCREB(VITENT,NUMEDD,'V','R',NEQ)
           CALL VTCREB(ACCENT,NUMEDD,'V','R',NEQ)
