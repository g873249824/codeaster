   [formule]
   ),),
   f  SYME_CHAR  =	 / 'SANS',	[DEFAUT]
   / 'SYME',
   / 'ANTI',
   
   
   # Comportement
   
   f  /  COMP_ELAS	 = _F (
   f   RELATION  =   / 'ELAS',            [DEFAUT]
   / 'ELAS_VMIS_LINE',
   / 'ELAS_VMIS_TRAC',
   f   DEFORMATION	= / 'PETIT',	       [DEFAUT]
   / 'GREEN',
   f  /  TOUT        = 'OUI',              [DEFAUT]
