   /  |  GROUP_MA = lgrma ,            [l_gr_maille]
   | MAILLE    = lma,               [l_maille]
   ),
   f   ETAT_INIT	=_F (
   o  /  |  DEPL  = dep ,                   [cham_no_DEPL_R]
   |  SIGM  = sig ,                   /[carte_SIEF_R]
   /[cham_elem_SIEF_R]
   ),
   # Methode de discretisation de theta en fond de fissure (3D local)
   
