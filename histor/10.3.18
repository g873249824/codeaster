         !    File "./Python/Macro/calc_europlexus_ops.py", line 138, in                  !
         ! calc_europlexus_ops                                                            !
         !     EPX.ecrire_fichier()                                                       !
         !    File "./Python/Macro/calc_europlexus_ops.py", line 1693, in ecrire_fichier  !
         !     eval('self.'+fct+'()')                                                     !
         !    File "<string>", line 0, in ?                                               !
         !    File "./Python/Macro/calc_europlexus_ops.py", line 621, in export_CARA_ELEM !
