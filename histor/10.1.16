                                      EVOL=THERLINE,
                                      VALE_REF=45.0,),);
   
   CL=AFFE_CHAR_MECA(MODELE=MODMECA,
                     DDL_IMPO=_F(GROUP_NO='TOP',
                                 DX=00,
                                 DY=00,
