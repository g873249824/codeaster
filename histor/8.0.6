                                 EXCENTREMENT=0.,));

    # On affecte l'orientation
      CARA_COQ=AFFE_CARA_ELEM(MODELE=MODEL,
                              INFO=1,
                              COQUE=_F(GROUP_MA='PLAQUE',
                              EPAIS=40.,
                              COEF_RIGI_DRZ=0.,),
                              GRILLE=LIST_GRI,
                              );

