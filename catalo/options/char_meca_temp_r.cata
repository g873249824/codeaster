        PPMILTO  N792_R   <<  PPMILTO : CHAMP SPECIFIQUE XFEM  >>
        PFISNO   NEUT_I   <<  PFISNO  : CHAMP SPECIFIQUE XFEM  >>
   OUT__
PVECTUR VDEP_R RESL__
PCONTRT SIEF_R ELGA__ <<  PCONTRT : CHAMP SPECIFIQUE XFEM  >>

COND_CALCUL__   PLUS__  PHENO='ME' BORD='0'
