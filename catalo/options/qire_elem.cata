QIRE_ELEM
       <<  QIRE_ELEM :
           ESTIMATEUR D ERREUR EN QUANTITE D INTERET
           PRODUIT UN CHAMP PAR ELEMENT  >>

OPTION__
   IN__
        PGEOMER  GEOM_R
       <<  PGEOMER : COORDONNEES DES NOEUDS >>
        
        PCONTNOP  SIEF_R
       <<  PCONTNOP : CONTRAINTES PB. PRIMAL INSTANT ACTUEL >>
        PCONTNOD  SIEF_R
       <<  PCONTNOD : CONTRAINTES PB. DUAL INSTANT ACTUEL >>
        PTEMPSR  INST_R
       <<  PTEMPSR :  INSTANT ACTUEL >>
        PVOISIN  VOISIN
       <<  PVOISIN : VOISINS DE L ELEMENT >>
        PFRVOLUP  FORC_R
       <<  PFRVOLUP : CHARGEMENT INTERNE DE TYPE VOLUMIQUE PB. PRIMAL>>
        PFRVOLUD  FORC_R
       <<  PFRVOLUD : CHARGEMENT INTERNE DE TYPE VOLUMIQUE PB. DUAL>>
        PPESANRP  PESA_R
       <<  PPESANRP : CHARGEMENT INTERNE DE TYPE PESANTEUR PB. PRIMAL>>
        PPESANRD  PESA_R
       <<  PPESANRD : CHARGEMENT INTERNE DE TYPE PESANTEUR PB. DUAL>>
        PROTATRP  ROTA_R
       <<  PROTATRP : CHARGEMENT INTERNE DE TYPE ROTATION PB. PRIMAL>>
        PROTATRD  ROTA_R
       <<  PROTATRD : CHARGEMENT INTERNE DE TYPE ROTATION PB. DUAL>>
        PFFVOLUP  FORC_F
       <<  PFFVOLUP : CHARGEMENT INTERNE DE TYPE VOLUMIQUE PB. PRIMAL (FONCTION)  >>
        PFFVOLUD  FORC_F
       <<  PFFVOLUD : CHARGEMENT INTERNE DE TYPE VOLUMIQUE PB. DUAL (FONCTION)  >>
        PFORCEP   NEUT_I
       <<  PFORCEP : CHARGEMENT DES BORDS DE TYPE FORCE PB. PRIMAL>>
        PFORCED   NEUT_I
       <<  PFORCED : CHARGEMENT DES BORDS DE TYPE FORCE PB. DUAL>>
        PPRESSP   NEUT_I
       <<  PPRESSP : CHARGEMENT DES BORDS DE TYPE PRESSION PB. PRIMAL >>
        PPRESSD   NEUT_I
       <<  PPRESSD : CHARGEMENT DES BORDS DE TYPE PRESSION PB. DUAL>>
        PCONSTR   NEUT_R
       <<  PCONSTR : CARTE CONSTANTE DU COEFFICIENT DE PONDERATION S>>
   OUT__
        PERREUR ERRE_R ELEM__
       <<  PERREUR : ESTIMATEUR D ERREUR  >>
COND_CALCUL__   PLUS__  PHENO='ME' BORD='0'
