        PMATUNS MDNS_R RESL__ << MATRICE RIGIDITE TANGENTE NON-SYMETRIQUE >>
        PVECTUR VDEP_R RESL__ << VECTEUR FORCES INTERNES + BT.LAMBDA >>
        PCONTPR SIEF_R ELGA__ << VECTEUR DES CONTRAINTES POUR T+ >>
        PVARIPR VARI_R ELGA__ << VARIABLES INTERNES POUR T+ >>
        PCODRET CODE_I ELEM__ << CODE RETOUR INTEGRATION COMPORTEMENT >>
        PCACO3D CACO3D ELEM__ << COQUE_3D (ROTATION FICTIVE AUTOUR DE LA NORMALE) >>
        PSTRXPR STRX_R ELGA__ <<  PSTRXPR : CHAMPS SPECIAL ELEMENTS DE STRUCTURE >>
