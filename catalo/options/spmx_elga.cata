SPMX_ELGA
       <<  SPMX_ELGA : CALCUL D'UNE VALEUR EXTREMALE D'UNE COMPOSANTE
           D'UN CHAMPS A SOUS-POINT. >>
OPTION__
   IN__
        PNBSP_I  NBSP_I 'CARA!.CANBSP'
       <<  PNBSP_I :  NOMBRE DE SOUS_POINTS >>
        PGEOMER  GEOM_R 'MAIL!.COORDO'
       <<  PGEOMER : COORDONNEES DES NOEUDS >>
        PCONTRR  SIEF_R
       <<  PCONTRR : CONTRAINTES >>
        PDEFORR  EPSI_R
       <<  PDEFORR : DEFORMATIONS >>
        PCONTEQ  SIEF_R 'RESU!SIEQ_ELGA!N'
       <<  CONTRAINTES EQUIVALENTES >>
        PDEFOEQ  EPSI_R
       <<  DEFORMATIONS EQUIVALENTES >>
        PNOMCMP  NEUT_K24 'VOLA!&&OP0058.ELGA_MAXI'
       <<  NOM DE LA COMPOSANTE CHOISIE >>
   OUT__
        PMINMAX  SPMA_R  ELGA__

