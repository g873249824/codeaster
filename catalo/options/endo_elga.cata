ENDO_ELGA
       <<  ENDO_ELGA : CALCUL DU TAUX DE TRIAXIALITE DES CONTRAINTES,
           DE LA CONTRAINTE EQUIVALENTE D'ENDOMMAGEMENT ET DE
           L'ENDOMMAGEMENT DE LEMAITRE-SERMAGE
           AUX POINTS DE GAUSS DE CHAQUE ELEMENT >>
OPTION__
   IN__
        PCONTGP  SIEF_R 'RESU!SIGM_ELGA!N'
       <<  PCONTGP : CONTRAINTES INSTANT ACTUEL  >>
        PVARCMR  VARI_R 'VOLA!&&CCPARA.VARI_INT_N'
       <<  PVARCMR : TEMPERATURES INSTANT PRECEDENT  >>
        PVARCPR  VARI_R 'VOLA!&&CCPARA.VARI_INT_NM1'
       <<  PVARCPR : TEMPERATURES INSTANT ACTUEL  >>
        PVARIMR  VARI_R 'RESU!VARI_ELGA!NM1'
       <<  PVARIMR : VARIABLES INTERNES INSTANT PRECEDENT  >>
        PVARIPR  VARI_R 'RESU!VARI_ELGA!N'
       <<  PVARIPR : VARIABLES INTERNES INSTANT ACTUEL  >>
        PMATERC  ADRSJEVE 'CHMA!.MATE_CODE'
       <<  PMATERC : CHAMP DE MATERIAU >>
        PTRIAGM  ENDO_R 'RESU!ENDO_ELGA!NM1'
       <<  PTRIAGM : TRIAXIALITE, CONTRAINTE ENDOMMAGEMENT,
           ET DOMMAGE DE LEMAITRE-SERMAGE >>
        PCOMPOR  COMPOR 'RESU!COMPORTEMENT!N'
       <<  PCOMPOR : COMPORTEMENT >>
   OUT__
        PTRIAPG ENDO_R ELGA__
       <<  PTRIAGP : TRIAXIALITE, CONTRAINTE ENDOMMAGEMENT,
           ET DOMMAGE DE LEMAITRE-SERMAGE INSTANT ACTUEL >>
COND_CALCUL__   PLUS__  PHENO='ME' BORD='0'
