D_DEPL_R_VI5


TYPE_ELEM__
ENTETE__ ELEMENT__ D_DEPL_R_VI5     MAILLE__ SEG3
   ELREFE__  V_SEG3
   ENS_NOEUD__  EN1     =     1
   ENS_NOEUD__  EN2     =     2  3

MODE_LOCAL__
    MALPHAR  = NEUT_R   ELEM__         (X1       )
    MDDLIMF  = DDLI_F   ELEM__         (C        )
    MDDLIMR  = DDLI_R   ELEM__         (C        )
    MDDLMUR  = DDLM_R   ELEM__         (A1       )
    DDL_MECA = DEPL_R   ELNO__ DIFF__
                                 EN1   (VI5      )
                                 EN2   (LAGR     )
    MGEOMER  = GEOM_R   ELNO__ DIFF__
                                 EN2   ()
                                 EN1   (X        Y        Z        )
    MTEMPSR  = INST_R   ELEM__         (INST     )

VECTEUR__
    MVECTUR = VDEP_R DDL_MECA

MATRICE__
    MMATUUR = MDEP_R DDL_MECA DDL_MECA

OPTION__
    MECA_BTLA_R         2     IN__   MDDLMUR  PDDLMUR  DDL_MECA PLAGRAR
                              OUT__  MVECTUR  PVECTUR
