D_DEPL_R_H1X


TYPE_ELEM__
ENTETE__ ELEMENT__ D_DEPL_R_H1X      MAILLE__ SEG3
   ELREFE__  V_SEG3
   ENS_NOEUD__  EN1     =     2  3
   ENS_NOEUD__  EN2     =     1

MODE_LOCAL__
    MALPHAR  = NEUT_R   ELEM__         (X1       )
    MDDLIMC  = DDLI_C   ELEM__         (C        )
    MDDLIMF  = DDLI_F   ELEM__         (C        )
    MDDLIMR  = DDLI_R   ELEM__         (C        )
    MDDLMUR  = DDLM_R   ELEM__         (A1       )
    NDEPLAC  = DEPL_C   ELNO__ DIFF__
                                 EN1   (LAGR     )
                                 EN2   (H1X       )
    DDL_MECA = DEPL_R   ELNO__ DIFF__
                                 EN1   (LAGR     )
                                 EN2   (H1X       )
    MGEOMER  = GEOM_R   ELNO__ DIFF__
                                 EN1   ()
                                 EN2   (X        Y        Z        )
    MTEMPSR  = INST_R   ELEM__         (INST     )

VECTEUR__
    MVECTUC = VDEP_C NDEPLAC
    MVECTUR = VDEP_R DDL_MECA

MATRICE__
    MMATUUR = MDEP_R DDL_MECA DDL_MECA

OPTION__
    MECA_BTLA_R        2  IN__  MDDLMUR  PDDLMUR  DDL_MECA PLAGRAR  
                          OUT__ MVECTUR  PVECTUR  
    MECA_BU_R          2  IN__  MALPHAR  PALPHAR  DDL_MECA PDDLIMR  MDDLMUR  PDDLMUR  
                          OUT__ MVECTUR  PVECTUR  
    MECA_DDLI_C        2  IN__  MDDLIMC  PDDLIMC  
                          OUT__ MVECTUC  PVECTUC  
    MECA_DDLI_F        2  IN__  MDDLIMF  PDDLIMF  MGEOMER  PGEOMER  MTEMPSR  PTEMPSR  
                          OUT__ MVECTUR  PVECTUR  
    MECA_DDLI_R        2  IN__  MDDLIMR  PDDLIMR  
                          OUT__ MVECTUR  PVECTUR  
    MECA_DDLM_R        2  IN__  MDDLMUR  PDDLMUR  
                          OUT__ MMATUUR  PMATUUR
