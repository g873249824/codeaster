GENER_MEDPL1_XH
TYPE_GENE__

%  CATALOGUES DES ELEMENTS 2D X-FEM HEAVISIDE DE BORD SANS CONTACT (LINEAIRES ET QUADRATIQUES)

ENTETE__ ELEMENT__ MEPLSE2_XH     MAILLE__ SEG2
   ELREFE__ SE2      GAUSS__  RIGI=FPG2
ENTETE__ ELEMENT__ MEPLSE3_XH     MAILLE__ SEG3
   ELREFE__ SE3      GAUSS__  RIGI=FPG4
   ELREFE__ SE2      GAUSS__  RIGI=FPG2

MODE_LOCAL__
    DDL_MECA = DEPL_R   ELNO__ IDEN__  (DX     DY      H1X     H1Y  )
    NTHETAR  = DEPL_R   ELNO__ IDEN__  (DX     DY )
    CFORCEF  = FORC_F   ELEM__         (FX       FY       )
    NFORCER  = FORC_R   ELNO__ IDEN__  (FX       FY       )

    NGEOMER  = GEOM_R   ELNO__ IDEN__  (X        Y       )
    EGTHETA  = G        ELEM__         (GTHETA   )
    EKTHETA  = G        ELEM__         (GTHETA   FIC1     FIC2     K1       K2       )
    CTEMPSR  = INST_R   ELEM__         (INST     )
    CPRESSF  = PRES_F   ELEM__         (PRES     CISA    )
    EPRESNO  = PRES_R   ELNO__ IDEN__  (PRES     CISA    )
    CPRES_R  = PRES_R   ELEM__         (PRES     CISA    )
    N1NEUT_R = NEUT_R   ELNO__ IDEN__  (X1       )
    E2NEUTI = N128_I    ELEM__         (X[2])
    E6NEUTI = N512_I    ELEM__         (X[6])
    E10NEUTI = N120_I    ELEM__         (X[10]     )
    N5NEUTI  = N120_I   ELNO__ IDEN__  (X[5]     )
    E8NEUTI = N120_I    ELEM__         (X[8])


    E4NEUTR = N480_R    ELEM__         (X[4])
    E6NEUTR = N132_R    ELEM__         (X[6])

    STANO_I  = N120_I   ELNO__ IDEN__  (X1       )

    ENNEUT_F = NEUT_F   ELNO__ IDEN__  (X[30]    )
    ENNEUT_R = NEUT_R   ELNO__ IDEN__  (X[30]    )

VECTEUR__
    MVECTUR = VDEP_R DDL_MECA

MATRICE__
    MMATUUR = MDEP_R DDL_MECA DDL_MECA

OPTION__

%   -- te0580 : ne resout que les cas triviaux : 0.
    CALC_G           580  IN__  NTHETAR PTHETAR  NFORCER  PFR1D2D  EPRESNO  PPRESSR    OUT__ EGTHETA  PGTHETA
    CALC_G_F         580  IN__  NTHETAR PTHETAR  CFORCEF  PFF1D2D  CPRESSF  PPRESSF    OUT__ EGTHETA  PGTHETA
    CALC_K_G         580  IN__  NTHETAR PTHETAR  NFORCER  PFR1D2D  EPRESNO  PPRESSR    OUT__ EKTHETA  PGTHETA
    CALC_K_G_F       580  IN__  NTHETAR PTHETAR  CFORCEF  PFF1D2D  CPRESSF  PPRESSF    OUT__ EKTHETA  PGTHETA

    CHAR_MECA_FF1D2D  36  IN__  E6NEUTI  PCNSETO  CFORCEF  PFF1D2D  NGEOMER  PGEOMER  E2NEUTI  PHEAVTO
                                E10NEUTI  PLONCHA  N1NEUT_R PLSN     N1NEUT_R PLST     E6NEUTR  PPINTTO
                                E4NEUTR  PPMILTO  STANO_I  PSTANO   CTEMPSR  PTEMPSR  N5NEUTI  PHEA_NO
                                E2NEUTI  PHEA_SE
                          OUT__ MVECTUR  PVECTUR
    CHAR_MECA_FR1D2D  36  IN__  E6NEUTI  PCNSETO  NFORCER  PFR1D2D  NGEOMER  PGEOMER  E2NEUTI  PHEAVTO
                                E10NEUTI  PLONCHA  N1NEUT_R PLSN     N1NEUT_R PLST     E6NEUTR  PPINTTO
                                E4NEUTR  PPMILTO  STANO_I  PSTANO   N5NEUTI  PHEA_NO  E2NEUTI  PHEA_SE
                          OUT__ MVECTUR  PVECTUR
    CHAR_MECA_PRES_F  36  IN__  E6NEUTI  PCNSETO  NGEOMER  PGEOMER  E2NEUTI  PHEAVTO  E10NEUTI  PLONCHA
                                N1NEUT_R PLSN     N1NEUT_R PLST     E6NEUTR  PPINTTO  E4NEUTR  PPMILTO
                                CPRESSF  PPRESSF  STANO_I  PSTANO   CTEMPSR  PTEMPSR  N5NEUTI  PHEA_NO
                                E2NEUTI  PHEA_SE
                          OUT__ MVECTUR  PVECTUR
    CHAR_MECA_PRES_R  36  IN__  E6NEUTI  PCNSETO  NGEOMER  PGEOMER  E2NEUTI  PHEAVTO  E10NEUTI  PLONCHA
                                N1NEUT_R PLSN     N1NEUT_R PLST     E6NEUTR  PPINTTO  E4NEUTR  PPMILTO
                                EPRESNO  PPRESSR  STANO_I  PSTANO   N5NEUTI  PHEA_NO  E2NEUTI  PHEA_SE
                          OUT__ MVECTUR  PVECTUR
    INI_XFEM_ELNO     99  IN__    OUT__  N1NEUT_R PLSN     N1NEUT_R PLST     STANO_I  PSTANO
    TOPOSE           514  IN__  NGEOMER  PGEOMER  N1NEUT_R PLEVSET
                          OUT__ E6NEUTI  PCNSETO  E2NEUTI  PHEAVTO  E10NEUTI  PLONCHA  E6NEUTR  PPINTTO
                                E4NEUTR  PPMILTO
    TOU_INI_ELEM      99  IN__
                          OUT__ CPRES_R  PPRES_R
    TOU_INI_ELNO      99  IN__
                          OUT__ NGEOMER PGEOM_R  ENNEUT_F PNEUT_F  ENNEUT_R PNEUT_R  EPRESNO  PPRES_R
    TOPONO           120  IN__  E6NEUTI  PCNSETO  E2NEUTI  PHEAVTO  E10NEUTI  PLONCHA  N1NEUT_R PLEVSET
                          OUT__ N5NEUTI  PHEA_NO  E2NEUTI  PHEA_SE
